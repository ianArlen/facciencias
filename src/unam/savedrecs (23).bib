@article{ ISI:000385900700040,
Author = {Guerrero-Sanchez, J. and Takeuchi, Noboru},
Title = {{Antiferromagnetic MnN layer on the MnGa(001) surface}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{2016}},
Volume = {{390}},
Pages = {{328-332}},
Month = {{DEC 30}},
Abstract = {{Spin polarized first principles total energy calculations have been
   applied to study the stability and magnetic properties of the MnGa(001)
   surface and the formation of a topmost MnN layer with the deposit of
   nitrogen. Before nitrogen adsorption, surface formation energies show a
   stable gallium terminated ferromagnetic surface. After incorporation of
   nitrogen atoms, the antiferromagnetic manganese terminated surface
   becomes stable due to the formation of a MnN layer (Mn-N bonding at the
   surface). Spin density distribution shows a
   ferromagnetic/antiferromagnetic arrangement in the first surface layers.
   This thermodynamically stable structure may be exploited to growth
   MnGa/MnN magnetic heterostructures as well as to look for exchange
   biased systems. (C) 2016 Elsevier B.V. All rights reserved.}},
