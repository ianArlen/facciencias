@article{ ISI:000390624600052,
Author = {Aguilar-Castillo, A. and Aguilar-Hernandez, J. R. and Garcia-Hipolito,
   M. and Lopez-Romero, S. and Swarnkar, R. K. and Baez-Rodriguez, A. and
   Fragoso-Soriano, R. J. and Falcony, C.},
Title = {{White light generation from HfO2 films co-doped with Eu3+ + Tb3+ ions
   synthesized by pulsed laser ablation technique}},
Journal = {{CERAMICS INTERNATIONAL}},
