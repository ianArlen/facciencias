@article{ ISI:000389838400005,
Author = {Garcia-Bernabe, Abel and Hernandez, S. I. and del Castillo, L. F. and
   Jou, David},
Title = {{Continued-Fraction Expansion of Transport Coefficients with Fractional
   Calculus}},
