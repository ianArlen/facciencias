@article{ ISI:000388733700011,
Author = {Jasso-Martinez, J. M. and Castaneda-Sortibran, A. N. and Pozo, C. and
   Garcia-Sandoval, R. and Prado, B. R. and Luis-Martinez, A. and
   Llorente-Bousquets, J. and Rodriguez-Arnaiz, R.},
Title = {{The Enantia jethys Complex(1): Insights from COI Confirm the Species
   Complex and Reveal a New Potential Cryptic Species}},
Journal = {{SOUTHWESTERN ENTOMOLOGIST}},
Year = {{2016}},
Volume = {{41}},
