Title = {{RESULTS FROM THE 2014 NOVEMBER 15TH MULTI-CHORD STELLAR OCCULTATION BY
   THE TNO (229762) 2007 UK126}},
Journal = {{ASTRONOMICAL JOURNAL}},
Year = {{2016}},
Volume = {{152}},
