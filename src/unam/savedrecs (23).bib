@article{ ISI:000388938300010,
Author = {Jaimes, Miguel A. and Lermo, Javier and Garcia-Soto, Adrian D.},
Title = {{Ground-Motion Prediction Model from Local Earthquakes of the Mexico
   Basin at the Hill Zone of Mexico City}},
