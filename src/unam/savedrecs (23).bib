@article{ ISI:000387999300001,
Author = {Heras, Jose A.},
Title = {{A note on the electric and magnetic susceptibilities}},
Journal = {{EUROPEAN JOURNAL OF PHYSICS}},
Year = {{2017}},
Volume = {{38}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{The linear electric and magnetic susceptibilities exhibit different
   values in Gaussian and SI units even when these constants are
   dimensionless. In this note we fix this practical inconvenience by
   redefining the permittivity and permeability of a linear medium in
   Gaussian units in such a way that their associated electric and magnetic
   susceptibilities have the same numerical values as those of the SI
   units.}},
