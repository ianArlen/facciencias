@article{ ISI:000395251300006,
Author = {Mendez, Franklin J. and Solano, Roger and Villasana, Yanet and Guerra,
   Julia and Curbelo, Susana and Inojosa, Marcel and Olivera-Fuentes,
   Claudio and Brito, Joaquin L.},
Title = {{Selective hydrogenation of 1,3-butadiene in presence of 1-butene under
   liquid phase conditions with NiPd/Al2O3 catalysts}},
Journal = {{APPLIED PETROCHEMICAL RESEARCH}},
Year = {{2016}},
Volume = {{6}},
Number = {{4}},
Pages = {{379-387}},
Month = {{DEC}},
Abstract = {{The catalytic performance of Al2O3-supported monometallic and bimetallic
   catalysts in selective hydrogenation of 1,3-butadiene in the presence of
   1-butene under liquid phase conditions was studied. Bimetallic catalysts
   were prepared by the coimpregnation method with the required amounts of
   the precursors salts {[}Ni(NO3)(2)center dot 6H(2)O and Pd(NH3)(4)Cl-2
   center dot H2O] over pellet-form gamma-Al2O3 with a constant content of
   Pd (0.5 wt\%) and varying Ni/Pd atomic ratio (0.25, 0.5, 0.75, and 1)
   obtaining egg-shell profiles of the active components. The catalysts
   were characterized by X-ray diffraction, temperature-programmed
   techniques, such as reduction in hydrogen and desorption of ammonia, N-2
   physisorption, and transmission electron microscopy. The catalytic test
   showed that the 1,3-butadiene was selectively hydrogenated when
   bimetallic catalysts were used. The addition of Ni to the Pd-based
   catalysts suppressed n-butane formation and increased recovery of
   1-butene at medium conversion. Therefore, it was observed an improved
   catalytic performance of the bimetallic catalysts being highest in the
   case of the 1NiPd/Al2O3.}},
