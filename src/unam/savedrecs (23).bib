@article{ ISI:000391169600061,
Author = {Merritt, Rachael L. and Cackett, Edward M. and Brown, Edward F. and
   Page, Dany and Cumming, Andrew and Degenaar, Nathalie and Deibel, Alex
   and Homan, Jeroen and Millers, Jon M. and Wijnands, Rudy},
Title = {{THE THERMAL STATE OF KS 1731-260 AFTER 14.5 YEARS IN QUIESCENCE}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2016}},
Volume = {{833}},
Number = {{2}},
Month = {{DEC 20}},
Abstract = {{Crustal cooling of accretion-heated neutron stars provides insight into
   the stellar interior of neutron stars. The neutron star X-ray transient,
   KS 1731-260, was in outburst for 12.5 years before returning to
   quiescence in 2001. We have monitored the cooling of this source since
   then through Chandra and XMM-Newton observations. Here we present a 150
   ks Chandra observation of KS. 1731-260 taken in 2015 August, about 14.5
   years into quiescence and 6 years after the previous observation. We
   find that the neutron star surface temperature is consistent with the
   previous observation, suggesting that crustal cooling has likely stopped
   and the crust has reached thermal equilibrium with the core. Using a
   theoretical crust thermal evolution code, we fit the observed cooling
   curves and constrain the core temperature (T-c = 9.35 +/- 0.25 x 10(7)
   K), composition (Q(imp) 4.4(-0.5)(+2.2)), and level of extra shallow
   heating required (Q(sh) = 1.36 +/- 0.18 MeV/nucleon). We find that the
   presence of a low thermal conductivity layer, as expected from nuclear
   pasta, is not required to fit the cooling curve well, but cannot be
   excluded either.}},
