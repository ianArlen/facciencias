@inproceedings{ ISI:000463095000006,
Author = {Juarez-Sandoval, Oswaldo and Fragoso-Navarro, Eduardo and
   Cedillo-Hernandez, Manuel and Nakano, Mariko and Perez-Meana, Hector and
   Cedillo-Hernandez, Antonio},
Book-Group-Author = {{IEEE}},
Title = {{Improved Unseen-Visible Watermarking for Copyrigth Protection of Digital
   Image}},
Booktitle = {{2017 5TH INTERNATIONAL WORKSHOP ON BIOMETRICS AND FORENSICS (IWBF 2017)}},
Series = {{International Workshop on Biometrics and Forensics IWBF}},
