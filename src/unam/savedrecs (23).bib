@article{ ISI:000389521800015,
Author = {Rial, Carlos and Varela, Rosa M. and Molinillo, Jose M. G. and Bautista,
   Elihu' and Hernandez, Alfredo Ortega and Macias, Francisco A.},
Title = {{Phytotoxicity evaluation of sesquiterpene lactones and diterpenes from
   species of the Decachaeta, Salvia and Podachaenium genera}},
Journal = {{PHYTOCHEMISTRY LETTERS}},
Year = {{2016}},
Volume = {{18}},
Pages = {{68-76}},
Month = {{DEC}},
Abstract = {{Decachaeta, Salvia and Podachaenium genera are known for their wide
   variety of biological activities. Synthetic herbicides have caused a
   variety of environmental and resistance problems. Natural products
   represent an important alternative to combat such issues. Sesquiterpene
   lactones and diterpenes are families of bioactive natural products for
   which a range of activities have been described. The bioactivities of
   nine sesquiterpene lactones, eight heliangolides, one guaianolide and
   twenty two diterpenes isolated from species of the Decachaeta, Salvia
   and Podachaenium genera were tested by applying a methodical procedure
   that involves assays of compounds on etiolated wheat coleoptiles
   (Triticum aestivum), Standard Target Species (STS) and two important
   weeds (barnyardgrass and brachiaria). The results clearly show that all
   of the sesquiterpene lactones studied were active on coleoptiles. In
   addition, six lactones were phytotoxic on both STS and weeds, meaning
   that these compounds could be used in the development of natural
   herbicide models. (C) 2016 Phytochemical Society of Europe. Published by
   Elsevier Ltd. All rights reserved.}},
