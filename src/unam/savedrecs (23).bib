@article{ ISI:000389869500009,
Author = {Fleury, Agnes and Sastre, Patricia and Sciutto, Edda and Correia, Silvia
   and Monedero, Alejandro and Toledo, Andrea and Hernandez, Maricela and
   Harrison, Leslie J. S. and Parkhouse, R. Michael E.},
Title = {{A lateral flow assay (LFA) for the rapid detection of extraparenchymal
   neurocysticercosis using cerebrospinal fluid}},
Journal = {{EXPERIMENTAL PARASITOLOGY}},
Year = {{2016}},
Volume = {{171}},
Pages = {{67-70}},
Month = {{DEC}},
Abstract = {{A lateral flow assay (LFA) for the diagnosis and monitoring of
   extraparenchymal neurocysticercosis, has been developed. The assay is
   based on the use of the monoclonal antibody HP10, and when applied to
   cerebrospinal fluid, correctly identified 34 cases of active
   extraparenchymal neurocysticercosis, but was negative with 26 samples
   from treated and cured neurocysticercosis patients and with 20 samples
   from unrelated neurological diseases. There was complete agreement
   between the HP10 Ag-ELISA results and the HP10-LFA. The HP10-LFA thus
   has utility for diagnosis and treatment of extraparenchymal
   neurocysticercosis, frequently a more dangerous form of the infection.
   (C) 2016 Elsevier Inc. All rights reserved.}},
