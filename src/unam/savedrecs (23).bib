@article{ ISI:000390671100009,
Author = {Reyes, P. G. and Gomez, A. and Martinez, H. and Flores, O. and Torres,
   C. and Vergara, J.},
Title = {{Characterization of Ethanol Plasma Glow Discharge, Decomposition in
   Several Species and Solid Film Formation}},
Journal = {{IEEE TRANSACTIONS ON PLASMA SCIENCE}},
Year = {{2016}},
Volume = {{44}},
Number = {{12, 1, SI}},
Pages = {{2995-3000}},
Month = {{DEC}},
Abstract = {{Ethanol plasma produced in low pressure with constant flux has been
   characterized in situ by optical emission spectroscopy (OES) and a
   Langmuir probe. From these methods, hydrogen and carbon monoxide are the
   main products from the ethanol decomposition in the plasma. In addition,
   C-2, OH, CH, O-2(+), and H-alpha species were identified. The values
   obtained for electron density and temperatures were 1.26 x 10(18) m(-3)
   and 1.16 eV, respectively. The mass deposited (MD) on the electrode was
   analyzed by scanning electron microscopy and Fourier transform infrared
   spectroscopy (FT-IR) techniques. The morphology of the MD changes as a
   function of the discharge time from thin film to a cluster formation.
   The FT-IR results show that the main absorbance corresponds with C-H
   bonds, indicating that the mass deposit is formed mainly by this
   molecular fraction, which agrees with the CH species identified as the
   less abundant molecule in the plasma by OES. In addition, C=C, O-H, C-H,
   C=C, and C-O bonds were identified.}},
