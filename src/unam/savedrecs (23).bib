@article{ ISI:000390275100003,
Author = {Hentschinski, M. and Kusina, A. and Kutak, K.},
Title = {{Transverse momentum dependent splitting functions at work:
   Quark-to-gluon splitting}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2016}},
Volume = {{94}},
Number = {{11}},
Month = {{DEC 15}},
Abstract = {{Using the recently obtained P-gq splitting function we extend the low-x
   evolution equation for gluons to account for contributions originating
   from quark-to-gluon splitting. In order to write down a consistent
   equation we resum virtual corrections coming from the gluon channel and
   demonstrate that this implies a suitable regularization of the P-gq
   singularity, corresponding to a soft emitted quark. We also note that
   the obtained equation is in a straightforward manner generalized to a
   nonlinear evolution equation which takes into account effects due to the
   presence of high gluon densities.}},
