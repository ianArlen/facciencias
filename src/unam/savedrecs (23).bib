Author = {Araujo-Escalona, Victoria and Acosta, L. and Andrade, E. and
   Barron-Palos, L. and de Lucio, O. and Favela, F. and Huerta, A. and
   Lopez, E. and Santa Rita, P. and Solis, C. and Chavez, E.},
Editor = {{Parriott, C}},
Title = {{Study of the Si-28(d,alpha)Al-26 nuclear reaction at low energies}},
Booktitle = {{CONFERENCE ON THE APPLICATION OF ACCELERATORS IN RESEARCH AND INDUSTRY
   (CAARI 2016)}},
Series = {{Physics Procedia}},
