@article{ ISI:000394939300007,
Author = {Martinez-Galicia, R. and Panayotaros, Panayotis},
Title = {{Coherent states and localization in a quantized discrete NLS lattice}},
Journal = {{JOURNAL OF NONLINEAR OPTICAL PHYSICS \& MATERIALS}},
Year = {{2016}},
Volume = {{25}},
Number = {{4}},
Month = {{DEC}},
Note = {{8th Conference on Nonlinear Guided Waves, Oaxaca, MEXICO, APR, 2016}},
Abstract = {{We study the evolution of a quantum discrete nonlinear Schrodinger
   (DNLS) system using as initial conditions coherent states corresponding
   to points in the vicinity of breather solutions of the classical system.
   We consider various examples of stable and unstable breathers and
   examine the distance between exactly evolved states and coherent states
   with parameters that evolve according to classical dynamics. Initial
   conditions near stable breathers and their vicinity are seen to lead to
   recurrences to small distances between the two evolving states. Similar
   recurrences are not observed for initial conditions near unstable
   breathers.}},
