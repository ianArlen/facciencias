@article{ ISI:000398451900012,
Author = {Angel Suarez-Rosales, Miguel and Pinto-Segura, Raul and Guadalupe
   Palacios-Beas, Elia and Hernandez-Herrera, Alfredo and Chavez-Alcala,
   Jose Federico},
Title = {{Effect of Rapid Solidification and Addition of Cu3P on the Mechanical
   Properties of Hypereutectic Al-Si Alloys}},
Journal = {{MATERIALS RESEARCH-IBERO-AMERICAN JOURNAL OF MATERIALS}},
Year = {{2016}},
Volume = {{19}},
Number = {{1}},
Pages = {{67-73}},
Month = {{DEC}},
Note = {{6th Latin American Conference on Metastable and Nanostructured Materials
   (NANOMAT), Cancun, MEXICO, AUG 19-20, 2015}},
Organization = {{Inst Politecnico Nacl, Dept Engn Metal \& Mat; Fed Univ Sao Carlos, Dept
   Mat Engn}},
Abstract = {{The combined processes; rapid solidification, addition of Cu3P compound
   and heat treatments to improve the mechanical properties of the
   hypereutectic Al-13Si, Al-20Si and Al-20Si-1.5Fe-0.7Mn alloys (in wt.
   \%) was studied. Optical microscopy and scanning electron microscopy
   were used to characterize the microstructures. The mechanical properties
   were evaluated by tensile tests. It was found that the cooling rate
   (20-50 degrees C/s) used to solidify the alloys plus the addition of
   Cu3P compound favored the formation of fine primary Si and the
   transformation of the Al/Si eutectic from acicular to semi-transformed
   morphology. The spheroidization of the Al/Si eutectic after heat
   treatment caused an increase in the elongation (\%) compared with the
   as-cast alloys. Al-13Si alloy showed the highest UTS and elongation (\%)
   values, reaching values up to 215 MPa and 9.6\%, respectively. On the
   other hand, the Al-20Si-1.5Fe-0.7Mn alloy showed the lowest UTS (180MPa)
   and elongation (3.4\%) values. The formation of the Fe-intermetallic
   compounds caused a negative effect on the mechanical properties of the
   Al-20Si-1.5Fe-0.7Mn alloy.}},
