@article{ ISI:000389768800018,
Author = {Flores Morales, Shirley Sarai and Leon Flores, Jesus A. and Perez
   Mazariego, Jose Luis and Marquina Fabrega, Vivianne and Gomez Gonzalez,
   Raul W.},
Title = {{Synthesis of Bi25FeO39 by molten salts method and its mossbauer spectrum}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
Year = {{2017}},
Volume = {{504}},
Pages = {{109-111}},
Month = {{JAN 1}},
Abstract = {{A sample of Bi25FeO39 was synthesized by the molten salts method in 2 h
   at 750 degrees C with KCl-KBr as reaction media. The sample was
   characterized with X-ray powder diffraction and Mossbauer spectroscopy.
   The Rietveld refinement of the X-ray diffraction confirms that a
   sillenite type structure with a cell parameter a=10.190(4) angstrom was
   achieved, with only small traces of Bi2O3. In order to get a Mossbauer
   spectrum with a good signal to noise ratio, the synthesis was made using
   95\% enriched alpha-57Fe(2)O(3). To our knowledge, this is the first
   time that a Mossbauer spectrum for the Bi25FeO39 has been recorded. The
   nonappearance of a magnetic sextet confirms its paramagnetic behavior.
   The measured Mossbauer parameters (IS=0.20 +/- 0.004 mm/s and QS=0.2 +/-
   0.07 mm/s) correspond to Fe+3 in very symmetric surroundings, and rule
   out some interpretations given to the doublet observed in some Mossbauer
   spectra of BiFeO3.}},
