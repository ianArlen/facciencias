@article{ ISI:000389503400009,
Author = {Wong, Arturo and Mireles, Francisco},
Title = {{Zero-field splitting of the Kondo resonance and quantum criticality in
   triple quantum dots}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2016}},
Volume = {{94}},
Number = {{24}},
Month = {{DEC 5}},
Abstract = {{We consider a triple-quantum-dot (TQD) system composed by an interacting
   quantum dot connected to two effectively noninteracting dots, which in
   turn are both connected in parallel to metallic leads. As we show, this
   system can be mapped onto a single-impurity Anderson model with a
   nontrivial density of states. The TQD's transport properties are
   investigated under a continuous tuning of the noninteracting dots'
   energy levels, employing the numerical renormalization group technique.
   Interference between single and many-particle resonances splits the
   Kondo peak, fulfilling a generalized Friedel sum rule. In addition, a
   particular configuration in which one of the noninteracting dots is held
   out of resonance with the leads allows us to access a pseudogap regime
   where a Kosterlitz-Thouless-type quantum-phase transition (QPT) occurs,
   separating the Kondo and non-Kondo behavior. Within this same
   configuration, the TQD exhibits traces of the Fano-Kondo effect, which
   is in turn strongly affected by the QPT. Signatures of all these
   phenomena are neatly displayed by the calculated linear conductance.}},
