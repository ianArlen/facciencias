@article{ ISI:000392963200011,
Author = {Mosquera-Artamonov, J. D. and Vasco-Leal, J. F. and Acosta-Osorio, A. A.
   and Hernandez-Rios, I. and Ventura-Ramos, E. and Gutierrez-Cortez, E.
   and Rodriguez-Garcia, M. E.},
Title = {{Optimization of castor seed oil extraction process using response
   surface methodology}},
Journal = {{Ingenieria e Investigacion}},
Year = {{2016}},
Volume = {{36}},
Number = {{3}},
Pages = {{82-88}},
Month = {{DEC}},
Abstract = {{This work focuses on the study of the oil extraction yield from castor
   seed using three different seed conditions: whole, minced and bare
   endosperm. Taguchi design was used to determine the contribution of the
   following parameters: seed condition, seed load in the extractor,
   temperature, and pressure. It was proved that it is necessary to
   introduce the whole seed and that the presence of the pericarp increases
   the extraction yield. The contribution of the control factors has an
   extraction yield limit. After determining which factors contributed to
   the process, these were left at their optimum levels aiming to reduce
   the control factors to only two. The complete analysis was done using a
   surface response methodology giving the best parameter for temperature
   and pressure that allows a better yielding mechanical extraction. The
   oil extraction yield can be kept up to 35 \% of the seed.}},
