@article{ ISI:000388056800004,
Author = {Velazquez-Lopez, I. and Leon-Cruz, E. and Pardo, J. P. and Sosa-Peinado,
   A. and Gonzalez-Andrade, M.},
Title = {{Development of new hCaM-Alexa Fluor (R) biosensors for a wide range of
   ligands}},
Journal = {{ANALYTICAL BIOCHEMISTRY}},
Year = {{2017}},
Volume = {{516}},
Pages = {{13-22}},
Month = {{JAN 1}},
Abstract = {{Eight new fluorescent biosensors of human calmodulin (hCaM) using Alexa
   Fluor (R) 350, 488, 532, and 555 dyes were constructed. These biosensors
   are thermodynamically stable, functional, and highly sensitive to
   ligands of the CaM. They resolve the problem of CaM ligands with similar
   spectroscopic properties to the intrinsic and extrinsic fluorophores of
   other biosensors previously reported. Additionally, they can be used in
   studies of protein-protein interaction through Forster resonance energy
   transfer (FRET). The variation in T-m (range 78.07-81.47 degrees C;
   79.05 to WT) is no larger than two degrees in all cases in regards to
   CaM WT. The K-ds calculated with all biosensors for CPZ and BIMI (a new
   inhibitor of CaM) are in the range of 0.45-1.86 and 0.69-1.54 mu m
   respectively. All biosensors retain their ability to activate
   Calcineurin about 70\%. Structural models built ``in silico{''} show
   their possible conformation taking the fluorophores in protein thus we
   can predict system stability. Finally, these new biosensors represent a
   biotechnological development applied to an analytical problem, which
   aims to determine accurately the affinity of inhibitors of CaM without
   possible interference, to be put forward as possible drugs related to
   CaM. (C) 2016 Elsevier Inc. All rights reserved.}},
