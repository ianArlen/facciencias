@inproceedings{ ISI:000455235900020,
Author = {Zurbriggen, E. and Costa, A. and Esquivel, A. and Schneiter, M. and
   Cecere, M.},
Editor = {{Dominguez, SV and Kosovichev, AG and Antolin, P and Harra, L}},
Title = {{MHD simulations of coronal dark downflows considering thermal conduction}},
Booktitle = {{FINE STRUCTURE AND DYNAMICS OF THE SOLAR ATMOSPHERE}},
Series = {{IAU Symposium Proceedings Series}},
Year = {{2017}},
Volume = {{12}},
