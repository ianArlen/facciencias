@article{ ISI:000417443500014,
Author = {Mendoza Huizar, L. H. and Rios-Reyes, C. H. and Rivera, M.},
Title = {{ELECTRODEPOSITION OF COBALT NANOCLUSTERS FROM AMMONICAL CHLORIDE
   SOLUTIONS ONTO HOPG ELECTRODES. A KINETICAL AND MORPHOLOGICAL STUDY}},
Journal = {{JOURNAL OF THE CHILEAN CHEMICAL SOCIETY}},
Year = {{2017}},
Volume = {{62}},
Number = {{3}},
Pages = {{3621-3626}},
Abstract = {{A kinetical study about the Co electrodeposition onto Highly Oriented
   Pyrolytic Graphite electrodes from an aqueous solution containing 0.01 M
   CoCl2 + 1 M NH4CI (pH = 7) was conducted at overpotential conditions
   through potentiostatic studies. The entire chronoamperograms were
   adequately predicted, considering the contribution to the total current
   of three different processes: a Langmuir-type adsorption process, a
   three-dimensional nucleation and growth and a proton reduction process.
   In all cases, the nucleation rate and the number of active nucleation
   sites are potential dependent. The Atomic Force Microscopy study
   revealed the presence of homogeneous cobalt clusters of less of 100 nm
   in height and 50 nm in diameter at different potential values.}},
