@article{ ISI:000425126300007,
Author = {Orozco Rivas, Bernardo and Mendoza Perez, Mabel},
Title = {{Repair of the foundation and induced subsidence of an industrial
   building by using control piles and material extraction}},
Journal = {{HORMIGON Y ACERO}},
