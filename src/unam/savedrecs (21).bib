@article{ ISI:000418061400003,
Author = {Ben, Roberto I. and Pablo Borgna, Juan and Panayotaros, Panayotis},
Title = {{PROPERTIES OF SOME BREATHER SOLUTIONS OF A NONLOCAL DISCRETE NLS
   EQUATION}},
