@article{ ISI:000408053400002,
Author = {Perlo Cohen, Manuel and Zamora Saenz, Itzkuauhtli},
Title = {{ENVIRONMENTAL PERSPECTIVES ON THE CONTAMINATION AND RECOVERY OF THE
   MAGDALENA RIVER IN THE CITY OF MEXICO}},
