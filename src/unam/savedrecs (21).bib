Author = {Maldonado-Ramirez, Alan and Lopez-Juarez, Ismael and Rios-Cabrera, Reyes
   and Osorio-Comparan, Roman and Pena-Cabrera, Mario and Lefranc, Gaston},
Book-Group-Author = {{IEEE}},
Title = {{A Fault Compensation Algorithm for a Distributed Manufacturing System}},
