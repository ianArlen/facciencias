@article{ ISI:000423965400091,
Author = {Moreno, Jaime A. and Mujica-Ortega, H. and Espinosa-Perez, G.},
Title = {{A Global Bivalued-Observer for the Sensorless Induction Motor}},
Journal = {{IFAC PAPERSONLINE}},
Year = {{2017}},
Volume = {{50}},
