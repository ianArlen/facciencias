@article{ ISI:000414703700008,
Author = {Bautista Rojas, Enrique},
Title = {{INCLUSIVE EDUCATION OR EDUCATIONAL INCLUSION? REFLECTION ABOUT THE
   INDIGENOUS EDUCATION IN MEXICO}},
Journal = {{COLLECTIVUS-REVISTA DE CIENCIAS SOCIALES}},
Year = {{2017}},
Volume = {{4}},
Number = {{1}},
Pages = {{155-180}},
Month = {{JAN-JUN}},
Abstract = {{In Mexico, indigenous schools have historically been scenarios with
   various difficulties and challenges to ensure that the education of
   children and young people of indigenous communities. In consideration of
   this, the research explores the situation of indigenous education to
   analyze the way in which this has resulted in neglect and acculturation
   activities. The referents based on the review and analysis of
   specialized literature and the approaches taken from intercultural
   education and inclusive education in relation to indigenous communities.
   It aims to motivate the reflection to look at the indigenous schools
   from a perspective contextualized where diversity represents
   opportunities for the training of children and young people in a
   holistic manner.}},
