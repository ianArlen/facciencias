@article{ ISI:000417391500022,
Author = {Propin Frejomil, Enrique and Sanchez Crispin, Alvaro and Alvarado Sizzo,
   Ilia},
Title = {{Levels of territorial selectivity of tourist destinations in Mexico}},
Journal = {{CUADERNOS DE TURISMO}},
Year = {{2017}},
Number = {{39}},
Pages = {{495-520}},
Month = {{JAN-JUN}},
Abstract = {{The diversification of the tourist market in Mexico has resulted in a
   growing number of destinations, some of which have been given special
   attention by different social agents. This paper proposes a
   classification of Mexican tourist destinations arranged into ten
   categories of territorial selectivity. Firstly, we portray the
   conceptual framework for this research, then we indicate the evolution
   of the territorial sponsorship of tourism in Mexico; further on, we make
   clear the methodological path followed and, finally, we explain the
   fundamentals of each category of tourist destinations found.}},
