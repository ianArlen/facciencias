@article{ ISI:000416115800001,
Author = {Battcock, Clementina and Montoya, Alejandra Davila},
Title = {{Illustrations of the Tenochca wars in Tovar and Duran. Versions and
   misunderstandings}},
Journal = {{REVISTA DE INDIAS}},
Year = {{2017}},
Volume = {{77}},
Number = {{271}},
Pages = {{691-725}},
Abstract = {{There are two versions of the war between Tenochtitlan and Azcapotzalco
   in three sources of indigenous tradition from the second half of the
   sixteenth century: Fray Diego Duran's History of the Indies of New
   Spain, and the two taken from Juan de Tovar's Segunda Relacion: the
   Codex Ramirez and the Tovar Manuscript. Through analysis of the
   differences between the alphabetic and the pictographic accounts of this
   episode in these sources, this paper presents and discusses the
   disparities and misinterpretations regarding the pictographic materials
   that accompany these works.}},
