@article{ ISI:000425062200002,
Author = {Hirsch Adler, Ana and Navia Antezana, Cecilia},
Title = {{Research ethics with persons and groups in vulnerable condition in
   Mexico and Bolivia}},
Journal = {{PRAXIS SOCIOLOGICA}},
Year = {{2017}},
Number = {{22}},
Pages = {{15-28}},
Abstract = {{The purpose of the article is to understand research ethics, especially
   when the persons, groups and communities involved are vulnerable. The
   theoretical framework is about two different approaches: intercultural
   research and the professional ethics principles and rules. It is an
   exploratory study, with a questionnaire of open questions that was
   applied to a sample of 79 professors that are in charge of teaching
   future professionals of the educational basic levels: 56 in Mexico and
   23 in Bolivia. One of the conclusions is about the relevance of the
   ethical aspects, and among them the values of respect, responsibility,
   honesty, solidarity and transparency and also the need to promote
   benefits and avoid producing risks and harms to research participants.}},
Publisher = {{UNIV CASTILLA-LA MANCHA, FAC CIENCIAS JURIDICAS \& SOCIALES}},
