@article{ ISI:000406670600001,
Author = {Castro Perez, Jaime and Medina Barcenas, Mauricio and Rios Montes, Jose},
Title = {{Modules with ascending chain condition on annihilators and Goldie
   modules}},
Journal = {{COMMUNICATIONS IN ALGEBRA}},
Year = {{2017}},
Volume = {{45}},
Number = {{6}},
Pages = {{2334-2349}},
Abstract = {{Using the concepts of prime module, semiprime module and the concept of
   ascending chain condition (ACC) on annihilators for an R-module M, we
   prove that if M is semiprime and projective in sigma{[}M], such that M
   satisfies ACC on annihilators, then M has finitely many minimal prime
   submodules. Moreover, if each submodule N subset of M contains a uniform
   submodule, we prove that there is a bijective correspondence between a
   complete set of representatives of isomorphism classes of indecomposable
