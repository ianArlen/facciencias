@article{ ISI:000425251400005,
Author = {Robles Zamora, J. Alfredo},
Title = {{EPISTEMIC RESPONSIBILITY IN ETHNOARCHAEOLOGICAL ANALOGY: APPROACHES TO A
   THEORY OF JUSTIFICATION FOR ARCHAEOLOGICAL PRACTICE IN MEXICO}},
Journal = {{REVISTA COLOMBIANA DE FILOSOFIA DE LA CIENCIA}},
Year = {{2017}},
Volume = {{17}},
Number = {{34}},
Pages = {{117-136}},
Month = {{JAN-JUN}},
Abstract = {{It is argues that if the overcoming between foundationalism and
   coherentism proposed by Ernest Sosa is accepted, and a theory of
   epistemic justification based on the exercise of intellectual virtues is
   sustained, this is not a sufficient condition for the use of analogical
   reasoning in archaeological research, therefore, although it can be
   argued the need for the present case, it is not enough unless otherwise
   directed recourse to the virtue of responsibility (Code) as the axis of
   epistemic justification. Finally its viability and importance in the
   case of archeology in Mexico is discussed.}},
