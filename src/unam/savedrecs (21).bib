@inproceedings{ ISI:000423777500001,
Author = {Garcia-Illescas, M. A. and Alvarez-Icaza, Luis},
Book-Group-Author = {{ASME}},
Title = {{ON-LINE IDENTIFICATION OF THREE-DIMENSIONAL SHEAR BUILDING MODELS}},
Booktitle = {{PROCEEDINGS OF THE ASME 10TH ANNUAL DYNAMIC SYSTEMS AND CONTROL
   CONFERENCE, 2017, VOL 3}},
Year = {{2017}},
Note = {{10th ASME Annual Dynamic Systems and Control Conference, Tysons, VA, OCT
   11-13, 2017}},
Organization = {{ASME, Div Syst \& Control Div}},
Abstract = {{An on-line identification scheme for shear building models using
   recursive least squares with a matrix parameterized model is presented.
   Based on Gershgorin circles and tridiagonal matrices properties, the
   identified model stability is guaranteed in the presence of low
   excitation or low damping. Stability of the model helps in the design of
   more robust control laws. The scheme is evaluated in an experimental
   test-bed with a scaled five stories building where an on-line reduced
   order model is derived. Results indicate that when employing this matrix
   parametrization, a significant reduction in the number of calculations
   involved is achieved, when compared to the standard vector
   parametrization based schemes, such that real-time applications are
   feasible to implement. Moreover, the stability on the identified model
   is preserved.}},
