@article{ ISI:000411932300009,
Author = {Gujt, J. and Dominguez, H. and Sokolowski, S. and Pizio, O.},
Title = {{Isobaric-isothermal molecular dynamics computer simulations of the
   properties of water-1,2-dimethoxyethane model mixtures}},
Journal = {{CONDENSED MATTER PHYSICS}},
Year = {{2017}},
Volume = {{20}},
Number = {{3}},
Abstract = {{Isothermal-isobaric molecular dynamics simulations have been performed
   to examine a broad set of properties of the model
   water-1,2-dimethoxyethane (DME) mixture as a function of composition.
   The SPC-E and TIP4P-Ew water models and the modified TraPPE model for
   DME were applied. Our principal focus was to explore the trends of
   behaviour of the structural properties in terms of the radial
   distribution functions, coordination numbers and number of hydrogen
   bonds between molecules of different species, and of conformations of
   DME molecules. Thermodynamic properties, such as density, molar volume,
   enthalpy of mixing and heat capacity at constant pressure have been
   examined. Finally, the self-diffusion coefficients of species and the
   dielectric constant of the system were calculated and analyzed.}},
