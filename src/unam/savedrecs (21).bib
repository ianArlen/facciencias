@inproceedings{ ISI:000413743100023,
Author = {Zuniga Reyes, A. and Hernandez Almada, A. and Sandoval Espinosa, A. and
   HAWC Collaboration},
Editor = {{Acosta, L and Bijker, R and GomezCamacho, A and Velazquez, V}},
Title = {{Muon study for gamma/hadron air-shower discrimination in the HAWC
   observatory}},
Booktitle = {{XL SYMPOSIUM ON NUCLEAR PHYSICS 2017 (COCOYOC2017)}},
Series = {{Journal of Physics Conference Series}},
Year = {{2017}},
Volume = {{876}},
Note = {{40th Symposium on Nuclear Physics (Cocoyoc), Cocoyoc, MEXICO, JAN 04-07,
   2017}},
Organization = {{UNAM, Inst Ciencias Nucleares; UNAM, Inst Fisica; Inst Nacl
   Investigaciones Nucleares; SMF Div Fisica Nucl}},
Abstract = {{The High-Altitude Water Cherenkov observatory is a ground-based array
   designed to study energetic gamma-rays. Experiments with this purpose
   have to face a huge rate of undesired hadronic background. Motivated by
   the fact that muon content is quite different in gamma-induced (poor in
   muons) and hadronic-induced (rich in muons) air-showers, we study the
   idea of formulating a new variable for background reduction related with
   counting the number of muons candidates present in such showers.
   Therefore, in this work we used the time differences between
   photomultipliers tubes to identify the signature of muons inside the
   water Cherenkov detectors. Showers with a high presence of muons
   typically must produce a characteristic time difference around 5 ns
   among the central detector (PMT\_C) and each one of the laterals (PMT:
   A, B, D).}},
