@inproceedings{ ISI:000423874100020,
Author = {Vyas, Manan},
Editor = {{Danielewicz, P and Zelevinsky, V}},
Title = {{Non-Equilibrium Many-Body Dynamics Following A Quantum Quench}},
Booktitle = {{FIFTH CONFERENCE ON NUCLEI AND MESOSCOPIC PHYSICS}},
