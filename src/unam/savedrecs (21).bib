@inproceedings{ ISI:000418465700026,
Author = {Ramirez-Cedillo, Erick and Lara-Padilla, Hernan and Zamudio-Pena, Luis
   F. and Rodriguez-Garcia, Aida and Ruiz-Huerta, Leopoldo and Caballero
   Ruiz, Alberto and Siller, Hector R.},
Editor = {{Shih, A and Cao, J}},
Title = {{Process chain for the fabrication of a custom 3D barrier for guided bone
   regeneration}},
Booktitle = {{3RD CIRP CONFERENCE ON BIOMANUFACTURING}},
