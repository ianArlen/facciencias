@article{ ISI:000418884500001,
Author = {Ayala Guerrero, Fructuoso and Mexicano Medina, Graciela},
Title = {{Effect of a medicinal plant (\&ITPassiflora incarnata \&ITL) on sleep}},
Journal = {{SLEEP SCIENCE}},
Year = {{2017}},
Volume = {{10}},
Number = {{3}},
Pages = {{96-100}},
Abstract = {{Introduction: Extracts of the plant Passiflora incarnata L.
   (Passifloraceae) were administered intraperitoneally in order to test
   its effects on sleep. Method: Experiments were carried out on
   chronically implanted male adult wistar rats to obtain cerebral (EEG),
   ocular (EOG) and muscular (EMG) activities throughout their states of
   vigilance. Polygraphic recordings were taken during 9 continuous hours
   before and after the extract administration (500 mg/kg). Results:
   Passiflora incarnata induced a significant increment in the total sleep
   time (p<0.05). This increment was due to an increase in the time spent
   by animals in slow wave sleep (SWS). Concomitantly, a significant
   decrement in wakefulness (W) was observed (p<0.05). In contrast, time
   spent in rapid eye movement (REM) sleep showed a decreasing tendency,
   since both its frequency and mean duration were reduced. Conclusions:
   The extracts obtained from Passiflora incarnata can be considered as
   appropriated sleep inducers.}},
