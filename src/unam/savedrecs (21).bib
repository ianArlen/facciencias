@inproceedings{ ISI:000413743100022,
Author = {Yepez-Martinez, T. and Amor Quiroz, D. A. and Hess, P. O. and
   Civitarese, O.},
Editor = {{Acosta, L and Bijker, R and GomezCamacho, A and Velazquez, V}},
Title = {{TDA and RPA pseudoscalar and vector solutions for the low energy regime
   of a motivated QCD Hamiltonian}},
Booktitle = {{XL SYMPOSIUM ON NUCLEAR PHYSICS 2017 (COCOYOC2017)}},
Series = {{Journal of Physics Conference Series}},
