@inproceedings{ ISI:000417377900167,
Author = {Daniel Macias, Juan and Melissa Herrera-Zamora, Dallely and Ivan
   Lizama-Tzec, Francisco and Bante-Guerra, Jose and Eduardo Ares-Muzio,
   Oscar and Oskam, Gerko and Romero-Paredes Rubioa, Hernando and Jose
   Alvarado-Gil, Juan and Arancibia-Bulnes, Camilo and Ramos-Sanchez,
   Victor and Isabel Villafan-Vidales, Heidi},
Editor = {{AlObaidli, A and Calvet, N and Richter, C}},
Title = {{Optical and Thermal Properties of Selective Absorber Coatings Under CSP
   Conditions}},
Booktitle = {{INTERNATIONAL CONFERENCE ON CONCENTRATING SOLAR POWER AND CHEMICAL
   ENERGY SYSTEMS (SOLARPACES 2016)}},
