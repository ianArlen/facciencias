@article{ ISI:000288272500015,
Author = {Rangarajan, Radhika and Vicent, Luis Edgar and U'Ren, Alfred B. and
   Kwiat, Paul G.},
Title = {{Engineering an ideal indistinguishable photon-pair source for optical
   quantum information processing}},
Journal = {{JOURNAL OF MODERN OPTICS}},
