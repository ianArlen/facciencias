@inproceedings{ ISI:000294438200019,
Author = {Perez-Rendon, Brenda and Pineda-Leon, Horacio and Santillan, Alfredo and
   Hernandez-Cervantes, Liliana},
Editor = {{Neiner, C and Wade, G and Meynet, G and Peters, G}},
Title = {{Evolutionary effects of stellar rotation of massive stars in their
   pre-supernova environments}},
Booktitle = {{ACTIVE OB STARS: STRUCTURE, EVOLUTION, MASS-LOSS, AND CRITICAL LIMITS}},
Series = {{IAU Symposium Proceedings Series}},
Year = {{2011}},
Number = {{272}},
Pages = {{99+}},
Note = {{272nd Symposium of the International-Astronomical-Union on Active OB
   Stars: Structure, Evolution, Mass-Loss, and Critical Limits, Paris,
   FRANCE, JUL 19-23, 2010}},
Organization = {{Int Astronom Union; Region Ile France; CNRS, Inst Natl Sci Univ; Minist
   Enseignement Superieur \& Rech; Programme Natl Phys Stellaire; GEPI Lab;
   Sci Council Paris Observ; A \& A; EDP Sci}},
Abstract = {{Massive main sequence stars are fast rotators. Stellar rotation affects
   massive stellar rotation due to rotationally induced mixing processes,
   the increase of mass loss rates, etc. and also affects the circumstellar
   medium due to their interaction with the stellar wind. The parameters of
   stellar winds depends on stellar parameters so the wind parameters
   change as the star evolves, coupling the evolution of circumstellar
   medium to the star itself. In this work we used a stellar code to build
   models of two massive stars (30 and 40 M-circle dot) and we used their
   wind parameters to simulate the hydrodynamics of their surrounding gas
   with the ZEUS-3D code in order to explore the effects of stellar
   rotation in the pre-supernova environments.}},
Publisher = {{CAMBRIDGE UNIV PRESS}},
Address = {{THE PITT BUILDING, TRUMPINGTON ST, CAMBRIDGE CB2 1RP, CAMBS, ENGLAND}},
Type = {{Proceedings Paper}},
Language = {{English}},
Affiliation = {{Perez-Rendon, B (Reprint Author), Univ Sonora, Dept Invest Fis, Hermosillo 83000, Sonora, Mexico.
