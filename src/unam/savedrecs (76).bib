@article{ ISI:000290233500005,
Author = {Campos Sanchez, Minerva and Guadalupe Priego-Santander, Angel},
Title = {{Biophysical landscapes of a coastal area of Michoacan state in Mexico}},
Journal = {{JOURNAL OF MAPS}},
Year = {{2011}},
Volume = {{7}},
Number = {{1}},
Pages = {{42-50}},
Abstract = {{The heterogeneity of the biophysical landscapes in the sierra-coast
   region of Michoacan, Mexico, determines the distribution of ecological,
   societal and economical variables within the area. At the same time,
   this heterogeneity influences the land-use potential and susceptibility
   to degradation by those uses. The mapping, characterization and
   classification of such landscape units can play an important role as
   basic information and provide reference units for landscape visions in
   planning processes at regional levels. This study aims to describe the
   complex biophysical structure of the San Telmo-Ticuiz-Boca de Apiza
   area, on the northwest coast of Michoacan, Mexico at a scale of 1:
   50,000. The various landscape units are described in this context to
   form an orientation and basis for spatial planning in the sierra-coast
   region.}},
