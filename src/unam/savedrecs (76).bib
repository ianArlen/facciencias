@inproceedings{ ISI:000393807600130,
Author = {Ramirez Aguilar, J. Alberto and Santillan Gutierrez, S. Daniel},
Book-Group-Author = {{IEEE}},
Title = {{Non-Linear PHEMT Model and it Implementation in High Gain Monolithic
   X-Band Amplifier for Space Communications Systems}},
Booktitle = {{2011 IEEE INTERNATIONAL CONFERENCE ON MICROWAVES, COMMUNICATIONS,
   ANTENNAS AND ELECTRONIC SYSTEMS (COMCAS 2011)}},
Series = {{IEEE International Conference on Microwaves Communications Antennas and
   Electronic Systems}},
Year = {{2011}},
Note = {{IEEE International Conference on Microwaves, Communications, Antennas
   and Electronic Systems (COMCAS), Tel Aviv, ISRAEL, NOV 07-09, 2011}},
Organization = {{IEEE}},
Abstract = {{This paper describes the design of high gain amplifiers for space
   communications systems in the frequency range of 8-12 GHz using a PHEMT
   operated at the bias point where the maximum value of transconductance
   (Gm) occurs. The amplifiers design is based on the knowledge of the
   small signals equivalent circuit of a PHEMT F4X25 featured by a 0.25 mu
   m gate length. The amplifiers design processes involve synthesis of
   matching networks with Chebyshev response in passive and distribute
   elements using MDS and Libra programs. A negative feedback is used to
   achieve the stability condition and flat response of the gain amplifier.
   In order to predict Imp (intermodulation distortion), we use the
   ``chen's{''} Non Linear model modeling the drain current Ids(Vgs, Vds)
   and the first three associates derivaties. Finally, the performance of
   the amplifier was evaluating using ``MDS{''} commercial simulation
   software. The achieved gain is 11.5 dB with an input and output VSWR of
   1.9 in all the bandwidth.}},
