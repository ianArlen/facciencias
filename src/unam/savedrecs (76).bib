@article{ ISI:000285520100004,
Author = {Rodriguez-Andrade, M. A. and Aragon-Gonzalez, G. and Aragon, J. L. and
   Gomez-Rodriguez, A.},
Title = {{Coincidence lattices in the hyperbolic plane}},
Journal = {{ACTA CRYSTALLOGRAPHICA SECTION A}},
Year = {{2011}},
Volume = {{67}},
Number = {{1}},
Pages = {{35-44}},
Month = {{JAN}},
Abstract = {{The problem of coincidences of lattices in the space R-p,R-q, with p + q
   = 2, is analyzed using Clifford algebra. We show that, as in R-n, any
   coincidence isometry can be decomposed as a product of at most two
   reflections by vectors of the lattice. Bases and coincidence indices are
   constructed explicitly for several interesting lattices. Our procedure
   is metric-independent and, in particular, the hyperbolic plane is
   obtained when p = q = 1. Additionally, we provide a proof of the
   Cartan-Dieudonne theorem for R-p,R-q, with p + q = 2, that includes an
   algorithm to decompose an orthogonal transformation into a product of
   reflections.}},
Publisher = {{WILEY-BLACKWELL}},
Address = {{COMMERCE PLACE, 350 MAIN ST, MALDEN 02148, MA USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Aragon, JL (Reprint Author), Univ Nacl Autonoma Mexico, Ctr Fis Aplicada \& Tecnol Avanzada, Dept Nanotecnol, Apartado Postal 1-1010, Queretaro 76000, Mexico.
