@inproceedings{ ISI:000295993300055,
Author = {Sahu, Sarira and Fraija, Nissim and Keum, Yong-Yeon},
Editor = {{Ayala, A and Contreras, G and Leon, I and Podesta, P}},
Title = {{Neutrino oscillation in Magnetized Gamma-Ray Burst Fireball}},
Booktitle = {{XII MEXICAN WORKSHOP ON PARTICLES AND FIELDS}},
Series = {{AIP Conference Proceedings}},
Year = {{2011}},
Volume = {{1361}},
Pages = {{389+}},
Note = {{12th Mexican Workshop on Particles and Fields, Mazatlan, MEXICO, NOV
   09-14, 2009}},
Organization = {{CONACYT; SMF; UNAM; UAS; UNISON; BAUP; CINVESTAV; UMSNH; UG; UAEH; AMC}},
Abstract = {{Energy MeV neutrinos are produced within the fireball due to electron
   positron annihilation and nucleonic bremsstrahlung. Many of these
   neutrinos will propagate through the dense and relativistic magnetized
   plasma of the fireball. We have studied the possibility of resonant
   oscillation of v(e) <-> v mu,tau by taking into account the neutrino
