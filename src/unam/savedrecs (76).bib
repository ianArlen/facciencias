Title = {{Femto-, pico- and nano-second refractive nonlinearities exhibited by Au
   nanoparticles}},
Booktitle = {{22ND CONGRESS OF THE INTERNATIONAL COMMISSION FOR OPTICS: LIGHT FOR THE
   DEVELOPMENT OF THE WORLD}},
Series = {{Proceedings of SPIE}},
Year = {{2011}},
Volume = {{8011}},
