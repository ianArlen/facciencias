@article{ ISI:000289882000003,
Author = {Baran, A. S. and Winiarski, M. and Krzesinski, J. and Fox-Machado, L.
   and Kawaler, S. D. and Drozdz, M. and Faltenbacher, A. and Thompson, M.
   A. and Reed, M. D.},
Title = {{Mt. Suhora Survey - Searching for Pulsating M Dwarfs. I}},
Journal = {{ACTA ASTRONOMICA}},
Year = {{2011}},
Volume = {{61}},
Number = {{1}},
Pages = {{37-58}},
Abstract = {{We present the first results of our M dwarf survey in search for stellar
   pulsation in low mass main sequence stars. Theoretical calculations
   predict that E mechanism might drive a fundamental radial mode in these
   stars and therefore pulsations could be observed photometrically.
   Although M dwarfs are known for their flare and spot activity they have
   not yet been the subject of dedicated time-series surveys for pulsation.
   In this work we include the light curves and amplitude spectra of 46 M
   dwarfs, which have been observed during the first two years of our
   survey. We did not detect any pulsations yet. As a by-product of our
   search, we describe the light curves of some flare M dwarfs. The survey
   will last for two more years and during that period more than a hundred
   of M0-M4 type main sequence stars will be observed.}},
