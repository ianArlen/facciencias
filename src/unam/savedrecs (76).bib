@inproceedings{ ISI:000305259300109,
Author = {Steffen, W. and Tamayo, F. and Koning, N.},
Editor = {{Henney, WJ and TorresPeimbert, S}},
Title = {{CRISS-CROSS MAPPING BD+30 3639: A NEW KINEMATIC ANALYSIS TECHNIQUE}},
