@article{ ISI:000292869000003,
Author = {Oubram, O. and Gaggero-Sager, L. M. and Navarro, O. and Ouadou, M.},
Title = {{TRANSPORT AND ELECTRONIC PROPERTIES OF THE GaAs ALD-FET}},
