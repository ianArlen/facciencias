@article{ ISI:000290288700003,
Author = {Karina Cuentas-Gallegos, Ana and Zamudio-Flores, Abraham and
   Casas-Cabanas, Montse},
Title = {{Dispersion of SiW12 Nanoparticles on Highly Oxidized Multiwalled Carbon
   Nanotubes and their Electrocatalytic Behavior}},
Journal = {{JOURNAL OF NANO RESEARCH}},
Year = {{2011}},
Volume = {{14}},
Pages = {{11-18}},
Abstract = {{Nanocomposite hybrid materials (NHM) based on the immobilization and
   dispersion of SiW12 polyanion (H4SiW12O40) on a highly oxidized
   multiwalled carbon nanobue matrix (NTS) are presented. The synthesis
   procedure followed is designed in order to increase the SiW12 particles
   concentration on the NHM. XRD, FTIR, N-2 adsorption isotherms, EDS, SEM,
   TEM, and cyclic voltammetry confirmed the presence of SiW12
   nanoparticles, whose microstructure and dispersion characteristics were
