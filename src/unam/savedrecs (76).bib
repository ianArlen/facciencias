@article{ ISI:000287947500008,
Author = {Grebert, Benoit and Villegas-Blas, Carlos},
Title = {{On the energy exchange between resonant modes in nonlinear Schrodinger
   equations}},
Journal = {{ANNALES DE L INSTITUT HENRI POINCARE-ANALYSE NON LINEAIRE}},
Year = {{2011}},
Volume = {{28}},
Number = {{1}},
Pages = {{127-134}},
Month = {{JAN-FEB}},
Abstract = {{We consider the nonlinear Schrodinger equation i psi(t) = -psi(xx) +/-
   2cos2x vertical bar psi vertical bar(2)psi, x is an element of S(1), t
   is an element of R and we prove that the solution of this equation, with
   small initial datum psi(0, x) = epsilon(A exp(ix) + Bexp(-ix)), will
   periodically exchange energy between the Fourier modes e(ix) and e(-ix)
   as soon as A(2) not equal B(2). This beating effect is described up to
   time of order epsilon(-5/2) while the frequency is of order epsilon(2).
   We also discuss some generalizations. (c) 2010 Elsevier Masson SAS. All
   rights reserved.}},
