Title = {{Lyapunov Approach for Analysis and Design of Second Order Sliding Mode
   Algorithms}},
Booktitle = {{SLIDING MODES AFTER THE FIRST DECADE OF THE 21ST CENTURY: STATE OF THE
   ART}},
Series = {{Lecture Notes in Control and Information Sciences}},
