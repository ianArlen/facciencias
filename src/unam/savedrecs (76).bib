@inproceedings{ ISI:000297496303018,
Author = {Marrufo, Liliana and Gonzalez, Fernando and Monsivais-Huertero,
   Alejandro and Ramos, Judith},
Book-Group-Author = {{IEEE}},
Title = {{SPATIO-TEMPORAL ESTIMATION OF SOIL MOISTURE IN A TROPICAL REGION USING A
   REMOTE SENSING ALGORITHM}},
Booktitle = {{2011 IEEE INTERNATIONAL GEOSCIENCE AND REMOTE SENSING SYMPOSIUM (IGARSS)}},
Series = {{IEEE International Symposium on Geoscience and Remote Sensing IGARSS}},
