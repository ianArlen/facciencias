@article{ ISI:A1994PM55400006,
Author = {HERNANDEZCRUZ, A and CHARLTON, MP and AUGUSTINE, GJ},
Title = {{PRESYNAPTIC CALCIUM SIGNALING DURING NEUROTRANSMITTER RELEASE -
   MEASUREMENTS WITH CONFOCAL LASER-SCANNING MICROSCOPY}},
Journal = {{BIOMEDICAL RESEARCH-TOKYO}},
Year = {{1994}},
Volume = {{15}},
Number = {{1}},
Pages = {{21-28}},
Abstract = {{Confocal laser-scanning microscopy offers a means of enhancing the
   resolution of fluorescence measurements of intracellular calcium levels.
   Here we illustrate that this method can be applied successfully to
   measurements at the squid `giant' synapse and yields new images of
   presynaptic calcium signaling. Continued use of this method should yield
   data capable of discriminating among various theoretical models that
   predict the magnitude and time course of the calcium signal that
   triggers transmitter release.}},
