@article{ ISI:A1994NN74000010,
Author = {RODRIGUEZ, LF and CANTO, J and TORRELLES, JM and GOMEZ, JF and ANGLADA,
   G and HO, PTP},
Title = {{SUBARCSECOND VLA1 MAPS OF THE DISK AND THE JET IN HL TAURI}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1994}},
Volume = {{427}},
Number = {{2, 2}},
Pages = {{L103-L106}},
Month = {{JUN 1}},
Abstract = {{High-angular resolution (approximately 0.3''-0.4'') VLA observations of
   HL Tau at 3.6 and 1.3 cm show extended structures associated with this
   young star that are strikingly different at each wavelength. While the
   structure observed at 3.6 cin is elongated approximately in the
   east-west direction, the 1.3 cm structure is elongated approximately in
   the north-south direction. We interpret these results to imply that
   while the 3.6 cm emission is dominated by free-free radiation from an
   ionized outflow, the 1.3 cm emission traces dust emission from a
   perpendicular, collimating disk. If our interpretation is correct, these
   VLA images represent the first subsecond images of both jet and disk in
   a young star.}},
