@article{ ISI:A1994PF42800002,
Author = {TROLTENIER, D and DRAAYER, JP and HESS, PO and CASTANOS, O},
Title = {{INVESTIGATIONS OF ROTATIONAL NUCLEI VIA THE PSEUDOSYMPLECTIC MODEL}},
Journal = {{NUCLEAR PHYSICS A}},
Year = {{1994}},
Volume = {{576}},
Number = {{3}},
Pages = {{351-386}},
Month = {{SEP 5}},
Abstract = {{After presenting an overview of the pseudo-symplectic model, the theory
   is used to investigate the properties of characteristic rotational
   nuclei of the rare-earth (Dy-160, Er-168) and actinide (U-234,236,238)
   regions. Calculated eigenstate energies, static quadrupole moments, and
   reduced E2-transition matrix elements are compared to the results of
   other models (IBM and GCM) and the experimental data. Limiting features
   of the current version of the pseudo-symplectic theory, such as
   assumptions regarding the role of intruder-level particles, are
   discussed.}},
