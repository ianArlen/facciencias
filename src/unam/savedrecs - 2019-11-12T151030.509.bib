@article{ ISI:A1994PR47000008,
Author = {PEIMBERT, M and SARMIENTO, A and COLIN, P},
Title = {{CHEMICAL EVOLUTION OF THE SOLAR NEIGHBORHOOD - YIELDS, BLACK-HOLES AND
   THE AMOUNT OF MASS IN SUBSTELLAR OBJECTS}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{1994}},
Volume = {{28}},
Number = {{2}},
Pages = {{181-186}},
Month = {{OCT}},
Abstract = {{We have computed the DELTAY/DELTAZ ratio, the oxygen yield, y(O), and
   the oxygen to other heavy elements yields ratio, y(O)/y(Z - O), expected
   for the solar neighborhood based on recent stellar evolution
   computations by Maedar and several initial mass functions, IMFs. We find
   that the computed values are in good agreement with recent observations
   only when the IMF derived by Kroupa, Tout, \& Gilmore is used. This
   agreement is obtained without invoking black hole production by massive
   stars. The implied mass density is substellar objects for the solar
   neighborhood, derived from chemical evolution considerations, is smaller
   than 0.02 M.pc-3. This result imposes a strong restriction on dynamical
   models of the solar neighborhood.}},
