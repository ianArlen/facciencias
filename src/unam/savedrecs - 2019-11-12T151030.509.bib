@article{ ISI:A1994NQ27300007,
Author = {JIMENEZMIER, J},
Title = {{CONTRIBUTION OF THE INSTRUMENT WINDOW FUNCTION TO THE PROFILE OF
   AUTOIONIZING RESONANCES}},
Journal = {{JOURNAL OF QUANTITATIVE SPECTROSCOPY \& RADIATIVE TRANSFER}},
Year = {{1994}},
Volume = {{51}},
Number = {{5}},
Pages = {{741-749}},
Month = {{MAY}},
Abstract = {{Analytic expressions for the convolution of Lorentzian and triangular
   instrumental profiles with Fano-Beutler resonance functions are
   presented. The effect of a Gaussian window is shown to be approximated
   very well by a triangular function. A systematic procedure to determine
   q-values, line centers and total widths from experimental curves is
   presented, and the parameters obtained are compared with the results of
   direct fits of experimental data. A Lorentzian function does not change
   the q parameter or the line center, and the instrument width adds
   linearly to the natural width. A triangular profile alters the q
   parameter, the line center and the total width of the measured function.}},
