@article{ ISI:A1994PB73400011,
Author = {CORKIDI, G and MARQUEZ, J and GARCIARUIZ, M and DIAZCINTRA, S and GRAUE,
   E},
Title = {{CARTOGRAPHIC SYSTEM FOR SPATIAL-DISTRIBUTION ANALYSIS OF CORNEAL
   ENDOTHELIAL-CELLS}},
