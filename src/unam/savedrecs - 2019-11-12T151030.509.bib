@inproceedings{ ISI:A1994BC20N00084,
Author = {CASTILLO, R and AWASARKAR, PA and PAPADOPOULOU, C and ACOSTA, D and
   RUIZ, P},
Editor = {{Corberan, VC and Bellon, SV}},
Title = {{CREATION OF NEW SELECTIVE SITES BY SPILL-OVER OXYGEN VIA ALPHA-SB2O4 IN
   THE OXIDATION OF ETHANOL}},
Booktitle = {{NEW DEVELOPMENTS IN SELECTIVE OXIDATION II}},
Series = {{STUDIES IN SURFACE SCIENCE AND CATALYSIS}},
Year = {{1994}},
Volume = {{82}},
Pages = {{795-802}},
Note = {{2nd World Congress/4th European Workshop Meeting on New Developments in
   Selective Oxidation II, BENALMADENA, SPAIN, SEP 20-24, 1993}},
Organization = {{REAL SOCIEDAD ESPANOLA QUIM, GRUPO ESPECIALIZASO CATALISIS; CONSEJO SUP
   INVESTIGACIONES CIENT, INST CATALISIS \& PETR}},
Abstract = {{Two system were studied. The oxidative dehydrogenation of ethanol to
   acetaldehyde over Fe-2(MoO4)(3) + alpha-Sb2O4 catalysts and the
   oxidation of ethanol to acetic acid over SnO2 + MoO3 + alpha-Sb2O4
   catalysts. Catalysts were formed by mechanical mixtures. In catalysts
   containing alpha-Sb2O4 an important increase in the selectivity was
   observed. To explain the synergy it is proposed that alpha-Sb2O4
   produces spill-over oxygen which creates Cor maintains) selective sites
   on Fe-2(MoO4)(3) and on SnO2 + MoO3. Two mechanisms could explain the
   creation of selective sites by spill-over oxygen : i) maintaining the
   surface of Fe-2(MoO4)(3) in a high oxidation state, avoiding the
   segregation of reduced phases and ii) promoting the cristallization of
   SnO2 + MoO3 mixtures, avoiding reduction and degradation of MoO3.}},
