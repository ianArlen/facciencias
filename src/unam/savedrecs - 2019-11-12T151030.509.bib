@article{ ISI:A1994PG47400020,
Author = {ZERTUCHE, F and LOPEZPENA, R and WAELBROECK, H},
Title = {{RECOGNITION OF TEMPORAL SEQUENCES OF PATTERNS USING STATE-DEPENDENT
   SYNAPSES}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{1994}},
Volume = {{27}},
Number = {{17}},
Pages = {{5879-5887}},
Month = {{SEP 7}},
Abstract = {{Using an asymmetric associative network with synchronous updating, it is
   possible to recall a sequence of patterns. To obtain a stable sequence
   generation with a large storage capacity, we introduce a threshold that
   elminates the contribution of weakly correlated patterns. For this
   system we find a set of evolution equations for the overlaps of the
   states with the patterns to be recognized. We solve these equations in
   the limit of the stationary cycle, and obtain the critical value of the
   capacity as a function of the threshold and temperature. Finally, a
   numerical simulation is made, confirming the theoretical results.}},
