@article{ ISI:A1994NZ63000006,
Author = {MUNGUIA, AL and RODARTE, CW and ROLZ, C and MOSER, A},
Title = {{INDIGENOUS TECHNOLOGICAL INTEGRATION AND MODERN BIOTECHNOLOGIES - AN
   UTOPIA}},
