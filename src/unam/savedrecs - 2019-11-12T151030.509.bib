@article{ ISI:A1994MW74000001,
Author = {MIRABEL, IF and DUC, PA and RODRIGUEZ, LF and TEYSSIER, R and PAUL, J
   and CLARET, A and AURIERE, M and GOLOMBEK, D and MARTI, J},
Title = {{THE RADIO INFRARED COUNTERPART OF THE HARD X-RAY TRANSIENT IN AQUILA - A
   POSSIBLE SOURCE OF REPEATED SOFT GAMMA-RAY BURSTS}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{1994}},
Volume = {{282}},
Number = {{2}},
Pages = {{L17-L20}},
Month = {{FEB}},
Abstract = {{We report the discovery of the radio and infrared counterpart of the
   hard X-ray transient GRS1915+105 in the constellation of Aquila. The
   compact object is located beyond approximately 30 magnitudes of optical
   absorption. At radio wavelengths it exhibits drastic time variations in
   flux which are correlated with changes in the hard X-rays. The source
   also shows unusual variability in the H(1.65/mum) and K(2.2mum) infrared
   bands.
   We propose that in the course of its slow rise to maximum intensity, GRS
   1915+105 may have triggered three soft gamma-ray bursts of short
   duration that were observed from the same region of the sky during
   June-August 1992.}},
