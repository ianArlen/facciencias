@article{ ISI:A1994NF14000030,
Author = {RODRIGUEZFERNANDEZ, L and MIRANDA, J and OLIVER, A},
Title = {{L-SUBSHELL X-RAY-PRODUCTION BY 400-700 KEV PROTONS IN SELECTED ELEMENTS
   WITH 52-LESS-THAN-OR-EQUAL-TO-Z-LESS-THAN-OR-EQUAL-TO-71}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{1994}},
Volume = {{85}},
Number = {{1-4}},
Pages = {{150-153}},
Month = {{MAR}},
Note = {{11th International Conference on Ion Beam Analysis (IBA-11),
   BALATONFURED, HUNGARY, JUL 05-09, 1993}},
Organization = {{KFKI RES INST MAT SCI; KFKI RES INST PARTICLE \& NUCL PHYS; COMMISS
   EUROPEAN COMMUNITIES; IBA-10 NETHERLANDS; NATL COMM TECHNOL DEV HUNGARY;
   HUNGARIAN ACAD SCI}},
Abstract = {{The L-shell X-ray production cross section for proton impact of thin
   film targets of some elements with 52 less-than-or-equal-to Z
   less-than-or-equal-to 71 have been measured in the energy range 400-700
   keV.  Corrections due to proton stopping and X-ray attenuation in the
   films are taken into account.  Experimental results are compared with
   the ECPSSR theory using the relativistic reduced proton velocity xiLiR. 
   The L(alpha) line is also compared with a semiempirical function based
   on a fit to the ratios of published experimental data with the ECPSSR
   predictions.  Present experimental results are higher than the
   theoretical predictions, in accordance with previous results, except for
