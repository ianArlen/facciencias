@article{ ISI:A1994NR22900026,
Author = {PEREZ, R and JUAREZISLAS, JA and MARTINEZ, L},
Title = {{STRUCTURE TRANSFORMATIONS OF QUASI-CRYSTALLINE PHASES IN QUATERNARY
   ALLOYS}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{1994}},
Volume = {{181}},
Pages = {{823-827}},
Month = {{MAY 15}},
Abstract = {{We report experimental results related to transformations of the
   quasicrystalline phases in alloys of Al-Cu-Co-Fe to crystalline
   structures. The alloys were fabricated by normal casting methods under
   inert gas atmospheres. Icosahedral and decagonal quasicrystalline phases
   were found to coexist. Annealing induces transformation from
   quasicrystalline to crystalline structures. Evidence of crystallization
   is clearly obtained from electron diffraction patterns along the
   two-fold axes. Low magnification dark field images suggest that twins
   are formed by the deformation mechanism associated with the
   transformation.}},
