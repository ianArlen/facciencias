Title = {{ISOLATION AND RELAY SYNTHESIS OF 11-ALPHA-HYDROPEROXY DIACETYL
   HEDERAGENIN, A NOVEL TRITERPENOID DERIVATIVE FROM SERJANIA-TRIQUETRA
   (SAPINDACEAE) - BIOGENETIC IMPLICATIONS}},
Journal = {{TETRAHEDRON}},
Year = {{1994}},
Volume = {{50}},
