@article{ ISI:A1994PC32100003,
Author = {DIAZ, JMM and CISNEROS, AC and ALONSO, VL and OCAMPO, MT and RAYON, GA
   and AREVALO, RV and CASTILLO, LO and LEDEZMA, SR},
Title = {{EFFECTS OF 5-HDLTP ON DIETARY SELF-SELECTION IN OVEREATING RATS}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1994}},
Volume = {{11}},
Number = {{1}},
Pages = {{25-32}},
Month = {{JUN}},
Abstract = {{Recent research has suggested that self-selection of carbohydrates in
   rats may be mediated by serotonin (5-HTP). Since drugs enhancing
   serotonergic transmission reduce carbohydrate selection by sparing
   proteins, a mechanism depending on brain serotonin levels has been
   suggested to control carbohydrate intake. In this study the precursor of
   serotonin 5-hydroxytryptophan (5-HTP) was injected (i.p) to overeating
   rats depending on 5-HTP, who were offered separate sources of nutriments
   to see if the mechanism also acted with animals on overeat regime.
   Animals selected the expected amounts of either nutriment in the overeat
   regime. When 5-HTP was injected, a significant decrease in the
   self-selection of carbohydrate was observed and the protein sparing
   effect was not found. Results of the present study agree with previous
   data suggesting that carbohydrate self-selection is mediated by
   serotonin.}},
