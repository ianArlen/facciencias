@article{ ISI:A1994PF69000003,
Author = {RIVERA, RE and BESSIERE, J},
Title = {{IONIC FLOTATION AND GALLIUM EXTRACTION IN ALKALINE-MEDIUM (PH 11,0) WITH
   8-HYDROXYQUINALDINE}},
Journal = {{AFINIDAD}},
Year = {{1994}},
Volume = {{51}},
Number = {{452}},
Pages = {{272-276}},
Month = {{JUL-AUG}},
Abstract = {{The ionic flotation is established as an alternative technique for the
   recovery metallic values. The ionic flotation was studied and the
   gallium precipitation reaction was distinguished at pH 11.0 with the
   8-hydroxyquinaldine. For a value of {[}MeOx-]/{[}Ga(III)]=4 the
   precipitation is complete and the ionic flotation revovery is nearly
   90\%. For a ratio of 12 the flotation is complete. The conditional
   solubility product constant value at pH 11.0 non buffered is 1 x
   10(-11.6) with the same pH but in a buffer medium is 1 x 10(-7). Ionic
   flotation and solvent extraction have been compared. For a value of 4
   the cloroform extraction yields 5\% and with querosene 2\%. For a ratio
   of 20 the cloroform extraction hardly reaches 50\%.}},
