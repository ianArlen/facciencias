@article{ ISI:A1994PZ39600008,
Author = {SANCHEZFLORES, A},
Title = {{ON TOURNAMENTS AND THEIR LARGEST TRANSITIVE SUBTOURNAMENTS}},
Journal = {{GRAPHS AND COMBINATORICS}},
Year = {{1994}},
Volume = {{10}},
Number = {{4}},
Pages = {{367-376}},
Abstract = {{In this work we present some results in connection with the following
   problem, posed by Erdos and Moser:  For each positive integer n,
   determine the greatest integer v(n) such that all tournaments of order n
   contain the transitive subtournament of order v(n) (denoted TT(v(n)). 
   It is known that v(n) = 3 for 4 less-than-or-equal-to n
   less-than-or-equal-to 7, v(n) = 4 for 8 less-than-or-equal-to n
   less-than-or-equal-to 13, v(n) = 5 for 14 less-than-or-equal-to n
   less-than-or-equal-to 27, and v(n) less-than-or-equal-to 6 for n > 27. 
   Moreover, the uniqueness of the tournaments of orders 7 and 13, free of
   TT4 and TT5, respectively, has been proved.  Here we prove that there
   exists only one tournament of order 27 free of TT6 and that every
   tournament of order 55 contains TT7, which implies v(n)
   greater-than-or-equal-to left perpendicularlog2(n/55)right perpendicular
   + 7, for n greater-than-or-equal-to 55, improving the best lower bound
   known for v(n).}},
