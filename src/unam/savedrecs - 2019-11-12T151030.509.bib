@article{ ISI:A1994QN59400041,
Author = {RENDONRODRIGUEZ, S},
Title = {{RELATION BETWEEN MOLTING AND REPRODUCTION IN ABLATED
   PENAEUS-STYLIROSTRIS FEMALES (DECAPODA, PENAEIDAE) UNDER LABORATORY
   CONDITIONS}},
Journal = {{REVISTA DE BIOLOGIA TROPICAL}},
Year = {{1994}},
Volume = {{42}},
Number = {{1-2}},
Pages = {{343-345}},
Month = {{APR-AUG}},
Abstract = {{Unilaterally eyestalk-ablationed females of Penaeus stylirostris showed
   ovaries undeveloped in the early postmolt and late premolt stages. The
   molt cycle duration in days was: posmolt (A, B) 4.29 +/- 0.47 (27.1\% of
   the cycle); intermolt (C) 5.14 +/- 1.51 (32.4\%), and premolt (D) 6.43
   +/- 1.09 (40.5\%). Most reproductive activity was during the intermolt,
   when 75\% of the spawns were obtained. The number of spawns in a molt
   cycle was significantly correlated (r= 0.89{*}{*}) with the duration of
   the intermolt stage.}},
