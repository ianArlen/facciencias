@article{ ISI:A1994NF79600001,
Author = {COCHO, G and JAUREGUI, R and MORENO, M and VILLARREAL, C},
Title = {{LORENTZ STRUCTURE OF THE CORNELL POTENTIAL}},
Journal = {{NUOVO CIMENTO DELLA SOCIETA ITALIANA DI FISICA A-NUCLEI PARTICLES AND
   FIELDS}},
Year = {{1994}},
Volume = {{107}},
Number = {{3}},
Pages = {{341-352}},
Month = {{MAR}},
Abstract = {{Quasi-relativistic variational calculations supplemented by the virial
   theorem are used to improve the determination of the Lorentz structure
   of the quark-antiquark potential in quarkonium. No satisfactory solution
   exists for a purely vectorlike internal 1/r and a purely scalar external
   r interactions. We present the result of assuming an admixture of
   scalar, pseudoscalar and vector Lorentz effective potentials. Solutions
   with and without a pseudoscalar potential are found for charmonium and
   bottomonium, within a 1 MeV precision. The toponium spectra for m(t) =
   150 GeV top quark are reported.}},
