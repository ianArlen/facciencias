@article{ ISI:A1994PC32100006,
Author = {VELASCO, CS and ARUMBURU, MCE and MENDEZ, GB},
Title = {{EXTENSION OF THE OBSERVATIONAL AND BEHAVIORAL SYSTEM OF
   SOCIAL-INTERACTION - QUALITY, DIRECTION, CONTENTS AND RESOLUTION}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1994}},
Volume = {{11}},
Number = {{1}},
Pages = {{55-68}},
Month = {{JUN}},
Abstract = {{The observational and behavioral system of social interaction is an
   instrument which permits the systematic study of individual and social
   behavior of subjects in their natural setting. The first section of this
   paper, discusses the methodological importance and conceptual
   implications derived of their use in the context of the development of
   models for the study and prediction of social interchanges. The
   following section, describes the uses and characteristics of the system
   regarding some investigations where the system was used. The third
   section describes some limitations of previous versions of the
   instrument. These limitations justify the extension of the system toward
   the inclusion of information about quality, contents, direction and
   context of social interaction. Each concept is defined and some examples
   of decision are presented. Finally, some implications of the extensions
   of the system are discussed including one appendix of the actual system
   of categories and some examples of segmentation for data analysis.}},
