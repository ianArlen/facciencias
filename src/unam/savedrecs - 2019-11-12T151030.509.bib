@article{ ISI:A1994NN99300092,
Author = {ROMO, R and GARCIACALDERON, G},
Title = {{STRONG OVERLAP AND TRANSMISSION IN TRIPLE-BARRIER RESONANT STRUCTURES}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{1994}},
Volume = {{49}},
Number = {{19}},
Pages = {{14016-14019}},
Month = {{MAY 15}},
Abstract = {{The transition from two peaks of unit height to a single peak, with
   height less than one, in the transmission coefficent T of symmetrical
   triple-barrier resonant structures, as the width of the central barrier
   is increased, is analyzed in terms of a resonance formalism in a
   Breit-Wigner fashion to show that the above situation is due to strong
   overlap between the two resonance states of the corresponding miniband,
   the condition for this being (epsilon2 - epsilon1) less-than-or-equal-to
   {[}GAMMA1GAMMA2]1/2. We also discuss the implications of strong overlap
   on the current-voltage characteristics of these triple-barrier
   structures.}},
