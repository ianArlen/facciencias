@article{ ISI:A1994PF62400004,
Author = {FLORES, C and GARCIA, J and HERNANDEZ, J and BOKHIMI and MURRIETA, H and
   ESCUDERO, R},
Title = {{FLUORESCENCE STUDY OF EU-BA-CU-O TYPE COMPOUNDS}},
Journal = {{JOURNAL OF LUMINESCENCE}},
Year = {{1994}},
Volume = {{59}},
Number = {{4}},
Pages = {{257-262}},
Month = {{MAY}},
Abstract = {{A study of the excitation and fluorescence spectra of the high
   temperature superconductor EuBa2Cu3O7-x, the green phase Eu2BaCuO5, and
   the insulating phase EuCuO2 has been performed at room temperatures as
   well as at low temperatures. The spectra of the trivalent europium ion
   in these compounds show a similar fluorescence structure when the F-7(0)
   --> 5D2 transition is excited. On the other hand, these compounds also
   present a structured emission band characteristics of the Cu+ ion in low
   symmetry sites. From a comparison with the corresponding spectra of the
   EuCuO2 compound it can be concluded that this is present as an impurity
   phase in the superconductor as well as in the green phase, giving rise
   to the observed spectra.}},
