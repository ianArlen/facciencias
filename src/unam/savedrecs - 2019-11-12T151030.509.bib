@article{ ISI:A1994NR99200046,
Author = {MOCHAN, WL and BARRERA, RG and BORENSZTEIN, Y and TADJEDDINE, A},
Title = {{OPTICAL REFLECTANCE ANISOTROPY OF AG AND AU (110) SINGLE-CRYSTALS}},
