@article{ ISI:A1994MV31900008,
Author = {GARCIA, DE and CAVALIE, A and LUX, HD},
Title = {{ENHANCEMENT OF VOLTAGE-GATED CA2+ CURRENTS INDUCED BY DAILY STIMULATION
   OF HIPPOCAMPAL-NEURONS WITH GLUTAMATE}},
