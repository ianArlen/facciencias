@article{ ISI:A1994NH32200012,
Author = {ALVAREZCASTILLO, A and CASTANO, VM},
Title = {{SEMIINTERPENETRATING POLYMER NETWORKS PRODUCED WITH POLYETHYLENE
   TEREPHTHALATE OLIGOMER AND UNSATURATED POLYESTER RESIN}},
