@article{ ISI:A1994MN42600059,
Author = {PIZIO, OA},
Title = {{ANALYTIC SOLUTION OF THE RESTRICTED PRIMITIVE MODEL OF
   ELECTROLYTE-SOLUTIONS WITH DOUBLE STICKY INTERACTION BETWEEN THE
   OPPOSITELY CHARGED IONS}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{1994}},
Volume = {{100}},
Number = {{1}},
Pages = {{548-555}},
Month = {{JAN 1}},
Abstract = {{A model for the description of the association phenomena and chemical
   reactions in the ionic fluids is considered. It generalizes the sticky
   electrolyte model and provides simultaneous formation of species with
   different bond length. The interaction between the oppositely charged
   ions is characterized by the presence of two narrow and deep attractive
   square wells inside a hard-core region. It determines the double sticky
   electrolyte model (DSEM). Application of the simple interpolation scheme
   by Zhou and Stell and the approximation by Rasaiah and Zhu for the
   cavity function is investigated for DSEM in order to provide the mass
   action limiting law. Analytic solution for the DSEM is presented.}},
