@article{ ISI:A1994NG74300011,
Author = {HEREDIAZAVONI, E and VANMARCKE, EH},
Title = {{SEISMIC RANDOM-VIBRATION ANALYSIS OF MULTISUPPORT-STRUCTURAL SYSTEMS}},
Journal = {{JOURNAL OF ENGINEERING MECHANICS-ASCE}},
Year = {{1994}},
Volume = {{120}},
Number = {{5}},
Pages = {{1107-1128}},
Month = {{MAY}},
Abstract = {{A random-vibration methodology is formulated for the seismic-response
   analysis of linear multisupport-structural systems. It reduces the
   response evaluation to that of a series of linear one-degree systems in
   a way that fully accounts for the multiple-support input and the
   space-time correlation structure of the ground motion. Three spectral
   parameters are introduced and interpreted in terms of cross-correlation
   coefficients and spectral moments associated with the response of
   one-degree systems. The influence of local-spatial variation of ground
   motion on the spectral parameters is also studied.}},
