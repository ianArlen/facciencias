@article{ ISI:A1994NR06100030,
Author = {COCHO, G and GELOVERSANTIAGO, A and MARTINEZMEKLER, G and RODIN, A},
Title = {{NONLINEAR MODELING OF THE AIDS VIRUS GENETIC SEQUENCE EVOLUTION}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C-PHYSICS AND COMPUTERS}},
Year = {{1994}},
Volume = {{5}},
Number = {{2}},
Pages = {{321-324}},
Month = {{APR}},
Note = {{2nd IMACS Conference on Computational Physics, ST LOUIS, MO, OCT 06-09,
   1993}},
Organization = {{INT ASSOC MATH \& COMP SIMULAT; ST LOUIS UNIV, PARKS COLL}},
Abstract = {{A network of coupled maps is introduced to model the evolution of the
   genetic sequence of the HIV1 AIDS virus. Within a space of RNA chemical
   composition, short range interactions correspond to mutations.
   Ecological constraints generate long range couplings. The resulting
   equations are of a reaction-diffusion type. Quasi-species with an error
   threshold emerge from the model dynamics. Predictions relating chemical
   composition regularity properties with the variability of the HIV RNA
   sequence agree with a statistical analysis from gene data banks. The
   model suggests a clue for an alternative therapeutical treatment.}},
