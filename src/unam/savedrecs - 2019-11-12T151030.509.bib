@article{ ISI:A1994PB59200028,
Author = {ALVAREZ, RN and BONATSOS, D and SMIRNOV, YF},
Title = {{Q-DEFORMED VIBRON MODEL FOR DIATOMIC-MOLECULES}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{1994}},
Volume = {{50}},
Number = {{2, A}},
Pages = {{1088-1095}},
Month = {{AUG}},
Abstract = {{A deformed version of the vibron model for diatomic molecules is
   constructed. Both the O(4) and U(3) dynamical symmetries of the model
   are rewritten, using the concept of complementary subalgebras, in a more
   convenient form, which is subsequently deformed. The present model
   unifies the so far independent successful quantum-algebraic approaches
   to rotational and to vibrational spectra of diatomic molecules. In
   addition, the method can be used for the construction of deformed
   versions of the U(5) and O(6) limits of the interacting boson model of
   nuclear structure.}},
