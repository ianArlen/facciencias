@article{ ISI:A1994NQ56200012,
Author = {GARCIAFERREIRA, S and GARCIAMAYNEZ, A},
Title = {{ON WEAKLY-PSEUDOCOMPACT SPACES}},
Journal = {{HOUSTON JOURNAL OF MATHEMATICS}},
Year = {{1994}},
Volume = {{20}},
Number = {{1}},
Pages = {{145-159}},
Abstract = {{A space X is weakly-pseudocompact if X is G(delta)-dense in one of its
   compactifications. This concept generalizes, in a very natural way,
   pseudocompactness. In this paper, we study weakly-pseudocompact spaces.
   It is shown that a space X is compact iff X is Liudelof and
   weakly-pseudocompact. If X is locally compact, then X is
   weakly-pseudocompact iff X is either compact or it is not Lindelof;
   every weakly-pseudocompact metric space is completely metrizable; and
   the hedgehog J(alpha), for omega < alpha, is a weakly-pseudocompact
   space. We introduce and study M-bounded, M-compact, for empty-set
   not-equal M subset-or-equal-to C(X), and C(B(X))-embedded subsets of a
   space X: these concepts are generalizations of some notions that have
   been considered by several people. We also prove that a space X is
   pseudocompact iff the set of P-points of X and Fr(Z), for each zero set
   of X, are bounded, and the real numbers R cannot be embedded as a zero
   set in a pseudocompact space.}},
