@article{ ISI:A1994PC32100005,
Author = {GOMEZMAQUEO, EL and LAGUNES, IR},
Title = {{NEW VERSION OF THE MINNESOTA MULTIPHASIC PERSONALITY-INVENTORY MMPI-2
   FOR MEXICAN COLLEGE-STUDENTS}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1994}},
Volume = {{11}},
Number = {{1}},
Pages = {{45-54}},
Month = {{JUN}},
Abstract = {{The purpose of this study was to adapt and establish the norms of MMPI-2
   for Mexican students. This is the first study carried out in Mexico
   using the revised version of the inventory. The instrument was applied
   to a random sample (N = 1920) of university students at UNAM. The
   revised version of the inventory was translated into Spanish adapting
   language and item content to Mexican populations. Rather than a literal
   translation, it is a transliteration. Significant statistical
   differences between raw scores of Mexican students and the normative
   U.S. population were found. Nevertheless norms for both populations are
   similar, but some personality differences were detected, related perhaps
   to cultural issues. Profiles of Mexican students tend to normality,
   which was not the case with the original MMPI. The Mexican Spanish
   version of MMPI-2 seems to be more adequate than the original MMPI
   Spanish version.}},
