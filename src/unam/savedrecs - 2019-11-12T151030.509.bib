@article{ ISI:A1994NN49200004,
Author = {ROBLEDO, A and INDEKEU, JO},
Title = {{UNIVERSALITY AND THE CONTACT LINE AT 1ST-ORDER WETTING TRANSITIONS}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{1994}},
Volume = {{25}},
Number = {{1}},
Pages = {{17-22}},
Month = {{JAN 1}},
Abstract = {{We study the three-phase contact line L and its tension tau near the
   interfacial phase transition from partial to complete wetting. We
   analyse the singularity of tau at first-order wetting transitions and
   show that it displays the universal features of a critical endpoint.
   These are most notably represented by novel amplitude ratios for tau,
   but also, this identification explains the extreme spatial extent and
   asymmetry of the line inhomogeneity. We determine universal amplitude
   ratios for both short-range and long-range forces from an interface
   displacement model. We focus on the mean-field regime, valid for
   three-dimensional van der Waals systems.}},
