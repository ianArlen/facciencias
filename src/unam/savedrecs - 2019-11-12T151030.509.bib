@article{ ISI:A1994NB36800005,
Author = {CASTILLO, S and RAMIREZSOLIS, A and DIAZ, D and POULAIN, E and NOVARO, O},
Title = {{THEORETICAL-STUDY OF THE ACTIVATION OF METHANE BY PHOTOEXCITED ZINC
   ATOMS}},
Journal = {{MOLECULAR PHYSICS}},
Year = {{1994}},
Volume = {{81}},
Number = {{4}},
Pages = {{825-836}},
Month = {{MAR}},
Abstract = {{The Zn+ CH4 reaction has been studied through self-consistent field
   followed by extensive variational and perturbational second-order Moller
   Plesset multi-reference configuration interaction (CIPSI) calculations
   using extended Gaussian basis sets. Results indicate that for the
   formation of the HZnCH3 intermediate a previous excitation of Zn(1S) to
   its (1P) excited state is necessary. The following mechanism is
   proposed: the Zn(1P) + CH4 reactants follow the 2A' attractive surface
   until they reach the originally repulsive 1A' surface originating from
   the Zn (1S) + CH4 reactants. An avoided crossing is generated implying a
   non-adiabatic transition. When the 1A' surface reaches the avoided
   crossing it becomes attractive leading to the C3v HZnCH3 intermediate
   which is stable and needs 63.9 kcal mol-1 to yield, without an
   activation barrier, the HZn+CH3 products or 69.3 kcal mol-1 to yield the
