@article{ ISI:000167644100009,
Author = {Chavez, B and Vilchis, F and Zenteno, JC and Larrea, F and
   Kofman-Alfaro, S},
Title = {{Novel molecular defects in the androgen receptor gene of Mexican
   patients with androgen insensitivity}},
Journal = {{CLINICAL GENETICS}},
Year = {{2001}},
Volume = {{59}},
Number = {{3}},
Pages = {{185-188}},
Month = {{MAR}},
Abstract = {{The androgen insensitivity syndrome (AIS) is an X-linked form of male
   pseudohermaphroditism caused by mutations in the androgen receptor (AR)
   gene. In the present study, we analyzed the AR gene in 8 patients, 4
   sporadic and 2 familial cases with the syndrome, using exon-specific
   polymerase chain reaction, single-stranded conformational polymorphism
   and sequencing analysis and identified six new single base mutations,
   including one nonsense mutation at the hinge region of the receptor.
   These molecular lesions occurred in the steroid-binding domain (SBD) and
   all but one affected the first nucleotide of their respective codons. A
   nonsense mutation in exon 4, which converts a glutamine into a premature
   termination signal (Q657stop), a missense mutation changing arginine
   instead of glycine (G743R) and a conservative substitution of leucine
   with valine at amino acid 830 (L830V) were detected in patients with
   CAIS. Three other missense mutations located in exons 4 (L701I), 5
   (A765S), and 6 (Q802R) were present in individuals bearing a partial
   form of AIS. These data allow us to reaffirm the view that nonsense
   mutations in the AR results almost invariably in a CAIS phenotype and
   underly the importance of the SBD for the AR functional activity.}},
