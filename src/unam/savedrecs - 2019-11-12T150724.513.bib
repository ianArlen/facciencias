@article{ ISI:000168783300014,
Author = {de Urquijo, J and Basurto, E},
Title = {{Electron attachment, ionization and drift in c-C4F8}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{2001}},
Volume = {{34}},
Number = {{9}},
Pages = {{1352-1354}},
Month = {{MAY 7}},
Abstract = {{The pulsed Townsend method has been used to measure the drift velocity
   nu (e) and the density-normalized effective ionization coefficient
   (alpha - eta)/N, (alpha and eta are the ionization and attachment
   coefficients, respectively) in c-C4F8 over the density-normalized
   electric field strength E/N, 12 Td less than or equal to E/N less than
   or equal to 43 Td and 330 Td less than or equal to E/N less than or
   equal to 600 Td (1 Td = 10(-17) V cm(2)), at pressures between 1 and 7.5
   Torr(l Ton = 133.3 Pa). For 12 Td less than or equal to E/N less than or
   equal to 43 Td, the above parameters were found to be pressure
   independent, while for the range 330 Td less than or equal to E/N less
   than or equal to 600 Td an inverse dependence of the above coefficients
   was found for gas pressures less than 2 Ton: Such dependence is believed
   to be due to the autodetachment of the originally formed, unstable
   parent negative ion c-C4F8-{*}. At low E/N, no previous data for nu (e)
   or (alpha - eta)/N were found for comparison with the present data. A
   critical field strength of E/N-crit = 439.5 Td, for which alpha = eta,
   was found to be in good agreement with previous data.}},
