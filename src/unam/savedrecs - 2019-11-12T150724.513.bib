@article{ ISI:000169467800011,
Author = {Calderon, JS and Cespedes, CL and Rosas, R and Gomez-Garibay, F and
   Salazar, JR and Lina, L and Aranda, E and Kubo, I},
Title = {{Acetylcholinesterase and insect growth inhibitory activities of
   Gutierrezia microcephala on fall armyworm Spodoptera frugiperda J. E.
   Smith}},
Journal = {{ZEITSCHRIFT FUR NATURFORSCHUNG C-A JOURNAL OF BIOSCIENCES}},
