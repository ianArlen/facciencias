@article{ ISI:000168718300004,
Author = {Moreno-Hagelsieb, G and Trevino, V and Perez-Rueda, E and Smith, TF and
   Collado-Vides, J},
Title = {{Transcription unit conservation in the three domains of life: a
   perspective from Escherichia coli}},
Journal = {{TRENDS IN GENETICS}},
Year = {{2001}},
Volume = {{17}},
Number = {{4}},
Pages = {{175-177}},
Month = {{APR}},
Abstract = {{Here we address the question of the degree to which genes within
   experimentally characterized operons in one organism (Escherichia coli)
   are conserved in other genomes. We found that two genes adjacent within
   an operon are more likely both to have an ortholog in other organisms,
   regardless of relative position, than genes adjacent on the same strand
   but in two different transcription units. They are also more likely to
   occur next to, or fused to, one another in other genomes. Genes
   frequently conserved adjacent to each other, especially among
   evolutionarily distant species, must be part of the same transcription
   unit in most of them.}},
