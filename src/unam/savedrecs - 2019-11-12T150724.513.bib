@article{ ISI:000168879000012,
Author = {Castro, B and Whitcombe, MJ and Vulfson, EN and Vazquez-Duhalt, R and
   Barzana, E},
Title = {{Molecular imprinting for the selective adsorption of organosulphur
   compounds present in fuels}},
Journal = {{ANALYTICA CHIMICA ACTA}},
Year = {{2001}},
Volume = {{435}},
Number = {{1}},
Pages = {{83-90}},
