@article{ ISI:000167346000015,
Author = {Durdevich, M and Makaruk, HE and Owczarek, R},
Title = {{Generalized noiseless quantum codes utilizing quantum enveloping
   algebras}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{2001}},
Volume = {{34}},
Number = {{7}},
Pages = {{1423-1437}},
Month = {{FEB 23}},
Abstract = {{A generalization of the results of Rasetti and Zanardi concerning
   avoiding errors in quantum computers by using states preserved by
   evolution is presented. The concept of dynamical symmetry is generalized
   from the level of classical Lie algebras and groups, to the level of a
   dynamical symmetry based on quantum Lie algebras and quantum groups (in
   the sense of Woronowicz). An intrinsic dependence of the concept of
   dynamical symmetry on the differential calculus (which holds also in the
   classical case) is stressed. A natural connection between quantum states
   invariant under a quantum group action, and quantum states preserved by
   the dynamical evolution, is discussed.}},
