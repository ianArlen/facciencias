@article{ ISI:000169553600007,
Author = {Macias, RDF},
Title = {{Strategic instruction for students with learning disabilities}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2001}},
Volume = {{18}},
Number = {{2}},
Pages = {{247-256}},
Month = {{JUN}},
Abstract = {{The purpose of the present study was to examine some core aspects of the
   relation between strategic instruction and support for educational needs
   of students with learning disabilities. The analysis first covers some
   cognitive and motivational characteristics of students with learning
   disabilities as evaluated in terms of the actual learning strategies
   demanded by their academic environment. The analysis then proceeds to
   examine factors comprising adequate instructional situations leading to
   the development of effective learning strategies. Finally, a series of
   considerations address the application of strategic instruction
   procedures in the classroom.}},
