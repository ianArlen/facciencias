@article{ ISI:000166909800006,
Author = {Palomares, LA and Pedroza, JC and Ramirez, OT},
Title = {{Cell size as a tool to predict the production of recombinant protein by
   the insect-cell baculovirus expression system}},
Journal = {{BIOTECHNOLOGY LETTERS}},
Year = {{2001}},
Volume = {{23}},
Number = {{5}},
Pages = {{359-364}},
Month = {{MAR}},
Abstract = {{The increase of Sf9 cell diameter after infection with a recombinant
   baculovirus encoding VP8 protein of rotavirus can be used to predict
   culture productivity. A direct proportional correlation between the
   increase in cell size and VP8 concentration was obtained when
   manipulating selected medium components. Only yeast extract increased
   (38\%) VP8 concentration, while fetal bovine serum increased (55\%) the
   maximum cell concentration. An inexpensive and simplified culture media
   can thus be designed without detriment to protein yields.}},
