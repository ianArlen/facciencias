@article{ ISI:000167882000011,
Author = {Ashtekar, A and Corichi, A and Sudarsky, D},
Title = {{Hairy black holes, horizon mass and solitons}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{2001}},
Volume = {{18}},
Number = {{5}},
Pages = {{919-940}},
Month = {{MAR 7}},
Abstract = {{Properties of the horizon mass of hairy black holes are discussed with
   emphasis on certain subtle and initially unexpected features. A key
   property suggests that hairy black holes may be regarded as `bound
   states' of ordinary black holes (without hair) and coloured solitons.
   This model is then used to predict the qualitative behaviour of the
   horizon properties of hairy black holes, to provide a physical
   `explanation' of their instability and to put qualitative constraints on
   the end-point configurations that result from this instability. The
   available numerical calculations support these predictions. Furthermore,
   the physical arguments are robust and should also be applicable in more
   complicated situations where detailed numerical work is yet to be
   carried out.}},
