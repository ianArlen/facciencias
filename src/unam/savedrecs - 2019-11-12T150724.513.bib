@article{ ISI:000167897400002,
Author = {Castro, T and Mar, B and Longoria, R and Ruiz-Suarez, LG and Morales, L},
Title = {{Surface albedo measurements in Mexico City metropolitan area}},
Journal = {{ATMOSFERA}},
Year = {{2001}},
Volume = {{14}},
Number = {{2}},
Pages = {{69-74}},
Month = {{APR 1}},
Abstract = {{Optical and thermal properties of soils are important input data for the
   meteorological and photochemical modules of air quality models. As
   development of these models increase on spatial resolution good albedo
   data become more important. In this paper measurements of surface albedo
   of UV (295-385 nm) and visible (450-550 nm) radiation are reported for
   different urban and rural surfaces in the vicinity of Mexico City. It
   was found for the downtown zone an average albedo value of 0.05 which is
   in very good agreement with reported values for urban surfaces. Our
   albedo values measured in UV region for grey cement and geen grass are
   of 0.10 and 0.009, respectively, and quite similar to those found at the
   literature of 0.11 and 0.008 for those type of surfaces.}},
