@article{ ISI:000168730600116,
Author = {Muhl, S and Camps, E and Escobar-Alarcon, L and Olea, O and Miki, M and
   Morrison, NA},
Title = {{DLC films prepared by electron evaporation of graphite in an ECR plasma}},
Journal = {{DIAMOND AND RELATED MATERIALS}},
Year = {{2001}},
Volume = {{10}},
