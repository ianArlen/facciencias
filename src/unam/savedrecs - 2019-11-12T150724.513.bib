@article{ ISI:000170261700016,
Author = {Montesinos, M and Vergara, JD},
Title = {{Gauge invariance of complex general relativity}},
Journal = {{GENERAL RELATIVITY AND GRAVITATION}},
Year = {{2001}},
Volume = {{33}},
Number = {{5}},
Pages = {{921-929}},
Month = {{MAY}},
Abstract = {{In this paper it is implemented how to make compatible the boundary
   conditions and the gauge fixing condition, for complex general
   relativity written in terms of Ashtekar variables using the approach of
   Ref. {[}1]. Moreover, it is found that at first order in the gauge
   parameters, the Hamiltonian action is (on shell) fully gauge-invariant
   under the gauge symmetry generated by the first class constraints in the
   case when spacetime M has the topology M = R x Sigma and Sigma has no
   boundary. Thus, the statement that the constraints linear in the momenta
   do riot contribute to the boundary terms is right, but only in the case
   when Sigma has no boundary.}},
