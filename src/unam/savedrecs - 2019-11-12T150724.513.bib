@article{ ISI:000167816500013,
Author = {Flisser, A and Lightowlers, MW},
Title = {{Vaccination against Taenia solium cysticercosis}},
Journal = {{MEMORIAS DO INSTITUTO OSWALDO CRUZ}},
Year = {{2001}},
Volume = {{96}},
Number = {{3}},
Pages = {{353-356}},
Month = {{APR}},
Abstract = {{Taenia solium is a parasite that causes human cysticercosis. Its life
   cycle includes the adult stage, the egg and the larval stage. Human
   cysticercosis is a disease related to underdevelopment, the main
   clinical manifestation is neurocysticercosis. Control measures include
   mass cestocidal treatment aimed to cure possible taeniosis cases.
   Although useful it has certain disadvantages, such as the generation of
   symptomatology in occult neurocysticercosis. Alternatively, health
   education has been shown to be highly effective since people become
   aware of the importance of human and porcine cysticercosis and the
   possibility of eliminating it. Nevertheless it has to be implemented by
   knowledgeable people. On the other hand the life cycle can be controlled
   by avoiding swine cysticercosis. This review describes the studies
   performed to vaccinate pigs against T. solium and indicate that short
   time perspectives are very encouraging for the production of an optimal
   vaccine.}},
