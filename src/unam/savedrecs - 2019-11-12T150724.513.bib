@article{ ISI:000168729900002,
Author = {Garcia, S and Font, R and Portelles, J and Quinones, RJ and Heiras, J
   and Siqueiros, JM},
Title = {{Effect of Nb doping on (Sr,Ba)TiO3 (BST) ceramic samples}},
Journal = {{JOURNAL OF ELECTROCERAMICS}},
Year = {{2001}},
Volume = {{6}},
Number = {{2}},
Pages = {{101-108}},
Month = {{JUN}},
Abstract = {{The effect of doping the Sr0.3Ba0.7Ti(1-5y/4)NbyO3 ceramic with
   different concentration of Nb is studied by scanning electron microscopy
   (SEM), X-ray diffraction and thermoelectric analysis. It is observed
   that the grain size decreases as the Nb concentration increases. The
   critical temperature T-c has a linear decrease at a rate of 19
   degreesC/mol\% of Nb. The temperature dependence of the dielectric
   permittivity presents strongly broadened curves, which suggest a non
   Curie-Weiss behavior near the transition temperature. The diffuse phase
   transition coefficient (delta) was also determined and its value leads
   to the conclusion that the degree of disorder in the system increases
   with the presence of the Nb cation.}},
