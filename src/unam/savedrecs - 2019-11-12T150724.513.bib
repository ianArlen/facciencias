@article{ ISI:000167083000010,
Author = {Vazquez, A and Lopez, T and Gomez, R and Bokhimi, X},
Title = {{Synthesis, characterization and catalytic properties of Pt/CeO2-Al2O3
   and Pt/La2O3-Al2O3 sol-gel derived catalysts}},
