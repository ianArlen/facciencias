@article{ ISI:000169553600005,
Author = {Niebla, JC and Hernandez-Guzman, L},
Title = {{Are self-esteem dimensions defined by socio-environmental contexts in
   children and adolescents?}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2001}},
Volume = {{18}},
Number = {{2}},
Pages = {{229-237}},
Month = {{JUN}},
Abstract = {{Self-esteem theoretical explanatory models tend to agree concerning the
   identification of self-esteem's underlying psychological dimensions,
   including its development and expression. Self-esteem is thus considered
   a central self-assessing action, which affects the individual's
   behaviors, cognitions and emotions. Such action, in turn, draws feedback
   from these components leading to more definite self-esteem forms. The
   study of self-esteem using self-report instruments, however, is usually
   based on conceiving it as depending from socioenvironmental contexts
   represented by the family, the school, the peer group, etc. The present
   study used an instrument constructed on the bases of both
   socio-environmental contexts and psychological components of self-esteem
   in order to explore other plausible categories. By collecting data
   through two cross-sectional applications st two developmental points:
   childhood and adolescence. the study attempted to document if such
   structures remained along such developmental stages by contrasting their
   factorial structures. Results favor the effects of dimensions provided
   by psychological Variables for both children and adolescents.}},
