@article{ ISI:000167036800002,
Author = {Palacios, D and Zamorano, JJ and Gomez, A},
Title = {{The impact of present lahars on the geomorphologic evolution of
   proglacial gorges: Popocatepetl, Mexico}},
Journal = {{GEOMORPHOLOGY}},
