@article{ ISI:000169553600008,
Author = {Ambriz, MGJ and Hernandez-Guzman, L and Martinez, LR},
Title = {{Maternal child rearing practices as perceived by young children}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2001}},
Volume = {{18}},
Number = {{2}},
Pages = {{257-264}},
Month = {{JUN}},
Abstract = {{The present study aimed at developing an instrument to measure
   children's perception of maternal child rearing practices. Participating
   children reacted to each of 32 non-verbal graphic items (two of them
   training items) representing four hypothetical situations linked to
   specific rearing styles: authoritarian, authoritative, permissive and
   indifferent. All items had previously been rated by independent experts
   as belonging to each category or style. Statistical analyses included
   the rasch model and cluster analysis. A total of 256 preschool children
   participated in the study. Main results showed that children perceive
   their mother as either authoritarian or non authoritarian.}},
