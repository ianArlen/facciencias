@article{ ISI:000170916200069,
Author = {Amero, C and Golzarri, JI and Izerrouken, M and Espinosa, G},
Title = {{Gd-148, U-238, Pu-239 and Cm-244 alpha particle energy analysis using
   tracks in solids}},
