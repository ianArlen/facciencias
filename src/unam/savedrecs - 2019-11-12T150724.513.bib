Author = {Nicho, ME and Trejo, M and Garcia-Valenzuela, A and Saniger, JM and
   Palacios, J and Hu, H},
Title = {{Polyaniline composite coatings interrogated by a nulling
   optical-transmittance bridge for sensing low concentrations of ammonia
   gas}},
