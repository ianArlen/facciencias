@article{ ISI:000171552000015,
Author = {Rosas, MM and De La Rosa, MAM and Goldammer, KM and Avila, VMC},
Title = {{Micropropagation of Turbinicarpus laui glass et Foster, an endemic and
   endangered species}},
Journal = {{IN VITRO CELLULAR \& DEVELOPMENTAL BIOLOGY-PLANT}},
Year = {{2001}},
Volume = {{37}},
Number = {{3}},
Pages = {{400-404}},
Month = {{MAY-JUN}},
Abstract = {{In a short period of time, a large number of adventitious shoots were
   regenerated from longitudinal sections of in vitro-germinated seedlings
   of the endangered Mexican cactus, Turbinicarpus laui. The induction
   medium consisted of Murashige and Skoog salts, supplemented with
   6-benzylaminopurine (BA) and alpha -naphthaleneacetic acid (NAA) in a
   wide range of combinations. The most effective concentrations of growth
   regulators for shoots initiation were 8.8-13.32 muM BA with 0-2.68 muM
   NAA. After 3-4 mo., individualized shoots were rooted on half-strength
   Murashige and Skoog medium and then transferred to soil to acclimatize
   under greenhouse conditions. In vitro strategies play a key role in the
   conservation and propagation of this commercially important endangered
   species of cactus.}},
