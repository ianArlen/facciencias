@article{ ISI:000280033400008,
Author = {Flores-Dorantes, Teresa and Arellano-Campos, Olimpia and
   Posadas-Sanchez, Rosalinda and Villarreal-Molina, Teresa and
   Medina-Urrutia, Aida and Romero-Hidalgo, Sandra and Yescas-Gomez, Petra
