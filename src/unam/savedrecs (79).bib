@article{ ISI:000283483600005,
Author = {Sanchez-Mendoza, Ernesto and Hernandez-Trujillo, Jesus},
Title = {{Interpretation of vicinal proton-proton coupling constants of
   heteroaromatic molecules in terms of topological electron density
   descriptors}},
Journal = {{MAGNETIC RESONANCE IN CHEMISTRY}},
