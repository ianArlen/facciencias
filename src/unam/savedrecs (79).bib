@article{ ISI:000287612200006,
Author = {Fernando Rubio, Manuel and Jimenez-Cruz, Federico and Ramirez-Galicia,
   Guillermo},
Title = {{Theoretical Study of the Electronic Effects in the Intramolecular
   Ketene-Styrene {[}2+2] Cycloaddition}},
Journal = {{JOURNAL OF THE MEXICAN CHEMICAL SOCIETY}},
Year = {{2010}},
Volume = {{54}},
Number = {{4}},
Pages = {{209-215}},
Month = {{OCT-DEC}},
Abstract = {{DFT (B3LYP/6-31+G{*}) and post-Hartree-Fock
   (MP2/631+G{*}//B3LYP/6-31G{*}) calculations were carried out in order to
   explain the effect of a remote substituent in the intramolecular
   ketene-styrene {[}2+2] cycloaddition of p-substituted
   2-methyl-7-aryl-hepta-1,6-dien-1-one which produces the bicycle{[}3.1.1]
   or the bicycle{[}3.2.0] heptanones according to Belanger experimental
   results. The transition state geometries were found as an asynchronous
   process with a three-member ring structure and an incipient positive
   charge development. Kinetic and thermodynamic controls were proposed at
   B3LYP/6-31+G{*} to determine which product is most likely to form in a
   competence reaction. In addition, the value of the. experimental
   reaction constant was reproduced, rho similar to -1.34.}},
