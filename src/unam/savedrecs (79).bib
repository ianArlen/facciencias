@article{ ISI:000281899900011,
Author = {Mathews, N. R.},
Title = {{Charge transport in a pulse-electrodeposited SnS/Al Schottky device}},
Journal = {{SEMICONDUCTOR SCIENCE AND TECHNOLOGY}},
Year = {{2010}},
Volume = {{25}},
Number = {{10}},
Month = {{OCT 15}},
Abstract = {{SnS thin films were pulse-electrodeposited on conducting glass
   substrates from a solution containing Sn(II) salt and Na(2)S(2)O(3).
   Schottky devices with this film were prepared by vacuum depositing Al
   electrodes, and the devices were used to study the electrical transport
   and activation energy of the traps in the forbidden band of SnS. The
   charge transport is controlled by both diffusion and recombination at
   the depletion region. Two traps with activation energies 0.036 and 0.133
   eV were detected at a particular applied bias. The traps showed a bias
   dependence for the activation energy; the zero field values of the
   ionization energies were estimated at 0.14 and 0.27 eV respectively. To
   the best of our knowledge, this is the first report discussing the bias
   dependence of the activation energy of SnS thin films.}},
