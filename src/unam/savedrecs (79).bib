@article{ ISI:000283423400003,
Author = {Estevez, Miriam and Fonseca, Gerardo and Vargas, Susana and Rodriguez,
   Rogelio},
Title = {{Hybrid Porous Materials for Dental Applications}},
Journal = {{JOURNAL OF COMPOSITE MATERIALS}},
Year = {{2010}},
Volume = {{44}},
Number = {{23}},
Pages = {{2667-2678}},
Month = {{NOV}},
Abstract = {{New hybrid polymer-ceramic obturation materials were developed using
   synthetic hydroxyapatite, two different types of ceramic particles, and
   a polymer with a high agglutination capacity, high abrasion resistance,
   and good adhesion to the dentin. Silica nano-particles of 16nm and
   alumina micro-particles of 3.2 mu m were used: this difference in sizes
   allows a better packing of the particles into the resin rendering in an
   improvement in the mechanical and abrasion properties. The resin reacts
   with the dentin and the ceramic particles improving the adhesion with
   the substrate and reducing the micro-filtration in the interface
   dentin-obturation material. Additionally, to protect the obturated
   dental piece again staining, a transparent and glossy dental coating
   with a high scratching resistance was also designed.}},
