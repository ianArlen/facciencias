@article{ ISI:000282596800001,
Author = {Richer, M. G. and Lopez, J. A. and Diaz-Mendez, E. and Riesgo, H. and
   Baez, S-H and Garcia-Diaz, Ma-T and Meaburn, J. and Clark, D. M. and
   Calderon Olvera, R. M. and Lopez Soto, G. and Toledano Rebolo, O.},
Title = {{THE SAN PEDRO MARTIR PLANETARY NEBULA KINEMATIC CATALOGUE: EXTRAGALACTIC
   PLANETARY NEBULAE}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2010}},
Volume = {{46}},
Number = {{2}},
Pages = {{191-214}},
Month = {{OCT}},
Abstract = {{We present kinematic data for 211 bright planetary nebulae in eleven
   Local Group galaxies: M31 (137 PNe), M32 (13), M33 (33), Fornax (1),
   Sagittarius (3), NGC 147 (2), NGC 185 (5), NGC 205 (9), NGC 6822 (5),
   Leo A (1), and Sextans A (1). The data were acquired at the Observatorio
   Astronomic Nacional in the Sierra de San Pedro Matir using the 2.1 m
   telescope and the Manchester Echelle Spectrometer in the light of {[}O
   III]lambda 5007 at a resolution of 11 km s(-1). A few objects were
   observed in H alpha. The internal kinematics of bright planetary nebulae
   do not depend strongly upon the metallicity or age of their progenitor
   stellar populations, though small systematic differences exist. The
   nebular kinematics and H beta luminosity require that the nebular shells
   be accelerated during the early evolution of their central stars. Thus,
   kinematics provides an additional argument favoring similar stellar
   progenitors for bright planetary nebulae in all galaxies.}},
