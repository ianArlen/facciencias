@article{ ISI:000280260000026,
Author = {Banuelos-Ruedas, F. and Angeles-Camacho, C. and Rios-Marcuello, S.},
Title = {{Analysis and validation of the methodology used in the extrapolation of
   wind speed data at different heights}},
Journal = {{RENEWABLE \& SUSTAINABLE ENERGY REVIEWS}},
Year = {{2010}},
Volume = {{14}},
Number = {{8}},
Pages = {{2383-2391}},
Month = {{OCT}},
Abstract = {{The aim of this paper is the analysis of the methodologies and
   extrapolation laws subsequently used in the calculation of wind speed
   profiles. It also highlights the disadvantages of using low-sized
   measurements for creating the wind profile. It also demonstrates how to
   obtain different speed profiles at the same site and for the same month
   of a different year. This arrangement allows for the compilation of
   different roughness and friction coefficients for the same site. This
   paper also entails a full study case presentation for the site located
   in the Mexican state of Zacatecas and where the wind speed measurements
   were extrapolated with a view to estimate the wind power at a specific
   height. (C) 2010 Elsevier Ltd. All rights reserved.}},
