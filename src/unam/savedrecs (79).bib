@article{ ISI:000282596800014,
Author = {Trejo, Alfonso and Rodriguez, Luis F.},
Title = {{ON THE NATURE OF THE NON-THERMAL RADIO SOURCE AT THE CENTER OF THE ORION
   STREAMERS}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2010}},
Volume = {{46}},
Number = {{2}},
Pages = {{349-353}},
Month = {{OCT}},
Abstract = {{We present VLA continuum observations at 20 and 6 cm toward a
   non-thermal radio source located at the Streamers region in Orion. We
   compare 1991 archive observations with new ones taken by us in 2008. The
   radio source could be a young stellar source or an evolved object like a
   microquasar or even a jet from a radio galaxy. Our analysis shows that
   there is no evidence of changes in the flux densities between both
   epochs at a level higher than 10\%. Also, we set an upper limit at a 3
   sigma level of 23 mas year(-1) for the proper motions. At the distance
   of Orion this implies an upper limit to the velocity in the plane of the
   sky of 45 km s(-1). At 6 cm, we detect linearly polarized emission at a
   level of similar to 5\%, while at 20 cm we do not detect this emission
   above a level of similar to 2\%. We conclude that the non-thermal radio
   source most probably is a radio galaxy aligned by chance with the line
   of sight to the Streamers in Orion.}},
