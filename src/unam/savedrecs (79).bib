@article{ ISI:000281948100037,
Author = {Blasco-Herrera, J. and Fathi, K. and Beckman, J. and Gutierrez, L. and
   Lundgren, A. and Epinat, B. and Ostlin, G. and Font, J. and Hernandez,
   O. and de Denus-Baillargeon, M. -M. and Carignan, C.},
Title = {{An improved method for statistical studies of the internal kinematics of
   H ii regions: the case of M83}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2010}},
Volume = {{407}},
Number = {{4}},
Pages = {{2519-2529}},
Month = {{OCT 1}},
Abstract = {{We present the integrated H alpha emission line profile for 157 H ii
   regions in the central 3.4 x 3.4 arcmin2 of the galaxy M83 (NGC 5236).
   Using the Fabry-Perot interferometer GH alpha FaS, on the 4.2-m William
   Herschel Telescope on La Palma, we show the importance of a good
   characterization of the instrumental response function for the study of
   line profile shapes. The luminosity-velocity dispersion relation is also
   studied, and in the log(L)-log(Sigma) plane we do not find a linear
   relation, but an upper envelope with equation log(L(H alpha)) = 0.9
   log(Sigma) + 38.1. For the adopted distance of 4.5 Mpc, the upper
   envelope appears at the luminosity L = 1038.5 erg s-1, in full agreement
   with previous studies of other galaxies, reinforcing the idea of using H
   ii regions as standard candles.}},
