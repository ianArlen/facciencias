@article{ ISI:000283158700006,
Author = {Gonzalez, Yael and Venegas, Daniela and Mendoza-Hernandez, Guillermo and
   Camarena, Laura and Dreyfus, Georges},
Title = {{Na+- and H+-dependent motility in the coral pathogen Vibrio shilonii}},
Journal = {{FEMS MICROBIOLOGY LETTERS}},
Year = {{2010}},
Volume = {{312}},
Number = {{2}},
Pages = {{142-150}},
Month = {{NOV}},
Abstract = {{In this work, we analyzed motility and the flagellar systems of the
   marine bacterium Vibrio shilonii. We show that this bacterium produces
   lateral flagella when seeded on soft agar plates at concentrations of
   0.5\% or 0.6\%. However, at agar concentrations of 0.7\%, cells become
   round and lose their flagella. The sodium channel blocker amiloride
   inhibits swimming of V. shilonii with the sheathed polar flagellum, but
   not swarming with lateral flagella. We also isolated and characterized
   the filament-hook-basal body of the polar flagellum. The proteins in
   this structure were analyzed by MS. Eight internal sequences matched
   with known flagellar proteins. The comparison of these sequences with
   the protein database from the complete genome of V. shilonii allows us
   to conclude that some components of the polar flagellum are encoded in
   two different clusters of flagellar genes, suggesting that this
   bacterium has a complex flagellar system, more complex possibly than
   other Vibrio species reported so far.}},
