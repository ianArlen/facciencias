@article{ ISI:000287739700003,
Author = {Cervantes-Cardoza, Veronica and Elizabeth Rocha-Guzman, Nuria and
   Alberto Gallegos-Infante, Jose and Rosales-Castro, Martha and
   Medina-Torres, Luis and Francisco Gonzalez-Laredo, Ruben},
Title = {{Antioxidant activity of seed extracts from three apple cultivars (Malus
   domestica Borkh - Rosaceae-)}},
Journal = {{BOLETIN LATINOAMERICANO Y DEL CARIBE DE PLANTAS MEDICINALES Y AROMATICAS}},
Year = {{2010}},
Volume = {{9}},
Number = {{6}},
Pages = {{446-456}},
Month = {{NOV}},
Abstract = {{The oily (hexane) and aqueous extracts from apple seeds (Malus domestica
   Borkh -Rosaceae-) cultivars Winter Banana (WB), Winter Permain (WP) and
   Blanca de Asturias (BA) have shown significant differences in phenolic
   content and antioxidant activity. BA seed oily extracts and WB seed
   aqueous extracts have presented the best scavenging capacity of DPPH
   center dot radical and inhibition of hydroxyl radicals. In the
   biological assay apple seed extracts showed protection against low
   density lipoproteins (LDL) oxidation, which might fight the risk of
   atherosclerosis. The BA oily seed extract may have potential use as
   nutraceutical ingredient.}},
