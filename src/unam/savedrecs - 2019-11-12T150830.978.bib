@article{ ISI:000080821900008,
Author = {Wang, JA and Aguilar-Rios, G and Wang, R},
Title = {{Inhibition of carbon monoxide on methanol oxidation over gamma-alumina
   supported Ag, Pd and Ag-Pd catalysts}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{1999}},
Volume = {{147}},
Number = {{1-4}},
Pages = {{44-51}},
Month = {{MAY}},
Abstract = {{The activities of CH,OH and CO oxidative reactions over the
   gamma-alumina supported Ag,and Ag-Pd catalysts were measured with the
   MR-GC method. The CO-temperature-programmed desorption (CO-TPD) and in
   situ LR techniques were used to characterize the CO adsorption behavior
   on the surface of the catalysts. The oxidative activity for CO to CO,
   increased in the following sequence: 5\% Ag/gamma-Al-2,O-3, < 0.1\%
   Pd/gamma-Al2O3 ( 5\% Ag-0.1\% Pd/gamma-Al2O3. An inhibition action of CO
   to CB,OH oxidation, that is dependent of the active components of the
   catalysts, was observed when CO was present in the methanol-fed stream.
   The results of IR and CO-TPD showed that the poor oxidative activity of
   CO over Ag catalyst was due to its low adsorption capacity on this
   catalyst. The very strong adsorption ability of CO on the Pd catalyst
   was responsible for the strong inhibition of CO to CH3OH oxidation
   activity. The plausible mechanisms of CO strong inhibition behavior on
   methanol oxidation over the different catalysts are discussed in detail
   from the viewpoints of both electronic and geometric effects. (C) 1999
   Elsevier Science B.V. All rights reserved.}},
