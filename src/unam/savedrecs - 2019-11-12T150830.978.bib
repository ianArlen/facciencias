@article{ ISI:000080939700016,
Author = {Gonzalez, MI and Lopez-Colome, AM and Ortega, A},
Title = {{Sodium-dependent glutamate transport in Muller glial cells: regulation
   by phorbol esters}},
Journal = {{BRAIN RESEARCH}},
Year = {{1999}},
Volume = {{831}},
Number = {{1-2}},
Pages = {{140-145}},
Month = {{JUN 12}},
Abstract = {{The regulation of the Na(+)-dependent high affinity glutamate/aspartate
   transporter system expressed in cultured Muller glia cells from chick
   retina was studied. Treatment of the cells with the
   Ca(2+)/diacylglycerol dependent protein kinase C (PKC) activator,
   phorbol 12-tetradecanoil-13-acetate (TPA) produced a decrease in
   {[}(3)H]D-aspartate uptake which was reversed by staurosporine and
   partially by H7 {[}1-(5-isoquinolinesulfonyl)-2-methylpiperazine
   dihydrochoride], two PKC inhibitors. Long-term treatment with TPA
   resulted in a drastic decrease in the uptake activity, correlated with a
   substantial fall in the expression of the transporter protein. These
   findings suggest that PKC is involved in transport modulation at two
   different levels: phosphorylation and transporter expression in retinal
   Muller glial cells. (C) 1999 Elsevier Science B.V. All rights reserved.}},
