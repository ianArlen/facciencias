@article{ ISI:000081971600010,
Author = {Perezgasga, L and Segovia, L and Zurita, M},
Title = {{Molecular characterization of the 5 ` control region and of two lethal
   alleles affecting the hsp60 gene in Drosophila melanogaster}},
Journal = {{FEBS LETTERS}},
