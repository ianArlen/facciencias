@article{ ISI:000081437400013,
Author = {Markus, F and Gambar, K and Vazquez, F and del Rio, JA},
Title = {{Classical field theory and stochastic properties of hyperbolic equations
   of dissipative processes}},
Journal = {{PHYSICA A}},
Year = {{1999}},
Volume = {{268}},
Number = {{3-4}},
Pages = {{482-498}},
Month = {{JUN 15}},
Abstract = {{The set of damped hyperbolic transport equations is one of the wide
   class of equations for the description of dissipative physical
   processes. Deeper understanding into the structure of these physical
   phenomena can be obtained with the help of the Hamiltonian formalism. Tn
   the present paper, we show that the Hamilton-Lagrange formalism can be
   constructed for these kinds of transport equations. We obtain the
   Hamiltonian, the canonically conjugate quantities and the
   Poisson-bracket expressions for them. With this formalism we analyze the
   statistical properties of path fluctuations in the new conjugated
   thermodynamic variable space. We show that for short times the
   stochastic behavior under this new scope obeys the Chapman-Kolmogorov
   relationship. (C) 1999 Elsevier Science B.V. All rights reserved.}},
