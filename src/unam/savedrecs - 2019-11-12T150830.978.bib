@article{ ISI:000082633400009,
Author = {Quiroz, U and Morales, L and Moran, C and Dominguez, R},
Title = {{Effects of sympathetic denervation induced by guanethidine
   administration to pregnant rats on follicular development and puberty of
   the offspring}},
Journal = {{MEDICAL SCIENCE RESEARCH}},
Year = {{1999}},
Volume = {{27}},
Number = {{9}},
Pages = {{599-600}},
Month = {{SEP}},
Abstract = {{We have analysed the effects of fetal sympathetic denervation on
   puberty, follicular growth and ovulation, as induced by guanethidine in
   pregnant rats. Administration of guanethidine resulted in a delay of
   puberty (age of vaginal oestrus 47.0 +/- 0.9 vs 43.1 +/- 0.7, P < 0.05;
   and first ovulation 48.1 +/- 0.9 vs 44.0 +/- 0.7, P < 0.05) (means +/-
   SEM) and an acceleration in follicular growth (number of follicles
   measured: 172.0 +/- 11.4 in treated animals vs 112.0 +/- 11.5 in
   control, P < 0.05). There was also an increase in follicular atresia
   (number of atretic follicles: 154.3 +/- 13.2 in treated vs 88.7 +/- 8.4
   in control, P < 0.05). The results suggest that sympathetic innervation
   plays a modulatory-inhibitory role on the ovarian response to
   gonadotrophin beginning in the fetal period. Med Sci Res 27:599-600 (C)
   1999 Lippincott Williams \& Wilkins.}},
