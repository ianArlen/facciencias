@article{ ISI:000080697300005,
Author = {Villar-Martin, M and Binette, L and Fosbury, RAE},
Title = {{The nature of the extreme kinematics in the extended gas of high
   redshift radio galaxies}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{1999}},
Volume = {{346}},
Number = {{1}},
Pages = {{7-12}},
Month = {{JUN}},
Abstract = {{We present UV rest frame spectra of 3 powerful narrow line radio
   galaxies and the hyperluminous type 2 active galaxy SMM J02399-0136, all
   at high redshift (z > 2). We find high velocities (FWHM > 1000 km s(-1))
   in the extended gas of all objects. A natural explanation is the
   interaction between the radio jet and the ambient gas, that drives
   shocks into the gas and accelerates the clouds. However, the existence
   of high velocities in regions where such interactions are not taking
   place implies that other processes can play a role. We discuss here
   several possible mechanisms.}},
