@article{ ISI:000082112600004,
Author = {Baron-Szabo, RC and Gonzalez-Leon, CM},
Title = {{Lower Cretaceous corals and stratigraphy of the Bisbee Group (Cerro de
   Oro and Lampazos areas), Sonora, Mexico}},
Journal = {{CRETACEOUS RESEARCH}},
