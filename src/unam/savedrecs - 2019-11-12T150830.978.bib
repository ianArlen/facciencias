@article{ ISI:000086042100002,
Author = {Lope Blanch, JM},
Title = {{The syntactic structure of Christopher Columbus' discourse in the
   `Diario'}},
Journal = {{BULLETIN HISPANIQUE}},
Year = {{1999}},
Volume = {{101}},
Number = {{2}},
Pages = {{375-385}},
Month = {{JUL-DEC}},
Abstract = {{The syntactic structure of Christopher Columbus' prose, as revealed in
   two passages taken from his Diary of the voyage in which he discovered
   America, shows notable variety and, above all, an expressive range and
   high level of formal complexity equaled only by that of Nebrija in the
   ``Prologue{''} to his Castilian grammar and that of Hernan Cortes in his
   Letters to the emperor Charles V, that is to say, by texts in an
   elevated and solemn style addressed to a monarch. The syntax displayed
   by these three writers is the richest and most ornate of all those which
   I have yet analyzed in Medieval, Classical and Modern Spanish.}},
