@article{ ISI:000081771500006,
Author = {Sahu, S},
Title = {{Cosmic rays propagation in a pseudo-scalar medium}},
Journal = {{MODERN PHYSICS LETTERS A}},
Year = {{1999}},
Volume = {{14}},
Number = {{19}},
Pages = {{1287-1295}},
Month = {{JUN 21}},
Abstract = {{We have calculated the dispersion relation for the high energy cosmic
   rags (protons, electrons and neutrinos) propagating in the Bose gas as
   well as Bose condensate (BC) medium of the pseudo-scalar particles
   (axions and majorons). For cosmic rays propagating in the Bose gas, the
   mass of the particle will decrease and this is proportional to the boson
   density in the medium. On the other hand, the BC of the same particle
   will increase the mass of the fermion. If the propagating fermion is
   massless, it will develop an imaginary mass and thus will be absorbed in
   the medium. But if the condition m(2)/m(phi)p(0) similar or equal to 1
   is satisfied, the cosmic rays will propagate freely without losing
   energy in the BC medium. The Bose gas and BC of the pseudo-scalar
   background contribution to the dispersion relation of the high energy
   cosmic rays is too small to have any practical importance to high energy
   cosmic rays physics.}},
