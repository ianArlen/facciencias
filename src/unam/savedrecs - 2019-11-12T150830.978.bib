@article{ ISI:000080937500006,
Author = {Panayotaros, P},
Title = {{Near-monochromatic water waves on the sphere}},
Journal = {{PHYSICA D}},
Year = {{1999}},
Volume = {{130}},
Number = {{3-4}},
Pages = {{273-290}},
Month = {{JUN 15}},
Abstract = {{We study analytically and numerically a class of traveling and standing
   waves in a model of weakly non-linear gravity water waves on the sphere.
   These waves are `near-monochromatic' in space, i.e. their amplitude
   consists of one spherical harmonic plus small corrections, and we see
   numerically that they retain this property for long time. A main feature
   of the model we consider is that it possesses a Hamiltonian structure.
   This structure is preserved by our numerical implementation, and we use
   formal and rigorous arguments from classical perturbation theory to
   understand the numerical observations. (C)1999 Elsevier Science B.V. All
   rights reserved.}},
