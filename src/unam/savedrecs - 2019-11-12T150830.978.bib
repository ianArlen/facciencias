@article{ ISI:000080482900007,
Author = {Verduzco, MB and Montero, PAA and Duarte, ED and Kampfner, OA},
Title = {{Dispersion experiments of a tracer in a laboratory}},
Journal = {{INGENIERIA HIDRAULICA EN MEXICO}},
Year = {{1999}},
Volume = {{14}},
Number = {{2}},
Pages = {{65-74}},
Month = {{MAY-AUG}},
Abstract = {{The differential equation of one-dimensional transport and dispersion of
   pollutants in rivers has as parameter the longitudinal dispersion
   coefficient. There are several ways to obtain that coefficient through
   theoretical-experimental developments. In this article, dispersion
   experiments of a tracer in a laboratory flume are described; besides
   observing the phenomenon, the experiments allow to compare the different
   methods in order to estimate the longitudinal dispersion coefficient.
   Furthermore, the data of the experiments can be used for the validation
   of mathematical models.}},
