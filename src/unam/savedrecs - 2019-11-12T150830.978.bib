@article{ ISI:000081944000026,
Author = {Garzon-Rodriguez, W and Yatsimirsky, AK and Glabe, CG},
Title = {{Binding of Zn(II), Cu(II), and Fe(II) ions to Alzheimer's A beta peptide
   studied by fluorescence}},
Journal = {{BIOORGANIC \& MEDICINAL CHEMISTRY LETTERS}},
Year = {{1999}},
Volume = {{9}},
Number = {{15}},
Pages = {{2243-2248}},
Month = {{AUG 2}},
Abstract = {{Binding of Zn(II), Cu(II) and Fe(II) ions to A beta 1-40, A beta 1-42
   and a single tryptophan mutant of A beta 1-40 in solution at pH 7.4 was
   studied by fluorescent titration. Job plots and fitting of titration
   curves revealed formation of 1:1 and 1:2 peptide-metal complexes. For
   dimeric peptides A beta 1-40 and A beta F4W the order of metal to
   peptide affinities is Fe < Cu > Zn, which is in agreement with the
   Irving-Williams series of complex stability. The affinity of A beta 1-42
   for. Fe increases dramatically upon aggregation: K-D changes from ca.
   100 to ca. 0.2 mu M. (C) 1999 Elsevier Science Ltd. All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
