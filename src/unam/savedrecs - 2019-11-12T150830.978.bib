@article{ ISI:000081792200003,
Author = {Lopez, T and Gomez, R and Pecci, G and Reyes, P and Bokhimi, X and
   Novaro, O},
Title = {{Effect of pH on the incorporation of platinum into the lattice of
   sol-gel titania phases}},
Journal = {{MATERIALS LETTERS}},
Year = {{1999}},
Volume = {{40}},
Number = {{2}},
Pages = {{59-65}},
Month = {{JUL}},
Abstract = {{Titania and platinum/titania catalysts were prepared by co-gelling
   titanium ethoxide and platinum acetylacetonate in acid or basic media.
   For the acid conditions, platinum promoted the formation of rutile; but,
   when the medium was basic, the samples were a mixture of anatase and
   rutile. Platinum also affected the titania energy band-gap. The
   reactivity, for methylcyclohexane dehydrogenation, of the samples
   prepared in a basic environment, was larger than that for those prepared
   in an acid medium. The highest effect and the most important
   modification of the TiO2 energy band-gap occurred for pH 3, which
   suggests a large incorporation of platinum into the lattice of titania
   phases. (C) 1999 Elsevier Science B.V. All rights reserved.}},
