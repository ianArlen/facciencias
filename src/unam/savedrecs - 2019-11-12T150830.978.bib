@article{ ISI:000081045900011,
Author = {Rojas, Y and Alvarez, F and Villalobos, JL},
Title = {{A new species of crayfish of the genus Procambarus (Crustacea : Decapoda
   : Cambaridae) from Veracruz, Mexico}},
Journal = {{PROCEEDINGS OF THE BIOLOGICAL SOCIETY OF WASHINGTON}},
Year = {{1999}},
Volume = {{112}},
Number = {{2}},
Pages = {{396-404}},
Month = {{JUN 15}},
Abstract = {{Procambarus (Austrocambarus) citlaltepetl, new species, is described
   from Ciudad Mendoza, Veracruz, Mexico. The new species can be placed in
   the mexicanus Group, and is most similar to P. (A.) mexicanus, from
   which it can be distinguished by the absence of branchiostegal spines,
   gonopod with blade-like mesial process curved over the apex and thicker
   lateral portion, and annulus ventralis with complete preannular plate.
   SEM micrographs of the male and female genitalia of the new species, as
   well of those of P. (A.) mexicanus, are provided to facilitate
   comparisons, and to clarify the identity of the latter.}},
