@article{ ISI:000081158600027,
Author = {Bohigas, J and Sauvageot, JL and Decourchelle, A},
Title = {{The warm interstellar medium around the Cygnus Loop}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1999}},
Volume = {{518}},
Number = {{1, 1}},
Pages = {{324-333}},
Month = {{JUN 10}},
Abstract = {{Observations of the oxygen lines {[}O II] 3729 Angstrom and {[}O III]
   5007 Angstrom, in the medium immediately beyond the Cygnus Loop
   supernova remnant were carried out with the scanning Fabry-Perot
   spectrophotometer Experience Spectroscopique d'Observation des Plasmas
   (ESOP. Both lines were detected in three different directions least,
   northeast, and southwest), and up to a distance of 15 pc from the shock
   front. The ionized medium is in the immediate vicinity of the remnant,
   as evinced by the smooth brightening of both lines as the adiabatic
   shock transition (defined by the X-ray perimeter) is crossed. These
   lines are usually brighter around the Cygnus Loop than in the general
   background in directions where the Galactic latitude is greater than or
   equal to 5 degrees. There is also marginal (but significant) evidence
   that the degree of ionization is somewhat larger around the Cygnus Loop.
   We conclude that the energy necessary to ionize this large bubble of gas
   could have been supplied by an O8 or O9 type progenitor, or by the
   particles heated by the expanding shock front. The second possibility,
   although highly attractive, would have to be assessed by extensive
   modeling.}},
