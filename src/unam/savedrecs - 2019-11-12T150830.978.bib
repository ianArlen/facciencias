@article{ ISI:000082197200025,
Author = {Nakamura, T and Kaiya, K and Takahashi, N and Matsuzawa, T and Rowlands,
   CC and Beltran-Lopez, V and Smith, GM and Riedi, PC},
Title = {{EPR investigations on europium(II)-doped barium aluminate}},
