@article{ ISI:000081014700006,
Author = {Manzo, J and Cruz, MR and Hernandez, ME and Pacheco, P and Sachs, BD},
Title = {{Regulation of noncontact erection in rats by gonadal steroids}},
Journal = {{HORMONES AND BEHAVIOR}},
