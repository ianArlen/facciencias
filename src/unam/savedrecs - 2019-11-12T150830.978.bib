@article{ ISI:000082186800005,
Author = {Martinez-Palacios, A and Eguiarte, LE and Furnier, GR},
Title = {{Genetic diversity of the endangered endemic Agave victoriae-reginae
   (Agavaceae) in the Chihuahuan Desert}},
Journal = {{AMERICAN JOURNAL OF BOTANY}},
Year = {{1999}},
Volume = {{86}},
Number = {{8}},
Pages = {{1093-1098}},
Month = {{AUG}},
Abstract = {{Long-lived perennials are a species-rich, ecologically important
   component of the North American deserts: yet we know little about their
   genetic structure, information important for their conservation. Agave
   victoriae-reginae is an endemic of the Chihuahuan Desert of northern
   Mexico that is endangered by collection for the ornamental trade. We
   examined levels and patterns of variation at ten polymorphic allozyme
   loci in ten populations representing the range of the species. Levels of
   genetic variation (mean H-e = 0.335) and differentiation (mean F-ST =
   0.236) were high. Phenetic clustering suggested the existence of at
   least three distinct groups of populations. If this pattern of variation
   is representative of other long-lived desert perennials, it may explain
   the species richness of this group and will pose a real challenge to
   gene conservation efforts.}},
