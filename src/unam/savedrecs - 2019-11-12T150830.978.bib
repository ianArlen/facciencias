@article{ ISI:000080594900016,
Author = {Montesinos, M and Morales-Tecotl, HA and Urrutia, LF and Vergara, JD},
Title = {{Complex canonical gravity and reality constraints}},
Journal = {{GENERAL RELATIVITY AND GRAVITATION}},
Year = {{1999}},
Volume = {{31}},
Number = {{5}},
Pages = {{719-723}},
Month = {{MAY}},
Note = {{Mexican Meeting on Gauge Theories of Gravity, in Honor of Professor
   Hehls 60th Birthday, UNIV AUTONOMA METROPOLITANA IZTAPALAPA, MEXICO
   CITY, MEXICO, OCT 06-10, 1997}},
Abstract = {{Ashtekar canonical variables for general relativity yielding low degree
   polynomial constraints are complex and describe complex canonical
   gravity. To pick the real sector, ci la Dirac, one must introduce
   reality constraints; they turn out to be second-class. It is shown here
   that this holds not only for pure gravity but also for a scalar held
   non-minimally coupled to gravity. The original simplicity produced by
   the complex variables is spoiled if one gets rid of the second-class
   constraints via Dirac brackets, however. To circumvent such an
   undesirable feature, alternative possibilities are pointed out.}},
