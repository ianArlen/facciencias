Journal = {{JOURNAL OF NEW MATERIALS FOR ELECTROCHEMICAL SYSTEMS}},
Year = {{1999}},
Volume = {{2}},
Number = {{3}},
Pages = {{207-210}},
Month = {{JUL}},
Abstract = {{The electrochemical hydrogen absorption - desorption in p-Si and Ni
   sponge was studied by cyclic voltametry The voltamogram for Si in the
   alkaline media in the potential range E = -1.8 to -0.3 V (vs. SCE)
   showed an increase in the hydrogen evolution current at a potential of
