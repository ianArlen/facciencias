@article{ ISI:000082455500012,
Author = {Gamboa, SA and Sebastian, PJ and Mathew, X and Nguyen-Cong, H and
   Chartier, P},
Title = {{A CdTe/PMeT photovoltaic structure formed by electrodeposition and
   processing}},
