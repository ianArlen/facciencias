@article{ ISI:000083728500001,
Author = {Fujita, S and Ho, HC and Godoy, S},
Title = {{Theory of the Seebeck coefficient in alkali and noble metals}},
Journal = {{MODERN PHYSICS LETTERS B}},
Year = {{1999}},
Volume = {{13}},
Number = {{20}},
Pages = {{689-694}},
Month = {{AUG 30}},
Abstract = {{A new formula for the Seebeck coefficient S is obtained: S =
   (21n2/3)(qn)(-1 epsilon)(F)k(B) x (N/V), where q, n, epsilon(F), N and V
   are, respectively, charge, carrier density, Fermi energy, density of
   states at epsilon(F) and volume. Seebeck (S) and Hall (R(H))
   coefficients in alkali metals are both negative while these coefficients
   in noble metals haw opposite signs. This difference is shown to arise
   from the different shapes of the Fermi surface.}},
