@article{ ISI:000083586600010,
Author = {Molina, J and Masso, F and Paez, A and Mendez, C and Rodriguez, E and
   Mandoki, JJ and De Leon, LD and Montano, LF},
Title = {{Differential effect of estradiol on antibody secretion of murine
   hybridomas}},
Journal = {{HYBRIDOMA}},
