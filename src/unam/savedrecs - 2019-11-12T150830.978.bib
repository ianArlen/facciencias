@article{ ISI:000082044200003,
Author = {Carreno, AL and Coimbra, JC and do Carmo, DA},
Title = {{Late Cenozoic sea level changes evidenced by ostracodes in the Pelotas
   basin, southernmost Brazil}},
Journal = {{MARINE MICROPALEONTOLOGY}},
Year = {{1999}},
Volume = {{37}},
Number = {{2}},
Pages = {{117-129}},
Month = {{AUG}},
Abstract = {{Ostracodes recovered from sediments in seven onshore boreholes within
   the Pelotas basin, State of Rio Grande do Sul, indicate several
   continuous nearshore environments and at least four brief
   transgressive-regressive cycles are present. This interpretation is
   based on the distribution throughout upper Neogene and Quaternary
   sequences of species that characterize one or more biofacies depending
   on the borehole position in the basin. Bradleya pelotensis, Krithe
   coimbrai, Henryhowella kempfi and H. rectangulata are considered
   shelf-upper bathyal species whereas the shelf species are represented by
   Ambostracon? crucicostatum, Argenticytheretta variabilis, A.
   laevipunctata, Brasilicythere reticulispinosa, Caudites posdiagonalis,
   Costa riograndensis, Coquimba bertelsae, C. tenuireticulata, Cytheretta
   punctata, Quadracythere eichlerae and Protocytheretta sp. Species with
   lagoon-estuarine shelf affinities include Callistocythere litoralensis,
   C. marginalis and Perissocytheridea kroemmelbeini whereas Cyprideis
   maxipunctata, C. mostardensis, C. posteroinflata, C. salebrosa and C.
   sparsopunctata represent a lagoon or estuarine environment, and Cypris
   cassinensis and Limnocythere sp. are freshwater species. Correlation of
   the ostracode biozones places thr four maximum regressive events in the
   Late Pliocene acid Pleistocene (1250-485 ka, 185 ka and 15 ka), whereas
   the maximum transgressive events occur in the Late Miocene and
   Pleistocene (1.6 Ma, 423-400 ka and 120 ka), in a barrier-lagoonal
   depositional system controlled by glacio-eustatic sea level changes. (C)
   1999 Elsevier Science B.V. All rights reserved.}},
