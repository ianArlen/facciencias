@article{ ISI:000081893900003,
Author = {Lopez, F and Menez, M and de la Paz, C and Trejo, A},
Title = {{Attending behavior of preschoolers in the classroom: An observational
   study}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1999}},
Volume = {{16}},
Number = {{1}},
Pages = {{25-36}},
Month = {{JUN}},
Abstract = {{Eight girls and two boys attending to a preschool facility were
   videotaped during assigned academical activities in the classroom. We
   undertook an observational analysis of the attending behavior of the
   children and its relationship with other behavioral categories. The two
   predominant behavior categories were Attending Behavior and
   Conversation. Several analysis indicated that those two activities tend
   to compete between each other in the classroom and that the episodes of
   Conversation tend to interfere with the attending span. As a
   consequence, the attending behavior span tend to be brief (mainly with
   values below 15 sec) and the cumulated attending time was low. This
   deteriorated attending behavior was contributed by the fact that
   children=s motivation decayed as the class period increased, since brief
   attending times increased in late class periods. These findings are
   discussed in the context of preschool settings with pre-academic
   objectives and with regard to the relationships between the two
   identified sources of control: social and academical.}},
