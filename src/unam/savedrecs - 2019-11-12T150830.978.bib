@article{ ISI:000079982000012,
Author = {Trueta, C and Diaz, M and Vaca, LA and Clapp, C and de la Escalera, GM},
Title = {{Functional uncoupling between intracellular calcium dynamics and
   secretion in the alpha T3-1 gonadotropic cell line}},
Journal = {{JOURNAL OF CELLULAR PHYSIOLOGY}},
Year = {{1999}},
Volume = {{179}},
Number = {{3}},
Pages = {{347-357}},
Month = {{JUN}},
Abstract = {{Gonadotropin releasing hormone (GnRH) stimulates both transcription and
   secretion of the alpha subunit of the gonadotropins in a Ca2+-dependent
   fashion. In this study, we examined the role of Ca2+ as the signal
   coupling agonist occupancy of GnRH receptors to hormone secretion using
   the gonadotropic cell line alpha T3-1. Treatment of alpha T3-1 cells for
   60 min with GnRH (0.1-100 nM) veratridine (50 mu M) or high K+ (56 mM)
   was completely ineffective in stimulating secretion. The tack of effect
   occurred in spite of a robust, specific, and dose-dependent biphasic
   {[}Ca2+](i) response consisting of a rapid peak sensitive to
   thapsigargin (200 nM) followed by a smaller plateau sensitive to the
   extracellular application of EGTA (5 mM). On the other hand, treatment
   of aT3-1 cells with the Ca2+ ionophore ionomycin resulted in a
   significant dose-dependent stimulation of secretion and {[}Ca2+](i)
   responses comparable to those elicited by GnRH. Binding assays revealed
   the presence of Ins(1,4,5)P-3 receptors (Kd = 3.2 nM, Bmax = 50.5
   fmol/mg protein) but not ryanodine receptors in alpha T3-1 cell
   membranes. Together, these results show a functional uncoupling between
   the {[}Ca2+](i) response and secretion in this cell line, suggesting
   that the increase in {[}Ca2+](i) triggered by GnRH and depolarization
   may be necessary but not sufficient to stimulate exocytosis. (C) 1999
   Wiley-Liss, Inc.}},
