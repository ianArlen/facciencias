@article{ ISI:000085023200033,
Author = {Guzman-Cornejo, MD and Garcia-Prieto, L},
Title = {{Trematodiasis in some fishes from Cuitzeo lake, Michoacan, Mexico}},
Journal = {{REVISTA DE BIOLOGIA TROPICAL}},
Year = {{1999}},
Volume = {{47}},
Number = {{3}},
Pages = {{593-595}},
Month = {{SEP}},
Abstract = {{In September, 1995 and January, 1996, six species of fishes from Lake
   Cuitzeo, Michoacan, Mexico, were collected (Alloophorus robustus n=30,
   Goodea atripinnis n=30, Xenotoca variata n=41, Chirostoma jordani n=30,
   Carassius auratus n=30 and Oreochromis aureus n=30), to establish the
   infection levels reached by Posthodiplostomum minimum, Clinostomum
   complanatum and Diplostomum (Tylodelphys) sp., in their hosts. Infection
   caused by P. minimum metacercariae was the most important helminthiasis
   considering the number of hosts they parasite (five of six), and the
   high levels of abundance of the infection; we suggest that fish
   preferences and the generalist character of the trematode are the causes
   that determine this infection. Finally, the role of goodeids and
   atherinids in transmission of infections in the lake is confirmed; the
   former being the main group maintaining such infections.}},
