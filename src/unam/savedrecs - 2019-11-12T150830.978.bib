@article{ ISI:000081685700014,
Author = {Mendez, MEG and Takeuchi, N},
Title = {{Defects and energy barriers in the Si(100)-Sb and Si(100)-As surfaces: a
   theoretical study}},
Journal = {{SURFACE SCIENCE}},
Year = {{1999}},
Volume = {{432}},
Number = {{3}},
Pages = {{239-244}},
Month = {{JUL 20}},
Abstract = {{When one monolayer of Sb or As is adsorbed on Si(100), the surface shows
   a (2 x 1) reconstruction formed by symmetric ad-metal dimers on an
   almost ideal Si surface. In spite of the similarities, some experimental
   differences have been reported. Scanning tunneling microscope images
   show that in the Si(100) (2 x 1)-Sb surface the long-range order is
   broken up by a high density of antiphase domain boundaries. Similar
   defects are rarely observed in Si(100) (2 x 1)-As. Using local density
   functional theory, we have studied the structural and electronic
   properties of the Si(100) (2 x 1)-Sb and Si(100) (2 x 1)-As surfaces
   with a defect: one dimer is shifted to the trench between two rows. We
   have found that energetics crucially determines the formation and
   stability of the defects. In the case of Sb, the total energy is similar
   to 0.01 eV per dimer higher than the ideal (2 x 1) structure. For As,
   this value is similar to 0.30 eV per dimer. This difference can be
   traced down to the atomic structure: the As-As dimer bond is weakened by
   the disorder, while the Sb-Sb bond is almost unaffected. We have also
   calculated the energy barriers between the ordered surface and the one
   with defects. (C) 1999 Published by Elsevier Science B.V. All rights
   reserved.}},
