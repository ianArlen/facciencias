@article{ ISI:000082196400006,
Author = {Taud, H and Parrot, JF and Alvarez, R},
Title = {{DEM generation by contour line dilation}},
Journal = {{COMPUTERS \& GEOSCIENCES}},
Year = {{1999}},
Volume = {{25}},
Number = {{7}},
Pages = {{775-783}},
Month = {{AUG}},
Abstract = {{A method is developed to generate digital elevation models by means of
   the dilation of contour lines stored in a raster grid. An iterative
   procedure produces an extension of contour lines by applying alternative
   four-connected and eight-connected erosions of the background until the
   produced surfaces become contiguous. The limits between these surfaces
   correspond to a new family of intermediate contour lines that are added
   to the raster grid. This procedure stops when no new contour lines can
