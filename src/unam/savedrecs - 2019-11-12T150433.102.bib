@inproceedings{ ISI:000230926300025,
Author = {Rodriguez-Romo, S and Taft, EJ},
Editor = {{DelaPena, JA and Vallejo, E and Atakishiyev, N}},
Title = {{One-sided Hopf algebras}},
Booktitle = {{Algebraic Structures and Their Representations}},
Series = {{CONTEMPORARY MATHEMATICS SERIES}},
Year = {{2005}},
Volume = {{376}},
Pages = {{377-384}},
Note = {{15th Latin American Algebra Colloquium, Cocoyoc, MEXICO, JUL 20-26, 2003}},
Abstract = {{A one-sided Hopf algebra, say a left Hopf algebra, is a bialgebra with a
   linear endomorphism S which satisfies the left antipode condition E
   S(h(1))h(2) = is an element of(h)1. We first recall the construction of
   a one-sided Hopf algebra for which S is an algebra antimorphism, and
   another one in which no left antipode is an algebra antimorphism. Then
   we describe our recent attempt to construct a one-sided Hopf algebra in
   the framework of quantum groups. Due to some technical choices, the
   result is a (two-sided) Hopf algebra, but one which remains
   noncommutative when q = 1. Finally, we indicate some ongoing work in
   which we are trying several variations of these technical choices.
   However, at the moment, we still have not constructed a one-sided
   quantum group.}},
