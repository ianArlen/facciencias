@article{ ISI:000226915400002,
Author = {Gonzalez-Hernandez, JC and Jimenez-Estrada, M and Pena, A},
Title = {{Comparative analysis of trehalose production by Debaryomyces hansenii
   and Saccharomyces cerevisiae under saline stress}},
Journal = {{EXTREMOPHILES}},
Year = {{2005}},
Volume = {{9}},
Number = {{1}},
Pages = {{7-16}},
Month = {{FEB}},
Abstract = {{The comparative analysis of growth, intracellular content of
   Na(+)supercript stop and K+, and the production of trehalose in the
   halophilic Debaryomyces hansenii and Saccharomyces cerevisiae were
   determined under saline stress. The yeast species were studied based on
   their ability to grow in the absence or presence of 0.6 or 1.0 M NaCl
   and KCl. D. hansenii strains grew better and accumulated more Na+ than
   S. cerevisiae under saline stress (0.6 and 1.0 M of NaCl), compared to
   S. cerevisiae strains under similar conditions. By two methods, we found
   that D. hansenii showed a higher production of trehalose, compared to S.
   cerevisiae; S. cerevisiae active dry yeast contained more trehalose than
   a regular commercial strain (S. cerevisiae La Azteca) under all
   conditions, except when the cells were grown in the presence of 1.0 M
   NaCl. In our experiments, it was found that D. hansenii accumulates more
   glycerol than trehalose under saline stress (2.0 and 3.0 M salts).
   However, under moderate NaCl stress, the cells accumulated more
   trehalose than glycerol. We suggest that the elevated production of
   trehalose in D. hansenii plays a role as reserve carbohydrate, as
   reported for other microorganisms.}},
