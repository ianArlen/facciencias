@inproceedings{ ISI:000236985300035,
Author = {Watson, Alan M. and Stapelfeldt, Karl R.},
Editor = {{HidalgoGamez, AM and Gonzalez, JJ and Espinosa, JMR and TorresPeimbert, S}},
Title = {{The optical-NIR opacity law in YSO disks evidence for dust growth?}},
Booktitle = {{II INTERNATIONAL GTC WORKSHOP: SCIENCE WITH GTC 1ST-LIGHT INSTRUMENTS
   AND THE LMT}},
Series = {{Revista Mexicana de Astronomia y Astrofisica-Serie de Conferencias}},
Year = {{2005}},
Volume = {{24}},
