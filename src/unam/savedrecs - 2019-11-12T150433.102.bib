@article{ ISI:000227309500014,
Author = {Pereda-Miranda, R and Escalante-Sanchez, E and Escobedo-Martinez, C},
Title = {{Characterization of lipophilic pentasaccharides from beach morning glory
   (Ipomoea pes-caprae)}},
Journal = {{JOURNAL OF NATURAL PRODUCTS}},
Year = {{2005}},
Volume = {{68}},
Number = {{2}},
Pages = {{226-230}},
Month = {{FEB}},
Abstract = {{The hexane-soluble extract from the aerial parts of the herbal drug
   Ipomoea pes-caprae (beach morning-glory), through preparative-scale
   recycling HPLC, yielded six lipophilic glycosides, namely, five new
   pentasaccharides of jalapinolic acid, pescaproside A (1) and pescapreins
   I-IV (2-5), as well as the known stoloniferin III (6). Saponification of
   the crude resin glycoside mixture yielded simonic acid B (7) as the
   glycosidic acid component, whereas the esterifying residues of the
   natural oligosaccharides comprised five fatty acids: 2-methylpropanoic,
   (2S)-methylbutyric, n-hexanoic, n-decanoic, and n-dodecanoic acids.
   Pescaproside A (1), an acylated glycosidic acid methyl ester, is related
   structurally to the product obtained from the macrolactone hydrolysis of
   pescapreins I-IV (2-5). All the isolated compounds (1-6), characterized
   through high-field NMR spectroscopy, were found to be weakly cytotoxic
   to a small panel of cancer cell lines.}},
