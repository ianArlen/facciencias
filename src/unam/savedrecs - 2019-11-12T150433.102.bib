@article{ ISI:000227961100034,
Author = {Navarrete, JM and Urbina, VM and Martinez, T and Cabrera, L},
Title = {{Autoradiography of mineral ions in green leaves and flowers, absorbed
   with and without synthetic fulvic acids}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2005}},
Volume = {{263}},
Number = {{3}},
Pages = {{779-781}},
Month = {{FEB}},
Abstract = {{Autoradiography of green leaves and flowers are presented, after
   adsorption with three kinds of mineral ions: the very common one in
   nature Na+, the toxic CrO42- and the nutritional I-,labeled with Na-22,
   Cr-51 and I-131 respectively, in order to test directly how these ions
   are distributed in a different way throughout the vegetable tissues,
   depending whether they are associated or not to synthetic fulvic acids
   commercially produced as fertilizers in Mexico. Two kinds Polaroid films
   have been used and the necessary exposition time for the activity used
   is one week as a mimimum, since these tracers are gamma- and X-rays
   emitters and, therefor, their action on the film is much weaker than
   that obtained with beta particles. It has been verified the effect
   brought about by synthetic fulvic acids, as fertilizers which distribute
   more evenly mineral ions into vegetables tissues, when they form a
   complex {[}I-], but they act as discriminators in the case of toxic ions
   {[}CrO42-] or when they do not form a complex {[}Na+].}},
