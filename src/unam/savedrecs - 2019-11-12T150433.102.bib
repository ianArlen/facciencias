@inproceedings{ ISI:000229609400014,
Author = {Centeno, C and Ruiz, U},
Editor = {{Wahl, KJ and Huber, N and Mann, AB and Bahr, DF and Cheng, YT}},
Title = {{Effect of fluoride on mechanical properties of teeth demineralized by
   use of orthodontic appliances}},
Booktitle = {{FUNDAMENTALS OF NANOINDENTATION AND NANOTRIBOLOGY III}},
Series = {{Materials Research Society Symposium Proceedings}},
Year = {{2005}},
Volume = {{841}},
Pages = {{93-98}},
Note = {{Symposium on Fundamentals of Nanoindentation and Nanotribology III held
   at the 2004 MRS Fall Meeting, Boston, MA, NOV 29-DEC 03, 2004}},
Organization = {{MRS}},
Abstract = {{The risk of dental caries increases with the use of orthodontic
   appliances, and it does not only depend on a patient's oral hygiene.
   Caries cause teeth enamel dernincralization close to the orthodontic
   bracket. The hardness of the teeth consequently decreases, and they
   become brittle and loose. The type of adhesive used to fix brackets may
   or may not reduce enamel demineralization. Previous studies have shown
   that a resin-modified glass ionomer (RMGI) inhibits demineralization.
   The purpose of this work is to evaluate the effect of a
   fluoride-releasing bracket on mechanical properties of dentin and enamel
   on regions adjacent to orthodontic brackets bonded with RMGI and
   composite resin cements. The remineralization effect due to fluoride on
   teeth was assessed by measuring hardness and reduced elastic modulus on
   dentin, enamel and dentin-enamel junction (DEJ) around the bracket area
   by nanoindentation methods. Nanoindentation was performed using a
   TriboScope from Hysitron. For this purpose two sets of polished samples
   were prepared: one sample was a cut along a longitudinal section and the
   other one along a cross-section of a tooth with orthodontic treatment
   using RMGl. Anisotropy in the mechanical properties of the teeth is
   observed along each direction.}},
