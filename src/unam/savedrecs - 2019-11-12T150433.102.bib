@inproceedings{ ISI:000231877300013,
Author = {Mendoza, B},
Editor = {{JatencoPereira, V and Chian, ACL and ValdesGalicia, JF and Shea, MA}},
Title = {{Total solar irradiance and climate}},
Booktitle = {{FUNDAMENTALS OF SPACE ENVIRONMENT SCIENCE}},
