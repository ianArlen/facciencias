@article{ ISI:000234137300016,
Author = {Gama-Castro, JE and de Tapia, EM and Solleiro-Rebolledo, E and Ibarra, E
   and Sedov, S and Jasso-Castaneda, C and Vallejo-Gomez, E and Pi-Puig, T
   and Cabadas-Baez, H},
Title = {{Incorporation of ethnopedological knowledge in the study of soils in the
   Teotihuacan Valley, Mexico}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{2005}},
Volume = {{38}},
Number = {{1}},
Pages = {{S95-S98}},
Note = {{International Conference on Soil Classification, Petrozavodsk, RUSSIA,
   AUG 03-08, 2004}},
Abstract = {{Some aspects of indigenous cartography and knowledge of the soil are
   preserved in sixteenth-century documents such as the Santa Maria
   Asuncion and Vergara codices. These codices contains a pictorial
   representation from the 1540s of soil types found in agricultural plots
   near Texcoco, in the state of Mexico. In order to evaluate its utility,
   photointerpretation, soil classification, and interviews with producers
   from various Nahuatl-speaking communities in the modern states of
   Mexico, Morelos, and Guerrero were undertaken to map the soil units
   present in the Teotihuacan Valley, state of Mexico, utilizing the
   traditional Nahuatl soil classification system. Thirteen soil units
   corresponding to the Nahuatl classification were detected in the study
   area. Elderly local producers were able to identify the significance of
   the Nahuatl names given to the soils and understood their spatial
   distribution, especially when represented on a digital elevation model.
   The producers themselves emphasized that the combination of both
   traditional approaches from their knowledge of soils and modern
   technology is required to develop useful applications.}},
