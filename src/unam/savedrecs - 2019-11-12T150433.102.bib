@article{ ISI:000226971000016,
Author = {Gonzalez-Moran, MG},
Title = {{Immunohistochemical detection of estrogen receptor alpha in the growing
   and regressing ovaries of newly hatched chicks}},
Journal = {{JOURNAL OF MOLECULAR HISTOLOGY}},
