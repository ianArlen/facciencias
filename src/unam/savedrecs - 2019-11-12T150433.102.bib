@article{ ISI:000228649400005,
Author = {Corona, AM and Morrone, JJ},
Title = {{Track analysis of the species of Lampetis (Spinthoptera) Casey, 1909
   (Coleoptera : Buprestidae) in North America, Central America, and the
   West Indies}},
Journal = {{CARIBBEAN JOURNAL OF SCIENCE}},
Year = {{2005}},
Volume = {{41}},
Number = {{1}},
Pages = {{37-41}},
Abstract = {{Based on the comparison of 29 individual tracks, distributional patterns
   of species of Lampetis (Spinthoptera) in North and Central America, and
   the West Indies were analyzed by a track analysis. Fifteen generalized
   tracks and six nodes were found. Three generalized tracks were found in
   the Nearctic region, six in the Mexican transition zone, and eight in
   the Neotropical region (two in the Antillean dominion and six in the
   Mesoamerican dominion). Nodes were located in the Mexican transition
   zone (Sierra Madre del Sur and Transmexican Volcanic Belt biogeographic
   provinces) and the Neotropical region (Mexican Gulf and Mexican Pacific
   Coast biogeographic provinces). Two nodes were located in the Isthmus of
   Tehuantepec, which represents a key area to the understanding of the
   biotic evolution of Mesoamerica.}},
