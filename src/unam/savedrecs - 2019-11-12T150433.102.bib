@article{ ISI:000227732100001,
Author = {Bugeaud, Y and Luca, F and Mignotte, A and Siksek, S},
Title = {{On Fibonacci numbers with few prime divisors}},
Journal = {{PROCEEDINGS OF THE JAPAN ACADEMY SERIES A-MATHEMATICAL SCIENCES}},
Year = {{2005}},
Volume = {{81}},
Number = {{2}},
Pages = {{17-20}},
Month = {{FEB}},
Abstract = {{If n is a positive integer, write F-n for the nth Fibonacci number, and
   w(n) for the number of distinct prime divisors of n. We give a
   description of Fibonacci numbers satisfying w(F-n) <= 2. Moreover, we
   prove that the inequality w(F-n) >= (log n)(log 2+o(1)) holds for almost
   all n. We conjecture that w(F-n) >> log n for composite n, and give a
   heuristic argument in support of this conjecture.}},
