@article{ ISI:000226300100006,
Author = {Paredes, R and Barrio, RA},
Title = {{Crystalline and non-crystalline growth of two-dimensional
   square-triangle lattices}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{2005}},
Volume = {{351}},
Number = {{2}},
Pages = {{136-143}},
Month = {{JAN 15}},
Abstract = {{We have studied the possible structures of two-dimensional lattices
   composed of squares and equilateral triangles obtained by a coherent way
   of growing them. The model of growth uses the Stochastic Matrix Method,
   which is based on the concept of statistical agglomeration of elementary
   units. The studied lattices satisfy two conditions, they are free of
   voids and defects and they conserve the relative concentration of
   elementary units before and after the growth process. In order to
   compare our results with some specific tilings, we also define an
   algorithm to construct crystalline families of a given symmetry. (C)
   2004 Published by Elsevier B.V.}},
