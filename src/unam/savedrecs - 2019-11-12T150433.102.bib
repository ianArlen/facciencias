@article{ ISI:000234751500005,
Author = {Carballo, JL and Cruz-Barraza, JA},
Title = {{Cliona microstrongylata, a new species of boring sponge from the Sea of
   Cortes (Pacific Ocean, Mexico)}},
Journal = {{CAHIERS DE BIOLOGIE MARINE}},
Year = {{2005}},
Volume = {{46}},
Number = {{4}},
Pages = {{379-387}},
Abstract = {{A new species of excavating, sponge of the family Clionaidae, Cliona
   microstrongylata sp. nov. was found in detritic bottoms from the Sea of
   Cortes at 26 m depth. The type grows in alpha stage boring a dead
   bivalve shell. It presents very small orange-red papillae (from 150 to
   850 mu m in diameter), which are regularly distributed oil the surface
   of the shell. They are level with the surface of the substratum and
   don't fuse. The boring activity of the species produces a network of
   reticulate spherical-ovoid or quadrangulate chambers with the longer
   axis from 1.6 to 2.6 mm in length. Tylostyles are thin or ensiform
   (shaft thicker in central part). Reduced tylostrongyles occasionally
   appear. However, the most notable characteristic of the new species is
   the presence Of unusual oval and Curved bean-like rnicrostrongyles,
   which are unique in the genus Cliona so far. Eggs from 70 to 90 mu m in
   diameter were present in different parts of the choanosome. After the
   present study, the number of boring species along the Mexican Pacific
   coast has increased to 15 species.}},
