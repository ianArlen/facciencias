@article{ ISI:000230733500004,
Author = {Castro, M and Cruz, J and Lopez-Sandoval, H and Barba-Behrens, N},
Title = {{On the (CHCu)-Cu-... agostic interaction: chiral copper(II) compounds
   with ephedrine and pseudoephedrine derivatives}},
Journal = {{CHEMICAL COMMUNICATIONS}},
Year = {{2005}},
Number = {{30}},
Pages = {{3779-3781}},
Abstract = {{The ephedrine derivative, (H(2)ceph), yields {[}Cu(Hceph)(2)], showing a
   (CHCu)-Cu-...( II) agostic interaction; while in the analogous compound
   {[}Cu(Hcpse)(2)], with pseudoephedrine (H(2)cpse), that interaction is
   absent, despite the fact that these two diasteromers differ only in the
   orientation of the methyl and phenyl groups: erythro in H(2)ceph and
   threo in H(2)cpse. The X-ray crystal structure of {[}Cu(Hceph)(2)],
   indicates a (CuHC)-H-... length of 2.454 angstrom and the theoretical
   study reveals the formation of a (CuHC)-H-... bond since the associated
   electronic density shows both a bond critical point and a bond ring
   critical point.}},
