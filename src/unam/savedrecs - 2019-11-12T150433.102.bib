@article{ ISI:000226120100006,
Author = {Dainese, A and Loizides, C and Paic, G},
Title = {{Leading-particle suppression in high energy nucleus-nucleus collisions}},
Journal = {{EUROPEAN PHYSICAL JOURNAL C}},
Year = {{2005}},
Volume = {{38}},
