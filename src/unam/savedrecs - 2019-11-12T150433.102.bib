@article{ ISI:000224589700003,
Author = {Otero, JA and Rodriguez-Ramos, R and Monsivais, G and Perez-Alvarez, R},
Title = {{Dynamical behavior of a layered piezocomposite using the asymptotic
   homogenization method}},
Journal = {{MECHANICS OF MATERIALS}},
Year = {{2005}},
Volume = {{37}},
Number = {{1}},
Pages = {{33-44}},
Month = {{JAN}},
Abstract = {{In the present paper, a dynamic model of the piezoceramic polymer
   composite with 2-2 connectivity is presented. Expressions for the
   effective coefficients (elastic, piezoelectric and dielectric) using the
   asymptotic homogenization method (AHM) are found. The formulation of the
   dynamical problem is given in term of overall properties of the
   composite for the first approximation of AHM. The dispersion relations
   are computed and their curves are shown. An analysis of the dispersion
   curves is presented for different volume fractions. The behavior of the
   mechanical displacement and the electric potential is illustrated. (C)
   2004 Elsevier Ltd. All rights reserved.}},
