@article{ ISI:000227319800090,
Author = {Sahu, S and D'Olivo, JC},
Title = {{Can there be neutrino oscillations in a gamma-ray burst fireball?}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2005}},
Volume = {{71}},
Number = {{4}},
Month = {{FEB}},
Abstract = {{The central engine which powers the gamma-ray burst fireball produces
   neutrinos in the energy range of about 5-20 MeV. Fractions of these
   neutrinos may propagate through the fireball which is far away from the
   central engine. We have studied the propagation of these neutrinos
   through the fireball which is contaminated by baryons and have shown
   that resonant conversion of neutrinos is possible for the oscillations
   of nu(e)<---->nu(mu,tau), nu(e)<---->nu(s), and (nu) over bar
   (mu,tau)<---->(nu) over bar (s) if the neutrino mass square difference
   and mixing angle are in the atmospheric and/or Liquid Scintillator
   Neutrino Detector range. On the other hand, it is probably difficult for
   neutrinos to have resonant oscillation if the neutrino parameters are in
   the solar neutrino range. From the resonance condition, we have
   estimated the fireball temperature and the baryon load in it.}},
