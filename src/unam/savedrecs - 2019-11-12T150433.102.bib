@article{ ISI:000228212600004,
Author = {Quintana-Hau, JD and Cruz-Olmos, E and Lopez-Sanchez, MI and
   Sanchez-Castellanos, V and Baiza-Duran, L and Gonzalez, JR and
   Tornero-Montano, R and Mondragon-Flores, R and Hernandez-Santoyo, A},
Title = {{Characterization of the novel ophthalmic drug carrier sophisen in two of
   its derivatives: 3A Ofteno (TM) and modusik-A Ofteno (TM)}},
Journal = {{DRUG DEVELOPMENT AND INDUSTRIAL PHARMACY}},
Year = {{2005}},
Volume = {{31}},
Number = {{3}},
Pages = {{263-269}},
Abstract = {{Sophisen, a new ophthalmic drug carrier, was characterized using
   physicochemical and morphological criteria. Diclofenac belongs to a
   nonsteroidal anti-inflammatory molecule group and its ophthalmic use
   avoids side effects produced by steroid drugs. Cyclosporine-A is a
   cyclic peptide used as an immunosuppressive when administrated
   systemically. Its application in ophthalmology has been reported, but it
   is a very poor soluble drug. Diclofenac sodium and Cyclosporine-A were
   mixed with Sophisen to render two new ophthalmic solutions that were
   named 3A Ofteno(TM) and Modusik-A Ofteno(TM), respectively. Based on
   transmission electron microscopy and dynamic light scattering studies,
   we concluded that Sophisen is a polydisperse solution with a molecular
   weight of 413 +/- 122 kDa, whereas 3A Ofteno(TM) and Modusik-A
   Ofteno(TM) are monodisperse solutions with molecular weights of 169 +/-
   44 and 153 +/- 10, respectively. Sophisen was shown to be a good carrier
   for diclofenac sodium as evaluated by passive diffusion through the
   cornea. A comparative study suggests that diclofenac applied as eye
