@article{ ISI:000232283400005,
Author = {Tapia, A and Canto, G and Murrieta, G and de Coss, R},
Title = {{Electronic structure of FCC carbon}},
Journal = {{JETP LETTERS}},
Year = {{2005}},
Volume = {{82}},
Number = {{3}},
Pages = {{120-123}},
Abstract = {{We report first-principles calculations of the electronic structure for
   carbon in the fcc structure with the experimentally observed lattice
   parameter. The calculated orbital population shows that the chemical
   bond in fcc carbon is close to the s(2)p(2) bonding with a small s-p
   hybridization. We find that, in contrast to graphite and diamond, fcc
   carbon exhibits metallic behavior with an electronic density of states
   at the Fermi level of 0.5 states/(eV atom). (C) 2005 Pleiades
   Publishing, Inc.}},
