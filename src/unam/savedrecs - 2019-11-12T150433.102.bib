@article{ ISI:000227910300002,
Author = {De Koninck, JM and Luca, F and Shparlinski, IE},
Title = {{Powerful numbers in short intervals}},
Journal = {{BULLETIN OF THE AUSTRALIAN MATHEMATICAL SOCIETY}},
Year = {{2005}},
Volume = {{71}},
Number = {{1}},
Pages = {{11-16}},
Month = {{FEB}},
Abstract = {{Let kappa >= 2 be an integer. We show that there exist infinitely many
   positive integers N such that the number of kappa-full integers in the
   interval (N-kappa, (N + 1)(kappa)) is at least (log N)(1/3+o(1)). We
   also show that the ABC-conjecture implies that for any fixed delta > 0
   and sufficiently large N, the interval (N, N + N1-(2+delta)/kappa)
   contains at most one kappa-full number.}},
