@article{ ISI:000234895600010,
Author = {Cisneros-Molina, JL},
Title = {{An invariant of 2x2 matrices}},
Journal = {{ELECTRONIC JOURNAL OF LINEAR ALGEBRA}},
Year = {{2005}},
Volume = {{13}},
Pages = {{146-152}},
Abstract = {{Let W be the space of 2 x 2 matrices over a field K. Let f be any linear
   function on W that kills scalar matrices. Let A. W and define f(k)(A) =
   f(A(k)). Then the quantity f(k+ 1)(A)/f(A) is invariant under
   conjugation and moreover f(k+ 1)(A)/f(A) = trace S(k)A, where S(k)A is
   the k-th symmetric power of A, that is, the matrix giving the action of
   A on homogeneous polynomials of degree k.}},
