Title = {{Potable water and sanitation in Tenochtitlan: Aztec culture}},
Booktitle = {{Insights into Water Management: Lessons from Water and Wastewater
   Technologies in Ancient Civilizations}},
Series = {{WATER SCIENCE AND TECHNOLOGY: WATER SUPPLY}},
