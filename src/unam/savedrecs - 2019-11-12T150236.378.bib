Author = {Lopez-Bastidas, Catalina and Maytorena, Jesus A. and Mireles, Francisco},
Editor = {{Ulloa, SE and Prieto, PP}},
Title = {{Interplay of the Rashba and Dresselhaus spin-orbit coupling in the
   optical spin susceptibility of 2D electron systems}},
Booktitle = {{PHYSICA STATUS SOLIDI C - CURRENT TOPICS IN SOLID STATE PHYSICS, VOL 4,
   NO 11}},
Series = {{PHYSICA STATUS SOLIDI C-CURRENT TOPICS IN SOLID STATE PHYSICS}},
