@inproceedings{ ISI:000255181703153,
Author = {Ferreira, Alejandra and Bejarano, Francisco and Fridman, Leonid},
Book-Group-Author = {{IEEE}},
Title = {{Output hierarchical super twisting observation based robust feedback
   control: With or without chattering?}},
Booktitle = {{PROCEEDINGS OF THE 46TH IEEE CONFERENCE ON DECISION AND CONTROL, VOLS
   1-14}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2007}},
Pages = {{5989+}},
