
@article{ ISI:000244857200002,
Author = {Barragan, Ricardo and Szives, Ottilia},
Title = {{New records of Mathoceras casey (Deshayesitidae, Ammonoidea) from the
   Aptian (Lower Cretaceous) of Mexico and Hungary: Biostratigraphic and
   paleobiogeographic implications}},
Journal = {{GEOBIOS}},
Year = {{2007}},
Volume = {{40}},
Number = {{1}},
Pages = {{21-30}},
Month = {{JAN-FEB}},
Abstract = {{The deshayesitid ammonite genus Mathoceras is recorded for the first
   time from Aptian (Lower Cretaceous) strata in Mexico and Hungary. The
   present paper comprises descriptions of M. cf. venezolanum Renz, M. cf.
   sumerensis (Stoykova), M. celestinae nov. sp., and M. neoleonensis nov.
   sp. In Mexico, Mathoceras has been collected from an Aptian sequence in
   which there is a distinctive horizon defined by an ammonite assemblage
   of mid-Aptian age. Material from Hungary comes from a condensed bed that
   yields biota of early Aptian to early Albian age. Previously, the genus
   was held to be confined to Venezuela, Tunisia, the Balearic Islands
   (Spain) and Bulgaria. The new records, from two extremes of the former
   Tethys, increase the paleobiogeographic significance of this poorly
   known Aptian ammonite genus. (c) 2006 Elsevier Masson SAS. All rights
   reserved.}},
