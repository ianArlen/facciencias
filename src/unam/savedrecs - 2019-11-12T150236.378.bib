@article{ ISI:000251446800006,
Author = {Mayret-Mesquiti, Mayi and Perez-Mendez, Oscar and Rodriguez, Maria Elena
   and Fortoul, Teresa I. and Gorocica, Patricia and Bernal-Alcantara,
   Demetrio and Montano, Luis Felipe and Alvarado-Vasquez, Noe},
Title = {{Hypertriglyceridemia is linked to reduced nitric oxide synthesis in
   women with hypertensive disorders of pregnancy}},
Journal = {{HYPERTENSION IN PREGNANCY}},
Year = {{2007}},
Volume = {{26}},
Number = {{4}},
Pages = {{423-431}},
Abstract = {{Background: Hypertensive disorders of pregnancy (HDP), which affect 8\%
   to 15\% of pregnancies, are associated with nitric oxide dysfunction and
   hyperlipidemia, but their precise role in HDP remains controversial. In
   order to gain more insight into the mechanisms underlying HDP, we
   evaluated some indicators common to the diseases associated with
   endothelial dysfunction. Methods: Plasma samples were obtained from 47
   normotensive women (control group) and from 27 women with HDP
   (experimental group). All women were 7 months pregnant. Body mass index
   as well as triglycerides, nitrite concentrations, total cholesterol,
   LDL-cholesterol, HDL-cholesterol, glucose, and glyeated hemoglobin were
   determined. Results: Our results showed significant differences in body
   mass index (30.4 +/- 1.3 vs 28.3 +/- 0.6 kg/m(2), P < 0.05),
   triglycerides (363 +/- 137 vs. 263 80 mg/dL, p < 0.01), nitrites (19.6
   +/- 5.2 vs. 15.2 +/- 5.0 mu mol/L, p < 0.01), and glucose (92 25 us. 81
   +/- 10.8 mg/dL, p < 0.05) in women from the experimental group compared
   with the control group. Interestingly, nitric oxide synthesis was
   significantly reduced when triglycerides and cholesterol concentrations
   were increased (p < 0.018 and p < 0.002, respectively). Moreover, there
   was a strong association (odds ratio, 3.5) between a family history of
   type 2 diabetes mellitus and the development of HDP, especially
   preeclampsia. Conclusions: It may be useful to screen pregnant women for
   plasma nitrites and serum triglycerides to identify those at risk of
   developing HDP, especially in women with a family history of type 2
   diabetes mellitus.}},
