@inproceedings{ ISI:000256282900008,
Author = {de Urquijo, J.},
Editor = {{Petrovic, ZL and Mason, NJ and Hamaguchi, S and RadmilovicRadenovic, M}},
Title = {{Is CF3I a good gaseous dielectric? A comparative swarm study of CF3I and
   SF6}},
