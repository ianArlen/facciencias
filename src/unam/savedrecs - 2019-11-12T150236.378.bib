@article{ ISI:000245902500011,
Author = {Gutman, Ivan and Firoozabadi, Sanaz Zare and de la Pena, Jose Antonio
   and Rada, Juan},
Title = {{On the energy of regular graphs}},
Journal = {{MATCH-COMMUNICATIONS IN MATHEMATICAL AND IN COMPUTER CHEMISTRY}},
Year = {{2007}},
Volume = {{57}},
Number = {{2}},
Pages = {{435-442}},
Abstract = {{The energy E of any n-vertex regular graph G of degree r, r > 0, is
   greater than or equal to n. Equality holds if and only if every
   component of G is isomorphic to the complete bipartite graph Kr,r. If G
   is triangle- and quadrangle-free, then E >= nr/root 2-r-1. In
   particular, for any fullerene and nanotube with n carbon atoms, 1.34n <=
   E <= 1.73n.}},
