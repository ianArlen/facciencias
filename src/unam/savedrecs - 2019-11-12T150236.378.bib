Title = {{A competitiveness model for Mexican software development companies}},
Booktitle = {{PICMET `07: PORTLAND INTERNATIONAL CENTER FOR MANAGEMENT OF ENGINEERING
   AND TECHNOLOGY, VOLS 1-6, PROCEEDINGS: MANAGEMENT OF CONVERGING
   TECHNOLOGIES}},
