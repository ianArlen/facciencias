@article{ ISI:000252009200010,
Author = {Antonio Reyes-Agueero, Juan and Carlin-Castelan, Fernando and Rogelio
   Aguirre R, Juan and Hernandez, Hector M.},
Title = {{Preparation of Opuntia herbarium specimens}},
Journal = {{HASELTONIA}},
Year = {{2007}},
Number = {{13}},
Pages = {{76-82}},
Abstract = {{We present a best-practice method for preparing high-quality Opuntia
   herbarium specimens, from collection methods to storage. A botanical
   press having wide, thick, wooden boards is used for cladode drying. A
   layer consisting of corrugated cardboard, absorbent paper, newspaper,
   and a thin wood laminate is placed between each cladode. The botanical
   press is placed in a forced-air furnace at 50-90 degrees C. In the case
   of nopalitos (young, developing cladodes), podaria are first removed
   from one side, followed by drying at 45-70 degrees C. Flowers are
   pressed immediately after collection at 45-60 degrees C. Fruits are
   preserved in 70\% ethanol. Seeds and pulp are removed, and empty fruits
   are immersed in alcohol for one month; afterwards they are removed from
   the solution and washed. Fruits are then drained, stuffed with fine
   sand, and placed in a forced-air furnace at 40-45 degrees C. Finally,
   the sand is removed and the cavity is filled with resin.}},
