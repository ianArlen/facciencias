@inproceedings{ ISI:000252560800226,
Author = {Segura-Salazar, Juan and Frausto-Solis, Juan},
Book-Group-Author = {{ATlantic PRess}},
Title = {{A partition rule for SAT solvers: The multiple partition rule (MPR)}},
Booktitle = {{PROCEEDINGS OF THE INTERNATIONAL CONFERENCE ON INTELLIGENT SYSTEMS AND
   KNOWLEDGE ENGINEERING (ISKE 2007)}},
Series = {{Advances in Intelligent Systems Research}},
Year = {{2007}},
Note = {{2007 International Conference on Intelligent Systems and Knowledge
   Engineering, Southwest Jiaotong Univ, Chengdu, PEOPLES R CHINA, OCT
   15-16, 2007}},
Abstract = {{We propose a new partition rule for DPLL-based SAT Solvers. Most of the
   complete SAT solver usually are based on Davis, Logemann and Loveland
   (DPLL) rules. One most DPLL rule actually used in the modern algorithms
   is the Classical Partition Rule (CPR), that divides the problem into
   sub-problems (resolvents) and thereby it finds a solution through a
   decision tree. In this paper a new partition rule named Multiple
   Partition Rule (MPR) is presented. MPR generates a new decision tree
   according to clauses instead CPR which generates a decision tree
   according to variables. MPR can be used for developing new SAT's
   algorithms and to improve existence ones that use CPR. Experimental
   results comparing MPR versus CPR show that using MPR makes more
   efficient solutions than CPR.}},
