@article{ ISI:000255022300007,
Author = {Vargas-Malvaez, Edgar and Pimentel, Georgina and Trujillo, Gustavo and
   Martin, Erika},
Title = {{Synthesis of rhodium (I) complexes with mono and dithiolato ligands:
   Application in catalytic hydroformylation of olefins}},
Journal = {{JOURNAL OF THE MEXICAN CHEMICAL SOCIETY}},
Year = {{2007}},
Volume = {{51}},
Number = {{1}},
Pages = {{33-38}},
Month = {{JAN-MAR}},
Abstract = {{The thiols,1-phenylethanothiol 1 and 2,4-pentanodithiol 2 were
   synthesised and used to prepare binuclear rhodium species 3, 4 of the
   type {[}Rh-2(mu-L)(n)(cod)(2)] (L = 1, n = 2; L = 2, n = 1; cod =
   1,5cyclooctadiene). NMR and FAB(+) mass spectrometry data are consistent
   with a binuclear structure for the species. These complexes, plus PPh3
   or diphosphines (dppp, dppb), were used as catalytic precursors for the
   hydroformylation of styrene under mild conditions. Fairly good
   activities and regioselectivities were achieved for catalytic system
   3/PPh3.}},
