@article{ ISI:000245787200008,
Author = {Moreno-Coutino, Ana and Calderon-Ezquerro, Carmen and Drucker-Colin,
   Rene},
Title = {{Long-term changes in sleep and depressive symptoms of smokers in
   abstinence}},
