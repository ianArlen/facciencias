@article{ ISI:000243882600004,
Author = {Soto-Cruz, Isabel and Oliver, Janet M. and Ortega, Enrique},
Title = {{Analysis of proteins binding to the ITAM motif of the beta-subunit of
   the high-affinity receptor for IgE (Fc epsilon RI)}},
Journal = {{JOURNAL OF RECEPTORS AND SIGNAL TRANSDUCTION}},
