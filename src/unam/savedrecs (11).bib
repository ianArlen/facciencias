@article{ ISI:000427306400010,
Author = {Pruneda Senties, David},
Title = {{A REFUSAL TO RECEIVE: READING (OF THE) CRITICISM}},
Journal = {{452 F-REVISTA DE TEORIA DE LA LITERATURA Y LITERATURA COMPARADA}},
Year = {{2018}},
Number = {{18}},
Pages = {{163-179}},
Abstract = {{This article discusses the differences between reading as an activity of
   reception and reading as an activity of criticism. By means of a
   revision of Wolfgang Iser's, Hans Robert Jauss,' and Stanley Fish's
   proposals, this paper's argument is that the condition of possibility
   for critical reading is the refusal to receive the literary text.
   Therefore, critical reading must elaborate its own object of study in
   order to produce a critical discourse. Instead of a commentary on a
   literary text, from this perspective literary studies become an analysis
   of an act of reading.}},
