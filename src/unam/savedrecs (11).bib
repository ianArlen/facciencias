@article{ ISI:000412570000013,
Author = {Flores-Garrido, Marisol and Ariel Carrasco-Ochoa, J. and
   Martinez-Trinidad, Jose Fco.},
Title = {{Extensions to AGraP Algorithm for Finding a Reduced Set of Inexact Graph
   Patterns}},
Journal = {{INTERNATIONAL JOURNAL OF PATTERN RECOGNITION AND ARTIFICIAL INTELLIGENCE}},
Year = {{2018}},
Volume = {{32}},
