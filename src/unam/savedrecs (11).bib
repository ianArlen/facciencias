@article{ ISI:000423635000005,
Author = {Willers, Susanne},
Title = {{Migration and reproductive strategies of Central American women in
   transit through Mexico}},
Journal = {{JOURNAL OF FAMILY STUDIES}},
Year = {{2018}},
Volume = {{24}},
Number = {{1, SI}},
Pages = {{59-75}},
Note = {{3rd ISA Forum of Sociology on Futures We Want - Global Sociology and the
   Struggles for a Better World, RC 06 Family Res, Vienna, AUSTRIA, JUL
   10-14, 2016}},
Organization = {{ISA; RC 31 Sociol Migrat}},
Abstract = {{For many Central American women, migration to the United States is a
   strategy for survival in order to cope with myriad situations of
   violence and to ensure their families' well-being. Yet, migration
   policies and generalized social violence have turned transit through
   Mexico into a prolonged experience with high probabilities of exposure
   to violence. This article examines the impact violence and uncertainty
   during transit have on the strategies of social reproduction of migrant
   mothers. The research draws on qualitative interviews with women in
   transit in Mexico. The analysis shows how strategies for the social
   reproduction of women vary, depending on the changing circumstances of
   transit and concrete experiences of violence. The data provide insight
   into the impact of structural changes in migration regimes on migrant
   women's capacities to provide care and other types of support to their
   families.}},
