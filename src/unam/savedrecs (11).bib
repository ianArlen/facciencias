@article{ ISI:000424201200005,
Author = {Meza-Prieto, Bertha and Aparicio, Javier},
Title = {{Evaluation of small-scale hydropower potential in watersheds using a
   rainfall-runoff model}},
Journal = {{TECNOLOGIA Y CIENCIAS DEL AGUA}},
Year = {{2018}},
Volume = {{9}},
Number = {{1}},
Pages = {{69-87}},
Month = {{JAN-FEB}},
Abstract = {{One way of promoting renewable energy-based projects, such as the
   small-scale hydroelectric systems, is to determine viable placements for
   them, as well as the exploitable energy. This work presents a simple
   method to estimate the hydropower potential in watersheds. The main
   advantage of this method is that the continuous hydrological modeling is
   performed using free software. The delimitation and cartographic
   processing of the sub basins, as well as the longitudinal profile
   tracing of the riverbed is performed using the Arcgis 10.1 software. The
   criteria for the available sites selection were established as: river
   slope of at least 3\%, and vertical drop of 3 meters or more. A common
   precipitation and average daily flow period is chosen in order to
   calibrate and validate the Soil Moisture Accounting (SMA)
   semi-distributed hydrological model. The SMA method is a suitable tool
   to estimate the mean streamflow in ungauged watersheds. In the Alto
   Amacuzac River Basin, 578 sites with a gross hydropower potential of
   49.2 MW were found. The methodology used is practical because it only
   needs basic knowledge of geographic information systems and uses
   HEC-HMS. The economical evaluation shows initial investments of
   \$3,672,636.00 Mexican pesos on average. With these costs, investments
   may be feasible as long as a long-term plan is considered. The
   streamflow Q(90) is a good alternative to design run-of-river small
   hydropower plants.}},
Publisher = {{INST MEXICANO TECHNOLOGIAAGUA}},
Address = {{APARTADO POSTAL 202, MORELOS 62550 CIVAC, MEXICO}},
