@article{ ISI:000426427000008,
Author = {Gonzalez-Viveros, Pohema and Caetano, Ernesto and Garcia-Garcia,
   Fernando},
Title = {{Characterization and Modeling of Fog in the Mexico Basin}},
Journal = {{AEROSOL AND AIR QUALITY RESEARCH}},
Year = {{2018}},
