@article{ ISI:000426593100016,
Author = {Resendiz-Hernandez, Juan M. and Falfan-Valencia, Ramces},
Title = {{Genetic polymorphisms and their involvement in the regulation of the
   inflammatory response in asthma and COPD}},
Journal = {{ADVANCES IN CLINICAL AND EXPERIMENTAL MEDICINE}},
