@article{ ISI:000417542100007,
Author = {Mathur, S. and Nicastro, F. and Gupta, A. and Krongold, Y. and
   McLaughlin, B. M. and Brickhouse, N. and Pradhan, A.},
Title = {{The O VI Mystery: Mismatch between X-Ray and UV Column Densities}},
Journal = {{ASTROPHYSICAL JOURNAL LETTERS}},
Year = {{2017}},
Volume = {{851}},
Number = {{1}},
Month = {{DEC 10}},
Abstract = {{The UV spectra of Galactic and extragalactic sightlines often show O VI
   absorption lines at a range of redshifts, and from a variety of sources
   from the Galactic circumgalactic medium to active galactic nuclei (AGN)
   outflows. Inner shell O VI absorption is also observed in X-ray spectra
   (at lambda = 22.03 angstrom), but the column density inferred from the
   X-ray line was consistently larger than that from the UV line. Here we
   present a solution to this discrepancy for the z = 0 systems. The O Pi K
   beta line S-4(0) (D-3)3p(4)P at 562.40 eV ( 22.04 angstrom) is blended
   with the O VI K alpha line in X-ray spectra. We estimate the strength of
   this O Pi line in two different ways, and show that in most cases the O
   Pi line accounts for the entire blended line. The small amount of O VI
   equivalent width present in some cases has column density entirely
   consistent with the UV value. This solution to the O VI discrepancy,
   however, does not apply to high column-density systems like AGN
   outflows. We discuss other possible causes to explain their UV/X-ray
   mismatch. The O VI and O IT lines will be resolved by gratings on board
   the proposed mission Arcus and the concept mission Lynx, and would allow
   the detection of weak O VI lines not just at z = 0, but also at higher
   redshift.}},
