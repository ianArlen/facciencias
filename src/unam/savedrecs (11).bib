@article{ ISI:000428629200003,
Author = {Hinojosa-Romero, David and Rodriguez, Isaias and Valladares, Alexander
   and Valladares, Renela M. and Valladares, Ariel A.},
Title = {{Possible superconductivity in Bismuth (111) bilayers. Their electronic
   and vibrational properties from first principles}},
Journal = {{MRS ADVANCES}},
Year = {{2018}},
Volume = {{3}},
Number = {{6-7}},
Pages = {{313-319}},
Abstract = {{Using a 72-atom supercell we report ab initio calculations of the
   electronic and vibrational densities of states for the bismuth (111)
   bilayers (bismuthene) with periodic boundary conditions and a vacuum of
   5 A, 10 A and 20 A. We find that the electronic density of states shows
   a metallic character at the Fermi level and that the vibrational density
   of states manifests the expected gap due to the layers. Our results
   indicate that a vacuum down to 5 A does not affect the electronic and
   vibrational structures noticeably. A comparison of present results with
   those obtained for the Wyckoff structure is displayed. Assuming that the
   Cooper pairing potential is similar for all phases and structures of
   bismuth, an estimate of the superconducting transition temperature gives
   2.61 K for the bismuth bilayers.}},
