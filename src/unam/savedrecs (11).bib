@article{ ISI:000423240800025,
Author = {Nanjareddy, Kalpana and Arthikala, Manoj-Kumar and Aguirre, Alma-Leticia
   and Gomez, Brenda-Mariana and Lara, Miguel},
Title = {{Plant Promoter Analysis: Identification and Characterization of Root
   Nodule Specific Promoter in the Common Bean}},
