@article{ ISI:000426756600001,
Author = {Torres-Torres, C. and Bornacelli, J. and Rangel-Rojo, R. and
   Garcia-Merino, J. A. and Can-Uc, B. and Tamayo-Rivera, L. and
   Cheang-Wong, J. C. and Rodriguez-Fernandez, L. and Oliver, A.},
Title = {{Photothermally Activated Two-Photon Absorption in Ion-Implanted Silicon
   Quantum Dots in Silica Plates}},
Journal = {{JOURNAL OF NANOMATERIALS}},
Year = {{2018}},
Abstract = {{The third-order nonlinear infrared and ultraviolet properties exhibited
   by silicon quantum dots irradiated by ultrashort pulses were studied.
   The samples were prepared by 1.5MeV Si+2 ion implantation processes in
   high-purity silica substrates. Femtosecond z-scan measurements conducted
   at 830 nm wavelength revealed strong self-focusing effects together with
   two-photon absorption that can be switched to saturable absorption as a
   function of the input irradiance. Changes in the main physical mechanism
   responsible for the picosecond absorptive nonlinearity in the sample
   were also observed at 355 nm, made possible by the assistance of
   photothermal phenomena. Ultraviolet self-diffraction explorations
   allowed us to estimate the Kerr effect of the nanostructures. Potential
   applications for developing all-optical filtering functions performed by
   silicon-based nanosystems can be considered.}},
Publisher = {{HINDAWI LTD}},
Address = {{ADAM HOUSE, 3RD FLR, 1 FITZROY SQ, LONDON, W1T 5HF, ENGLAND}},
Type = {{Article}},
