@article{ ISI:000422890600002,
Author = {Luongo, Orlando and Quevedo, Hernando},
Title = {{Self-accelerated Universe Induced by Repulsive Effects as an Alternative
   to Dark Energy and Modified Gravities}},
Journal = {{FOUNDATIONS OF PHYSICS}},
