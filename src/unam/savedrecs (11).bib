@article{ ISI:000423030100016,
Author = {Cordero-Schmidt, Eugenia and Barbier, Eder and Vargas-Mena, Juan C. and
   Oliveira, Paulino Pereira and Santos, Francisco de Assis R. and
   Medellin, Rodrigo A. and Rodriguez Herrera, Bernal and Venticinque,
   Eduardo M.},
Title = {{Natural history of the Caatinga endemic Vieira's flower bat,
   Xeronycteris vieirai}},
