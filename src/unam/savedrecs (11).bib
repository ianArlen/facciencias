@article{ ISI:000429757900013,
Author = {Zarmiento-Garcia, Rubi and Reyes-Serrato, Armando and Xiao, Mufei},
Title = {{First principles study of electronic structure and optical properties of
   beryllium nitride in beta phase (beta-Be3N2)}},
Journal = {{OPTIK}},
Year = {{2018}},
Volume = {{160}},
Pages = {{109-115}},
Abstract = {{Within the framework of density functional theory, electronic structure
   of beryllium nitride in beta phase (beta-Be3N2) is determined, and on
   the base of the electric structures, a number of key optical properties
   of beta-Be3N2 are calculated, with the method of full-potential
   linearized augmented plane wave under the random phase approximation for
   the boundaries and the generalized gradient approximation for the
   exchange-correlation potentials. The calculated optical properties as a
   function of frequency include the real and imaginary parts of the
   dielectric function epsilon(omega), the refraction index n(omega), the
   extinction coefficient kappa(omega), the reflectivity R(omega), the
   energy loss function L(omega), and the absorption coefficient I(omega)
   along the a and c directions of the primitive cell presenting the
   beta-Be3N2 crystals. It is demonstrated that the optical properties of
   beta-Be3N2 appear anisotropic for energies <35 eV, within the range of
   ultraviolet, and the p orbitals of nitrogen play the major role in
   relevant optical transitions. (C) 2018 Elsevier GmbH. All rights
   reserved.}},
