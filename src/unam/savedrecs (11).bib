@article{ ISI:000418211100006,
Author = {Lanery, Suzanne and Thiemann, Thomas},
Title = {{Projective limits of state spaces III. Toy-models}},
Journal = {{JOURNAL OF GEOMETRY AND PHYSICS}},
Year = {{2018}},
Volume = {{123}},
Pages = {{98-126}},
Month = {{JAN}},
Abstract = {{In this series of papers, we investigate the projective framework
   initiated by Kijowski (1977) and Okolow (2009, 2014, 2013) {[}1,2],
   which describes the states of a quantum theory as projective families of
   density matrices. A short reading guide to the series can be found in
   Lanery (2016).
   A strategy to implement the dynamics in this formalism was presented in
   our first paper Lanery and Thiemann (2017) (see also Lanery, 2016,
   section 4), which we now test in two simple toy-models. The first one is
   a very basic linear model, meant as an illustration of the general
   procedure, and we will only discuss it at the classical level. In the
   second one, we reformulate the Schrodinger equation, treated as a
   classical field theory, within this projective framework, and proceed to
   its (non-relativistic) second quantization. We are then able to
   reproduce the physical content of the usual Fock quantization. (c) 2017
