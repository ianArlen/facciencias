@article{ ISI:000431195400001,
Author = {Sanchez, Sergio and Cocho, Germinal and Flores, Jorge and Gershenson,
   Carlos and Iniguez, Gerardo and Pineda, Carlos},
Title = {{Trajectory Stability in the Traveling Salesman Problem}},
Journal = {{COMPLEXITY}},
Year = {{2018}},
Abstract = {{Two generalizations of the traveling salesman problem in which sites
   change their position in time are presented. The way the rank of
   different trajectory lengths changes in time is studied using the rank
   diversity. We analyze the statistical properties of rank distributions
   and rank dynamics and give evidence that the shortest and longest
   trajectories are more predictable and robust to change, that is, more
   stable.}},
