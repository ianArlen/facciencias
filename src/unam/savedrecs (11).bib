@article{ ISI:000423731200055,
Author = {Heyer, M. and Wilson, G. W. and Gutermuth, R. and Lizano, S. and
   Gomez-Ruiz, A. and Kurtz, S. and Luna, A. and Serrano Bernal, E. O. and
   Schloerb, F. P.},
Title = {{Early science with the Large Millimetre Telescope: fragmentation of
   molecular clumps in the Galaxy}},
