@article{ ISI:000418620200001,
Author = {Kumar Mohanty, Sudipta and Arthikala, Manoj-Kumar and Nanjareddy,
   Kalpana and Lara, Miguel},
Title = {{Plant-symbiont interactions: the functional role of expansins}},
Journal = {{SYMBIOSIS}},
