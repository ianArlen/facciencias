Title = {{Charged-particle multiplicity distributions over a wide pseudorapidity
   range in proton-proton collisions at root s=0.9, 7, and 8 TeV}},
Journal = {{EUROPEAN PHYSICAL JOURNAL C}},
Year = {{2017}},
Volume = {{77}},
Number = {{12}},
Month = {{DEC 9}},
Abstract = {{We present the charged-particle multiplicity distributions over a wide
   pseudorapidity range (-3.4<eta<5.0) for pp collisions at root s = 0.9,
   7, and 8 TeV at the LHC. Results are based on information from the
   Silicon Pixel Detector and the Forward Multiplicity Detector of ALICE,
   extending the pseudorapidity coverage of the earlier publications and
   the high-multiplicity reach. The measurements are compared to results
   from the CMS experiment and to PYTHIA, PHOJET and EPOS LHC event
   generators, as well as IP-Glasma calculations.}},
