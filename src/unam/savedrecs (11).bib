@article{ ISI:000424085800006,
Author = {Gonzalez-Moreno, D. and Hernandez Ortiz, R.},
Title = {{A note on the restricted arc connectivity of oriented graphs of girth
   four}},
Journal = {{AUSTRALASIAN JOURNAL OF COMBINATORICS}},
Year = {{2018}},
Volume = {{70}},
Number = {{3}},
Pages = {{340-348}},
Abstract = {{Let D be a strongly connected digraph. An arc set S of D is a restricted
   arc-cut of D if D - S has a non-trivial strong component D-1 such that D
   - V(D-1) contains an arc. The restricted arc-connectivity lambda'(D) of
   a digraph D is the minimum cardinality over all restricted arc-cuts of
   D. A strongly connected digraph D is lambda'-connected when lambda'(D)
   exists. This paper presents a family F of strong digraphs of girth four
   that are not lambda'-connected and for every strong digraph D is not an
   element of F with girth four it follows that it is lambda'-connected.
   Also, an upper and lower bound for lambda'(D) are given.}},
