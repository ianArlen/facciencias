@article{ ISI:000414322000012,
Author = {Fazende, Kylee F. and Phachansitthi, Manysa and Mota-Morales, Josue D.
   and Pojman, John A.},
Title = {{Frontal Polymerization of Deep Eutectic Solvents Composed of Acrylic and
   Methacrylic Acids}},
