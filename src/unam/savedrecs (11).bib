@article{ ISI:000419470700010,
Author = {Luis Marrugo-Negrete, Jose and Alonso Ruiz-Guzman, Javier and Carolina
   Ruiz-Fernandez, Ana},
Title = {{Biomagnification of Mercury in Fish from Two Gold Mining-Impacted
   Tropical Marshes in Northern Colombia}},
Journal = {{ARCHIVES OF ENVIRONMENTAL CONTAMINATION AND TOXICOLOGY}},
Year = {{2018}},
Volume = {{74}},
Number = {{1}},
Pages = {{121-130}},
Month = {{JAN}},
Abstract = {{Total mercury concentrations (T-Hg) and stable isotope ratios (delta
   N-15 and delta C-13) were analyzed in the muscle of fish, collected at
   two marshes impacted by gold mining in Northern Colombia, to evaluate
   the seasonal and spatial behavior of these variables and mercury
   biomagnification based on their relationship with delta N-15 ratios in
   the fish. There was not significant (p > 0.05) seasonal differences (dry
   and rainy season) in delta N-15 and delta C-13 values in none marsh,
   suggesting that these fish species keep a similar feed pattern within
   each marsh along the year. However, there were significant differences
   (p < 0.05) between marshes in delta N-15, delta C-13, and T-Hg, as well
   as significant seasonal differences (p < 0.05) in T-Hg values for some
   species, suggesting the possible influence of factors, such as
   differences in length of food chains between marshes and different
   degree of anthropogenic impact in relation to human settlements,
   farmland, livestock (higher in Ayapel marsh), and gold mining activity
   (higher in La Raya marsh). Correlations analysis showed significant (p <
   0.05) relationships between T-Hg concentrations and delta N-15 values,
   as well as trophic level, evidencing a T-Hg biomagnification process in
   the fish food chain from both marshes and consequently a potential
   health risk for human riverside inhabitants who eat these fish.}},
