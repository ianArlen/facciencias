@article{ ISI:000419156500004,
Author = {Fragoso-Martinez, Itzi and Martinez-Gordillo, Martha and Salazar,
   Gerardo A. and Sazatornil, Federico and Jenks, Aaron A. and Garcia Pena,
   Maria del Rosario and Barrera-Aveleida, Giovanna and Benitez-Vieyra,
   Santiago and Magallon, Susana and Cornejo-Tenorio, Guadalupe and
   Granados Mendoza, Carolina},
Title = {{Phylogeny of the Neotropical sages (Salvia subg. Calosphace; Lamiaceae)
   and insights into pollinator and area shifts}},
Journal = {{PLANT SYSTEMATICS AND EVOLUTION}},
