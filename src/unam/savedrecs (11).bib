@article{ ISI:000428656100005,
Author = {Illanes, Alejandro and Martinez-De-La-Vega, Veronica},
Title = {{CELLS IN n-FOLD HYPERSPACES}},
Journal = {{COLLOQUIUM MATHEMATICUM}},
Year = {{2018}},
Volume = {{152}},
Number = {{1}},
Pages = {{45-53}},
Abstract = {{Given a metric continuum X, let C-n(X) denote the hyperspace of non
   empty closed subsets of X with at most n components. A k-od in X is a
   subcontinuum B of X which contains a subcontinuum A such that
   B\textbackslash{}A has at least k components. We prove that if 1 <= n <=
   m, then C-n(X) contains an m-cell if and only if there exist positive
   integers k(1),...,k(n) and pairwise disjoint subcontinua B-1,...,B-n of
   X such that for each i, B-i is a k(i)-od in X and k(1) + ... + k(n) = m.}},
