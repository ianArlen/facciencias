@article{ ISI:000419360000025,
Author = {Montes-Leyva, Leobardo and Tellez-Valdes, Oswaldo and Bojorquez, Luis
   and Davila, Patricia and Lira, Rafael},
Title = {{Potential areas for conservation of useful flora of the
   Tehuacan-Cuicatlan Valley, Mexico}},
Journal = {{GENETIC RESOURCES AND CROP EVOLUTION}},
