@article{ ISI:000425255800012,
Author = {Estrada-Wiese, D. and del Rio, J. A.},
Title = {{Refractive index evaluation of porous silicon using bragg reflectors}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2018}},
Volume = {{64}},
