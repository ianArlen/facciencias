@article{ ISI:000418448000065,
Author = {Ulloa, Carmen Ulloa and Acevedo-Rodriguez, Pedro and Beck, Stephan and
   Belgrano, Manuel J. and Bernal, Rodrigo and Berry, Paul E. and Brako,
   Lois and Celis, Marcela and Davidse, Gerrit and Forzza, Rafaela C. and
   Gradstein, S. Robbert and Hokche, Omaira and Leon, Blanca and
   Leon-Yanez, Susana and Magill, Robert E. and Neill, David A. and Nee,
   Michael and Raven, Peter H. and Stimmel, Heather and Strong, Mark T. and
   Villasenor, Jose L. and Zarucchi, James L. and Zuloaga, Fernando O. and
   Jorgensen, Peter M.},
Title = {{An integrated assessment of the vascular plant species of the Americas}},
Journal = {{SCIENCE}},
Year = {{2017}},
Volume = {{358}},
Number = {{6370}},
Pages = {{1614+}},
Month = {{DEC 22}},
Abstract = {{The cataloging of the vascular plants of the Americas has a
   centuries-long history, but it is only in recent decades that an
   overview of the entire flora has become possible. We present an
   integrated assessment of all known native species of vascular plants in
   the Americas. Twelve regional and national checklists, prepared over the
   past 25 years and including two large ongoing flora projects, were
   merged into a single list. Our publicly searchable checklist includes
   124,993 species, 6227 genera, and 355 families, which correspond to 33\%
   of the 383,671 vascular plant species known worldwide. In the past 25
   years, the rate at which new species descriptions are added has averaged
   744 annually for the Americas, and we can expect the total to reach
