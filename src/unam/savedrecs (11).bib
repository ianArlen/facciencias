@article{ ISI:000423195800021,
Author = {Oseguera Galindo, David Omar and Oceguera Contreras, Eden},
Title = {{Copper Nanoparticles by Laser Ablation Confined in Methanol in the
   Presence of an Argon Gas Environment}},
Journal = {{JOURNAL OF LASER MICRO NANOENGINEERING}},
Year = {{2017}},
Volume = {{12}},
Number = {{3}},
Pages = {{304-306}},
Month = {{DEC}},
Abstract = {{The fundamental emission of Nd:YAG laser (1064 nm), with an energy 0.1
   J/pulse and a repetition rate of 10 Hz was employed to synthesize
   nanoparticles with the laser ablation method confined in methanol.
   Furthermore in the synthesis was used an experimental design that allow
   injection Argon gas to prevent combustion. The sample showed an
   absorption peak characteristic of copper nanoparticles. The optical
   properties of nanoparticles were analyzed by comparing the
   experimentally absorption with the extinction efficiency of the Mie
   Theory, including in the theoretical calculations a size distribution
   obtained through in the micrographs, resulting in both spectrum present
   an absorption peak approximate to 570 nm. Meanwhile, in these
   nanoparticles was used the Energy dispersive x-ray spectroscopy (EDS)
   showing high purity of the copper element.}},
