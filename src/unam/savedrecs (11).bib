@article{ ISI:000429060600008,
Author = {Serrano-Castaneda, Pablo and Juan Escobar-Chavez, Jose and Marlen
   Rodriguez-Cruz, Isabel and Maria Melgoza-Contreras, Luz and
   Martinez-Hernandez, Jessica},
Title = {{Microneedles as Enhancer of Drug Absorption Through the Skin and
   Applications in Medicine and Cosmetology}},
Journal = {{JOURNAL OF PHARMACY AND PHARMACEUTICAL SCIENCES}},
Year = {{2018}},
Volume = {{21}},
Pages = {{73-93}},
Abstract = {{The microneedles technology has found applications in many
   health-related fields. For example, their application in drugs and
   vaccines delivery as well, as the determination of biomarkers, has been
   reported. They also have a place in the dermatology and cosmetic areas
   such as the treatment of wounds from burns, scars, acne, depigmentation,
   and alopecia will be shown. Microneedles are used in therapeutic
   applications and are manufactured using materials such as metal (steel,
   titanium, nickel), polymer (oly-glycolic acid (PGA),
   poly-lactide-co-glycolide acid (PLGA), poly-L-lactic acid (PLA),
   chitosan), glass, silicon, ceramic, carbohydrates (trehalose, sucrose,
   mannitol). Examples of application of microneedles and their advantages
   and disadvantages are discussed.}},
