@article{ ISI:000249156400031,
Author = {Romero, David and Zertuche, Federico},
Title = {{Number of different binary functions generated by NK-Kauffman networks
   and the emergence of genetic robustness}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2007}},
Volume = {{48}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{We determine the average number Theta(N,K) of NK-Kauffman networks that
   give rise to the same binary function. We show that for N > 1, there
   exists a connectivity critical value K-c such that Theta(N,K)approximate
   to e(phi N) (phi>0) for K < K-c and Theta(N,K)approximate to 1 for
   K>K-c. We find that K-c is not a constant but scales very slowly with N
   as K-c approximate to log(2) log(2)(2N/ln 2). The problem of genetic
   robustness emerges as a statistical property of the ensemble of
   NK-Kauffman networks and impose tight constraints in the average number
   of epistatic interactions that the genotype-phenotype map can have.}},
