@article{ ISI:000247114000001,
Author = {Ackermann, Nils and Bartsch, Thomas and Kaplicky, Petr.},
Title = {{An invariant set generated by the domain topology for parabolic
   semiflows with small diffusion}},
Journal = {{DISCRETE AND CONTINUOUS DYNAMICAL SYSTEMS}},
Year = {{2007}},
Volume = {{18}},
Number = {{4}},
Pages = {{613-626}},
Month = {{AUG}},
Abstract = {{We consider the singularly perturbed semilinear parabolic problem u(t) -
   d(2)Delta u + with homogeneous Neumann boundary conditions on a smoothly
   bounded domain Omega subset of R-N. Here f is superlinear at 0, and (+)
   under bar infinity and has subcritical growth. For small d > 0 we
   construct a compact connected invariant set X-d in the boundary of the
   domain of attraction of the asymptotically stable equilibrium 0. The
   main features of X-d are that it consists of positive functions that are
   pairwise non-comparable, and its topology is at least as rich as the
   topology of partial derivative Omega in a certain sense. If the number
   of equilibria in X-d is finitte, then this implies the existence of
   connecting orbits within X-d that are not a consequence of a well known
   result by Matano.}},
