@article{ ISI:000248579300008,
Author = {Stephens, Christopher R. and Poli, Riccardo},
Title = {{Coarse-grained dynamics for generalized recombination}},
Journal = {{IEEE TRANSACTIONS ON EVOLUTIONARY COMPUTATION}},
