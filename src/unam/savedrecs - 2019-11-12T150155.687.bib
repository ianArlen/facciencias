@article{ ISI:000248284400009,
Author = {Espinosa, M. and Pacheco, S. and Rodriguez, R.},
Title = {{Synthesis and characterization of NH2-porphyrins covalently immobilized
   on modified-SBA-15}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{2007}},
Volume = {{353}},
Number = {{26}},
Pages = {{2573-2581}},
Month = {{AUG 1}},
Abstract = {{\textbackslash{}The immobilization of two different succinic-modified
   carboxy-imide-porphyrins on the surface of amino-modified SBA-15 is
   reported. The SBA-15 was synthesized and modified by reacting its
   hydroxyls groups with the OH and/or OR groups of 3-amino propyl
   triethoxy silane, anchoring the amino group on its surface. Separately,
   two different derivatives amino-porphyrins (NH2-5-m-Etio-III-Ni2+ and
   NH2-2 beta pyrrolic-m-5,10,15,20-TPP-Ni2+) were modified using succinic
   anhydride to provides these with the carboxyl group appropriate to react
   covalently with the amino groups previously anchored on the SBA-15;
   these new hybrid materials have the appropriated textural and chemical
   characteristics to be used in heterogeneous catalysis. The samples were
   characterized by XRD, N-2 adsorption isotherms, HR-TEM, TGA/DTA, FTIR,
   UV/VIS, C-13 and Si-29 NMR-CP/MAS. (C) 2007 Elsevier B.V. All rights
   reserved.}},
