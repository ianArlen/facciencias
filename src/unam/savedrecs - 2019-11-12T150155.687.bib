@article{ ISI:000247555200001,
Author = {Cereijido, Marcelino and Contreras, Ruben G. and Flores-Benitez, David
   and Flores-Maldonado, Catalina and Laffe, Isabel and Ruiz, Agustin and
   Shoshani, Liora},
Title = {{New diseases derived or associated with the tight junction}},
