@article{ ISI:000248784100031,
Author = {Cabrera-Trujillo, R. and Sabin, John R. and Deumens, Erik and Oehrn,
   Yngve},
Title = {{Theoretical investigation of energy deposition and electron capture
   cross-sections for helium ion impact on formaldehyde}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2007}},
Volume = {{261}},
Number = {{1-2}},
Pages = {{118-120}},
