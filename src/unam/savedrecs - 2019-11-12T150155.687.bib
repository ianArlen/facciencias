@article{ ISI:000245685900006,
Author = {Heywood, Vernon and Casas, Alejandro and Ford-Lloyd, Brian and Kell,
   Shelagh and Maxted, Nigel},
Title = {{Conservation and sustainable use of crop wild relatives}},
