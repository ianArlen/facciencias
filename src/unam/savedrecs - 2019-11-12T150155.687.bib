@article{ ISI:000247074600069,
Author = {Besprosvany, J.},
Title = {{Cosmology with dark energy decaying through its chemical-potential
   contribution}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2007}},
Volume = {{40}},
Number = {{25}},
Pages = {{7099-7104}},
Month = {{JUN 22}},
Note = {{IRGAC 2006 Meeting, Barcelona, SPAIN, 2006}},
Abstract = {{The consideration of dark energy's quanta, required also by
   thermodynamics, introduces its chemical potential into the cosmological
   equations. Isolating its main contribution, we obtain solutions with
   dark energy decaying to matter or radiation. When dominant, their energy
   densities tend asymptotically to a constant ratio, explaining today's
   dark-energy-dark-matter coincidence, and in agreement with supernova
   redshift data.}},
