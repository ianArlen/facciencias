@article{ ISI:000247641900058,
Author = {Georgopoulos, Apostolos P. and Merchant, Hugo and Naselaris, Thomas and
   Amirikian, Bagrat},
Title = {{Mapping of the preferred direction in the motor cortex}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
Year = {{2007}},
Volume = {{104}},
Number = {{26}},
Pages = {{11068-11072}},
Month = {{JUN 26}},
Abstract = {{Directional tuning is a basic functional property of cell activity in
   the motor cortex. Previous work has indicated that cells with similar
   preferred directions are organized in columns perpendicular to the
   cortical surface. Here we show that these columns are organized in an
   orderly fashion in the tangential dimension on the cortical surface.
   Based on a large number of microelectrode penetrations and systematic
   exploration of the proximal arm area of the motor cortex while monkeys
   made free reaching 3D movements, it was estimated that (i) directional
   minicolumns are approximate to 30 Am in width, (ii) minicolumns with
   similar preferred directions tend to occur in doublets or triplets, and
   (iii) such minicolumns tend to repeat every approximate to 240 mu m
   (estimated width of a column), with intermediate preferred directions
   represented in a gradient. These findings provide evidence for an
   orderly mapping of the preferred direction in the motor cortex.}},
