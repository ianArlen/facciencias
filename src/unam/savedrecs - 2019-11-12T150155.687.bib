@article{ ISI:000248784100133,
Author = {Andrade, E. and Solis, C. and Rocha, M. F.},
Title = {{Application of the SIMNRA code to calibrate the analyzing magnet
   associated with a Van de Graaff accelerator}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2007}},
Volume = {{261}},
Number = {{1-2}},
Pages = {{553-556}},
