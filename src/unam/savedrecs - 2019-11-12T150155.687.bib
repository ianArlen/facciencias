@article{ ISI:000247248800007,
Author = {Shu, Frank H. and Allen, Ronald J. and Lizano, Susana and Galli, Daniele},
Title = {{Formation of OB associations in galaxies}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2007}},
Volume = {{662}},
Number = {{2, 2}},
Pages = {{L75-L77}},
Month = {{JUN 20}},
Abstract = {{We consider the formation of OB associations from two perspectives: ( 1)
   the fractional gas consumption in star formation, epsilon, per dynamical
   timescale t(dyn) in a galaxy, and ( 2) the origin of the so-called
   Kennicutt-Schmidt law, that the rate of star formation per unit area is
   proportional to a power, alpha, of the surface density in H I and H(2)
   gas when certain thresholds are crossed. The empirical findings that
   epsilon approximate to 10(-2) and alpha approximate to 1.4 or 1.5 have
   simple explanations if the rate of star formation is magnetically
   regulated. An empirical test of the ideas resides in an analysis of why
   giant OB associations are ``strung out like pearls along the arms{''} of
