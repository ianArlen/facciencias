@article{ ISI:000248784100112,
Author = {Rebollo, N. R. and Ruvalcaba-Sil, J. L. and Miranda, J.},
Title = {{Ionoluminscence of partially-stabilized zirconia for thermal barrier
   coatings}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2007}},
Volume = {{261}},
Number = {{1-2}},
Pages = {{461-465}},
