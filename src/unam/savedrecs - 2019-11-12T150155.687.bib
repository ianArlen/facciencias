@article{ ISI:000248935000007,
Author = {Torres-Argueelles, Vianey and Ventura-Ramos, Eusebio Jr. and Oleschko,
   Klaudia and Parrot, Jean Francois and Vasquez-Mendez, Rebeca Del Carmen},
Title = {{Fractal descriptors of vertisol cracking}},
