@article{ ISI:000248435800020,
Author = {Natividad, C. and Salazar, M. and Espinosa-Medina, M. A. and Perez, R.},
Title = {{A comparative study of the SSC resistance of a novel welding process IEA
   with SAW and MIG}},
Journal = {{MATERIALS CHARACTERIZATION}},
Year = {{2007}},
Volume = {{58}},
Number = {{8-9, SI}},
Pages = {{786-793}},
Month = {{AUG-SEP}},
Note = {{Symposium on Materials Characterization held at the 14th International
   Materials Research Congress, Cancun, MEXICO, AUG 21-25, 2005}},
Organization = {{Acad Mexicana Ciencias Mat; Int Union Mat Res Soc; Natl Assoc Corros
   Engineers Int}},
Abstract = {{The Stress Sulphide Cracking resistance of X65 weldments produced by
   Indirect Electric Arc, Submerged Arc Welding (SAW) and Metal Inert Gas
   (MIG) processes were evaluated in a NACE solution saturated with H2S at
   25 degrees C, 37 degrees C and 50 degrees C using Slow Strain Rate Tests
   (SSRT) and electrochemical measurements. Weldments produced by the
   Indirect Electric Arc presented the best Stress Sulphide Cracking
   resistance at 25 degrees C. This behavior is attributed to the
   microstructural modification of the weld bead from ferrite in a
   needlelike form to a fine grain microstructure, which was not observed
   at 37 degrees C and 50 degrees C. In addition, the hydrogen permeation
   flux increased with the temperature, this result is associated with the
   ferrite phase. The electrochemical results show a decrease of the
   trapping sites for the atomic hydrogen on this weldment. This behavior
   has not been observed for the other welding processes due to their
   microstructure (a typical columnar growth of coarse grain). (C) 2006
   Elsevier Inc. All rights reserved.}},
