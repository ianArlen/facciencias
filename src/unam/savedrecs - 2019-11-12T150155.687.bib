@article{ ISI:000249912300009,
Author = {McCauley, Ross A. and Ballard, Jr., Harvey E.},
Title = {{Systematics of North American Froelichia (Amaranthaceae subfam.
   Gomphrenoideae) II: Phylogeny and biogeographic speciation patterns
   inferred from nrITS sequence data}},
Journal = {{BRITTONIA}},
Year = {{2007}},
Volume = {{59}},
Number = {{3}},
Pages = {{275-289}},
Month = {{JUL-SEP}},
Abstract = {{Molecular phylogenetic analyses of the nuclear ribosomal DNA internal
   transcribed spacer (ITS 1 and ITS 2) and the 5.8S gene were used to
   infer a phylogeny among the ten recognized taxa of Froelichia in North
   America. Analyses using both maximum parsimony (MP) and
   maximum-likelihood (ML) depicted a low level of sequence divergence
   though it was sufficient in most cases to differentiate taxa. Froetichia
   xantusii, a species restricted to southern Baja California was shown to
   be the basalmost member of the group subtending three clades. Two of the
   clades received good bootstrap support in the MP analysis and
   corresponded to a genetically homogeneous F interrupta, and a clade
   comprising the two species F latifolia and E texana. A third clade
   receiving low bootstrap support contained F floridana, F gracilis, F
   arizonica, and F drummondii. Species diversity within the genus was
   centered within the Tamaulipan Brushland region of north-east Mexico and
   the southern portion of the US state of Texas where taxa from two of the
   three principal clades occurred, indicating a region of high speciation
   and diversification within the genus.}},
