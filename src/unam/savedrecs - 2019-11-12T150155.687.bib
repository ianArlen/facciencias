@article{ ISI:000247755500015,
Author = {Martinez-Vazquez, Pedro and Rodriguez-Cuevas, Neftali},
Title = {{Wind field reproduction using neural networks and conditional simulation}},
Journal = {{ENGINEERING STRUCTURES}},
