@article{ ISI:000248935000001,
Author = {Cisneros-Iturbe, Hector Leonardo and Pelczer, Ildiko Judit},
Title = {{Artificial neural networks to forecast rainfall depths every 15 minutes}},
Journal = {{INGENIERIA HIDRAULICA EN MEXICO}},
Year = {{2007}},
Volume = {{22}},
Number = {{3}},
Pages = {{5-21}},
Month = {{JUL-SEP}},
Abstract = {{In this study, the Artificial Neural Network (ANN) approach is used for
   forecasting rainfall at 15-minute intervals in a raingauge station of an
   urban zone. To choose the best model, the forecast accuracy of several
   ANNs was compared. The ANNs varied in their set of input variables from
   rain depth measurements corresponding to three 15-minute intervals from
   the station of interest and some neighboring stations. The ANN
   performances were compared by using the Pearson correlation coefficient.
   The results obtained were satisfactory except on the highest measured
   values of rain depth. In order to improve the forecast on this set of
   values, the initial data was clustered in five groups. The k-Nearest
   Neighbors (k-NN) method was used for clustering, and new ANNs were
   designed for each group. To improve forecast accuracy the concept of
   expected tendency was introduced to predict the expected variation of
   rain depth as rising or decreasing. Methods to identify groups and
   expected tendency were defined. The ANNs that best performed on historic
   data were evaluated on 24 measured storms, considered as real-time
   events. Best results were obtained with the clustering approach using
   the expected tendency variable, but the forecast of high rainfall depths
   was still not satisfactory. Finally, the use of strategies to improve
   forecast accuracy is suggested, such as the use of shorter time
   intervals and alternative methods, among others.}},
