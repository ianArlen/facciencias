@article{ ISI:000248435800004,
Author = {Contreras, A. and Angeles-Chavez, C. and Flores, O. and Perez, R.},
Title = {{Structural, morphological and interfacial characterization of Al-Mg/TiC
   composites}},
Journal = {{MATERIALS CHARACTERIZATION}},
Year = {{2007}},
Volume = {{58}},
