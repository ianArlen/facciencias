@article{ ISI:000248573600017,
Author = {Alexanderson, Erick and Cruz, Patricio and Vargas, Angelica and Meave,
   Aloha and Ricalde, Alejandro and Talayero, Jose A. and Romero-Ibarra,
   Jose Luis and Goldson, Tove M. and Vera-Lastra, Olga L. and Medina,
   Gabriela and Jara, Luis and Amigo, Mary-Carmen},
Title = {{Endothelial dysfunction in patients with antiphospholipid syndrome
   assessed with positron emission tomography}},
Journal = {{JOURNAL OF NUCLEAR CARDIOLOGY}},
Year = {{2007}},
Volume = {{14}},
Number = {{4}},
Pages = {{566-572}},
Month = {{JUL-AUG}},
Abstract = {{Background. There is limited knowledge about endothelial dysfunction in
   patients with primary antiphospholipid syndrome (PAPS). The purpose of
   this study was to evaluate endothelial function in patients with PAPS
   assessed by positron emission tomography.
   Methods and Results. A 3-phase protocol-rest, cold pressor test (CPT),
   and adenosine positron emission tomography with nitrogen 13 ammonia-was
   used in 18 patients with PAPS and 18 healthy volunteers (HVs).
   Myocardial blood flow (MBF) was measured in each phase, with calculation
   of the endothellial-dependent vasodilation index, the increase in the
   MBF in response to CPT, and the myocardial flow reserve. An important
   trend was found in the myocardial flow reserve (2.76 +/- 1.04 in PAPS
   group vs 3.27 +/- 0.72 in HV group, P > .05), in the
   endothelial-dependent vasodilation index (1.19 +/- 0.31 in PAPS group vs
   1.55 +/- 0.37 in HV group, P < .05), and in the percent change in the
   MBF in response to CPT (from rest) (19\% +/- 31\% in PAPS group vs 55\%
   +/- 37\% in HV group, P < .05).
   Conclusion. The CPT results obtained in. this study showed that the PAPS
   patients studied have endothelial dysfunction.}},
