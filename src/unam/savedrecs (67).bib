@article{ ISI:000311400600001,
Author = {Banks, William D. and Friedlander, John B. and Garaev, Moubariz Z. and
   Shparlinski, Igor E.},
Title = {{EXPONENTIAL AND CHARACTER SUMS WITH MERSENNE NUMBERS}},
Journal = {{JOURNAL OF THE AUSTRALIAN MATHEMATICAL SOCIETY}},
Year = {{2012}},
Volume = {{92}},
Number = {{1}},
Pages = {{1-13}},
Month = {{FEB}},
Abstract = {{We give new bounds on sums of the form Sigma(n <= N) Lambda(n) exp(2 pi
   iag(n)/m) and Sigma(n <= N) Lambda(n)chi(g(n) + a), where Lambda is the
   von Mangoldt function, m is a natural number, a and g are integers
   coprime to m, and chi is a multiplicative character modulo m. In
   particular, our results yield bounds on the sums Sigma(p <= N) exp(2 pi
   iaM(p)/m) and Sigma(p <= N) chi(M-p) with Mersenne numbers M-p = 2(p) -
   1, where p is prime.}},
