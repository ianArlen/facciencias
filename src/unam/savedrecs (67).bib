@article{ ISI:000302199700009,
Author = {Sheinbaum-Pardo, Claudia and Mora-Perez, Sergio and Robles-Morales,
   Guillermo},
Title = {{Decomposition of energy consumption and CO2 emissions in Mexican
   manufacturing industries: Trends between 1990 and 2008}},
Journal = {{ENERGY FOR SUSTAINABLE DEVELOPMENT}},
