@article{ ISI:000300211800016,
Author = {Avalos-Rendon, Tatiana and Lara, Victor H. and Pfeiffer, Heriberto},
Title = {{CO2 Chemisorption and Cyclability Analyses of Lithium Aluminate
   Polymorphs (alpha- and beta-Li5AlO4)}},
