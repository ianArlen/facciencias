@article{ ISI:000301778300011,
Author = {Dominguez-Melendez, V. and Silvestre-Santana, O. and Moreno-Fierros, L.
   and Aguiniga-Sanchez, I. and Martinez, Ledesma and Marroquin-Segura, R.
   and Garcia-Hernandez, A. L. and Weiss-Steider, B. and Marche-Cova, A.
   and Monroy-Garcia, A. and Mora-Garcia, L. and Santiago-Osorio, Edelmiro},
Title = {{Sodium caseinate induces mouse granulopoiesis}},
Journal = {{INFLAMMATION RESEARCH}},
Year = {{2012}},
Volume = {{61}},
Number = {{4}},
Pages = {{367-373}},
Month = {{APR}},
Abstract = {{Sodium caseinate (CasNa) induces differentiation and M-CSF production in
   mouse band granulocytes in vitro; however, it is not yet known if this
   molecule can also induce the proliferation and activation of the
   granulocyte lineage in vivo. In this work we evaluated the induction in
   vivo of granulopoiesis and the activation of granulocytes in mice
   treated with CasNa.
   BALB/c male mice 8-12 weeks old were used.
   The animals were inoculated intraperitoneally with 1 ml of CasNa (10\%
   in PBS p/v) four times (every 48 h).
   Granulocyte proliferation was evaluated by flow cytometry; activation
   was evaluated by phagocytic indices. The cytokine was measured using an
   ELISA assay.
   We show that CasNa increased bone marrow granulopoiesis percentage
   (38.35 +/- A 10.88 vs. 64.94 +/- A 34.14 BrdU+/Gr-1+ cells) and the
   granulocytes generated presented increased phagocytic indices (0.3 +/- A
   0.1 vs. 0.6 +/- A 0.11, p < 0.05). We also show that G-CSF (974 +/- A
   411 vs. 3189 +/- A 350 pg/ml, p < 0.05) and GM-CSF increased in serum,
   but only G-CSF in bone marrow plasma.
   CasNa induces granulopoiesis with functional granulocytes, suggesting
   that this molecule could be an innate immune system activator.}},
