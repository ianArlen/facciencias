@article{ ISI:000302540000002,
Author = {Bietenholz, Wolfgang and Hip, Ivan and Shcheredin, Stanislav and
   Volkholz, Jan},
Title = {{A numerical study of the 2-flavour Schwinger model with dynamical
   overlap hypercube fermions}},
