Title = {{Metal-Oxide-Semiconductor Structures Containing Silicon Nanocrystals for
   Application in Radiation Dosimeters}},
Journal = {{SENSOR LETTERS}},
Year = {{2012}},
Volume = {{10}},
Number = {{3-4}},
