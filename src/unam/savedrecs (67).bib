@article{ ISI:000303345000001,
Author = {Terborg, Heinrich and Perez, Luis A.},
Title = {{MINIMAL TRAFFIC MODEL WITH SAFE DRIVING CONDITIONS}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2012}},
Volume = {{23}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{We have developed a new computational traffic model in which security
   aspects are fundamental. In this paper we show that this model
   reproduces many known empirical aspects of vehicular traffic such as the
   three states of traffic flow and the backward speed of the downstream
   front of a traffic jam ( C), without the aid of adjustable parameters.
   The model is studied for both open and closed single lane traffic
   systems. Also, we were able to analytically compute the value of C as
   15: 37 km/h from a relation that only includes the human reaction time,
   the mean vehicle length and the effective friction coefficient during
   the braking process of a vehicle as its main components.}},
