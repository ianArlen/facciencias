@article{ ISI:000300805200007,
Author = {Ramirez, Mirella and Sabina, Federico J.},
Title = {{Out-of-plane modulus of semi-auxetic laminates (vol 28, pg 752, 2009)}},
Journal = {{EUROPEAN JOURNAL OF MECHANICS A-SOLIDS}},
Year = {{2012}},
Volume = {{32}},
Pages = {{59-61}},
Month = {{MAR-APR}},
Abstract = {{Lately Lim gave a formula for the out-of-plane Young modulus of a
   laminate consisting of a semi-auxetic cell that is repeated
   periodically. This cell is composed of two isotropic materials, one
   auxetic and one conventional. He gave plots of numerical examples, but
   these are incorrect. The plots do not correspond to the properties of
   the given materials. The purpose of this paper is to give the correct
   plots of his numerical examples. The enhancement of the effective
   out-of-plane Young modulus with respect to the stiffest phase for some
   semi-auxetic laminates is much bigger and spread over a wider interval
   of volume fraction, bigger and wider than the ones showed by him. In all
   cases the rules-of-mixture values are exceeded unlike in Lim's. (C) 2011
   Elsevier Masson SAS. All rights reserved.}},
