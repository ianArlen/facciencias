@article{ ISI:000302412400007,
Author = {Chakrabortty, Shankhadeep and Dey, Tanay K.},
Title = {{Correlators of giant gravitons from dual ABJ(M) theory}},
Journal = {{JOURNAL OF HIGH ENERGY PHYSICS}},
Year = {{2012}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{d We generalize the operators of ABJM theory, given by Schur
   polynomials, in ABJ theory by computing the two point functions in the
   free field and at finite (N-1,N-2) limits. These polynomials are then
   identified with the states of the dual gravity theory. Further, we
   compute correlators among giant gravitons as well as between giant
   gravitons and ordinary gravitons through the corresponding correlators
   of ABJ(M) theory. Finally, we consider a particular non-trivial back
   ground produced by an operator with an R-charge of O(N-2) and find, in
   presence of this background, due to the contribution of the non-planar
   corrections, the large (N-1,N-2) expansion is replaced by 1/(N-1+M) and
   1/(N-2+M) respective.}},
