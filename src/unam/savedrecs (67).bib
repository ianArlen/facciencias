@article{ ISI:000301689300002,
Author = {Cornejo-Garrido, Hilda and Nieto-Camacho, Antonio and Gomez-Vidales,
   Virginia and Teresa Ramirez-Apan, Maria and del Angel, Paz and Ascencion
   Montoya, Jose and Dominguez-Lopez, Mariana and Kibanova, Dania and
   Cervini-Silva, Javiera},
Title = {{The anti-inflammatory properties of halloysite}},
Journal = {{APPLIED CLAY SCIENCE}},
Year = {{2012}},
Volume = {{57}},
Pages = {{10-16}},
Month = {{MAR}},
Abstract = {{Halloysite can serve as an alternative agent for drug delivery because
   its production is not tedious, hazardous, or expensive. In this study we
   show that halloysite presented anti-inflammatory properties comparable
   to indomethacin as evidenced by determination of the mieloperoxidase
   (MPO) enzymatic activity, a specific marker for migration and cellular
   infiltration. Edema reached maximum levels after 4 h. Cellular migration
   was noted to be low. After 24 h, however, cellular migration was noted
   to increase. Experiments conducted with mice primary peritoneal
   macrophages showed that halloysite (vs aminoguanidine) inhibited the
   production of nitric oxide. Halloysite inhibited oxidative stress via
   lipid peroxidation (ca. IC50 = 2023 ppm). (C) 2011 Elsevier B.V. All
   rights reserved.}},
