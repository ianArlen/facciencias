@article{ ISI:000302293600003,
Author = {Sen, Mihir and Ramos, Eduardo},
Title = {{A Spatially Non-Local Model for Flow in Porous Media}},
Journal = {{TRANSPORT IN POROUS MEDIA}},
Year = {{2012}},
Volume = {{92}},
Number = {{1}},
Pages = {{29-39}},
Month = {{MAR}},
Abstract = {{A general mathematical model of steady-state transport driven by
   spatially non-local driving potential differences is proposed. The
   porous medium is considered to be a network of short-, medium-, and
   long-range interstitial channels with impermeable walls and at a
   continuum of length scales, and the flow rate in each channel is assumed
   to be linear with respect to the pressure difference between its ends.
   The flow rate in the model is thus a functional of the non-local driving
   pressure differences. As special cases, the model reduces to familiar
   forms of transport equations that are commonly used. An important
   situation arises when the phenomenon is almost, but not quite, locally
   dependent. The one-dimensional form of the model discussed here can be
   extended to multiple dimensions, temporal non-locality, and to heat,
   mass, and momentum transfer.}},
