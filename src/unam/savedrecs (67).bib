@article{ ISI:000301564200005,
Author = {Osorio, Roman and Garcia, Sinuhe and Pena, Mario and Lopez-Juarez,
   Ismael and Lefranc, Gaston},
Title = {{Movement and Color Detection of a Dynamic Object An Application to a
   Mobile Robot}},
Journal = {{STUDIES IN INFORMATICS AND CONTROL}},
Year = {{2012}},
Volume = {{21}},
Number = {{1, SI}},
Pages = {{33-40}},
Month = {{MAR}},
Abstract = {{The paper describes the integration of several image processing
   algorithms necessary to recognize a particular color and the movement of
   an object. The main objective is to detect the object by its color and
   track it by a mobile robot. Mean filter is applied to soften and sharpen
   the input image. Then, RGB filter is applied to calculate the center of
   mass and area of the object and to locate its position in a real
   environment to develop the robot motion. These algorithms are applied to
   a mobile robot, in a tested scenario, tracking an object.}},
