@article{ ISI:000302851100013,
Author = {Gonzalez, Agustin E. and Ixtlilco-Cortes, Leonardo},
Title = {{Fractal structure of the crystalline-nuclei boundaries in 2D colloidal
   crystallization: Computer simulations}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2012}},
Volume = {{376}},
Number = {{16}},
Pages = {{1375-1379}},
Month = {{MAR 19}},
Abstract = {{By performing 2D kinetic Monte Carlo simulations of colloidal
   crystallization we found that the boundaries of the crystalline nuclei
   are not only rough, as obtained by experimentalists, but fractal, whose
   value (d(f)) we calculated. The corresponding boundary for the crystals,
   above the critical size (N-c), is also fractal but smoother. A knowledge
   of the particles coordinates during the crystallization process allows
   us to calculate the N-c, the line tension (gamma) and the chemical
   potential difference (Delta mu) between the two phases. However,
   different from the experimentalists procedure, we found that the
   boundary fractalities are needed to derive gamma and Delta mu. (C) 2012
   Elsevier B.V. All rights reserved.}},
