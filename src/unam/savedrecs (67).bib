@article{ ISI:000301292000007,
Author = {Caspar, Gunther and Schoenenbach, Thomas and Otto Hess, Peter and
   Schaefer, Mirko and Greiner, Walter},
Title = {{PSEUDO-COMPLEX GENERAL RELATIVITY: SCHWAB ZSCHILD, REISSNER-NORDSTROM
   AND KERR SOLUTIONS}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS E}},
Year = {{2012}},
Volume = {{21}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{The pseudo-complex General Relativity (pc-OR) is further considered. A
   new projection method is proposed. It is shown that the pc-GR introduces
   automatically terms into the system which can be interpreted as dark
   energy. The modified pseudo-complex Schwarzschild solution is
   investigated. The dark energy part is treated as a liquid and possible
   solutions are discussed. As a consequence, the collapse of a large
   stellar mass into a singularity at r = 0 is avoided and no event-horizon
   is formed. Thus, black holes do not exist. The resulting object can be
   viewed as a gray star. It contains no singularity which emphasizes,
   again, that it is not a Hack hole. The corrections implied by a charged
   large mass object (Reissner-Nordstrom) and a rotating gray star (Kerr)
   are presented. For the latter, a special solution is presented. Finally,
   we will consider the orbital speed of a mass in a circular orbit and
   suggest a possible experimental verification.}},
