@article{ ISI:000297231600038,
Author = {Valencia, Dora and Alday, Efrain and Robles-Zepeda, Ramon and
   Garibay-Escobar, Adriana and Galvez-Ruiz, Juan C. and Salas-Reyes,
   Magali and Jimenez-Estrada, Manuel and Velazquez-Contreras, Enrique and
   Hernandez, Javier and Velazquez, Carlos},
Title = {{Seasonal effect on chemical composition and biological activities of
   Sonoran propolis}},
Journal = {{FOOD CHEMISTRY}},
Year = {{2012}},
Volume = {{131}},
Number = {{2}},
Pages = {{645-651}},
Month = {{MAR 15}},
Abstract = {{Propolis is widely used as a folk medicine and as a constituent of
   health foods in many parts of the world. The main purpose of this study
   was to evaluate the seasonal effect on the chemical composition and
   biological activities (antiproliferative and antioxidant activities) of
   Sonoran propolis (Mexico). Propolis was collected during all four
   seasons of the year and chemical composition, antiproliferative, and
   free-radical scavenging activities of collected propolis were evaluated
   by HPLC, MTT, and DPPH assays, respectively. The relative abundance of
   the main chemical constituents of propolis was similar in all propolis
   samples analysed. In contrast, significant differences were observed in
   their antiproliferative activity in the B-cell lymphoma cancer cell line
   M12.C3.F6. Propolis collected in the spring showed the highest
   inhibitory effect on the growth of cancer cells. All propolis samples
   had weak free-radical scavenging activity. Our results indicate that
   season had a significant effect on the antiproliferative properties of
   Sonoran propolis. (C) 2011 Elsevier Ltd. All rights reserved.}},
