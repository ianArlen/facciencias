@article{ ISI:000301811500002,
Author = {Martinez-Ramirez, J. D. and Quispe-Siccha, R. and Garcia-Segundo, C. and
   Gonzalez, F. J. and Espinosa-Luna, R. and Gutierrez-Juarez, G.},
Title = {{Photoacoustic Tomography System}},
