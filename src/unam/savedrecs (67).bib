@article{ ISI:000301159900081,
Author = {Martinez, H. and Avellaneda, D.},
Title = {{Modifications in SnS thin films by plasma treatments}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2012}},
Volume = {{272}},
Pages = {{351-356}},
Month = {{FEB 1}},
Note = {{17th International Conference on Ion Beam Modification of Materials
   (IBMM), Montreal, CANADA, AUG 22-27, 2010}},
Abstract = {{The present study shows modifications of structural, optical and
   electrical characteristics that occur in tin sulfide (SnS) thin films
   treated in air and nitrogen plasma at different pressure conditions.
   Films were obtained by the chemical bath deposition method, which
   results in SnS thin films with an orthorhombic crystalline structure,
   band gap (E-g) of 1.1-1.2 eV, and electrical conductivities (sigma) in
   the order of 10(-6) Omega(-1)cm(-1). The films treated with air plasma
   at pressures between 1 and 4 Torr, showed the presence of SnS2, Sn2S3,
   and SnO2 phases, within band gap values ranging from 0.9 to 1.5 eV. On
   the other hand, films treated with nitrogen plasma presented the same
   phases, but showed a significant modification in the electrical
   conductivity, it increasing from 10(-6) Omega(-1)cm(-1) (as-deposited)
   up to 10(-2)-10(-3) Omega(-1)cm(-1) (plasma treated). This result is a
   suitable range of conductivity for the improvement of the solar cells
   with SnS as an absorber material. Also, emission spectroscopy
   measurements were carried out in both air and nitrogen plasma
   treatments. (C) 2011 Elsevier B.V. All rights reserved.}},
