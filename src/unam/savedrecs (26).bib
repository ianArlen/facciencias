@article{ ISI:000383388800033,
Author = {Perez Carrascal, Olga M. and VanInsberghe, David and Juarez, Soledad and
   Polz, Martin F. and Vinuesa, Pablo and Gonzalez, Victor},
Title = {{Population genomics of the symbiotic plasmids of sympatric
   nitrogen-fixing Rhizobium species associated with Phaseolus vulgaris}},
Journal = {{ENVIRONMENTAL MICROBIOLOGY}},
Year = {{2016}},
Volume = {{18}},
Number = {{8}},
