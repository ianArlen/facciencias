@article{ ISI:000378360700046,
Author = {Montero-Martinez, Guillermo and Federico Torres-Perez, Eduardo and
   Garcia-Garcia, Fernando},
Title = {{A comparison of two optical precipitation sensors with different
   operating principles: The PWS100 and the OAP-2DP}},
