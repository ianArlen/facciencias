@article{ ISI:000381841200005,
Author = {Antonio Aliaga, Juan and Alonso-Nunez, Gabriel and Zepeda, Trino and
   Francisco Araya, Juan and Felipe Rubio, Pedro and Bedolla-Valdez, Zaira
   and Paraguay-Delgado, Francisco and Farias, Mario and Fuentes, Sergio
   and Gonzalez, Guillermo},
Title = {{Synthesis of highly destacked ReS2 layers embedded in amorphous carbon
   from a metal-organic precursor}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{2016}},
Volume = {{447}},
Pages = {{29-34}},
Month = {{SEP 1}},
Abstract = {{In the current study, a new approach to the synthesis of highly
   destacked ReS2 layers embedded in amorphous carbon via the thermal
   decomposition of a tetraoctylammonium perrhenate precursor, under
   sulfidizing atmosphere (15\% v/v H2S mixture H2S/H-2 gas), is described.
   X-ray diffraction, scanning electron microscopy, scanning transmission
   electron microscopy, energy-dispersive X-ray spectroscopy and X-ray
   photoelectron spectroscopy analysis of the thermolysis product confirm
   the formation of rhenium disulfide. The synthesized compound is found as
   single layers with a minor proportion of few-layer arrangements,
   embedded in amorphous carbon. Xray diffraction, UV-visible diffuse
   reflectance, and thermogravimetry analysis were made in order to
   characterize the metal-organic salt precursor, showing that the
   perrhenate ions are dispersed widely from each other in the matrix of
   the organic cations, forming an inorganic-organic salt. The special
   arrangement of these ReS2 layers has a potential use as a heterogeneous
   catalyst due to the high proportion of edge sites. (C) 2016 Elsevier
   B.V. All rights reserved.}},
