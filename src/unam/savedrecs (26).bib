@article{ ISI:000384807700020,
Author = {Cabrera, Rosina and Rodriguez-Romero, Adela and Guarneros, Gabriel and
   de la Torre, Mayra},
Title = {{New insights into the interaction between the quorum-sensing receptor
   NprR and its DNA target, or the response regulator Spo0F}},
Journal = {{FEBS LETTERS}},
Year = {{2016}},
Volume = {{590}},
Number = {{18}},
Pages = {{3243-3253}},
Month = {{SEP}},
Abstract = {{The NprR protein and NprRB signaling peptide comprise a bifunctional
   quorum-sensing system from the Bacillus cereus group that is involved in
   transcriptional activation through DNA-binding and in sporulation
   initiation by binding to Spo0F. We characterized in vitro the direct
   interactions established by NprR that may be relevant for performing its
   two functions. Apo-NprR interacted with Spo0F, but not with the target
   DNA. The NprRB signaling peptide SSKPDIVG that binds strongly to
   Apo-NprR, failed to bind and disrupt the NprR-Spo0F complex. Finally,
   the NprR-NprRB complex bound both to Spo0F and the target DNA with
   similar affinity. Based on our findings, we propose that rather than a
   switch triggered by NprRB, the NprR/NprRB ratio and the availability of
   Spo0F binding sites define the function of NprR.}},
