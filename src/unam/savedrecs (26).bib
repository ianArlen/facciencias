@article{ ISI:000391230600004,
Author = {Filomeno, Aldo},
Title = {{Abstract Entities in a Presentist World}},
Journal = {{METAPHYSICA-INTERNATIONAL JOURNAL FOR ONTOLOGY \& METAPHYSICS}},
Year = {{2016}},
Volume = {{17}},
Number = {{2}},
Pages = {{177-193}},
Month = {{SEP}},
Abstract = {{How can a metaphysics of abstract entities be built upon a metaphysics
   of time? In this paper, I address the question of how to accommodate
   abstract entities in a presentist world. I consider both the traditional
   metaontological approach of unrestricted fundamental quantification and
   then ontological pluralism. I argue that under the former we need to
   impose two constraints in the characterization of presentism in order to
   avoid undesired commitments to abstract entities: we have to
   characterize presentism as a thesis only about the concrete, and we also
   need to avoid the widely held distinction between tensed and tenseless
   senses of existence. Under ontological pluralism, instead, I argue that
   we can naturally accommodate any view of abstract objects in a
   presentist world.}},
