@article{ ISI:000382748100028,
Author = {Canales, M. and Ramirez-de-Arellano, J. M. and Magana, L. F.},
Title = {{Interaction of a Ti-doped semi-fullerene (TiC30) with molecules of CO
   and CO2}},
Journal = {{JOURNAL OF MOLECULAR MODELING}},
Year = {{2016}},
Volume = {{22}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{Using density functional theory (DFT) and molecular dynamics (MD), we
   studied the interaction of a titanium atom with a half of a C-60
   fullerene (i.e., C-30), formed from the corannulene structure with a
   pentagonal base. We considered atmospheric pressure and 300 K. We found
   that the most stable adsorption of the titanium atom on C-30 occurs in
   the concave surface of the molecule. Afterward, we investigated the
   interaction of the system C-30-titanium with carbon monoxide and carbon
   dioxide molecules, respectively. We found that each of these molecules
   is chemisorbed, with no dissociation. The value of the adsorption energy
   for the carbon monoxide molecule varies from -0.897 to -1.673 eV, and
   for the carbon dioxide molecule, it is between -1.065 and -1.274 eV.
   These values depend on the initial orientation of these molecules with
   respect to TiC30.}},
