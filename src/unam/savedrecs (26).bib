@article{ ISI:000408602100004,
Author = {Yam Chale, Hugo Alberto and Trujano Ruiz, Patricia},
Title = {{Captivity conditions and constructed meanings of economic kidnapping
   based on survivors' narrative accounts}},
Journal = {{REVISTA CRIMINALIDAD}},
Year = {{2016}},
Volume = {{58}},
Number = {{3}},
Pages = {{37-47}},
Month = {{SEP-DEC}},
Abstract = {{The following were the objectives pursued in this study: 1. Analyzing
   from the victims' narrations their captivity conditions and the
   treatment received from their captors, and 2. Identifying, from the
   social constructionism point of view, the meaning of abductions as built
   by the own kidnapped themselves. Case study and the analysis of
   narrative reports was the method used in this survey. The sample was
   integrated by eight victims (two women and six men, seven adults and one
   minor) having survived economic abductions, after being kept for three
   to five days on average. Some victims remained captive in unhealthy
   places and others at locations adapted with basic services. The violence
   exhibited by the kidnappers was both instrumental and senseless, with no
   other purpose than inflicting humiliation on their captives. The
   kidnapping was variedly and distinctly described as ``a terrible deed
   you would never wish anyone to experience; a consequence of sin; a
   strength test imposed by God; a degrading and inhuman act; an awful
   action aimed at obtaining money{''}, among other definitions.}},
