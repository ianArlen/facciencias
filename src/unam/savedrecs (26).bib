@article{ ISI:000388331000027,
Author = {Estrada-Navarrete, Georgina and Cruz-Mireles, Neftaly and Lascano,
   Ramiro and Alvarado-Affantranger, Xochitl and Hernandez-Barrera,
   Alejandra and Barraza, Aaron and Olivares, Juan E. and Arthikala,
   Manoj-Kumar and Cardenas, Luis and Quinto, Carmen and Sanchez, Federico},
Title = {{An Autophagy-Related Kinase Is Essential for the Symbiotic Relationship
   between Phaseolus vulgaris and Both Rhizobia and Arbuscular Mycorrhizal
   Fungi}},
Journal = {{PLANT CELL}},
Year = {{2016}},
