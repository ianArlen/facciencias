@article{ ISI:000382146100039,
Author = {Crespo, N. Alvarez and Massaro, F. and D'Abrusco, R. and Landoni, M. and
   Masetti, N. and Chavushyan, V. and Jimenez-Bailon, E. and La Franca, F.
   and Milisavljevic, D. and Paggi, A. and Patino-Alvarez, V. and Ricci, F.
   and Smith, Howard A.},
Title = {{Optical archival spectra of blazar candidates of uncertain type in the
   3rd Fermi Large Area Telescope Catalog}},
Journal = {{ASTROPHYSICS AND SPACE SCIENCE}},
Year = {{2016}},
Volume = {{361}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{Despite the fact that blazars constitute the rarest class among active
   galactic nuclei (AGNs) they are the largest known population of
   associated gamma-ray sources. Many of the gamma-ray objects listed in
   the Fermi-Large Area Telescope Third Source catalog (3FGL) are
   classified as blazar candidates of uncertain type (BCUs), either because
   they show multifrequency behavior similar to blazars but lacking optical
   spectra in the literature, or because the quality of such spectra is too
   low to confirm their nature. Here we select, out of 585 BCUs in the
   3FGL, 42 BCUs which we identify as probable blazars by their WISE
   infrared colors and which also have optical spectra that are available
   in the Sloan Digital Sky Survey (SDSS) and/or Six-Degree Field Galaxy
   Survey Database (6dFGS). We confirm the blazar nature of all of the
   sources. We furthermore conclude that 28 of them are BL Lacs, 8 are
   radio-loud quasars with flat radio spectrum and 6 are BL Lac whose
   emission is dominated by their host galaxy.}},
