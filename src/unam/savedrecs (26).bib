@article{ ISI:000381711100061,
Author = {Gil-Marin, Hector and Percival, Will J. and Cuesta, Antonio J. and
   Brownstein, Joel R. and Chuang, Chia-Hsun and Ho, Shirley and Kitaura,
   Francisco-Shu and Maraston, Claudia and Prada, Francisco and
