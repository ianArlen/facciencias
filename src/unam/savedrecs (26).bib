@article{ ISI:000382006600001,
Author = {Ohadi, H. and Gregory, R. L. and Freegarde, T. and Rubo, Y. G. and
   Kavokin, A. V. and Berloff, N. G. and Lagoudakis, P. G.},
Title = {{Nontrivial Phase Coupling in Polariton Multiplets}},
