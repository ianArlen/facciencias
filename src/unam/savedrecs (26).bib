@article{ ISI:000382501900004,
Author = {Rodriguez-Flores, C. I. and Arizmendi Arriaga, M. C.},
Title = {{The dynamics of hummingbird dominance and foraging strategies during the
   winter season in a highland community in Western Mexico}},
