Title = {{Semi-permeable species boundaries in the coral genus Madracis:
   Introgression in a brooding coral system}},
Journal = {{MOLECULAR PHYLOGENETICS AND EVOLUTION}},
Year = {{2010}},
Volume = {{57}},
Number = {{3}},
Pages = {{1072-1090}},
