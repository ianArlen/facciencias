@article{ ISI:000284090100038,
Author = {Lopez-Camara, D. and Raga, A. C.},
Title = {{SIDE ENTRAINMENT IN A JET EMBEDDED IN A SIDEWIND}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2010}},
Volume = {{723}},
Number = {{1}},
Pages = {{449-456}},
Month = {{NOV 1}},
Abstract = {{Numerical simulations of HH jets never show side entrainment of
   environmental material into the jet beam. This is because the bow shock
   associated with the jet head pushes the surrounding environment into a
   dense shell, which is never in direct contact with the sides of the jet
   beam. We present three-dimensional simulations in which a side-streaming
   motion (representing the motion of the outflow source through the
   surrounding medium) pushes the post-bow shock shell into direct contact
   with the jet beam. This is a possible mechanism for modeling
   well-collimated ``molecular jets{''} as an atomic/ionic flow which
   entrains molecules initially present only in the surrounding
   environment.}},
