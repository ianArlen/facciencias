@article{ ISI:000299902100006,
Author = {Maria Ortega, Rosa and Aguilar, Cesar and Villasenor, Luis and Montes,
   Manuel and Sierra, Gerardo},
Title = {{Towards the identification of hyponym/hypernym relations in the Internet}},
Journal = {{REVISTA SIGNOS}},
Year = {{2011}},
Volume = {{44}},
Number = {{75}},
Pages = {{68-84}},
Abstract = {{This paper presents an approach to the automatic extraction of hyponyms
   and hyperonyms. In particular, it proposes an information extraction
   method that is specially suited for identifying pairs of
   hyponym-hyperonym by using a set of Spanish lexical patterns. It also
   proposes a symmetric weighting scheme of pairs/patterns whose goal is to
   enhance the confidence of the extraction method. The effectiveness of
   the proposed approach was evaluated by extracting hyponyms from a given
   vocabulary of hyperonyms. Results show the usefulness of the proposed
   extraction method as well as the relevance of the pairs/patterns
   weighting scheme.}},
