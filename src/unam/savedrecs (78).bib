@article{ ISI:000290844800005,
Author = {Roman-Contreras, Ramiro and Martinez-Mayen, Mario},
Title = {{Notes on some alpheid shrimps (Decapoda: Caridea) of Thalassia
   testudinum meadows, from the Central-Southern Mexican Caribbean}},
Journal = {{HIDROBIOLOGICA}},
Year = {{2010}},
Volume = {{20}},
Number = {{3}},
Pages = {{222-229}},
Month = {{DEC}},
Abstract = {{This study provides an account of some shrimps of the family Alpheidae
   collected on Thalassia testudinum meadows in Bahia de la Ascension,
   Bahia del Espiritu Santo and Mahahual reef lagoon, Quintana Roo, on the
   Caribbean coast of Mexico. A total of 657 specimens of alpheid shrimps
   were collected; they represent two genera and seven species. Alpheus was
   the most diverse genus with five species. An updated geographic
   distribution is provided for all the seven species recorded; four of
   them represent new records for Bahia del Espiritu Santo and two for the
   Mahahual reef lagoon.}},
