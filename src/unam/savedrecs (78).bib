@article{ ISI:000296564600013,
Author = {Elias-Maxil, Jorge A. and Rigas, Fotis and Teresa Orta de Velasquez,
   Maria and Ramirez-Zamora, Rosa-Maria},
Title = {{Optimization of Fenton's reagent coupled to Dissolved Air Flotation to
   remove cyanobacterial odorous metabolites and suspended solids from raw
   surface water}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{2011}},
Volume = {{64}},
Number = {{8}},
Pages = {{1668-1674}},
Abstract = {{Ferrous salts are commonly used as coagulants in Water Treatment Plants
   (WTPs). When these salts are combined with hydrogen peroxide in acidic
   conditions - besides coagulation - an additional Advanced Oxidation
   Process (Fenton's reagent) can take place. Using a response surface
   methodology, this paper presents the optimization of a novel treatment
   system constituted by Fenton's reagent (FE) and Dissolved Air Flotation
   (DAF) for removing 2-Methylisoborneol (MIB), geosmin and Total Suspended
   Solids (TSS) from raw water. FE was proven able to remove completely
   both micro pollutants found in the influent of a drinking water
   treatment plant. Moreover, higher clarification rate was achieved by
   coupling FE-DAF with respect FE-Sedimentation.}},
