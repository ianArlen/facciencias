@article{ ISI:000286787200001,
Author = {Escudero, R. and Escamilla, R.},
Title = {{Ferromagnetic behavior of high-purity ZnO nanoparticles}},
Journal = {{SOLID STATE COMMUNICATIONS}},
Year = {{2011}},
Volume = {{151}},
Number = {{2}},
Pages = {{97-101}},
Month = {{JAN}},
Abstract = {{ZnO nanoparticles with the wurtzite structure were prepared by chemical
   methods at low temperature in aqueous solution. The size of the
   nanoparticles is in the range from about 10 to 30 nm. Ferromagnetic
   properties were observed from 2 K to room temperature and above.
   Magnetization versus temperature, M(T), and isothermal M(H) measurements
   were obtained. The coercive field clearly shows ferromagnetism above
   room temperature. An exchange bias was observed, and we related this
   behavior to the core-shell structure present in the samples. The
   chemical synthesis, structure, and defects in the bulk related to oxygen
   vacancies are the main factors for the observed magnetic behavior. (C)
   2010 Elsevier Ltd. All rights reserved.}},
