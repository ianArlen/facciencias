@article{ ISI:000285899100009,
Author = {Dufresne, Anja and Salinas, Sergio and Siebe, Claus},
Title = {{Substrate deformation associated with the Jocotitlan edifice collapse
   and debris avalanche deposit, Central Mexico}},
