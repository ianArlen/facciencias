@article{ ISI:000284854300012,
Author = {Marziani, P. and Sulentic, J. W. and Negrete, C. A. and Dultzin, D. and
   Zamfir, S. and Bachev, R.},
Title = {{Broad-line region physical conditions along the quasar eigenvector 1
   sequence}},
