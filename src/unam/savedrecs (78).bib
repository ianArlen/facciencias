@article{ ISI:000286910000023,
Author = {Garcia, Manuel and Fomina, Lioudmila and Fomine, Serguei},
Title = {{Electronic structure evolution of neutral and dicationic states of
   conjugated polymers with their band gap}},
Journal = {{SYNTHETIC METALS}},
Year = {{2010}},
Volume = {{160}},
Number = {{23-24}},
Pages = {{2515-2519}},
Month = {{DEC}},
Abstract = {{The electronic structures of neutral and dicationic states of 8
   conjugated oligomers were studied at UB3LYP/6-31G{*} level as models of
   conjugated polymers with different band gaps (E-g). The evolution of the
   electronic structure for polymers with experimental E-g ranging from 3.0
   to 0.0 eV has been investigated for both neutral and dicationic species.
   For polymers with E-g >1.1 eV the ground state of neutral oligomers was
   found to be a closed shell singlet (SO) while the dicationic state was a
   polaron pair open shell singlet (OSS) state. For polymers with lower Eg.
   OSS becomes the ground state for both dicationic and neutral states. The
   energy difference between OSS and SO state increases with closure of E-g
   for neutral oligomers. However, in dications the SO-OSS energy
   difference becomes less than 1 kcal/mol for the low band gap polymers.
   Both, the charge distribution and the bond length alternation pattern
   suggest that bipolaronic state and not polaron pair state dominates in
   dications of the low band gap polymers (E-g <1.1 eV). (C) 2010 Elsevier
   B.V. All rights reserved.}},
