@article{ ISI:000285859900025,
Author = {Zepeda, V. A. and Caderon-Apodaca, N. L. and Paasch, M. L. and Martin,
   P. G. and Paredes, D. A. and Ramirez-Apolinar, S. and Soriano-Vargas, E.},
Title = {{Histopathologic Findings in Chickens Experimentally Infected with
   Gallibacterium anatis by Nasal Instillation}},
Journal = {{AVIAN DISEASES}},
Year = {{2010}},
Volume = {{54}},
Number = {{4}},
Pages = {{1306-1309}},
Month = {{DEC}},
Abstract = {{Histologic findings in chickens experimentally infected by nasal
   instillation with reference strains of Gallibacterium anatis are
   described. No clinical signs were observed in experimentally infected
   birds; however, sequential histologic examinations of trachea, lung, air
   sacs, and liver revealed lesions in all infected chickens. Our
   observations suggest that the reference strains of G. anatis used in
   this experiment are capable of causing primary infection in chickens.
   Despite that the experimental birds were inoculated by intranasal route,
   lesions were detected in the liver, suggesting a probable bacteremia.
   Because several degrees of severity were established in histopathologic
   lesions, probable variations in virulence, among the experimental
   strains, also are discussed.}},
Publisher = {{AMER ASSOC AVIAN PATHOLOGISTS}},
Address = {{953 COLLEGE STATION RD, ATHENS, GA 30602-4875 USA}},
Type = {{Article}},
