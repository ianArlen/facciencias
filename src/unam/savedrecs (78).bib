@article{ ISI:000284454700004,
Author = {Eugenia Romero-Ibarra, Maria},
Title = {{The Agrarian reform of Cardenas and the sugar industry in Mexico,
   1930-1960}},
Journal = {{HISTORIA AGRARIA}},
Year = {{2010}},
Number = {{52}},
Pages = {{103+}},
Month = {{DEC}},
Abstract = {{In this work the process of expropriation of agricultural industries and
   the creation of cooperative societies are presented as integral parts of
   Lazar Cardenas land reform. The expropriation of the United Sugar
   Companies, SA and the subsequent formation of the Sociedad de Interes
   Colectivo Agricola Ejidal `Emancipacion proletaria' in 1938 is
   discussed. Our thesis is that as a result of the transformation of the
   land ownership system in Mexico, a scheme based on the commonly-owned
   ejido became the cornerstone of the economy. Moreover, the success of
   the process was based on the expropriation of the most successful
   agribusiness. Our case study allows us to see the role played in its
   failure by the lack of productive and financial autonomy of the
   cooperative and its political manipulation by the government's party}},
