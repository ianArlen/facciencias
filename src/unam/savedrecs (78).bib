@article{ ISI:000285365900006,
Author = {Manuel Fragoso, Jose and Delgadillo, Hilda and Juarez-Cedillo, Teresa
   and Manuel Rodriguez-Perez, Jose and Vallejo, Maite and Perez-Mendez,
   Oscar and Alvarez-Leon, Edith and Antonio Pena-Duque, Marco and Antonio
   Martinez-Rios, Marco and Vargas-Alarcon, Gilberto},
Title = {{The Interleukin 6-572 G > C (rs1800796) Polymorphism Is Associated with
   the Risk of Developing Acute Coronary Syndrome}},
Journal = {{GENETIC TESTING AND MOLECULAR BIOMARKERS}},
Year = {{2010}},
Volume = {{14}},
Number = {{6}},
Pages = {{759-763}},
Month = {{DEC}},
Abstract = {{Background: Inflammation plays an essential role in the development and
   progression of atherosclerotic lesions and plaque disruption. Cytokines
   are central regulators in immunoinflammatory mechanisms. The aim of the
   present study was to evaluate the role of INF-gamma and IL-6 gene
   polymorphisms as susceptibility markers for acute coronary syndromes
   (ACS) in a group of Mexican patients. Methods: Four polymorphisms
   (INF-gamma-155 G>A, INF-gamma -179 T>G, IL-6 -572 G>C, and IL-6 -1426
   T>G) of the INF-gamma and IL-6 genes were analyzed by 50 exonuclease
   TaqMan genotyping assays in a group of 284 patients with ACS (mean age =
   58.88 +/- 11.7 years) and 247 healthy unrelated controls (mean age =
   56.03 +/- 4.13 years). Results: Distribution of INF-gamma-179 G>T
   (rs2069709) and INF-gamma -155 G>A (rs2069710) polymorphisms was similar
   in patients and healthy controls. A significant difference was observed
   in the distribution of IL-6 -572 G>C (rs1800796) genotypes between
   patients with ACS and healthy controls (p = 0.019). Estimated odds ratio
   and 95\% confidence interval (CI) showed a protective role of the IL-6
   -572 (rs1800796) CC when compared with the GG genotype (wild type) (odds
   ratio = 0.6, 95\% CI = 0.33-1.08). In this case, heterozygous
   individuals with GC genotype had 2.19-fold increased risk of developing
   ACS than CC homozygous (p = 0.007, 95\% CI = 1.24-3.96). Conclusions:
   The results suggest that IL-6 -572 G>C (rs1800796) polymorphism could be
   involved in the risk of developing ACS in Mexican individuals.}},
