@article{ ISI:000282853500047,
Author = {Garcia, Carlos A. and Manzini, Fabio and Islas, Jorge},
Title = {{Air emissions scenarios from ethanol as a gasoline oxygenate in Mexico
   City Metropolitan Area}},
