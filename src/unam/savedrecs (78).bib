@article{ ISI:000284402800001,
Author = {Cambiaso, Mauro and Urrutia, Luis F.},
Title = {{Extended solution space for Chern-Simons gravity: The slowly rotating
   Kerr black hole}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2010}},
Volume = {{82}},
Number = {{10}},
Month = {{NOV 18}},
Abstract = {{In the Einstein-Cartan formulation, an iterative procedure to find
   solutions in nondynamical Chern-Simons gravity in vacuum is proposed.
   The iterations, in powers of a small parameter beta which codifies the
   Chern-Simons coupling, start from an arbitrary torsionless solution of
   Einstein equations. With Schwarzschild as the zeroth-order choice, we
   derive a second-order differential equation for the O(beta) corrections
   to the metric, for an arbitrary zeroth-order embedding parameter. In
   particular, the slowly rotating Kerr metric is an O(beta) solution in
   either the canonical or the axial embeddings.}},
