@article{ ISI:000283761900026,
Author = {Almora-Diaz, Cesar X. and Bunge, Carlos F.},
Title = {{Nonrelativistic CI Calculations for B+, B, and B- Ground States}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2010}},
