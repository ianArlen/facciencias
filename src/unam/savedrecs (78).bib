@article{ ISI:000283098800012,
Author = {Paez-Osuna, Federico and Calderon-Campuzano, Maria F. and Soto-Jimenez,
   Martin F. and Ruelas-Inzunza, Jorge R.},
Title = {{Trace Metals (Cd, Cu, Ni, and Zn) in Blood and Eggs of the Sea Turtle
   Lepidochelys olivacea from a Nesting Colony of Oaxaca, Mexico}},
