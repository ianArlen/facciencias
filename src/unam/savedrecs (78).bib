Author = {del Carmen Perrilliat, Maria and Vega, Francisco J. and Coutino, Marco
   A.},
Title = {{Miocene mollusks from the Simojovel area in Chiapas, southwestern Mexico}},
Journal = {{JOURNAL OF SOUTH AMERICAN EARTH SCIENCES}},
