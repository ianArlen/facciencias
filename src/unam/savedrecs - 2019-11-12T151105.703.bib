@article{ ISI:000202950200011,
Author = {Rios-Jara, D. and Guenin, G.},
Title = {{On the characterization and origin of the dislocations associated with
   the two way memory effect in Cu-Zn-Al thermoelastic alloys-II. The model
   for the formation of dislocations}},
Journal = {{ACTA METALLURGICA}},
Year = {{1987}},
Volume = {{35}},
Number = {{1}},
Pages = {{121-126}},
Month = {{JAN}},
Abstract = {{We propose a model for the formation of dislocations produced by the
   martensitic transformation of beta phases into 18R (or 9R) structures in
   thermoelastic alloys. The model is based on the combination of fault
   vectors associated with the structural stacking faults always present in
   18R or 9R martensites. This explains the type and geometry of the
   dislocations observed in the 8, phase after the reverse transformation.}},
