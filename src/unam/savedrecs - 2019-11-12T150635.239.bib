@article{ ISI:000177825600020,
Author = {Estrella, V and Nair, MTS and Nair, PK},
Title = {{Crystalline structure of chemically deposited thallium sulfide thin
   films}},
Journal = {{THIN SOLID FILMS}},
Year = {{2002}},
Volume = {{414}},
Number = {{2}},
Pages = {{281-287}},
Month = {{JUL 22}},
Abstract = {{Thin film coatings of Tl4S3 or Tl2S of 100-750 nm in thickness have been
   deposited from a mixture of citratothallium complex and thiourea
   solutions at 35 or 50degreesC by chemical bath deposition on glass
   substrates coated with a thin ZnS film. Upon heating in a nitrogen
   atmosphere at 300degreesC, all the films, irrespective of the initial
   composition, convert to crystalline Tl2S. The variation of the optical
   absorption coefficient with photon energy for a typical sample of Tl2S
   suggests forbidden direct transition as the mechanism for optical
   excitation. All the films, `as prepared' as well as those heated to
   300degreesC, are photoconductive. In the heated films dark conductivity
   is typically 7.5 X 10(-6) Omega(-1) cm(-1) and photoconductivity, 5 X
   10(-5) Omega(-5) cm(-1). (C) 2002 Elsevier Science B.V. All rights
   reserved.}},
