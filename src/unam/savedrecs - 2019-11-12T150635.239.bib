@article{ ISI:000179512100002,
Author = {Pech, GG and Brito, WF and Mena, GJ and Quijano, L},
Title = {{Constituents of Acacia cedilloi and Acacia gaumeri. Revised structure
   and complete NMR assignments of resinone}},
Journal = {{ZEITSCHRIFT FUR NATURFORSCHUNG SECTION C-A JOURNAL OF BIOSCIENCES}},
Year = {{2002}},
Volume = {{57}},
Number = {{9-10}},
Pages = {{773-776}},
Month = {{SEP-OCT}},
Abstract = {{The rare lupene derivative named resinone has only been isolated before
   from Fluorensia resinosa. We now report the isolation of this compound
   from the bark of the new recently described Acacia cedilloi (Fabaceae),
   and the revision of its structure to 16beta-hydroxylup-20(29)-en-3-one,
   based on NMR and MS spectral data. The detailed (1)H and (13)C NMR
   assignments of resinone and its acetate achieved by 1D and 2D NMR
   experiments (including DEPT, COSY, HMQC and HMBC) are reported. In
   addition, the study of A. cedilloi and A. gaumeri afforded the known
   related lupenes lupeol and lupenone, the acyclic squalene, the sterols
   beta-sitosterol, stigmasta-7,22-dien-3beta-ol (spinasterol) and
   stigmasta-5,22,25-trien-3beta-ol (22-dehydroclerosterol) as well as
   a-tocopherol and beta-carotene.}},
