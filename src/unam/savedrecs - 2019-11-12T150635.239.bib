@article{ ISI:000178425400002,
Author = {de la Pena, L and Cetto, AM},
Title = {{Planck's law as a consequence of the zeropoint radiation field}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2002}},
Volume = {{48}},
Number = {{1}},
Pages = {{1-8}},
Month = {{SEP}},
Note = {{Conference held in honor of Leopoldo Garcia Colin Scherers 70th
   Birthday, UNIV AUTONOMA METROPOLITANA, IZTAPALAPA, MEXICO, NOV 29-30,
   2000}},
Abstract = {{In this paper we show that a strong link exists between Planck's
   blackbody radiation formula and the zeropoint field. Specifically, the
   hypothesis that the equilibrium field (including the zeropoint term)
   follows a canonical distribution, in combination with the requirements
   that its specific heat remain finite as the temperature T --> 0 and that
   it behave classically as T --> infinity, implies that the energy of the
   zeropoint field has a fixed value and leads to Planck's law for the
   equilibrium distribution at arbitrary T; this in turn implies
   quantization of the energy of the field oscillators. There is no need to
   introduce any quantum property or ad hoc assumption for the purpose of
   the present derivation.}},
