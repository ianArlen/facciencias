@article{ ISI:000176822900001,
Author = {Padilla-Noriega, L and Paniagua, O and Guzman-Leon, S},
Title = {{Rotavirus protein NSP3 shuts off host cell protein synthesis}},
Journal = {{VIROLOGY}},
Year = {{2002}},
Volume = {{298}},
Number = {{1}},
Pages = {{1-7}},
Month = {{JUN 20}},
Abstract = {{A recombinant vaccinia virus encoding rotavirus protein NSP3 driven by
   an internal ribosome entry site (IRES) from the encephalomyocarditis
   (EMC) virus was able to abate protein synthesis in BSC1 cells by
   25-fold, with as much as 30\% of the remaining protein synthesis being
   NSP3. Hence NSP3 shuts off host cell protein synthesis down to the level
   seen during rotavirus infection but is unable to prevent translation
   from EMC IRES-driven genes. This effect was abolished by deletions in
   the eIF4G-binding (aa 274-313) and the dimerization (aa 150-206) but not
   the viral mRNA-binding (aa 83-149) domains, supporting that NSP3
   functions in vivo as a dimer, Binding of eIF4G by NSP3 has been
   implicated in interfering with mRNA 5'-3' circularization, hence such
   circularization is essential for translation in mammalian cells. (C)
   2002 Elsevier Science (USA).}},
