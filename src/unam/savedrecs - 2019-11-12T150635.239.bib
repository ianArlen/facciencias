@article{ ISI:000178383400002,
Author = {Perez-Cabrera, A and Kofman-Alfaro, S and Zenteno, JC},
Title = {{Mutational analysis of HOXD13 and HOXA13 genes in the triphalangeal
   thumb-brachyectrodactyly syndrome}},
Journal = {{JOURNAL OF ORTHOPAEDIC RESEARCH}},
Year = {{2002}},
Volume = {{20}},
Number = {{5}},
Pages = {{899-901}},
Month = {{SEP}},
Abstract = {{The triphalangeal thumb-brachyectrodactyly syndrome is a very rare
   autosomal dominant disorder of unknown etiology characterized by an
   unusual pattern of limb malformations: triphalangeal thumbs and
   brachyectrodactyly in the hands, and ectrodactyly in the feet. In a
   previous report, we described the clinical and radiographical features
   of three related subjects with the disease and suggest that due to the
   unusual combination of limb defects and to its phenotypic similarity
   with the limb malformative pattern induced by disrupting the Hoxd13 gene
   in mouse, the triphalangeal thumb-brachyectrodactyly syndrome might be
   caused by mutations in a HOX gene. After sequencing the entire coding
   region of HOXD13 and the highly conserved homeodomain encoding region of
   HOXA13, we do not detect any deleterious mutation in any of the patients
   excluding that alterations at these sequences are responsible for the
   disease. Mutations in regulatory regions of these genes or in other
   genes involved in limb development might be responsible for the disease.
   (C) 2002 Orthopaedic Research Society. Published by Elsevier Science
   Ltd. All rights reserved.}},
