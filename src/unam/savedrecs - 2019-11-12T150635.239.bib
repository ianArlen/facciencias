@article{ ISI:000178425400015,
Author = {Rodriguez, RF and Camacho, JE},
Title = {{Non-equilibrium effects on the light scattering spectrum of a nematic
   driven by a pressure gradient}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2002}},
Volume = {{48}},
Number = {{1}},
Pages = {{144-153}},
Month = {{SEP}},
Note = {{Conference held in honor of Leopoldo Garcia Colin Scherers 70th
   Birthday, UNIV AUTONOMA METROPOLITANA, IZTAPALAPA, MEXICO, NOV 29-30,
   2000}},
Abstract = {{The dynamics of thermal fluctuations about a non-equilibrium steady
   state of a nematic liquid crystal is investigated. We study
   theoretically their effects on the dynamic structure factor of a nematic
   under a Poiseuille flow induced by an external pressure gradient delp.
   We find that the equilibrium and non-equilibrium contributions vary as
   k(-4) and k(-5), respectively. The out of equilibrium correction
   introduces an asymmetry in the shape and a change in the amplitude of
   the equilibrium lorentzian, shifting its maximum towards the region of
   negative values of the frequency shift. Near equilibrium these changes
   depend linearly on the magnitude of the applied gradient. We find that
   these effects may be as large as 94.3\% for values of
   \textbackslash{}delp\textbackslash{} = 2.64 x 10(-2) atm/cm, which
   suggest that they might be observable. We discuss the limitations and
   possible generalizations of our model and approach, and conclude by
   making further physical remarks.}},
