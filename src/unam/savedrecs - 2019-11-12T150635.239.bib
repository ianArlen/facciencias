@article{ ISI:000177660400004,
Author = {O'Connor, D and Stephens, CR and Santiago, JA},
Title = {{Dimensional crossover in the non-linear sigma model}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2002}},
Volume = {{48}},
Number = {{4}},
Pages = {{300-306}},
Month = {{AUG}},
Abstract = {{We consider dimensional crossover for an O(N) model on a d-dimensional
   layered geometry of thickness L, in the sigma-model limit, using
   ``environmentally friendly{''} renormalization. We show how to derive
   critical temperature shifts, giving explicit results to one loop. We
   also obtain expressions for the effective critical exponents delta(eff)
   and beta(eff) that interpolate between their characteristic fixed point
   values associated with a d and (d - 1)-dimensional system in the limits
   T --> T(c) (L), with L(T - T(c) (L))(nu) --> infinity, and T --> T(c)
   (L) for L fixed respectively, where T(c) (L) is the L-dependent critical
   temperature of the system.}},
