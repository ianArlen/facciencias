@article{ ISI:000176740700017,
Author = {Mireles, F and Kirczenow, G},
Title = {{From classical to quantum spintronics: Theory of coherent spin injection
   and spin valve phenomena}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{2002}},
Volume = {{59}},
Number = {{1}},
Pages = {{107-113}},
Month = {{JUL}},
Abstract = {{We present a theory of coherent quantum transport in ferromagnetic/
   nonmagnetic/ ferromagnetic heterojunctions. We predict quantum coherence
   to give rise to a quantum spin valve effect that, unlike its familiar
   classical analog, occurs even in the absence of a net spin current
   through the heterostructure. Thus the relationship between spin and
   charge transport is qualitatively different in the presence of quantum
   interference than in the (semi-) classical regime. This has important
   implications for the design of quantum coherent spintronic devices and
   the interpretation of experiments.}},
