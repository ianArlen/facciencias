@article{ ISI:000181680400004,
Author = {Valencia, M and Doble, J and Arbiza, SI},
Title = {{Sources of environmental variation affecting lactation and pre-weaning
   growth characteristics in Saanen goats}},
Journal = {{CUBAN JOURNAL OF AGRICULTURAL SCIENCE}},
Year = {{2002}},
Volume = {{36}},
Number = {{2}},
Pages = {{117-122}},
Month = {{JUN}},
Abstract = {{One thousand four hundred and thirteen records on milk production and
   399 records on birth (BW) and weaning weights were analyzed to study the
   influence of certain environmental factors on lactation and preweaning
   growth traits in stabulated Saanen goats. Information from 1988 to 1997
   was analyzed. The influence of age, kidding season (KS), litter size
   (LS), year and interactions of first order on total milk yield (TY),
   accumulated milk yield to 120 d lactation (Y120) and lactation length
   (LL) and the effects of year, season of birth (SB) and their interaction
   with BW, daily gain (DG) and weight adjusted at 60 d (AW) were studied.
   The least square means for TY, Y120 and LL were 800.0 +/- 2 kg, 337.7
   +/- 95 kg and 284 +/- 19 d, respectively, and for BW, DG, and AW were
   3.6 +/- 0.5 kg, 191 +/- 49 kg and 15.2 +/- 2.6 kg, respectively. Average
   age at first kidding was 18.5 months and litter size was 1.70 kids per
   kidding. The year x KS, year x age and age x KS interactions were
   significant (P < 0.01) for TY and LL, also year x age and age x KS
   affected Y120. LS was significant (P < 0.01) only for TY and Y120. The
   year x SB interaction affected DG and AW and the year effect affected BW
   (P < 0.05). These results may allow to correct the environmental effects
   that notably affected the variables analyzed, which could improve
   accuracy in animal selection.}},
