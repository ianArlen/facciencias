@article{ ISI:000178539100004,
Author = {Alcocer, J and Bernal-Brooks, FW},
Title = {{Spatial and temporal heterogeneity of physical and chemical variables
   for an endorheic, shallow water body: Lake Patzcuaro, Mexico}},
Journal = {{ARCHIV FUR HYDROBIOLOGIE}},
Year = {{2002}},
Volume = {{155}},
Number = {{2}},
Pages = {{239-253}},
Month = {{SEP}},
Abstract = {{Homogeneity of water quality in a shallow endorheic lake in Mexico, Lake
   Patzcuaro, was studied through the analysis of 13 physical and chemical
   variables at 10 sampling stations monthly-between January 1998 and
   January 1999. Principal components analysis associated most temporal and
   spatial variance with two variables: total residue and conductance.
   Spatial analysis discriminated between limnetic and littoral areas, and
   among shallow stations. Temporal analysis segregated four time
   intervals: cold/dry, warm/dry, rainy, and highest rainfall. Even though
   Lake Patzcuaro is a continuous warm polymictic lake, our results show
   its environmental heterogeneity; the lake is a physical and chemical
   mosaic.}},
