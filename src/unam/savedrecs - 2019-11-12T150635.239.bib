@article{ ISI:000177660400016,
Author = {Socolovsky, M},
Title = {{On Bell's theorem}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2002}},
Volume = {{48}},
Number = {{4}},
Pages = {{384-390}},
Month = {{AUG}},
Abstract = {{Bell's theorem is a crucial toot to solve the contradiction between
   local realism conceptions a la Einstein-Podolsky-Rosen and quantum
   mechanics. In this note we present three versions of the theorem: the
   original one of Bell of 1964, the Clauser et al stronger version of
   1969, and the Wigner's version of 1970, and show how the simultaneous
   validity of EPR ideas and quantum theory leads to a contradiction with
   number theory. The quantum mechanical formula for the correlation
   between two spin 1/2 particles is derived in an appendix.}},
