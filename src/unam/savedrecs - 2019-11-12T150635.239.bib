@article{ ISI:000177488100002,
Author = {Ocampo, AL and Castellanos, RH and Sebastian, PJ},
Title = {{Kinetic study of the oxygen reduction reaction on Ru-y(CO)(n) in acid
   medium with different concentrations of methanol}},
Journal = {{JOURNAL OF NEW MATERIALS FOR ELECTROCHEMICAL SYSTEMS}},
Year = {{2002}},
Volume = {{5}},
Number = {{3}},
Pages = {{163-168}},
Month = {{JUL}},
Abstract = {{The electrocatalytic reduction of oxygen was investigated in aqueous
   solutions of 0.5 M H2SO4 containing dissolved methanol at differqnt
   concentrations with a rotating Ru-y(CO)(n) disk electrode. The catalyst
   was synthesized by condensation reactions under refluxing conditions.
   The electrochemical studies were performed at 25degreesC using
   voltamperometry and potential step techniques. The cyclic
   voltamperograms showed that the catalyst is insensitive to the methanol
   oxidation even upto two molar methanol concentrations. The kinetic
   parameters for Oxygen Reduction Reaction (ORR) such as Tafel slope,
   charge transfer coefficient, exchange current density and reaction order
   with respect to O-2 were calculated. The values of the diffusion
   coefficient and solubility of oxygen were determined for different
   concentrations of methanol. A variation in the transport parameters was
   found for each methanol concentration. The results showed that
   Ru-y(CO)(n) can be used as a selective cathode catalyst in a Direct
   Methanol Fuel Cell.}},
