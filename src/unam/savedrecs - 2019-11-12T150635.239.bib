@article{ ISI:000176444900012,
Author = {Martinez, F and Uribe, A and Espinosa-Garcia, MT and Flores-Herrera, O
   and Garcia-Perez, C and Milan, R},
Title = {{Calcium modulates the ATP and ADP hydrolysis in human placental
   mitochondria}},
Journal = {{INTERNATIONAL JOURNAL OF BIOCHEMISTRY \& CELL BIOLOGY}},
Year = {{2002}},
Volume = {{34}},
Number = {{8}},
Pages = {{992-1003}},
Month = {{AUG}},
Abstract = {{This study evaluated the effect of Ca2+ on the extramitochondrial
   hydrolysis of ATP and ADP by the extramitochondrial ATPase in isolated
   mitochondria and submitochondrial particles (SMPs) from human term
   placenta. The effect of different oxidizable substrates on the
   hydrolysis of ATP and ADP in the presence of sucrose or K+ was
   evaluated. Ca2+ increased phosphate release from ATP and ADR but this
   stimulation showed different behavior depending on the oxidizable
   substrate present in the incubation media. Ca2- stimulated the
   hydrolysis of ATP and ADP in the presence of sucrose. However, Ca2+-did
   not stimulate the hydrolysis of ADP in the medium containing K+. Ca2+
   showed inhibition depending on the respiratory substrate. This study
   suggests that the energetic state of mitochondria controls the
   extramitochondrial ATPase activity, which is modulated by Ca2+ and
   respiratory substrates. (C) 2002 Elsevier Science Ltd. All rights
   reserved.}},
