@article{ ISI:000076478100005,
Author = {Ricardo-Chavez, JL and Dorantes-Davila, J and Terrones, H},
Title = {{Environment effects on the electronic properties of carbon fullerenes: A
   local approach}},
Journal = {{FULLERENE SCIENCE AND TECHNOLOGY}},
Year = {{1998}},
Volume = {{6}},
Number = {{5}},
Pages = {{769-786}},
Abstract = {{The electronic structure of some relevant carbon fullerenes are
   determined by using a sp-electron tight-binding Hamiltonian including
   Coulomb interactions in the Hartree-Fock approximation. Surfaces with
   positive, non-positive and mixed curvatures are considered in order to
   study the effect of the local environment on the relevant electronic
   properties. Nanotubes having different diameters and helicities are
   analyzed and the results are compared with previous calculations. In
   each case, the equilibrium bond-length R is optimized by maximizing the
   cohesive energy E-coh(N). The perspectives and extensions of this study
   are also discussed.}},
