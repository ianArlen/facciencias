@article{ ISI:000071336800003,
Author = {Zhu, YH},
Title = {{Microstructure dependence of the creep behavior of a Zn-Al based alloy}},
Journal = {{JOURNAL OF MATERIALS PROCESSING TECHNOLOGY}},
Year = {{1998}},
Volume = {{73}},
Number = {{1-3}},
Pages = {{18-24}},
Month = {{JAN}},
Abstract = {{Based on investigations of creep-induced microstructural changes and
   phase transformation in furnace cooled, cast and extruded eutectoid
   Zn-Al alloys, the mechanism of creep rupture is discussed. It is found
   that similar microstructural change and phase transformations occurred
   in these eutectoid Zn-Al alloys during tensile creep testing, i.e.
   partial spheroidization of lamellae structure, and decomposition of
   metastable phases eta(Tau)(') and a four-phase transformation, alpha +
   epsilon --> Tau(') + eta. The structure spheroidization of the coarsened
   lamellae structure plays an important part in the attainment of better
   elongation and tensile ductility, in combination with which the
   strengthened fine lamellae structure much improves the creep resistance
   and creep rupture strain. (C) 1998 Elsevier Science S.A.}},
