@article{ ISI:000071820700002,
Author = {Garcia-Segundo, C and Villagran-Muniz, M and Muhl, S},
Title = {{Determination of thin film optical properties by the photoacoustic OPC
   technique}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{1998}},
Volume = {{31}},
Number = {{2}},
Pages = {{165-171}},
Month = {{JAN 21}},
Abstract = {{Through the use of maxima and minima of interference fringes in
   transmittance spectra registered by the open photoacoustic cell (OPC)
   detection technique, we have determined the refraction index n and
   thickness d of aluminium nitride thin films which had previously been
   deposited on opaque and transparent substrate. The results for the
   transparent substrate are in agreement with those obtained from the
   transmission spectra. To determine n as a function of lambda we
   developed a novel procedure for the analysis of the OPC spectra. The n
   values so obtained were introduced in an appropriate Airy transmittance
   function, and their numerical evaluation permits the reproduction of the
   experimental transmittance OPC data and optical curves. This scheme was
   used to establish the uniqueness of the calculated data.}},
