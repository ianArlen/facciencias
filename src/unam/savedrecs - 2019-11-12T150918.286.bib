@inproceedings{ ISI:000078577700058,
Author = {Klimova, T and Ramirez, J and Calderon, M and Dominguez, JM},
Editor = {{Bonneviot, L and Beland, F and Danumah, C and Giasson, S and Kaliaguine, S}},
Title = {{New Mo and NiMo catalysts supported on MCM-41/alumina for thiophene
   hydrodesulfurization}},
Booktitle = {{MESOPOROUS MOLECULAR SIEVES 1998}},
Series = {{STUDIES IN SURFACE SCIENCE AND CATALYSIS}},
Year = {{1998}},
Volume = {{117}},
Pages = {{493-500}},
Note = {{1st International Symposium on Mesoporous Molecular Sieves (ISMMS98),
   BALTIMORE, MARYLAND, JUL 10-12, 1998}},
Abstract = {{A series of Mo and NiMo catalysts supported on MCM-41-Al2O3 with
   different amounts of MCM-41 was prepared and tested in the thiophene
   hydrodesulfurization reaction. In the Mo unpromoted catalysts, an
   increase in the content of MCM-41 caused a decrease in the thiophene
   conversion. In contrast, for the NiMo catalysts, where Ni was
   impregnated first, an increase in the thiophene conversion was observed
   with the MCM-41 loading. These different trends in HDS catalytic
   activity of unpromoted and Ni promoted Mo catalysts were related,
   according to the characterization results, to changes in the dispersion
   of the Mo phase induced by the presence of the MCM-41 catalyst
   component, which also change when Ni is impregnated prior to Mo. It was
   also found for the reaction studied here, that the
   hydrodesulfurization/hydrogenation ratio increases with MCM-41 content
   in the NiMo catalysts.}},
