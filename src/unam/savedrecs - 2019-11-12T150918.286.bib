@article{ ISI:000071684600001,
Author = {Mercado-Ruaro, P and Delgado-Salinas, A},
Title = {{Karyotypic studies on species of Phaseolus (Fabaceae : Phaseolinae)}},
Journal = {{AMERICAN JOURNAL OF BOTANY}},
Year = {{1998}},
Volume = {{85}},
Number = {{1}},
Pages = {{1-9}},
Month = {{JAN}},
Abstract = {{Ten karyotypes of Phaseolus were analyzed. Possible directions of
   karyological evolution al those species ara suggested. All species
   studied have karyotypes composed exclusively of metacentric and
   submetacentric chromosomes, except P. filiformis with all metacentrics,
   and P. chiapasanus with one pair of subtelocentric chromosomes.
   Secondary constrictions and satellites are common among species studied.
   Phaseolus leptostachyus, P. micranthus, and P. macvaughii are considered
   as aneuploids with 2n = 20, whereas the rest of the species have 2n =
   22.
