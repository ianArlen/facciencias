@inproceedings{ ISI:000076910600024,
Author = {Rincon, ME and Corripio, C and Ruiz-Garcia, J},
Editor = {{Simpson, CR and Mendicino, L and Rajeshwar, K and Fenton, JM}},
Title = {{Photoelectrochemical characterization of multilayer structures obtained
   by chemical bath and screen printing techniques}},
Booktitle = {{ENVIRONMENTAL ISSUES IN THE ELECTRONICS/SEMICONDUCTOR INDUSTRIES AND
   ELECTROCHEMICAL/PHOTOCHEMICAL METHODS FOR POLLUTION ABATEMENT}},
Series = {{ELECTROCHEMICAL SOCIETY SERIES}},
Year = {{1998}},
Volume = {{98}},
Number = {{5}},
Pages = {{213-231}},
Note = {{1st International Symp on Environmental Issues with Materials and
   Processes for the Electronics / Electrochemical and Photochemical
   Methods for Chemical Recycling, SAN DIEGO, CA, MAY, 1998}},
Organization = {{Electrochem Soc}},
Abstract = {{TiO(2) films are prepared by screen printing and sintering techniques
   and sensitized with the subsequent chemical deposition of CdSe and
   Bi(2)S(3). After annealing in air, the composite films exhibit
   diffraction peaks of anatase, zincite and either bismuthinite or cadmium
   selenide crystals. The onset of absorption of the coupled films is at
   energies close to the reported optical band-gaps of chemically deposited
   bismuthinite (1.3 - 1.5 eV) and cadmium selenide (1.8 eV). The
   sensitization of TiO(2) with Bi(2)S(3) gives a composite that undergoes
   fast corrosion at basic py while sensitization with CdSe gives a
   photosensitive stable composite. Atomic force microscopy and X-ray
   diffraction data indicate that the sintering step on the preparation of
   TiO(2) and the post-deposition thermal treatment of the coupled films
   improve the stability of the photoelectrode substantially. Without any
   heat treatment, exposure of the sensitized TiO(2) films to the working
   conditions of a photocatalytic reactor (low pH) causes dramatic changes
   on the films morphology.}},
Publisher = {{ELECTROCHEMICAL SOCIETY INC}},
Address = {{65 S MAIN ST, PENNINGTON, NJ 08534-2839 USA}},
Type = {{Proceedings Paper}},
