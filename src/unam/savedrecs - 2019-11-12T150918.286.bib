@article{ ISI:000072191800012,
Author = {Lemini, C and Rubio-Poo, C and Silva, G and de Coronado, IZ},
Title = {{Evidence of non-genomic effect of a 17 beta-diamino estrogen, an
   estradiol derivative, on motor activity in rodents}},
Journal = {{MEDICAL SCIENCE RESEARCH}},
Year = {{1998}},
Volume = {{26}},
Number = {{1}},
Pages = {{47-49}},
Month = {{JAN}},
Abstract = {{Experimental and clinical studies have indicated that steroids can
   affect motor and cognitive performance by their action on the
   dopaminergic systems of the brain, mediated by genomic and non-genomic
   effects. 17 beta-(N, N-dietilendiamine)-1,3,5(10)estratrien-3-ol
   dihydrochloride (DED) and 17 beta-(N,N-diethylaminoethyl)-amino-5
   alpha-androstan-3 beta-ol dihydrochloride (DAD) were evaluated for their
   effect on motor activity in CD1 mice and in normal and hyperactive
   Wistar rats induced by prenatal capsaicin treatment. Experimental groups
   were injected intraperitoneally (ip) with DED (1 mg/100 g), DAD (1
   mg/100 g), EDTA(1) (ethylene diamino tetra acetic acid, 1 mg/100 g), or
   EDTA(2) (2 mg/100 g), and a control group which received saline solution
   (SS). Motor activity was evaluated 5 min after the injection. DED
   inhibited locomotion in both mice and rats. DAD did not produce any
   changes in motor activity, showing the importance of the estrogen in
   producing these changes in behaviour. The effect on motion produced by
   DED in the animals cannot be explained by its quelant properties, since
   administration to mice at equivalent or higher doses of EDTA did not
   produce significant changes in motor activity. The rapid onset, and
   short-term effect of DED indicates that the depression of motor activity
   by DED is probably mediated by interactions of the steroid with membrane
   receptors, a non-genomic effect. (C) 1998 Chapman \& Hall Ltd.}},
