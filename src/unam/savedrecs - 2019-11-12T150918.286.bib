@article{ ISI:000075806500055,
Author = {Zhu, YH},
Title = {{Creep deformation and microstructural change in extruded Zn-Al based
   alloy}},
Journal = {{METALS AND MATERIALS INTERNATIONAL}},
Year = {{1998}},
Volume = {{4}},
Number = {{4}},
Pages = {{878-882}},
Abstract = {{Microstructural evolution and related phase transformation of an
   extruded eutectoid Zn-Al based alloy are studied in detail under creep
   deformation at 150 degrees C. Lamellar structure in the extruded alloy
   spheroidized partially into fine grain structure. Also decomposition of
   a metastable eta(T)' phase and a four phase transformation,
   alpha+epsilon-->T+eta, were observed in the creep deformed alloy
   specimens. Creep rupture of the extruded Zn-Al alloy was studied in
   correlating with the creep induced phase transformation and
   microstructural changes.}},
