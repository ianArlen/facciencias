@article{ ISI:000074824600054,
Author = {Terrones, H and Terrones, M},
Title = {{Fullerenes and nanotubes with non-positive Gaussian curvature}},
Journal = {{CARBON}},
Year = {{1998}},
Volume = {{36}},
Number = {{5-6}},
Pages = {{725-730}},
Note = {{Symposium A on Fullerenes and Carbon Based Materials, at the
   European-Materials-Research-Society 1997 Meeting, STRASBOURG, FRANCE,
   JUN 16-20, 1997}},
Organization = {{European Mat Res Soc; Council Europe; Commiss European Communities;
   Banque Populaire, France; Elsevier Sci, Netherlands; Off Tourisme,
   Strasbourg; SGL Carbon AG, Germany}},
Abstract = {{Closed fullerenes and nanotubes exhibiting high topologies from genus 5
   to genus 21 are proposed. These structures (holey-bails and holey-tubes)
   possess only hexagonal and heptagonal (negative Gaussian curvature)
   rings of carbon with no dangling bonds and no pentagonal rings (no
   positive Gaussian curvature). Holey-balls and holey-tubes can be seen as
   finite zeolites since they present interconnected channels suitable for
   locating guest molecules or atoms such as alkali metals. The geometry,
   stability and possible applications are studied. (C) 1998 Elsevier
   Science Ltd. All rights reserved.}},
