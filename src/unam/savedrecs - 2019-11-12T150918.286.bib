@inproceedings{ ISI:000077938500034,
Author = {Sanchez-Mejorada, G and Rosales, C},
Editor = {{Talwar, GP and Nath, I and Ganguly, NK and Rao, KVS}},
Title = {{FcR-dependent MAPK and nuclear factor NF-kappa B activation}},
Booktitle = {{10TH INTERNATIONAL CONGRESS ON IMMUNOLOGY, VOLS 1 AND 2}},
Year = {{1998}},
Pages = {{195-201}},
Note = {{10th International Congress of Immunology, NEW DELHI, INDIA, NOV 01-06,
   1998}},
Abstract = {{Cross-linking of Fc gamma R with immune complexes leads, first to
   activation of protein tyrosine kinases. The molecular events that follow
   and that transduce signals from these receptors to the nucleus are still
   poorly defined. We have investigated the signal transduction pathway
   from Fc receptors that leads to gene activation and production of
   cytokines in monocytes. We found that cross-linking of Fc receptors by
   immune complexes results in both NF-kappa B and IL-1 production.
   Expression of dominant negative mutants Ras N17 and Raf 23-284 did not
   inhibit activation of the NF-kappa B driven reporter. In contrast,
   tyrosine kinase inhibitors and the specific MEK inhibitor, PD098059,
   both efficiently abolished this activation. These data strongly suggests
   that the signaling pathway for MAPK activation in response to Fc
   receptor ligation initiates with tyrosine kinases, but it does not
   involve Ras and Raf.}},
