@article{ ISI:000072126900010,
Author = {Ceccatto, HA and Navone, HD and Waelbroeck, H},
Title = {{Learning persistent dynamics with neural networks}},
Journal = {{NEURAL NETWORKS}},
Year = {{1998}},
Volume = {{11}},
Number = {{1}},
Pages = {{145-151}},
Month = {{JAN}},
Abstract = {{Persistence is one of the most common characteristics of real-world time
   series. In this work we investigate the process of learning persistent
   dynamics by neural networks. We show that for chaotic times series the
   network can get stuck for long training periods in a trivial minimum of
   the error function related to the long-term autocorrelation in the
   series. Remarkably, in these cases the transition to the trained phase
   is quite abrupt. For noisy dynamics the training process is smooth. We
   also consider the effectiveness of two of the most frequently used
   decorrelation methods in avoiding the problems related to persistence.
   (C) 1998 Elsevier Science Ltd. All rights reserved.}},
