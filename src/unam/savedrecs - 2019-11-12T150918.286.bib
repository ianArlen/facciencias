@article{ ISI:000073996700011,
Author = {Hernandez-Becerril, DU and Navarro, N},
Title = {{Observations on the diatom Roundia cardiophora from the Caribbean Sea}},
Journal = {{NOVA HEDWIGIA}},
Year = {{1998}},
Volume = {{66}},
Number = {{3-4}},
Pages = {{481-487}},
Abstract = {{A study of the diatom Roundia cardiophora (Thalassiosiraceae), found in
   plankton net hauls from coasts of the Mexican Caribbean Sea and cultured
   from Puerto Rico, provides undescribed morphological characteristics.
   The species usually occurs in colonies, attached to macroalgae by
   threads, extruded by a group of 7-10 marginal fultoportulae. Numerous
   small chloroplasts are distributed in a peripheral layer. Morphological
   variation from the original description was evident in the cell diameter
   and in the separation between the rimoportula and the group of marginal
   fultoportulae. This is the first record of the species from the
   Caribbean Sea and the Atlantic Ocean. R. cardiophora shows an apparent
   tropical distribution (between 20 degrees N and 20 degrees S) in the
   Indian, Pacific and Atlantic Oceans. No other species within the family
   Thalassiosiraceae, shows similar growth form and morphological
   characteristics,evidence that supports the separation of the
   monospecific genus from Thalassiosira and related genera.}},
