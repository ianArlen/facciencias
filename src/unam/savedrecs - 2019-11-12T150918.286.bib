@inproceedings{ ISI:000076793400073,
Author = {Orlov, VG and Voitsekhovich, VV and Cuevas, S},
Editor = {{Bonaccini, D and Tyson, RK}},
Title = {{Speckle interferometry, partial adaptive correction for big telescopes
   and the outer scale of turbulence}},
