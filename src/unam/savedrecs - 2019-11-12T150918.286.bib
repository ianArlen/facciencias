@article{ ISI:A1998YH99000012,
Author = {Amanullah, A and SerranoCarreon, L and Castro, B and Galindo, E and
   Nienow, AW},
Title = {{The influence of impeller type in pilot scale Xanthan fermentations}},
Journal = {{BIOTECHNOLOGY AND BIOENGINEERING}},
