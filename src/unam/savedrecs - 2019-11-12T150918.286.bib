@article{ ISI:000078181000002,
Author = {Gomez-Torrente, M},
Title = {{On a fallacy attributed to Tarski}},
Journal = {{HISTORY AND PHILOSOPHY OF LOGIC}},
Year = {{1998}},
Volume = {{19}},
Number = {{4}},
Pages = {{227-234}},
Abstract = {{The purpose of this paper is to examine some passages of Tarski's paper
   `On the concept of logical consequence' and to show that some recent
   readings of those passages are wrong. John Etchemendy has claimed that
   in those passages Tarski gave an argument purporting to show that the
   notion of logical consequence defined by him (as opposed to some
   pretheoretic notion of logical consequence) possesses certain modal
   properties. Etchemendy further claims that the argument he attributes to
   Tarski is fallacious. Some of Etchemendy's critics have granted him that
   Tarski did give an argument purporting to show that the defined notion
   possesses certain modal properties; but they have claimed that Tarski's
   argument was not a fallacious one. I will show that both Etchemendy and
   his critics are wrong; in the relevant passages, Tarski did not offer
   (nor did he intend to offer) an argument that the defined notion of
   logical consequence possesses any modal properties.}},
