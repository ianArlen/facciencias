@inproceedings{ ISI:000076574000041,
Author = {Richer, MG and McCall, ML and Stasinska, G},
Editor = {{Friedli, D and Edmunds, M and Robert, C and Drissen, L}},
Title = {{The chemical evolution of dynamically hot systems}},
Booktitle = {{ABUNDANCE PROFILES: DIAGNOSTIC TOOLS FOR GALAXY HISTORY}},
Series = {{ASTRONOMICAL SOCIETY OF THE PACIFIC CONFERENCE SERIES}},
Year = {{1998}},
