@inproceedings{ ISI:000075172900018,
Author = {Raga, AC and Canto, J},
Editor = {{Massaglia, S and Bodo, G}},
Title = {{The implications of observations on models of Herbig-Haro objects}},
Booktitle = {{ASTROPHYSICAL JETS: OPEN PROBLEMS}},
Year = {{1998}},
Pages = {{201-209}},
Note = {{Meeting on Astrophysical Jets - Open Problems, TURIN, ITALY, DEC 11-13,
   1996}},
Abstract = {{A few Herbig-Haro (HH) objects have now been observed with the Hubble
   Space Telescope (HST). These observations have clearly resolved the
   cooling regions behind the shocks associated with these objects for the
   first time. Problems arise in trying to model the HST observations, as
   they show a separation between the bow shock and Mach disc of the same
   order as the jet diameter. We propose a model in which the large bow
   shock/Mach disc separation results from the presence a cool ``nose
   cone{''} produced by a non-top hat ram-pressure cross section of the
   jet. The conditions necessary for producing a ``magnetic nose cone{''}
   are also discussed, though this possibility is not pursued in detail in
   the present paper.}},
