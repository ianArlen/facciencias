@inproceedings{ ISI:000077771200025,
Author = {Rodriguez-Jauregui, E and Mondragon, A},
Editor = {{DOlivo, JC and RuizAltaba, M and Cendejas, LV}},
Title = {{The CKM matrix from a scheme of flavour symmetry breaking.}},
Booktitle = {{PARTICLES AND FIELDS: SIXTH MEXICAN WORKSHOP}},
