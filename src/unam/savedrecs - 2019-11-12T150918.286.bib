@inproceedings{ ISI:000076377700149,
Author = {Villarreal, C and Jauregui, R and Esquivel-Sirvent, R},
Editor = {{Kasperkovitz, P and Grau, D}},
Title = {{Electrodynamics of confined atoms and sonoluminescence}},
Booktitle = {{5TH WIGNER SYMPOSIUM, PROCEEDINGS}},
Year = {{1998}},
Pages = {{499-501}},
Note = {{5th Wigner Symposium, VIENNA, AUSTRIA, AUG 25-29, 1997}},
Abstract = {{Sonoluminescence (SL) is the periodic emission of ultrashort (< 50 ps)
   light flashes by an oscillating bubble trapped in a standing acoustic
   field in a liquid. Up to date, no theory provides a comprehensive
   explanation of all available experimental information. Based in a
   numerical representation of the experimental time dependence of the
   bubble radius R(t), we show that a theory supported on vacuum radiation
   originating from the imploding bubble surface cannot account for
   observed light intensities. By considering that SL may arise from more
   conventional processes, we study geometry-induced modifications of
   dipole transition rates of atoms placed inside or outside the bubble. We
   find that a polarization analysis of the emitted light may give clues on
   the bubble regions that contribute to the observed radiation.}},
