@article{ ISI:000075791700001,
Author = {Alduncin, G},
Title = {{Numerical resolvent methods for macro-hybrid mixed variational
   inequalities}},
Journal = {{NUMERICAL FUNCTIONAL ANALYSIS AND OPTIMIZATION}},
Year = {{1998}},
Volume = {{19}},
Number = {{7-8}},
Pages = {{667-696}},
Abstract = {{Numerical resolvent methods are presented for macro-hybrid mixed
   variational inequalities, which render Uzawa and penalty-duality type
   iterative algorithms. The subdifferential methodology for constrained
   problems is extended to domain decomposition hybrid formulations. Then,
   systematically, finite element discretizations can be constructed, and
   numerical iterative algorithms are produced via, resolvent
   characterizations and proximal-point approximations. For associated
   evolution problems, the algorithms are interpreted as numerical time
   integration schemes.}},
