@article{ ISI:000071824800029,
Author = {Peter, M and Varga, Z and Panyi, G and Bene, L and Damjanovich, S and
   Pieri, C and Possani, LD and Gaspar, R},
Title = {{Pandinus imperator scorpion venom blocks voltage-gated K+ channels in
   human lymphocytes}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{1998}},
Volume = {{242}},
Number = {{3}},
Pages = {{621-625}},
Month = {{JAN 26}},
Abstract = {{Using the patch-clamp technique, we determined that Pandinus imperator
   scorpion venom blocked whole-cell n-type K+ currents in human peripheral
   blood lymphocytes in a dose-dependent manner with K-d = 0.02 mu g/ml. K+
   channel block was instantaneous and removable by washing with venom-free
   extracellular solution. The venom-induced block was independent of
   membrane potential. The venom did not influence activation and
   inactivation kinetics of the K+ channels, however, accelerated recovery
   from inactivation, Purified peptides Pi1, Pi2, and Pi3 from the P.
   imperator venom powerfully blocked Kv1.3 channels in human lymphocytes
   with K-d values of 9.7 nM, 50 pM, and 0.5 nM, respectively. Flow
   cytometric membrane potential measurements with the oxonol dye showed
   that Pi2, the most effective peptide toxin of the P. imperator venom,
   depolarizes human lymphocytes in accordance with its K+ channel blocking
   effect. (C) 1998 Academic Press.}},
