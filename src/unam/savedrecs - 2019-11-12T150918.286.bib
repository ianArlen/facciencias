@article{ ISI:000072470700006,
Author = {Fujita, S and Godoy, S},
Title = {{Quantum statistical derivation of the Ginzburg-Landau equation. Energy
   gap, condensed pairon density and penetration depth}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
