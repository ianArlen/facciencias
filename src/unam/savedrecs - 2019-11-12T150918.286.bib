@article{ ISI:000071818800028,
Author = {Mesa, ADS and Quesne, C and Smirnov, YF},
Title = {{Generalized Morse potential: Symmetry and satellite potentials}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{1998}},
Volume = {{31}},
Number = {{1}},
Pages = {{321-335}},
Month = {{JAN 9}},
Abstract = {{We study in detail the bound-state spectrum of the generalized Morse
   potential (GMP), which was proposed by Deng and Fan as a potential
   function for diatomic molecules. By connecting the corresponding
   Schrodinger equation with the Laplace equation on the hyperboloid and
   the Schrodinger equation for the Poschl-Teller potential, we explain the
   exact solvability of the problem by an so(2, 2) symmetry algebra, and
   obtain an explicit realization of the latter as su(l, 1) circle plus
   su(l, 1). We prove that some of the so(2, 2) generators connect among
   themselves wavefunctions belonging to different GMPs (called satellite
   potentials). The conserved quantity is some combination of the potential
   parameters instead of the level energy, as for potential algebras.
   Hence, so(2, 2) belongs to a new class of symmetry algebras. We also
   stress the usefulness of our algebraic results for simplifying the
   calculation of Frank-Condon factors for electromagnetic transitions
   between rovibrational levels based on different electronic states.}},
