@inproceedings{ ISI:000074517400137,
Author = {Savage-Carmona, J and Holden, A and Billinghurst, M},
Editor = {{Cantu, FJ and Soto, R and Liebowitz, J and Sucar, E}},
Title = {{A Virtual Reality mobile robot driven with voice commands}},
Booktitle = {{4TH WORLD CONGRESS OF EXPERT SYSTEMS, VOL 1 AND 2: APPLICATION OF
   ADVANCED INFORMATION TECHNOLOGIES}},
Year = {{1998}},
Pages = {{939-948}},
Note = {{4th World Congress of Expert Systems - Application of Advanced
   Information Technologies, INST TECNOLOG \& ESTUDIOS SUPER MONTERREY,
   MEXICO CITY, MEXICO, MAR 16-20, 1998}},
Organization = {{CEMEX; TV Azteca; ISIS; Mexican Soc Artificial Intelligence; CONACYT
   REDII; Amer Assoc Artificial Intelligence; Inst Technol \& Estudios
   Super Monterrey, Ctr Artificial Intelligence}},
Abstract = {{In this paper we show how symbolic Artificial Intelligence (AI)
   techniques can be used in Virtual Reality (VR) environments. We
   developed a simple application that allows a user to drive a software
   agent, represented by a Virtual Robot, around a Virtual environment with
   voice commands. This software agent is able to accept spoken commands
   from the user, it interprets them using natural language understanding,
   and it interacts and modifies the virtual environment according to them.
   In this paper we describe also how to use context to increase the speech
   recognition accuracy and the Virtual Robot performance. Context plays an
   important role for understanding the information provided in a
   conversation or talk. One way to represent the context is through
   scripts. They describe a possible sequences of events that an actor or
   entity may perform under certain conditions. Using scripts it is easy to
   make inferences about events for which we have incomplete information or
   are fuzzy. Scripts serve to organize knowledge of people in order to
   understand, it encodes the common sense knowledge. In our system we used
   also pronouns to have a broad range of operations without increasing the
   number of sentences to be recognized.}},
