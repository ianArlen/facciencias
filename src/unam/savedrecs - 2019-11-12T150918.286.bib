@inproceedings{ ISI:000077171800036,
Author = {Navarro-Gonzalez, R and Basiuk, VA},
Editor = {{ChelaFlores, J and Raulin, F}},
Title = {{Prebiotic synthesis by lightning in Martian volcanic plumes}},
Booktitle = {{EXOBIOLOGY: MATTER, ENERGY, AND INFORMATION IN THE ORIGIN AND EVOLUTION
   OF LIFE IN THE UNIVERSE}},
Year = {{1998}},
Pages = {{255-260}},
Note = {{50th Trieste Conference on Chemical Evolution - an Abdus Salam Memorial,
   TRIESTE, ITALY, SEP 22-26, 1997}},
Organization = {{Int Ctr Theoret Phys; Int Ctr Genet Engn \& Biotechnol; Commiss European
   Communities, European Commiss DGXII; CNR; UNESCO; European Space Agcy;
   NASA; Weizmann Inst Genome Project; Univ Paris VII; Univ Paris XII; Univ
   Nacl Autonoma Mexico, Inst Ciencias Nucl; Assicurazioni Gen; Univ Studi
   Trieste, Dipartimento Biol}},
Abstract = {{A thermochemical-hydrodynamic model of lightning in explosive volcanic
   eruption columns has been used to assess the potential significance of
   volcanic lightning in the production of key molecules needed for
   chemical evolution and the origins of life in early Mars. Martian
   magmatic gases were assumed to be composed of 40\% H2O, 15\% S, 5\% N,
   and 40\% C in various oxidation states ranging from CH4 to CO2. It is
   predicted that NO is the prime nitrogen containing species formed from
   mildly reducing (O/C = 0.75) to oxidizing (O/C = 2) conditions. NH3 and
   HCN are abundant products under reducing conditions (O/C < 0.25). HSCN
   is only formed in reducing conditions, and HCHO is more efficiently
   produced at O/C less than or equal to 1.5. The chemical composition of
   gases emitted by primitive Martian volcanoes is not known but presumably
   they had a O/C ratio less than or equal to 1. Therefore, volcanic
   lightning may have played an important role in the process of chemical
   evolution in early Mars.}},
