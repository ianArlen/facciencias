@article{ ISI:000078652700003,
Author = {Delgadillo, C},
Title = {{Floristic corridors for moss distribution across the Neovolcanic Belt of
   Mexico. III. The Michoacan corridors}},
Journal = {{JOURNAL OF BRYOLOGY}},
Year = {{1998}},
Volume = {{20}},
Number = {{1}},
Pages = {{27-39}},
Abstract = {{The distribution of mosses from four topographic discontinuities or
   corridors (Tuxpan, Morelia, Patzcuaro and Chapala) suggests that the
   floristic interchange between northern and southern areas across the
   Neovolcanic Belt in western Mexico is feasible. Mosses of various
   phytogeographical affinities are present on either side and in the
   corridors, but certain other taxa are only represented on one side of
   the Belt. The tropical species are virtually absent from northern
   (Plateau) areas indicating a joint barrier effect of the Belt and the
   Balsas Depression. The sources of the corridor moss flora are
   heterogeneous; in addition to diverse phytogeographical affinities, the
   altitudinal distribution shows a close relationship with the upper
   elevations, including the alpine areas. Bryum capillare var. flaccidum,
   Entodon sullivantii and Fabronidium guatemalensis are new records for
   Mexico.}},
