Author = {Estrada, C and Dorantes, R and Hernandez, F and Jimenez, M and Rincon, E},
Editor = {{CampbellHowe, R and Cortez, T and WilkinsCrowder, B}},
Title = {{A parabolic dish concentrator from a telecomunication anttena}},
