@inproceedings{ ISI:000084339400009,
Author = {Moshinsky, M and Sharma, A and Smirnov, YF},
Editor = {{Corney, SP and Delbourgo, R and Jarvis, PD}},
Title = {{Supermultiplets and relativistic problems}},
Booktitle = {{GROUP 22: PROCEEDINGS OF THE XII INTERNATIONAL COLLOQUIUM ON GROUP
   THEORETICAL METHODS IN PHYSICS}},
Year = {{1998}},
Pages = {{75-85}},
Note = {{12th International Colloquium on Group Theoretical Methods in Physics
   (Group 22), HOBART, AUSTRALIA, JUL 13-17, 1998}},
Abstract = {{The concept of multiplet appeared in atomic physics mainly in relation
   with spin. When in nuclear physics one introduces, besides the ordinary
   spin, also the isotopic one, they could be combined and give rise to
   supermultiplets. In this paper we start with the free particle Dirac
   equation and show that its 4x4 matrices gamma(nu), nu = 0, 1, 2, 3, can
   be expressed in terms of direct products of 2x2 matrices associated with
   the ordinary spin and a new concept we call sign spin. The Dirac
   equation can then be reformulated in terms of the generators of the
   SU(4) group that appeared first in the supermultiplets of nuclear
   physics. This reformulation becomes very relevant for the analysis of
   relativistic equations of higher spins and we apply it to the Bhabha
   equation showing, in an elementary way, its invariance under both
   Lorentz transformations as well as under the unitary symplectic subgroup
   Sp(4) of SU(4). As Sp(4) is isomorphic to O(5) we can use the chain O(5)
   superset of O(4) superset of O(3) superset of O(2) to characterize the
   states and get and get the spins and masses associated with irreps
   (n(1)n(2)) of O(5).}},
