@inproceedings{ ISI:000171844700016,
Author = {Tovar, R and Avila, FE and Rojas, J and Vargas, B},
Editor = {{Maron, DM}},
Title = {{Experimental studies on the transient natural convective motion of two
   stratified liquids inside a cavity}},
Booktitle = {{INTERNATIONAL SYMPOSIUM ON LIQUID-LIQUID TWO PHASE FLOW AND TRANSPORT
   PHENOMENA}},
Year = {{1998}},
Pages = {{177-185}},
Note = {{International Symposium on Liquid-Liquid 2 Phase Flow and Transport
   Phenomena, ANTALYA, TURKEY, NOV 03-07, 1997}},
Organization = {{Int Ctr Heat \& Mass Transfer}},
Abstract = {{A two dimensional study on the transient natural convection of two
   stratified miscible liquids inside a cavity is presented. We set out to
   investigate the motion of the fluids when one wall of the cavity is
   quickly heated up at a certain temperature, the stratified liquids being
   initially at rest and in thermal equilibrium. The experiment were done
   at different inclination angles of the hot wall so that its influence on
   the dynamics of the process could be observed. Also, several initial
   concentrations for the denser liquid were tested for each inclination,
   and the global effect of concentration investigated.
   From a global point of view, the experimental flow behaves nearly in the
   same way regardless both the value of the initial concentration and the
   angle of the wall inclination. However, a common trend to all
   experiments was soon evident: the swifter advance of the thermal
   intrusion in the upper layer toward the opposite wall as compared
   against that of the thermal intrusion in the bottom layer.}},
