@inproceedings{ ISI:000076424600052,
Author = {Taboada, VM and Martinez-Carvajal, H and Okamura, M},
Editor = {{Kimura, T and Kusakabe, O and Takemura, J}},
Title = {{Effect of input frequency on the dilative behavior of an infinite slope
   of loose sand}},
Booktitle = {{CENTRIFUGE 98, VOL 1}},
Year = {{1998}},
Pages = {{315-320}},
Note = {{International Conference on Centrifuge, TOKYO, JAPAN, SEP 23-25, 1998}},
Organization = {{Japanese Geotech Soc; Int Soc Soil Mech \& Geotech Engn, Tech Comm TC2
   Centrifuge Testing}},
Abstract = {{Five centrifuge tests were carried out in a laminar box at a centrifuge
   acceleration of 50 g, to study the effect of input frequency on the
   dilative behavior of an infinite slope of loose sand. This dilative
   response typically showed up in the raw data as upslope spikes in the
   acceleration records and simultaneous drops in the piezometric records.
   It was found that as the input frequency decreases this dilative
   response becomes stronger: the dips in the pore pressure, thickness of
   liquefied soil, spikes in the soil acceleration, and permanent lateral
   displacement, all increase. The permanent ground displacement D(H), is
   arrested by this dilative behavior. The recorded D(H) at a frequency of
   1 Hz is 1.5 times larger that at 2 Hz. A sliding block analysis would
   predict that D(H) at a frequency of 1 Hz should be 4 times larger that
   at 2 Hz.}},
