@article{ ISI:000071798400016,
Author = {Salas-Vidal, E and Lomeli, H and Castro-Obregon, S and Cuervo, R and
   Escalante-Alcalde, D and Covarrubias, L},
Title = {{Reactive oxygen species participate in the control of mouse embryonic
   cell death}},
