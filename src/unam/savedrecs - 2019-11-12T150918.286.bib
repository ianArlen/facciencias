@article{ ISI:000071263600009,
Author = {Sarquis, JI and Gonzalez, H and Dunlap, JR},
Title = {{Yield response of two cycles of selection from a semiprolific early
   maize (Zea mays L.) population to plant density, sucrose infusion and
   pollination control}},
Journal = {{FIELD CROPS RESEARCH}},
Year = {{1998}},
Volume = {{55}},
Number = {{1-2}},
Pages = {{109-116}},
Month = {{JAN}},
Abstract = {{Previous studies have demonstrated greater yield potential for prolific
   than for nonprolific maize cultivars. Although the number of ears per
   plant has been used as selection criterion by plant breeders for yield
   improvement, second ear growth has been shown to vary strongly with
   plant density, and the nature of this effect remains unclear. The
   objective of this study was to investigate the yield response of two
   cycles of selection from a semiprolific early maize population differing
   in prolificacy index, to high plant density, controlled timing of
   pollination, and to application of exogenous sugar in order to find
   evidence of a possible source limitation on the growth of a second ear
   in a prolific maize plant. No significant differences were observed in
   sucrose phosphate synthase (SPS) activity between prolific (C(6)) and
   nonprolific (C(0)) plants, although it was slightly higher in leaves of
   C(0). Yield per plant in C(6) exceeded that of C(0) by 37 and 59\% at
   low and high density, respectively. Plant yield reduction in C6( )at
   high density was associated with reduction of second-ear yield.
   Preliminary data indicates that yield of second ears was source-limited,
   while that of apical and single ears on Cg and C,, respectively, seemed
   to be sink-limited. Prolificacy seems to be a highly elastic component
   of maize yield that results in greater stability of yield of the apical
   ear, and which can constitute a significant proportion of total yield
   per plant. (C) 1998 Elsevier Science B.V.}},
