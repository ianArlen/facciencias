@article{ ISI:000278238400004,
Author = {Alva-Valdivia, L. M. and Morales, J. and Goguitchaichvili, A. and
   Popenoe de Hatch, M. and Hernandez-Bernal, M. S. and Mariano-Matias, F.},
Title = {{Absolute geomagnetic intensity data from preclassic Guatemalan pottery}},
