@article{ ISI:000277744200026,
Author = {Pena, Fernando and Lourenco, Paulo B. and Mendes, Nuno and Oliveira,
   Daniel V.},
Title = {{Numerical models for the seismic assessment of an old masonry tower}},
Journal = {{ENGINEERING STRUCTURES}},
Year = {{2010}},
Volume = {{32}},
Number = {{5}},
Pages = {{1466-1478}},
Month = {{MAY}},
Abstract = {{The present paper describes the seismic assessment of the Qutb Minar in
   Delhi, India. Three models with different levels of complexity and
   simplification were developed. The use of these models allows one to
   overcome the complexity of the study of the seismic behavior of ancient
   masonry structures; by combining the results of the different models it
   is possible to obtain a better and more comprehensive interpretation of
   the seismic behavior. The models were used for non-linear static
   (pushover) and non-linear dynamic analyses. The static and dynamic
   analyses give different behaviors, indicating that the push-over
   analysis should be used carefully in the seismic assessment of masonry
   structures. For the static analysis, the base of the tower is the most
   vulnerable part; while according to the dynamic analysis, it is the
   upper part of the tower. This last behavior is according to the
   historical damage suffered by the tower due to earthquakes. The
   different behaviors can be explained by the influence of the higher
   modes of vibration. (C) 2010 Elsevier Ltd. All rights reserved.}},
