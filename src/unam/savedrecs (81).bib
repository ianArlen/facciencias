@article{ ISI:000275741400007,
Author = {Vazquez-Lopez, J. A. and Lopez-Juarez, I. and Pena-Cabrera, M.},
Title = {{On the Use of the FuzzyARTMAP Neural Network for Pattern Recognition in
   Statistical Process Control using a Factorial Design}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTERS COMMUNICATIONS \& CONTROL}},
Year = {{2010}},
Volume = {{5}},
Number = {{2}},
Pages = {{205-215}},
Month = {{JUN}},
Abstract = {{Time-series statistical pattern recognition is of prime importance in
   statistics, especially in quality control techniques for manufacturing
   processes. A frequent problem in this application is the complexity when
   trying to determine the behaviour (pattern) from sample data. There have
   been identified standard patterns which are commonly present when using
   the X chart; its detection depends on human judgement supported by norms
   and graphical criteria. In the last few years, it has been demonstrated
   that Artificial Neural Networks (ANN's) are useful to predict the type
   of time-series pattern instead of the use of rules. However, the ANN
   control parameters have to be fixed to values that maximize its
   performance. This research proposes an experimental design methodology
   to determine the most appropriate values for the control parameters of
   the FuzzyARTMAP ANN such as: learning rate (beta) and network vigilance
   (rho(a), rho(b), rho(ab)) in order to increment the neural network
   efficiency during unnatural pattern recognition.}},
