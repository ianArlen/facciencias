@article{ ISI:000279656500010,
Author = {Sanchez-Torres, L. E. and Torres-Martinez, J. A. and Godinez-Victoria,
   M. and Omar, J. -M. and Velasco-Bejarano, B.},
Title = {{Perezone and its isomer isoperezone induce caspase-dependent and
   caspase-independent cell death}},
Journal = {{PHYTOMEDICINE}},
Year = {{2010}},
Volume = {{17}},
Number = {{8-9}},
Pages = {{614-620}},
Month = {{JUL}},
Abstract = {{In this study, we investigated the cytotoxic effect of perezone, a
   constituent isolated from the roots of Perezia spp. and of its synthetic
   isomer isoperezone on the 1662 human leukemia cell line. Perezone showed
   greater cytotoxic effect than isoperezone but both compounds were found
   to induce cytotoxicity trough a caspase-dependent and a
   caspase-independent mechanisms; important changes in their light
   scattering properties, phosphatidylserine translocation and
   mitochondrial membrane potential disruption were detected by cytometry.
