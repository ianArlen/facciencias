@article{ ISI:000277086300008,
Author = {Hernandez-Vazquez, Liliana and Mangas, Susana and Palazon, Javier and
   Navarro-Ocana, Arturo},
Title = {{Valuable medicinal plants and resins: Commercial phytochemicals with
   bioactive properties}},
Journal = {{INDUSTRIAL CROPS AND PRODUCTS}},
Year = {{2010}},
Volume = {{31}},
Number = {{3}},
Pages = {{476-480}},
Month = {{MAY}},
Abstract = {{A simple, fast and efficient method was developed for the isolation of
   phytochemicals with bioactive properties from valuable resins and
   medicinal plants. The method involves rapid qualitative detection of
   triterpenes by thin-layer chromatography (TLC), efficient extraction
   with n-hexane, quantitative analysis by HPLC and preparative separation
   (preparative TLC and crystallization) to obtain high-purity alpha-amyrin
   (98.5\%), 3-epi-lupeol (98.0\%) and beta-amyrin (82.0\%). The presence
   of alpha- and beta-amyrin, or a mixture of both, was detected by TLC on
   silica gel in all the sources investigated, while 3-epi-lupeol was only
   found in the Mexican Copal resins. The results demonstrated that HPLC is
   a selective method for the analysis of alpha- and beta-amyrin and
   3-epi-lupeol, and that the best source of alpha-amyrin was Mexican Copal
   Tepoztlan, that of beta-amyrin was Nance wastes and that of 3-epi-lupeol
   was Mexican Copal Sonora. (C) 2010 Elsevier B.V. All rights reserved.}},
