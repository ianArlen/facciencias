@article{ ISI:000279858400003,
Author = {Nunez-Gonzalez, Roberto and Reyes-Serrato, Armando and Galvan, Donald H.
   and Posada-Amarillas, Alvaro},
Title = {{DFT calculation of the electronic properties and EEL spectrum of NiSi2}},
Journal = {{COMPUTATIONAL MATERIALS SCIENCE}},
