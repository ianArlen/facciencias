@article{ ISI:000278519700006,
Author = {Estrada, R. and Rodriguez, R. and Castano, V. M.},
Title = {{Smart polymeric membranes: pH-induced non-linear changes in pore size}},
Journal = {{APPLIED PHYSICS A-MATERIALS SCIENCE \& PROCESSING}},
Year = {{2010}},
Volume = {{99}},
Number = {{4}},
Pages = {{723-728}},
Month = {{JUN}},
Abstract = {{A new kind of synthetic membrane was designed with the capacity to
   adjust the pore diameter in a wide range of sizes (from 0.8 to 22.3
   microns) just by changing the pH of the aqueous medium where it is
   immersed; in this way it is possible to control externally the pore size
   of the membrane. A non-linear mathematical model, based on the overlap
   of the electrical double layers of charged particles, was developed to
   explain the oscillatory behavior of the pore size when the pH was
   varied. This model fits correctly this oscillatory behavior. Scanning
   electron microscope images were obtained in low-vacuum conditions to
   reduce, as much as possible, possible changes in the external conditions
   after the immersion in water.}},
