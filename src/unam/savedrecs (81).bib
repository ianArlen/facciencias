@article{ ISI:000275746100006,
Author = {Suarez, J. R. and Vallejo, E. and Navarro, O. and Avignon, M.},
Title = {{Magneto-elastic phase transition in a linear chain within the double and
   super-exchange model}},
Journal = {{JOURNAL OF MAGNETISM AND MAGNETIC MATERIALS}},
