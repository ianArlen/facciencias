@article{ ISI:000278712700004,
Author = {Hermoso-Salazar, Margarita and Solis-Weiss, Vivianne},
Title = {{Distribution and morphological variation of Synalpheus superus Abele and
   Kim, 1989 and notes on the distribution of S. fritzmuelleri Coutiere,
   1909 (Decapoda: Caridea: Alpheidae)}},
Journal = {{ZOOTAXA}},
Year = {{2010}},
Number = {{2505}},
Pages = {{65-68}},
Month = {{JUN 14}},
Abstract = {{A significant intraspecific morphological variation was observed in the
   eastern Pacific snapping shrimp Synalpheus superus Kim \& Abele, 1989,
   especially in the proportions of some cephalotoracic appendages. The
   northern limit of the distribution of S. superus is expanded from Panama
   to Guerrero, Mexico. We also consider that Synalpheus
   fritzmuelleriCoutiere, 1909 is most probably not an amphi-American
   species. Its former status as such was due to taxonomic
   misinterpretations by Hernandez Aguilera et al. (1986) and probably by
   Coutiere (1909) himself, although his questionable record of a single
   specimen of S. fritzmuelleri from the Gulf of California could not be
   verified, because the specimen could not be located and may be lost.}},
