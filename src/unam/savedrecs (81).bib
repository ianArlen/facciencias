@article{ ISI:000277982800006,
Author = {Juarez, A. M. and de Urquijo, J. and Hinojosa, G. and Hernandez-Avila,
   J. L. and Basurto, E.},
Title = {{Relaxation effects in ionic mobility and cluster formation: negative
   ions in SF6 at high pressures}},
Journal = {{PLASMA SOURCES SCIENCE \& TECHNOLOGY}},
Year = {{2010}},
Volume = {{19}},
Number = {{3}},
Month = {{JUN}},
