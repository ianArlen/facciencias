Author = {Lopez-Esquivel Kranksith, L. and Negron-Mendoza, A. and Mosqueira, F. G.
   and Ramos-Bernal, Sergio},
Title = {{Radiation-induced reactions of amino acids adsorbed on solid surfaces}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
