@article{ ISI:000278347000001,
Author = {Saldivar-Guerra, Enrique and Infante-Martinez, Ramiro and Vivaldo-Lima,
   Eduardo and Flores-Tlacuahuac, Antonio},
Title = {{Returning to Basics: Direct Integration of the Full Molecular-Weight
   Distribution Equations in Addition Polymerization}},
Journal = {{MACROMOLECULAR THEORY AND SIMULATIONS}},
Year = {{2010}},
Volume = {{19}},
Number = {{4}},
Pages = {{151-157}},
Month = {{MAY 21}},
Abstract = {{Direct integration of the equations describing the full MWD in addition
   polymerization is shown to be feasible in reasonable computing times
   using standard computers for a considerable number of realistic systems.
   This is possible if the stiffness of the system is removed (if present)
   by the application of the OSSA, and the use of suitable numerical
   methods. This communication provides the basic ideas of a general
   method, based on the chemical nature of the system and the structure of
   the MWD equations. The application of the method provides additional
   insight on the dynamics and the underlying chemistry of the system.
   Examples of free radical polymerizations including controlled radical
   polymerization are offered, which can be solved in less than an hour on
   a PC.}},
