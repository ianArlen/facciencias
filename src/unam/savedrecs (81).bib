@article{ ISI:000279004600010,
Author = {Mendez-Martinez, E. F. and Reyes, P. G. and Osorio-Gonzalez, D. and
   Castillo, F. and Martinez, H.},
Title = {{Langmuir Probe and Optical Emission Spectroscopy Studies of Low-Pressure
   Gas Mixture of CO2 and N-2}},
