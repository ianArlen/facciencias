@article{ ISI:000276294800004,
Author = {De La Torre, Luis and Langer, Jacob C.},
Title = {{Transanal endorectal pull-through for Hirschsprung disease: technique,
   controversies, pearls, pitfalls, and an organized approach to the
   management of postoperative obstructive symptoms}},
Journal = {{SEMINARS IN PEDIATRIC SURGERY}},
