@article{ ISI:000281832200006,
Author = {Zamora Quesada, Miguel Angel and Jeny, Aguilar A. and Sumano Lopez,
   Hector},
Title = {{Clinical efficacy of parenteral vs ophthalmic florfenicol for the
   treatment of infectious bovine keratoconjunctivitis}},
Journal = {{VETERINARIA MEXICO}},
Year = {{2010}},
Volume = {{41}},
Number = {{3}},
Pages = {{219-225}},
Month = {{JUL-SEP}},
Abstract = {{The clinical and bacteriological efficacy, the quantity of mg and
   florfenicol treatment length for infectious bovine keratitis (Moraxella
   bovis) was assessed using either the parenteral administration of
   florfenicol (PF), or the ophthalmic administration of the drug in spray
   (OF). Sixty four cows and heifers were randomly divided into the two
   referred treatments. All animals were cured, however, the OF group
   required a mean of 378 mg/bovine and 12 days of treatment, while the PF
   group needed 22 800 mg/bovine (3.8 treatments per bovine) and 7.6 days.
   Although 100\% efficacy was obtained in both groups cost-benefit ratio
   in the OF group is pondered}},
