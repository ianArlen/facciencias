@article{ ISI:000277860300007,
Author = {Pellicer, Daniel},
Title = {{Extensions of dually bipartite regular polytopes}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2010}},
Volume = {{310}},
Number = {{12, SI}},
Pages = {{1702-1707}},
Month = {{JUN 28}},
Abstract = {{This paper addresses the problem of finding abstract regular polytopes
   with preassigned facets and preassigned last entry of the Schlafli
   symbol. Using C-group permutation representation (CPR) graphs we give a
   solution to this problem for dually bipartite regular polytopes when the
   last entry of the Schlafli symbol is even. This construction is related
   to a previous construction by Schulte that solves the problem when the
   entry of the Schlafli symbol is 6. (C) 2009 Elsevier B.V. All rights
   reserved.}},
