@article{ ISI:000280054600004,
Author = {Loser, Hannes},
Title = {{REVISION OF THE EARLY CRETACEOUS CORAL GENUS FELIXIGYRA}},
Journal = {{RIVISTA ITALIANA DI PALEONTOLOGIA E STRATIGRAFIA}},
Year = {{2010}},
Volume = {{116}},
Number = {{2}},
Pages = {{189-200}},
Month = {{JUL}},
Abstract = {{The Early Cretaceous coral genus Felixigyra Prever, 1909 is revised on
   the basis of type material from Italy Felixigyra has a
   hydnophoroid-meandroid colony organisation with conical monticules
   attached to each other The very duck monticules are arranged in a way
   that calicular centres become apparent The septa are compact and
   rhopaloid The genus can be related to other genera of the Eugvridae
   family, but differs from them by as particularly developed monticules It
   also shows certain resemblance to meandroid genera of the
   Trochoidomeandridae family Of the six species originally assigned to
   Felixigyra only five are recognized, since the type of Felixigyra crassa
   is too poorly preserved to give a diagnosis The remaining five species
   have almost no significant dad dice in calicular dimensions In addition
   to the Italian material, one sample from the Early Cenomanian of Greece
   and one sample from the Early Albian of Mexico are also assigned to the
   genus Material assigned to Felixigyra after Prever (1909) needs to be
   mutely reclassified to the genus Eo-hydriophora}},
