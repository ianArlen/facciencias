@article{ ISI:000277961600006,
Author = {Calzada, Fernando and Yepez-Mulia, Lilian and Tapia-Contreras, Amparo
   and Bautista, Elihu and Maldonado, Emma and Ortega, Alfredo},
Title = {{Evaluation of the Antiprotozoal Activity of Neo-clerodane Type
   Diterpenes from Salvia polystachya against Entamoeba histolytica and
   Giardia lamblia}},
Journal = {{PHYTOTHERAPY RESEARCH}},
Year = {{2010}},
Volume = {{24}},
Number = {{5}},
Pages = {{662-665}},
Month = {{MAY}},
Abstract = {{Chia (Salvia polystachya Ort., Lamiaceae) is frequently used in Mexican
   traditional medicine to treat dysentery. In this study the main
   neo-clerodane diterpenes (polystachynes A, B and D, as well as
   linearolactone) were isolated from the aerial parts of chia, and their
   antiprotozoal activities toward Entamoeba histolytica and Giardia
   lamblia trophozoites were evaluated in vitro. Linearolactone was the
   most potent antiamoebic and antigiardial compound with IC(50) values of
   22.9 mu M for E. histolytica and 28.2 mu M for G. lamblia. Polystachynes
   A, B and D, showed moderate antiprotozoal activity against both
   protozoans with IC(50) values ranging from 1.17.0 to 160.6 mu M for E.
   histolytica and from 107.5 to 134.7 mu M for G. lamblia. These data
   suggest that linearolactone may play an important role in the
   antidiarrhoeal activity of S. polystachya. Copyright (C) 2009 John Wiley
   \& Sons, Ltd.}},
