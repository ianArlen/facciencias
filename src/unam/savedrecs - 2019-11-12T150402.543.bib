@article{ ISI:000231220700004,
Author = {Thome, PE},
Title = {{Heterologous expression of glycerol 3-phosphate dehydrogenase gene
   {[}DhGPD1] from the osmotolerant yeast Debaryomyces hansenii in
   Saccharomyces cerevisiae}},
Journal = {{CURRENT MICROBIOLOGY}},
Year = {{2005}},
Volume = {{51}},
Number = {{2}},
Pages = {{87-90}},
Month = {{AUG}},
Abstract = {{The role for the gene encoding glycerol 3-phosphate dehydrogenase
   (DhGPD1) from the osmotolerant yeast Debaryomyces hansenii, in glycerol
   production and halotolerance, was studied through its heterologous
   expression in a Saccharomyces cerevisiae strain deficient in glycerol
   synthesis (gpd1 Delta). The expression of the DhGPD1 gene in the gpd1
   Delta background restored glycerol production and halotolerance to wild
   type levels, corroborating its role in the salt-induced production of
   glycerol. Although the gene was functional in S. cerevisiae, its
   heterologous expression was not efficient, suggesting that the
   regulatory mechanism may not be shared by these two yeasts.}},
