@article{ ISI:000231993900008,
Author = {Salinas-Melgoza, A and Renton, K},
Title = {{Seasonal variation in activity patterns of juvenile Lilac-crowned
   Parrots in tropical dry forest}},
Journal = {{WILSON BULLETIN}},
Year = {{2005}},
Volume = {{117}},
Number = {{3}},
Pages = {{291-295}},
Month = {{SEP}},
Abstract = {{We used radio-telemetry techniques to determine hourly activity patterns
   of 29 juvenile Lilac-crowned Parrots (Amazona finschi) during 1996-2000
   in tropical dry forest of Jalisco, Mexico. Parrots had two peak activity
   periods-early morning and late afternoon-for both overall activity and
   local movement. Individuals were generally inactive and did not change
   location for 5-6 hr during the middle of the day. Parrots were more
   active in the dry season than in the rainy season, although movements
   resulting in a change of location did not vary between seasons. Seasonal
   variations in activity of Lilac-crowned Parrots may be related to
   variations in food availability or temperature. Activity patterns of
   parrots need to be considered when evaluating habitat use or survey
   data.}},
