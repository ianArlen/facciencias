@article{ ISI:000231969800003,
Author = {Hernandez, T and Canales, M and Caballero, J and Duran, A and Lira, R},
Title = {{Quantitative analysis of traditional knowledge about plants used in the
   treatment of gastrointestinal diseases among the inhabitants of
   Zapotitlan Salinas, Puebla, Mexico}},
Journal = {{INTERCIENCIA}},
Year = {{2005}},
Volume = {{30}},
Number = {{9}},
Pages = {{529+}},
Month = {{SEP}},
Abstract = {{The distribution of traditional knowledge about plants used in the
   treatment of gastrointestinal diseases among the inhabitants of
   Zapotitlan Salinas, Puebla, Mexico, was evaluated. The study was
   performed using ethnobotanical methods applied to 119 representative
   persons of the community. A total of 44 species were recorded as used
   for the treatment of gastrointestinal diseases. The species with highest
   values of relative importance, most frequently mentioned in the
   interviews, were ``Oregano{''} (Lippia graveolens Knuth), ``Cinco
