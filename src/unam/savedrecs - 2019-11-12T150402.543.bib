@article{ ISI:000231920800025,
Author = {Torres-Garcia, E and Rodriguez-Gattorno, G and Ascencio, JA and
   Aleman-Vazquez, LO and Cano-Dominguez, JL and Martinez-Hernandez, A and
   Santiago-Jacinto, P},
Title = {{New insights on molybdenum suboxide: Nature of carbons in isomerization
   reactions}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY B}},
Year = {{2005}},
Volume = {{109}},
Number = {{37}},
Pages = {{17518-17525}},
Month = {{SEP 22}},
Abstract = {{MoO3 transformations under isomerization process conditions were
   studied. The products obtained after different times under stream
   (H-2/n-heptane mixture, 18.5 bar, at 370 degrees C) were characterized
   by X-ray diffraction, Raman spectroscopy, thermal analysis, and
   high-resolution transmission electron microscopy (HRTEM). Theoretical
   quantum calculations were carried out with the aim of understanding the
   paradox of the real active phase in isomerization reactions. Theoretical
   calculations predict the existence of a metallic-like MoO phase with a
   structure that matches the X-ray diffraction experimental results. From
   experimental and simulated HRTEM images it was possible to identify the
   presence of small MoO cubic crystallites inside MoOx matrix phases.
   These results also support the previously proposed idea that
   isomerization reactions take place as a result of the existence of a
   bifunctional catalyst. The Raman and thermo-programmed oxidation (TPO)
   analyses show the existence of at least two types of carbonaceous
   deposits which tend to increase its ordering with the increase of time
   under stream. The carbon K edge in electron energy loss spectroscopy
   (EELS) of a sample after 24 h under stream shows that these carbonaceous
   deposits consist of a mixture of sp(2)- and sp(3)-hybridized carbons.}},
