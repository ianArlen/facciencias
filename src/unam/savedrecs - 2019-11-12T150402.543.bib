@article{ ISI:000230288400006,
Author = {de la Rosa, GZ and Venegas-Vega, CA and Monroy, N and Contreras-Bucio, G
   and Friedrich, U and Houman, M and Saad, A and Fernandez, P and
   Kofman-Alfaro, S and Cervantes, A},
Title = {{Trisomy 3q25.1-qter and monosomy 8p23.1-pter in a patient: Cytogenetic
   and molecular analysis with delineation of the phenotype}},
Journal = {{AMERICAN JOURNAL OF MEDICAL GENETICS PART A}},
Year = {{2005}},
Volume = {{136A}},
Number = {{3}},
Pages = {{259-264}},
Month = {{JUL 30}},
Abstract = {{We describe a 4-year-old boy with partial 3q trisomy and distal 8p
   monosomy. The patient presented with mental retardation, dysmorphic
   face, congenital heart defect, brain and genital anomalies, and
   behavioral problems. The conventional cytogenetic analysis showed a
   46,XY,add(8p) karyotype. Reverse painting and microsatellite analysis
   demonstrated a partial monosomy of 8p23.1 -> pter and a partial trisomy
   of 3q25.1 -> qter. The data suggest that the chromosomal rearrangement
