@article{ ISI:000231920800024,
Author = {Gonzalez, AL and Noguez, C and Ortiz, GP and Rodriguez-Gattorno, G},
Title = {{Optical absorbance of colloidal suspensions of silver polyhedral
   nanoparticles}},
