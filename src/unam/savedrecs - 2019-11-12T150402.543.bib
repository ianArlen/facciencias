@article{ ISI:000231192900004,
Author = {Estrada-Mondaca, S and Delgado-Bustos, LA and Ramirez, OT},
Title = {{Mannosamine supplementation extends the N-acetylglucosaminylation of
   recombinant human secreted alkaline phosphatase produced in Trichoplusia
   ni (cabbage looper) insect cell cultures}},
Journal = {{BIOTECHNOLOGY AND APPLIED BIOCHEMISTRY}},
Year = {{2005}},
Volume = {{42}},
Number = {{1}},
Pages = {{25-34}},
Month = {{AUG}},
Abstract = {{A major limitation of the insect cell-baculovirus expression vector
   system is its poor capacity to perform complex glycosylation, since the
   glycoproteins produced are usually only of the high-mannose and
   paucimannose types. Nonetheless, recent evidence indicates that, under
   various conditions, some insect cell lines are capable of producing
   complex-type glycans. In the present study, we assessed the effects of
   supplementation with ManN (mannosamine) ManNAc (N-acetyl-mannosamine)
   and Cyt (cytidine) on the glycosylation of recombinant human secreted
   alkaline phosphatase produced by suspension cultures of Trichoplusia ni
   (cabbage looper) BTI-Tn5BI-4 cells. Addition of ManN in the range 5-20
   mM resulted in a 10-fold increase of the terminal GlcNAc
   (N-acetylglucosamine) associated with the recombinant protein produced
   after baculovirus infection. Such an increase yielded a maximum of
   12.5\% hybrid glycans having terminal GIcNAc: with respect to total
   Winked glycans. In contrast, no changes in the glycan composition
   associated with recombinant human secreted alkaline phosphatase were
   observed on supplementation with up to 20 mM ManNAc or up to 1.5 mM Cyt.
   N-acetyl-beta-glucosaminidase activity in cell extracts was decreased on
   incubation with 20 mM ManN, but not with 20 mM ManNAc or 1.5 mM Cyt,
   indicating that the increased proportion of hybrid glycans obtained on
   the addition of ManN could be a result of N-acetyl-beta-glucosaminidase
   inhibition.}},
