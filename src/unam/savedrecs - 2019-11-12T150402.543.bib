@article{ ISI:000230671100007,
Author = {Valderrama, B and Vazquez-Duhalt, R},
Title = {{Electron-balance during the oxidative self-inactivation of cytochrome c}},
Journal = {{JOURNAL OF MOLECULAR CATALYSIS B-ENZYMATIC}},
Year = {{2005}},
Volume = {{35}},
Number = {{1-3}},
Pages = {{41-44}},
Month = {{AUG 1}},
Abstract = {{During the oxidative self-inactivation of cytochrome c, four molecular
   events were detected and kinetically characterized; heme destruction,
   loss Of the iron-sulfur coordination, protein multimerization, and
   protein inactivation. Electron transfer balance from the four molecular
   events account for 100\% of the electrons transferred in the first
   minute of protein incubation with hydrogen peroxide. However, an
   unidentified electron source started donating electrons after the first
   minute and reaches 23\% of the transferred electrons after 10 min
   incubation. Thus, the electron transfer kinetics and its balance allowed
   us to detect a non-considered electron transfer and free radical
   formation. (c) 2005 Elsevier B.V. All rights reserved.}},
