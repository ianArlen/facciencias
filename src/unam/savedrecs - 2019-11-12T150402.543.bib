@article{ ISI:000231803500025,
Author = {Garcia-Rojas, J and Esteban, C and Peimbert, A and Peimbert, M and
   Rodriguez, M and Ruiz, MT},
Title = {{Deep echelle spectrophotometry of S 311, a Galactic H II region located
   outside the solar circle}},
