@article{ ISI:000231564200132,
Author = {Turbiner, AV and Vieyra, JCL and Guevara, NL},
Title = {{Exotic H-3(2+) ion in a strong magnetic field: Linear configuration}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2005}},
Volume = {{72}},
Number = {{2}},
Month = {{AUG}},
Abstract = {{An accurate study of the lowest 1 sigma(g) and the low-lying excited 1
   sigma(u),1 pi(u,g),1 delta(g,u) electronic states of the exotic
   molecular ion H-3(2+) in linear configuration parallel to a magnetic
   field is carried out. The magnetic field ranges from 10(10) G up to
   4.414x10(13) G, where nonrelativistic considerations are justified. The
   variational method is exploited and the same trial function is used for
   different magnetic fields. It is shown that the states of positive z
   parity 1 sigma(g),1 pi(u),1 delta(g) are bound states of the H-3(2+)
   exotic ion: the state 1 sigma(g) exists for magnetic fields B greater
   than or similar to 10(10) G, and the states 1 pi(u),1 delta(g) exist for
   B greater than or similar to 2.35x10(10) G. It is demonstrated that for
   magnetic fields B greater than or similar to 2.35x10(12) G the potential
   energy surface well corresponding to the 1 sigma(g) state contains at
   least one longitudinal vibrational state. It is shown that the negative
   z-parity states 1 sigma(u),1 pi(g),1 delta(u) are pure repulsive in the
   whole range of magnetic fields studied, B=10(10)-4.414x10(13) G.}},
