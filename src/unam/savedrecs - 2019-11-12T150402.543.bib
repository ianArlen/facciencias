@article{ ISI:000232938300010,
Author = {Martinez, J and Meneses, C and Zapata, JA},
Title = {{Geometry of C-flat connections, coarse graining and the continuum limit}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2005}},
Volume = {{46}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{A notion of effective gauge fields which does not involve a background
   metric is introduced. The role of scale is played by cellular
   decompositions of the base manifold. Once a cellular decomposition is
   chosen, the corresponding space of effective gauge fields is the space
   of flat connections with singularities on its codimension two skeleton,
   A(C-flat)/G (\$) over bar (M,star)subset of A (\$) over bar (M)/G (\$)
   over bar (M,star). If cellular decomposition C-2 is finer than cellular
   decomposition C-1, there is a coarse graining map pi(C2)->
   C-1:A(C2)-flat/G (\$) over bar (M,star)-> A(C1)-flat/G (\$) over bar
   (M,star). We prove that the triple (A(C2)-flat/G (\$) over bar
   (M,star),pi(C2)-> C-1,A(C1)-flat/G (\$) over bar (M,star)) is a
   principal fiber bundle with a preferred global section given by the
   natural inclusion map i(C1)-> C-2:A(C1)-flat/G (\$) over bar (M,star)->
   A(C2)-flat/G (\$) over bar (M,star). Since the spaces A(C-flat)/G (\$)
   over bar (M,star) are partially ordered (by inclusion) and this order is
   directed in the direction of refinement, we can define a continuum
   limit, C -> M. We prove that, in an appropriate sense, lim(C -> M)
   A(C-flat)/G (\$) over bar (M,star)=A (\$) over bar (M)/G (\$) over bar
   (M,star). We also define a construction of measures in A (\$) over bar
   (M)/G (\$) over bar (M,star) as the continuum limit (not a projective
   limit) of effective measures. (c) 2005 American Institute of Physics.}},
