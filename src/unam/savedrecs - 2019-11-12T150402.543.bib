@article{ ISI:000231740800007,
Author = {Alvarez, L and Xiao, MF},
Title = {{Resolution dependence of scanning near field optical microscopy on probe
   sample distance}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2005}},
Volume = {{47}},
Number = {{1}},
Pages = {{S24-S29}},
Month = {{AUG}},
Note = {{8th International Conference on Near-Field Nano Optic and Related
   Techniques, Seoul, SOUTH KOREA, SEP 05-09, 2004}},
Organization = {{Seoul Natl Univ, BK21 Phys Res; Korean Minist Sci \& Technol; Korea Inst
   Sci \& Technol Evaluat \& Planning; Korea Sci \& Engn Fdn; Korea Natl
   Tourism Org; Ambassade France Coree; Samsung Elect Co; LG Elect Co;
   Kwangju Inst Sci \& Technol; Korea Res Inst Chem Technol; Korea Res Inst
   Stand \& Sci; NAMIL Elect Instrument; Wooyang Trading Co; PSIA Corp; JPK
   Instruments; Sangki Trading Co Ltd}},
Abstract = {{In scanning near-field optical microscopy (SNOM) a probe tip scans the
   sample surface in nanometric distances. In such short distances, the
   probe tip interacts mainly with the evanescent field where diffraction
   is exempted. Therefore, SNOM may obtain resolution much beyond the
   diffraction Abbe-Rayleigh limit. In SNOM, one of the most critical
   parameters is the probe-sample distance. Specifically, the resolution of
   the microscope varies dramatically with distance. In the present work,
   we have studied numerically the resolution dependence of collecting mode
   SNOM on the probe-sample distance. The calculations were based on the
   direct moment method (DMM). Our attention was concentrated particularly
   on the influence of the probe-sample distance to the resolution; the
   distance between probe and sample has proved to be critical. In the
   calculations the probe tip was modeled as a sub-wavelength aperture in a
   perfectly conducting wall attached to a lossless step-index cylindrical
   dielectric waveguide. Within the above framework, we arc able to discuss
   the resolution dependence on the probe-sample distance, as well as its
   relation to other parameters such as permittivity of the core and
   cladding, core radius and aperture radius.}},
