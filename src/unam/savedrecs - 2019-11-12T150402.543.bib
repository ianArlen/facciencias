@article{ ISI:000231565000022,
Author = {de la Macorra, A},
Title = {{Realistic particle physics dark energy model}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2005}},
Volume = {{72}},
Number = {{4}},
Month = {{AUG}},
Abstract = {{We present a realistic dark energy model derived from particle physics.
   Our model has no free parameters and has an equivalent fit to the
   observational data (CMB, SN1a and LSS) as cosmological constant model
   (LCDM) and a better fit than the best effective w(z) model. The only
   free parameter is the number of extra particles, besides the particles
   in the standard model and our dark energy model, contained in the
   Universe. With the lack of a clear determination of the cosmological
   parameters theoretical considerations should be taken seriously to
   distinguish between dark energy models.}},
