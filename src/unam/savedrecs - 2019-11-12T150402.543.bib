@article{ ISI:000230798500006,
Author = {Guzman-Ribcon, J and Delfin-Loya, A and Urena-Nunez, E and Paredes, LC
   and Zambrano-Achirica, E and Graf, U},
Title = {{Genotoxicity of neutrons in Drosophila melanogaster. Somatic mutation
   and recombination induced by reactor neutrons}},
Journal = {{RADIATION RESEARCH}},
Year = {{2005}},
Volume = {{164}},
Number = {{2}},
Pages = {{157-162}},
Month = {{AUG}},
Abstract = {{This paper describes the observation of a direct relationship between
   the absorbed doses of neutrons and the frequencies of somatic mutation
   and recombination using the wing somatic mutation and recombination test
   (SMART) of Drosophi1a nielanogaster. This test was used for evaluating
   the biological effects induced by neutrons from the Triga Mark III
   reactor of Mexico. Two different reactor power levels were used, 300 and
   1000 kW, and two absorbed doses were tested for each power level: 1.6
   and 3.2 Gy for 300 kW and 0.84 and 1.7 Gy for 1000 kW. A linear
   relationship was observed between the absorbed dose and the somatic
   mutation and recombination frequencies. Furthermore, these frequencies
   were dependent on larval age: In 96-h-old larvae, the frequencies were
   increased considerably but the sizes of the spots were smaller than in
   72-h-old larvae. The analysis of the balancer-heterozygous progeny
   showed a linear absorbed doseresponse relationship, although the
   responses were clearly lower than found in the marker-trans-heterozygous
   flies. Approximately 65\% of the genotoxicity observed is due to
   recombinational events. The results of the study indicate that thermal
   and fast neutrons are both mutagenic and recombinagenic in the D.
   inelanogaster wing SMART, and that the frequencies are dependent on
   neutron dose, reactor power, and the age of the treated larvae. (c) 2005
   by Radiation Research society.}},
