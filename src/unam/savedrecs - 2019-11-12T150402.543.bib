@article{ ISI:000230698100052,
Author = {Patrykiejew, A and Sokolowski, S and Pizio, O},
Title = {{On the separation of nonadditive symmetric mixtures in nanoscopic
   slitlike pores: A simple model for racemic fluids}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY B}},
Year = {{2005}},
Volume = {{109}},
Number = {{29}},
Pages = {{14227-14234}},
Month = {{JUL 28}},
Abstract = {{A grand canonical ensemble Monte Carlo simulation method is used to
   study the adsorption of nonadditive symmetric mixtures of Lennard-Jones
   spherical particles in nanoscopic slitlike pores. The walls of the pore
   are assumed to be formed by the parallel (100) planes of the model face
   centered cubic crystal of adjustable corrugation potential. It is
   demonstrated that depending on the nonadditivity effects in the mixture
   and the pore width the condensed phases formed inside the pore may have
   different structures. In particular, it is shown that the mixture may
   separate into layers containing only one component each and the stacking
   may depend on the pore width and properties of the mixture.}},
