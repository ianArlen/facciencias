@article{ ISI:000237626100030,
Author = {Garza-Gonzalez, Carlos and Camprubi, Antoni and Gonzalez-Partida,
   Eduardo and Arriaga-Garcia, German and Rosique-Naranjo, Fernando},
Title = {{Hydrothennal alteration and fluid inclusion study of the Lower
   Cretaceous porphyry Cu-Au deposit of Tiamaro, Michoacan, Mexico}},
Journal = {{JOURNAL OF GEOCHEMICAL EXPLORATION}},
Year = {{2006}},
Volume = {{89}},
Number = {{1-3, SI}},
Pages = {{124-128}},
Month = {{APR-JUN}},
Note = {{5th International Conference on Fluid Evolution, Migration and
   Interaction in Sedimentary Basin and Orogenic Belts, Univ Windsor, Dept
   Earth Sci, Windsor, CANADA, MAY 16-21, 2006}},
Abstract = {{The Tiamaro deposit in Michoacan state has been dated as Lower
   Cretaceous (Valanginian), though most of the porphyry deposits in
   central Mexico were dated or have an attributed Eocene-Oligocene age.
   The host rocks belong to a volcanoplutonic complex overlain by red
   conglomerates. These rocks were intruded by pre-Valanginian plutonic and
   hypabissal rocks. Propylitic, phyllic, and argillic alteration
   assemblages developed, and their superimposition draws the evolution of
   the deposit. Stage I is represented by propylitic assemblages, stage H
   contains the main ore forming stockworks and both phyllic and argillic
   assemblages, and stage III contains late carbonatization assemblages.
   The obtained temperatures and salinities from inclusion fluids are low
   for a porphyry-type deposit, but we interpret that the known part of the
   deposit represents the shallow portion of a bigger deposit. The
   evolution of mineralizing fluids draws a dilution trend of brines from
   ``porphyry-like{''} to ``epithermal-like{''} stages. The richest ore
   zone is roughly located between the 300 and 350 degrees C isotherms,
   though unnoticed resources may occur at depth. (C) 2006 Elsevier B.V.
   All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
