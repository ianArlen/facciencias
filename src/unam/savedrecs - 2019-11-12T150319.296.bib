@article{ ISI:000239223200009,
Author = {De Diego, Jose A. and Nunez, Dario and Zavala, Jesus},
Title = {{Pioneer anomaly? Gravitational pull due to the Kuiper belt}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS D}},
Year = {{2006}},
Volume = {{15}},
Number = {{4}},
Pages = {{533-544}},
Month = {{APR}},
Abstract = {{In this work, we study the gravitational influence of the material
   extending from Uranus orbit to the Kuiper belt and beyond on objects
   moving within these regions. We conclude that a density distribution
   given by rho(r) = 1/r (for r >= 20 AU) generates a constant acceleration
   towards the Sun on those objects, which, with the proper amount of mass,
   accounts for the blue shift detected on the Pioneers space crafts. We
   also discuss the effect of this gravitational pull on Neptune, and
   comment on the possible origin of such a matter distribution.}},
