@article{ ISI:000235826700016,
Author = {Rivera, JM and Reyes, H and Cortes, A and Santillan, R and Lacroix, PG
   and Lepetit, C and Nakatani, K and Farfan, N},
Title = {{Second-harmonic generation within the P2(1)2(1)2(1) space group, in a
   series of chiral (salicylaldiminato)tin Schiff base complexes}},
Journal = {{CHEMISTRY OF MATERIALS}},
Year = {{2006}},
Volume = {{18}},
Number = {{5}},
Pages = {{1174-1183}},
Month = {{MAR 7}},
Abstract = {{A series of 10 (2-11) new chiral (salicylaidiminato)tin Schiff base
   complexes obtained from the Schiff base condensation of
   4-(diethylamino)salicylaldehyde and various amino acids, in the presence
   of diphenyltin oxide, is reported together with the parent achiral (1)
   derivative. Compounds 2-7 crystallize in the P2(1)2(1)2(1) orthorhombic
   space group, and their quadratic nonlinear optical properties are
   investigated. At the molecular level, the derivatives possess similar
   electronic spectra (lambda(max) approximate to 395 nm), and hence
   molecular hyperpolarizabilities (beta), in relation to the same
   T-conjugated ``push-pull{''} electronic structure. In the solid state,
   they exhibit efficiencies in second-harmonic generation (SHG) up to 8
   times that of urea. The SHG intensities of chromophores 2-6 appear
   largely correlated with a simple ``degree of chirality{''} (d(x))
   parameter, defined from the molecular geometries. The intriguing issue
   of a possible quantification of chirality by means of SHG measurement is
   addressed.}},
