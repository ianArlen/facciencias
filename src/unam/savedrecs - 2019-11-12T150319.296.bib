@article{ ISI:000237626100033,
Author = {Gonzalez-Partida, Eduardo and Camprubi, Antoni},
Title = {{Evolution of mineralizing fluids in the Zn-Pb-Cu(-Ag +/- Au) skarn and
   epithermal deposits of the world-class San Martin district, Zacatecas,
   Mexico}},
Journal = {{JOURNAL OF GEOCHEMICAL EXPLORATION}},
Year = {{2006}},
Volume = {{89}},
Number = {{1-3, SI}},
Pages = {{138-142}},
Month = {{APR-JUN}},
Note = {{5th International Conference on Fluid Evolution, Migration and
   Interaction in Sedimentary Basin and Orogenic Belts, Univ Windsor, Dept
   Earth Sci, Windsor, CANADA, MAY 16-21, 2006}},
Abstract = {{Mineralizing fluids at the San Martin skarn show an evolution
   characterized by prograde and retrograde associations. The prograde
   mineral associations consist of (1) a massive garnet zone, (2) a
   tremolite garnet zone, and (3) a late association of quartz, sphalerite,
   calcite and fluorite lining the vugs in the garnet zone. The fluids of
   the prograde associations exhibit decreasing temperatures of
   homogenization (Th) and variable salinities. The fluids of the massive
   garnet zone have salinities of similar to 36 wt.\% NaCl equiv. and Th of
   645 to 570 degrees C, corresponding to pressures of similar to 1055 bar.
   At the tremolite +/- garnet zone, Th range from 438 to 354 degrees C. In
   the late association at the endoskam, the following evolution can be
   drawn: (a) salinities of 50 to 42 wt.\% NaCl equiv., and Th of 455 to
   346 degrees C in quartz, (b) salinities of similar to 46 wt.\% NaCl
   equiv., and Th of 415 to 410 degrees C in sphalerite, (c) salinities of
   50 to 37 wt.\% NaCl equiv., and Th of 479 to 3 10 degrees C in calcite,
   (d) salinities of 33 to 28 wt.\% NaCl equiv. and of 24 to 22 wt.\% KCl
   in fluorite, and (e) two types of fluids with salinities of similar to 2
   and 39 wt.\% NaCl equiv. and Th similar to 344 and 300 degrees C,
   respectively, in later saccharoidal quartz segregations. The retrograde
   mineral associations comprise pervasive propylitic alteration to
   carbonization, and mantos with sulfides. Fluids in epidote have
   salinities of similar to 7.6 wt.\% NaCl equiv. and Th of 287 to 252
   degrees C, and in calcite have salinities of 9.2 to 1 wt.\% NaCl equiv.
   and Th of 188 to 112 degrees C. Fluids in the sulfide assemblages in the
   mantos have salinities of 8 to 3 wt.\% NaCl equiv. and Th similar to 300
   degrees C, with corresponding pressures of similar to 94 bar. Fluids in
   late epithermal veins close to the intrusive body have salinities of 10
   to 5 wt.\% NaCl equiv. and Th of 275 to 200 degrees C, and distal veins
   show salinities of 2 to 1 wt.\% NaCl equiv. and Th of similar to 160
   degrees C. (C) 2006 Elsevier B.V. All rights reserved.}},
