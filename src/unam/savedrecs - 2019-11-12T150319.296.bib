@article{ ISI:000236700400005,
Author = {Lemus-Santana, AA and Reyes-Lezama, M and Zuniga-Villarreal, N and
   Toscano, RA and Espinosa-Perez, GE},
Title = {{Spontaneous formation of an organometallic-inorganic dirhenium carbonyl
   cryptate encapsulating a sodium cation}},
Journal = {{ORGANOMETALLICS}},
Year = {{2006}},
Volume = {{25}},
Number = {{8}},
Pages = {{1857-1860}},
Month = {{APR 10}},
Abstract = {{The reactions between the rhenium(1) complexes {[}ReBr(CO)(5)],
   {[}Re2Br2(CO)(6)(THF)(2)], and {[}ReOTf(CO)(5)] and
   Na{[}Ph2P(O)NP(O)Ph-2] in toluene and THF under reflux and in
   dichloromethane at room temperature, respectively, afforded the complex
   Na{[}Re-2(CO)(6)\{mu-Ph2P(O)NP(O)Ph-2-kappa O-2\}(3)] in good to medium
