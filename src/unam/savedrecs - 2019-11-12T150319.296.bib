@article{ ISI:000237626100034,
Author = {Gonzalez-Partida, Eduardo and Camprubi, Antoni and Gonzalez-Sanchez,
   Francisco and Sanchez-Torres, Jaime},
Title = {{Fluid inclusion study of the Plomositas-Los Arcos polymetallic
   epithennal vein tract, Plomosas district, Sinaloa, Mexico}},
Journal = {{JOURNAL OF GEOCHEMICAL EXPLORATION}},
Year = {{2006}},
Volume = {{89}},
Number = {{1-3, SI}},
Pages = {{143-148}},
Month = {{APR-JUN}},
Note = {{5th International Conference on Fluid Evolution, Migration and
   Interaction in Sedimentary Basin and Orogenic Belts, Univ Windsor, Dept
   Earth Sci, Windsor, CANADA, MAY 16-21, 2006}},
Abstract = {{The most important deposit in the Plomosas-Rosario district, Sinaloa, is
   the vein tract named Plomositas-Plomosas-Los Arcos. These are NNW-SSE
   striking veins hosted in rocks of the Lower Volcanic Supergroup (LVS),
   and also in rocks at the bottom of the Upper Volcanic Supergroup (UVS).
   Both supergroups belong to the Sierra Madre Occidental. These veins
   evolved from an early intermediate sulfidation stage (1), rich in base
   metal sulfides, to a low sulfidation stage (2), rich in silver sulfides
   and sulfosalts. There is also a 45 m-wide stockwork with native silver
   and gold. Stage I is found in the deeper portion of the veins whereas
   stage 2 is found in the most shallow portion of the deposit. These
   stages record fluid inclusion salinities ranging from 7 to 12 wt.\% NaCl
   equiv., and from 0.2 to 3.5 wt.\% NaCl equiv., respectively.
   Homogenization temperatures range from 120 degrees C for surface samples
   to similar to 200 degrees C at a depth of 320 m. The low homogenization
   temperatures recorded, and the dispersion of veins within host rocks as
   veinlets, suggest that this deposit formed at shallow depths and was
   probably blind. (C) 2006 Elsevier B.V. All rights reserved.}},
