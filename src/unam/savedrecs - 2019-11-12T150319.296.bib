@article{ ISI:000237549100027,
Author = {Miranda-Cardenas, Yuritzia and Rojas-Piloni, Gerardo and
   Martinez-Lorenzana, Guadalupe and Rodriguez-Jimenez, Javier and
   Lopez-Hidalgo, Monica and Freund-Mercier, Marie Jose and Condes-Lara,
   Miguel},
Title = {{Oxytocin and electrical stimulation of the paraventricular hypothalamic
   nucleus produce antinociceptive effects that are reversed by an oxytocin
   antagonist}},
Journal = {{PAIN}},
Year = {{2006}},
Volume = {{122}},
Number = {{1-2}},
Pages = {{182-189}},
Month = {{MAY}},
Abstract = {{In recent years, oxytocin has been implicated in a wide diversity of
   functions. The role of oxytocin in analgesia and pain modulation
   represents an important new function of an endogenous system controlling
   sensorial information. The paraventricular (PV) nucleus of the
   hypothalamus is one of the most important sources of oxytocin, and it
   has a very well-defined projection to the spinal cord. The location of
   this PV spinal cord projection correlates well with oxytocin binding
   sites at the dorsal horn of the spinal cord. In this work, we used rats
   with a chronic (46 days) sciatic loose ligature, an electrical
   stimulating electrode, and an intrathecal cannula, which reached the
   L4-L5 levels of the spinal cord. We compared the oxytocin effects with
   electrical stimulation of the PV and observed a significant reduction of
   the withdrawal responses to mechanical and cold stimulation applied to
   the ipsilateral and contralateral hind paws. An oxytocin antagonist
   administered intrathecally blocked the PV effects. Naloxone was also
   intrathecally injected 2 min before the PV stimulation, and we also
   observed a significant reduction of the withdrawal responses; however,
   this reduction was less pronounced. Our results support the hypothesis
   that oxytocin is part of the descending inhibitory control mechanisms
   having an important antinociceptive action. We cannot exclude a minor
   opiate participation in the OT action. (c) 2006 International
   Association for the Study of Pain. Published by Elsevier B.V. All rights
   reserved.}},
