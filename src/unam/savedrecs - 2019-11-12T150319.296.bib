@article{ ISI:000236744300007,
Author = {Xiao, MF and Rakov, N and Alvarez, L},
Title = {{Theoretical and experimental study on optical near and far field
   transmission of thin metallic films with randomly distributed
   subwavelength holes}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2006}},
Volume = {{20}},
Number = {{7}},
Pages = {{841-852}},
Month = {{MAR 20}},
Abstract = {{We present theoretical calculations of the optical transmission through
   subwavelength apertures in a conducting screen of finite thickness. The
   results are employed to study the origin of the enhanced transmission
   observed in experiments where randomly distributed small holes of
   various subwavelength sizes are fabricated in a thin gold film. In the
   wavelength spectrum from 350 nm to 650 nm, a number of strongly enhanced
   transmission peaks are observed. These transmission peaks can only be
   observed in the near field. We discuss the experimental results in the
   light of the model calculations.}},
