@article{ ISI:000238021300005,
Author = {Raina, P. K. and Shukla, A. and Singh, S. and Rath, P. K. and Hirsch, J.
   G.},
Title = {{The 0(+)-> 0(+) positron double-beta decay with emission of two
   neutrinos in the nuclei Ru-96, Pd-102, Cd-106 and Cd-108}},
Journal = {{EUROPEAN PHYSICAL JOURNAL A}},
Year = {{2006}},
Volume = {{28}},
Number = {{1}},
Pages = {{27-36}},
Month = {{APR}},
Abstract = {{Theoretical results for two neutrinos in the nuclei Ru-96, Pd-102,
   Cd-106 and Cd-108 are presented. The study employs the
   Hartree-Fock-Bogoliubov model to obtain the wave functions of the parent
   and daughter nuclei, in conjunction with the summation method to
   estimate the double-beta decay nuclear matrix elements. The reliability
   of the intrinsic wave functions of Ru-96,Ru-102, Mo-96,
   Pd-102,Pd-106,Pd-108 and Cd-106,Cd-108 nuclei is tested by comparing the
   theoretically calculated spectroscopic properties with the available
   experimental data. The calculated half-lives T-1/2(2 nu) of Ru-96,
   Pd-102, Cd-106 and Cd-108 nuclei for 2 nu beta(+)beta(+), 2 nu beta+EC
   and 2 nu ECEC modes are presented. The effect of deformation on the
   nuclear transition matrix element M-2 nu is also studied.}},
