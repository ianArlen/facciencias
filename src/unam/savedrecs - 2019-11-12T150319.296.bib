@article{ ISI:000236231400009,
Author = {Lucio-Lopez, MA and Luna-Arias, MA and Maldonado, A and Olvera, MD and
   Acosta, DR},
Title = {{Preparation of conducting and transparent indium-doped ZnO thin films by
   chemical spray}},
