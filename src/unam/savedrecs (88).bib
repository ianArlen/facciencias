@article{ ISI:000266226900009,
Author = {Murcio, Roberto and Rodriguez-Romo, Suemi},
Title = {{Colored diffusion-limited aggregation for urban migration}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{388}},
Number = {{13}},
Pages = {{2689-2698}},
Month = {{JUL 1}},
Abstract = {{In this paper we study the growth probability and Cluster morphologies
   which emerge in ail off-lattice, two-dimensional, colored
   diffusion-limited aggregation model for urban dynamics, particularly
   migration. To reach this goal, three immobile interacting clusters that
   include the geographical concept of gravity are studied by exact
   enumeration. In our simulations we find a strong correlation between the
   seed's distance, migration rules and number of aggregated particles. The
   growth probability of a certain augular subset and its rate and route of
   convergence to a Normal distribution when migration cost is acting are
   also shown. We search how all the factors mentioned above determine the
   Cluster morphologies. (C) 2009 Elsevier B.V. All rights reserved.}},
