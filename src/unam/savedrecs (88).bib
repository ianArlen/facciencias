Author = {Zapata, Luis A. and Ho, Paul T. P. and Schilke, Peter and Rodriguez,
   Luis F. and Menten, Karl and Palau, Aina and Garrod, Robin T.},
Title = {{A RING/DISK/OUTFLOW SYSTEM ASSOCIATED WITH W51 NORTH: A VERY MASSIVE
   STAR IN THE MAKING}},
Journal = {{ASTROPHYSICAL JOURNAL}},
