@article{ ISI:000267784100008,
Author = {Castro-Eguiluz, Denisse and Pelayo, Rosana and Rosales-Garcia, Victor
   and Rosales-Reyes, Roberto and Aipuche-Aranda, Celia and
   Ortiz-Navarrete, Vianney},
Title = {{B cell precursors are targets for Salmonella infection}},
Journal = {{MICROBIAL PATHOGENESIS}},
Year = {{2009}},
Volume = {{47}},
Number = {{1}},
Pages = {{52-56}},
Month = {{JUL}},
Abstract = {{We previously reported that, in mice, B cells are a reservoir for
   bacteria during Salmonella infection. Here, we show that, within the
   bone marrow, B cells and their precursors are targeted for infection by
