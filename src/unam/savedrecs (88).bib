@article{ ISI:000268611900069,
Author = {Diaz-Torres, Roberto and Jimenez Sandoval, Sergio J. and Ibanez-Orozco,
   Oscar and Rodriguez-Romo, Suemi},
Title = {{Polyethylcyanoacrylate nanoparticle transport through the stratum
   corneum}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2009}},
Volume = {{95}},
Number = {{4}},
Month = {{JUL 27}},
Abstract = {{The traceability of polyethylcyanoacrylate nanoparticles transported
   through human skin is studied in this paper. Photoluminescence is used
   to find the precise diffusion path of polyethylcyanoacrylate
   nanoparticles through the skin stratum corneum (SC). Reproducible data
   were obtained, and the nanoparticles' distribution in each layer of the
   SC is presented. We contribute to the basic knowledge of these phenomena
   as a stationary stochastic process using a one-dimensional linear chain
   model with the left end open and the right end closed. Possible
   applications are in the field of pharmaceutical technology, especially
   concerning drug transport through the skin by nanoparticle carriers.}},
