@article{ ISI:000267240700021,
Author = {Araujo-Pardo, G. and Balbuena, C. and Olsen, M.},
Title = {{On (k, g; l)-dicages}},
Journal = {{ARS COMBINATORIA}},
Year = {{2009}},
Volume = {{92}},
Pages = {{289-301}},
Month = {{JUL}},
Abstract = {{The semigirth l of a digraph D is a parameter related with the number of
   shortest paths in D. In particular, if G is a graph the semigirth of the
   associated symmetric digraph G{*} is l(G{*}) = {[}(g(G) - 1)/2], where
   q(G) is the girth of the graph G. In this paper some bounds for the
   minimum number of vertices of a k-regular digraph D having girth g and
   semigirth E, denoted by n(k, g; l) are obtained. Moreover we construct a
   family of digraphs which achieve the lower bound for some particular
   values of the parameters.}},
