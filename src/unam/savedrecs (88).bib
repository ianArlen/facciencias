@article{ ISI:000268564200007,
Author = {Cruz-Zaragoza, E. and Chernov, V. and Melendrez, R. and Barboza-Flores,
   M.},
Title = {{Gamma-radiation effects on NaCl:Cu crystals}},
Journal = {{PHYSICA STATUS SOLIDI A-APPLICATIONS AND MATERIALS SCIENCE}},
Year = {{2009}},
Volume = {{206}},
Number = {{7}},
Pages = {{1425-1428}},
Month = {{JUL}},
Abstract = {{The gamma-radiation effects on copper-doped NaCl:Cu crystals were
   investigated. Single crystals exposed to gamma radiation from 0.001 kGy
   to 30 kGy at room temperature exhibited optical absorption bands
   attributed to Cu- (236 nm), Cu+ (255 nm) and F (463 nm) centers. The F
   and Cu absorption annealing bands were correlated to the
   thermoluminescence (TL) glow-curve peak structure around 30-165 degrees
   C and 215-350 degrees C, respectively. The TL glow curves of samples
   doped with 0.04 mol\% and 0.08 mol\% of Cu+ ions showed a strong
   quenching effect for higher doping concentration. In addition, thermal
   bleaching of gamma-irradiated specimens confirmed the color centers and
   Cu-doping participation on the TL process. (C) 2009 WILEY-VCH Verlag
   GmbH \& Co. KGaA, Weinheim}},
