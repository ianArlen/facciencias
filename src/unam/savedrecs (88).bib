@article{ ISI:000270446900001,
Author = {Sulaiman, Yiming and Flores-Serrano, Cesar and Ortiz-Hernandez, Antonio
   and Berta Angulo-Mejorada, Rosa and Montaldo, Hugo H.},
Title = {{Evaluation of adjustment methods for environmental effects for weaning
   weight in Suffolk lambs}},
Journal = {{VETERINARIA MEXICO}},
Year = {{2009}},
