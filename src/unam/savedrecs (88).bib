@article{ ISI:000267011600002,
Author = {Gonzalez-Morales, Lidia and Diego-Garcia, Elia and Segovia, Lorenzo and
   Gutierrez, Maria del Carmen and Possani, Lourival D.},
Title = {{Venom from the centipede Scolopendra viridis Say: Purification, gene
   cloning and phylogenetic analysis of a phospholipase A2}},
Journal = {{TOXICON}},
