@article{ ISI:000266877900008,
Author = {Yousfi, Mohammed and de Urquijo, Jaime and Juarez, Antonio and Basurto,
   Eduardo and Luis Hernandez-Avila, Jose},
Title = {{Electron Swarm Coefficients in CO(2)-N(2) and CO(2)-O(2) Mixtures}},
Journal = {{IEEE TRANSACTIONS ON PLASMA SCIENCE}},
