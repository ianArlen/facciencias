@article{ ISI:000269627300021,
Author = {Hortelano-Moncada, Yolanda and Cervantes, Fernando A. and Trejo-Ortiz,
   Aida},
Title = {{Wild mammals of Reserva Ecologica del Pedregal de San Angel, Ciudad
   Universitaria, UNAM, Mexico, DF}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2009}},
Volume = {{80}},
Number = {{2}},
Pages = {{507-520}},
Month = {{AUG}},
Abstract = {{This paper documents the mammals from the Reserva Ecologica del Pedreal
   de San Angel. Data were gathered from records in mammal collections, the
   specialized literature, and through collecting efforts in the area.
   Records spanning 1943 to the present document the presence of 33
   species, 28 genera, 15 families, and 6 orders of mammals. One squirrel
   and one mouse (Sciurus; aureogaster nigrescens, and Peromyscus in.
   melanophrys) are reported for the first time for the reserve. Two
   additional records have been previously published but have not been
   included in previous inventories for the reserve: the red bat, Lasiurus
   blossevillii teliotis and the cuinique, Spermophilus odocetus odocetus.
   Two species are currently listed under the risk category of threatened,
   1 is under special protection, and 7 are endemic to Mexico. This mammal
   component is important for the ecosystem of the area and the
   conservation of biodiversity. The richness of mammals of the. area
   deserves efforts to conserve and preserve the biodiversity of this
   unique ecosystem within Mexico City.}},
