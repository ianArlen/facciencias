@article{ ISI:000267900500005,
Author = {Vazquez, Lorena and Alpuche, Juan and Maldonado, Guadalupe and Agundis,
   Concepcion and Pereyra-Morales, Ali and Zenteno, Edgar},
Title = {{Immunity mechanisms in crustaceans}},
