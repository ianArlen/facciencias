@article{ ISI:000265657700006,
Author = {Varela, R. Enrique and Jose Sanchez-Sosa, Juan and Biggs, Bridget K. and
   Luis, Timothy M.},
Title = {{Parenting strategies and socio-cultural influences in childhood anxiety:
   Mexican, Latin American descent, and European American families}},
Journal = {{JOURNAL OF ANXIETY DISORDERS}},
Year = {{2009}},
Volume = {{23}},
Number = {{5}},
Pages = {{609-616}},
Month = {{JUN}},
Abstract = {{This Study examined the relationship between anxiety in Latin American
   children and Latino cultural schemas, parenting strategies, being ail
   ethnic minority, and assimilation. Latin American (n = 72; LA) and white
   European-American (n = 46; EA) children living in the U.S., Mexican
   children living in Mexico (n = 99; M), and at least one parent per
   family (n = 283) were administered measures assessing anxiety, parenting
   strategies, collectivism, family cohesion, simpatia, parent-child
   communication, and assimilation. M and LA children expressed more
   anxiety symptoms than EA children. More mother control and less father
   acceptance were associated with childhood anxiety across all three
   groups. However, father control was associated with more anxiety for the
   EA group but not the MA group, and mother acceptance was associated with
   more anxiety for the EA and MA groups but With less anxiety for the M
   group. Family cohesion was negatively associated with children's anxiety
   independent of ethnic group. Finally, differing from parents in
   assimilation did not influence LA children's anxiety. (C) 2009 Elsevier
   Ltd. All rights reserved.}},
