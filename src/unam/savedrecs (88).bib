@article{ ISI:000267875500012,
Author = {Acosta, Gerardo and Hernandez-Gutierrez, Rodrigo and
   Martinez-de-la-Vega, Veronica},
Title = {{DENDRITES AND SYMMETRIC PRODUCTS}},
Journal = {{GLASNIK MATEMATICKI}},
Year = {{2009}},
Volume = {{44}},
Number = {{1}},
Pages = {{195-210}},
Month = {{JUN}},
Abstract = {{For a given continuum X and a natural number n, we consider the
   hyperspace F-n(X) of all nonempty subsets of X with at most n points,
   metrized by the Hausdorff metric. In this paper we show that if X is a
   dendrite whose set of end points is closed, n epsilon N and Y is a
   continuum such that the hyperspaces F-n(X) and F-n(Y) are homeomorphic,
   then Y is a dendrite whose set of end points is closed.}},
