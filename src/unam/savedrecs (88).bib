@article{ ISI:000267218300010,
Author = {Antonyan, Natella and Antonyan, Sergey A. and Rodriguez-Medina, Leonardo},
Title = {{Linearization of proper group actions}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{156}},
Number = {{11}},
Pages = {{1946-1956}},
Month = {{JUN 15}},
Note = {{Conference on General Topology in Honour of Peter Collins and Mike Reed,
   Univ Oxford, Oxford, ENGLAND, AUG 07-10, 2006}},
Abstract = {{We prove that if G is a locally compact group acting properly (in the
   sense of R. Palais) on a space X that is metrizable by a G-invariant
   metric, then X can be embedded equivariantly into a normed linear
   G-space E endowed with a linear isometric G-action which is proper on
   the complement E \textbackslash{} \{0\}. If, in addition, G is a Lie
   group then E \textbackslash{} \{0\} is a G-equivariant absolute
   extensor. One can make this equivariant embedding even closed, but in
   this case the non-proper part of the linearizing G-space E may be an
   entire subspace instead of \{0\}. (C) 2009 Elsevier B.V. All rights
   reserved.}},
