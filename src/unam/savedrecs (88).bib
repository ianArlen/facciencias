@article{ ISI:000268617700013,
Author = {Dominguez, Fabiola and Chavez, Marco and Luisa Garduno-Ramirez, Maria
   and Chavez-Avila, Victor M. and Mata, Martin and Cruz-Sosa, Francisco},
Title = {{Production of Honokiol and Magnolol in Suspension Cultures of Magnolia
   dealbata Zucc.}},
Journal = {{NATURAL PRODUCT COMMUNICATIONS}},
