@article{ ISI:000269660700006,
Author = {Juarez Olguin, Hugo and Carrasco Portugal, Miriam and Flores Perez,
   Janett and Camacho Vieyra, Angelica and Flores Perez, Carmen and
   Rodriguez Alfaro, Alfonso},
Title = {{Effect of Mannitol on the Pharmacokinetics of Amikacin in Wistar Rats}},
Journal = {{BRAZILIAN ARCHIVES OF BIOLOGY AND TECHNOLOGY}},
Year = {{2009}},
Volume = {{52}},
Number = {{4}},
Pages = {{835-839}},
Month = {{JUL-AUG}},
Abstract = {{The study analyzed the effect of mannitol on the pharmacokinetics (PK)
   of amikacin. Adult Wistar rats were treated as follows: Group 1 (G1)
   received mannitol for three days, Group 2 (G2) received mannitol plus 10
   mg/kg of amikacin simultaneously, and Group 3 only amikacin. The PK
   study was conducted on the 4th day. For which, blood samples were drawn
   at fixed times during 24 h and immunoenzymatically analyzed. Results
   revealed significant differences (p<0.05) between the groups, e.g. Cmax
   were 62.26 +/- 15.75 mu g/ml for G1, 72.63 +/- 24.80 mu g/ml for G2 and
   68.61 +/- 27.40 mu g/ml for G3. The AUC also differed groups, being
   largest for G2, 222.52 +/- 47.30 mu g/ml/h, and smallest for G1, 135.59
   +/- 39.00 mu g/ml/h. Alteration of the PK parameters observed between
   the groups must be considered when both drugs are prescribed, although
   human studies are necessary to confirm the results.}},
