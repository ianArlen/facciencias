@article{ ISI:000180009200009,
Author = {Solis-Ortiz, S and Cabrera, MC},
Title = {{EEG pattern of anxiety along the menstrual cycle}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2002}},
Volume = {{19}},
Number = {{2}},
Pages = {{187-195}},
Month = {{DEC}},
Abstract = {{Electroencephalographic (EEG) profile associated with anxiety along the
   menstrual cycle wash investigated in 9 healthy women with regular
   menstrual cycles. EEG and anxiety level (STAI scale) were recorded in 10
   sessions, three times per week. EEC, spectral power and correlations
   were obtained. Anxiety was high in the luteal and low in the ovulatory
   phase and was inversely correlated with theta power. High: anxiety was
   correlated with high alpha activity and high intrahemispheric
   correlation within the right hemisphere. Present results demonstrate for
   the first time in women that self-reported anxiety is associated with
   Specific EEO profiles. Low anxiety with an EEC, pattern similar to that
   observed after anxiolytic administration and high anxiety with an EEC,
   profile similar to that reported after anxiolytics withdrawal. They also
   demonstrate that anxiety in women is correlated with more homogenous
