@article{ ISI:000179446800008,
Author = {Psenicka, B and Garcia-Ugalde, F and Heirera-Camacho, A},
Title = {{The bilinear Z transform by pascal matrix and its application in the
   design of digital filters}},
Journal = {{IEEE SIGNAL PROCESSING LETTERS}},
Year = {{2002}},
Volume = {{9}},
Number = {{11}},
Pages = {{368-370}},
Month = {{NOV}},
Abstract = {{In this letter, the Pascal matrix is used for transforming the
   normalized analog transfer function H(s) from the lowpass to the lowpass
   and highpass discrete transfer functions H(z). This algorithm is very
   simple; therefore, the transfer function H (, s) can be easily
   transformed to the z domain using an appropriate calculator. The inverse
   Pascal matrix can be obtained without computing the determinant of the
   system, and then it is very easy to obtain the associated analog
   transfer function H(8) if the discrete transfer function H(z) is known.}},
