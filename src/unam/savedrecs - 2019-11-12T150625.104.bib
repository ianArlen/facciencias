@article{ ISI:000178095500041,
Author = {Prosen, T and Seligman, TH and Weidenmuller, HA},
Title = {{Integration over matrix spaces with unique invariant measures}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2002}},
Volume = {{43}},
Number = {{10}},
Pages = {{5135-5144}},
Month = {{OCT}},
Abstract = {{We present a method to calculate integrals over monomials of matrix
   elements with invariant measures in terms of Wick contractions. The
   method gives exact results for monomials of low order. For higher-order
   monomials, it leads to an error of order 1/N-alpha, where N is the
   dimension of the matrix and where alpha is independent of the degree of
   the monomial. We give a lower bound on the integer alpha and show how
   alpha can be increased systematically. The method is particularly suited
   for symbolic computer calculation. Explicit results are given for O(N),
   U(N), and for the circular orthogonal ensemble. (C) 2002 American
   Institute of Physics.}},
