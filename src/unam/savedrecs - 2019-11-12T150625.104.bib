@article{ ISI:000178500800008,
Author = {Benammi, M and Soe, AN and Tun, T and Bo, B and Chaimanee, Y and
   Ducrocq, S and Thein, T and Wai, S and Jaeger, JJ},
Title = {{First magnetostratigraphic study of the Pondaung Formation: Implications
   for the age of the Middle Eocene anthropoids of Myanmar}},
Journal = {{JOURNAL OF GEOLOGY}},
Year = {{2002}},
Volume = {{110}},
Number = {{6}},
Pages = {{748-756}},
Month = {{NOV}},
Abstract = {{We report the results of a magnetostratigraphic investigation to improve
   the stratigraphical and chronological resolution of the Pondaung
   Formation of central Myanmar. A total of 98 samples were collected from
   45 sites through a 319-m-thick section at the fossiliferous locality of
   Yashe Kyitchaung or the Primate Resort (yielding primate species Bahinia
   pondaungensis, Amphipithecus mogaungensis, and Myanmarpithecus
   yarshensis) near the Bahin village. Thermal and alternating field
   demagnetization allowed separation of two remanence components. The
   high-temperature component is interpreted as the characteristic
   magnetization. Rock magnetic experiments show that the remanence
   magnetization is mainly carried by magnetite. This investigation
   documents normal polarity remanent magnetization, with a mean direction
   D = 336.7degrees, I = 45.5degrees (N = 74, alpha95 = 7.1, k = 12). A
   local anticlockwise tectonic rotation of about 32degrees +/- 0.94degrees
   about a vertical axis is suggested, therefore. The magnetostratigraphic
   data indicate that the Bahin section can be correlated with chron C17n.
   1n, and an age of between 37 and 37.4 Ma can thus be assigned to the
   anthropoid locality.}},
