@article{ ISI:000180858000007,
Author = {Garcia-Maynez, A and Watson, S},
Title = {{Samuel uniformities}},
Journal = {{BOLETIN DE LA SOCIEDAD MATEMATICA MEXICANA}},
Year = {{2002}},
Volume = {{8}},
Number = {{2}},
Pages = {{173-186}},
Month = {{OCT}},
Abstract = {{Every T-2-uniformity U on a set X determines a totally bounded
   uniformity S(U) on X which determines the same topology and satisfies a
   universal property. This is the so-called Samuel uniformity of U. If V
   is another uniformity on X and S(U) = S(V), we describe a homeomorphic
   copy of the completion (X, V) as a subspace of the Samuel
   compactification (X, S(U) If V-1, V-2 are two uniformities on X lying
   between S(U) and U, we find conditions which insure that the completions
   (X, V-1), M V-2) are homeomorphic. We find conditions for two subsets A
   and B of (X, U) to be completely separated in (X, S(U); for (X, U) to be
   Z-embedded, G(S)-dense or C-1(1)-embedded in its Samuel
   compactification; for a subset A of (X, U) to be G(S)-dense in its
   closure in (X, S(U) and for the existence of a normal Wallman basis 93
   on X, consisting of cozero sets of X and whose corresponding uniformity
   coincides with S(U).}},
