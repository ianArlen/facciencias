@article{ ISI:000178622000061,
Author = {Oleschko, K and Korvin, G and Balankin, AS and Khachaturov, RV and
   Flores, L and Figueroa, B and Urrutia, J and Brambila, F},
Title = {{Fractal scattering of microwaves from soils}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2002}},
Volume = {{89}},
Number = {{18}},
Month = {{OCT 28}},
Abstract = {{Using a combination of laboratory experiments and computer simulation we
   show that microwaves reflected from and transmitted through soil have a
   fractal dimension correlated to that of the soil's hierarchic
   permittivity network. The mathematical model relating the
   ground-penetrating radar record to the mass fractal dimension of soil
   structure is also developed. The fractal signature of the scattered
   microwaves correlates well with some physical and mechanical properties
   of soils.}},
