@article{ ISI:000178530100012,
Author = {Pasapera, AM and Gutierrez-Sagal, R and Herrera, J and Galicia-Canales,
   N and de la Mora, GG and Ulloa-Aguirre, A},
Title = {{Norethisterone is bioconverted to oestrogenic compounds that activate
   both the oestrogen receptor alpha and oestrogen receptor beta in vitro}},
Journal = {{EUROPEAN JOURNAL OF PHARMACOLOGY}},
Year = {{2002}},
Volume = {{452}},
Number = {{3}},
Pages = {{347+}},
Month = {{OCT 11}},
