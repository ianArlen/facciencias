@article{ ISI:000179772800006,
Author = {Vacher, H and Alami, M and Crest, M and Possani, LD and Bougis, PE and
   Martin-Eauclaire, MF},
Title = {{Expanding the scorpion toxin alpha-KTX 15 family with AmmTX3 from
   Androctonus mauretanicus}},
Journal = {{EUROPEAN JOURNAL OF BIOCHEMISTRY}},
Year = {{2002}},
Volume = {{269}},
Number = {{24}},
Pages = {{6037-6041}},
Month = {{DEC}},
Abstract = {{A novel toxin, AmmTX3 (3823.5 Da), was isolated from the venom of the
   scorpion Androctonus mauretanicus . It showed 94\% sequence homology
   with Aa1 from Androctonus australis and 91\% with BmTX3 from Buthus
   martensi which, respectively, block A-type K+ current in cerebellum
   granular cells and striatum cultured neurons. Binding and displacement
   experiments using rat brain synaptosomes showed that AmmTX3 and Aa1
   competed effectively with (125) I-labelled sBmTX3 binding. They fully
   inhibited the (125) I-labelled sBmTX3 binding (K (i) values of 19.5 pm
   and 44.2 pm, respectively), demonstrating unambiguously that the three
   molecules shared the same target in rat brain. The specific binding
   parameters of (125) I-labelled AmmTX3 for its site were determined at
   equilibrium (K (d) = 66 pm, B (max) = 22 fmol per mg of protein).
   Finally, patch-clamp experiments on striatal neurons in culture
   demonstrated that AmmTX3 was able to inhibit the A-type K+ current (K
   (i) = 131 nm).}},
