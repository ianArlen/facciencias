@article{ ISI:000179773300008,
Author = {Bravo-Perez, G and Garzon, IL},
Title = {{Ab initio study of small AunS, (n=1-5), and AunS2, (n=1-4), clusters}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE-THEOCHEM}},
Year = {{2002}},
Volume = {{619}},
Pages = {{79-89}},
Month = {{DEC 9}},
Abstract = {{The geometries of the lowest-lying isomers of AunS, (n = 1-5), and
   AunS2, (n = 1-4), clusters are determined through a post Hartree-Fock
   second order perturbative Moller-Plesset method. A structural analysis
   of the most stable isomers shows that S atoms and S-2 dimers combine
   with very stable triangular and tetrahedral subunits of the Ann
   clusters. Our results indicate that the gold-sulphur interaction is
   strong enough to modify the known growth pattern of bare gold clusters.
   (C) 2002 Elsevier Science B.V. All rights reserved.}},
