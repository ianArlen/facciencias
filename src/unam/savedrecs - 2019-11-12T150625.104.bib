@article{ ISI:000178447400043,
Author = {Vazquez, GJ and Avendano, C and Reyes, JA and Del Castillo-Mussot, M and
   Spector, H},
Title = {{Stark effect in a finite region for a 1D Coulomb potential}},
Journal = {{SURFACE REVIEW AND LETTERS}},
Year = {{2002}},
Volume = {{9}},
Number = {{5-6}},
Pages = {{1827-1830}},
Month = {{OCT-DEC}},
Abstract = {{We calculate the states of a one-dimensional hydrogen atom under the
   effect of an electric field (Stark effect) in the strong field regime
   being the field confined in a finite region of width 2a (capacitor
   region). We find numerically the solution inside the capacitor and match
   it to an analytical solution outside the capacitor. Although the
   electric field tends to separate the two opposite charges particles, the
   total energy of the system decreases with increasing electric field
   strength. Our results are useful as a guideline to study strong electric
   field effects in electronic states of impurities or excitons in 1D
   systems.}},
