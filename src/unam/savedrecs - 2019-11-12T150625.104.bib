@article{ ISI:000180317600015,
Author = {Aguilar-Arevalo, AA and D'Olivo, JC},
Title = {{Active neutrino oscillations and the SNO neutral current measurement}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2002}},
Volume = {{66}},
Number = {{11}},
Month = {{DEC 1}},
Abstract = {{We discuss the relation between the observed charged current, elastic
   scattering, and neutral current fluxes and the flavor fractional content
   of the solar neutrino flux seen by SNO. By using existing estimates of
   the cross sections for the charged and neutral current reactions which
   take into account the detector resolution, we show how the forthcoming
   SNO rates unconstrained by the standard B-8 shape could test the
   oscillations into active states. We perform a model independent analysis
   for the Super-Kamiokande and SNO data, assuming a nondistorted spectrum.}},
