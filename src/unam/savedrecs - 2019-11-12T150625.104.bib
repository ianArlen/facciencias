@article{ ISI:000181339200004,
Author = {Alarcon-Aguilar, FJ and Campos-Sepulveda, AE and Xolalpa-Molina, S and
   Hernandez-Galicia, E and Roman-Ramos, R},
Title = {{Hypoglycaemic activity of Ibervillea sonorae roots in healthy and
   diabetic mice and rats}},
Journal = {{PHARMACEUTICAL BIOLOGY}},
Year = {{2002}},
Volume = {{40}},
Number = {{8}},
Pages = {{570-575}},
Month = {{DEC}},
Abstract = {{The acute effects of the freeze-dried decoction (traditional
   preparation) of Ibervillea sonorae Greene roots (Cucurbitaceae) on blood
   glucose levels were investigated in fasting mice. The plant orally
   administrated to healthy mice did not cause a significant decrease of
   the blood glucose level. However, I. sonorae reduced the blood glucose
   of normal mice in a dose-dependent manner after intraperitoneal
   injection (P < 0.05). Also, this extract significantly lowered the
   glycemia of mild alloxan-diabetic mice and rats, but did not in severe
   alloxan-diabetic rats, so it seems that this antidiabetic plant needs
   the presence of insulin to show its hypoglycaemic activity. Chemical,
   pharmacological, and toxicological investigations of I. sonorae must
   continue to establish its use as an alternative in the control of
   diabetes mellitus. Furthermore, it is important to start programs
   leading to the preservation of this interesting resource.}},
