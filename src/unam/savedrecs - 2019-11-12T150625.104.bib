@article{ ISI:000178885600009,
Author = {Bruce, NC},
Title = {{A comparison of the converging and diverging geometries for measuring
   enhanced backscatter}},
Journal = {{JOURNAL OF MODERN OPTICS}},
Year = {{2002}},
Volume = {{49}},
Number = {{13}},
Pages = {{2167-2181}},
Month = {{NOV}},
Abstract = {{In this paper, numerical calculations are performed to compare scattered
   light distributions for light reflected from randomly rough surfaces in
   the converging and diverging illumination geometries. It is found that
   the enhanced backscatter peak can be measured accurately if the source
   and detector are at the same distance in the diverging geometry, and if
   the source and detector distances are approximately 100 times the size
   of the illuminated surface in the converging geometry.}},
