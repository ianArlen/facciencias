@article{ ISI:000180607700055,
Author = {Alvarez, R},
Title = {{Banderas Rift Zone: A plausible NW limit of the Jalisco block}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2002}},
Volume = {{29}},
Number = {{20}},
Month = {{OCT 15}},
Abstract = {{{[}1] Echo soundings recently made in Bahia de Banderas show that this
   region is a graben with steeply dipping walls and several basins; it is
   the offshore continuation of the Valle de Banderas graben, and of a
   branching rift (Rio Ameca rift) originating in the Tepic- Zacoalco rift
   zone. The general trend of the three structures is ENE with some NE
   trending offsets, and they have a total length of 150 km; this Banderas
   Rift Zone is proposed as the NW limit of the Jalisco block. The
   existence of this limit suggests that there is another platelet, or
   block, between the Jalisco block and a portion of the Rivera plate,
   probably bounded by the Tres Marias escarpment, the Jalisco block and
   the North America plate.}},
