@article{ ISI:000179573800008,
Author = {Ayala, A and Cuautle, E and Herrera, G and Montano, LM},
Title = {{Quark-Gluon plasma formation and Lambda(0) polarization in
   ultra-relativistic heavy-ion collisions}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2002}},
Volume = {{48}},
Number = {{2}},
Pages = {{49-54}},
Month = {{NOV}},
Note = {{25th Symposium on Nuclear Physics, TAXCO, MEXICO, JAN 02-07, 2002}},
Organization = {{Div Fis Nucl Soc Mexicana Fis; Inst Ciencias Nucl; Inst Fis; Ctr
   Latinoamer Fis}},
Abstract = {{We calculate and compare the polarization of Lambda(0)'s produced in
   ultra-relativistic heavy-ion and proton-proton collisions as a signature
   for the production of Quark Gluon Plasma. Lambda(0)'s produced in
   heavy-ions come from two different regions characterized by different
   densities of participants within the interaction zone. In the first one,
   where the density of participants is larger than a certain critical
   density, quarks are free and coalesce to produce unpolarized
   Lambda(0)'s. In the second zone, where the density is below a certain
   critical density, Lambda(0)'s are produced by a recombination mechanism
   and are therefore polarized. We also discuss possible depolarization
   effects arising from multiple scattering of Lambda(0)'s within nuclear
   matter.}},
Publisher = {{SOCIEDAD MEXICANA DE FISICA}},
Address = {{APARTADO POSTAL 70-348, COYOACAN 04511, MEXICO}},
Type = {{Article; Proceedings Paper}},
Language = {{English}},
