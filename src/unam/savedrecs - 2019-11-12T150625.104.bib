@article{ ISI:000180009200007,
Author = {Castillo-Parra, G and De Jesus, AI and Ostrosky-Solis, F},
Title = {{Affective valence, arousal and reaction time: A study with Mexican
   population}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2002}},
Volume = {{19}},
Number = {{2}},
Pages = {{167-176}},
Month = {{DEC}},
Abstract = {{The experimental study of emotion requires stimuli that reliably evoke
   psychological and physiological reactions that vary systematically over
   the range of expressed emotions and that are calibrated in the
   dimensions that constitute the affective space (valence and arousal).
   The objective of this study was to select a group of visual stimuli with
   emotional content and to characterize them in terms of reaction time as
   well as valence and arousal in a Mexican population. We selected 700
   pictures of the International Affective Picture System (IAPS) (Lang,
   Bradley \& Cuthbert, 1999). The materials evoke reactions across the
   entire range of each dimension. Gender differences in the pattern of
   affective reports were found.. Stimuli with affective content generate
   shorter reaction times. Although our results are consistent with the
   data obtained with English speaking Population in the USA, we found
   significant differences in the rating of valence and arousal in several
   stimuli. These findings point Out the importance of taking into account,
   cultural and gender effects in the selection of affective stimuli.}},
