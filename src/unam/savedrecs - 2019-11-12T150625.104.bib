@article{ ISI:000179861900043,
Author = {Casas, M and de Llano, M and Puente, A and Rigo, A and Solis, MA},
Title = {{Pre-formed Cooper pairs and Bose-Einstein condensation in cuprate
   superconductors}},
Journal = {{JOURNAL OF PHYSICS AND CHEMISTRY OF SOLIDS}},
Year = {{2002}},
Volume = {{63}},
Number = {{12}},
