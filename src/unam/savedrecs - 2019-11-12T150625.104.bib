@article{ ISI:000179080800034,
Author = {Takeuchi, N},
Title = {{Surface properties of YN(001): A first-principles calculation}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2002}},
Volume = {{66}},
Number = {{15}},
Month = {{OCT 15}},
Abstract = {{We have studied the structural and electronic properties of the YN(001)
   surface by first-principles total energy calculations. In analogy with
   the ScN(001) surface, we have restricted our study to models having a
   (1x1) periodicity. Our calculations predict that the relaxed bulk
   terminated surface has the lowest formation energy for most of the range
   of allowed chemical potentials. Electronic properties of this surface
   are similar to those of the bulk, and the surface is a semiconductor.
   For strongly Y-rich conditions, a pure (1x1)-Y terminated structure is
   more favorable than the ideal-relaxed surface. In this case, the surface
   is found to be metallic.}},
