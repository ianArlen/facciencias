@article{ ISI:000178791600005,
Author = {Pacheco-Lopez, G and Espinosa, E and Zamorano-Rojas, HM and
   Ramirez-Amaya, V and Bermuden-Rattoni, F},
Title = {{Peripheral protein immunization induces rapid activation of the CNS, as
   measured by c-Fos expression}},
Journal = {{JOURNAL OF NEUROIMMUNOLOGY}},
Year = {{2002}},
Volume = {{131}},
Number = {{1-2}},
Pages = {{50-59}},
Month = {{OCT}},
Abstract = {{To investigate immune-to-brain communication, we challenged rats
   intraperitoneally with a protein antigen (keyhole limpet hemocyanin,
   KLH), and measured c-Fos expression in different brain structures. We
   found a rapid c-Fos expression 120 min after immunization in brain stem
   and forebrain structures, using a dose of KLH not inducing fever or
   malaise. Using proper controls, we found that this central response is
   related to the immunogenicity of the antigen and the magnitude of the
   immune response. Our results suggest that different interconnected brain
   cell groups respond rapidly to an immune challenge in the periphery,
   constituting an afferent pathway of neuroimmune communication. (C) 2002
   Elsevier Science B.V. All rights reserved.}},
