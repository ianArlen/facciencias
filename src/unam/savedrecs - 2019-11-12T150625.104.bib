@article{ ISI:000179322700002,
Author = {Valdes-Martinez, J and Hernandez-Ortega, S and Hermetet, AK and
   Ackerman, LJ and Presto, CA and Swearingen, JK and Kelman, DR and
   Goldberg, KI and Kaminsky, W and West, DX},
Title = {{Structural studies of N-2-(3-picolyl)- and N-2-(4-picolyl)-N
   `-tolylthioureas}},
