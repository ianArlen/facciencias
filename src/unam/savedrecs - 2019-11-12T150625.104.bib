@article{ ISI:000180009200002,
Author = {Acuna, L and Bruner, CA},
Title = {{The effect of people on health}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2002}},
Volume = {{19}},
Number = {{2}},
Pages = {{115-124}},
Month = {{DEC}},
Abstract = {{This study determined the effects of sociometric statu's as a modulator
   of the noxious effects of stress on health. In addition, the correlation
   between sociometric status and perceived social support was determined.
   University students (N = 443) received sociometric evaluations from
   their peers and also responded to three questionnaires: the Social
   Readjustment Rating Scale, the Health and Daily Living Form, and the
   Social Support Questionnaire. While a positive sociometric status
   mitigated the noxious effects of stress on health, a negative
   sociometric status agravated these noxious effects. The correlation
   between sociometric status and perceived social support was reliable. It
   was concluded that independent judgements, rather than self-reports, may
   be preferable in determining the effects of the interaction with other
   people.}},
