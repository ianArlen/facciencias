@article{ ISI:000180317800012,
Author = {Barkovich, M and D'Olivo, JC and Montemayor, R and Zanella, JF},
Title = {{Neutrino oscillation mechanism for pulsar kicks reexamined}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2002}},
Volume = {{66}},
Number = {{12}},
Month = {{DEC 15}},
Abstract = {{Anisotropic neutrino emission during neutron star formation can be the
   origin of the observed proper motions of pulsars. We derive a general
   expression for the momentum asymmetry in terms of the neutrino energy
   flux gradient, and show that a nonvanishing effect is induced at the
   lowest order by a deformed neutrinosphere. In particular, this result is
   valid for a neutrino flux transported through a spherical atmosphere
   with constant luminosity.}},
