@article{ ISI:000179681500035,
Author = {Fordham, JLA and Vranesevic, N and Carraminana, A and Michel, R and
   Much, R and Wehinger, P and Wyckoff, S},
Title = {{Phase-resolved spectroscopic imaging of the crab pulsar}},
