@article{ ISI:000178852800032,
Author = {Hinojosa-Torres, J and Castano, VM},
Title = {{Properties in hardness of Zn-22Al-2Cu alloy wire artificially aged}},
Journal = {{MATERIALS LETTERS}},
Year = {{2002}},
Volume = {{56}},
Number = {{3}},
Pages = {{312-316}},
Month = {{OCT}},
Abstract = {{Thin wire was produced by the mechanical processing (i.e. extrusion and
   wiredrawing) of a Zn-22Al-2Cu (wt.\%) alloy rod prepared by the
   continuous casting technique. Samples of this wire were artificially
   aged at 120, 130, 140 and 150 degreesC for different periods. Hardness
   measurements on the longitudinal section of the heat-treated wire showed
   the dependence of this mechanical property on temperature and ageing
   time. The decomposition of the metastable eta(T)' phase into the Zn-fich
   hcp stable,eta phase as a consequence of the artificial ageing produced
   the observed strengthening as discussed in terms of the energy involved.
   In contrast, for the stable tau' precipitate (Al(4)Cu(3)Zn), which is
   present in all the artificially aged samples, the contribution of this
   precipitate to the strengthening of the material is shown to be
   negligible. (C) 2002 Elsevier Science B.V All rights reserved.}},
