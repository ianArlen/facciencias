@article{ ISI:000179050600004,
Author = {Colin, J and Gonzalez, C and Herrera, R and Juarez-Islas, JA},
Title = {{Analysis of chill-cast NiAl intermetallic compound with copper additions}},
Journal = {{JOURNAL OF MATERIALS ENGINEERING AND PERFORMANCE}},
Year = {{2002}},
Volume = {{11}},
Number = {{5}},
Pages = {{487-491}},
Month = {{OCT}},
Abstract = {{This study carried out a characterization of chill-cast NiAl alloys with
   copper additions, which were added to NiAl, such that the resulting
   alloy composition occurred in the beta-field of the ternary NiAlCu phase
   diagram. The alloys were vacuum induction melted and casted in copper
   chill molds to produce ingots 0.002 m thick, 0.020 m wide, and 0.050 m
   long. X-ray diffractometry (XRD) and transmission electron microscopy
   (TEM) performed in chill-cast ingots identified mainly the presence of
   the beta-(Ni,Cu)Al phase. As-cast ingots showed essentially no ductility
   at room temperature except for the Ni50Al40Cu10 alloy, which showed
   1.79\% of elongation at room temperature. Ingots with this alloy
   composition were then heat treated under a high-purity argon atmosphere
   at 550degreesC (24 h) and cooled either in the furnace or in air, until
   room temperature was reached. beta-(Ni,Cu)AI and gamma'(Ni,Cu)(3)Al were
   present in specimens cooled in the furnace and beta-(Ni,Cu)Al,
   gamma'(Ni,Cu)(3)Al plus martensite-(Ni,Cu)Al were present in specimens
   cooled in air. Thermogravimetric analysis indicated that martensite
   transformation was the result of a solid-state reaction with M-s similar
   to 470 and M-f similar to 430degreesC. Tensile tests performed on bulk
   heat-treated ingots showed room-temperature ductility between 3 and 6\%,
   depending on the cooling media.}},
