@article{ ISI:000178852800037,
Author = {Valdiviezo-Mijangos, OC and Sabina, FJ and Bravo-Castillero, J and
   Rodriguez-Ramos, R and Guinovart-Diaz, R},
Title = {{Fiber-reinforced composite with cubic symmetry constituents}},
Journal = {{MATERIALS LETTERS}},
Year = {{2002}},
Volume = {{56}},
Number = {{3}},
Pages = {{339-343}},
Month = {{OCT}},
Abstract = {{An elastic material with unidirectional cylindrical fibers periodically
   distributed is analyzed. Each periodic cell is a binary homogeneous
   elastic medium with cubic symmetry constituents. Perfect bonding
   conditions at the interface are considered. Simple closed-form formulae
   are obtained for the overall properties using the asymptotic
   homogenization method. The analytical solution of the required resulting
   plane- and antiplane-strain local problems, which turns out to be only
   three, makes use of potential methods of a complex variable and
   properties of Weierstrass elliptic and related functions of periods
   (1,0) and (0,1). Two new exact relations are derived without solving any
   local problem and are valid for any shape of the interface compatible
   with the square symmetry. Some numerical examples are shown. (C) 2002
   Published by Elsevier Science B.V.}},
