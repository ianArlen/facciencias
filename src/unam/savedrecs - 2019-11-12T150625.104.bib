@article{ ISI:000179681500024,
Author = {Lefloch, B and Cernicharo, J and Rodriguez, LF and Miville-Deschenes, MA
   and Cesarsky, D and Heras, A},
Title = {{The photoionization of a star-forming core in the Trifid Nebula}},
Journal = {{ASTROPHYSICAL JOURNAL}},
