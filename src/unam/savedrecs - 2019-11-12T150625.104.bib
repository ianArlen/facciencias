@article{ ISI:000179708300008,
Author = {Arancibia-Bulnes, CA and Bandala, ER and Estrada, CA},
Title = {{Radiation absorption and rate constants for carbaryl photocatalytic
   degradation in a solar collector}},
Journal = {{CATALYSIS TODAY}},
