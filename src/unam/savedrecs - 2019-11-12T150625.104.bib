@article{ ISI:000180230700009,
Author = {Tovar-Hernandez, MA and Granados-Barba, A and Solis-Weiss, V},
Title = {{Typosyllis papillosus, a new species (Annelida : Polychaeta : Syllidae)
   from the southwest Gulf of Mexico}},
Journal = {{PROCEEDINGS OF THE BIOLOGICAL SOCIETY OF WASHINGTON}},
Year = {{2002}},
Volume = {{115}},
Number = {{4}},
Pages = {{760-768}},
Month = {{DEC 30}},
Abstract = {{A new species of syllid polychaete, Typosyllis papillosus, is described
   from oil platforms in the Campeche Sound, southwestern Gulf of Mexico.
   The species is distinguished from other Typosyllis species by the
   following features: unique composite heterogomph falcigers bearing
   blades with bidentate tips with distal tooth larger than subdistal and
   rounded and proximal part of proboscis subdivided into ten regions, each
   of which with ten longitudinal rows of five or six rounded to triangular
   papillae. The species is closely related to T. typica (Moore, 1909) due
   to the presence of similar chaetae in both; nevertheless, no superior
   composite heterogomph falcigers with long blades and coarse basal
   serrations are present in the latter, and T. typica lacks the ten groups
   of rounded papillae distributed along the pharyngeal basal area in T.
   papillosus.}},
