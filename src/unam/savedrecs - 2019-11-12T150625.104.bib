@article{ ISI:000179573800016,
Author = {Padilla, E and Galindo-Uribarri, A and Castanos, O and Baktash, C and
   Batchelder, JC and Bijker, R and Fuentes, B and Gross, CJ and Hartley,
   DJ and Hausladen, PA and Mueller, PE and Radford, DC and Stracener, DW
   and Yu, CH},
Title = {{Studies of nuclei far from stability in the regions A=80 and A=130}},
Journal = {{REVISTA MEXICANA DE FISICA}},
