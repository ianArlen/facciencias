@article{ ISI:000181190900002,
Author = {Espinosa-Solares, T and Brito-De La Fuente, E and Tecante, A and
   Medina-Torres, L and Tanguy, PA},
Title = {{Mixing time in rheologically evolving model fluids by hybrid dual mixing
   systems}},
