@article{ ISI:000181183200014,
Author = {Vazquez-Fernandez, E},
Title = {{Low-head portable system for small scale irrigation}},
Journal = {{WATER INTERNATIONAL}},
Year = {{2002}},
Volume = {{27}},
Number = {{4}},
Pages = {{585-589}},
Month = {{DEC}},
Abstract = {{A localized irrigation system consisting of an open-pipe network of
   polyethylene hose fed from a low-head (0.3 to 1.5 m) is proposed. The
   feasible area for irrigation covers 0.1 ha of flat ground The irrigation
   laterals are portable and have an approximate length of 10 m with 12 to
   15 nozzles. The nozzles can be 0.89 mm diameter diffusers, or else,
   holes punched with a stainless steel 1.19 mm diameter punch. Flow rates
   measured at the laboratory in a single lateral vary from 2 to 6 l/s,
   with the smaller values corresponding to the punched hose. Average
   coefficients of variation are to 0.03 for the hose with diffusers and to
   0.10 for the punched hose. The field measurements for the pipes with
   diffusers at seven laterals with 14 outlets each show that the flow rate
   should be reduced by 35 percent with respect to the value measured in
   the laboratory. The cost is lower than that of any other low-head system
   available.}},
