@article{ ISI:000283051200005,
Author = {Auvinet-Guichard, Gabriel and Rodriguez-Rebolledo, Juan F. and
   Rangel-Nunez, Jose L.},
Title = {{Construction of deep tunnel shafts in Mexico city soft clays by the
   flotation method}},
Journal = {{ACTA GEOTECHNICA}},
Year = {{2010}},
Volume = {{5}},
Number = {{1}},
Pages = {{63-68}},
Month = {{APR}},
Abstract = {{The drainage system of Mexico City, one of the largest metropolitan
   areas in the world, is formed by a large network of interceptors and
   collectors leading to the main outlet: a 49.3-km-long tunnel with a 6.5
   m diameter. To build the access shafts to the tunnels of the system and
   the deep sumps of the pumping stations in the very soft clays of the
   lacustrine area of Mexico valley, a technique known as ``flotation
   method{''} has been commonly used since 1969. The paper presents the
   main construction steps followed and some aspects of the analysis and
   design of shafts constructed by this technique. The experience gained
   obtained after more than 30 shafts have been constructed by this method,
   and the actual trends in its application are also discussed.}},
