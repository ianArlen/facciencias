@article{ ISI:000276686400014,
Author = {Flores-Gutierrez, Daniel and Urrutia-Fucugauchi, Jaime and Perez-Cruz,
   Ligia and Linares-Lopez, Carlos},
Title = {{Micromagnetic and microstructural analyses in chondrules of the Allende
   meteorite}},
Journal = {{REVISTA MEXICANA DE CIENCIAS GEOLOGICAS}},
Year = {{2010}},
Volume = {{27}},
Number = {{1}},
