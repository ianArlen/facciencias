@article{ ISI:000278299500002,
Author = {Zizzari, Z. V. and Machida, R. and Tsutsumi, K. and Reynoso-Velasco, D.
   and Lupetti, P. and Dallai, R.},
Title = {{Ultrastructural studies on euspermatozoa and paraspermatozoa in
   Mantispidae (Insecta, Neuroptera)}},
Journal = {{TISSUE \& CELL}},
