@article{ ISI:000274309700009,
Author = {Smolentsev, S. and Cuevas, S. and Beltran, A.},
Title = {{Induced electric current-based formulation in computations of low
   magnetic Reynolds number magnetohydrodynamic flows}},
Journal = {{JOURNAL OF COMPUTATIONAL PHYSICS}},
Year = {{2010}},
