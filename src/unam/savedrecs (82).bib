@article{ ISI:000273753700021,
Author = {Rios-Reyes, C. H. and Mendoza-Huizar, L. H. and Rivera, M.},
Title = {{Electrochemical kinetic study about cobalt electrodeposition onto GCE
   and HOPG substrates from sulfate sodium solutions}},
Journal = {{JOURNAL OF SOLID STATE ELECTROCHEMISTRY}},
Year = {{2010}},
Volume = {{14}},
