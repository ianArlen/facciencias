@article{ ISI:000279529700013,
Author = {Moreno-Altamirano, Laura},
Title = {{Disease, body and corporeity: an anthropological perspective}},
Journal = {{GACETA MEDICA DE MEXICO}},
Year = {{2010}},
Volume = {{146}},
Number = {{2}},
Pages = {{150-156}},
Month = {{MAR-APR}},
Abstract = {{The aim of this article is to present a reflection on the perception of
   disease, taking the body and its relation to the world as a crucial
   point of departure. We develop some key notions of body and corporeity
   that throughout history involve different symbolic conceptions of the
   diseased body from an anthropological perspective. We highlight the
   polysemic nature of bodily performance that involves nature and culture
   as well as the mind-body dual condition. Thus, the notion of embodiment
   involves our body consciousness, not just the experience of what we feel
   through it, but the set of meanings from it that we give the world.}},
