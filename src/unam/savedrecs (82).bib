@article{ ISI:000276971000005,
Author = {Ricker, Martin and Hernandez, Hector M.},
Title = {{Tree and tree-like species of Mexico: gymnosperms, monocotyledons, and
   tree ferns}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2010}},
Volume = {{81}},
Number = {{1}},
Pages = {{27-38}},
Month = {{APR}},
Abstract = {{Trees or tree-like plants are defined here broadly as perennial,
   self-supporting plants with an adult height of at least 5 m (without
   ascending leaves or inflorescences), and with 1 or several erect sterns
   with a diameter of at least 10 cm. We present an updated list of all
   Mexican tree species under that definition in the Gymnospermae (86
   species, 38\% endemic to Mexico), Monocotyledonae (75 species, 55\%
   endemic), and Pteridophyta (9 species, none endemic). The list contains
   a total of 170 species in 37 genera and 12 families, with 74 species
   (44\%) being endemic to Mexico. The list was compiled consulting recent
   literature, the National Herbarium of Mexico (MEXU), and taxonomic
   specialists.}},
