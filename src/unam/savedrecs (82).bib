@article{ ISI:000276751900005,
Author = {Alvarez, Fernando and Luis Villalobos, Jose and Iliffe, Thomas M.},
Title = {{A new species of mud shrimp of the genus Espeleonaushonia Juarrero \&
   Martinez-Iglesias, 1997 (Decapoda: Gebiidea: Laomediidae) from Palau}},
Journal = {{ZOOTAXA}},
Year = {{2010}},
Number = {{2429}},
Pages = {{61-68}},
Month = {{APR 19}},
Abstract = {{A new species of Espeleonaushonia is described from a single specimen
   collected in an underwater cave in Babeldaob Island, Republic of Palau.
   The new species is placed in Espeleonaushonia based on the following
   characters: exopod of third maxilliped well developed, propodus of first
   pereopod with spines on dorsal surface and along mesial and lateral
   margins; second pereopod not chelate with dactylus densely setose, fifth
   pereopod with simple dactylus, fourth to sixth abdominal pleura with
   serrate ventrodistal angle, and both rami of uropods completely divided.
   The description of this new species represents the first report of the
   genus from the tropical southwest Pacific.}},
