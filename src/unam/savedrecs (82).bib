@article{ ISI:000275635000041,
Author = {Lopez, Rigoberto and Gonzalez, Federico and Villafuerte-Castrejon, M. E.},
Title = {{Structural and electrical characterization of
   (K0.48Na0.52)(0.96)Li0.04Nb0.85Ta0.15O3 synthesized by spray drying}},
Journal = {{JOURNAL OF THE EUROPEAN CERAMIC SOCIETY}},
Year = {{2010}},
Volume = {{30}},
Number = {{6}},
Pages = {{1549-1553}},
Month = {{APR}},
Abstract = {{The synthesis of lead-free ferroelectric materials with composition
   (K0.48Na0.52)(0.96)Li0.04Nb0.85Ta0.15O3 has been achieved by spray
   drying technique. Pure perovskite phase was obtained after calcination
   of as-prepared powders at 800 degrees C for 1 h. Crystallized powders
   have a particle size average of 100 nm. Well sintered samples were
   obtained at 1120 and 1130 degrees C for 2h in air with densities of 4.58
   and 4.49 g/cm(3), respectively. We attributed this improvement as the
   result of sintering process and the very small particle size in powders.
   Sintered samples have promising piezoelectric parameters, k(p)
   0.40-0.41, d(31) 50-55 pC/N and dielectric losses around 1\%. (C) 2009
   Elsevier Ltd. All rights reserved.}},
