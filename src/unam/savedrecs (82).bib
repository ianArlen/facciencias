@article{ ISI:000275884400001,
Author = {Barquera-Lozada, Jose E. and Quiroz-Garcia, Beatriz and Quijano,
   Leovigildo and Cuevas, Gabriel},
Title = {{Conformational Properties of the Germacradienolide
   6-Epidesacetyllaurenobiolide by Theory and NMR Analyses}},
Journal = {{JOURNAL OF ORGANIC CHEMISTRY}},
