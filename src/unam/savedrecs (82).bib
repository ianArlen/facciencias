@article{ ISI:000278749900041,
Author = {Castillo, N. and Perez, R. and Martinez-Ortiz, M. J. and Diaz-Barriga,
   L. and Garcia, L. and Conde-Gallardo, A.},
Title = {{Structural analysis of platinum-palladium nanoparticles dispersed on
   titanium dioxide to evaluate cyclo-olefines reactivity}},
