@article{ ISI:000275336200030,
Author = {Olivares-Pilon, H. and Baye, D. and Turbiner, A. V. and Lopez Vieyra, J.
   C.},
Title = {{One-electron atomic-molecular ions containing lithium in a strong
   magnetic field}},
Journal = {{JOURNAL OF PHYSICS B-ATOMIC MOLECULAR AND OPTICAL PHYSICS}},
Year = {{2010}},
Volume = {{43}},
Number = {{6}},
Month = {{MAR 28}},
Abstract = {{The one-electron lithium-containing Coulomb systems of atomic type
   Li(2+) and molecular type Li(2)(5+), LiHe(4+) and LiH(3+) are studied in
   the presence of a strong magnetic field B <= 10(7) au in a
   non-relativistic framework. They are considered at the Born-Oppenheimer
   approximation of zero order (infinitely massive centres) within the
   parallel configuration (molecular axis parallel to the magnetic field).
   The variational and Lagrange-mesh methods are employed, complementing
   each other. It is demonstrated that the molecular systems LiH(3+),
   LiHe(4+) and Li(2)(5+) can exist for sufficiently strong magnetic fields
   B greater than or similar to 10(4) au and that Li(2)(5+) can even be
   stable at magnetic fields typical of magnetars.}},
