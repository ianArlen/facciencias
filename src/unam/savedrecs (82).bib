@article{ ISI:000276574700003,
Author = {Pena, O. and Muhl, S. and Lopez, W. and Rodriguez-Fernandez, L. and
   Ruvalcaba-Sil, J. L.},
Title = {{Hydrogen plasma etching of silicon dioxide in a hollow cathode system}},
Journal = {{THIN SOLID FILMS}},
Year = {{2010}},
Volume = {{518}},
Number = {{12}},
Pages = {{3156-3159}},
Month = {{APR 2}},
Abstract = {{Chemical etching of various materials has been observed when hydrogen
   plasmas are used in material processing. In the case of the deposition
   of diamond films the preferential etching of sp(2) bonded carbon is
   considered to be of fundamental importance. A few papers have been
   published which have indicated that etching by hydrogen ions is
   different to that by hydrogen atoms. In this paper we describe the
   etching of silicon dioxide by hydrogen which was plasma-activated in a
   molybdenum-lined RF hollow cathode. The etch rate was seen to be
   thermally activated but decreased with increasing plasma power. The
   addition of a few percentage of helium increased the etch rate. The
   application of a 50 V bias to the sample holder almost doubled the etch
   rate indicating the importance of ion bombardment for the chemical
   reaction. At high plasma powers and substrate temperatures in excess of
   450 degrees C a thin molybdenum deposit was formed on the quartz
   samples. (C) 2009 Elsevier B.V. All rights reserved.}},
