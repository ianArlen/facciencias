@article{ ISI:000280512500005,
Author = {Filobello, U. and de la Macorra, A.},
Title = {{Accelerating cosmological models for an interacting tachyon}},
Journal = {{NUOVO CIMENTO DELLA SOCIETA ITALIANA DI FISICA B-BASIC TOPICS IN PHYSICS}},
Year = {{2010}},
Volume = {{125}},
Number = {{3}},
Pages = {{315-331}},
Month = {{MAR}},
Abstract = {{We study the cosmological evolution of a tachyon scalar field T with a
   Dirac-Born-Infeld-type Lagrangian and potential V(T) coupled to a
   canonically normalized scalar field phi with an arbitrary but
   factorizable interaction potential B(T, phi) in the presence of a
   barotropic fluid. We will assume that the force between the barotropic
   fluid and the scalar field is only gravitational and we show that all
   models dependence is given in terms of three parameters: lambda(1) =
   -V(T)/V, lambda(2) = -B(T)/B and lambda(3) = -B(phi)/B, which are field
   and time dependent. We give the general solution to the cosmological
   equations of motion and determine in which cases we can have an
   accelerating universe giving rise to dark energy.}},
