@article{ ISI:000276840200015,
Author = {Samano, Vicente and Guerrero, Fania and Ancheyta, Jorge and Trejo,
   Fernando and Diaz, Jose A. I.},
Title = {{A batch reactor study of the effect of deasphalting on hydrotreating of
   heavy oil}},
