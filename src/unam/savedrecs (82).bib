@article{ ISI:000276002500070,
Author = {Cipparrone, Gabriella and Ricardez-Vargas, Ibis and Pagliusi, Pasquale
   and Provenzano, Clementina},
Title = {{Polarization gradient: exploring an original route for optical trapping
   and manipulation}},
Journal = {{OPTICS EXPRESS}},
Year = {{2010}},
Volume = {{18}},
Number = {{6}},
Pages = {{6008-6013}},
Month = {{MAR 15}},
Abstract = {{We report a study of the capabilities of an optical tweezer based on
   polarization gradient. We use a light polarization pattern that is able
   to simultaneously exert forces and torques in opposite directions
   depending on the particle's position. It allows to perform oscillatory
   displacements and control the sense of rotation of several particles
   inside a uniformly illuminated region. Unconventional trapping of
   spinning particles in circularly polarized fringes has been observed,
   which suggests the involvement of hydrodynamic forces. (C) 2010 Optical
   Society of America}},
