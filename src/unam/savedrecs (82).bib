@article{ ISI:000276300100007,
Author = {Mota-Morales, Josue D. and Quintero-Ortega, Irais and Saldivar-Guerra,
   Enrique and Luna-Barcenas, Gabriel and Albores-Velasco, Martha and
   Percino, Judith and Chapela, Victor and Ocampo, Miguel A.},
Title = {{Mechanism and Kinetics of the Spontaneous Thermal Copolymerization of
   Styrene/Maleic Anhydride. Experimental and Simulation Studies in the
   Presence of 4-oxo-TEMPO}},
Journal = {{MACROMOLECULAR REACTION ENGINEERING}},
Year = {{2010}},
Volume = {{4}},
Number = {{3-4, SI}},
Pages = {{222-234}},
Month = {{MAR 19}},
Abstract = {{The mechanism and kinetics of the spontaneous copolymerization of
   styrene (S) and maleic anhydride (MA) in the presence of 4-oxo-TEMPO
   nitroxide (N) were studied. Experiments were performed at 125 degrees C
   varying the S/MA and the N/S ratios and the evolution of conversion was
   measured by dilatometry up to 20\% conversion. Clean induction periods
   or severe retardation in the initial stage of the reaction were
   observed. From a proposed kinetic mechanism a mathematical model was
   built, which was used for fitting the relevant kinetic constants for the
   self-initiation steps, and the ratio (k) over bar (p)/(k) over bar
   (-1/2)(t). The model performs well in certain concentration regimes, but
   it remains a challenge to completely understand this complex system in
   other concentration regimes.}},
