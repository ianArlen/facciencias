@article{ ISI:000273306900020,
Author = {Arenas-Alatorre, J. and Silva-Velazquez, Y. and Medina, A. Alva and
   Rivera, M.},
Title = {{Advantages and limitations of OM, SEM, TEM and AFM in the study of
   ancient decorated pottery}},
Journal = {{APPLIED PHYSICS A-MATERIALS SCIENCE \& PROCESSING}},
Year = {{2010}},
Volume = {{98}},
Number = {{3}},
Pages = {{617-624}},
Month = {{MAR}},
Abstract = {{This paper presents results from the study of two fragments of
   pre-Hispanic pottery, decorated with red pigment, using Optical
   Microscopy (OM), Scanning Electron Microscopy (SEM), High Resolution
   Transmission Electron Microscopy (HRTEM), Atomic Force Microscopy (AFM)
   and Magnetic Force Microscopy (MFM). Capabilities and limitations of
   these techniques in the analysis of archaeological material are
   highlighted with special emphasis on TEM, AFM and MFM due to their
   contribution in the study of the pigment layer at micro and nano scale.
   The analyzed samples come from the archaeological sites of El Tajin and
   Xochicalco, both in Mexico. Results of conventional TEM and HRTEM
   analysis of the red pigment showed nanometric Fe(2)O(3) particles in
   both samples but different particle shape and size distributions:
   specimen from El Tajin presented irregular particles between 50-100 nm
   while that from Xochicalco exhibited semispherical shapes in the 3-25 nm
   range. AFM images showed the topography of the pigments, which are
   related to the texture of their surface and thus to the production
   process. Finally, MFM showed different contrast regions suggesting the
   presence of ferromagnetic elements forming clusters and domain
   orientations on the color layer.}},
