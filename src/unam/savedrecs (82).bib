@article{ ISI:000275883500021,
Author = {Maria Cotero-Villegas, Ave and Garcia y Garcia, Patricia and
   Hoeplf-Bachner, Herbert and del Carmen Perez-Redondo, Maria and
   Martinez-Salas, Patricia and Lopez-Cardoso, Marcela and Cea Olivares,
   Raymundo},
Title = {{Synthesis and spectroscopic characterization of six new monosubstituted
   dithiocarbamate compounds containing organotellurium(IV) heterocycles:
   Single-crystal structures of C5H10TeI{[}S2CN(CH2CH3)(2)],
   C4H8TeI{[}S2CN(CH2CH2)(2)], C4H8OTeI{[}S2CN(CH2CH2)(2)] and
   C5H10TeI{[}S2CN(CH2CH2)(2)]}},
Journal = {{JOURNAL OF ORGANOMETALLIC CHEMISTRY}},
Year = {{2010}},
Volume = {{695}},
Number = {{8}},
Pages = {{1246-1252}},
Month = {{APR 15}},
Abstract = {{Six novel monosubstituted dithiocarbamate compounds containing
   organotellurium(IV) heterocycles, C4H8OTeI{[}S2CN(CH2CH3)(2)] (1),
   C5H10TeI{[}S2CN(CH2CH3)(2)] (2), C4H8TeI{[}S2CN(CH2CH2)(2)] (3),
   C8H8TeI{[}S2CN(CH2CH2)(2)] (4), C4H8OTeI{[}S2CN(CH2CH2)(2)] (5) and
   C5H10TeI{[}S2CN(CH2CH2)(2)] (6) were synthesized. The compounds were
   characterized by common spectroscopic methods; IR, EI-MS and
   multinuclear NMR (H-1, C-13 and Te-125). The structure of compounds 2,
   3, 5 and 6 was uniequivocally established by single-crystal X-ray
   diffraction studies. Compounds 3, 4 and 5 exhibit dimeric arrangements
   achieved through secondary bonding (Te center dot center dot center dot
   S) and in the case of 2, an additional Te center dot center dot center
   dot S interaction resulted in the formation of a self-assembled chain.
   Furthermore, the geometrical arrangement displayed in the tellurium
   atoms of all the compounds corresponds to a distorted octahedron while
   the coordination mode for the dithiocarbamate ligands is considered
   anisobidentate in all cases. (C) 2010 Elsevier B.V. All rights reserved.}},
