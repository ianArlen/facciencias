@article{ ISI:000078779500006,
Author = {Bagnoli, F and Rechtman, R},
Title = {{Synchronization and maximum Lyapunov exponents of cellular automata}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{1999}},
Volume = {{59}},
Number = {{2, A}},
Pages = {{R1307-R1310}},
Month = {{FEB}},
Abstract = {{We study the synchronization of totalistic one-dimensional cellular
   automats (CA). The CA with a nonzero synchronization threshold exhibit
   complex nonperiodic space time patterns and vice versa. This
   synchronization transition is related to directed percolation. We also
   study the maximum Lyapunov exponent for CA, defined analogous to
   continuous dynamical systems as the exponential rate of expansion of the
   linear map induced by the evolution rule of CA, constructed with the aid
   of the Boolean derivatives. The synchronization threshold is strongly
   correlated to the maximum Lyapunov exponent and we propose approximate
   relations between these quantities. The value of this threshold can he
   used to parametrize the space time complexity of CA.
   {[}S1063-651X(99)51502-X].}},
