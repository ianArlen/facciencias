@article{ ISI:000081146600008,
Author = {Molina, M and Contreras, CM and Tellez-Alcantara, P and Rodriguez, F},
Title = {{Sedative actions of Ternstroemia sylvatica in the male rat}},
Journal = {{PHYTOMEDICINE}},
Year = {{1999}},
Volume = {{6}},
Number = {{2}},
Pages = {{115-118}},
Month = {{MAY}},
Abstract = {{Ternstroemia sylvatica is a plant reputed popularly to possess a
   anxiolytic properties but has not yet been systematically tested for
   such activity. The behavioral actions of T. sylvatica were examined
   using the open field test, the elevated plus-maze test, and the forced
   swim test in male rats. T. sylvatica (7.1 mg/kg and 14.2 mg/kg, I.P.)
   reduced ambulatory behavior in the open field test and cancelled the
   anti-immobility actions produced by desipramine (32 mg/kg, I.P) in the
   forced swim test, as did diazepam. In the elevated plus-maze test, T.
   sylvatica (7.1 mg/kg, I.P.) failed to show anxiolytic actions. It is
   concluded that Ternstroemia sylvatica produces sedative effects rather
   than the attributed anxiolytic actions.}},
