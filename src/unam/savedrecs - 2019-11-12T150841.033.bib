@article{ ISI:000079199800016,
Author = {Xiao, MF},
Title = {{Evanescent fields do contribute to the far field}},
Journal = {{JOURNAL OF MODERN OPTICS}},
Year = {{1999}},
Volume = {{46}},
Number = {{4}},
Pages = {{729-733}},
Month = {{MAR 20}},
Abstract = {{By rigorously verifying the exact expressions for separate homogeneous
   waves and evanescent fields in the far field of dipole radiation, it is
   shown that (1) the homogeneous waves alone in the angular spectrum
   representation of the dipole field do not generate the far field; (2)
   the contribution of the evanescent fields to the far field is as 1/r, or
   as the inverse distance from the dipole, and thus cannot be neglected;
   and (3) only with the combination of the two parts, i.e. the complete
   angular spectrum, can the far field be precisely retrieved.}},
