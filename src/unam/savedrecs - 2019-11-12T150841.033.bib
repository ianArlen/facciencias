@article{ ISI:000077967100002,
Author = {Costas, ME and Acevedo-Chavez, R},
Title = {{Density functional study of neutral allopurinol tautomeric forms}},
Journal = {{JOURNAL OF COMPUTATIONAL CHEMISTRY}},
Year = {{1999}},
Volume = {{20}},
Number = {{2}},
Pages = {{200-206}},
Month = {{JAN 30}},
Abstract = {{Optimized structures and total molecular energy density functional
   theory calculations for the 14 possible neutral allopurinol tautomers
   give the decreasing stability order (N) under bar(1)-H/(N) under
   bar(5)-H > (N) under bar(2)-H/(N) under bar(5)-H > cis-enolic/(N) under
   bar(1)-H for the three mast stable forms. Several molecular and
   electronic structure properties, the stabilities in oxidation and
   reduction processes, the tautomeric equilibrium constants, and the IR
   vibrational spectra were obtained for these. Those properties
   corresponding to the ketonic (N) under bar(5)-H forms are discussed and
   compared with the theoretical ones for the two most stable species of
   the isomer hypoxanthine. A noticeable agreement is found between the
   predicted properties and the experimental behavior known up to date for
   both isomers. (C) 1999 John Wiley \& Sons, Inc.}},
