@article{ ISI:000078877000019,
Author = {Xiao, MF},
Title = {{Physical picture for light emission in scanning tunneling microscopy}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{1999}},
Volume = {{82}},
Number = {{9}},
Pages = {{1875-1878}},
Month = {{MAR 1}},
Abstract = {{In this Letter it is proposed that the light emission in scanning
   tunneling microscopy is caused by the coupling modes inside the
   tip-sample cavity. The coupling modes are determined by various
   environmental aspects, such as the material responses, the tip size, the
   tip-surface distance, and the microfeatures on the sample surface.
   Tunneling electrons interact inelastically with the cavity modes, and
   the excited modes decay by radiating photons to the far-field zone. It
   is pointed out that the radiation and the optical microscopy can be
   described in the framework of scattering scanning near-field optical
   microscopy. {[}S0031-9007(99)08593-2].}},
