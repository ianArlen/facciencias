@article{ ISI:000080047300011,
Author = {Poveda, A and Herrera, MA and Garcia, JL and Hernandez-Alcantara, A and
   Curioca, K},
Title = {{The expected frequency of collisions of small meteorites with cars and
   aircraft}},
Journal = {{PLANETARY AND SPACE SCIENCE}},
Year = {{1999}},
Volume = {{47}},
Number = {{5}},
Pages = {{715-719}},
Month = {{MAY}},
Abstract = {{The cumulative distribution N(d) of diameters of Earth-Crossing
   Asteroids (ECAs) derived by Poveda et al. (1998, submitted) is used to
   estimate the frequency of collisions of meteoroids with cars and with
   aircraft. The expected frequency of collisions of a car with a meteorite
   larger than 10 cm in diameter turns out to be one impact every 16 years.
   This frequency is consistent with the known incidence of such events
   (Lewis, 1996). The expected frequency of collisions of a cruising
   airplane with a meteorite larger than 1 cm in diameter turns out to be
   one impact every 30 years. Such an impactor hitting an airplane at a
   velocity of several hundreds of meters per second could cause a serious
   accident. (C) 1999 Elsevier Science Ltd. All rights reserved.}},
