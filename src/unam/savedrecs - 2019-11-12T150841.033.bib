@article{ ISI:000078827900039,
Author = {Larrion, F and Neumann-Lara, V},
Title = {{Clique divergent graphs with unbounded sequence of diameters}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{1999}},
Volume = {{197}},
Number = {{1-3}},
Pages = {{491-501}},
Month = {{FEB 28}},
Abstract = {{The clique graph kG of a graph G is the intersection graph of the family
   of all maximal complete subgraphs of G. The iterated clique graphs k(n)G
   are defined by k(0)G = G and k(n+1)G = kk(n)G. A graph G is said to be
   k-divergent if \textbackslash{}V(k(n)G)\textbackslash{} tends to
   infinity with n. We provide examples of k-divergent graphs such that the
   diameters of the iterated clique graphs also tend to infinity with n.
   Furthermore, the sizes of the cliques and even the chromatic numbers
   remain bounded. (C) 1999 Elsevier Science B.V. All rights reserved.}},
