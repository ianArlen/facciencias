@article{ ISI:000080315400004,
Author = {Olguin, MT and Solache-Rios, M and Acosta, D and Bosch, P and Bulbulian,
   S},
Title = {{Uranium sorption in zeolite X: the valence effect}},
Journal = {{MICROPOROUS AND MESOPOROUS MATERIALS}},
Year = {{1999}},
Volume = {{28}},
Number = {{3}},
Pages = {{377-385}},
Month = {{MAY}},
Abstract = {{The capacity of zeolite X to sorb UO22+ and U4+ from aqueous solutions
   was investigated. The U4+ and UO22+ uptake was determined at various
   concentrations. The maximum uptake values in equilibrium using 0.05 N
   UO22+ or U4+ solutions were 1.86 and 2.86 meg per gram of zeolite X,
   respectively. No desorption of either UO22+ or U4+ was observed up to
   200 h. The crystallinity of the zeolite samples was lost when the
   concentration of the UCl4 or UO2(NO3)(2) solutions was higher than 0.01
   N. This effect was more evident for U4+, showing that the uranium charge
   plays an important role in retention uptake in zeolite X. The
   incorporation of radioactive material in zeolite X was confirmed by
   electron microscopy. The absorption and diffusion of radioactive ions
   did not alter the surface of the zeolite grains. Radioactive ions were
   found to occupy preferentially cationic positions in the zeolite
   structure as a consequence of diffusion processes. (C) 1999 Elsevier
   Science B.V. All rights reserved.}},
