@article{ ISI:000080177400007,
Author = {Harmony, T and Fernandez, T and Silva, J and Bosch, J and Valdes, P and
   Fernandez-Bouzas, A and Galan, L and Aubert, E and Rodriguez, D},
Title = {{Do specific EEG frequencies indicate different processes during mental
   calculation?}},
Journal = {{NEUROSCIENCE LETTERS}},
Year = {{1999}},
Volume = {{266}},
Number = {{1}},
Pages = {{25-28}},
Month = {{APR 30}},
Abstract = {{EEG recordings during mental calculation and a control task (with
   presentation of stimuli with similar physical characteristics to the
   arithmetic symbols) were obtained in 10 subjects. Narrow band analyses
   of the EEG and distributed sources for each EEG frequency were
   calculated using variable resolution electromagnetic tomography.
   Significant differences between the sources for arithmetic and control
   tasks were observed at 3.9 Hz within Broca's and left parietotemporal
   cortices, suggesting that this frequency may be related with the
   production of internal speech, storage and rehearsal of verbal working
   memory. Differences at 5.46 Hz within the right dorsolateral prefrontal
   cortex were considered to be associated to sustained attention. The
   decrease at 12.46 Hz within the left parietal cortex was interpreted as
   a sign of retrieval of arithmetic facts from long term memory. (C) 1999
   Elsevier Science Ireland Ltd. All rights reserved.}},
