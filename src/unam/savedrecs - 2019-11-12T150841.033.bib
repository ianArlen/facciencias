@article{ ISI:000079516200012,
Author = {Gomez-Pasten, M and Mora, O and Pedraza-Chaverri, J and Shimada, A},
Title = {{The effect of a long term feed restriction on metabolism and tissue
   composition of goats}},
Journal = {{JOURNAL OF AGRICULTURAL SCIENCE}},
