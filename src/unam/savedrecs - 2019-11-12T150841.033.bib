@article{ ISI:000079188400003,
Author = {Lemus, R and Frank, A},
Title = {{A local mode description of anharmonic vibrational excitations in linear
   molecules}},
Journal = {{CHEMICAL PHYSICS}},
Year = {{1999}},
Volume = {{242}},
Number = {{1}},
Pages = {{25-35}},
Month = {{MAR 15}},
Abstract = {{We have recently proposed a symmetry adapted local mode model to
   describe vibrational excitations in polyatomic molecules. The model was
   initially restricted to the description of non-linear (and non-planar)
   polyatomic molecules. In this work we extend the symmetry-adapted
   algebraic approach to include linear molecules. In the harmonic limit
   the model reduces to the standard spectroscopic methods for molecular
   vibrations. Results for the triatomic molecules HCN and CO(2) are
   reported. (C) 1999 Elsevier Science B.V. All rights reserved.}},
