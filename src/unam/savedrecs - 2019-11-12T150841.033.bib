@article{ ISI:000079710900014,
Author = {Lara, MPR},
Title = {{Teaching physics in Mexico during the 18th century - The process
   institutionalization}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1999}},
Volume = {{45}},
Number = {{2}},
Pages = {{193-203}},
Month = {{APR}},
Abstract = {{This article proceeds to study the relationships between the New Spain
   socioeconomic conditions and those of Spain, which give rise to the
   process of institutionalization of physics in Mexico in the 18th
   century. It is very important to point out that for a long time it was
   considered that this process was carried out by the Spanish Crown alone.
   Notwithstanding, recent studies have shown that it was due to an
   initiative of the New Spain Miners to promote and improve the mining
   production that negotiations with the king took place. They proposed to
   the king the foundation of a Mining College.}},
