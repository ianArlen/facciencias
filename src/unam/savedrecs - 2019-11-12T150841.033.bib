@article{ ISI:000084631900044,
Author = {Naumis, GG},
Title = {{Use of the trace map for evaluating localization properties}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{1999}},
Volume = {{59}},
Number = {{17}},
Pages = {{11315-11321}},
Month = {{MAY 1}},
Abstract = {{The use of Lyapunov exponents for evaluating localization lengths of
   wave functions in one-dimensional lattices is discussed. As a result, it
   is shown that it is more practical to calculate this length by using the
   scaling properties of the trace map of the transfer matrix. This leads
   to a relationship between localization and the fixed points of the map,
   which is considered as a dynamical system. The localization length is
   then defined by a Lyapunov exponent, used in the sense of chaos theory.
   All these results are discussed for periodic, disordered, and
   quasiperiodic chains. In particular, the Fibonacci quasiperiodic chain
   is studied in detail. {[}S0163-1829(99)05217-0].}},
