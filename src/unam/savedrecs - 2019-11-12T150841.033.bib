@article{ ISI:000078463100098,
Author = {Lopez-Bolanos, R and Cocoletzi, GH and Wang, S},
Title = {{Surface current effects on the optical response of a multilayer
   excitonic system}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{1999}},
Volume = {{59}},
Number = {{4}},
Pages = {{3234-3240}},
Month = {{JAN 15}},
Abstract = {{Optical response of an insulator-excitonic semiconductor multilayer
   system is studied in the presence of surface currents at the interfaces
   of the inert layer-excitonic active layer. p-polarized light is
   considered to obtain the reflectance of a semi-infinite superlattice and
   the dispersion relation of the electromagnetic normal modes of an
   infinite superlattice. A blueshift of the resonance frequencies is
   manifested due to the existence of currents. The interpretation of the
   optical spectra is given in comparison with the case of a
   single-excitonic film, and in terms of transverse and longitudinal modes
   resonances in the excitonic layers. {[}S0163-1829(99)06004-X].}},
