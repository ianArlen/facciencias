@article{ ISI:000079233200098,
Author = {Martinez, H and Reyes, PG},
Title = {{Absolute differential and total cross sections for single electron
   capture in low-energy Kr+-Ar collisions}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{1999}},
Volume = {{59}},
Number = {{3}},
Pages = {{2504-2507}},
Month = {{MAR}},
Abstract = {{Absolute differential and total cross sections for single electron
   capture were determined from Kr+ ions on Ar in the energy range 0.3-5.0
   keV. Using reduced variables {[}tau = E theta vs theta sin(theta)d
   sigma/d Omega = rho], we deduced. from the experimental differential
   cross sections, that the electron capture channel `'opens'' at a
   critical projectile-target separation between 1.63a(0) and 2.21a(0) in
   the present energy range and that at least two different processes are
   involved. The total cross section for single electron capture is
   compared with other available measurements. These results give a general
   shape of the curve for single electron capture cross sections for the
   Kr+-Ar system in a wide range of energy. {[}S1050-2947(99)00203-6].}},
