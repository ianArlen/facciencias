@article{ ISI:000078827900048,
Author = {Neumann-Lara, V},
Title = {{The acyclic disconnection of a digraph}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{1999}},
Volume = {{197}},
Number = {{1-3}},
Pages = {{617-632}},
Month = {{FEB 28}},
Abstract = {{In this paper we introduce a numerical invariant of digraphs which
   generalizes that of the number of connected components of a graph. The
   acyclic disconnection <(omega)over right arrow>(D) of a digraph D is the
   minimum number of (weakly) connected components of the subdigraphs
   obtained from D by deleting an acyclic set of arcs. We state some
   results about this invariant and compute its value for a variety of
   circulant tournaments. Relations between the dichromatic number and the
   acyclic disconnection of digraphs are explored and a generalization of
   the second invariant is presented. (C) 1999 Elsevier Science B.V. All
   rights reserved.}},
