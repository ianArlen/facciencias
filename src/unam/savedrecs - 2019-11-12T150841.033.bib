@article{ ISI:000079659700013,
Author = {Cosio, FA and Davies, BL},
Title = {{Automated prostate recognition: a key process for clinically effective
   robotic prostatectomy}},
Journal = {{MEDICAL \& BIOLOGICAL ENGINEERING \& COMPUTING}},
Year = {{1999}},
Volume = {{37}},
Number = {{2}},
Pages = {{236-243}},
Month = {{MAR}},
Abstract = {{Clinical trials of PROBOT, a robotic system for prostate surgery, have
   shown that robotic surgery of soft tissue can be successful. Monitoring
   of the progress of the resection has shown to be a necessary feature of
   an effective robotic system for prostate surgery. It should provide the
   surgeon with a reliable method of assessing the cavity during resection.
   An automatic system for intraoperative monitoring of the progress of the
   resection during robotic prostatectomy consists of two subsystems:
   real-time intraoperative imaging of the prostate and automatic
   identification of the contour of the gland on each image. The
   development of a fully automatic scheme for prostate recognition on
   transurethral ultrasound scans is reported. A genetic algorithm has been
   developed to automatically adjust a model of the prostate boundary until
   an optimum fit to the prostate in a given image is obtained. An analysis
   of ifs performance on 22 different ultrasound images showed an average
   error of 6.21 mm. Use of a genetic algorithm and a constrained prostate
   model have shown to be a robust way to automatically identify the
   prostate in ultrasound images. The scheme is able to produce approximate
   prostate boundaries, without any human intervention, on ultrasound scans
   of varying quality. In addition to soft tissue robotic surgery, the
   generic algorithm technique is also applicable to a wide range of
   computer assisted surgical techniques.}},
