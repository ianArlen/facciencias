@article{ ISI:000079776900003,
Author = {Zenteno, JC and Mendez, JP and Maya-Nunez, G and Ulloa-Aguirre, A and
   Kofman-Alfaro, S},
Title = {{Renal abnormalities in patients with Kallmann syndrome}},
