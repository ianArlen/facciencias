@article{ ISI:000080843400095,
Author = {D'Olivo, JC and Fernandez, A and Medina, M and Nellen, L and Roman, S
   and Salazar, H and Valdes-Galicia, J and Vargas, M and Villasenor, L and
   Zepeda, A},
Title = {{Stability and calibration of a water Cerenkov detector prototype}},
Journal = {{NUCLEAR PHYSICS B-PROCEEDINGS SUPPLEMENTS}},
Year = {{1999}},
Volume = {{75A}},
Pages = {{389-391}},
Month = {{APR}},
Note = {{10th International Symposium on Very High Energy Cosmic Ray Interactions
   (ISVHECRI), GRAN SASSO LAB, ASSERGI, ITALY, JUL 12-17, 1998}},
Organization = {{Int Union Pure \& Appl Phys, Cosmic Ray Commiss \& Emulsion Chamber Comm}},
Abstract = {{We present results of studies made with a reduced-scale water Cerenkov
   detector (WCD) prototype for the Pierre Auger Observatory. This detector
   is made of high-density polyethylene and was operated continuously for
   four months. We studied time variations in the amplitude and shape of
   Cerenkov pulses due to cosmic ray muons and studied the correlation of
   these parameters with the bacterial population of the water. We also
   developed and tested a new technique to calibrate and monitor WCDs
   remotely, based on muons stopping and decaying inside the tank. We
   conclude that high-density polyethylene tanks fulfill the requirements
   for use in the water Cerenkov detectors of the Pierre Auger Observatory.}},
