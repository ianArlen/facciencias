@article{ ISI:000080207800002,
Author = {Achnine, L and Mata, R and Lotina-Hennsen, B},
Title = {{Interference of the natural product 7-oxo-7-deacetoxygedunin with CF0 of
   H+-ATPase of spinach chloroplasts}},
Journal = {{PESTICIDE BIOCHEMISTRY AND PHYSIOLOGY}},
Year = {{1999}},
Volume = {{63}},
Number = {{3}},
Pages = {{139-149}},
Month = {{MAR}},
Abstract = {{7-Oxo-7-deacetoxygedunin, a limonoid isolated from Guarea grandiflora
   (Meliaceae) has been found to act as an inhibitor of
   photophosphorylation in spinach thylakoids. ATP synthesis and
   phosphorylating electron flows were inhibited by 88 and 83\%,
   respectively, at 300 mu M of 7-oxo-7-deaceroxygedunin without affecting
   proton uptake, basal and uncoupled electron transports. The ketone group
   of 7-oxo-7-deacetoxygedunin is an important structural requirement for
   the displayed inhibitory activity on chloroplast Hi-ATPase, since
   gedunin possessing a 7 alpha-acetoxy group was a less potent energy
   transfer inhibitor. The light-activated membrane bound Mg2+-ATPase and
   the heat-activated Ca2+-ATPase of the isolated coupling factor were
   insensitive to 7-oxo-7-deacetoxygedunin. 7-oxo-7-deacetoxygedunin shows
   a competitive kinetic behavior with
   5-O-beta-D-galactopyranosyl-7-methoxy-3',4'-dihydroxy-4-phenylcoumarin
   when they inhibit H+-ATPase activity, but a noncompetitive kinetic with
   DCCD. These results suggest that 7-oxo-7-deacetoxygedunin and the
   4-phenylcoumarin have the same surrounding site of inhibition in CF0. Ki
   for 7-oxo-7-deacetoxygedunin, 4-phenylcoumarin, and DCCD were 146, 220,
   and 10 mu M, respectively. (C) 1999 Academic Press.}},
