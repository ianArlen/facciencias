Author = {Taguena-Martinez, J and Rubo, YG and Cruz, M and Beltran, MR and Wang, C},
Title = {{Tight-binding description of disordered nanostructures: an application
   to porous silicon}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{1999}},
Volume = {{142}},
Number = {{1-4}},
Pages = {{564-568}},
Month = {{APR}},
