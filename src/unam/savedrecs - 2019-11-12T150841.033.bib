@article{ ISI:000078444300007,
Author = {Correa, D and Sarti, E and Tapia-Romero, R and Rico, R and
   Alcantara-Anguiano, I and Salgado, A and Valdez, L and Flisser, A},
Title = {{Antigens and antibodies in sera from human cases of epilepsy or
   taeniasis from an area of Mexico where Taenia solium cysticercosis is
   endemic}},
