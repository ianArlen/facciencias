@article{ ISI:000331949500004,
Author = {Sokell, E. and Siggel-King, M. R. F. and Weadick, E. and Juarez, A. M.
   and Bolognesi, P. and King, G. C.},
Title = {{Near-threshold beta-parameter measurements of state-selected rotational
   transitions to the v(+)=0 level of normal and ortho-D-2}},
Journal = {{JOURNAL OF PHYSICS B-ATOMIC MOLECULAR AND OPTICAL PHYSICS}},
Year = {{2014}},
Volume = {{47}},
Number = {{5}},
Month = {{MAR 14}},
Abstract = {{Photoelectron angular distributions in D-2 have been measured for the
   first time with rotational selectivity. The study focuses on the R(2) 5p
   pi and R(0) 5p pi Rydberg states, converging on the v(+) = 4 vibrational
   level of the D-2(+) ion. These Rydberg states autoionize to populate the
   X-2 Sigma(+)(g) (v(+) = 0) state of the ion. The angular asymmetry
   parameter (beta) has been determined, across the photon energy region
   15.671-15.684 eV, for transitions involving changes in rotational
   quantum number, between the initial neutral and final ion state, Delta
   N, of +2 and +4. The beta-parameter values obtained for the Delta N= +2
   transition indicate that the channel is populated via both p-and f-wave
   photoemission. The beta-parameter value for the R(2) n = 5p pi Rydberg
   state was found to be closer to zero, particularly for the Delta N=+ 4
   transition. It is suggested that this near-isotropic emission is
   associated with the photo-excited D-2 molecule rotating before the
   autoionizing decay has had a chance to occur. Measurements have been
   made in both normal and ortho-D-2, confirming the results and verifying
   that the resonant features originate in even rotational levels of the
   D-2 molecule.}},
