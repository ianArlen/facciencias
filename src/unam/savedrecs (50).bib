@article{ ISI:000332337900002,
Author = {Bastarrachea-Magnani, M. A. and Lerma-Hernandez, S. and Hirsch, J. G.},
Title = {{Comparative quantum and semiclassical analysis of atom-field systems.
   II. Chaos and regularity}},
Journal = {{PHYSICAL REVIEW A}},
