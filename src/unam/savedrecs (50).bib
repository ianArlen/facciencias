@article{ ISI:000333536300002,
Author = {Organista-Nava, Jorge and Gomez-Gomez, Yazmin and Gariglio, Patricio},
Title = {{Embryonic stem cell-specific signature in cervical cancer}},
Journal = {{TUMOR BIOLOGY}},
Year = {{2014}},
Volume = {{35}},
Number = {{3}},
Pages = {{1727-1738}},
Month = {{MAR}},
Abstract = {{The wide range of invasive and noninvasive lesion phenotypes associated
   with high-risk human papillomavirus (HR-HPV) infection in cervical
   cancer (CC) indicates that not only the virus but also specific cervical
   epithelial cells in the transformation zone (TZ), such as stem cells
   (SCs), play an important part in the development of cervical neoplasia.
   In this review, we focused in an expression signature that is specific
   to embryonic SCs and to poorly differentiated cervical malignant tumors
   and we hypothesize that this expression signature may play an important
   role to promote cell growth, survival, colony formation, lack of
   adhesion, as well as cell invasion and migration in CC.}},
