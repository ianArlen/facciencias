@article{ ISI:000332005200006,
Author = {Calogero, F. and Leyvraz, F.},
Title = {{A nonautonomous yet solvable discrete-time N-bodyproblem}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2014}},
Volume = {{47}},
Number = {{10}},
Month = {{MAR 14}},
Abstract = {{A new discrete-time N-body problem is introduced. Its equations of
   motion-which become Newtonian equations of motion (accelerations equal
   forces) in the continuous-time limit-are nonautonomous, featuring an
   arbitrary function f (l) of the discrete-time variable l = 0, 1, 2, 3
   .... They are nevertheless solvable by algebraic operations: the
   solution of their initial-value problem are the zeros of a polynomial of
   degree N in z, P-N (z;l), explicitly known for all time-via an
   appropriate discrete-time quadrature-in terms of the initial data. This
   model generalizes a previously known model, in which the arbitrary
   function f (l) is an arbitrary constant eta.}},
