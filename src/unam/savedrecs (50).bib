@article{ ISI:000334507700035,
Author = {Emiliano Canton, Pablo and Lopez-Diaz, Jazmin A. and Gill, Sarjeet S.
   and Bravo, Alejandra and Soberon, Mario},
Title = {{Membrane binding and oligomer membrane insertion are necessary but
   insufficient for Bacillus thuringiensis Cytl Aa toxicity}},
Journal = {{PEPTIDES}},
Year = {{2014}},
Volume = {{53}},
Pages = {{286-291}},
Month = {{MAR}},
Abstract = {{Bacillus thuringiensis Cyt proteins are pore-forming toxins that have
   insecticidal activity mainly against dipteran insects. However, certain
   Cyt proteins have toxicity to some insect orders, but not toxicity of
   CytlAa against lepidopteran larvae has been found. Insect specificity
   has been proposed to rely in specific binding to certain lipids on the
   brush border membrane of midgut cells since no protein receptors have
   been described so far. To determine the molecular basis of CytlAa insect
   specificity we compared different steps of CytlAa mode of action in a
   susceptible insect as the dipteran Aedes aegypti and also in the
   nonsusceptible lepidopteran Manduca sexta. Our data shows that the lack
   toxicity of CytlAa to M. sexta larvae does not rely on protoxin
   processing, membrane binding interaction, and oligomerization of CytlAa
   since these steps were similar in the two insect species analyzed. (c)
   2013 Elsevier Inc. All rights reserved.}},
