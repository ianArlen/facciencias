@article{ ISI:000347551500002,
Author = {Rivera-Sanchez, Liliana and Odgers-Ortiz, Olga and Hernandez, Alberto},
Title = {{International migration and religious diversity in the State of Morelos,
   Mexico. A sociodemographic overview}},
Journal = {{PAPELES DE POBLACION}},
