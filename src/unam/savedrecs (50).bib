@article{ ISI:000332433900014,
Author = {Burns, Jordan N. and Acuna-Soto, Rodolfo and Stahle, David W.},
Title = {{Drought and Epidemic Typhus, Central Mexico, 1655-1918}},
Journal = {{EMERGING INFECTIOUS DISEASES}},
Year = {{2014}},
Volume = {{20}},
Number = {{3}},
Pages = {{442-447}},
Month = {{MAR}},
Abstract = {{Epidemic typhus is an infectious disease caused by the bacterium
   Rickettsia prowazekii and transmitted by body lice (Pediculus humanus
   corporis). This disease occurs where conditions are crowded and
   unsanitary. This disease accompanied war, famine, and poverty for
   centuries. Historical and proxy climate data indicate that drought was a
   major factor in the development of typhus epidemics in Mexico during
   1655-1918. Evidence was found for 22 large typhus epidemics in central
   Mexico, and tree-ring chronologies were used to reconstruct moisture
   levels over central Mexico for the past 500 years. Below-average tree
   growth, reconstructed drought, and low crop yields occurred during 19 of
   these 22 typhus epidemics. Historical documents describe how drought
   created large numbers of environmental refugees that fled the
   famine-stricken countryside for food relief in towns. These refugees
   often ended up in improvised shelters in which crowding encouraged
   conditions necessary for spread of typhus.}},
