@article{ ISI:000331622700008,
Author = {Wang, Feng and Wang, Hui and Tuan, Han-Fang and Nguyen, Duy H. and Sun,
   Vincent and Keser, Vafa and Bowne, Sara J. and Sullivan, Lori S. and
   Luo, Hongrong and Zhao, Ling and Wang, Xia and Zaneveld, Jacques E. and
