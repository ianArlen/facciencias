@article{ ISI:000335424600001,
Author = {Rodriguez, Luis F. and Masque, Josep M. and Dzib, Sergio A. and Loinard,
   Laurent and Kurtz, Stanley E.},
Title = {{NEW RADIO CONTINUUM OBSERVATIONS OF THE COMPACT SOURCE PROJECTED INSIDE
   NGC 6334A}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2014}},
Volume = {{50}},
Number = {{1}},
Pages = {{3-6}},
Month = {{APR}},
Abstract = {{A handful of HIT regions are known to exhibit a compact radio source
   near their centers. The nature of these compact radio sources is not
   well established. We present the analysis of new as well as archival
   Very Large Array observations of the compact source projected near the
   center of the NGC 6334A HIT region, part of the NGC 6334 complex. We
   show that the compact source is time variable on a scale of years and
   determine for one epoch a non-thermal spectrum, suggestive of
   synchrotron emission. We propose that this source could be the wind
   interaction region of a massive binary system that could be the ionizing
   source of NGC 6334A.}},
