@article{ ISI:000333106900006,
Author = {Guzman, F. S. and Lora-Clavijo, F. D. and Gonzalez-Aviles, J. J. and
   Rivera-Paleo, F. J.},
Title = {{Rotation curves of rotating Galactic Bose-Einstein condensate dark
   matter halos}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2014}},
Volume = {{89}},
Number = {{6}},
Month = {{MAR 5}},
Abstract = {{We present the dynamics of rotating Bose condensate Galactic dark matter
   halos, made of an ultralight spinless boson. We restrict to the case of
   adding axisymmetric rigid rotation to initially spherically symmetric
   structures and show there are three regimes: (i) small angular momentum,
   which basically retains the drawbacks of spherically symmetric halos
   related to compactness and failure at explaining Galactic rotation
   curves; (ii) an intermediate range of values of angular momentum that
   allow the existence of long-lived structures with acceptable rotation
   curve profiles; and (iii) high angular momentum, in which the structure
   is dispersed away by rotation. We also present in detail the new code
   used to solve the Gross-Pitaevskii Poisson system of equations in three
   dimensions.}},
