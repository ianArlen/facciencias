@article{ ISI:000343522600005,
Author = {Bezanilla, Alejandro and Krueger, Arne and Stremme, Wolfgang and
   Grutter, Michel},
Title = {{Solar absorption infrared spectroscopic measurements over Mexico City:
   Methane enhancements}},
Journal = {{ATMOSFERA}},
Year = {{2014}},
