@article{ ISI:000336875800002,
Author = {Barcelo Aspeitia, Axel Arturo},
Title = {{TRANS-WORLD CAUSATION REVISITED}},
Journal = {{CRITICA-REVISTA HISPANOAMERICANA DE FILOSOFIA}},
Year = {{2014}},
Volume = {{46}},
Number = {{136}},
Pages = {{27-41}},
Month = {{APR}},
Abstract = {{In a recent paper, Garcia-Ramirez (2012) has argued that Lewis'
   counterfactual analysis of causation has the undesirable consequence of
   making trans-world causation possible. In this paper I argue, against
   Garcia-Ramirez, that the possibility of trans-world causation cannot be
   derived from Lewis' account of in-world causation, since there is no way
   of extending Lewis' closeness relation among worlds into a similar
   closeness relation among pairs of worlds that is neither trivial nor
   ad-hoc.}},
