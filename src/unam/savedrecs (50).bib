@article{ ISI:000333446300002,
Author = {Bustamante, Victor H. and Calva, Edmundo},
Title = {{LeuO, a dormant sentinel for SPI-1?}},
Journal = {{MOLECULAR MICROBIOLOGY}},
Year = {{2014}},
Volume = {{91}},
Number = {{6}},
Pages = {{1054-1056}},
Month = {{MAR}},
Abstract = {{A new mechanism for the turning-off of gene expression in Salmonella
   Pathogenicity Island 1 (SPI-1) is proposed by Espinosa and Casadesus,
   which involves the action of the LeuO quiescent regulator, by two
   different pathways. A major one through the activation of the hilE gene,
   where the HilE protein would in turn inactivate HilD, a major positive
   transcriptional regulator of SPI-1; and a minor HilE-HilD-independent
   pathway. This could constitute a back-up or an aid for the turn-off of
   SPI-1 genes mediated by the nucleoid protein H-NS.}},
