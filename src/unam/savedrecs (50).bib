@article{ ISI:000331665200012,
Author = {Cruz-Rivera, Mayra and Vaughan, Gilberto and Mendlovic, Fela and
   Vergara-Castaneda, Arely and Romero-Valdovinos, Mirza and Leon-Cabrera,
   Sonia and Alonso, Monica and Avila, Guillermina and Flisser, Ana},
Title = {{Cytokine expression at the anchor site in experimental Taenia solium
   infection in hamsters}},
Journal = {{VETERINARY PARASITOLOGY}},
Year = {{2014}},
Volume = {{200}},
Number = {{3-4}},
Pages = {{299-302}},
Month = {{MAR 1}},
Abstract = {{The establishment of Taenia solium adult parasite in the human intestine
   causes taeniosis. Importantly, the immunological mechanisms occurring at
   the interface between the parasite and its host are not fully known. The
   development of experimental animal models has facilitated the
   understanding of the host-parasite relationship. In this study we
   standardized a quantitative RT-PCR method for analyzing hamster
   messenger RNA for interferon-gamma (IFN-gamma) and interleukins (IL):
   IL-4 IL-10, IL-12 and IL-13. This method was then used to evaluate the
   local cytokine response elicited against the adult parasite at the
   attachment site in the intestine of infected hamsters. The results
   showed an intense IFN-gamma response, as well as an up-regulation of
   IL-4 as early as three days post-infection, permanence of IL-10 until
   the end of the experiment and down regulation of IL-12. These data are
   in agreement with a bias toward a Th-2 response as the infection
   progresses. (C) 2014 Elsevier B.V. All rights reserved.}},
