@inproceedings{ ISI:000368592600056,
Author = {Aguirre-Aguirre, D. and Izazaga-Perez, R. and Villalobos-Mendoza, B. and
   Carrasco-Licea, E. and Granados-Agustin, F. S. and Percino-Zacarias, M.
   E. and Salazar-Morales, M. F. and Cruz-Zavala, E.},
