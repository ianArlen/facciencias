@inproceedings{ ISI:000372372700082,
Author = {Roca-Fabrega, S. and Figueras, F. and Valenzuela, O. and Romero-Gomez,
   M. and Antoja, T. and Colin, P. and Pichardo, B. and Velazquez, H.},
Editor = {{Walton, NA and Figueras, F and BalaguerNunez, L and Soubiran, C}},
Title = {{NOVEL KINEMATIC METHODS TO TRACE SPIRAL ARMS NATURE USING GAZA DATA}},
Booktitle = {{MILKY WAY UNRAVELLED BY GAIA: GREAT SCIENCE FROM THE GAIA DATA RELEASES}},
Series = {{EAS Publications Series}},
Year = {{2015}},
Volume = {{67-68}},
