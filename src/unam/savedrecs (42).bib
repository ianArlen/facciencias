@incollection{ ISI:000385021100009,
Author = {Jaramillo-Correa, Juan P. and Prunier, Julien and Vazquez-Lobo,
   Alejandra and Keller, Stephen R. and Moreno-Letelier, Alejandra},
Editor = {{Plomion, C and AdamBlondon, AF}},
Title = {{Molecular Signatures of Adaptation and Selection in Forest Trees}},
