@inproceedings{ ISI:000377420400045,
Author = {Torres, Lizeth and Verde, Cristina and Borges, Flavia and Magluta,
   Carlos and Roitman, Ney},
Book-Group-Author = {{ASME}},
Title = {{PARAMETER IDENTIFICATION OF A STEEL CATENARY RISER USING HILBERT AND
   WAVELET TRANSFORMS}},
Booktitle = {{PROCEEDINGS OF THE ASME 34TH INTERNATIONAL CONFERENCE ON OCEAN, OFFSHORE
   AND ARCTIC ENGINEERING, 2015, VOL 5B}},
Year = {{2015}},
Note = {{34th ASME International Conference on Ocean, Offshore and Arctic
   Engineering (OMAE2015), St John's, CANADA, MAY 31-JUN 05, 2015}},
Organization = {{Amer Soc Mech Engn, Ocean Offshore \& Arct Engn Div}},
Abstract = {{This article presents a methodology for Multiple Degree of Freedom
   (MDOF) systems to identify the modal parameters of a steel catenary
   riser (SCR) by employing both the Hilbert and the wavelet transforms. To
   validate the proposed approach, a set of experiments were performed in a
   small scale model of a real SCR that is installed in the P18 platform
   operating in Campos Basin, Brazil. During the experiments, the damping
   factor along the SCR was modified by adding viscoelastic sandwich layers
   on its structure. Each change was recorded with two accelerometers
   mounted at the bottom of the riser; one of them was set to get
   measurements in the catenary plane and the other one to get measurements
   out of the plane. Then, in order to obtain the impulse response of the
   structure from which is possible to obtain the modal parameters some
   hammer impacts were applied at locations close to these accelerometers.}},
Publisher = {{AMER SOC MECHANICAL ENGINEERS}},
Address = {{THREE PARK AVENUE, NEW YORK, NY 10016-5990 USA}},
Type = {{Proceedings Paper}},
Language = {{English}},
