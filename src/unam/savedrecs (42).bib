@inproceedings{ ISI:000380938801073,
Author = {Garcia, Silvia and Lopez-Molina, Jorge and Romo, Miguel},
Editor = {{Manzanal, D and Sfriso, AO}},
Title = {{Fast moving landslides simulation by cellular automata}},
Booktitle = {{FROM FUNDAMENTALS TO APPLICATIONS IN GEOTECHNICS}},
Year = {{2015}},
Pages = {{1373-1379}},
Note = {{15th Pan-American Conference on Soil Mechanics and Geotechnical
   Engineering (PCSMGE) / 8th South American Congress on Rock Mechanics
   (SCRM), Buenos Aires, ARGENTINA, NOV 15-18, 2015}},
Abstract = {{Cellular Automata CA are discrete computational models for simulating
   dynamic systems that evolve on the basis on local interactions. Some
   natural events, such as some types of landslides, fall into this type of
   phenomena and lend themselves well to be simulated with this approach.
   This paper describes a Cellular Automata model based on the equivalent
   fluid approach, specifically developed to simulate debris-flows type
   landslides. The methodology to approximately describe and model the main
   features of a debris flow, is developed for overcoming some of the
   limitation affecting conventional frames. The presented CA model for
   debris flow is based on the principle of the equivalent fluid, stating
   that: ``the flowing mass behaves like a fluid, whose rheological
   features cannot be measured through laboratory or in situ testing, but
   can only be obtained by the back-analysis of real past events{''}. A
   general description of the adopted approach will be given in this paper,
   together with the results of their application to real cases.}},
