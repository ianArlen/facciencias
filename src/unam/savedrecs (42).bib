@article{ ISI:000369403800015,
Author = {Espejel-Ayala, F. and Solis-Lopez, M. and Schouwenaars, R. and
   Ramirez-Zamora, R. M.},
Title = {{SYNTHESIS OF ZEOLITE P USING COPPER MINING TAILINGS}},
