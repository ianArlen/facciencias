@inproceedings{ ISI:000377585500005,
Author = {Torres-Peimbert, Silvia},
Editor = {{Cunningham, BA and ORiordan, C and Ghose, S}},
Title = {{Chemical Composition of Planetary Nebulae: The Abundance Discrepancy
   Problem}},
Booktitle = {{WOMEN IN PHYSICS: 5TH IUPAP INTERNATIONAL CONFERENCE ON WOMEN IN PHYSICS}},
Series = {{AIP Conference Proceedings}},
Year = {{2015}},
Volume = {{1697}},
Note = {{5th IUPAP International Conference on Women in Physics, Wilfrid Laurier
   Univ, Waterloo, CANADA, AUG 05-08, 2014}},
Organization = {{IUPAP Working Grp Women Phys; Canadian Assoc Physicists; Laurier Ctr
   Women Sci}},
Abstract = {{It is important to determine the composition of the ejecta of
   intermediate-mass stars to understand the internal processes of the
   parent star, as well as to establish the chemical evolution of the
   Galaxy. Traditionally, the compositions of gaseous nebulae have been
   determined from bright, collisionally excited lines, which are
   relatively easy to measure. However, a persistent problem exists: a
   discrepancy between the abundances determined from collisionally excited
   lines and abundances determined from faint recombination lines. The
