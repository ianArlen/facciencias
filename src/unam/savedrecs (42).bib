@inproceedings{ ISI:000382950700052,
Author = {Murrieta-Rico, Fabian N. and Tchernykh, Andrei and Petranovskii, Vitalii
   and Raymond-Herrera, Oscar and Sergiyenko, Oleg and Flores-Fuentes,
   Wendy and Rodriguez-Quinonez, Julio C. and Hernandez-Balbuena, Daniel
   and Nieto Hipolito, Juan Ivan and Tyrsa, Vera and Kartashov, Vladimir M.},
Book-Group-Author = {{IEEE}},
Title = {{Rational Approximations Principle for Frequency Shifts Measurement in
   Frequency Domain Sensors}},
Booktitle = {{IECON 2015 - 41ST ANNUAL CONFERENCE OF THE IEEE INDUSTRIAL ELECTRONICS
   SOCIETY}},
Series = {{IEEE Industrial Electronics Society}},
Year = {{2015}},
Pages = {{226-231}},
Note = {{41st Annual Conference of the IEEE-Industrial-Electronics-Society
   (IECON), Yokohama, JAPAN, NOV 09-12, 2015}},
Organization = {{IEEE Ind Elect Soc; SICE; Robot Soc Japan; IEEJ; IEEJ Cias; JSPE; JSME;
   EiC; SAE Japan}},
Abstract = {{Frequency domain sensors (FDS) are important elements in control, data
   acquisition and monitoring systems. Such sensors have some outstanding
   characteristics like output of quasi-digital signals, high sensitivity,
   high resolution, wide dynamic range, anti-interference capacity and good
   stability. A FDS converts a physical variable (measurand) into a
   frequency domain ouput. When the measurand changes, the output has a
   proportional frequency shift. In systems that use FDS, measuring the
   frequency shift is desirable. Accuracy of most frequency measurement
   techniques is limited by measurement time, and if more precision is
   required, longer times for measuring are needed. In this work, a novel
   approach using the rational approximations principle for measuring
   frequency shift in the output of a FDS is introduced. Also algorithms
   for simulating the mathematical model of frequency measurement process
   are proposed, and resolution of measurement is improved by analyzing the
   data obtained.}},
