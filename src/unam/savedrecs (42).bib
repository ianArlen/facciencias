@incollection{ ISI:000376189800007,
Author = {Valdes-Lopez, Oswaldo and Hernandez, Georgina},
Editor = {{Bais, H and Sherrier, J}},
Title = {{Early Responses in the Root-Rhizobia Interaction}},
Booktitle = {{PLANT MICROBE INTERACTIONS}},
Series = {{Advances in Botanical Research}},
Year = {{2015}},
Volume = {{75}},
Pages = {{137-147}},
Abstract = {{The symbiotic interaction between legumes and nitrogen-fixing bacteria
   has a high relevance at ecological and agronomical level. This
   mutualistic interaction has been extensively studied, over the years.
   However, recent experimental evidence suggests that there are many gaps
   that have to be filled-up. For example, large-scale studies indicate the
   existence of several yet-uncharacterized regulators of the very early
   events of the legume-rhizobia interaction. In addition, it has been
   reported that microRNAs play a critical role in the regulation of the
   early responses. Likewise, different studies suggest that a very
   critical step to establish this symbiosis is the negative regulation of
   the plant innate immunity. In this chapter, we will summarize and
   discuss the most recent discoveries on the control of the early
   responses of the symbiosis between legumes and nitrogen-fixing bacteria.}},
