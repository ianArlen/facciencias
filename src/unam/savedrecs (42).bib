@inproceedings{ ISI:000380440800116,
Author = {Banuelos-Saucedo, Miguel A.},
Book-Group-Author = {{IEEE}},
Title = {{Compensation of THz Spectrum Spurious Oscillations by Local Apodization}},
Booktitle = {{2015 IEEE SENSORS}},
