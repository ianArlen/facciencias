@inproceedings{ ISI:000380938800024,
Author = {Beltran, Gloria and Romo, Miguel},
Editor = {{Manzanal, D and Sfriso, AO}},
Title = {{Evaluation of structural indices inferred from nondestructive deflection
   tests on pavements}},
Booktitle = {{FROM FUNDAMENTALS TO APPLICATIONS IN GEOTECHNICS}},
Year = {{2015}},
Pages = {{196-203}},
Note = {{15th Pan-American Conference on Soil Mechanics and Geotechnical
   Engineering (PCSMGE) / 8th South American Congress on Rock Mechanics
   (SCRM), Buenos Aires, ARGENTINA, NOV 15-18, 2015}},
Abstract = {{Based on measured responses of pavements to impact deflection tests, it
   is possible to estimate both layer moduli and some additional indices of
   empirical nature, proposed as stiffness classifiers of pavement. It was
   found that pavement systems with different features and diverse
   deflection responses, could lead to similar values in each parameter
   derived from deflection basins, making it difficult to select the
   pavement property they are supposed to be linked to. Detailed
   evaluations of these empirical indices, related to basin deflection
   shape are carried out in this study, taking into account different
   pavement systems and deflection responses. The results of the
   comprehensive analysis carried out, allow formulating some
   recommendations to select the most appropriate indicators to assess the
   structural adequacy of pavements.}},
