@incollection{ ISI:000385704500002,
Author = {Garduno-Monroy, V. H. and Macias, J. L. and Molina Garza, R. S.},
Editor = {{Scolamacchia, T and Macias, JL}},
Title = {{Geodynamic Setting and Pre-volcanic Geology of Active Volcanism in
   Chiapas}},
Booktitle = {{ACTIVE VOLCANOES OF CHIAPAS (MEXICO): EL CHICHON AND TACANA}},
Series = {{Active Volcanos of the World}},
Year = {{2015}},
Pages = {{1-23}},
Abstract = {{El Chichon and Tacano, two active volcanoes in the southeastern Mexico
   Chiapas state, illustrate the complexity of the magmatic systems in the
   Middle America Trench. Tacana represents the northwestern end of the
   Central American Volcanic Arc, whilst El Chichon is the northernmost
   volcano of the Modern Chiapanecan Volcanic Arc. Tacana was built on
   Mesozoic metamorphic and Tertiary intrusive rocks (35-13 Ma) and
   deposits from mid-Pleistocene calderas (1-2 Ma). El Chichon was built on
   folded Cretaceous to Miocene sedimentary rocks, established similar to
   150 km north of the position of a Miocene arc that existed in the
   southernmost Maya block. Both the Central American Volcanic Arc and the
   Modern Chiapanecan Arc have been interpreted in terms of
   supra-subduction zones of magmatism associated with the subduction of
   the Cocos Plate beneath the Caribbean and North America plates. A
   well-defined Wadati-Benioff Zone supports such model for the Central
   American Volcanic Arc. Nevertheless, available data indicate that the
   subducted Cocos Plate does not reach the mantle region below the Modern
   Chiapanecan Volcanic Arc. El Chichon and the other volcanoes of the
   Chiapanecan Volcanic Arc are situated some 200 km above the down dip
   projection of the slab over a region that, based on seismic tomography,
   can be interpreted as a slab gap or an anomalous hot region of the
   mantle.}},
