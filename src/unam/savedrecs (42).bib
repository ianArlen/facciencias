@inproceedings{ ISI:000376685705103,
Author = {Mata Hernandez, G.},
Editor = {{GomezChova, L and LopezMartinez, A and CandelTorres, I}},
Title = {{METHODOLOGY BASED ON UNITS OF INTERACTIVE LEARNING}},
Booktitle = {{EDULEARN15: 7TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
Series = {{EDULEARN Proceedings}},
Year = {{2015}},
Pages = {{5662-5669}},
Note = {{7th International Conference on Education and New Learning Technologies
   (EDULEARN), Barcelona, SPAIN, JUL 06-08, 2015}},
Abstract = {{There are subjects in undergraduate courses in Electrical Electronic
   (EE), Computing (C), and Telecommunications (T) Engineering, in which,
   instead of promoting the student interest, attraction and motivation,
   are considered difficult, heavy, with lots of mathematical analysis, and
   sometimes making no sense for them. Frequently arise questions such as:
   what is the use of this? Where am I going to use it? If clear and
   convincing answers are not given, they are discouraged. It is easy for
   them to lose the sequence in the subjects and the interest in the
   subjects, and investing the minimum effort to approve it either way or
   even leave the course.
   To overcome this situation a methodology based on units of interactive
   learning (UIL) has been developed, conceived as a set of content,
   resources and educational multimedia materials, that strategically link
   theory, simulation and practice across a wide range of resources and
