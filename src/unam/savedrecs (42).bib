@inproceedings{ ISI:000366570200059,
Author = {Velasco-Segura, Roberto and Rendon, Pablo L.},
Editor = {{BlancBenon, P and Sparrow, VW and Dragna, D}},
Title = {{Numerical Modelling of Nonlinear Full-Wave Acoustic Propagation}},
Booktitle = {{RECENT DEVELOPMENTS IN NONLINEAR ACOUSTICS}},
Series = {{AIP Conference Proceedings}},
Year = {{2015}},
Volume = {{1685}},
Note = {{20th International Symposium on Nonlinear Acoustics (ISNA) including the
   2nd International Sonic Boom Forum (ISBF), Ecole Centrale Lyon, Lyon,
   FRANCE, JUN 29-JUL 03, 2015}},
Organization = {{Acoust Soc Amer; Int Commiss Acoust; Gulfstream Aerosp Corp; Lab
   Mecanique Fluides Acoustique; Soc Franvaise Acoustique; Ctr Lyonnais
   Acoustique}},
Abstract = {{The various model equations of nonlinear acoustics are arrived at by
   making assumptions which permit the observation of the interaction with
   propagation of either single or joint effects. We present here a form of
   the conservation equations of fluid dynamics which are deduced using
   slightly less restrictive hypothesis than those necessary to obtain the
   well known Westervelt equation. This formulation accounts for full wave
   diffraction, nonlinearity, and thermoviscous dissipative effects. A
   two-dimensional, finite-volume method using Roe's linearisation has been
   implemented to obtain numerically the solution of the proposed
   equations. This code, which has been written for parallel execution on a
   GPU, can be used to describe moderate nonlinear phenomena, at low Mach
   numbers, in domains as large as 100 wave lengths. Applications range
   from models of diagnostic and therapeutic HIFU, to parametric acoustic
   arrays and nonlinear propagation in acoustic waveguides. Examples
   related to these applications are shown and discussed.}},
