@inproceedings{ ISI:000380938800059,
Author = {Ovando-Shelley, Efrain and Rodriguez, Miguel and Flores-Castrellon,
   Osvaldo and Flores-Guzman, Mario},
Editor = {{Manzanal, D and Sfriso, AO}},
Title = {{Comparison of laboratory and field techniques to measure shear wave
   velocity in Texcoco Lake Clay}},
Booktitle = {{FROM FUNDAMENTALS TO APPLICATIONS IN GEOTECHNICS}},
Year = {{2015}},
Pages = {{485-492}},
Note = {{15th Pan-American Conference on Soil Mechanics and Geotechnical
   Engineering (PCSMGE) / 8th South American Congress on Rock Mechanics
   (SCRM), Buenos Aires, ARGENTINA, NOV 15-18, 2015}},
Abstract = {{We analyse shear wave velocities obtained from field tests performed at
   a site in the former Texcoco Lake, some 15 km north east of central
   Mexico City. The objective of our study was to evaluate shear wave
   velocity profiles obtained by combining the Microtremor Analysis Method
   (MAM) and the Multichannel Analysis of Surface Waves (MASW). We compare
   these profiles with those obtained using the suspension logging
   technique and with a seismic Marchetti dilatometer. The results of
   laboratory tests (resonant column and bender element tests) are also
   brought into our discussion. Field and laboratory data show that the
   shear wave velocity values obtained with the experimental techniques
   presented here are scattered within a relatively narrow range. Regarding
   the combined MAM-MASW method, our results show that this method reliably
   provides shear wave velocity values that closely resemble those obtained
   with the other methods down to about 40 m. The MAM-MASW is cheaper, it
   is less time consuming and it requires a modest investment in equipment.
   Nevertheless, a high degree of expertise is required to interpret and
   analyse the time signals obtained here.}},
