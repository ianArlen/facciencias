@article{ ISI:000384431500009,
Author = {Rincon Becerra, Ovidio and Garcia Acosta, Gabriel},
Title = {{Determining anthropometric dimensions for the production of gloves
   according to design requirements}},
Journal = {{ICONOFACTO}},
