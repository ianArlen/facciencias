@article{ ISI:000375842300004,
Author = {Vargas, Alejandro and Moreno, Jaime A.},
Title = {{On-line maximization of biogas production in an anaerobic reactor using
   a pseudo-super-twisting controller}},
Journal = {{IFAC PAPERSONLINE}},
Year = {{2015}},
Volume = {{48}},
Number = {{8}},
Pages = {{14-19}},
Note = {{9th IFAC Symposium on Advanced Control of Chemical Processes ADCHEM
   2015, Whistler, CA, JUN 07-10, 2015}},
Abstract = {{We consider an apparently oversimplified first order model of an
   anaerobic digester operated as a CSTR, where the dilution rate is the
   controlled input and the biogas production rate is the measured output.
   The parameters of this model are considered slowly time-varying. The
   output function depends on the only state (the substrate), and at any
   instant has a unique maximum. We propose a simple output-feedback
   controller based on the super-twisting algorithm combined with a state
   machine, which converges in a practical sense to this maximum. The
   controller was tested by simulations of an anarobic digester, maximizing
   the biogas production rate, showing very good results. (C) 2015, IFAC
   (International Federation of Automatic Control) Hosting by Elsevier Ltd.
   All rights reserved.}},
