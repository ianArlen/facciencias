@article{ ISI:000381173300022,
Author = {Roig, Pablo and Jose Sanz-Cillero, Juan},
Title = {{Assessing the accuracy of the Brodsky-Lepage prediction}},
Journal = {{NUCLEAR AND PARTICLE PHYSICS PROCEEDINGS}},
Year = {{2015}},
Volume = {{258}},
Pages = {{98-101}},
Month = {{JAN-FEB}},
