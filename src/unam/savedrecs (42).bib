@inproceedings{ ISI:000380938801034,
Author = {Vargas, Omar and Ortiz, Ricardo and Flores, Francisco},
Editor = {{Manzanal, D and Sfriso, AO}},
Title = {{Liquefaction Analysis Using Pore Pressure Generation Models During
   Earthquakes}},
Booktitle = {{FROM FUNDAMENTALS TO APPLICATIONS IN GEOTECHNICS}},
Year = {{2015}},
Pages = {{1057-1064}},
Note = {{15th Pan-American Conference on Soil Mechanics and Geotechnical
   Engineering (PCSMGE) / 8th South American Congress on Rock Mechanics
   (SCRM), Buenos Aires, ARGENTINA, NOV 15-18, 2015}},
Abstract = {{One of the main objectives of geotechnical earthquake engineering is the
   evaluation of liquefaction hazards or liquefaction potential and
   failures induces by this phenomenon. Commonly the professional practice
   for liquefaction problems involves three aspects: 1) Triggering
   analysis, 2) Stability analysis or flow slide and 3) displacement
   analysis. Currently, empirical and analytical methods are commonly used
   for triggering and displacement analysis, which are based on field test
   parameters. However, those methods have limitations because they treat
   sequentially the three aspects for liquefaction analysis, when actually
   these interact locally in several zones affecting the overall behavior
   of the soil mass. According to the above, in this work has performed a
   liquefaction analysis for a site using two-dimensional numerical models
   that take into account the irregular ground and the constitutive models
   that involve the dynamic pore-pressure generation from volumetric strain
   induced by seismic excitation. This paper highlights the means
   considerations of numeric model, as such as, input parameters, model
   calibration, steps calculations, among other. Moreover, model results
   are compared with simplified empirical methods and field measurements.}},
