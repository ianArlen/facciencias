@inproceedings{ ISI:000378634700041,
Author = {Mejia-Munoz, J. and Flores-Espinosa, C. and Pena-Ortega, G. and
   Alvarado-Cano, M. E. and Gaspar-Hernandez, R. and Reyes-Santiago, J.},
Editor = {{VanHuylenbroeck, J and Dhooghe, E}},
Title = {{Interspecific Hybridization between Dahlia dissecta and D. rupicola}},
Booktitle = {{XXV INTERNATIONAL EUCARPIA SYMPOSIUM SECTION ORNAMENTALS: CROSSING
   BORDERS}},
Series = {{Acta Horticulturae}},
