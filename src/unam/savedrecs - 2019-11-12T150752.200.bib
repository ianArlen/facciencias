@article{ ISI:000089520300009,
Author = {Hirsch, JG and Vargas, CE and Draayer, JP},
Title = {{Low energy spectra of A=159 and 161 nuclei}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2000}},
Volume = {{46}},
Number = {{1}},
Pages = {{54-59}},
Month = {{SEP}},
Note = {{23rd Symposium on Nuclear Physics, INST MEXICANO SEGURO SOCIAL,
   OAXTEPEC, MEXICO, JAN 10-13, 2000}},
Organization = {{Univ Nacl Autonoma Mexico, Inst Fis; Univ Nacl Autonoma Mexico, Inst
   Ciencias Nucl; Ctr Invest \& Estudios Avanzados Inst Politecn Nacl; Soc
   Mexicana Fis; Consejo Nacl Ciencia \& Technol; Ctr Latinoamer Fis}},
Abstract = {{The pseudo SU(3) model is used to describe the low-energy spectra of
   Tb-159, Dy-159, Eu-159, Tb-161 and Tm-161 and B(E2) electromagnetic
   transition strengths in Tb-159. Th, building blocks of the model are the
   SU(3) proton and neutron states having pseudo spin zero and 1/2, which
   describe even and odd number of nucleons, respectively. The
   many-particle states are built as SU(3) coupled states with a
   well-defined particle number and total angular momentum. The Hamiltonian
   includes spherical NiIsson single-particle energies, the
   quadrupole-quadrupole and pairing interactions, as well as two rotor
   terms which are diagonal in the SU(3) basis. As expected, the use of
   realistic single-particle energies plays a key role in an appropriate
   description of these nuclei. The interactions are microscopic, and
   closely related to those known to be the most relevant for the
   low-energy spectra.}},
