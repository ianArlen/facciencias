@article{ ISI:000088197400001,
Author = {Xiao, MF},
Title = {{Rigorous solution for transient propagation of electromagnetic waves
   through a medium: causality plus diffraction in time}},
Journal = {{OPTICS LETTERS}},
Year = {{2000}},
Volume = {{25}},
Number = {{14}},
Pages = {{995-997}},
Month = {{JUL 15}},
Abstract = {{A rigorous solution for transient propagation of electromagnetic waves
   through a medium has been found. Because of this rigorousness, the
   solution exhibits apparent consistency with Einsteinian causality. Thus,
   it is confirmed that faster-than-light or superluminal propagation of
   electromagnetic waves is not possible. Evanescent transmission gives
   rise to diffraction in time, which is the actual reason for deformation
   of group propagation. (C) 2000 Optical Society of America OCIS codes:
   350.5500, 260.2110.}},
