@article{ ISI:000088716300010,
Author = {Calixto-Albarran, I and Osorno, JL},
Title = {{The diet of the Magnificent Frigatebird during chick rearing}},
Journal = {{CONDOR}},
Year = {{2000}},
Volume = {{102}},
Number = {{3}},
Pages = {{569-576}},
Month = {{AUG}},
Abstract = {{We describe the diet of the Magnificent Frigatebird Fregata magnificens
   at Isla Isabel off the Pacific coast of Mexico based on 158 regurgitates
   (555 prey items) from males, females, chicks, and juveniles. The diet
   included 50 species of fishes (21 identified to genus only), 1 species
   of squid, and 2 species of crustaceans. Diplectum pacificum and Anchoa
   lucida were the most frequent species of fish in the frigatebird's diet.
   No differences in prey composition or prey size were found between males
   and females, but females disgorged 62\% more food. This difference
   mirrors the bigger size (15\% larger than males) and larger contribution
   of females to chick feeding. Flying juveniles consumed prey in different
   proportions than the adults and disgorged marginally more food.
   Juveniles may obtain food in different patches than adults and
   complement this source with maternal feedings, or they may be more
   likely to regurgitate. Diet composition of males, females, and dying
   juveniles changed during the 4-month period of this study, but the mass
   of regurgitates did not change as the breeding season progressed,
   suggesting that availability of prey species changes over time. Most of
   the diet of this population probably comes from opportunistic feeding on
   fisheries, because the variety of fishes disgorged is remarkably similar
   to the published list of fishes discarded by prawn-fishing boats in the
   area. If our assumptions true, kleptoparasitism and direct fishing are
   only marginally represented in the diet of this population.}},
Publisher = {{COOPER ORNITHOLOGICAL SOC}},
