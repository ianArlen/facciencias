@article{ ISI:000089520300012,
Author = {Draayer, JP and Popa, G and Hirsch, JG and Vargas, C},
Title = {{M1 strengths (scissors and twist modes) in heavy deformed nuclei
   (Gd156-160)}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2000}},
Volume = {{46}},
Number = {{1}},
Pages = {{71-76}},
Month = {{SEP}},
Note = {{XXIIIrd Symposium on Nuclear Physics, INST MEXICANO SEGURO SOCIAL,
   OAXTEPEC, MEXICO, JAN 10-13, 2000}},
Organization = {{Univ Nacl Autonoma Mexico, Inst Fis; Univ Nacl Autonoma Mexico, Inst
   Ciencias Nucleares; Ctr Invest \& Estudios Avanzados Inst Politecnico
   Nacl; Soc Mexicana Fis; Consejo Nacl Cienciy \& Technol; Ctr Latinoamer
   Fis}},
Abstract = {{The Elliott SU(3) Model, extended for heavy nuclei using pseudo-spin, is
   used to study low-lying magnetic dipole excitations in deformed nuclei.
   The Hamiltonian includes single-particle energies as well as
   quadrupole-quadrupole and pairing interaction terms. Since the
   quadrupole-quadrupole interaction dominates, a strong SU(3)-dictated
   truncation of the model space is used. With interaction strengths fixed
   by systematics, good agreement with experimental results is found for
   the excitation and M1 transition spectra of the well-deformed Gd156-160
   isotopes.}},
