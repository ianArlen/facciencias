@article{ ISI:000088008400057,
Author = {Takeuchi, N},
Title = {{First-principles calculations of the Si(110)(2X3)Sb surface}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2000}},
Volume = {{61}},
Number = {{24}},
Pages = {{16704-16707}},
Month = {{JUN 15}},
Abstract = {{Using first-principles total-energy calculations, we have studied the
   energetics and structural properties of the (2x3) reconstruction that Sb
   forms on the (110) surface of Si. Three different models were examined.
   The first corresponds to a coverage of In monolayer (ML) of Sb and
   consists of four Sb atoms saturating all the 12 Si dangling bonds of the
   unit cell. The second and third models are, formed by Sb trimers in
   milk-stool positions. They correspond to coverages of 1/2 and 1 ML of
   Sb, respectively. The 1 ML structure is formed by four trimers per unit
   cell, saturating all Si dangling bonds. The 1/2 ML model is formed by
   two Sb trimers per unit cell. leaving six Si dangling bonds unsaturated.
   Our calculations show that only the 1/3 and 1 ML phases are stable.}},
