@article{ ISI:000088710200017,
Author = {Martinez, A and Rodriguez, ME and York, SW and Preston, JF and Ingram,
   LO},
Title = {{Use of UV absorbance to monitor furans in dilute acid hydrolysates of
   biomass}},
Journal = {{BIOTECHNOLOGY PROGRESS}},
Year = {{2000}},
Volume = {{16}},
Number = {{4}},
Pages = {{637-641}},
Month = {{JUL-AUG}},
Abstract = {{A simple method based on UV spectra was developed for the estimation of
   total furans (furfural and hydroxymethylfurfural) in hemicellulose
   hydrolysates. UV spectra of hemicellulose hydrolysate contained a single
   dominant peak at around 278 nm. Approximately two-thirds of this peak
   can be attributed to furan absorbance (furfural and
   hydroxymethylfurfural). At 284 nn, both furfural and
   hydroxymethylfurfural have equal absorbance on a weight basis. A
   comparison of HPLC determinations for different samples of hydrolysate
   was used to develop a simple equation that allows the accurate
   prediction of total furans based on the difference in absorbance at 284
   and 320 nm. This method may prove useful for quality control
   applications during the production of biomass syrups using a dilute acid
   hydrolysis process and during treatments for the amelioration of toxins.
   Although furans represent only a portion of the toxins present in
   hemicellulose hydrolysates, the abundance of furans appears to serve as
   a useful marker to predict relative toxicity.}},
