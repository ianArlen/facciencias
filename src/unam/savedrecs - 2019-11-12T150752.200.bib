@article{ ISI:000088768800104,
Author = {Alvarez-Fregoso, O and Lopez, S and Juarez-Islas, JA and Garcia, M and
   Martinez, E and Alvarez-Perez, MA and Ramirez, JC and Granados, S},
Title = {{Cu/Mo nanostructured alloy thin films}},
