@article{ ISI:000165452900009,
Author = {Donnelly, P and Rodrigues, ER},
Title = {{Convergence to stationarity in the Moran model}},
Journal = {{JOURNAL OF APPLIED PROBABILITY}},
Year = {{2000}},
Volume = {{37}},
Number = {{3}},
Pages = {{705-717}},
Month = {{SEP}},
Abstract = {{Consider a population of fixed size consisting of N haploid individuals.
   Assume that this population evolves according to the two-allele neutral
   Moran model in mathematical genetics. Denote the two alleles by A(1) and
   A(2). Allow mutation from one type to another and let 0 < <gamma> < 1 be
   the sum of mutation probabilities. All the information about the
   population is recorded by the Markov chain X = (X(t)(t<greater than or
   equal to>0) which counts the number of individuals of type At. In this
   paper we study the time taken for the population to `reach' stationarity
   tin the sense of separation and total, variation distances) when
   initially all individuals are of one type. We show that after t{*} = N
   gamma (-1) log N + c N the separation distance between the law of
   X(t{*}) and its stationary distribution converges to 1 -
   exp(-gammae(-gamma c)) as N --> infinity. For the total variation
   distance an asymptotic upper bound is obtained. The results depend on a
   particular duality, and couplings, between X and a genealogical process
   known as the lines of descent process.}},
