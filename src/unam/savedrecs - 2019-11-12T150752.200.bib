@article{ ISI:000089040800010,
Author = {Plana, H and Amram, P and de Oliveira, CM and Balkowski, C},
Title = {{Kinematics and morphology of ionized gas in Hickson compact group 18}},
Journal = {{ASTRONOMICAL JOURNAL}},
Year = {{2000}},
Volume = {{120}},
Number = {{2}},
Pages = {{621-629}},
Month = {{AUG}},
Abstract = {{We present new observations of Her emission in the Hickson compact group
   18 (HCG 18), obtained with a scanning Fabry-Perot interferometer. The
   velocity field does not show motions of individual group members but
   instead a complex common velocity field for the whole group. The gas
   distribution is very asymmetric, with clumps of maximum intensity
   coinciding with the optically brightest knots. Comparing H alpha and H I
   data, we conclude that HCG 18 is not a compact group but instead a large
   irregular galaxy with several star-forming clumps.}},
