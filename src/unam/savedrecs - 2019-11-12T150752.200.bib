@article{ ISI:000088592600010,
Author = {Gonzalez-Martinez, S and Lippert-Heredia, E and Hernandez-Esparza, M and
   Doria-Serrano, C},
Title = {{Reactor kinetics for submerged aerobic biofilms}},
Journal = {{BIOPROCESS ENGINEERING}},
Year = {{2000}},
Volume = {{23}},
Number = {{1}},
Pages = {{57-61}},
Month = {{JUL}},
Abstract = {{Due to water shortage in cities, small wastewater treatment plants are
   needed and new technologies are demanded. Pilot plant experiments were
   made to evaluate the performance of a submerged aerobic biofilm system
   for the treatment of municipal wastewater. Influent flow rates were
   varied to adjust organic loading rates from 5.2 to 28.8 gCOD/m(2) d.
   Using COD values, the pilot plant performance was analyzed for kinetics
   simulation. The Stover-Kincannon and a general variable-order models
   were used to analyze the data obtained over several months of
   experimentation. The data showed good correlation with the
   Stover-Kincannon model but not with the variable-order model. The
   results lead to a design organic load of 20 gCOD/m(2) d to reach an
   effluent COD under 50 mg/l.}},
