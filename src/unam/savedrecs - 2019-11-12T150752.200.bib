@article{ ISI:000088200600004,
Author = {Illanes, A and Martinez-de-la-Vega, V},
Title = {{Product topology in the hyperspace of subcontinua}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2000}},
Volume = {{105}},
Number = {{3}},
Pages = {{305-317}},
Month = {{AUG 18}},
Abstract = {{Let X be a metric continuum. Let C(X) be the hyperespace of subcontinua
   of X. Given two finite subsets P and Q of X, let U(P, Q) = \{A is an
   element of C(X): P subset of A and A boolean AND Q = 0\}. In this paper
   we consider C(X) with the topology tau(P) which have the sets U(P, Q) as
   a basis. In this paper we show that, for a dendroid X, some topological
   properties of X are very closely related to the topological structure of
   (C(X), tau(P)). (C) 2000 Elsevier Science B.V. All rights reserved.}},
