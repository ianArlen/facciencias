@article{ ISI:000089520300013,
Author = {Hess, PO and Misicu, S and Greiner, W},
Title = {{How do trinuclear molecules rotate and vibrate?}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2000}},
Volume = {{46}},
Number = {{1}},
Pages = {{77-83}},
Month = {{SEP}},
Note = {{XXIIIrd Symposium on Nuclear Physics, INST MEXICANO SEGURO SOCIAL,
   OAXTEPEC, MEXICO, JAN 10-13, 2000}},
Organization = {{Univ Nacl Autonoma Mexico, Inst Fis; Univ Nacl Autonoma Mexico, Inst
   Ciencias Nucleares; Ctr Invest \& Estudios Avanzados Inst Politecnico
   Nacl; Soc Mexicana Fis; Consejo Nacl Cienciy \& Technol; Ctr Latinoamer
   Fis}},
Abstract = {{A review is given about a geometrical model for tri-cluster molecules.
   An analytical solution is obtained provided the nuclei are connected in
   line to each other. Furthermore, the tri-nuclear molecule is composed of
   two heavy and one light cluster, the latter sandwiched between the two
   heavy clusters. In the calculation of the interaction between the
   clusters higher multipole deformations are taken into account, including
   the hexadecupole one. A repulsive nuclear core is introduced in the
   potential in order to ensure a quasi-stable configuration of the system.
   The model is applied to two nuclear molecules, namely Sr-96 + Be-10 +
   Ba-146, and Sr-96 + C-12 + Xe-144, where the last system has not been
   investigated yet. As a result we conclude that the influence of the
   deformation on the spectrum is negligible and only plays a role in
   determining the relative distance of the center of masses. There are few
   band heads at low energy and none below 1 MeV. The rotational band above
   each band head is strongly squeezed due to the large moment of inertia
   of the system.}},
