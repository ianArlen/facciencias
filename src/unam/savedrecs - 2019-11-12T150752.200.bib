@article{ ISI:000166277700009,
Author = {Gomez, Y and Rodriguez-Rico, CA and Rodriguez, LF and Garay, G},
Title = {{A new warm molecular clump toward the star forming region G34.26+0.15}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2000}},
Volume = {{36}},
Number = {{2}},
Pages = {{161-169}},
Month = {{OCT}},
Abstract = {{In this work we reanalyze VLA ammonia (2,2) and (3,3) observations
   toward the massive star forming region G34.26+0.15. We find one new
   ammonia clump with (2,2) and (3,3) emission toward the northwest in
   addition to the main molecular structure previously reported. The new
   clump is associated with water maser spots and we estimate for it a
   rotational temperature of similar to 54 +/- 12 K, a diameter of 0.15 pc
   (assuming a distance of 3.8 kpc) and a molecular mass of similar to 10
   M. (assuming an {[}H-2/NH3] ratio of 10(7)). These results suggest that
   the clump could have an embedded star and that in addition to the
   massive stars that excite the H II regions previously reported, there
   may be other young stars of lower mass in the region.}},
