@article{ ISI:000088319600078,
Author = {Santamaria, RI and Reyes-Duarte, MD and Barzana, E and Fernando, D and
   Gama, FM and Mota, M and Lopez-Munguia, A},
Title = {{Selective enzyme-mediated extraction of capsaicinoids and carotenoids
   from chili guajillo puya (Capsicum annuum L.) using ethanol as solvent}},
Journal = {{JOURNAL OF AGRICULTURAL AND FOOD CHEMISTRY}},
Year = {{2000}},
Volume = {{48}},
Number = {{7}},
Pages = {{3063-3067}},
Month = {{JUL}},
Abstract = {{The selective extraction of capsaicinoids and carotenoids from chili
   guajillo ``puya{''} flour was studied.. When ethanol was used as
   solvent, 80\% of capsaicinoids and 73\% of carotenoids were extracted,
   representing an interesting alternative for the substitution of hexane
   in industrial processes. Additionally, when the flour was pretreated
   with enzymes that break the cell wall and then dried, extraction in
   ethanol increased to 11 and 7\% for carotenoid and capsaicinoid,
   respectively. A selective two-stage extraction process after the
   treatment with enzymes is proposed. The first step uses 30\% (v/v)
   ethanol and releases up to 60\% of the initial capsaicinoids, and the
   second extraction step with industrial ethanol permits the recovery of
   83\% of carotenoids present in the flour.}},
Publisher = {{AMER CHEMICAL SOC}},
Address = {{1155 16TH ST, NW, WASHINGTON, DC 20036 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Lopez-Munguia, A (Reprint Author), Univ Nacl Autonoma Mexico, Fac Quim, Cd Univ, Mexico City 04510, DF, Mexico.
   Univ Nacl Autonoma Mexico, Fac Quim, Mexico City 04510, DF, Mexico.
