@article{ ISI:A1997WX75200027,
Author = {Tapia, M and Persi, P and Bohigas, J and FerrariToniolo, M},
Title = {{IRAS sources associated with small nebulae in star forming regions:
   Optical and near infrared images'}},
Journal = {{ASTRONOMICAL JOURNAL}},
Year = {{1997}},
Volume = {{113}},
Number = {{5}},
Pages = {{1769+}},
Month = {{MAY}},
Abstract = {{Optical CCD and near-IR broad and narrow-band imaging have been obtained
   of a sample of 11 small optical nebula associated with IRAS sources in
   star forming regions in order to perform a morphological and photometric
   study. More than 130 sources were detected in K. The J-H versus H-K
   diagram was used to establish their nature. The most massive regions, Gy
   2-18, Gy 4-2, and Gy 3-7, have embedded clusters of early-type stars and
   their associated nebulae were found to be photoionized. The IRAS sources
   in L1455, L1473, L1165 and Gy 4-1 with near-IR reflection nebulae were
   found to be Class I young stellar objects with associated molecular
   outflows. The results for each region is discussed in detail. (C) 1997
   American Astronomical Society.}},
