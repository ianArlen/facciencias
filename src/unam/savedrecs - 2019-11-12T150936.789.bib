@article{ ISI:000077133900006,
Author = {Gomez-Maqueo, EL and Perez, JM and Ampudia, FYA},
Title = {{A reliability test-retest study of the MMPI-2 in a group of Mexican
   university students}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1997}},
Volume = {{14}},
Number = {{1}},
Pages = {{55-62}},
Month = {{JUN}},
Abstract = {{The purpose of the present study was to determine the test-retest
   reliability of the MMPI-2 Spanish version for Mexico (Lucio \& Reyes,
   1995) in a sample of university students. Participants included 106
   females and 146 males from a private university in Mexico City. Scores
   were compared with those obtained by Butcher (1989), in a test-retest
   study of MMPI-2 in a U.S. sample. Frequencies, means and standard
   deviations for Basic, Content and Supplementary scales were obtained for
   this sample. Then Pearson's Coefficient correlations, and Cronbach's
   Alpha Coefficients were computed in order to test the internal
   consistency of the scales. No significant differences were found between
   test and re-test scores of Mexican students. These results show, that
   this version of MMPI-2 is a stable and reliable test measure for this
   sample of Mexican university students.}},
