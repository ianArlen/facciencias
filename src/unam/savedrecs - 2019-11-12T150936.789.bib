@article{ ISI:A1997XQ63900001,
Author = {Eckertson, FW},
Title = {{Resolvable, not maximally resolvable spaces}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{1997}},
Volume = {{79}},
Number = {{1}},
Pages = {{1-11}},
Month = {{AUG 12}},
Abstract = {{The formation of maximal topologies and the use of maximal independent
   families are the only known techniques for constructing irresolvable
   spaces. Using side conditions to control maximal constructions, we give
   a new construction of n-resolvable, not (n + 1)-resolvable spaces. In
   another direction, we use the existence of a crowded, irresolvable,
   strong P-kappa space to construct infinitely resolvable, not maximally
   resolvable spaces. Relative to the existence of a measurable cardinal,
   the latter result answers several longstanding questions in this area.
   (C) 1997 Elsevier Science B.V.}},
