Journal = {{JOURNAL OF MOLECULAR STRUCTURE-THEOCHEM}},
Year = {{1997}},
Volume = {{397}},
Pages = {{159-166}},
Month = {{JUN 2}},
Abstract = {{The structure and electronic properties of the radical anions and
   dianions of sym-dibenzo-1,5-cyclooctadiene-3,7-diyne,
   dibenzo{[}a,e]-cyclooctene and sym-dibenzo-1,3,5-cyclooctadiene-7-yne
