@article{ ISI:A1997XA15700008,
Author = {Zhu, YH and Torres, J},
Title = {{Tensile deformation in extruded eutectoid Zn-Al based alloy}},
Journal = {{ZEITSCHRIFT FUR METALLKUNDE}},
Year = {{1997}},
Volume = {{88}},
Number = {{4}},
Pages = {{329-332}},
Month = {{APR}},
Abstract = {{Tensile deformation induced phase transformations and microstructural
   changes in an extruded eutectoid Zn-Al based alloy were investigated
   using X-ray diffraction and scanning electron microscopy. Decomposition
   of the metastable eta(E)' phase and a four phase transformation, alpha +
   epsilon --> T' + eta, were found to occur during tensile deformation.
   Tensile deformation resulted in spheroidizing of the coarse lamellar
   structure, within which rupture holes were initiated to form in the
   rupture part of the specimen. The fine lamellar structure withstood the
   tensile rupture.}},
