@article{ ISI:A1997XE08800012,
Author = {Sharma, VK and Rhudy, K and Brooks, R and Hollyfield, S and Vazquez, FG},
Title = {{Petroleum hydrocarbons in sediments of Upper Laguna Madre}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{1997}},
Volume = {{34}},
Number = {{4}},
Pages = {{229-234}},
Month = {{APR}},
Abstract = {{The Laguna Madre system is the largest hypersaline coastal basin in the
   United States, This system supports the Gulf Intracoastal Waterway
   (GIWW), and petroleum production wells and pipelines, Sediments from 52
   Upper Laguna Madre (ULM) sites were analysed for petroleum hydrocarbons
   to assess the extent of contamination in the area, The resolved
