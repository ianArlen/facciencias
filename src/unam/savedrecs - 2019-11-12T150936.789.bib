@article{ ISI:A1997XM67800007,
Author = {Politzer, P and Seminario, JM and Concha, MC and Zacarias, AG},
Title = {{Density-functional investigation of some decomposition routes of methyl
   nitrate}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{1997}},
Volume = {{64}},
Number = {{2}},
Pages = {{205-210}},
Month = {{AUG 5}},
Abstract = {{Two density-functional methods, B3/LYP/6-31+G(d) and B3/PW91/6-31G(d, p)
   have been used to investigate some possible decomposition pathways for
   methyl nitrate. Two likely ones were found to be (a) loss of NO2,
   followed by eventual rearrangement to H2C-OH, and (b) formation of H2C=O
   and HONO. The initial energy requirement for each process-is about 40
   kcal/mol, and the second is exothermic, Delta H = -16 kcal/mol of
   H3C-ONO2. The B3/LYP procedure is found to underestimate the H3CO-NO2
   and H3C-ONO2 dissociation energies by about 6 kcal/mol. (C) 1997 John
   Wiley \& Sons, Inc.}},
