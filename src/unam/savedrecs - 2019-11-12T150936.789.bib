@article{ ISI:A1997WK65700002,
Author = {Xiao, MF},
Title = {{Polarization effects in reflection scanning near field optical
   microscopy}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1997}},
Volume = {{136}},
Number = {{3-4}},
Pages = {{213-218}},
Month = {{MAR 15}},
Abstract = {{Effects of the polarization state of incident light on the resolution of
   reflection scanning near field optical microscopes are studied
   conceptually and numerically, Generally speaking, the imaging quality
   depends significantly on the polarization of the incident light at the
   probe tip of the microscope. For example, the resolution of the
   microscope is better when the incident light is polarized vertically to
   the surface than when it is horizontally polarized. When the incident
   light is horizontally polarized, however, the resolution is better in
   the direction along the incident field than in the direction
   perpendicular to it, These results are the basis for a proposal for how
   to select useful tips before they are mounted on the microscope.
   Finally, the resolution of the microscope can be significantly improved
   by utilizing resonances that can occur in the tip-sample system.}},
