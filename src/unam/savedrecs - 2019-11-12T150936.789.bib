@article{ ISI:000077133900004,
Author = {Perez-Mitre, GG},
Title = {{Body image disturbances in a Mexican sample of preadolescent students}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1997}},
Volume = {{14}},
Number = {{1}},
Pages = {{31-40}},
Month = {{JUN}},
Abstract = {{In order to explore the incidence of body image disturbances, a sample
   of two hundred, fifth and sixth grade, preadolescents, 90 boys and 110
   girls, age average 10.8 years, answered a questionnaire. Half the
   students attended private and half public schools. Such disturbances
   have been widely documented as risk factors associated to eating
   disorders. The study main findings showed similar risk indicators to
   those found in older adolescent samples, as well as strong motivation
   and tendency toward thinness. An outstanding expression of this trend
   included choosing an almost-anorectic figure as an ideal. Body mass
   index data revealed higher subject percentages with very low weight than
   both those reported elsewhere and those observed in other Mexican
   adolescent samples.}},
