@article{ ISI:A1997WR33000002,
Author = {AltamiranoLozano, M and RoldanReyes, E and Bonilla, E and Betancourt, M},
Title = {{Effect of some metal compounds on sperm motility in vitro}},
Journal = {{MEDICAL SCIENCE RESEARCH}},
Year = {{1997}},
Volume = {{25}},
Number = {{3}},
Pages = {{147-150}},
Month = {{MAR}},
Abstract = {{In order to test in vitro the reprotoxic effects of various metal
   compounds, a system using pig spermatozoa was used. Semen samples were
   analysed and evaluated, transferred into a defined medium, treated with
   vanadium pentoxide (V2O5), lithium chloride (LiCl), potassium dichromate
   (K2Cr2O7), calcium chromate (CaCrO4), sodium arsenate (Na2HAsO4 .
   7H(2)O), or mercuric chloride (HgCl2), and incubated for 4 to 5 h at 37
   degrees C. 0, 1+, 2+, 3+, 4+, or 5+ h after treatment 20 mu L aliquots
   were examined to evaluate the sperm motility. HgCl2 was the most
   spermatoxic metal; after 15 min of exposure sperm motility was
   completely abolished at all doses. When the cells were treated with V2O5
   or LiCl a dose-and time-dependent reduction in sperm motility was
   observed. K2Cr2O7 and Na2HAsO4 . 7H(2)O treatments had less evident
   effects on sperm motility, and CaCrO4 showed no significant effect. Our
   results show that metals may have different mechanisms of action on
   sperm motility, and that in vitro assays are excellent screening tests
   to establish the reproductive toxicity potential of the direct action of
   chemical agents.}},
