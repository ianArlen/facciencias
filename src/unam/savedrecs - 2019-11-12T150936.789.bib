@article{ ISI:000077133900003,
Author = {Tena, RO and Velazquez, HA},
Title = {{An exploratory study on teaching the reading of musical notes through
   the use of conditional discrimination}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1997}},
Volume = {{14}},
Number = {{1}},
Pages = {{13-29}},
Month = {{JUN}},
Abstract = {{The present study explores the utility of a conditional discrimination
   procedure in teaching the reading of musical notes. A group of seven
   children attending preschool which had not yet learned reading skills
   participated in the study. The main objetive was to evaluate the
   effectiveness of a procedure that established stimulus equivalence
   between the pictorial representation of musical notes in a staff an
   their names. A matching to sample computerized program was developed for
   this study. In the training phases of the study, all correct responses
   were reinforced. The criteria established to change across phases varied
   from a correct performance of 90\% to 100\%. A total of 28 stimuli were
   used: 7 dictated names of the musical notes, 7 written names of the
   musical notes, 7 letter symbols representing the musical notes and 7
   pictorial representations of the musical notes in a staff in the G clef.
   The results from the symmetry, transitivity and reflexivity tests show
   that stimulus equivalence was achieved for all subjects. These findings
   seem to suggest that the use of a conditional discrimination procedure
   is effective in teaching the reading of musical notes.}},
