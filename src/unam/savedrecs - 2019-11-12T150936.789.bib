@article{ ISI:A1997XJ14100033,
Author = {Marti, J and Mirabel, IF and Duc, PA and Rodriguez, LF},
Title = {{A candidate optical and infrared counterpart for GRS 1739-278}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{1997}},
Volume = {{323}},
Number = {{1}},
Pages = {{158-162}},
Month = {{JUL}},
Abstract = {{The accurate radio position of the hard X-ray transient GRS 1739-278 has
   been observed with different ESO telescopes in a search for optical and
   near infrared counterpart. A candidate counterpart has been detected in
   all filters observed within less than 1'' of the radio position. No
   significant optical nor near infrared photometric variability is
   observed within similar to 0.2 mag in a time scale of several weeks. Our
   spectral type and reddening analysis appears to be consistent with
   either a normal massive X-ray binary or a low mass X-ray binary with a
   giant companion, both at roughly the galactic center distance. The a
   priori probability of a chance superposition in a field as crowded as
   that of GRS 1739-278 is estimated to be 14\%. However, this decreases to
   less than 1\% if the source belongs to any of the spectral types and
   luminosity classes proposed.}},
