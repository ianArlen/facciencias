Title = {{Synthesis of Li3Cu2SbO6, a new partially ordered rock salt structure}},
Journal = {{JOURNAL OF SOLID STATE CHEMISTRY}},
Year = {{1997}},
Volume = {{131}},
Number = {{1}},
Pages = {{115-120}},
Month = {{JUN}},
Abstract = {{The new phase Li3Cu2SbO6, synthesised by solid state reaction at 1000
   degrees C, is monoclinic, a=5.4659(1), b=8.7272(2), c=9.7807(3)
   Angstrom, beta=95.080(2)degrees, space group C2/c, and has a partially
   ordered rock salt structure related to the ordered structure of Li2TiO3
   by additional cation order: Ti sites are occupied by Cu and Sb in
   ordered fashion; Li sites contain Li/Cu in two sets, but with
   nonstatistical occupancy, and Li. Bond lengths show a Jahn-Teller
   elongation of CuO6 octahedra consistent with magnetic susceptibility
   data that indicate paramagnetic Cu2+ ions. Li3Cu2SbO6 shows a low level
   of electronic conductivity. (C) 1997 Academic Press.}},
