@article{ ISI:000075032000003,
Author = {Campo, A and Hernandez-Morales, B},
Title = {{Lumped-capacitance analysis for the quenching of workpieces: Limitations
   in terms of Rayleigh numbers and solid/fluid thermal conductivity ratios}},
Journal = {{JOURNAL OF MATERIALS PROCESSING \& MANUFACTURING SCIENCE}},
Year = {{1997}},
Volume = {{5}},
Number = {{4}},
Pages = {{295-301}},
Month = {{APR}},
Abstract = {{This paper deals with the establishment of the conditions that need to
   be fulfilled for the accurate analysis of temperature-time curves
   measured during heat treating processes of manufactured parts. Within
   the framework of the lumped-capacitance model of unsteady heat
   conduction (Bi < 0.1), it was found that, for natural convective
   cooling, the two primary dimensionless groups are the solid-fluid
   thermal conductivity ratio, K-sf, and the modified Rayleigh number,
   Ra-D,Ra-i. For a selected combination of solids having low and high
   thermal conductivities and quenching fluids such as air: oil acid water,
   values of the critical Ra-D,Ra-i have been calculated. This information
   may be useful to engineers which are engaged in the construction of
   cooling curves for heat treating processes.}},
