@article{ ISI:A1997WY55000015,
Author = {Buenfil, AE and Flores, A and Brandan, ME},
Title = {{Thermoluminescent characteristics of KBr:Eu and KBr:Eu,Mg irradiated
   with UV light}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{1997}},
Volume = {{30}},
Number = {{9}},
Pages = {{1399-1402}},
Month = {{MAY 7}},
Abstract = {{The TL response of KBr:Eu (0.1\% Eu) and KBr:Eu,Mg (0.1\% Eu, 0.01 and
   0.001\% Mg) to 254 nm UV light has been studied. Crystals were subjected
   to different thermal treatments. The Mg concentration and the thermal
   treatment that produced the best glow curve reproducibility and
   sensitivity increase were identified. The sensitivity to UV radiation
   increased by about three times after the optimum thermal treatment. The
   addition of Mg (0.001\%) was responsible for improvements in the
   reproducibility of the glow curve characteristics for different samples.
   The glow curves showed two high-temperature peaks above 170 degrees C,
   similar to those observed after exposure to gamma rays, and the TL
   response was linear for fluences between 15 and 150 J m(-2).}},
