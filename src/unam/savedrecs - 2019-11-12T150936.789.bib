@article{ ISI:A1997WQ10800025,
Author = {Papai, I and Castro, M},
Title = {{A density functional study of Sc-2 and Sc-3}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{1997}},
Volume = {{267}},
Number = {{5-6}},
Pages = {{551-556}},
Month = {{MAR 28}},
Abstract = {{Density functional theory, using a gradient-corrected
   exchange-correlation functional, has been applied to determine the
   equilibrium properties of various spin states of the Sc-2 and Sc-3
   molecules. The ground state of Sc-2 is predicted to be (5) Sigma(u)(-)
   with R(e) = 2.64 Angstrom, omega(e) = 222 cm(-1) and D-e = 0.92 eV,
   followed by the (3) Sigma(u)(-) and (1) Sigma(g)(+) states. The
   equilibrium structure of the (2)A(1)' ground state of Sc-3 is found to
   have D-3h symmetry with a bond length of 2.83 Angstrom. The calculated
   harmonic frequencies are 272 cm(-1) (a(1)') and 153 cm(-1) (e'), while
   the atomization energy is 2.49 eV, Several low-lying excited states of
   Sc-3 are found to lie about 0.2 eV above the ground state. The predicted
   ground states of Sc-2 and Sc-3 and their vibrational frequencies are in
   accordance with the findings of ESR and resonance Raman studies. (C)
   1997 Elsevier Science B.V.}},
