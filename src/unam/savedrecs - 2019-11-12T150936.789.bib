@article{ ISI:A1997XX87000007,
Author = {Mapes, C and Basurto, F and Bye, R},
Title = {{Ethnobotany of quintonil: Knowledge, use and management of edible greens
   Amaranthus spp. (Amaranthaceae) in the Sierra Norte de Puebla, Mexico}},
Journal = {{ECONOMIC BOTANY}},
Year = {{1997}},
Volume = {{51}},
Number = {{3}},
Pages = {{293-306}},
Month = {{JUL-SEP}},
Abstract = {{In addition to the seeds consumed as a pseudocereal, amaranths provide
   important edible greens. In the Sierra Norte de Puebla, Mexico, the
   management and utilization of amaranths occur within a wide variety of
   environmental, biological and cultural contexts. This paper presents
   both the results of ethnobotanical exploration in this region and the
   comparison of different species and races of amaranths used as greens
   that were grown in common experimental garden plots in Chalco, Mexico.
   Ethnobotanical exploration, cultural practices and growth analysis
   suggests that these plants have been selected for their use as leafy
   vegetables. The experimental data support the divergence of amaranths
   used for greens from those used principally for edible seed.}},
