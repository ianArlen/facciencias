@article{ ISI:A1997WR69300003,
Author = {VazquezMemije, ME and Beltran, C and DeGomezPuyou, MT},
Title = {{Isolation and comparative studies of mitochondrial F-1-ATPase from rat
   testis and beef heart}},
Journal = {{COMPARATIVE BIOCHEMISTRY AND PHYSIOLOGY B-BIOCHEMISTRY \& MOLECULAR
   BIOLOGY}},
Year = {{1997}},
Volume = {{116}},
Number = {{3}},
Pages = {{303-309}},
Month = {{MAR}},
Abstract = {{The isolation and properties of F-1-mitochondrial ATPase from rat testis
   are described. The isolation medium involves a chloroform extraction,
   and it is suitable even with small amounts of starting material that
   have a relatively low specific activity as in the case of rat testis
   submitochondrial particles. The isolated enzyme from rat testis had a
   specific activity of 30-45 mu mol Pi/min/mg protein, which could be
   increased up to 90 mu mol Pi/min/mg protein only in the presence of
   bicarbonate and maleate. The isolated enzyme represented less than 0.6\%
