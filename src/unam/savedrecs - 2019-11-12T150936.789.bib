@article{ ISI:000077133900008,
Author = {Diaz, JMM and Zapian, GGC},
Title = {{A point of view about feeding behavior}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1997}},
Volume = {{14}},
Number = {{1}},
Pages = {{75-81}},
Month = {{JUN}},
Abstract = {{Recent research suggests a new emergent field called biochemical
   psychology; which is analagous to physiological psychology, but
   recognizes the need for new approaches and techniques. The area is
   exciting in its conception, but still possesses many technical and
   philosophical problems including issues on the action of drugs,
   relations between biochemical and psychological events, lack of basic
   information about the relative importance of several metabolic pathways,
   regulatory mechanisms, the importance of genetic processes and, finally,
   the application of new knowledge to human disorders. The present study
   gives a general view of the reviewed literature about the hypothalamic
   monoaminergic systems involved in feeding behavior. This conceptual
   reorientation draws attention on the methodological level enhancing the
   external validity of animal experiments, since neurochemical analysis
   supplemented with behavioral strategies offers great contributions to
   the development of this area.}},
