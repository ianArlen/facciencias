@article{ ISI:A1997WV34000003,
Author = {ValienteBanuet, A and RojasMartinez, A and Arizmendi, MD and Davila, P},
Title = {{Pollination biology of two columnar cacti (Neobuxbaumia mezcalaensis and
   Neobuxbaumia macrocephala) in the Tehuacan Valley, Central Mexico}},
Journal = {{AMERICAN JOURNAL OF BOTANY}},
Year = {{1997}},
Volume = {{84}},
Number = {{4}},
Pages = {{452-455}},
Month = {{APR}},
Abstract = {{We document the pollination biology and mating systems of Neobuxbaumia
   mezcalaensis and Neobuxbaumia macrocephala, two Mexican giant columnar
   cacti. These two species form mixed forests in the western Tehuacan
   Valley, Mexico. The flowers of both N. mezcalaensis and N. macrocephala
   exhibit nocturnal anthesis, are self-incompatible, and are pollinated
   primarily by three species of nectar-feeding bats (Choeronycteris
   mexicana, Leptonycteris curasoae, and Leptonycteris nivalis).
   Neobuxbaumia mezcalaensis is androdioecious, a breeding system that
   appears to be uncommon among Cactaceae. Neobuxbaumia macrocephala is
   hermaphroditic. we hypothesize that columnar cacti show a geographical
   dichotomy in floral biology specialization that probably can be related
   to predictability in pollinator abundance.}},
