@article{ ISI:A1997XG60500157,
Author = {Rickards, J and Garcia, J and Garcia, R and Ramirez, E and Lopez, K},
Title = {{SiO2:chlorophyll gels under He-4 irradiation}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{1997}},
Volume = {{127}},
Pages = {{691-693}},
Month = {{MAY}},
Note = {{10th International Conference on Ion Beam Modification of Materials
   (IBMM-96), ALBUQUERQUE, NM, SEP 01-06, 1996}},
Abstract = {{SiO2 gels doped with chlorophyll extracted from natural leaves were
   exposed to different fluences of a 3-MeV He-4 beam from the Institute de
   Fisica Pelletron Accelerator. Optical absorption, emission and
   excitation spectra were measured as a function of fluence in the
   interval (0-2.5) x 10(14) cm(-2). Slight increases in both emission and
   excitation spectra at small fluences were observed. For larger fluences
   a characteristic exponential-type decay was produced due to degradation
   of the organic molecules. The data were fitted with a Lorentzian curve.
   A discussion of possible mechanisms is given in terms of the irradiation
   parameters.}},
