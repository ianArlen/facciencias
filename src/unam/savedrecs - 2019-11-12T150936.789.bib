@article{ ISI:A1997WX64000021,
Author = {Rosenblueth, JF and Warga, J and Zhu, QJ},
Title = {{On the characterization of properly relaxed delayed controls}},
Journal = {{JOURNAL OF MATHEMATICAL ANALYSIS AND APPLICATIONS}},
Year = {{1997}},
Volume = {{209}},
Number = {{1}},
Pages = {{274-290}},
Month = {{MAY 1}},
Abstract = {{In previous papers Warga and Zhu have proven the conjecture of
   Rosenblueth and Vinter that a particular abstract model characterizes
   the set (R) over bar of relaxed controls that are limits of ordinary
   controls with (possibly time-dependent) delays. Rosenblueth and Vinter
   have also provided a simple and concrete characterization of (R) over
   bar for the case of constant delays that are consecutive multiples of a
   common divisor h. In the present paper we provide a concrete
   characterization of (R) over bar for the case of delays that are
   arbitrary (and not necessarily consecutive) multiples of some h. We also
   exhibit a simple compactification of the set of solutions of controlled
   differential equations with such delays in control. (C) 1997 Academic
   Press.}},
