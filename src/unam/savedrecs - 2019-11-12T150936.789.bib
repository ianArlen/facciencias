@article{ ISI:A1997XP22400003,
Author = {Reynolds, WF and Yu, M and Enriquez, RG and Leon, I},
Title = {{Investigation of the advantages and limitations of forward linear
   prediction for processing 2D data sets}},
Journal = {{MAGNETIC RESONANCE IN CHEMISTRY}},
Year = {{1997}},
Volume = {{35}},
Number = {{8}},
Pages = {{505-519}},
Month = {{AUG}},
Abstract = {{Although the potential advantages of f(1) forward linear prediction for
   the processing of multi-dimensional MMR spectra are well established,
   this method is surprisingly little used for 2D spectra used for organic
   structure determination. A detailed investigation df the advantages and
   limitations of f(1) forward linear prediction for this purpose is
   reported. This is a reliable technique which is particularly useful for
   H-1-detected C-13-H-1 shift correlation spectra, allowing up to 16-fold
   linear prediction of the C-13 axis of HSQC spectra. In general, the use
   of linear prediction allows one to obtain comparable 2D spectra in one
   quarter of the time or double the sensitivity in a comparable lime
   relative to similar spectra without linear prediction. The one exception
   is the absolute value COSY spectrum, where linear prediction beyond a
   factor of two gives poor results. Linear prediction is generally
   superior to zero filling as a time-saving technique, although the
   difference between the two approaches disappears as the f(1) data point
   resolution approaches the natural Iinewidth. By contrast, f, forward
   linear prediction is not recommended. (C) 1997 by John Wiley \& Sons,
   Ltd.}},
