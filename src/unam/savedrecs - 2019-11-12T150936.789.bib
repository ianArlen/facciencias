@article{ ISI:A1997XF89400009,
Author = {Gonzalez, MMM and Villasenor, GT},
Title = {{Effects of rolling on the microstructure of ZINALCO(MR)}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1997}},
Volume = {{43}},
Number = {{3}},
Pages = {{412-420}},
Month = {{JUN}},
Abstract = {{The rolling effect on the ZINALCO(MR) alloy was studied by scanning and
   transmission electron microscopy. The original extruded material shows
   perlitic structure, which transforms into a finely-grained
   microstructure under the influence of the rolling process. This process
   was carried out at room temperature and 240 degrees C over extruded
   sheets having perlitic microstructure. The microhardness decreases, by
   increasing the rolling degree, was observed and its value was found
   lower for room temperature rolled samples with respect to the 240
   degrees C rolled ones. The electron diffraction patterns were obtained
   to observe the rolling effect on the preferred orientation of material.
   The preferred orientation decreased according to increasing rolling
   degree at room temperature and 240 degrees C.}},
