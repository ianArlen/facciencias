@article{ ISI:A1997XB33000004,
Author = {Rodriguez, RF and Reyes, JA},
Title = {{Propagation of optical solitons in a nematic waveguide}},
Journal = {{JOURNAL OF MOLECULAR LIQUIDS}},
Year = {{1997}},
Volume = {{71}},
Number = {{2-3}},
Pages = {{115-128}},
Month = {{APR}},
Note = {{XXV Winter Meeting on Statistical Physics, CUERNAVACA, MEXICO, JAN
   09-12, 1996}},
Abstract = {{The propagation of an optical wavepacket through a planar multimodal
   waveguide with a thermotropic nematic core is studied. We show that due
   to the coupling between the orientation and the TM modes of the
   wavepacket, the dynamics of the latter is nonlinear and is described by
   the Nonlinear Schordinger Equation (NLSE). In the weakly nonlinear limit
   (Kerr medium) and assuming that attenuation effects are small, we show
   that the Ri modes may propagate as optical solitons along the central
   part of the cell. Using experimental values of the dispersion relation
   for MBBA, we calculate the nonlinear refraction index as a function of
   the incidence angle and the power required to generate them. We find
   that the former exhibits a large optical nonlinearity and the latter is
   a tenth of a miliwatt. Finally, we discuss the limitations and possible
   generalizations of our model and give some concluding remarks. (C) 1997
   Elsevier Science B.V.}},
