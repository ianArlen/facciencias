@article{ ISI:A1997YA08300007,
Author = {Hess, PO},
Title = {{Comparative study of the Sp(2,R) and the Sp(6,R) models and an
   application to the Ba chain of isotopes}},
Journal = {{FOUNDATIONS OF PHYSICS}},
Year = {{1997}},
Volume = {{27}},
Number = {{7}},
Pages = {{1061-1081}},
Month = {{JUL}},
Abstract = {{We investigate the Sp(2, R) model for light and heavy nuclei and compare
   it to recent applications of the Sp(6, R) model The same Hamiltonian is
   used and the basis stales will have good angular momentum. We not only
   confirm that the restricted space of the Sp(6, R) model gives the
   dominant contribution bur also that there is no practical difference
   from the Sp(6, R) model calculations. The Sp(2, R) model is then applied
   to the chain of Ba-isotopes which show, according to a recent
   application of the geometrical model, a dominant prolate behavior with
   an important gamma-unstable component. They represent limiting cases
   where the Sp(2, R) model can be put to an extreme test.}},
