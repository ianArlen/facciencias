@article{ ISI:A1997WV25400012,
Author = {CirosPerez, J and EliasGutierrez, M},
Title = {{Macrothrix smirnovi, a new species (Crustacea: Anomopoda:
   Macrothricidae) from Mexico, a member of the M-triserialis-group}},
Journal = {{PROCEEDINGS OF THE BIOLOGICAL SOCIETY OF WASHINGTON}},
Year = {{1997}},
Volume = {{110}},
Number = {{1}},
Pages = {{115-127}},
Month = {{APR 15}},
Abstract = {{Macrothrix smirnovi, new species, is described from a small reservoir
   located at a transitional zone between neartic and neotropic regions in
   Mexico. It resembles taxa related to M. triserialis, mainly the
   South-American M. superaculeata, but parthenogenic females are
   characterized by differences in spinulation of the antennae. Trunk limb
   II has a unique lobe carrying a stout conical seta densely ciliated at
   its distal portion located at the external surface of the endopod, near
   to the insertion of scrapers 4 and 5. The postabdomen is also distinct.
   The ephippium has a structure similar to M. rosea. Adult males have a
   postabdomen fairly similar to females and a copulatory hook with one
   crescentic ridge at the tip of an irregular margin which gives a
   spoon-like appearance to this portion.}},
