@article{ ISI:A1997XL61000006,
Author = {Fuchs, R and Mendoza, CI and Barrera, RG and Carrillo, JL},
Title = {{Electron energy-loss spectroscopy of inhomogeneous systems}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{1997}},
