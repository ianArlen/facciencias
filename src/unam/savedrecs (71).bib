@article{ ISI:000297841200105,
Author = {Pereira-Santaella, M. and Alonso-Herrero, A. and Santos-Lleo, M. and
   Colina, L. and Jimenez-Bailon, E. and Longinotti, A. L. and Rieke, G. H.
