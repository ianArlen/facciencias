@article{ ISI:000295735400058,
Author = {Ilieva, Lyuba and Pantaleo, Giuseppe and Ivanov, Ivan and Zanella,
   Rodolfo and Sobczak, Janusz W. and Lisowski, Wojciech and Venezia, Anna
   Maria and Andreeva, Donka},
Title = {{Preferential oxidation of CO in H-2 rich stream (PROX) over gold
   catalysts supported on doped ceria: Effect of water and CO2}},
Journal = {{CATALYSIS TODAY}},
Year = {{2011}},
Volume = {{175}},
