@article{ ISI:000296113000008,
Author = {Vazquez Soto, Veronica},
Title = {{The ``uphill{''} and ``downhill{''} system in Meseno Cora}},
Journal = {{LANGUAGE SCIENCES}},
Year = {{2011}},
Volume = {{33}},
Number = {{6, SI}},
Pages = {{981-1005}},
Month = {{NOV}},
Abstract = {{This article shows that Meseno Cora (Uto-Aztecan) displays a frame of
   reference (FOR) derived from topographic features that is based on two
   strong axes. This system is the most favored strategy in the language
   for orientation descriptions. Examination of the topography of the
   community in which this research was conducted shows that both axes of
   the dominant FoR are extremely representative of a geomorphic FoR.
   Additionally, this article presents the use of a relative FoR in Meseno
   Cora mostly based on the relative uses of terms for `right' and `left'.
   (C) 2011 Elsevier Ltd. All rights reserved.}},
