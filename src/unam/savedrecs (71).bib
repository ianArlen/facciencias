@article{ ISI:000296477000025,
Author = {Cruz-Zavala, Emmanuel and Moreno, Jaime A. and Fridman, Leonid M.},
Title = {{Uniform Robust Exact Differentiator}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
Year = {{2011}},
Volume = {{56}},
Number = {{11}},
Pages = {{2727-2733}},
Month = {{NOV}},
Abstract = {{The differentiators based on the Super-Twisting Algorithm (STA) yield
   finite-time and theoretically exact convergence to the derivative of the
   input signal, whenever this derivative is Lipschitz. However, the
   convergence time grows unboundedly when the initial conditions of the
   differentiation error grow. In this technical note a Uniform Robust
   Exact Differentiator (URED) is introduced. The URED is based on a STA
   modification and includes high-degree terms providing finite-time, and
   exact convergence to the derivative of the input signal, with a
   convergence time that is bounded by some constant independent of the
   initial conditions of the differentiation error. Strong Lyapunov
   functions are used to prove the convergence of the URED.}},
