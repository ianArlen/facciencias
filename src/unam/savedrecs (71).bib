@article{ ISI:000295622200009,
Author = {Banos-Gonzalez, Manuel A. and Calderon-Colmenero, Juan and
   Aranda-Fraustro, Alberto and Pena-Duque, Marco A. and Martinez-Rios,
   Marco A. and Valente-Acosta, Benjamin and Linares-Lopez, Carlos and
