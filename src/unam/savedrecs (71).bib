@article{ ISI:000295409100003,
Author = {Mueller, Markus F. and Baier, Gerold and Lopez Jimenez, Yurytzy and
   Marin Garcia, Arlex O. and Rummel, Christian and Schindler, Kaspar},
Title = {{Evolution of Genuine Cross-Correlation Strength of Focal Onset Seizures}},
Journal = {{JOURNAL OF CLINICAL NEUROPHYSIOLOGY}},
Year = {{2011}},
Volume = {{28}},
Number = {{5}},
Pages = {{450-462}},
Month = {{OCT}},
Abstract = {{To quantify the evolution of genuine zero-lag cross-correlations of
   focal onset seizures, we apply a recently introduced multivariate
   measure to broad band and to narrow-band EEG data. For frequency
   components below 12.5 Hz, the strength of genuine cross-correlations
   decreases significantly during the seizure and the immediate postseizure
   period, while higher frequency bands show a tendency of elevated
   cross-correlations during the same period. We conclude that in terms of
   genuine zero-lag cross-correlations, the electrical brain activity as
   assessed by scalp electrodes shows a significant spatial fragmentation,
   which might promote seizure offset.}},
