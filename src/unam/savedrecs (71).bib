@article{ ISI:000293936800006,
Author = {Ayala-Berdon, Jorge and Rodriguez-Pena, Nelly and Orduna-Villasenor,
   Monica and Stoner, Kathryn E. and Kelm, Detlev H. and Schondube, Jorge
   E.},
Title = {{Foraging behavior adjustments related to changes in nectar sugar
   concentration in phyllostomid bats}},
Journal = {{COMPARATIVE BIOCHEMISTRY AND PHYSIOLOGY A-MOLECULAR \& INTEGRATIVE
   PHYSIOLOGY}},
Year = {{2011}},
Volume = {{160}},
Number = {{2}},
