@article{ ISI:000295375800022,
Author = {Pedraza-Alva, Gustavo and Merida, Lilia B. and del Rio, Roxana and
   Fierro, Nora A. and Cruz-Munoz, Mario E. and Olivares, Norma and Melchy,
   Erika and Igras, Vivian and Hollaender, Georg A. and Burakoff, Steven J.
   and Rosenstein, Yvonne},
Title = {{CD43 Regulates the Threshold for T Cell Activation by Targeting Cbl
   Functions}},
Journal = {{IUBMB LIFE}},
Year = {{2011}},
Volume = {{63}},
Number = {{10, SI}},
Pages = {{940-948}},
Month = {{OCT}},
Abstract = {{T cell (TC) activation requires the coordinated signaling of the T cell
   receptor (TCR) and coreceptor molecules, allowing TCs to respond to
   lower degrees of TCR occupancy. Coreceptor molecules set the threshold
   for TC activation by controlling different regulatory signaling loops.
   The Cbl family members prevent undesired activation of T cells by
   regulating TCR signals. In this report, we show that TC prestimulation
   by the CD43 coreceptor molecule before TCR engagement inhibits
   TCR-dependent c-Cbl tyrosine phosphorylation, c-Cbl interaction with the
   adapter molecule Crk-L and promotes Cbl-b degradation in a PKC
   theta-dependent manner. Consequently, the prolonged tyrosine
   phosphorylation and delayed degradation of ZAP-70 and of the zeta chain
   lead to enhanced mitogen-activated protein kinase activation and robust
   TC response. These data indicates that CD43-mediated signals lower the
   threshold for TC activation by restricting the c-Cbl and Cbl-b
   inhibitory effects on TCR signaling. In addition to the strength and
   duration of intracellular signals, our data underscore temporality with
   which certain molecules are engaged as yet another mechanism to fine
   tune TC signal quality, and ultimately immune function. (C) 2011 IUBMB
   IUBMB Life, 63(10): 940-948, 2011}},
