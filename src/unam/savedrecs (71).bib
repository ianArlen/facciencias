@article{ ISI:000297070600001,
Author = {Knopfmacher, Arnold and Luca, Florian},
Title = {{ON PRIME-PERFECT NUMBERS}},
Journal = {{INTERNATIONAL JOURNAL OF NUMBER THEORY}},
Year = {{2011}},
Volume = {{7}},
Number = {{7}},
Pages = {{1705-1716}},
Month = {{NOV}},
Abstract = {{We prove that the Diophantine equation p(1)(r1)...p(k)(rk) = ((r1
   +...+rk)(r1,...rk)) has only finitely many positive integer solutions k,
   p1,..., pk, r1,..., rk, where p1,..., pk are distinct primes. If a
   positive integer n has prime factorization p(1)(r)...p(k)(rk), then ((r1
   +...+rk)(r1,...,rk)) represents the number of ordered factorizations of
   n into prime parts. Hence, solutions to the above Diophantine equation
   are designated as prime-perfect numbers.}},
