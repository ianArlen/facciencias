@article{ ISI:000300084700003,
Author = {Carolina Ruiz-Fernandez, Ana and Luis Marrugo-Negrete, Jose and
   Paternina-Uribe, Roberth and Hascibe Perez-Bernal, Libia},
Title = {{Pb-210-derived Sedimentation Rates and C-org Fluxes in Soledad Lagoon
   (Cispata Lagoon System, NW Caribbean Coast of Colombia)}},
Journal = {{ESTUARIES AND COASTS}},
Year = {{2011}},
Volume = {{34}},
Number = {{6}},
Pages = {{1117-1128}},
Month = {{NOV}},
Abstract = {{With the aim of evaluating temporal changes in sedimentation and organic
   carbon (C-org) supplied over the last similar to 100 years, a sediment
   core was collected at Soledad Lagoon, a costal ecosystem surrounded by
   mangroves, located in the Cispata Estuary (Caribbean coast of Colombia).
   The core sediments were characterized by low concentrations of calcium
   carbonate (0.2-2.9\%), organic matter (3-8\%), total nitrogen
   (0.11-0.38\%), and total phosphorus (0.19-0.65 mg g(-1)). Fe and Al
   concentrations ranged from 4\% to 5\%, and Mn from 356 to 1,047 mu g
   g(-1). The Pb-210-derived sediment and mass accumulation rates were 1.54
   +/- 0.18 mm year(-1) and 0.08 +/- 0.01 gcm(-2) year(-1), respectively.
   The sediment core did not provide evidence of human impact, such as
   enhancement of primary production or nutrient enrichment, which may
   result from recent land uses changes or climate change. The C-org fluxes
   estimated for Soledad Lagoon core lay in the higher side of carbon
   fluxes to coastal ecosystems (314-409 gm(-2) year(-1)) and the
   relatively high C-org preservation observed (similar to 45\%) indicate
   that these lagoon sediments has been a net and efficient sink of C-org
   during the last century, which corroborate the importance of mangrove
   areas as important sites for carbon burial and therefore, long-term
   sequestration of C-org.}},
