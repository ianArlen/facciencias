@article{ ISI:000295375800019,
Author = {Bernardo Luna-Ulloa, Luis and Hernandez-Maqueda, Jose G. and Cristina
   Castaneda-Patlan, M. and Robles-Flores, Martha},
Title = {{Protein Kinase C in Wnt Signaling: Implications in Cancer Initiation and
   Progression}},
Journal = {{IUBMB LIFE}},
