@article{ ISI:000295956500042,
Author = {Molina-Trinidad, E. M. and Estevez-Hernandez, O. and Rendon, L. and
   Garibay-Febles, V. and Reguera, E.},
Title = {{Electronic and vibrational spectra of novel Lanreotide peptide capped
   gold nanoparticles}},
