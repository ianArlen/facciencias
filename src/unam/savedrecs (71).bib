@article{ ISI:000291199600009,
Author = {Nestor, Mendoza-Munoz and Eddy Kei, Noriega-Pelaez and Maria Guadalupe,
   Nava-Arzaluz and Susana Elisa, Mendoza-Elvira and Adriana,
   Ganem-Quintanar and David, Quintanar-Guerrero},
Title = {{Preparation and in vitro evaluation of poly(D, L-lactide-co-glycolide)
   air-filled nanocapsules as a contrast agent for ultrasound imaging}},
Journal = {{ULTRASONICS}},
Year = {{2011}},
Volume = {{51}},
Number = {{7}},
Pages = {{839-845}},
Month = {{OCT}},
Abstract = {{The aim of this study was to prepare air-filled nanocapsules intended
   ultrasound contrast agents (UCAs) with a biodegradable polymeric shell
   composed of poly(D, L-lactide-co-glycolide) (PLGA). Because of their
   size, current commercial UCAs are not capable of penetrating the
   irregular vasculature that feeds growing tumors. The new generation of
   UCAs should be designed on the nanoscale to enhance tumor detection, in
   addition, the polymeric shell in contrast with monomolecular stabilized
   UCAs improves the mechanical properties against ultrasound pressure and
   lack of stability. The preparation method of air-filled nanocapsules was
   based on a modification of the double-emulsion solvent evaporation
   technique. Air-filled nanocapsules with a mean diameter of 370 +/- 96 nm
   were obtained. Electronic microscopies revealed spherical-shaped
   particles with smooth surfaces and a capsular morphology, with a shell
   thickness of similar to 50 nm. Air-filled nanocapsules showed echogenic
   power in vitro, providing an enhancement of up to 15 dB at a
   concentration of 0.045 mg/mL at a frequency of 10 MHz. Loss of signal
   for air-filled nanocapsules was 2 dB after 30 min, suggesting high
   stability. The prepared contrast agent in this work has the potential to
   be used in ultrasound imaging. (C) 2011 Elsevier B. V. All rights
   reserved.}},
