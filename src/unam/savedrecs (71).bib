@article{ ISI:000296309200001,
Author = {Pulido Herraez, Begona},
Title = {{Fray Bartolome de Las Casas in the Work and Thought of fray Servando
   Teresa de Mier}},
Journal = {{HISTORIA MEXICANA}},
Year = {{2011}},
Volume = {{61}},
Number = {{2}},
Pages = {{429+}},
Month = {{OCT-DEC}},
Abstract = {{This paper studies the presence and influence of fray Bartolome de Las
   Casas's thought on the work of fray Servando Teresa de Mier,
   particularly the way in which it influenced the shaping of his original
   idea of the existence of a pact, a set of fundamental laws, a
   constitution established in the years after the conquest and where
   America would be included in the Crown of Castile as an independent
   realm, and not a colony. We examine the different texts where Mier
   starts to appropriate Las Casas' thought, especially two poorly known
   texts: the Speech of Doctor Don Servando de Mier, born in Mexico,
   confirming the apology of Bishop Casas, written by the Reverend Bishop
   of Blois, Monsignor Henrique Gregoire, in a letter written in the year
   1806, and the prologue to the edition of A Short Account of the
   Destruction of the Indies by Bartolome de Las Casas.}},
Publisher = {{COLEGIO DE MEXICO CENTRO DE ESTUDIOS HISTORICOS}},
Address = {{REDACTOR LUIS MURO CAMINO AL AJUSCO 20, MEXICO CITY 10740, MEXICO}},
