@article{ ISI:000295500800017,
Author = {Naumis, Gerardo G. and Salazar, F.},
Title = {{Thermal conductivity in a rigidity transition}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2011}},
Volume = {{375}},
Number = {{39}},
Pages = {{3483-3489}},
Month = {{SEP 12}},
Abstract = {{The thermal conductivity (kappa(T)) in a lattice is studied as a
   function of rigidity by using the Kubo-Greenwood formula. The rigidity
   is modulated by changing the second neighbour interactions. The results
   show that kappa(T) is strongly determined by the rigid character of the
   network through the low frequency vibrational modes. The transition from
   an isostatic to overconstrained lattices is thus reflected in the
   behavior of kappa(T). (C) 2011 Elsevier B.V. All rights reserved.}},
