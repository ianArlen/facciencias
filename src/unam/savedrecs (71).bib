@article{ ISI:000294581300001,
Author = {Castro, Luis M. and Fuerte-Esquivel, Claudio R. and Tovar-Hernandez, J.
   H.},
Title = {{A unified approach for the solution of power flows in electric power
   systems including wind farms}},
Journal = {{ELECTRIC POWER SYSTEMS RESEARCH}},
Year = {{2011}},
Volume = {{81}},
Number = {{10}},
Pages = {{1859-1865}},
Month = {{OCT}},
Abstract = {{This paper proposes a solution approach of the power flow problem to
   assess the steady-state condition of power systems with wind farms in a
   single frame of reference, in which the state variables of the wind
   generators are combined with the nodal voltage magnitudes and angles of
   the entire network for a unified iterative solution through the
   Newton-Raphson method. Different wind energy conversion systems (WECS)
   are mathematically derived from the steady-state representation of the
   induction generator. Suitable strategies for initializing the state
   variables of the wind generators are also proposed in this paper.
   Lastly, three numerical examples are presented to numerically illustrate
   the applicability of the proposed approach. (C) 2011 Elsevier B.V. All
   rights reserved.}},
