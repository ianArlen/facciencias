@article{ ISI:000295492600034,
Author = {Larrion, F. and Pizana, M. A. and Villarroel-Flores, R.},
Title = {{SMALL LOCALLY nK(2) GRAPHS}},
Journal = {{ARS COMBINATORIA}},
Year = {{2011}},
Volume = {{102}},
Pages = {{385-391}},
Month = {{OCT}},
Abstract = {{A locally nK(2) graph G is a graph such that the set of neighbors of any
   vertex of G induces a subgraph isomorphic to nK(2). We show that a
   locally nK(2) graph G must have at least 6n - 3 vertices, and that a
   locally nK(2) graph with 6n - 3 vertices exists if and only if n is an
   element of \{1, 2,3, 5\}, and in these cases the graph is unique up to
   isomorphism. The case n = 5 is surprisingly connected to a classic
   theorem of algebraic geometry: The only locally 5K(2) graph on 6 x 5 - 3
   = 27 vertices is the incidence graph of the 27 straight lines on any
   nonsingular complex projective cubic surface.}},
