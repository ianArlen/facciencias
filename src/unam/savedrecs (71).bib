@article{ ISI:000296524200008,
Author = {de la Pena, Jose A.},
Title = {{Impact functions on the citation network of scientific articles}},
Journal = {{JOURNAL OF INFORMETRICS}},
Year = {{2011}},
Volume = {{5}},
Number = {{4}},
Pages = {{565-573}},
Month = {{OCT}},
Abstract = {{Scientometric models, which consider papers in a undifferentiated way,
   are blind to important features of the citation network. We propose an
   approach for the definition of a function P-S, for any set of scientific
   articles S, which reflects global properties of the citation network
   associated to S. Such a function, that we propose as a measure of the
   impact of scientific papers, is constructed as solution of an iterated
   system of Perron-eigenvalue problems. We discuss differences with
   previously defined measures, in particular of the PageRank type. (C)
   2011 Elsevier Ltd. All rights reserved.}},
