@article{ ISI:000301087200003,
Author = {Sanchez Vargas, Armando and Gay Garcia, Carlos and Estrada Porrua,
   Francisco},
Title = {{Climate change and poverty in Mexico City}},
Journal = {{INVESTIGACION ECONOMICA}},
Year = {{2011}},
Volume = {{70}},
Number = {{278}},
Pages = {{45+}},
Month = {{OCT-DEC}},
Abstract = {{In this article we use microeconometric tools and household surveys to
   estimate the economic impacts of climate change on poor households in
   Mexico City, to offer some brief public policy recommendations to
   ameliorate the negative effects of climate change at the household
   level. Our main findings suggest that inhabitants of Gustavo A. Madero,
   Iztapalapa and Tlalpan will be severely affected by climate change
   shocks, which would reduce household's water supply (almost five days
   per month), food expenditure (almost 358 pesos per month) and income (1
   493 pesos per month). At the same time, it seems that household's bad
   health perception and migration would increase as a result of climate
   change shocks.}},
