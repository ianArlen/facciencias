@article{ ISI:000300817800008,
Author = {Murayama-Rendon, Ciro},
Title = {{Acquisition of medicines in the Seguro Popular: inefficiency and
   inequity}},
Journal = {{GACETA MEDICA DE MEXICO}},
Year = {{2011}},
Volume = {{147}},
Number = {{6}},
Pages = {{497-503}},
Month = {{NOV-DEC}},
Abstract = {{In this article presents the main results of an analysis of drug
   purchases made by all the states in Mexico, with the resources from the
   Popular Secure to cover the Health Services Univesal Catalog (CAUSES by
   its Spanish acronym) in the first half of 2009. The conclusion is that
   at least 50\% of drugs keys have been purchased with a 20\% extra in the
   reference price set by the authorities. Also, spending on drugs is low
   despite the fact that there is budget availability and that it varies
   widely among States. This provides robust evidence that the model of
   drug purchasing for the scheme is inefficient from the economic point of
   view, and inequitable from the social point of view.}},
