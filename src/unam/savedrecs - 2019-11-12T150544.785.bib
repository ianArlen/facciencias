@article{ ISI:000186884100010,
Author = {Galvan, DH},
Title = {{Electronic structure calculations for PrFe4P12 filled skutterudite using
   extended huckel tight-binding method}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
