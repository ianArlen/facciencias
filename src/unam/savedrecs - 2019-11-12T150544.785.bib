@article{ ISI:000185634600015,
Author = {Aguilera-Navarro, VC and Fortes, M and De Llano, M},
Title = {{Cooper pairing and ladder correlations in a BCS ground state}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2003}},
Volume = {{17}},
Number = {{18-20, 1}},
Pages = {{3304-3309}},
Month = {{AUG 10}},
Note = {{4th International Conference on New Theories, Discoveries and
   Applications of Superconductors and Related Materials, SAN DIEGO,
   CALIFORNIA, JAN 16-21, 2003}},
Abstract = {{A Bethe-Salpeter treatment of Cooper pairs (CPs) based on an ideal Fermi
   gas (IFG) ``sea{''} produces unstable CPs if holes are not ignored.
   Stable CPs with damping emerge when the BCS ground state replaces the
   IFG, and are positive-energy, finite-lifetime resonances for nonzero
   center-of-mass momentum with a linear dispersion leading term.
   Bose-Einstein condensation of such pairs may thus occur in exactly two
   dimensions as it cannot with quadratic dispersion.}},
