@article{ ISI:000220033200002,
Author = {Alvarez, C},
Title = {{Incongruent counterparts and symmetrical figures}},
Journal = {{CRITICA-REVISTA HISPANOAMERICANA DE FILOSOFIA}},
Year = {{2003}},
Volume = {{35}},
Number = {{104}},
Pages = {{31-68}},
Month = {{AUG}},
Abstract = {{The question concerning the incongruent counterparts is raised by Kant
   in 1768, but it is also discussed later in 1770 and 1783. This question,
   which is related to his conception about the nature of space, is also
   closely related to his ideas about the nature of geometric knowledge. My
   aim in this paper is to analyze Kant's remarks on this topic - three of
   these remarks seem particularly relevant to us - in view of Euclidean
   solid geometry, which constitutes precisely the theoretic and scientific
   frame where he pretended they should fit.}},
