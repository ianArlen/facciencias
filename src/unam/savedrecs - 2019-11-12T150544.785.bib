@article{ ISI:000185642300008,
Author = {Grether, A and de Llano, A and Solis, MA},
Title = {{Anomalous behavior of ideal Fermi gas below two dimensions}},
Journal = {{EUROPEAN PHYSICAL JOURNAL D}},
Year = {{2003}},
Volume = {{25}},
Number = {{3}},
Pages = {{287-291}},
Month = {{SEP}},
Abstract = {{Normal behavior of the thermodynamic properties of a Fermi gas in d > 2
   dimensions, integer or not, means monotonically increasing or decreasing
   of its specific heat, chemical potential or isothermal sound velocity,
   all as functions of temperature. However, for 0 < d < 2 dimensions these
   properties develop a ``hump{''} (or ``trough{''}) which increases (or
   deepens) as d --> 0. Though not the phase transition signaled by the
   sharp features ({''}cusp{''} or ``jump{''}) in those properties for the
   ideal Bose gas in d > 2 (known as the Bose-Einstein condensation), it is
   nevertheless an intriguing structural anomaly which we exhibit in
   detail.}},
