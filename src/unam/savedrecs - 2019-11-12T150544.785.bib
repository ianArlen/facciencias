@article{ ISI:000184583600001,
Author = {Silva, R and Govaere, G and Salles, P},
Title = {{Wave interaction with cylindrical porous piles}},
Journal = {{OCEAN ENGINEERING}},
Year = {{2003}},
Volume = {{30}},
Number = {{14}},
Pages = {{1719-1740}},
Month = {{OCT}},
Abstract = {{The interaction of waves with arrays of porous circular cylinders is
   studied theoretically and, under the assumption of potential flow and
   linear wave theory, an analytical solution is derived. The solution is
   valid for either submerged or emerged structures. The extension to the
   cases of unidirectional and multidirectional waves is obtained by means
   of a transfer function. For specific conditions the model gives the same
   solution as those previously presented by other authors. Numerical
   results are presented which exemplify diverse wave. and mechanical
   parameters on the wave transformation due to the presence of a system of
   circular cylinders. (C) 2003 Elsevier Science Ltd. All rights reserved.}},
