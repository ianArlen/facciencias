@article{ ISI:000185423200012,
Author = {Verma, SP and Santoyo, E},
Title = {{An unusual systematic behaviour of detection limits for elements from
   Cs-55 to Ta-73}},
Journal = {{ANALYTICAL AND BIOANALYTICAL CHEMISTRY}},
Year = {{2003}},
Volume = {{377}},
Number = {{1}},
Pages = {{82-84}},
Month = {{SEP}},
Note = {{European Winter Conference on Plasma Spectrochemistry, GARMISCH
   PARTENKIRCHEN, GERMANY, JAN 12-17, 2003}},
Abstract = {{A representative database for detection limits for all elements from
   Cs-55 to Ta-73 reveals that these limits are governed by a systematic
   zigzag pattern, according to which the odd atomic number elements have
   systematically lower detection limits than the even atomic number
   neighbour elements. This is true even when the actual detection limits
   vary by several orders of magnitude. We propose that such a systematic
   pattern be used as a requisite analytical criterion to evaluate the
   detection limit data, and any departure from this pattern be looked on
   with caution to check the analytical technique for any interference or
   matrix effect problems.}},
