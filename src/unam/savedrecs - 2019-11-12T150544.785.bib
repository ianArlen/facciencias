@article{ ISI:000186127000013,
Author = {Vega-Rodriguez, A and Pineiro, A and Perez-Casas, S},
Title = {{Thermodynamics of the interaction between
   hydroxypropyl-alpha-cyclodextrin and alkanols in aqueous solutions}},
Journal = {{THERMOCHIMICA ACTA}},
Year = {{2003}},
Volume = {{405}},
Number = {{1}},
Pages = {{109-115}},
Month = {{OCT 14}},
Note = {{17th International Conference for Chemical Thermodynamics, ROSTOCK,
   GERMANY, JUL 29-AUG 02, 2002}},
Organization = {{Int Assoc Chem Thermodynam; IUPAC}},
Abstract = {{The interactions of hydroxvpropyl-alpha-cyclodextrin (HP-alpha-CD) with
   1-alkanols and with some alpha,omega-alkanediols have been studied by
   calorimetric titration at 298.15 K. This technique enables the
   determination of the enthalpy and association constant for the complex
   formation, from which Gibbs energy and entropy can be derived. The
   results are compared to those reported for the complex formation between
   the native alpha-cyclodextrin (alpha-CD) and 1-alkanols or
   alpha,omega-alkanediols in the literature. Thermodynamic parameters
   corresponding to the transfer process of the alkanol from the native to
   the modified alpha-CD are also calculated. The results clearly show that
   the hydrophobic interactions are important in this process, but there
   are other effects like the size of the alkanol that are also of some
   importance. (C) 2003 Elsevier Science B.V. All rights reserved.}},
