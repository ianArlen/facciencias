@article{ ISI:000185503400041,
Author = {Espinosa, G and Golzarri, JI and Vazquez, C and Fragoso, R and
   Chadderton, LT and Cruz, SA},
Title = {{AFM observations of latent fission tracks on surfaces: amorphous SiO2
   and quartz}},
Journal = {{RADIATION MEASUREMENTS}},
