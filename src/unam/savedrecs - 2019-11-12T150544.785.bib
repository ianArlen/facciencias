@article{ ISI:000184578100008,
Author = {Trujillo-Roldan, MA and Pena, C and Galindo, E},
Title = {{Components in the inoculum determine the kinetics of Azotobacter
   vinelandii cultures and the molecular weight of its alginate}},
Journal = {{BIOTECHNOLOGY LETTERS}},
Year = {{2003}},
Volume = {{25}},
Number = {{15}},
Pages = {{1251-1254}},
Month = {{AUG}},
Abstract = {{In cultures of Azotobacter vinelandii inoculated using washed cells (
   avoiding exhausted broth components) alginates of a higher molecular
   weight (1200 kDa) than those obtained in cultures conventionally
   inoculated ( 350 kDa), were produced. Also, when comparing
   conventionally inoculated cultures with those inoculated with
   washed-cells, the alginate lyase activity was delayed and the final
   polymer concentration decreased from 4.8 to 3.5 g l(-1). This suggests
   that components in the exhausted inoculum broth play important
   regulatory roles in alginate biosynthesis and needs to be taken into
   account when describing polymer biosynthesis.}},
