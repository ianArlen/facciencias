@article{ ISI:000186571200061,
Author = {Hernandez-Machado, A and Lacasta, AM and Mayoral, E and Poire, EC},
Title = {{Phase-field model of Hele-Shaw flows in the high-viscosity contrast
   regime}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2003}},
Volume = {{68}},
Number = {{4, 2}},
Month = {{OCT}},
Abstract = {{A one-sided phase-field model is proposed to study the dynamics of
   unstable interfaces of Hele-Shaw flows in the high viscosity contrast
   regime. The corresponding macroscopic equations are obtained by means of
   an asymptotic expansion from the phase-field model. Numerical
   integrations of the phase-field model in a rectangular Hele-Shaw cell
   reproduce finger competition with the final evolution to a steady-state
   finger.}},
