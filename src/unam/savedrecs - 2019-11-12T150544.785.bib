@article{ ISI:000185599800011,
Author = {Graizbord, B and Rowland, A and Aguilar, AG},
Title = {{Mexico City as a peripheral global player: The two sides of the coin}},
Journal = {{ANNALS OF REGIONAL SCIENCE}},
Year = {{2003}},
Volume = {{37}},
Number = {{3}},
Pages = {{501-518}},
Month = {{SEP}},
Note = {{Conference on the Impact of Globalization on Urban Development,
   BELLAGIO, ITALY, AUG, 2002}},
Abstract = {{This article considers the effects of globalization on Mexico City, as
   well as whether this urban area, one of the largest in the world, can be
   considered a ``global city.{''} We base our arguments on a number of
   scales of analysis suggested in the literature on these topics. At the
   international scale, we look at the increased concentration of corporate
   headquarters and air traffic flows in the city. In terms of its role in
   the national urban system, we argue that while domestic migration
   patterns have shifted toward other destinations, the majority of
   domestically produced merchandise continues to find its way to the
   capital. At the metropolitan scale, our analysis suggests increasing
   spatial segregation, as well as longer commutes. At the intraurban
   level, we find that the sectoral composition of jobs has shifted toward
   commercial and service sectors, the informal sector has expanded, the
   labor force is polarizing, and that high-level service sector growth is
   spatially concentrated. In view of these findings, we suggest that the
   effects of globalization on Mexico City are mixed, as it consolidates
   its position as a second-tier global city. We also argue that, in spite
   of welcome steps toward democratization, pre-existing income
   inequalities in the country have accentuated the socioeconomic
   polarization predicted by the literature on global cities and
   globalization, giving rise to a megacity with two very distinct sides.}},
