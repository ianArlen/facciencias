@article{ ISI:000189260900001,
Author = {Hendrickx, ME},
Title = {{Distribution and estimation of body size and weight of four species of
   deep water shrimps in the southeastern Gulf of California, Mexico}},
Journal = {{CRUSTACEANA}},
Year = {{2003}},
Volume = {{76}},
Number = {{9}},
Pages = {{1025-1036}},
Month = {{OCT}},
Abstract = {{There is virtually no fishery for deep-water shrimps in the Gulf of
   California, Mexico, mostly due to lack of information and to the
   presence of a wide minimum oxygen layer that spans from depths of c.
   100-150 m to 600-800 m. The body weight and size of four species of
   deep-water shrimps (Benthesicymus tanneri, Heterocarpus affinis,
   Pandalopsis ampla, and Glyphocrangon spinulosa) were analysed. Samples
   were collected with a benthic sledge during four research cruises
   (August and December 2000; March and June 2001) in the SE Gulf of
   California to depths over 2000 m. Relationships between total length and
   carapace length, total length and total weight, and between tail weight
   with and without carapace were established for each species.
   Relationships were not significantly different between males and
   females, except in the case of the total weight/total length regression
   in B. tanneri. A tail weight/body weight index was calculated from
   regression equations: B. tanneri, 0.43; H. affinis, 0.46; P. ampla,
   0.50; G. spinulosa, 0.40. Maximum body size (153 mm) and weight (28 g)
   indicate that H. affinis should be considered as the target species
   while the other species should be considered as by-catch.}},
