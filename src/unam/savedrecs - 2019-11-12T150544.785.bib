@article{ ISI:000184808400008,
Author = {Patrykiejew, A and Pizio, O and Pusztai, L and Sokolowski, S},
Title = {{Effects of slit-like pore confinement on the closed loop immiscibility
   in symmetric binary model mixtures: fundamental measure density
   functional approach}},
Journal = {{MOLECULAR PHYSICS}},
Year = {{2003}},
Volume = {{101}},
Number = {{14}},
Pages = {{2219-2223}},
Month = {{JUL 20}},
Abstract = {{We investigate the adsorption of model symmetric binary mixtures,
   exhibiting closed loop immiscibility in the bulk phase, in slit-like
   pores by using a density functional approach. Our focus is on changes in
   the closed loop phase diagram owing to confinement. We have found that,
   in general, confinement narrows immiscibility loops in the
   temperature-selectivity plane.}},
