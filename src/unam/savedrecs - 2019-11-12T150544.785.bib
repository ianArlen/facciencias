@article{ ISI:000185862500033,
Author = {Rubo, YG and Laussy, FP and Malpuech, G and Kavokin, A and Bigenwald, P},
Title = {{Dynamical theory of polariton amplifiers}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2003}},
Volume = {{91}},
Number = {{15}},
Month = {{OCT 10}},
Abstract = {{We present the theory of the dynamics of the polariton amplifier in the
   region of small polariton densities. We give an analytical solution for
   the polariton condensate density matrix and show that the formation of a
   coherent quantum state is possible. Once the condensate is formed, the
   coherence becomes macroscopically long living. Polariton amplifier
   represents, therefore, an optical memory element, where the input weak
   coherent signal can be amplified and kept.}},
