@article{ ISI:000185089800039,
Author = {Betancourt, I and Valenzuela, R},
Title = {{Effect of helical-induced anisotropy on the magnetoinductance response
   of Co-based amorphous wires}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2003}},
Volume = {{83}},
Number = {{10}},
Pages = {{2022-2024}},
Month = {{SEP 8}},
Abstract = {{In this letter, the effect of helical anisotropy, induced by torsion
   strain, on the circular permeability mu(phi), of a vanishing negative
   magnetostrictive CoFeSiB wire is presented. As a function of a dc
   applied magnetic field, H-dc, mu(phi) shows an asymmetric behavior
   dependent on the direction of the applied torsion strain: for
   counterclockwise torsion angles (-), a monotonous decrease in the
   mu(phi) (H-dc) double peak magnetoimpedance response is observed
   together with a linear increase in the wire's circular anisotropy field
   H-k. In contrast, for clockwise torsion angles (+), a gradual
   suppression of the mu(phi) (H-dc) maxima was evident, for which a
   magnetic softening of the wire is assumed. Results are interpreted in
   terms of a counterbalance/enhancement effect of the wire's circular
   anisotropy for + and - torsion angles, respectively. (C) 2003 American
   Institute of Physics.}},
