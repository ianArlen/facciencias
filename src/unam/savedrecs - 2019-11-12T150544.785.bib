@article{ ISI:000186052900015,
Author = {Montgomery, DR and Lopez-Blanco, J},
Title = {{Post-Oligocene river incision, southern Sierra Madre Occidental, Mexico}},
Journal = {{GEOMORPHOLOGY}},
Year = {{2003}},
Volume = {{55}},
Number = {{1-4}},
Pages = {{235-247}},
Month = {{SEP 30}},
Note = {{Binghamton International Geomorphology Symposium, UNIV N CAROLINA,
