@article{ ISI:000183360900010,
Author = {Lopez-Suarez, A and Rickards, J and Trejo-Luna, R},
Title = {{Analysis of hydrogen absorption by Ti and Ti-6Al-4V using the ERDA
   technique}},
Journal = {{INTERNATIONAL JOURNAL OF HYDROGEN ENERGY}},
Year = {{2003}},
Volume = {{28}},
Number = {{10}},
Pages = {{1107-1113}},
Month = {{OCT}},
Abstract = {{Ti and Ti-6Al-4V alloy samples were hydrogenated at different
   temperatures from 150degreesC to 750degreesC. They were analyzed using
   elastic recoil detection analysis (ERDA) with 3 MeV alpha particles and
   Rutherford Backscattering Spectrometry (RBS) with 6.585 MeV alpha
   particles. ERDA shows an increase in the amount of hydrogen absorbed for
   temperatures greater than 550degreesC in both materials, reaching
   concentrations of almost 3 x 10(22) H atoms/cm(3). The Ti shows cracks
   after hydrogenation and loses its mechanical properties; not so the
   Ti-6Al-4V alloy. A subsequent loss of hydrogen was measured in all the
   samples after several months. (C) 2003 International Association for
   Hydrogen Energy. Published by Elsevier Science Ltd. All rights reserved.}},
