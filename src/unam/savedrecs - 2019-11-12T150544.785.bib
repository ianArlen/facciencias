@article{ ISI:000185193000020,
Author = {Garcia-Ramos, JE and Arias, JM and Barea, J and Frank, A},
Title = {{Phase transitions and critical points in the rare-earth region}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{2003}},
Volume = {{68}},
Number = {{2}},
Month = {{AUG}},
Abstract = {{A systematic study of isotope chains in the rare-earth region is
   presented. For chains Nd-144-154(60),Sm-146-160(62),Gd-148-162(64), and
   Dy-150-166(66), energy levels, E2 transition rates, and two-neutron
   separation energies are described by using the most general (up to
   two-body terms) interacting boson model (IBM) Hamiltonian. For each
   isotope chain a general fit is performed in such a way that all
   parameters but one are kept fixed, to describe the whole chain. In this
   region, nuclei evolve from spherical to deformed shapes and a method
   based on catastrophe theory, in combination with a coherent-state
   analysis to generate the IBM energy surfaces, is used to identify
   critical phase transition points.}},
