@article{ ISI:000224131400042,
Author = {Dembowski, C and Dietz, B and Friedrich, T and Graf, HD and Heine, A and
   Mejia-Monasterio, C and Miski-Oglu, M and Richter, A and Seligman, TH},
Title = {{First experimental evidence for quantum echoes in scattering systems}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2004}},
Volume = {{93}},
Number = {{13}},
Month = {{SEP 24}},
Abstract = {{A self-pulsing effect termed quantum echoes has been observed in
   experiments with an open superconducting and a normal conducting
   microwave billiard whose geometry provides soft chaos, i.e., a mixed
   phase space portrait with a large stable island. For such systems a
   periodic response to an incoming pulse has been predicted. Its period
   has been associated with the degree of development of a horseshoe
   describing the topology of the classical dynamics. The experiments
   confirm this picture and reveal the topological information.}},
