@article{ ISI:000223803700012,
Author = {Vazquez-Garcia, M and Elias-Vinas, D and Reyes-Guerrero, G and
   Dominguez-Gonzalez, A and Verdugo-Diaz, L and Guevara-Guzman, R},
Title = {{Exposure to extremely low-frequency electromagnetic fields improves
   social recognition in male rats}},
Journal = {{PHYSIOLOGY \& BEHAVIOR}},
Year = {{2004}},
Volume = {{82}},
Number = {{4}},
Pages = {{685-690}},
Month = {{SEP 30}},
Abstract = {{The effect of exposure to extremely low-fi-equency electromagnetic
   fields (ELF EMFs) on social recognition was studied. The test was based
   upon a comparison between two encounters of an adult rat and a
   conspecific juvenile, separated by an interexposure interval (IEI). The
   exposure to ELF EMF of 1 mT intensity during 2 h for 9 days increased
   the duration of short-term memory of adult male Wistar rats up to 300
   min. These data indicate, for the first time, that ELF EMF improves
   social recognition memory in rats. (C) 2004 Elsevier Inc. All rights
   reserved.}},
