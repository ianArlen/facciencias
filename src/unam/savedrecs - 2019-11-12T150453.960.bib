@article{ ISI:000223945300023,
Author = {Ordaz, A},
Title = {{Some integrals useful in probabilistic seismic hazard analysis}},
Journal = {{BULLETIN OF THE SEISMOLOGICAL SOCIETY OF AMERICA}},
Year = {{2004}},
Volume = {{94}},
Number = {{4}},
Pages = {{1510-1516}},
Month = {{AUG}},
Abstract = {{Closed-form solutions are presented for three simple cases of
   probabilistic seismic hazard analysis: (1) point source with
   deterministic median attenuation relation, (2) point source with
   probabilistic attenuation relation, and (3) a circular source with
   probabilistic attenuation relation. These solutions are obtained for
   commonly used models of magnitude exceedance rates and attenuation
   relations. Although the expressions presented correspond to simple
   cases, rarely found in real applications, they are believed to be useful
   as canonical cases against which to calibrate the numerical codes or as
   the basis to construct quadrature formulas for more complex cases.}},
