@article{ ISI:000223750600031,
Author = {Lira, A and Camarillo, I and Camarillo, E and Ramos, F and Flores, M and
   Caldino, U},
Title = {{Spectroscopic characterization of Er3+ transitions in Bi4Si3O12}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2004}},
Volume = {{16}},
Number = {{32}},
Pages = {{5925-5936}},
Month = {{AUG 18}},
Abstract = {{A systematic investigation of optical absorption and luminescence is
   presented for Er3+ ions in the Bi4Si3O12 crystal. The intrinsic emission
   of this crystal gives rise to a radiative energy transfer among the host
   lattice and Er3+ ions. Measured oscillator strengths for seven different
   transitions from the ground state to excited state manifolds compare
   favourably with the-electric dipole oscillator strengths calculated from
   the Judd-Ofelt formalism. Radiative lifetimes of the former seven
   excited states were determined from calculated spontaneous emission
   probabilities. The ratio of the I-4(11/2) --> F-4(7/2) excited state
   absorption oscillator strength to the I-4(15/2) --> I-4(11/2) ground
   state absorption oscillator strength turned out to be similar to those
   found for materials pumped efficiently at I-4(11/2) level excitation
   wavelengths. The stimulated emission cross section and gain spectra were
   determined for the I-4(13/2) --> I-4(15/2) transition of special
   interest for laser application.}},
