@article{ ISI:000222924800027,
Author = {Baldovin, F and Moyano, LG and Majtey, AP and Robledo, A and Tsallis, C},
Title = {{Ubiquity of metastable-to-stable crossover in weakly chaotic dynamical
   systems}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
