@article{ ISI:000225362600012,
Author = {De La Macorra, A and German, G},
Title = {{Cosmology with negative potentials with omega phi <-1}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS D}},
Year = {{2004}},
Volume = {{13}},
Number = {{9}},
Pages = {{1939-1953}},
Month = {{OCT}},
Abstract = {{We study the cosmology of canonically normalized scalar fields that lead
   to an equation of state parameter of wphi = pphi/pphi < -1 without
   violating the weak energy condition: p = Sigmaipi greater than or equal
   to 0 and pi + pi greater than or equal to 0. This kind of behavior
   requires a negative scalar potential V, widely predicted in particle
   physics. We show that the energy density pphi = E-k + V takes negative
   values with an equation of state with wphi < -1. However, the net effect
   of the 0 field on the scale factor is to decelerate it giving a total
   equation of state parameter w = p/p > w(b) = Pb/Pb, where Pb stands for
   any kind of energy density with -1 less than or equal to w(b) less than
   or equal to 1, such as radiation, matter, cosmological constant or other
   scalar field with a potential V greater than or equal to 0. The fact
   that pphi < 0 allows, at least in principle, to have a small
   cosmological constant or quintessence today as the cancellation of high
   energy scales such as the electroweak or susy breaking scale. While V is
   negative \textbackslash{}pphi\textbackslash{} is smaller than the sum of
   all other energy densities regardless of the functional form of the
   potential V. We show that the existence of a negative potential leads,
   inevitable, to a collapsing universe, i.e. to a would be ``big
   crunch.{''} In this picture we would still be living in the expanding
   universe.}},
