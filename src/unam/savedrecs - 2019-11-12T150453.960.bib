@article{ ISI:000224603200008,
Author = {Perez-Gavilan, JJ and Aliabadi, MH},
Title = {{A Galerkin formulation for shear deformable plate bending dynamics}},
Journal = {{INTERNATIONAL JOURNAL FOR NUMERICAL METHODS IN ENGINEERING}},
Year = {{2004}},
Volume = {{61}},
Number = {{7}},
Pages = {{1093-1106}},
Month = {{OCT 21}},
Abstract = {{A Galerkin boundary element formulation for shear deformable plate
   bending dynamics is developed. The formulation makes use of the static
   fundamental solutions for the weighted residual integral equations. The
   domain integrals carrying the inertia terms and generic static loads are
   considered as body forces and approximated with boundary values using
   the dual reciprocity method. The load is modelled as a series of impact
   loads of time varying intensity and moving in space in a predetermined
   path. The formulation was implemented and tested solving a benchmark
   problem. The results are compared with finite element solutions.
