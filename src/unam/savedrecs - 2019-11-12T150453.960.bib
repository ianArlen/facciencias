@article{ ISI:000223791100001,
Author = {Cruz, F and Velazquez, H},
Title = {{Formation of cuspy density profiles through gravitational cold collapses
   around a central supermassive black hole}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2004}},
Volume = {{612}},
Number = {{2, 1}},
Pages = {{593-596}},
Month = {{SEP 10}},
Abstract = {{We have carried out a set of N-body simulations to investigate the
   formation of cuspy density profiles through collapses starting from cold
   initial conditions around a preexisting supermassive central black hole.
   It is found that a cold collapse leads to an inner power-law profile
   rho(-gamma), with 0.2less than or similar togammaless than or similar
   to2 depending on the mass of the central black hole as well as on the
   slope of the initial density profile rho(-gamma 0) (0less than or equal
   togamma(0)less than or equal to3/2). The upper limit obtained for gamma
   corresponds to the more massive black hole and to the steeper initial
   profile with gamma(0)=3/2. Models without a central black hole form a
   core rather than a cusp. Also, our numerical simulations with a virial
   ratio of eta=0.05 do not show compelling evidence that a radial
   anisotropy in the initial velocity distribution would play an important
   role in the formation of steeper cusps, as was suggested by previous
   authors.}},
