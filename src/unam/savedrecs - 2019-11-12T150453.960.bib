@article{ ISI:000223112600022,
Author = {Stehlik, M},
Title = {{A generalisation of matching and colouring}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2004}},
Volume = {{285}},
Number = {{1-3}},
Pages = {{257-265}},
Month = {{AUG 6}},
Abstract = {{We define an r-bounded cover of a graph G to be a subgraph T subset of
   or equal to G such that T contains all vertices of G and each component
   of T is a complete subgraph of G of order at most r. A 2-bounded cover
   of a graph G corresponds to a matching of G, and an omega(G)-bounded
   cover of G corresponds to a colouring of the vertices of the complement
   (G) over bar. We generalise a number of results on matching and
   colouring of graphs to r-bounded covers, including the Gallai-Edmonds
   Structure Theorem, Tutte's 1-Factor Theorem, and Gallai's theorem on the
   minimal order of colour-critical graphs with connected complements. (C)
   2004 Elsevier B.V. All rights reserved.}},
