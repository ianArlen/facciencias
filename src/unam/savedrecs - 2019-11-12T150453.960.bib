@article{ ISI:000223478700008,
Author = {Monsivais, G},
Title = {{The relativistic Ampere-Maxwell law in integral form}},
Journal = {{AMERICAN JOURNAL OF PHYSICS}},
Year = {{2004}},
Volume = {{72}},
Number = {{9}},
Pages = {{1178-1182}},
Month = {{SEP}},
Abstract = {{We discuss an extension of the integral form of the Ampere-Maxwell law
   for surfaces moving with relativistic velocities in the presence of
   electromagnetic fields, including when different parts of the surface
   move with arbitrary accelerated and deformed motion. We also discuss the
   case of Faraday's law and compare our result to an expression of the
   same type derived by Gelman. These general formulations are necessary
   for theoretical consistency, but are absent from textbooks. (C) 2004
   American Association of Physics Teachers.}},
