@article{ ISI:000223397400010,
Author = {Garcia-Valladares, O and Perez-Segarra, CD and Rigola, J},
Title = {{Numerical simulation of double-pipe condensers and evaporators}},
Journal = {{INTERNATIONAL JOURNAL OF REFRIGERATION-REVUE INTERNATIONALE DU FROID}},
Year = {{2004}},
Volume = {{27}},
Number = {{6}},
Pages = {{656-670}},
Month = {{SEP}},
Abstract = {{A detailed one-dimensional steady and transient numerical simulation of
   the thermal and fluid-dynamic behaviour of double-pipe heat exchangers
   (evaporators and condensers) has been carried out. The governing
   equations (continuity, momentum and energy) inside the internal tube and
   the annulus, together with the energy equation in the internal tube
   wall, external tube wall and insulation, are solved iteratively in a
   segregated manner. The discretized governing equations in the zones with
   fluid flow are efficiently coupled using an implicit step by step
   method. This formulation requires the use of empirical correlations for
   the evaluation of convective heat transfer, shear stress and void
   fraction. An implicit central difference numerical scheme and a
   line-by-line solver was used in the internal and external tube walls and
   insulation. A special treatment has been implemented in order to
   consider transitions (single-phase/two-phase, dry-out,...). All the flow
   variables (enthalpies, temperatures, pressures, mass fractions,
   velocities, heat fluxes....) together with the thermophysical properties
   are evaluated at each point of the grid in which the domain is
   discretized. Different numerical aspects and comparisons with analytical
