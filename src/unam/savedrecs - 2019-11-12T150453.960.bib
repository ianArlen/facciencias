@article{ ISI:000224516100002,
Author = {Porta-Gandara, MA and Cervantes, JG and Solorio, FJ},
Title = {{Periodic enclosed natural convection in a laboratory solar still}},
Journal = {{EXPERIMENTS IN FLUIDS}},
Year = {{2004}},
Volume = {{37}},
Number = {{4}},
Pages = {{483-487}},
Month = {{OCT}},
Abstract = {{In this paper, an experimental description of the periodic behavior of
   the flow pattern in a laboratory shallow solar still is presented. The
   convective structure in this kind of system is measured through
   visualization techniques in a laboratory apparatus that experimentally
   simulates a shallow solar still. Using a video camera, together with a
   laser sheet and smoke particles, the flow patterns that characterize the
   phenomena were captured. A typical value of 0.147 for the Strouhal
   number agrees well with previously published findings in several other
   physical situations and the numerical predictions of resonant response
   of a periodic excited cavity.}},
