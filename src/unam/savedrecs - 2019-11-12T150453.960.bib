@article{ ISI:000223143300031,
Author = {Hirsch, JG and Velazquez, V and Frank, A},
Title = {{Quantum chaos and nuclear mass systematics}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2004}},
Volume = {{595}},
Pages = {{231-236}},
Month = {{AUG 12}},
Abstract = {{The presence of quantum chaos in nuclear mass systematics is analyzed by
   considering the differences between measured and calculated nuclear
   masses as a time series described by the power law 1/f(alpha). While for
   the liquid droplet model plus shell corrections a quantum chaotic
   behavior alpha approximate to I is found, errors in the microscopic mass
   formula have alpha approximate to 0.5, closer to white noise. The
   chaotic behavior seems to arise from many body effects not included in
   the mass formula. (C) 2004 Elsevier B.V. All rights reserved.}},
