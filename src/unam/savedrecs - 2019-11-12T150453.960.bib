@article{ ISI:000224751200006,
Author = {Contreras, I and Trevino, C and Prince, JC},
Title = {{Oscillatory heat transfer process in a vertical strip immersed in a
   porous medium}},
Journal = {{HEAT AND MASS TRANSFER}},
Year = {{2004}},
Volume = {{40}},
Number = {{12}},
Pages = {{937-942}},
Month = {{OCT}},
Abstract = {{In this work, the transient conjugated heat transfer characteristics of
   a thin long vertical strip embedded in a porous medium has been studied
   using numerical and asymptotic techniques. The oscillatory
   nondimensional temperature evolution in the strip and the reduced
   Nusselt number at the top of the strip are obtained as a function of the
   Fourier number, Fo, which relates the fluctuating to the steady thermal
   penetration lengths. The numerical results of the root mean square of
   the fluctuating reduced Nusselt number have been obtained covering the
   complete transition from fast (Fomuch less than1) to slow (Fomuch
   greater than1) thermal oscillations. Asymptotic solutions are obtained
   in these two limits and compared with the numerical results.}},
