@article{ ISI:000224350100010,
Author = {Yazdizadeh, M and Tapia, JL and Baharvand, M and Radfar, L},
Title = {{A case of neurofibromatosis-Noonan syndrome with a central giant cell
   granuloma}},
Journal = {{ORAL SURGERY ORAL MEDICINE ORAL PATHOLOGY ORAL RADIOLOGY AND
   ENDODONTOLOGY}},
Year = {{2004}},
Volume = {{98}},
Number = {{3}},
Pages = {{316-320}},
Month = {{SEP}},
Abstract = {{Neurofibromatosis Type 1 (NF1) is one of the most common autosomal
   dominant diseases affecting multiple systems including the vascular,
   skeletal, and central nervous system. Noonan syndrome (NS) is an
   autosomal dominant genetic disorder, associated with musculoskeletal and
   skin manifestations. Coexistence of central giant cell lesions in
   patients with both NS and NF1 were reported in the literature.
   Development of multiple central giant cell lesions in a patient with a
   Noonan syndrome has been referred to as Noonan-like syndrome. A few
