@article{ ISI:000224074400024,
Author = {Oey, MS and Garcia-Segura, G},
Title = {{Ambient interstellar pressure and superbubble evolution}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2004}},
Volume = {{613}},
Number = {{1, 1}},
Pages = {{302-311}},
Month = {{SEP 20}},
Abstract = {{High ambient interstellar pressure is suggested as a possible factor to
   explain the ubiquitous observed growth rate discrepancy for
   supernova-driven superbubbles and stellar wind bubbles. Pressures of P/k
   similar to 10(5) cm(-3) K are plausible for regions with high star
   formation rates, and these values are intermediate between the estimated
   Galactic midplane pressure and those observed in starburst galaxies.
   High-pressure components also are commonly seen in Galactic interstellar
   medium (ISM) localizations. We demonstrate the sensitivity of shell
   growth to the ambient pressure and suggest that superbubbles ultimately
   might serve as ISM barometers.}},
