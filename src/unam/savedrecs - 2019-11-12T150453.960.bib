@article{ ISI:000224833800002,
Author = {Quintanar-Quintanar, ME and Jarillo-Luna, A and Rivera-Aguilar, V and
   Ventura-Juarez, J and Tsutsumi, V and Shibayama, M and Campos-Rodriguez,
   R},
Title = {{Immunosuppressive treatment inhibits the development of amebic liver
   abscesses in hamsters}},
Journal = {{MEDICAL SCIENCE MONITOR}},
Year = {{2004}},
Volume = {{10}},
Number = {{9}},
Pages = {{BR317-BR324}},
Month = {{SEP}},
Abstract = {{Background: The aim of the present study was to determine if the
   inflammation and/or immunosuppression induced by Entowoeba histolytica
   may contribute to amebic invasion.
   Material/Methods: Dexamethasone was administered three days before and
   three and three days after inoculation of hamsters with E.histolytica.
   Seven days alter inoculation the animals were sacrificed and the sizes
   of their amebic liver abscesses were determined. The number of
   neutrophils, macrophages, T and B cells in the peritoneum as well as the
   production of nitric oxide and susceptibility to Listeria monacytogens
   infection was determined.
   Results: Dexamethasone treatment significantly reduced the number of T
   lymphocytes in thymus and spleen. The number of neutrophils, macrophages
   and T lymphocytes in the peritoneal exudate was also reduced as well as
   the production of the nitric oxide adn the microbicidal activity against
   Listeria monocytogenes. However, in the animals treated with a high dose
   of dexamethasone the size of the liver abcesses was significantly
   smaller than in the untreated animals.
   Conclusions: The results suggest that macrophage and T cell-mediated
   immunity is not relevant as a protective mechanism because tissue
   invasion by E.histolytica was reduced in immunosuppressesd animals. On
   the contrary, the inflammation process may contribute to the invasion
   and liver damage.}},
