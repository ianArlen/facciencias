@article{ ISI:000224452700001,
Author = {Flores-Alamo, M and Sosa-Torres, ME and Toscano, RA and Camarillo, E and
   Hernandez, JM and Murrieta, H},
Title = {{Crystal structure and energy transfer of
   trans-RSSR-{[}CrCl2(cyclam)](3){[}Cr(CN)6] center dot 14H(2)O}},
Journal = {{INORGANIC CHEMISTRY COMMUNICATIONS}},
Year = {{2004}},
Volume = {{7}},
Number = {{10}},
Pages = {{1087-1090}},
Month = {{OCT}},
Abstract = {{Within the study of the counterion effect on the optical properties of
   the cation in trans-RSSR-{[}CrCl2(cyclam)](2)ZnCl4, we synthesised and
   characterised by spectroscopic techniques and X-ray crystallography the
   trans-RSSR-{[}CrCl2(cyclaM)](3){[}Cr(CN)6] (.) 14H(2)O (1) complex. The
   X-ray structural analysis of 1 shows that the crystal network involves
   hydrogen bonding distances of (N(1)H(1)N-...(27)C(26) 2.29(7) and
   N(18)H(18)N-...(27)C(26) 2.33(6) Angstrom, and distances between cation
   and anion: Cr(1)Cr-...(3) and Cr(2)Cr-...(3) were found to be 6.999 and
   6.924 Angstrom. The lifetime emission for Cr(1)(2) was found to be 17
   mus, while the lifetime for Cr(3) was found to be 100 mus. These values
   are a consequence of an energy transfer process. This energy transfer
   through the supramolecular interactions of the cation and anion via
   hydrogen bonding is probably responsible for the optical properties
   shown by 1 in the solid state. (C) 2004 Published by Elsevier B.V.}},
