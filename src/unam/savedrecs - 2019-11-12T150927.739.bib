@article{ ISI:A1997YA57400012,
Author = {Salgado, M and Sudarsky, D and Quevedo, H},
Title = {{Has cosmological dark matter been observed?}},
Journal = {{PHYSICS LETTERS B}},
Year = {{1997}},
Volume = {{408}},
Number = {{1-4}},
Pages = {{69-74}},
Month = {{SEP 11}},
Abstract = {{We present evidence supporting a model in which the cosmological dark
   matter corresponds to a scalar field, which can be `'seen'', however
   indirectly, in the oscillation of the effective gravitational constant,
   and manifests itself in the periodicity of the number density
   distribution of galaxies observed by Broadhurst et al. {[}T. Broadhurst,
   R. Ellis, D. Koo and A. Szalay, Nature 343 (1990) 726]. A numerical
   analysis shows that the model successfully passes all cosmological
   tests, a fact that lends support to the conclusion that approximate to
   98\% of the energy density of the Universe is stored in this scalar
   field. This suggests that, if the observations of the galactic
   periodicity are not a statistical fluke, the cosmological component of
   dark matter might already have been observed. (C) 1997 Published by
   Elsevier Science B.V.}},
