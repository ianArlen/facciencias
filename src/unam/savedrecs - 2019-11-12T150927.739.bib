@article{ ISI:000077134100003,
Author = {Ayala, HE and Echeverria, L and Sobell, MB and Sobell, LC},
Title = {{Directed self control: Brief interventions for problem drinkers in
   Mexico}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1997}},
Volume = {{14}},
Number = {{2}},
Pages = {{113-127}},
Month = {{DEC}},
Abstract = {{An out-patient cognitive-behavioral treatment program was applied to a
   sample of 177 subjects with alcohol related problems in Mexico City. The
   treatment program is a motivational intervention based on self-control
   procedures, a functional analysis of drinking behavior and the
   identification of risk situations for excessive drinking as a basis to
   promote a change in the individual's addictive behavior. Results
   revealed that the subject's excesive drinking pattern changed to a
   moderate drinking one, as a result of their participation in the
   treatment program. This change was maintained at six and 12-month
   follow-ups. Change is discussed in terms of promotion polices for early
   and brief interventions with these populations, as well as the need to
   systematically evaluate their effectiveness.}},
