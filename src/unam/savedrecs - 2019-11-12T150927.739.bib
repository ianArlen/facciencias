@article{ ISI:000071056000008,
Author = {Stephens, CR},
Title = {{Geometric and kinematic dimensional reduction in quantum field theory}},
Journal = {{MODERN PHYSICS LETTERS A}},
Year = {{1997}},
Volume = {{12}},
Number = {{37}},
Pages = {{2905-2917}},
Month = {{DEC 7}},
Abstract = {{Dimensional reduction in quantum field theory is discussed with emphasis
   on the difference between ``geometric{''} versus ``kinematic{''}
   dimensional reduction. It is shown how environmentally friendly
   renormalisation can also be applied to the latter case. As concrete
   examples, finite temperature lambda phi(4) theory and the Regge limit of
   phi(2) psi are considered. In the latter, crossover between large and
   small energies at fixed momentum transfer is considered and asymptotic
   forms of the crossover scaling functions are exhibited for all
   two-particle scattering processes in this channel.}},
