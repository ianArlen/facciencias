@article{ ISI:000071236100011,
Author = {Fernandez-Anaya, G and Munoz-Gutierrez, S and Sanchez-Guzman, RA and
   Mayol-Cuevas, WW},
Title = {{Simultaneous stabilization using evolutionary strategies}},
Journal = {{INTERNATIONAL JOURNAL OF CONTROL}},
Year = {{1997}},
Volume = {{68}},
Number = {{6}},
Pages = {{1417-1435}},
Month = {{DEC}},
Abstract = {{In this work a new procedure is presented for the simultaneous
   stabilization of linear time invariant SISO plants; evolutionary
   strategies and substitutions by polynomials and rational functions are
   used. The algorithm presented requires minimal computational resources
   for its execution. In addition, it is possible to work with some types
   of plants that could not be simultaneously stabilized until now, and
   there are many more of these than is described in the literature. The
   restrictions are minimal and the method can be extended with little
   difficulty to MIMO plants. Moreover, a mechanism of substitutions by
   polynomials and rational function is presented, and this mechanism
   preserves simultaneously stabilizable plants and their controllers.
   Several examples are given, showing the advantages of these procedures.}},
