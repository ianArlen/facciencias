@article{ ISI:A1997YE24300002,
Author = {Wang, S},
Title = {{Near-field optical image of a homogeneous dielectric surface structure:
   A passive probe model}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1997}},
Volume = {{143}},
Number = {{4-6}},
Pages = {{179-184}},
Month = {{NOV 15}},
Abstract = {{We have studied the constant-intensity and constant-height near-field
   image using a passive probe model based on a non-perturbative Rayleigh
   method. We calculated the optical signal along a line above a dielectric
   substrate with a rectangular bar and a rectangular groove placed on the
   surface. Two-dimensional numerical analysis has been carried out with
   s-polarization. We find out that the transmitted image is correlated
   with the shape of the medium of high refractive index. The near-field
   intensity is sensible to the lateral and axial size of the surface
   structure and the angle of incidence of the light. Our numerical
   simulation is in good agreement with the experimental observation. (C)
   1997 Elsevier Science B.V.}},
