@article{ ISI:A1997XY84100008,
Author = {Dubois, M and Weisbrod, A and Shtuka, A and MartinezSerrano, R},
Title = {{The low-temperature (T<120 degrees C) H2O-RbCl phase diagram -
   Comparison with other water alkali chloride systems}},
Journal = {{EUROPEAN JOURNAL OF MINERALOGY}},
Year = {{1997}},
Volume = {{9}},
Number = {{5}},
Pages = {{987-992}},
Month = {{SEP-OCT}},
Abstract = {{The eutectic temperature in the H2O-RbCl system was determined using
   synthetic fluid inclusions. Two experiments were carried out using a
   39.99+/-0.01 wt\% RbCl solution. By isolating the last solid (solid
   RbCl) and freezing the remaining liquid, the temperature at which the
   liquid begins to melt was accurately measured (-16.4+/-0.1 degrees C)
   during subsequent heating measurements. The ice melting (ILV) and salt
   melting (SLV) curves were fitted using a constrained least square
   procedure and the eutectic composition was calculated (39.55+/-0.01 wt\%
   RbCl). The phase diagram is similar to those of the H2O-KCl and H2O-CsCl
   systems but differ from those including the smallest alkali ions (LiCl
   and NaCl) which are complicated by occurrences of hydrates.}},
