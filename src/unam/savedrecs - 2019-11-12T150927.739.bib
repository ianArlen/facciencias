@article{ ISI:A1997YC43000024,
Author = {GarciaColunga, J and Miledi, R},
Title = {{Opposite effects of lanthanum on different types of nicotinic
   acetylcholine receptors}},
Journal = {{NEUROREPORT}},
Year = {{1997}},
Volume = {{8}},
Number = {{15}},
Pages = {{3293-3296}},
Month = {{OCT 20}},
Abstract = {{THE effects of lanthanum (La3+) were studied on muscle and neuronal
   nicotinic acetylcholine receptors (AChRs) expressed in Xenopus oocytes.
   La3+ exerts a dose-dependent positive modulation on alpha 1 beta 1 gamma
   delta muscle AChRs, whereas it modulates negatively either alpha 2 beta
   2, alpha 2 beta 4 or alpha 3 beta 4 neuronal AChRs. Moreover, La3+
   appears to accelerate the desensitization of neuronal receptors. In both
   muscle and neuronal AChRs, the respective potentiating or inhibiting
   effects of La3+ on the ACh-currents are voltage-independent, suggesting
   that La3+ is acting at a site located in the external domain of the
   receptor.}},
