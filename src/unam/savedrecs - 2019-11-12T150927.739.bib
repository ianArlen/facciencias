@article{ ISI:A1997YK78700009,
Author = {MolinaCruz, A},
Title = {{Closing of the Central American Gateway and its effects on the
   distribution of Late Pliocene radiolarians in the eastern tropical
   Pacific}},
Journal = {{TECTONOPHYSICS}},
Year = {{1997}},
Volume = {{281}},
Number = {{1-2}},
Pages = {{105-111}},
Month = {{NOV 15}},
Note = {{VIth International Congress on Pacific Neogene Stratigraphy / Oji
   Seminar for IGCP Project No 355 on Neogene Evolution of Pacific Ocean
   Gateways, SERPONG, INDONESIA, JUL 02-09, 1995}},
Organization = {{Reg Comm Pacific Neogene Stratig; SNS; IUGS; IGCP 355}},
Abstract = {{Analysis of radiolarians in Late Pliocene sediments at DSDP sites 158,
   476, and 490 off the Pacific coasts of Mexico and Central America was
   undertaken to evaluate the paleoceanographic impact on the tropical
   eastern Pacific Ocean of the closing of the Central American Gateway,
   which occurred ca. 3.2 Ma. Results of the analysis indicate that (1) the
   relative abundance of radiolarian tests is extremely low in Late
   Pliocene sediments relative to their abundance in older and younger
   sediments, and (2) the relative abundance of nassellarians is also
   significantly depressed in this interval. Tectonism associated with the
   uplift, closing and subsequent development of the gateway and the
   Pacific margins of Central America and Mexico apparently triggered an
   increased flux of terrigenous sediment into the eastern tropical
   Pacific. This increased flux is thought to be responsible for the Late
   Pliocene dilution of radiolarian abundance and a coincident change in
   habitat of nassellarians in this region. Closing of the Central American
   Gateway was also responsible for the reorganization of surface
   circulation in the tropical eastern Pacific. The time-transgressive
   appearance of radiolarian species noted in Pliocene sediments at DSDP
   sites along the Pacific coasts of Central America and Mexico likely
   reflects development of the Costa Rican Coastal Current.}},
