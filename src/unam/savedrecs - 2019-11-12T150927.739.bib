@article{ ISI:A1997YG74200018,
Author = {Gonzalez, MM and Brandan, ME},
Title = {{Application of the dispersion relation to the radial dependence of the
   real optical potential for C-12+C-12 at intermediate energies}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1997}},
Volume = {{43}},
Number = {{1}},
Pages = {{137-143}},
