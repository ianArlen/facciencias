@article{ ISI:000071820800007,
Author = {Chicurel, E},
Title = {{Stress calculation in welds taking into account the bearing force
   between the welded parts}},
Journal = {{JOURNAL OF STRAIN ANALYSIS FOR ENGINEERING DESIGN}},
Year = {{1997}},
Volume = {{32}},
Number = {{6}},
Pages = {{461-465}},
Month = {{NOV}},
Abstract = {{Direct normal stresses in welds are usually calculated assuming that, in
   addition to resisting the tension and the shear loads, they alone resist
   all of the compression load even when the welded parts make contact in a
   large area. A method is proposed here assuming that, as far as direct
   normal stresses are concerned, the welds resist the tension stresses
   while the welded parts themselves resist a portion (quite often a very
   substantial one) of the compression stresses.}},
