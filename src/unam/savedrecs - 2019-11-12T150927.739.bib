@article{ ISI:A1997YF44300009,
Author = {Bedolla, SG and Sareyan, JP and Chauville, J and Flores, D},
Title = {{Discrepant photometric amplitudes in the pulsation of EN Lacertae}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{1997}},
Volume = {{33}},
Number = {{2}},
Pages = {{137-141}},
Month = {{OCT}},
Abstract = {{We show here that recent ephemeris, accounting for the light curve of EN
   Lac in the 1980's cannot represent all the observations made at that
   time. Likely explanations can be: either rapid variations in the
   amplitude of the third period (0.182d), or large imprecisions in the
   published amplitudes of the two first periods (0.169 and 0.171d). Thus,
   if we accept the periods previously determined, a serious doubt is
   raised against the amplitudes published previously.}},
