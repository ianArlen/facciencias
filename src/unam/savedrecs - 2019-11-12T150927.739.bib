@article{ ISI:000070981000044,
Author = {Alvarez-Fregoso, O and Chavez-Carvayar, JA and Juarez-Islas, JA and
   Camps, E and Garcia, JL},
Title = {{FeN and FeN-H thin films prepared by ECR microwave plasma nitriding}},
Journal = {{SURFACE \& COATINGS TECHNOLOGY}},
Year = {{1997}},
Volume = {{94-5}},
Number = {{1-3}},
Pages = {{255-260}},
Month = {{OCT}},
Note = {{24th International Conference on Metallurgical Coatings and Thin Films
   (ICMCTF 97), SAN DIEGO, CALIFORNIA, APR 21-25, 1997}},
Organization = {{Amer Vacuum Soc, Vacuum Met \& Thin Films Div}},
Abstract = {{Iron-nitrided films were obtained by using an electron cyclotron
   resonance (ECR) plasma system. The dependence of crystalline morphology,
   structure and growth mechanism of the films was studied as a function of
   the hydrogen/nitrogen ratio in the working atmosphere. A single-phase
   epsilon-Fe3N, semihard compound, can be synthesized under an 80\%
   H-2/20\% N-2 atmosphere. Scanning electron microscopy (SEM) on the
   cross-section of the single-phase compound revealed a bilayer structure
   consisting of dense columnar and needlelike grains. SEM results suggest
   that the FeN layer was formed from melting of the iron surface due to
   the ion bombardment. Other H-2/N-2 concentrations and conditions led to
   mixed phase layers. The thickness of the FeN layers increases as the
   H-2/N-2 ratio increases. The X-ray diffraction patterns, atomic force
   images and Vicker hardness of the FexN films are presented. (C) 1997
   Elsevier Science S.A.}},
