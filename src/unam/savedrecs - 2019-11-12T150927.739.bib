@article{ ISI:000071508000007,
Author = {Mendez, N and Romero, J and Flos, J},
Title = {{Population dynamics and production of the polychaete Capitella capitata
   in the littoral zone of Barcelona (Spain, NW Mediterranean)}},
Journal = {{JOURNAL OF EXPERIMENTAL MARINE BIOLOGY AND ECOLOGY}},
Year = {{1997}},
Volume = {{218}},
Number = {{2}},
Pages = {{263-284}},
Month = {{DEC 1}},
Abstract = {{The population dynamics and production of the opportunistic species
   Capitella capitata was studied in the littoral zone of Barcelona.
   Sampling was carried out from July, 1991 to June, 1992, with a frequency
   ranging between 5 and 35 days. Two sampling sites subjected to different
   degrees of organic pollution were chosen: the Besos site, which is
   located near the main pollution focus, and the Olympic Harbour site,
   about 1.5 miles southwards. In both sites, continuous recruitment during
   the whole year was observed. The maximum longevity was about 43 days and
   was inversely correlated with growth. The presence of trochophore and
   metatrochophore larvae inside the parental tubes indicates the
   non-pelagic development of this species in both studied sites. Estimated
   C. capitata production was about 47 g m(-2) year(-1) with an annual P/B
   ratio of 23 at the Olympic Harbour site and about 270 g m(-2) year(-1)
   with an annual P/B ratio of 36 at the Besos site. These differences
   observed at the two sites indicate that both populations respond to
   different energy inputs through different reproductive strategies: the
   Besos populations produce higher density and biomass supported by
   individuals with smaller size ranges and higher production values with a
   turnover almost twice than at the Olympic Harbour site. (C) 1997
   Elsevier Science B.V.}},
