@article{ ISI:A1997XW98400009,
Author = {Salgado, P and Rojas, R and Sotelo, J},
Title = {{Cysticercosis - Clinical classification based on imaging studies}},
Journal = {{ARCHIVES OF INTERNAL MEDICINE}},
Year = {{1997}},
Volume = {{157}},
Number = {{17}},
Pages = {{1991-1997}},
Month = {{SEP 22}},
Abstract = {{Cysticerosis is a pleomorphic disease in which the diagnosis, treatment,
   and prognosis greatly vary in relation to the location, number, and
   stage of the lesions. A new classification if proposed that includes the
   information gathered by magnetic resonance imaging and computed
   tomography, supplemented by immunodiagnostic studies and clinical data.
   The different clinical entities produced by cysticerosis are grouped by
   the frequency and location of the parasites, including valuable data as
   the number and stage of the lesions as well as the degree of th
   inflammatory response from the host to the parasite. In this way, the
   disease can be individualized to define the reliability of diagnostic
   studies and to design specific therapeutic approaches that depend on the
   characteristics that the disease acquires in every location.}},
