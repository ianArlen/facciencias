@article{ ISI:000077134100001,
Author = {Ribes, E and Moreno, D and Martinez, C},
Title = {{Interaction of descriptive verbal repertories and visual demonstration
   training in a matching task performance with children}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1997}},
Volume = {{14}},
Number = {{2}},
Pages = {{89-103}},
Month = {{DEC}},
Abstract = {{Two experiments were carried out with 910 year-old children in order to
   evaluate the interaction of the verbal description of relations,
   second-order stimuli in matching-to-sample tasks and the use of a visual
   demonstration procedure during the acquisition of a conditional
   discrimination. In both experiments children were selected by their
   previous descriptions of their performance in a sorting and a matching
   test. In Experiment 1 children were trained to respond to a second-order
   matching-to-sample task involving identity and similarity relations.
   After training, children were exposed to intramodal, extramodal and
   extrarelational transfer tests. Children with non-relevant descriptive
   behavior showed better performance during training and transfer tests.
   In Experiment 2, children were trained to respond to a first-order
   matching-to-sample task involving identity, similarity and difference
   relations, and they were exposed to intramodal and extramodal transfer
   tests. During training, three initial sessions were superimposed with a
   visual procedure demonstrating the correct matching relation. No
   differences were observed in the performance of children selected
   according to relevant and non-relevant descriptions. The outcome of both
   experiments suggest that available descriptive behavior has varied
   effects on performance in a conditional discrimination depending on the
   use of second-order stimuli and visual demonstration plus verbal
   descriptions.}},
