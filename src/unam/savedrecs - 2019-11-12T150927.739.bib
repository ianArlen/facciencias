@article{ ISI:A1997XY12500003,
Author = {Alfaro, HB and Ferro, AA and Schuster, WJ},
Title = {{Temperatures for A0-K0 supergiants from 13-color photometry}},
Journal = {{PUBLICATIONS OF THE ASTRONOMICAL SOCIETY OF THE PACIFIC}},
Year = {{1997}},
Volume = {{109}},
Number = {{739}},
Pages = {{958-968}},
Month = {{SEP}},
Abstract = {{Observations on the 13-color photometric system are reported for 71
   A0-K0 supergiant stars brighter than V = 6.0. Three independent methods
   to determine the effective temperatures for A0-K0 supergiants from their
   13C photometry are discussed: (1) Calibrations between T-eff and
   reddening-free indices are developed; temperatures were collected from
   the literature for the calibration stars, and most of them were
   spectroscopically determined. (2) An empirical correlation between
   intrinsic colors in the 13C system and T-eff has been derived. (3) The
   unreddened colors are compared with synthetic colors calculated by
   Kurucz (1989, personal communication); this leads to simultaneous
   estimates for T-eff and log g. The estimated uncertainties in T-eff by
   the three methods are comparable and are approximately +/- 200-300 K.}},
Publisher = {{ASTRONOMICAL SOC PACIFIC}},
Address = {{390 ASHTON AVE, SAN FRANCISCO, CA 94112}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Alfaro, HB (Reprint Author), CNRS,OBSERV MEUDON,DAEC,F-92195 MEUDON,FRANCE.
   CNRS,IRA 173,F-92195 MEUDON,FRANCE.
