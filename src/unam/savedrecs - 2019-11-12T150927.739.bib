@inproceedings{ ISI:000076562900003,
Author = {Brown, AGA},
Editor = {{Gilmore, G and Howell, D}},
Title = {{The initial mass function in nearby OB associations}},
Booktitle = {{STELLAR INITIAL MASS FUNCTION: 38TH HERSTMONCEUX CONFERENCE}},
Series = {{Astronomical Society of the Pacific Conference Series}},
Year = {{1998}},
Volume = {{142}},
Pages = {{45-59}},
Note = {{38th Herstmonceux Conference on Stellar Initial Mass Function, INST
   ASTRON, ROYAL GREENWICH OBSERV, CAMBRIDGE, ENGLAND, JUL 14-18, 1998}},
Abstract = {{In this contribution the current knowledge on the initial mass function
   in nearby OB associations is reviewed and new results based on data from
   the Hipparcos Catalogue are presented. Previously, astrometric
   membership determinations in OB associations were available only for
   stars of spectral type earlier than B5. Using newly developed methods to
   search for moving groups in the Hipparcos data, the lists of previously
   known B and A-type members are significantly improved and many new
   members are identified, including a significant number of F stars. This
   information is used to construct improved initial mass functions for the
   nearby associations. Although much of the emphasis in the investigation
   of associations has always been on the high mass O and B stars, recent
   work based on X-ray and Her surveys and optical followups has revealed a
   significant low-mass population in the Sco OB2 and Ori OB1 associations.
   These results and the implications for the overall IMF in nearby OB
   associations will be discussed.}},
