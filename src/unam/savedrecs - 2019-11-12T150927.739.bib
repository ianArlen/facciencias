@article{ ISI:A1997YA78000001,
Author = {Hacyan, S},
Title = {{Bell's inequality violation with four entangled fermions}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{1997}},
Volume = {{56}},
Number = {{4}},
Pages = {{R2489-R2490}},
Month = {{OCT}},
Abstract = {{A simple scheme for a Bell-inequality experiment employing four spin-1/2
   particles is presented. Two pairs of fermions, say, P-1-P-2 and P-3-P-4,
   are emitted in different directions; then, particles P-1-P-3 and P-2-P-4
   meet at two causally disconnected regions where their phases are
   shifted. The result is a correlation between the spin states of distant
   pairs of particles that violates Bell's inequality. The required phase
   shifts could be manipulated, for instance, with magnetic fields.}},
