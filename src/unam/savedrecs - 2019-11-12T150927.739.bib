@article{ ISI:A1997YA04200014,
Author = {Bryce, M and Lopez, JA and Holloway, AJ and Meaburn, J},
Title = {{A bipolar, knotty outflow with velocities of 500 kilometers per second
   or above from the engraved hourglass planetary nebula MyCn 18}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1997}},
Volume = {{487}},
Number = {{2, 2}},
Pages = {{L161+}},
Month = {{OCT 1}},
Abstract = {{High spectral resolution observations of the {[}N II] 6548 + 6584
   Angstrom and H alpha emission lines from the young planetary nebula MyCn
   18 have been obtained using the Manchester echelle spectrometer combined
   with the Angle-Australian Telescope. These spatially resolved spectra
   reveal not only the kinematical characteristics of the two bright rings
   of emission, which define the well-known `'engraved hourglass''
   structure of this object, but also the dramatic motions of the faint,
   irregular knots of emission observed outside the main nebular structure.
   The knots are moving with velocities equal to or greater than 500 km
   s(-1) away from the central star, an order of magnitude faster than the
   motions observed from the main nebula, making them among the highest
   velocity outflows ever observed from a planetary nebula. A bipolar,
   rotating, episodic jet mechanism is favored for their origin.}},
