@article{ ISI:000071696800005,
Author = {Arevalo, RV and Escursell, RMR},
Title = {{The role of family in eating disorders}},
Journal = {{PSICOLOGIA CONDUCTUAL}},
Year = {{1997}},
Volume = {{5}},
Number = {{3}},
Pages = {{391-407}},
Month = {{DEC}},
Abstract = {{The family represents a highly specific social context within which
   attitudes and behaviors related to weight, appearance and eating are
   developed. Therefore, studies to understand the family's role in eating
   disorders are necessary. The aim of this paper was to study three
   dimensions of family life which are relevant far eating disorders.
   parental attitudes toward weight, family dynamics and eating patterns.
   Sixteen families participated -mother, father and daughter with eating
   disorder -, they were given three tests; one of family environment (FES)
   (Moos y Moos, 1986), another about altitudes toward body shape (CIMEX)
   (Toro, Salamero y Martinet, 1994) and a questionnaire of eating
   patterns. The results show that there is a similarity between mothers
   and daughters about family perceptions and eating patterns. The
   daughters were more vulnerable in front of socials stereotypes than
   their parents and they differed significantly with their father in their
   eating patterns. The fathers seem to encourage organization and
   achievement.}},
