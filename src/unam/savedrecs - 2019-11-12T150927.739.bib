@article{ ISI:A1997XY94800009,
Author = {Lopez, F and Reyes, J and Campillo, B and AguilarSahagun, G and
   JuarezIslas, JA},
Title = {{Rapid solidification of copper alloys with high strength and high
   conductivity}},
Journal = {{JOURNAL OF MATERIALS ENGINEERING AND PERFORMANCE}},
Year = {{1997}},
Volume = {{6}},
Number = {{5}},
Pages = {{611-614}},
Month = {{OCT}},
Abstract = {{Rapid solidification has been employed to develop
   high-strength/high-conductivity copper alloys, because it offers
   advantages not achievable by conventional ingot metallurgy practice. The
   effect of rapid solidification on mechanical properties and electrical
   conductivity on copper alloys (with and without heat treatment) has been
   studied. Results indicated that alloys of the Cu-Cr-Zr type, rapidly
   solidified and aged, show a good combination of electrical conductivity
   {[}45.82 x 10(6)(1/Omega.m)] and microhardness Vickers (24.46 x 10(6)
   Pa) values. These values are superior to those of optimally aged
   conventional copper alloys for resistance welding electrode
   applications.}},
