@article{ ISI:A1997YF44300012,
Author = {Lebron, M and Rodriguez, LF},
Title = {{Photodissociated H I in NGC 2023}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{1997}},
Volume = {{33}},
Number = {{2}},
Pages = {{165-171}},
Month = {{OCT}},
Abstract = {{We report the detection of photodissociated H I tovvard the reflection
   nebula NGC 2023. The Very Large Array observations reveal compact
   (similar to 0.3 pc) emission associated with the source. The observed
   spectrum has a double-peaked profile that we believe is caused by
   absorbing foreground neutral gas. We corrected the observed spectrum for
   the absorption, and determined with this absorption-corrected profile an
   HI column density and mass of 1.7 x 10(21) cm(-2) and 1.2 M.,
   respectively. This column density agrees well with theoretical model
   predictions. The HI 21 cm map shows a morphology very similar to that
   present in the images of {[}C II] at 158 mu m, HIRES 60 mu m IRAS, and
   POSS optical.}},
