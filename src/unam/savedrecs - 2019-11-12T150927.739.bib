@article{ ISI:A1997YG73900002,
Author = {Salvador, J and Cortes, JF and Villa, GGY},
Title = {{Qualitative properties of the execution in Rey's Complex Figure for
   children along their development in an open population}},
Journal = {{SALUD MENTAL}},
Year = {{1997}},
Volume = {{20}},
Number = {{3}},
Pages = {{9-14}},
Month = {{SEP}},
Abstract = {{This work presents the analysis of the main qualitative aspects of the
   execution of 750 normal subjets on the Key Complex Figure Test for
   children, age range 4 to 8 years, from a systematized registration of
   the possible errors that the subjects make along the in development, in
