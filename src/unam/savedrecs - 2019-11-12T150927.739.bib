@article{ ISI:A1997YA85400014,
Author = {Seade, J and Verjovsky, A},
Title = {{Topological invariance of the Euler class for singular foliations}},
Journal = {{COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE I-MATHEMATIQUE}},
Year = {{1997}},
Volume = {{325}},
Number = {{6}},
Pages = {{645-648}},
Month = {{SEP}},
Abstract = {{Let F be an oriented singular foliation of dimension p on an oriented,
   compact manifold M of dimension n. If the singular set of F has
   dimension strictly less that (n - p - 1), then F has a well defined
   Euler class, characterized by the fact that, when restricted to the
   regular part of F, it coincides with the usual Euler class of the
   tangent bundle of F. In this Note, we prove that the Euler class of F is
   a topological invariant.}},
