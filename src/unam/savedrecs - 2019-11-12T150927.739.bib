@article{ ISI:000071082200001,
Author = {Silaghi-Dumitrescu, L and Haiduc, I and Cea-Olivares, R and
   Silaghi-Dumitrescu, I and Escudie, J and Couret, C},
Title = {{On the structure of bis(9-fluorenyl)dimethylsilane: a sterically crowded
   molecule with relatively low barriers of rotations around the
   Si-C(fluorenyl) bonds - X-ray diffraction analysis and AM1 molecular
   orbital calculations}},
Journal = {{JOURNAL OF ORGANOMETALLIC CHEMISTRY}},
Year = {{1997}},
Volume = {{545}},
Pages = {{1-8}},
Month = {{OCT 30}},
Abstract = {{The X-ray structure of bis(9-fluorenyl)dimethylsilane, the first silane
   compound containing two voluminous fluorenyl groups, has been
   determined. The crystal system is orthorhombic with Aba2 space group.
   The unit cell (a = 18.672, b = 36.089, c = 12.802 Angstrom) contains two
   sets of four molecules, each one of one set being symmetrically related
   with the others of the same set. The conformations around the
   HC(fluorenyl)SiC(fluorenyl) bonds may be grossly described as in-in,
   in-out and out-out. The AM1 calculations have shown that the barrier
   between the in-out and in-in conformations is relatively low while that
   between the in-out and out-out is higher; thus, probably, the first two
   are the most populated in solution. These results provide a better
   understanding of some unusual chemical shifts previously observed in
   (1)H-NMR for Me(2)Si(CHR(2))(2) and Me(2)Si(CHR(2))(CR'R(2)). (C) 1997
   Elsevier Science B.V.}},
