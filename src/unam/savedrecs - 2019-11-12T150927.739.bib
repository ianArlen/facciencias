@article{ ISI:A1997YH44500048,
Author = {Bokhimi, X and Morales, A and Novaro, O and Lopez, T and Chimal, O and
   Asomoza, M and Gomez, R},
Title = {{Effect of copper precursor on the stabilization of titania phases, and
   the optical properties of Cu/TiO2 prepared with the sol-gel technique}},
Journal = {{CHEMISTRY OF MATERIALS}},
Year = {{1997}},
Volume = {{9}},
Number = {{11}},
Pages = {{2616-2620}},
Month = {{NOV}},
Abstract = {{In the sol-gel Cu/TiO2 system, nanocrystalline brookite, which was
   stable even after annealing the sample at 400 degrees C, was generated
   in high concentrations. This was achieved with copper chloride as copper
   precursor. The respective samples had a light absorption band at 562 nm
   that corresponded to an energy bandgap of 2.30 eV, which is smaller than
   the values reported for titania. When copper sulfate was the precursor,
   brookite was absent, but nanocrystalline anatase, the main phase, was
   stabilized even at 800 degrees C by sulfate ions. After annealing the
   samples at low temperatures, copper was well dispersed for 1 wt \%
   copper, but part of it segregated forming copper compounds when the
   copper concentration was 10 wt \%. By annealing the samples at 800
   degrees C, copper oxidized to CuO; except in the 10 wt \%-copper sample
   prepared with copper sulfate, where it formed antlerite.}},
