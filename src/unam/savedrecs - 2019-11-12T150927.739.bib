@article{ ISI:A1997YG73900005,
Author = {PerezRincon, H and Cortes, J and Ortiz, S and Pena, J and Ruiz, J and
   DiazMartinez, A},
Title = {{Validation and standardization of the Spanish version of Toronto's
   Modified Scale of Alexitimia}},
Journal = {{SALUD MENTAL}},
Year = {{1997}},
Volume = {{20}},
Number = {{3}},
Pages = {{30-34}},
Month = {{SEP}},
Abstract = {{This study reports the application for validation and standardization
   purposes, of the Spanish version of the Modified Toronto Alexythimia
   Scale (TAS 20) in a population of 153 subjects under the maximum
   variance sampling criteria The validity of the items was determined by
   Johnson's method; its reliability was calculated by Cronbach's alpha
   coefficient. The factorial structure and communality were determined, as
   well as the psychometric structure. We conclude that its application
   meets the validity and reliability criteria. The fact that our factorial
   analysis separates only two factors instead of the three described by
   the authors of the instrument is discussed.}},
