@article{ ISI:000071586100014,
Author = {Cuevas, G and Juaristi, E and Vela, A},
Title = {{Rationalization of the anomalous H-1 NMR chemical shifts in
   1,3-diheterocyclohexanes}},
Journal = {{THEOCHEM-JOURNAL OF MOLECULAR STRUCTURE}},
Year = {{1997}},
Volume = {{418}},
Number = {{2-3}},
Pages = {{231-241}},
Month = {{NOV 24}},
Abstract = {{Ab initio calculations, within the frame of density functional theory,
   were carried out on cyclohexane, 1,3-dioxane, 1,3-dithiane and
   1,3-oxathiane. The H-1 and C-13 NMR shifts were estimated according to
   the recently developed sum-over-states density functional perturbation
   theory (SOS-DFTPT) as implemented in a modified deMon-KS program. The
   calculations reproduced the observed NMR chemical shifts, quantitatively
   in the case of H-1 nuclei, and qualitatively in the case of C-13 nuclei.
   Analysis of the relevant orbital contributions to the NMR shielding
   factors does not provide support of the stereoelectronic interactions
   which have been generally ascribed to the so-called `Perlin effects' and
   `reverse Perlin' effects. (C) 1997 Elsevier Science B.V.}},
