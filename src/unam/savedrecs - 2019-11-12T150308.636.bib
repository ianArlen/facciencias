@article{ ISI:000236007900012,
Author = {Beramendi-Orosco, LE and Vane, CH and Cooper, M and Sun, CG and Large,
   DJ and Snape, CE},
Title = {{Evaluation of errors associated with delta(13) C analysis of
   lignin-derived TMAH thermochemolysis products by gas
   chromatography-combustion-isotope ratio mass spectrometry}},
Journal = {{JOURNAL OF ANALYTICAL AND APPLIED PYROLYSIS}},
Year = {{2006}},
Volume = {{76}},
Number = {{1-2}},
Pages = {{88-95}},
Month = {{JUN}},
Abstract = {{This study reports the compound specific stable carbon isotope
   compositions (delta(13)C) of lignin tetramethylammonium hydroxide (TMAH)
   chemolysates obtained with gas chromatography-combustion-isotope ratio
   mass spectrometry (GC-C-IRMS). The possible sources of the errors
   associated are considered. Off-line (TMAH) thermochemolysis was
   performed on wood samples and the delta(13)C values of the chemolysates
   were compared with the bulk delta(13)C of the native woods and their
   Klason lignins. For the four woods investigated, the delta(13)C values,
   corrected for derivative carbons added, were spread over a wide range of
   values, ranging from -40 to -25 parts per thousand and were, on average,
   depleted in C-13 by ca. 9 parts per thousand relative to the native
   woods and by ca. 7 parts per thousand relative to the Klason lignins.
   This large variability can be partially attributed to overlapping
   chromatographic peaks and to the low intensity of some of the peaks.
   However, isotopic fractionation cannot be ruled out, especially in
   compounds resulting from C-C bond cleavage in the propyl side chain. The
