@article{ ISI:000238832900033,
Author = {Sargent, B. and Forrest, W. J. and D'Alessio, P. and Li, A. and Najita,
   J. and Watson, D. M. and Calvet, N. and Furlan, E. and Green, J. D. and
   Kim, K. H. and Sloan, G. C. and Chen, C. H. and Hartmann, L. and Houck,
   J. R.},
Title = {{Dust processing in disks around T Tauri stars}},
Journal = {{ASTROPHYSICAL JOURNAL}},
