@article{ ISI:000238384200069,
Author = {Procopio, L. M. and Villarreal, C. and Mochan, W. L.},
Title = {{Casimir-like tunnelling-induced electronic forces}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{2006}},
Volume = {{39}},
Number = {{21}},
Pages = {{6679-6686}},
Month = {{MAY 26}},
Note = {{7th Workshop on Quantum Field Theory under the Influence of External
   Conditions (QFEXT05), Barcelona, SPAIN, SEP 05-09, 2005}},
Abstract = {{We study the quantum forces that act between two nearby conductors due
   to electronic tunnelling. We derive an expression for these forces by
   calculating the flux of momentum arising from the overlap of evanescent
   electronic fields. Our result is written in terms of the electronic
   reflection amplitudes of the conductors and it has the same structure as
   Lifshitz's formula for the electromagnetically mediated Casimir forces.
   We evaluate the tunnelling force between two semiinfinite conductors and
   between two thin films separated by an insulating gap. We discuss some
   applications of our results.}},
