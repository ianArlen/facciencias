@article{ ISI:000238372600004,
Author = {Vivancos, Ignacio B. and Minzoni, Antonmaria A.},
Title = {{Chaotic behaviour in a singularly perturbed system}},
Journal = {{NONLINEARITY}},
Year = {{2006}},
Volume = {{19}},
Number = {{7}},
Pages = {{1535-1551}},
Month = {{JUL}},
Abstract = {{The aim of this paper is to present a novel type of chaos observed in a
   four-dimensional singularly perturbed system. We derive a geometric
   model where we can show the existence of chaotic orbits. The proof for
   the existence of chaos relies upon the construction of a Poincare map
   possessing a positive Lyapunov exponent. This canonical problem was
   identified as a coupled problem of two Hopf bifurcations with the fast
   jumps process.}},
