@article{ ISI:000241312300007,
Author = {Ramos-Arroyo, Y. R. and Hansen, A. M. and Siebe-Grabach, C.},
Title = {{Simulation of geochemical processes in mine tailings}},
Journal = {{INGENIERIA HIDRAULICA EN MEXICO}},
Year = {{2006}},
Volume = {{21}},
Number = {{3}},
Pages = {{87-100}},
Month = {{JUL-SEP}},
Abstract = {{Knowledge of behavior of acid mine waste drainage may be attained by
   modeling of geochemical processes, which control the solubility of
   potentially hazardous elements (PHE): As, Cu, Pb and Zn in mine
   tailings. The chemical equilibrium program PHREEQC2 was applied, using
   as input file the mineralogy of an epithermal ore from the Guanajuato
   mining district. These simulations describe the acidification potential
   and liberation of PHE when sulphides are oxidized. Sequential additions
   of calcite were simulated, and pH influences was evaluated in processes
   that control PHE solubility (precipitation of secondary phases and/or
   adsorption on ferrihydrite surfaces). The results indicate that the
   arsenic solubility is controlled by adsorption on ferrihydrite at acid
   pH and that this PHE desorbs at neutral and alkaline pH. Copper mobility
   is controlled by adsorption up to pH 3.7, by precipitation of
   brochantite {[}Cu-4 (OH)(6)SO4] between pH 3.7 and 7 and by
   precipitation of malachite {[}Cu-2 (OH)(2)CO3] at higher pH. Lead has a
   low solubility, at acid pH it precipitates as anglesite {[}PbSO4], at pH
   above 6.5 it adsorbs on ferrihydrite, whereas at pH above 7, this metal
   precipitates as Pb(OH)(2). Zn remains dissolved at pH below 6.5 due to
   its low affinity to ferrihydrite and it precipitates as Zn(OH)(2) at
   higher pH. The sulphate-reduction reaction causes precipitation of
   sulphide minerals such as pyrite, chalcopyrite, galena, and sphalerite.
   Acid mine drainage can be controlled adding calcite and methane to
   adjust the pH at an adequate level to assure adsorption and
   precipitation of PHE.}},
Publisher = {{INST MEXICANO TECHNOLOGIAAGUA}},
Address = {{APARTADO POSTAL 202, MORELOS 62550 CIVAC, MEXICO}},
Type = {{Article}},
