@article{ ISI:000238694900030,
Author = {Garcia-Calderon, Gaston and Villavicencio, Jorge},
Title = {{Full-time nonexponential decay in double-barrier quantum structures}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2006}},
Volume = {{73}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{We examine an analytical expression for the survival probability for the
   time evolution of quantum decay to discuss a regime where quantum decay
   is nonexponential at all times. We find that the interference between
   the exponential and nonexponential terms of the survival amplitude
   modifies the usual exponential decay regime in systems where the ratio
   of the resonance energy to the decay width is less than 0.3. We suggest
   that such a regime could be observed in semiconductor double-barrier
   resonant quantum structures with appropriate parameters.}},
