@article{ ISI:000240280500033,
Author = {Juarez, M. Rodriguez and Aguirre, N. Munoz and Perez, L. Martinez and
   Garibay-Febles, V. and Lozada-Cassou, M. and Becerril, M. and Angel, O.
   Zelaya},
Title = {{Optical characterization of polyethylene and cobalt phthalocyanine
   ultrathin films by means of the ATR technique at surface plasmon
   resonance}},
Journal = {{PHYSICA STATUS SOLIDI A-APPLICATIONS AND MATERIALS SCIENCE}},
Year = {{2006}},
Volume = {{203}},
Number = {{10}},
Pages = {{2506-2512}},
Month = {{AUG}},
Abstract = {{It is well known that the development and determination of optical
   properties of ultrathin films is an important issue in many
   technological areas. In this work organic polyethylene (PE) and cobalt
   phthalocyanines (CoPc) ultrathin films were deposited over metal films
   using the r.f sputtering and thermal evaporation techniques,
   respectively. Attenuated total reflection (ATR) measurements for the
   system organic film/metal at the surface plasmon resonance (SPR) were
   used for determining the thicknesses and optical properties of the PE
   and CoPc thin films. Thicknesses of the order of some nanometers were
   found, fitting the theoretical multilayer ATR model, for p polarization
   monochromatic light, to the experimental reflection data. The dielectric
   function of CoPc ultrathin films was determined at a wavelength of 632.8
   nm. (c) 2006 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim.}},
