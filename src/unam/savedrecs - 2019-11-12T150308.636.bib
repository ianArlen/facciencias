@article{ ISI:000238549700004,
Author = {Bruner, Carlos A. and Acuna, Laura},
Title = {{The influence of the enemies of science on preparatory students}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2006}},
Volume = {{23}},
Number = {{1}},
Pages = {{27-35}},
Month = {{JUN}},
Note = {{12th Mexican Congress of Psychology, Guanajuato, MEXICO, SEP, 2004}},
Abstract = {{Preparatory students (N = 588; from the last three years of the six-year
   high-school system) from six different schools of the National
   University of Mexico (UNAM) expressed their agreement with 30
   statements, either in favor or against science. The statements were
   based on the main tenets of postmodernism, of constructivism, of radical
   feminism and other minority groups and of radical environmentalism.
   Globally, preparatory students held an indifferent view of science. No
   differences were found by gender, by social class or by years of study.
   However, those students with a stated preference for mathematics,
   physics, chemistry and biology held a more positive view of science than
   students with a stated preference for drawing, psychology, languages,
   informatics, history, law, philosophy, and literature. Students that
   preferred geography were not statistically different from those that
   preferred either group of subjects. In comparison with the results from
   our previous studies, preparatory students held a worse view of science
   than undergraduate students of UNAM. It was concluded that although
   teachers with a hostile view of science are believed to be a typical
   university phenomenon, they are also present at the preparatory level
   and as in the university, they belong to the areas of humanities and
   social sciences.}},
