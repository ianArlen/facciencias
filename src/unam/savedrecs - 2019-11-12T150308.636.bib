@article{ ISI:000238373800007,
Author = {Campa-Molina, J. and Ulloa-Godinez, S. and Barrera, A. and Bucio, L. and
   Mata, J.},
Title = {{Nano and micro reoriented domains and their relation with the crystal
   structure in the new ferroelectric boracite Zn3B7O13Br}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2006}},
Volume = {{18}},
Number = {{20}},
Pages = {{4827-4837}},
Month = {{MAY 24}},
Abstract = {{A new zinc brome boracite Zn3B7O13Br has been grown by a chemical
   transport reaction in closed quartz ampoules at 920 K. The crystal
   structure was characterized by Rietveld refinement. Ferroelectric nano
   and micro reorientable domains were found in this material using
   polarizing optical microscopy (PLM), scanning electron microscopy (SEM)
   and transmission electron microscopy (TEM). Chemical analysis was
   performed with x-ray energy dispersive spectroscopy (EDX). In the
   crystal, a new structure transition at 586 K from orthorhombic (Pca2(1))
   to cubic cell (F (4) over bar 3c) has been found. This transition was
   corroborated by differential scanning calorimetry (DSC).}},
