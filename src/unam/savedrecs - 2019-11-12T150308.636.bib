@article{ ISI:000238507700013,
Author = {Matos, Tonatiuh and Nunez, Dario},
Title = {{Rotating scalar field wormhole}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{2006}},
Volume = {{23}},
Number = {{13}},
Pages = {{4485-4495}},
Month = {{JUL 7}},
Abstract = {{We derive an exact solution of Einstein's equations with a scalar field
   stress energy tensor with the opposite sign, and show that such a
   solution describes the inner region of a rotating wormhole. We also show
   that the non-rotating case of such a solution represents a static,
   asymptotically flat wormhole solution. We match the radial part of the
   rotating solution to the static one at both mouths, thus obtaining an
   analytic description for the asymptotic radial region of spacetime. We
   explore some of the features of these solutions.}},
