@article{ ISI:000243430400012,
Author = {Garcia-Guerrero, Marcelo and Hendrickx, Michel E.},
Title = {{Embryology of decapod crustaceans III : Embryonic development of
   Eurypanopeus canalensis Abele \& Kim, 1989, and Panopeus chilensis H.
   Milne Edwards \& Lucas, 1844 (Decapoda, Brachyura, Panopeidae)}},
Journal = {{BELGIAN JOURNAL OF ZOOLOGY}},
Year = {{2006}},
Volume = {{136}},
Number = {{2}},
Pages = {{249-253}},
Month = {{JUL}},
Abstract = {{Embryonic development in two coastal lagoon crabs, Eurypanopeus
   canalensis and Panopeus chilensis. is described. Embryos were sampled
   and illustrated at intervals of 48 hours. The complete embryonic
   development at 26-28 degrees C was similar (13 +/- 1 day) for both
   species. Growth was synchronous and appendages were formed during the
   same periods for the two species. Differences were observed in eye size
   and cephal othorax-abdomen proportions. The egg-volume increased
   significantly, three differences statistically in volume were detected
   for E. canalensis and two for P. chilensis, especially at the two last
   stages of development.}},
