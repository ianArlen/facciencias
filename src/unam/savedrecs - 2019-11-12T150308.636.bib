@article{ ISI:000239612300006,
Author = {Jimenez, B. and Martinez, M. and Vaca, M.},
Title = {{Alum recovery and sludge stabilization with sulfuric acid in an APT
   wastewater treatment plant}},
Journal = {{JOURNAL OF RESIDUALS SCIENCE \& TECHNOLOGY}},
Year = {{2006}},
Volume = {{3}},
Number = {{3}},
Pages = {{169-176}},
Month = {{JUL}},
Abstract = {{Coagulation-flocculation can be used to remove helminth ova and
   phosphorus from wastewater intended for agricultural reuse.
   Nevertheless, it has the drawback of producing a large amount of sludge
   that together with the chemicals needed for the process increases the
   operating cost. By recovering and recycling aluminum with an acid
   process the operating cost can be reduced by: (a) diminishing alum
   demand and (b) producing a lower amount of partially treated sludge.
   Adding sulfuric acid to lower pH to 2 for 30 min, at a TS under 6\% and
   300 rpm, 70\% of the aluminum added was recovered reducing the sludge
   volume by 45\% and mass volume by 63\%, and inactivating 5 logs of fecal
   coliforms and 68\% of helminth ova. Due to the lower alum consumption,
   the cost is reduced by 3.78 US\$/1000 m(3), and a 60\% additional
   savings may be obtained due to the amount of sludge to be treated.}},
