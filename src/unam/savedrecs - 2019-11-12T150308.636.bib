@article{ ISI:000239987000017,
Author = {Flores, J. F. and Olaya, J. J. and Colas, R. and Rodil, S. E. and
   Valdez, B. S. and Fuente, I. G.},
Title = {{Corrosion behaviour of TaN thin PVD films on steels}},
