@article{ ISI:000238549700012,
Author = {Zuniga Bohigas, Alejandra and Bouzas Riano, Arturo},
Title = {{Alcohol consumption among adolescents: Perception of positive and
   negative outcomes}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2006}},
Volume = {{23}},
Number = {{1}},
Pages = {{97-110}},
Month = {{JUN}},
Abstract = {{The purpose of this research was to analyze the correlation between the
   estimation of the probability that alcohol would produce particular
   negative or positive outcomes and the following variables: experience
   with drinking alcohol, gender, age, socioeconomic status, and
   religiosity. A group of 264 Mexican adolescents estimated the
   probability of five positive consequences and six negative outcomes
   related to alcohol consumption. Results indicated that although all
   adolescents expected to experience some negative outcomes, those who
   have experience with alcohol are more likely than those without
   experience to minimize the negative outcomes and to expect more positive
   benefits of alcohol consumption.}},
