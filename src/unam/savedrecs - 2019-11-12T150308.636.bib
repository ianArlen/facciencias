@article{ ISI:000237783400049,
Author = {Neustroev, V. V. and Zharikov, S. and Michel, R.},
Title = {{Simultaneous photometry and echelle spectroscopy of the dwarf nova BZ
   Ursae Majoris during the 2005 January outburst}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2006}},
Volume = {{369}},
Number = {{1}},
Pages = {{369-375}},
Month = {{JUN 11}},
Abstract = {{We report simultaneous photometric and echelle-spectroscopic
   observations of the dwarf nova BZ UMa during which we were lucky to
   catch the system at the onset of an outburst, the development of which
   we traced in detail from quiescence to early decline. The outburst had a
   precursor, and was of a short duration (similar to 5 d) with a highly
   asymmetrical light curve. On the rise we observed a `jump' during which
   the brightness almost doubled over the course of half an hour. Power
   spectra analysis revealed well-defined oscillations with period of
   similar to 42 min. Using Doppler tomography we found that the unusual
   emission distribution detected in quiescence held during the outburst.
   After the maximum a new emission source arose, from the inner hemisphere
   of the secondary star, which became the brightest at that time. We
