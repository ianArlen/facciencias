Author = {Solis, C. and Sandoval, J. and Perez-Vega, H. and Mazari-Hiriart, M.},
Title = {{Irrigation water quality in southern Mexico City based on bacterial and
   heavy metal analyses}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2006}},
Volume = {{249}},
