@article{ ISI:000238693700008,
Author = {Bruce, NC},
Title = {{Randomly rough rectangular-groove surfaces with predetermined
   backscatter intensities}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2006}},
Volume = {{52}},
Number = {{3}},
Pages = {{246-254}},
Month = {{JUN}},
Abstract = {{The backscattered intensity of the scattered light in
   rectangular-grooved surfaces is used as a design parameter for randomly
   rough surfaces. A modified Kirchhoff method and the integral equation
   method are used to calculate the scattered light dissurfaces. The
   results of the two calculation methods are found to show good agreement,
   with the Kirchhoff method slightly overestimating the double scattered
   intensity, perhaps due to the limitations of the geometrical shadow
   functions used in this method. These results show that the backscattered
   intensity can be controlled in this type of surface.}},
