@article{ ISI:000279282200007,
Author = {Zumeta, Inti and Diaz, David and Santiago, Patricia},
Title = {{Synthesis of TiO2 Nanoparticles with Narrow Size Distribution and Their
   Evaluation in the Photocatalytic Oxidative Degradation of
   Bis(4-nitrophenyl) Phosphate}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2010}},
Volume = {{114}},
Number = {{26}},
Pages = {{11381-11389}},
Month = {{JUL 8}},
Abstract = {{A new synthesis method of anatase titanium dioxide (TiO2) nanoparticles
   (NPs) of an average size of 2.1 +/- 0.3 nm is reported. This is a fast,
   inexpensive, one-pot, and one-step method. It takes place under normal
   reaction conditions in dimethyl sulfoxide (DMSO) colloidal dispersions.
   The electronic absorption spectrum of these TiO2 colloidal particles
   shows an absorption band edge blue shift with respect to the
   macrocrystal due to the quantum confinement effect. After TiO2 NPs
   precipitation, the isolated powder changes its crystalline phase with
   the annealing process, up to 500 degrees C anatase is the unique phase,
   and at 1000 degrees C the precipitate is totally rutile. UV irradiated
   aqueous TiO2 NPs suspensions play an active role in the degradation of
   the bis-4-nitrophenyl phosphate (BNPP), as well as of its primary
   decomposition products, the 4-nitrophenyl phosphate (4-NPP) and the
   4-nitrophenol (4-NP). As far as we now, the complete BNPP and 4-NPP
   photocatalytic or just catalytic degradation, using inorganic
   nanoclusters, has not been reported. Commercial TiO2 powder (Degussa
   P25) exhibits the highest photocatalytic activity in the BNPP
   degradation process; however, the activity of the synthesized TiO2 NPs
   is of the same order of magnitude as that of Degussa's P25.}},
