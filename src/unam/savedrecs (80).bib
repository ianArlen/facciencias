@article{ ISI:000285403000006,
Author = {Fuentes-Martinez, Yanet and Godoy-Alcantar, Carolina and Medrano, Felipe
   and Dikiy, Alexander and Yatsimirsky, Anatoly K.},
Title = {{Protonation of kanamycin A: Detailing of thermodynamics and protonation
   sites assignment}},
