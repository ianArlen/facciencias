@article{ ISI:000280461000008,
Author = {Macias, Ramon Rodriguez and Alcantar Gonzalez, Ernesto G. and
   Covarrubias, Gilberto Iniguez and Natera, Francisco Zamora and Garcia
   Lopez, Pedro M. and Ruiz Lopez, Mario A. and Perez, Eduardo Salcedo},
Title = {{PHYSICAL AND CHEMICAL CHARACTERIZATION OF SUBSTRATES OBTAINED FROM
   TEQUILA AGAVE BAGASSE}},
Journal = {{INTERCIENCIA}},
Year = {{2010}},
Volume = {{35}},
Number = {{7}},
Pages = {{515-520}},
Month = {{JUL}},
Abstract = {{Tequila agave bagasse was characterized and evaluated in order to know
   its potential as an alternate substrate comparable to commercial peat. A
   mixture of bagasse and lamb manure 4:1 v/v, was subjected to two
   biotransformation processes: vermicomposting for 130 days to obtain
   vermicompost (by), and conventional composting in order to obtain two
   different materials: a mature compost (bc) of 130 days, and an immature
   precompost (bp) of 60 days. Water-washed bagasse (bf) was also utilized.
   Two commercial peats imported from Canada, Sunshine 3 (s-s) and VTM
   Sogemix (vim), were employed as control. Physical properties of the
   materials such as apparent density, real density, and humidity and
   retention were determined. Chemical properties of cationic exchange
   capacity, pH, electrical conductivity (CE), phenolic acids
   concentration, and nutritional contents were also determined. The
   results show that the physical and chemical properties of the bp, by and
   bc substrates provide appropriate growing conditions, a similar way to
   the commercial peats s-s and vtm. It is concluded that bagasse agave
   offers a significant potential as a agricultural substrate.}},
