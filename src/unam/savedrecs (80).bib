@article{ ISI:000285800000002,
Author = {Jasso-Gutierrez, Luis and Ignacio Santos-Preciado, Jose},
Title = {{Comparative profile of medications at the ``Hospital Infantil de
   Mexico{''} and the ``Instituto Mexicano del Seguro Social{''}}},
Journal = {{GACETA MEDICA DE MEXICO}},
Year = {{2010}},
Volume = {{146}},
Number = {{5}},
Pages = {{304-310}},
Month = {{SEP-OCT}},
Abstract = {{Background: Compare the distribution of medications according to
   therapeutic groups (TGs) as a function of codes, cost, price weighted
   index (PWI) and volume between the Instituto Mexicano del Seguro Social,
   a conglomerate of hospitals and family medicine clinics and the Hospital
   Infanta de Mexico ``Federico Gomez{''} a tertiary care referral
   pediatric hospital.
   Methods: Medications were classified by TGs according to drug code,
   purchase cost, PIN, and volumes purchased.
   Results: TGs showed similar profiles according to cost of purchase,
   volume and code numbers, not according to PWI There were only minor
   differences in cost among the 15 most expensive medications among both
   institutions.
   Conclusions: Despite the marked asymmetry between both institutions,
   drug profiles were not significantly different, to a large extent due to
   the fact that the Hospital Infantil de Mexico acquires all TGs.}},
