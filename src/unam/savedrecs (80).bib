@article{ ISI:000279491900004,
Author = {Barrera, Ruben G. and Luis Mochan, W. and Garcia-Valenzuela, Augusto and
   Gutierrez-Reyes, Edahi},
Title = {{On the definition of the Poynting vector: A non-local derivation}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
Year = {{2010}},
Volume = {{405}},
Number = {{14}},
Pages = {{2920-2924}},
