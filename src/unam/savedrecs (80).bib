@article{ ISI:000284126000006,
Author = {Roman-Contreras, Ramiro and Martinez-Mayen, Mario},
Title = {{NOTES ON MARINE SHALLOW-WATER CARIDEANS (PASIPHAEIDAE, GNATHOPHYLLIDAE,
   AND PROCESSIDAE) FROM QUINTANA ROO, MEXICO}},
Journal = {{CRUSTACEANA}},
Year = {{2010}},
Volume = {{83}},
Number = {{8}},
Pages = {{957-970}},
Month = {{AUG}},
Abstract = {{A checklist of caridean shrimps of the families Pasiphaeidae,
   Gnathophyllidae, and Processidae, collected in shallow waters on turtle
   grass Thalassia testudinum Banks ex Konig in Bahia de In Ascension,
   Bahia del Espiritu Santo, and the Mahahual reef lagoon, Quintana Roo,
   Mexico, is presented. The list contains seven species belonging to four
   genera. The family Processidae was the most diverse with four species,
   followed by the Pasiphaeidae and Gnathophyllidae with two and one
   species, respectively. Updated data on their known geographic range are
   included for all species, and their distribution in the marine
   zoogeographic provinces of the Americas is discussed. New regional
   records are given, and the occurrence in the Mexican Caribbean of
   Leptochela (Leptochela) bermudensis Gurney, 1939, is cited for the first
   time.}},
