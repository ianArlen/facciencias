@article{ ISI:000282899600019,
Author = {Gouygou, Maryse and Daran, Jean-Claude and Robe, Emmanuel and Ortega,
   Carmen},
Title = {{Chiral diphosphines and diphosphinites derived from 2,2-biphosphole:
   Stereodynamic ligands for asymmetric catalysis}},
Journal = {{COMPTES RENDUS CHIMIE}},
Year = {{2010}},
Volume = {{13}},
Number = {{8-9}},
Pages = {{1054-1062}},
Month = {{AUG-SEP}},
Abstract = {{In this this account, the recent advances on chiral stereochemically
   dynamic 2,2'-biphosphole ligands for applications in asymmetric
   catalysis are reported. In the first part, the synthesis of
   stereodynamic diphosphines and diphosphinites derived from
   2,2'-biphosphole is presented. The second part describes the kinetic
   dynamic resolution to give diastereo- and enantiopure complexes.
   Applications in asymmetric allylic substitution, hydroformylation and
