@article{ ISI:000279787300006,
Author = {Albarran, Guadalupe and Boggess, William and Rassolov, Vital and
   Schuler, Robert H.},
Title = {{Absorption Spectrum, Mass Spectrometric Properties, and Electronic
   Structure of 1,2-Benzoquinone}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY A}},
