@article{ ISI:000208342300056,
Author = {Aguilar Meza, Maria del Carmen and Moncada Maya, Jose Omar},
Title = {{THE LANDSCAPE OF THE CITY OF MEXICO. CURRENT CHALLENGE}},
Journal = {{SCRIPTA NOVA-REVISTA ELECTRONICA DE GEOGRAFIA Y CIENCIAS SOCIALES}},
Year = {{2010}},
Volume = {{14}},
Number = {{331, SI}},
Month = {{AUG 1}},
Abstract = {{The green areas make public spaces whose main element is the vegetation.
   The progress of urbanization in the City of Mexico has impacted their
   environmental conditions, making it essential to protect, conserve and
   enhance urban green areas because they perform essential functions for
   the quality of life for residents. In this paper highlights the
   importance of proper management of vegetation, which is attributed to a
