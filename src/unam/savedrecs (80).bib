@article{ ISI:000282999200008,
Author = {Shen, Lei and Jiang, Qi-Zhong and Gan, Tao and Shen, Min and Rodriguez
   Varela, F. J. and Ocampo, A. L. and Ma, Zi-Feng},
Title = {{TiO2 Nanotubes Promoted PT-NI/C Catalyst with Low PT Content as Anode
   Catalyst for Direct Ethanol Fuel Cells}},
Journal = {{JOURNAL OF NEW MATERIALS FOR ELECTROCHEMICAL SYSTEMS}},
Year = {{2010}},
Volume = {{13}},
Number = {{3, SI}},
Pages = {{205-211}},
Month = {{JUL}},
Note = {{10th International Congress of the Mexican-Hydrogen-Society, Toluca,
   MEXICO, SEP 27-OCT 01, 2010}},
Organization = {{Mexican Hydrogen Soc}},
Abstract = {{TiO2 nanotubes (TiO2NTs) are added into a low-platinum content Pt-Ni/C
   catalyst to improve its catalytic activity for the ethanol oxidation
   reaction (EOR). The promotion effect of TiO2NTs on Pt-Ni/C catalyst is
   studied. The results obtained by cyclic voltmmetry (CV) and
   chronoamperometry indicate that TiO2NTs can greatly enhance the
   catalytic activity of the Pt-Ni/C catalyst. Compared with a commercial
   Pt-Ru/C catalyst, the Pt-Ni-TiO2NT/C catalyst has a larger
   electrochemical active surface (EAS) and shows lower onset potential for
   the EOR. The elemental composition and electronic structure of the
   catalyst are characterized by EDX, ICP-OES, XRD and XPS. Morphological
   properties of these catalysts are characterized by HRTEM It can be
   concluded that the presence of TiO2NTs can retain more of the Pt
   metallic species and provide more hydroxides groups, which results in a
   facile removal of reaction intermediates and lower onset oxidation
   potentials.}},
Publisher = {{ECOLE POLYTECHNIQUE MONTREAL}},
Address = {{C P 6079, SUCC CENTRE-VILLE, MONTREAL, PQ H3C 3A7, CANADA}},
Type = {{Article; Proceedings Paper}},
Language = {{English}},
