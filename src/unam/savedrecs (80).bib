@article{ ISI:000281437500027,
Author = {Luis Aguilar-Lopez, Jose and Pineda, Eduardo and Garcia-Vazquez, Uri},
Title = {{Geographic-attitudinal range extension of Duellmanohyla chamulae
   (Amphibia: Hylidae) and addition to amphibian fauna of Veracruz}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2010}},
Volume = {{81}},
Number = {{2}},
Pages = {{579-580}},
Month = {{AUG}},
Abstract = {{The first record of Duellmanohyla chamulae in Veracruz, Mexico is
   informed. Reported geographic range of this species was restricted to
   few locations on the northern slopes of the central highlands of
   Chiapas, Mexico. Site where this species was collected is located at a
   tropical rainforest within the Uxpanapa region, 170 km west to the known
   geographic range and 120 m lower than the reported altitudinal range.
   Our record suggests that the species is rare in the study area and its
   distribution is restricted to tropical rainforest.}},
