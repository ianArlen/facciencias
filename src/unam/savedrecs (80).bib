@article{ ISI:000280460600030,
Author = {Isabel Chavez, Maria and Vila-Perello, Miguel and Javier Canada,
   Francisco and Andreu, David and Jimenez-Barbero, Jesus},
Title = {{Effect of a serine-to-aspartate replacement on the recognition of chitin
   oligosaccharides by truncated hevein. A 3D view by using NMR}},
