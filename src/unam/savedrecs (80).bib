@article{ ISI:000280184700002,
Author = {Zaldivar-Riveron, Alejandro and Salvador De Jesus-Bonilla, Vladimir},
Title = {{Redescription of species of the Neotropical parasitoid Notiospathius
   Mathews et Marsh (Braconidae: Doryctinae) based on their nineteenth and
   early twentieth century types}},
Journal = {{ZOOTAXA}},
Year = {{2010}},
Number = {{2543}},
Pages = {{31-42}},
Month = {{JUL 21}},
Abstract = {{Seven species of the speciose Neotropical parasitoid wasp genus
   Notiospathius Mathews et Marsh are redescribed. These species were
   originally described during the nineteenth and early twentieth
   centuries. Examination of the holotpyes of N. meliorator and N. necator
   revealed that they actually belong to an undescribed doryctine genus
   that is similar to Ptesimogaster Marsh, 1965. A key is provided to
   distinguish these species.}},
