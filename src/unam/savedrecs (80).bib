@article{ ISI:000280914100014,
Author = {Benet, Luis and Merlo, Olivier},
Title = {{Structure in narrow planetary rings: Open questions and recent results}},
Journal = {{REVISTA MEXICANA DE CIENCIAS GEOLOGICAS}},
Year = {{2010}},
Volume = {{27}},
Number = {{2}},
Pages = {{347-357}},
Month = {{AUG}},
Abstract = {{In this paper we review some open questions in the context of the
   structure observed in narrow planetary rings, and summarize some recent
   results of our work directed to answer them. Using the scattering
   approach to narrow rings we have succeeded to reproduce some of their
   structural properties in a qualitative sense, using unrealistic toy
   models as examples. We obtain narrow rings which are non-circular and
   display sharp edges. In addition, these rings May have multiple
   components which may entangle in a complicated dynamically evolving way
   forming a braided structure, or may display strongly azimuthal dependent
   features such as arcs. The appearance of these structural properties can
   be understood in terms of the underlying phase space.}},
