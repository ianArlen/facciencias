@article{ ISI:000278576500020,
Author = {Manuel Garcia-Islas, J.},
Title = {{Entropy in spin foam models: the statistical calculation}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{2010}},
Volume = {{27}},
Number = {{14}},
Month = {{JUL 21}},
Abstract = {{Recently an idea for computing the entropy of black holes in the spin
   foam formalism has been introduced. Particularly complete calculations
   for the three-dimensional Euclidean BTZ black hole were performed. The
   whole calculation is based on observables living at the horizon of the
   black hole universe. Departing from this idea of observables living at
   the horizon, we now go further and compute the entropy of the BTZ black
   hole in the spirit of statistical mechanics. We compare both
   calculations and show that they are very interrelated and equally valid.
   This latter behaviour is certainly due to the importance of the
   observables.}},
