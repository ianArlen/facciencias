@article{ ISI:000282075500038,
Author = {Carriquiry, Jose D. and Horta-Puga, Guillermo},
Title = {{The Ba/Ca record of corals from the Southern Gulf of Mexico:
   Contributions from land-use changes, fluvial discharge and oil-drilling
   muds}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2010}},
Volume = {{60}},
Number = {{9}},
Pages = {{1625-1630}},
Month = {{SEP}},
Abstract = {{The Ba/Ca in the growth bands of Montastraea faveolata from the Veracruz
   Reef System was used to reconstruct the long-term environmental change
   associated to anthropogenic activity in the Southern Gulf of Mexico
   (SGM). The 168-yr coral record shows two periods of distinct Ba
   concentrations: a pre-industrial period (1835-1965: 7.54 mu mol/mol)
   followed by an industrial one (1966-2000: 8.57 mu mol/mol). As human
   population quadrupoled during the latter, sediment load in the fluvial
   discharge also increased due to changes in land-use, yielding a 14\%
   increase in the Ba-levels. A remarkable finding is that the periods at
   which the coral Ba/Ca ratio losses its correlation with fluvial
   discharge coincide exactly with peak periods of high barite consumption
   (used for oil drilling) in the Northern Gulf of Mexico, and the onset of
   oil drilling in the SGM. This finding suggests that barite may be one of
   the dominant sources for dissolved-Ba in the SGM. (C) 2010 Elsevier Ltd.
   All rights reserved.}},
