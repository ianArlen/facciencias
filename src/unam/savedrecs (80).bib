@article{ ISI:000281268200009,
Author = {Jaidar, Omar and Carrillo-Reid, Luis and Hernandez, Adan and
   Drucker-Colin, Rene and Bargas, Jose and Hernandez-Cruz, Arturo},
Title = {{Dynamics of the Parkinsonian Striatal Microcircuit: Entrainment into a
   Dominant Network State}},
