@article{ ISI:000280531500007,
Author = {Pedemera-Romano, C. and Aurioles-Gamboa, D. and Valdez, R. A. and
   Brousset, D. M. and Romano, M. C. and Galindo, F.},
Title = {{Serum cortisol in California sea lion pups (Zalophus californianus
   californianus)}},
