@article{ ISI:000281214900009,
Author = {Mello, Pier A. and Rodriguez, Rosalio F.},
Title = {{The equipartition theorem revisited}},
Journal = {{AMERICAN JOURNAL OF PHYSICS}},
Year = {{2010}},
Volume = {{78}},
Number = {{8}},
Pages = {{820-827}},
Month = {{AUG}},
Abstract = {{We generalize the equipartition theorem to confined systems of
   structureless independent molecules by introducing a confining
   potential, which is usually not done explicitly in most textbooks. We
   show that if the size of the system is much larger than the range of the
   confining potential, the generalization is independent of the form of
   the potential but contains corrections produced by the confinement. We
   demonstrate the important role played by the walls and show how the
   boundary terms may be employed to describe their contribution to the
   pressure exerted on the system. (C) 2010 American Association of Physics
   Teachers. {[}DOI: 10.1119/1.3386255]}},
