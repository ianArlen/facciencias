@article{ ISI:000281011100008,
Author = {Gersenowies, Jorge and Marosi, Erzsebet and Cansino, Selene and
   Rodriguez, Mario},
Title = {{EEG Power Spectral Measurements Comparing Normal and ``Thatcherized{''}
   Faces}},
Journal = {{INTERNATIONAL JOURNAL OF NEUROSCIENCE}},
Year = {{2010}},
Volume = {{120}},
Number = {{8}},
Pages = {{570-579}},
Month = {{AUG}},
Abstract = {{In this paper we have made a broadband analysis to detect the
   electroencephalogram (EEG) frequencies that change selectively during
   the presentation of normal and ``thatcherized{''} faces. Referential
   recordings to linked ears were obtained in 21 leads in 48 right-handed
   healthy male volunteers. Increase of delta power (1.75-3.91 Hz) was
   observed, related to the detection of distortion in faces at bifrontal
   and left temporoparietal cortex. The other bands had no contribution,
   when normal and modified faces were compared. These results support our
   hypothesis that a change in EEG power spectral may be related to
   discrimination between normal and thatcherized faces.}},
