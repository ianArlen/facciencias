@article{ ISI:000288472600004,
Author = {Solorzano-Lopez, J. and Ramirez-Argaez, M. A. and Zenit, R.},
Title = {{Physical modeling of the impingement of an air jet on a water surface}},
Journal = {{REVISTA DE METALURGIA}},
