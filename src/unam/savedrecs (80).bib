@article{ ISI:000279976200016,
Author = {MacArthur, Lauren A. and McDonald, Michael and Courteau, Stephane and
   Jesus Gonzalez, J.},
Title = {{INTEGRATED STELLAR POPULATIONS: CONFRONTING PHOTOMETRY WITH SPECTROSCOPY}},
Journal = {{ASTROPHYSICAL JOURNAL}},
