@article{ ISI:000277943200008,
Author = {Lopez-Vancell, R. and Arreguin Espinosa, R. and Gonzalez-Canto, A. and
   Nequiz Avendano, M. and Garcia de Leon, M. C. and Olivos-Garcia, A. and
   Lopez-Vancell, D. and Perez-Tamayo, R.},
Title = {{Entamoeba histolytica: Expression and localization of Gal/GalNAc lectin
   in virulent and non-virulent variants from HM1:IMSS strain}},
Journal = {{EXPERIMENTAL PARASITOLOGY}},
Year = {{2010}},
Volume = {{125}},
