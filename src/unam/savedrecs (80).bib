@article{ ISI:000282588500001,
Author = {Sanchez-Rangel, Diana and Plasencia, Javier},
Title = {{The role of sphinganine analog mycotoxins on the virulence of plant
   pathogenic fungi}},
Journal = {{TOXIN REVIEWS}},
Year = {{2010}},
Volume = {{29}},
Number = {{3-4}},
Pages = {{73-86}},
Month = {{AUG}},
Abstract = {{Functional genomics and recent analytical developments have contributed
   to partially resolving the complexity and the important roles of
   sphingolipids in plant cell physiology. In the light of these
   discoveries, it is worthwhile to reassess the function of the
   sphinganine analog mycotoxins, which are produced by plant pathogenic
   fungi. The mode of action of sphinganine analog mycotoxins, which
   disrupt cellular sphingolipid metabolism, contributes to unravelling the
   role of sphingolipid intermediaries in the defense reaction mediated by
   the phytohormones salicylic acid, jasmonic acid, and ethylene. Thus
   toxin production appears to be an effective pathogen strategy to
   modulate the host cell defense response.}},
