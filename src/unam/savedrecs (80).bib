@article{ ISI:000280909900003,
Author = {Solis, D. and De la Rosa, E. and Meza, O. and Diaz-Torres, L. A. and
   Salas, P. and Angeles-Chavez, C.},
Title = {{Role of Yb3+ and Er3+ concentration on the tunability of
   green-yellow-red upconversion emission of codoped ZrO2:Yb3+-Er3+
   nanocrystals}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2010}},
Volume = {{108}},
Number = {{2}},
Month = {{JUL 15}},
Abstract = {{Strong green and red visible emissions were obtained from ZrO2:Yb3+-Er3+
   nanocrystals synthesized by sol-gel method and annealed at 1000 degrees
   C for 5 h. The average crystallite size was similar to 70 nm with
   tetragonal phase for total concentration lower than 3 mol \% and cubic
   phase for concentration higher than 5 mol \%. The color coordinate of
   the upconverted signal was tailored by controlling the dopant
   composition that change the red/green ratio dominated by the cross
   relaxation and energy back transfer process as was demonstrated
   theoretically and confirmed experimentally. Both coefficients were
   calculated, C-51 similar to 1.02 X 10(-16) and C-5b similar to 6.04 X
   10(-17), from the theoretical model based on the rate equations. The
   highest energy transfer efficiency was eta similar to 64\% for 2 mol \%
   of Yb and 2 mol \% of Er3+. However, for the highest upconverted signal
   was only eta similar to 29\% obtained for 2 mol \% Yb and 1 mol \% Er
   with effective decay time tau(eff)similar to 438 mu s for red and
   tau(eff) similar to 290 mu s for green band. (C) 2010 American Institute
   of Physics. {[}doi:10.1063/1.3465325]}},
