@article{ ISI:000427179700011,
Author = {Noguera, Felipe A. and Gutierrez, Nayeli and Monne, Miguel A.},
Title = {{A new species, and a new distribution record for the genus Sympleurotis
   Bates, 1881 (Coleoptera, Cerambycidae, Lamiinae) from Mexico}},
Journal = {{ZOOTAXA}},
Year = {{2018}},
Volume = {{4394}},
Number = {{1}},
Pages = {{144-146}},
Month = {{MAR 13}},
Abstract = {{A new species of the genus Sympleurotis Bates from Jalisco, Mexico is
   described and illustrated. Details of a new state record of Sympleurotis
   rudis Bates, 1881 from Chiapas, Mexico is also included.
   In the course of an inventory of the Cerambycidae family in the
   cloud-pine forest, of the Ecotouristic Center of Amixtlan, San Jose del
   Carmen, Jalisco, Mexico, a new species of the genus Sympleurotis Bates
   was discovered and is described below.
   The genus was recently revised by Julio \& Monne (2005), who registered
   four species, S. albofasciatus Julio \& Monne, 2005 (Mexico), S. armatus
   Gahan, 1892 (Mexico, Guatemala, Costa Rica and Panama), S. rudis Bates,
   1881 (Mexico, Guatemala, Honduras and Panama) and S. wappesi Julio \&
   Monne, 2005 (Guatemala). Julio \& Monne (2005) characterized
   Sympleurotis as having the prothorax unarmed on the sides, by the
   presence of a center-basal tubercle on the elytra and females with
   abdominal segment V projected to form a long ovipositor.}},
