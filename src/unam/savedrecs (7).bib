@article{ ISI:000434080000004,
Author = {Vidal-Amaro, Juan J. and Sheinbaum-Pardo, Claudia},
Title = {{A Transition Strategy from Fossil Fuels to Renewable Energy Sources in
   the Mexican Electricity System}},
