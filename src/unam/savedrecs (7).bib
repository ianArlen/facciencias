@article{ ISI:000426550800034,
Author = {Muriel, Jaime and Graves, Jeff A. and Gil, Diego and Magallanes, S. and
   Salaberria, Concepcion and Casal-Lopez, Miriam and Marzal, Alfonso},
Title = {{Molecular characterization of avian malaria in the spotless starling
   (Sturnus unicolor)}},
