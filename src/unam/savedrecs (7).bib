@article{ ISI:000426368700002,
Author = {Castillo-Robles, Jose M. and Orgaz, Emilio},
Title = {{Structural and optical properties of Ni atoms and Ni-55 cluster adsorbed
   on a rutile TiO2 (110) surface}},
Journal = {{THEORETICAL CHEMISTRY ACCOUNTS}},
Year = {{2018}},
Volume = {{137}},
Number = {{3}},
Month = {{FEB 20}},
Abstract = {{Adsorbed Ni in a clean rutile TiO2 (110) surface has been investigated
   by computing the electronic band structure and the optical linear
   response properties within the density functional theory. We have
   determined the nucleation preferred sites by investigating the
   equilibrium geometries and their corresponding binding energies. The
   electronic properties of a closed-shell Mackay icosahedral Ni-55 cluster
   on TiO2 show a strong trend to keep the free cluster shape with a small
   but significant charge transfer to the TiO2 surface. The optical
   properties investigation of the Ni-55 cluster on rutile (110) indicates
   the development of an absorption band in the visible region.}},
