@article{ ISI:000424125400005,
Author = {Garcia-Azpeitia, Carlos},
Title = {{Standing waves in a counter-rotating vortex filament pair}},
Journal = {{JOURNAL OF DIFFERENTIAL EQUATIONS}},
Year = {{2018}},
Volume = {{264}},
Number = {{6}},
Pages = {{3918-3932}},
Month = {{MAR 15}},
Abstract = {{The distance among two counter-rotating vortex filaments satisfies a
   beam-type of equation according to the model derived in{[}15]. This
   equation has an explicit solution where two straight filaments travel
   with constant speed at a constant distance. The boundary condition of
   the filaments is 2 pi-periodic. Using the distance of the filaments as
   bifurcating parameter, an infinite number of branches of periodic
   standing waves bifurcate from this initial configuration with constant
   rational frequency along each branch. (C) 2017 Elsevier Inc. All rights
   reserved.}},
