@article{ ISI:000470702700008,
Author = {de Jesus Almonte, Leobardo and Morales Fajardo, Maria Esther and
   Carbajal Suarez, Yolanda},
Title = {{Direct Foreign Investment and manufacture employment. A regional
   analysis with panel data for Mexico, 2007-2014}},
Journal = {{PAPELES DE POBLACION}},
Year = {{2018}},
Volume = {{24}},
Number = {{96}},
Pages = {{187-216}},
Month = {{APR-JUN}},
Abstract = {{In this paper we analyze the effects of foreign direct investment (FDI)
   on the generation of manufacturing employment in the regions of Mexico.
   From the estimation of an employment function with panel data for each
   region of the country and for the period 2007-2014, it is found that
   there is a very important relationship between GDP growth and employment
   growth for the total of the manufacturing sector and for the different
   regions of Mexico. Nevertheless, there was no consistent evidence to
   confirm that the accumulation of FDI in the regions is a factor that
   influences the generation of manufacturing employment.}},
