@article{ ISI:000429079600023,
Author = {Mata-Martinez, Esperanza and Darszon, Alberto and Trevino, Claudia L.},
Title = {{pH-dependent Ca+2 oscillations prevent untimely acrosome reaction in
   human sperm}},
