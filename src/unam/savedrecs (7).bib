@article{ ISI:000425201900010,
Author = {Serratos, Iris N. and Millan-Pacheco, Cesar and Garza-Ramos, Georgina
   and Perez-Hernandez, Gerardo and Zubillaga, Rafael A.},
Title = {{Exploring interfacial water trapping in protein-ligand complexes with
   multithermal titration calorimetry}},
Journal = {{BIOCHIMICA ET BIOPHYSICA ACTA-PROTEINS AND PROTEOMICS}},
Year = {{2018}},
Volume = {{1866}},
