@article{ ISI:000426961500028,
Author = {Herrera-Ortiz, Antonia and Conde-Glez, Carlos J. and Leonidez
   Olamendi-Portugal, Ma. and Garcia-Cisneros, Santa and Plett-Torres,
   Tanya and Sanchez-Aleman, Miguel A.},
Title = {{College women, HPV genotyping and sexual behavior before HPV
   vaccination: Results from samples stored for a long time}},
Journal = {{JOURNAL OF INFECTION AND PUBLIC HEALTH}},
Year = {{2018}},
Volume = {{11}},
Number = {{2}},
Pages = {{286-289}},
Month = {{MAR-APR}},
Abstract = {{HPV is the sexually transmitted agent most common among young people,
   like college students. The aim of study was to associate sexual behavior
   characteristics of women with HPV, detected in genital samples taken
   before the introduction of the HPV vaccine. Female students during
   2001-2005 donated genital samples and the samples were re-analyzed in
   2013 for HPV genotyping by RT-PCR. The frozen storage of the students'
   genital samples allowed the detection of HPV DNA and its genotyping
   after years of sample collection. HPV prevalence was 22\%, HPV16 3.9\%,
   and HPV18 1.1\%. Age, multiple sexual partners and the partner's age at
   first sexual intercourse were significantly associated to HPV. Students
   with >= 3 sexual partners and who did not use condom had 12.8 higher
   odds of being HPV positive. These results made possible the analysis of
   HPV prevalence changes, before HPV vaccine introduction. (C) 2017 The
   Authors. Published by Elsevier Limited on behalf of King Saud Bin
   Abdulaziz University for Health Sciences.}},
