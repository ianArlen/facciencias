@article{ ISI:000432249800007,
Author = {Rodriguez-Yc, Jose R.},
Title = {{Grinding Stones from Mesoamerica}},
Journal = {{ARQUEOLOGIA IBEROAMERICANA}},
Year = {{2018}},
Volume = {{37}},
Pages = {{53-60}},
Month = {{MAR}},
Abstract = {{This paper classifies the various stones that were used for grinding
   activities based on their form, function and use, as technological
   binomials. Ancient Mesoamerican peoples used these grinding stones in
   their everyday lives as they used them to process different plant and
   mineral products. They are repeatedly found in archaeological
   excavations, but are not always extensively analyzed. It is necessary to
   perform systematic studies from different disciplines of expertise to
   determine how these important tools were used in the past.}},
