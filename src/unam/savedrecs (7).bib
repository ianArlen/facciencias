@article{ ISI:000428933900005,
Author = {Perez-Atilano, Yarely and Sanchez-Gonzalez, Arturo and Terrazas, Teresa
   and Vasco, Y. Alejandra},
Title = {{Morphological and anatomical analysis of the Myriopteris lendigera
   (Pteridaceae) complex in Mexico}},
