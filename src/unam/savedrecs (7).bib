@article{ ISI:000425071500095,
Author = {Calcines-Cruz, Carlos and Olvera, Alejandro and Castro-Acosta, Ricardo
   M. and Zavala, Guadalupe and Alagon, Alejandro and Trujillo-Roldan,
   Mauricio A. and Valdez-Cruz, Norma A.},
Title = {{Recombinant-phospholipase A2 production and architecture of inclusion
   bodies are affected by pH in Escherichia coli}},
Journal = {{INTERNATIONAL JOURNAL OF BIOLOGICAL MACROMOLECULES}},
Year = {{2018}},
Volume = {{108}},
Pages = {{826-836}},
Month = {{MAR}},
Abstract = {{Aggregation of recombinant proteins into inclusion bodies (IBs) is the
   major drawback of heterologous expression in Escherichia coli. Here, we
   evaluated the effects of a pH shift after expression induction on
   recombinant phospholipase A2 production and its aggregation in IBs in E.
   coli Origami (TM), as compared to cultures with pH maintained at 7.5 or
   uncontrolled pH. Cultures shifted from 7.5 to pH 6.5 or 8.5 produced
   similar to 15-25\% less biomass as compared with those kept at 7.5 or
   without pH control. The cultures shifted to pH 8.5 showed a similar to
   50\% higher yield of acetate per biomass, and the rPLA2 yield was
   improved 2.4-fold. Purified IBs formed at pH 8.5 containing similar to
   50\% of rPLA2, were more susceptible to proteinase-K cleavage and bound
   less thioflavin-T, indicating lower amyloid content, with the
   concomitant enrichment of alpha-helical and random-coil secondary
   structures, as demonstrated by FTIR Moreover, only one IB per cell was
   formed at pH 8.5; instead, more than two were observed under the other
   culture pH conditions. Nevertheless, under uncontrolled pH conditions,
   similar to 300 nm larger lBs were observed. Our work presents evidence
   of the usefulness of recombinant protein expression cultivated at pH 8.5
   allowing the reduction of amyloid content in IBs. (C) 2017 Elsevier B.V.
   All rights reserved.}},
