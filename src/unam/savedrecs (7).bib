@article{ ISI:000425661500015,
Author = {Aguilar-Roblero, Raul and Mejia-Lopez, Adriana and Cortes-Pedroza,
   Dominique and Chavez-Juarez, Jose L. and Gutierrez-Monreal, Miguel A.
   and Dominguez, Gabriela and Vergara, Paula and Segovia, Jose},
Title = {{Calcium-regulated chloride channel anoctamin-1 is present in the
   suprachiasmatic nuclei of rats}},
Journal = {{NEUROREPORT}},
Year = {{2018}},
Volume = {{29}},
Number = {{4}},
Pages = {{334-339}},
Month = {{MAR 7}},
Abstract = {{Calcium-regulated chloride channel (CaCC) anoctamin-1 has been recently
   identified in neurons. In neurons, which express the Na+-K+-2Cl(-)
   cotransporter, activation of CaCCs increases firing frequency, by
   reversion between the Cl- equilibrium potential and the membrane resting
   potential, leading to membrane depolarization by Cl- extrusion from the
   cell. Although there are no reports of CaCCs present in the
   suprachiasmatic nuclei (SCN), the fact that Na+-K+-2Cl(-) cotransporter
   is present in SCN neurons, where it has been shown to be involved in the
   excitatory effects of -aminobutyric acid, together with the increase of
   neuronal firing rate induced by release of intracellular Ca2+ after
   administration of 100nM ryanodine, leads us to determine whether CaCCs
   are present in the SCN. Immunohistochemistry and western blots show the
   expression of the CaCCs anoctamin-1 protein. Quantitative PCR
   demonstrated the expression of anoctamin-1 mRNA in the SCN. These
   results clearly indicate the presence of CaCC in SCN of rats.}},
