@article{ ISI:000460748100010,
Author = {Chinchilla-Lopez, Paulina and Ramirez-Perez, Oscar and Cruz-Ramon, Vania
   and Canizales-Quinteros, Samuel and Dominguez-Lopez, Aaron and
   Ponciano-Rodriguez, Guadalupe and Sanchez-Munoz, Fausto and
