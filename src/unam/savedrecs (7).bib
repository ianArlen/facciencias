@article{ ISI:000427453300047,
Author = {Andersson, Emma R. and Chivukula, Indira V. and Hankeova, Simona and
   Sjoqvist, Marika and Tsoi, Yat Long and Ramskold, Daniel and Masek, Jan
   and Elmansuri, Aiman and Hoogendoorn, Anita and Vazquez, Elenae and
   Storvall, Helena and Netusilova, Julie and Huch, Meritxell and Fischler,
