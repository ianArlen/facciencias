@article{ ISI:000415778300021,
Author = {Alegria-Galicia, Carlos and Orden, David and Seara, Carlos and Urrutia,
   Jorge},
Title = {{On the O-beta-hull of a planar point set}},
Journal = {{COMPUTATIONAL GEOMETRY-THEORY AND APPLICATIONS}},
Year = {{2018}},
Volume = {{68}},
Number = {{SI}},
Pages = {{277-291}},
Month = {{MAR}},
Abstract = {{We study the O-beta-hull of a planar point set, a generalization of the
   Orthogonal Convex Hull where the coordinate axes form an angle beta.
   Given a set P of n points in the plane, we show how to maintain the
   O-beta-hull of P while beta runs from 0 to pi in Theta(n logn) time and
   O(n) space. With the same complexity, we also find the values of beta
   that maximize the area and the perimeter of the O-beta-hull and,
   furthermore, we find the value of beta achieving the best fitting of the
   point set P with a two joint chain of alternate interior angle beta. (C)
   2017 Elsevier B.V. All rights reserved.}},
