@article{ ISI:000433224000006,
Author = {Lopez-Mora, Ulises and Llorente-Bousquets, Jorge},
Title = {{Checklist and illustrated key of the Phasmatodea (Insecta) genera from
   Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2018}},
Volume = {{89}},
Number = {{1}},
Pages = {{46-78}},
Month = {{MAR}},
Abstract = {{The order Phasmatodea has been poorly studied in Mexico, and it does not
   exist any work that lists its species in the country or any
   identification key to genus level. In this work we carried out an
   exhaustive bibliographic revision to obtain the list of species of
   Phasmatodea that occur in Mexico. A total of 17 genera and 65 species of
   Phasmatodea were registered for the Mexican territory. An illustrated
   identification generic key with its respective terminological glossary
   is provided.}},
