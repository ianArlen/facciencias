@article{ ISI:000424701400005,
Author = {Chavez, I. and Garcia, L. A. and Grether, M. and de Llano, M. and
   Tolmachev, V. V.},
Title = {{Extended BCS-Bose Crossover}},
Journal = {{JOURNAL OF SUPERCONDUCTIVITY AND NOVEL MAGNETISM}},
Year = {{2018}},
