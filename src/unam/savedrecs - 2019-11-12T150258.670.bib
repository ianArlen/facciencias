@article{ ISI:000239109300009,
Author = {Rodriguez-Trejo, R. and Bosch, P. and Bulbulian, S.},
Title = {{Combustion treatment of Co2+ and Cs+ exchanged zeolites}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2006}},
Volume = {{354}},
