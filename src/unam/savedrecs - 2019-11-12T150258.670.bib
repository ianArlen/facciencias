@article{ ISI:000240030200010,
Author = {Lopez-Juarez, A. and Gutierrez-Arenas, N. and Rivera-Santillan, R. E.},
Title = {{Electrochemical behavior of massive chalcopyrite bioleached electrodes
   in presence of silver at 35 degrees C}},
Journal = {{HYDROMETALLURGY}},
