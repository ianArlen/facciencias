@article{ ISI:000241708500001,
Author = {Trejo, A. and Rodriguez, L. F.},
Title = {{On the distance to the bright nonthermal radio sources in the direction
   of an extraordinarily massive cluster of red supergiants}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2006}},
Volume = {{42}},
Number = {{2}},
Pages = {{147-151}},
Month = {{OCT}},
Abstract = {{An extraordinarily massive cluster of red supergiants has been recently
   reported in the direction of the galactic coordinates l = 25.degrees 3;
   b = -0.degrees 2. This cluster is associated with an X-ray source, a
   very high-energy gamma-ray source, and three bright non-thermal radio
   sources. The a priori probability of these associations being only
   line-of-sight coincidences is very small. However, we have analyzed VLA
   archive data taken toward these three radio sources and find from their
   H1 absorption spectra that they are extragalactic and thus not directly
   associated with the galactic cluster.}},
