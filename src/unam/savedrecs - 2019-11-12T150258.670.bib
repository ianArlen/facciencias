@article{ ISI:000242671200008,
Author = {Baig, Salman and Damian, Raymond T. and Morales-Montor, Jorge and
   Ghaleb, Amr and Baghdadi, Amjed and White, Jr., A. Clinton},
Title = {{Protection from murine cysticercosis by immunization with a parasite
   cysteine protease}},
Journal = {{MICROBES AND INFECTION}},
Year = {{2006}},
Volume = {{8}},
Number = {{12-13}},
Pages = {{2733-2735}},
Month = {{OCT}},
Abstract = {{Central nervous system infection by Taenia solium cysts causes
   neurocysticercosis, a common neurological infection in the Third World.
   We have previously isolated cysteine proteases from Taenia crassiceps
   and T. solium. In this study, we immunized BALB/c mice with the purified
   T. solium cysteine protease and challenged them with Taenia crassiceps.
   Immunized mice had a 72\% reduction in parasite burden compared to mice
   that received no immunization. Immunized mice developed antigen specific
   lymphocyte proliferation. These data support further studies of the T.
   solium cysteine protease as a vaccine candidate. (c) 2006 Elsevier
   Masson SAS. All rights reserved.}},
