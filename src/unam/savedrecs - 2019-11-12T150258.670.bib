@article{ ISI:000240279200035,
Author = {Valente, Jaime S. and Falcon, Sofia and Lima, Enrique and Vera, Marco A.
   and Bosch, Pedro and Lopez-Salinas, Esteban},
Title = {{Phosphating alumina: A way to tailor its surface properties}},
Journal = {{MICROPOROUS AND MESOPOROUS MATERIALS}},
Year = {{2006}},
Volume = {{94}},
Number = {{1-3}},
Pages = {{277-282}},
Month = {{SEP 8}},
Abstract = {{Phosphated alumina with new surface properties was prepared by sol-gel
   synthesis. Aluminum coordinately unsaturated sites (CUS) were promoted
   and tuned through phosphate addition. Two phosphate species on alumina
   were disclosed by P-31 MAS NMR spectroscopy. The population of aluminum
   CUS was related to the amount of phosphorus incorporated into the
   alumina network. The relative amount of phosphorus species was
   controlled by the alumina gel's aging time. X-ray diffraction, performed
   on calcined solids, identified a microcrystalline structure of
   gamma-alumina. Phosphated aluminas showed both high surface areas
   (364-394 m(2)/g) and total pore volume (1.0-1.2 cm(3)/g), even after
   calcining at 550 degrees C. (c) 2006 Published by Elsevier Inc.}},
