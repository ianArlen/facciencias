@article{ ISI:000242920800018,
Author = {Madhavaraju, J. and Hussain, S. M. and Guruvappan, A. and Ramasamy, S.
   and Mohan, S. P.},
Title = {{Sequence stratigraphy of lower Niniyur Formation of Cauvery basin, Tamil
   Nadu}},
Journal = {{JOURNAL OF THE GEOLOGICAL SOCIETY OF INDIA}},
