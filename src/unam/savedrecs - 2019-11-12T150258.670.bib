@article{ ISI:000241596100013,
Author = {Atakishiyev, Natig M.},
Title = {{On q-extensions of Mehta's eigenvectors of the finite Fourier transform}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS A}},
Year = {{2006}},
Volume = {{21}},
Number = {{23-24}},
Pages = {{4993-5006}},
Month = {{SEP 30}},
Note = {{2nd Meeting on Interface of Gravitational and Quantum Realms, Zacatecas,
   MEXICO, DEC 04-08, 2005}},
Abstract = {{Mehta has shown that eigenvectors F-j((k)) of the finite Fourier
   transform with the matrix
   A(j,k)((N)) := 1/root N exp(2 pi i/N.jk), 0 <= j, k <= N - 1, can be
   defined in terms of the the classical
   Hermite functions H-k(x) := H-k(x)exp(-x(2)/2) as F-j((k)) := Sigma
   H-infinity(n)k(x(n)((j))), where
   x(n)((j)) := root 2 pi/N(nN + j). We argue that the finite Fourier
   transform A(j,k)((N)) does actually
   govern also some q-extensions of Mehta's eigenvectors F-j((k)),
   associated with certain well-known orthogonal q-polynomial families. For
   the pairs of the continuous q-Hermite and q(-1)-Hermite polynomials, the
   Rogers-Szego and Stieltjes-Wigert polynomials, and the discrete
   q-Hermite polynomials of types I and II such links are explicitly
   derived.
   In the limit when the base q -> 1 these q-extensions coincide with
   Mehta's eigen-vectors F-j((k)), whereas in the continuous limit (i.e.
   when the parameter N -> infinity) they correspond to the classical
   Fourier integral transforms between the above-mentioned pairs of
   q-polynomial families.}},
