@article{ ISI:000239273300048,
Author = {Naumis, G. G. and del Castillo-Mussot, M. and Perez, L. A. and Vazquez,
   G. J.},
Title = {{Phase transition and diffusivity in social hierarchies with attractive
   sites}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2006}},
Volume = {{369}},
Number = {{2}},
Pages = {{789-798}},
Month = {{SEP 15}},
Abstract = {{We study the effects of including a distribution of valuable or
   attractive sites in a two-dimensional lattice in self-organizing social
   hierarchies. Agents move aleatorily except in the case where an
   attractive site is located in their neighborhood. We find that the
   transition between an egalitarian society at low population density and
   a hierarchical one at high population density strongly depends on the
   distribution and percolation of strategic sites. Also, it is shown how
   agent diffusivity is closely related to the amount of inequality. The
   proposed model introduces an optimization aspect to the problem of
   social hierarchies since the system tends to maximize the occupation of
   attractive sites (wealth per capita). However, when the density of
   attractive sites is small, the system fails to reach this state, and is
   trapped in a local minimum, as in a glass or jam transition. (c) 2006
   Elsevier B.V. All rights reserved.}},
