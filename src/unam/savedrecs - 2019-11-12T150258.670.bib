@article{ ISI:000240781100002,
Author = {Zepeda, T. A. and Pawelec, B. and Fierro, J. L. G. and Halachev, T.},
Title = {{Effect of Ti on the catalytic properties of CoMo/Ti(x)-HMS catalysts in
   the reaction of hydrodesulfurization of 4-ethyl-6-methyl
   dibenzothiophene}},
Journal = {{JOURNAL OF CATALYSIS}},
