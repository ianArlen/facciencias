@article{ ISI:000240691100049,
Author = {Preciado-Flores, S. and Schreck, M. and Melendrez, R. and Chernov, V.
   and Bernal, R. and Cruz-Vazquez, C. and Cruz-Zaragoza, E. and
   Barboza-Flores, M.},
Title = {{Performance of CVD diamond as an optically and thermally stimulated
   luminescence dosemeter}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{2006}},
Volume = {{119}},
Number = {{1-4}},
Pages = {{226-229}},
Month = {{SEP}},
Note = {{14th International Conference on Solid State Dosimetry, New Haven, CT,
   JUN 27-JUL 02, 2004}},
Organization = {{Yale Univ Sch Med; Int Org Solid State Dosimetry; Oxford Univ Press;
   Landauer Inc; Canberra Ind; Global Dosimetry Solut Inc}},
Abstract = {{Diamond is a material with extreme physical properties. Its radiation
   hardness, chemical inertness and tissue equivalence qualify it as an
   ideal material for radiation dosimetry. In the present work, the
   optically stimulated luminescence (OSL) and thermoluminescence (TL)
   characteristics of a 10 mu m thick CVD diamond (polycrystalline diamond
   films prepared by chemical vapor deposition) film were studied in order
   to test its performance as a beta radiation dosemeter. The TL response
   is composed of four main TL glow peaks; two of these are in the range of
   150-200 degrees C and two additional peaks in the 250-400 degrees C
   temperature range. The integrated TL as a function of radiation dose is
   linear up to 100 Gy and increases with increasing dose exposure. The
   dose dependence of the integrated OSL exhibits a similar behavior. The
   observed OSL/TL behavior for the CVD diamond film clearly demonstrate
   its capability for applications in radiation dosimetry with special
   relevance in medical dosimetry owing to the diamond's intrinsic material
   properties.}},
