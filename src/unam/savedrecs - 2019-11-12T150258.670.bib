@article{ ISI:000241708500012,
Author = {Salas, L. and Cruz-Gonzalez, I. and Tapia, M.},
Title = {{San Pedro Martir mid-infrared photometric system}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2006}},
Volume = {{42}},
Number = {{2}},
Pages = {{273-289}},
Month = {{OCT}},
Abstract = {{With the goal of defining the San Pedro Martir Mid-infrared Photometric
   System (SPM-MIR), observations of well-defined calibration stars were
   carried out on 9 photometric runs from 2000 to 2005. The mid-infrared (2
   - 28 mu m) camera CID-BIB of the Observatorio Astronomico Nacional was
   used. A set of 9 filters, the ``silicate{''} series SiN, SiO, SiP, SiQ,
   SiR, SiS, the broad-band N (10.8 mu m) filter, and the narrow-band QH2
   (17.15 mu m) and Q2 (18.7 mu m), was used to derive the extinction
   coefficients and zero magnitude points. Atmospheric extinction
   corrections were carried out making use of Pade approximants and we
   found a linear dependence on the parameters with the low air mass
   extinction coefficient. The atmospheric transmission curves and
   extinction coefficients of SPM were compared with those obtained for the
   Mauna Kea site. Color terms were derived using a set of IRAS LSR sources
   observed with the CID-BIB.}},
