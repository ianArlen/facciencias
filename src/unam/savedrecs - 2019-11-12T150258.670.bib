@article{ ISI:000240237000040,
Author = {Lopez-Rendon, R. and Reyes, Y. and Orea, P.},
Title = {{Thermodynamic properties of short-range square well fluid}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{2006}},
Volume = {{125}},
Number = {{8}},
Month = {{AUG 28}},
Abstract = {{The interfacial properties of short-range square well fluid with
   lambda=1.15, 1.25, and 1.375 were determined by using single canonical
   Monte Carlo simulations. Simulations were carried out in the
   vapor-liquid region. The coexistence curves of these models were
   calculated and compared to those previously reported in the literature
   and good agreement was found among them. We found that the surface
   tension curves for any potential model of short range form a single
   master curve when we plot gamma({*}) vs T/T-c. It is demonstrated that
   the critical reduced second virial coefficient B-2({*}) as a function of
   interaction range or T-c({*}) is not constant. (c) 2006 American
   Institute of Physics.}},
