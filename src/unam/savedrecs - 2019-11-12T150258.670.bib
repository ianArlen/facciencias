@article{ ISI:000239054000032,
Author = {Salinas, Oscar H. and Lopez-Mata, C. and Hu, Hailin and Nicho, M. E.},
Title = {{Analysis of electrical parameters in heterojunctions based on poly
   3-octylthiophene and cadmium sulfide thin films}},
Journal = {{SOLAR ENERGY MATERIALS AND SOLAR CELLS}},
