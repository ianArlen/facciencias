@article{ ISI:000240149300005,
Author = {Arellano-Torres, A. and Perez-Castaneda, R. and Defeo, O.},
Title = {{Effects of a fishing gear on an artisanal multispecific penaeid fishery
   in a coastal lagoon of Mexico: mesh size, selectivity and management
   implications}},
Journal = {{FISHERIES MANAGEMENT AND ECOLOGY}},
Year = {{2006}},
Volume = {{13}},
Number = {{5}},
Pages = {{309-317}},
Month = {{OCT}},
Abstract = {{The spatiotemporal effects of a bottom net used in an artisanal
   multispecific penaeid fishery at Celestun Lagoon, Mexico, were
   evaluated. Selectivity experiments, based on the covered codend method,
   were carried out with two bottom nets operating simultaneously but using
   different mesh size codends: 13 mm (MS13: currently used in the fishery)
   and 25 mm (MS25). Length at 50\% retention, L-50, was twice as high with
   MS25 (15.34 mm carapace length, CL) than with MS13 (6.95 mm CL); this
   global pattern was recurrent for all species, seasons and hydrological
   zones. These differences in selectivity determined that MS13 mainly
   retained recruits and juveniles, whereas MS25 mainly retained
   sub-adults. However, smaller shrimps were caught by both mesh sizes
   during the `nortes' season (November to February) and at the outer zones
   of the lagoon. Thus, an increase in mesh size from 13 to 25 mm, together
   with fishery closures within specific area-season windows, is suggested
   to reduce risks of overfishing of these shrimp populations.}},
