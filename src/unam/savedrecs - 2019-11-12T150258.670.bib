@article{ ISI:000245492900001,
Author = {Navarrete, J. -P.},
Title = {{On the limit set of discrete subgroups of PU(2,1)}},
Journal = {{GEOMETRIAE DEDICATA}},
Year = {{2006}},
Volume = {{122}},
Number = {{1}},
Pages = {{1-13}},
Month = {{OCT}},
Abstract = {{Let G be a discrete subgroup of PU(2, 1); G acts on P-C(2) preserving
   the unit ball H-C(2), equipped with the Bergman metric. Let L(G)subset
   of S-3 = partial derivative H-C(2) be the limit set of G in the sense of
   Chen-Greenberg, and let Lambda(G) subset of P-C(2) be the limit set of
   the G-action on P-C(2) in the sense of Kulkarni. We prove that L(G) =
   Lambda(G) boolean AND S-3 and Lambda(G) is the union of all complex
   projective lines in P-C(2) which are tangent to S-3 at a point in L(G).}},
