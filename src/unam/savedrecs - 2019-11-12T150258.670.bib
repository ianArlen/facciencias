@article{ ISI:000239872000010,
Author = {Ascanio, Gabriel and Brito-Bazan, Magdalena and Tanguy, Philippe A.},
Title = {{A non-conventional approach for the make-down of concentrated
   suspensions}},
Journal = {{CHEMICAL ENGINEERING \& TECHNOLOGY}},
Year = {{2006}},
Volume = {{29}},
Number = {{8}},
Pages = {{968-973}},
Month = {{AUG}},
Abstract = {{Concentrated suspensions made of delaminated kaolin clay in water were
   prepared using two approaches, namely steady stirring and dynamic
   perturbation operating modes. In the first approach, the impeller was
   rotated at constant speed in one direction. For the second approach, the
   flow into the tank was dynamically perturbed by rotating the impeller
   cyclically in both directions. Two high-shear impellers were used: a
   Sevin impeller and a Cowles turbine. During the mixing, the evolution of
   torque was measured and samples were taken at regular intervals. All the
   results were compared in terms of the mixing energy consumption,
   particle average size and distribution, as well as the shear rheology.
   Suspensions prepared with the dynamic perturbation approach were found
   to require less energy and exhibited, in general, smaller particles and
   lower viscosity than with conventional mixing, which indicates that the
   proposed method could be an efficient alternative to conventional
   mixing.}},
