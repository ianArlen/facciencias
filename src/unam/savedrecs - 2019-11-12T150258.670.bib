@article{ ISI:000240874500092,
Author = {Martinez, Santiago A. and Montemayor, R. and Urrutia, Luis F.},
Title = {{Reality and causality in quantum gravity modified electrodynamics}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2006}},
Volume = {{74}},
Number = {{6}},
Month = {{SEP}},
Abstract = {{We present a general description of the propagation properties of
   quantum gravity modified electrodynamics characterized by constitutive
   relations up to second order in the correction parameter. The effective
   description corresponds to an electrodynamics in a dispersive and
   absorptive nonlocal medium, where the Green functions and the refraction
   indices can be explicitly calculated. The reality of the electromagnetic
   field together with the requirement of causal propagation in a given
   reference frame leads to restrictions in the form of such refraction
   indices. In particular, absorption must be present in all cases and,
   contrary to the usual assumption, it is the dominant aspect in those
   effective models which exhibit linear effects in the correction
   parameter not related to birefringence. In such a situation absorption
   is linear while propagation is quadratical in the correction parameter.}},
