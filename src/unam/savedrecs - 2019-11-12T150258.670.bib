@article{ ISI:000241020700034,
Author = {Navarro, O. and Carvajal, E. and Aguilar, B. and Avignon, M.},
Title = {{Ferromagnetic-antiferromagnetic transition in double perovskites}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
Year = {{2006}},
