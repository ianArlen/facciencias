@article{ ISI:000252042700009,
Author = {Fernandez-Osorio, A. and Castaneda-Guzman, R. and Vazquez-Olmos, A. and
   Huanosta-Tera, Alfonso},
Title = {{Photoacoustic and dielectiric study of lead zirconate titanate
   nanoparticles}},
Journal = {{FERROELECTRICS}},
Year = {{2007}},
Volume = {{361}},
Pages = {{92-104}},
Abstract = {{Using Sol-Gel method lead titanate zirconate (PZT) nanoparticles with an
   average size of 13.07 +/- 2.8 nm, estimated from transmission electron
   microscopy (TEM) images, were obtained. Their X-ray powder diffraction
   (XRD) pattern shows a single phase of Pb(Zr0.52Ti0.48)O-3
   nanocrystallites. Photoacoustic and dielectric experiments on these PZT
   nanoparticles were performed in a wide temperature range;from room
   temperature zip to 660 degrees C. Photoacoustic experiments performed in
   as synthesized powders reveal an extraordinary dispersion in a large
   temperature interval, attributable to the size of the PZT-nanoparticles.
   Combining photoacoustic and dielectric results, obtained from sintered
   samples it was possible to establish the existence of a dispersive
   paraelectric transition temperature at around 470 degrees C. Impedance
   curves were used to extract both frequency dependent (epsilon'(omega))
   and frequency independent (epsilon') dielectric constant. The
   temperature dependence of (epsilon'), behaves in a quite dispersive
   manner, although a peak around 470 degrees C is suggested. A much more
   elaborate situation is found when (epsilon'(omega)) is plotted against
   temperature. At selected temperatures,frequency dependent curves of
   epsilon'(omega) show that at low frequencies (<100 Hz) there is a strong
   dispersion, but as the frequency increases epsilon'(omega) tends to be
   frequency independent. The magnitude of epsilon'(omega) depends slightly
   on the temperature.}},
