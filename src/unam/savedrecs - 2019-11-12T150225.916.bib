@incollection{ ISI:000311351700019,
Author = {Armienta, M. A. and Rodriguez, C. R. and Ongley, L. K. and Brust, H. and
   Morales, F. and Aguayo, A. and Cruz, O. and Ceniceros, N.},
Editor = {{Bhattacharya, P and Mukherjee, AB and Bundschuh, J and Zevenhoven, R and Loeppert, RH}},
Title = {{Origin and fate of arsenic in a historic mining area of Mexico}},
Booktitle = {{ARSENIC IN SOIL AND GROUNDWATER ENVIRONMENT: BIOGEOCHEMICAL
   INTERACTIONS, HEALTH EFFECTS AND REMEDIATION}},
Series = {{Trace Metals and Other Contaminants in the Environment}},
Year = {{2007}},
Volume = {{9}},
Pages = {{473-498}},
Abstract = {{A discussion on groundwater-As pollution in Mexico in the framework of
   mining activities, and the research developed in one particular
   contaminated location are presented. More research is needed to
   ascertain the sources and fate of As at many other contaminated sites.
   Groundwater contamination in Zimapan, Central Mexico, is the result of
   both natural (ore) and anthropogenic (smelters and tailings'
   impoundments) processes. Arsenic released from arsenopyrite oxidation
   within tailings contaminates the shallow aquifer in spite of the
   sorption and formation of secondary minerals that retain some of the As.
   Alteration and dissolution of As-minerals in mineralized zones
   contaminate a deeper limestone aquifer. Arsenic is transported through
   faults and fractures to potable water wells and springs. In the town of
   Zimapan, treatment with ferric sulfate proved to be adequate to remove
   As from the contaminated municipal water. Another low-tech treatment
   technique using shaly limestone also removed most of the As.}},
