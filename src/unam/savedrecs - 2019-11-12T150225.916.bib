Author = {Moran, P. and Gomez, A. and Valadez, A. and Ramos, F. and Gonzalez, E.
   and Garcia, G. and Limon, A. and Valenzuela, O. and Ramiro, M. and
   Hidalgo, H. and Riebeling, C. and Melendro, E. I. and Ximenez, C.},
Editor = {{Boeree, MJ}},
Title = {{Amebic and pyogenic liver abscess: Importance of differential diagnosis
   in endemic areas of amebiasis}},
Booktitle = {{5TH EUROPEAN CONGRESS ON TROPICAL MEDICINE AND INTERNATIONAL HEALTH}},
Year = {{2007}},
Pages = {{57+}},
Note = {{5th European Congress on Tropical Medicine and International Health,
   Amsterdam, NETHERLANDS, MAY 24-28, 2007}},
Organization = {{Netherlands Soc Trop Med \& Int Hlth; Federat European Soc Trop Med \&
   Int Hlth}},
Abstract = {{The aim of the present work was to compare sera levels of anti-E.
   histolytica antibodies in patients with liver abscess and to establish
   the importance of serology in the differential diagnosis in endemic
   areas of amebiasis. We also show the validation, specificity and
   sensitivity of an ELISA test used in the serological diagnosis of amebic
   liver abscess (ALA) as the central tool in a fast and reliable
   differential diagnosis. The estimation of both specificity and
   sensitivity of the test were 82.4\% and 90.0\%, respectively, as
   assessed for diagnosis of amebic liver abscess. The statistical analysis
   of differences in anti-amebic antibody levels, performed through the chi
   square test between ALA, pyogenic liver abscess, and non-parasitized
   healthy individual groups show that these differences were statistically
   significant (p < 0.01). The analysis of the likelihood ratio of the
   ELISA test allows us to conclude that, the probability of obtaining a
   positive result in healthy individuals is negligible (LR-: 0.04, 95\%
   CI: 0.01-0.14).}},
