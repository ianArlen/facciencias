@article{ ISI:000244652700014,
Author = {Castillo, F. and Herrera, J. J. E. and Rangel, J. and Golzarri, J. I.
   and Espinosa, G.},
Title = {{Nuclear track methodology for the analysis of isotropic components in a
   plasma focus neutron yield}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2007}},
Volume = {{53}},
Number = {{3, S}},
Pages = {{61-64}},
Month = {{FEB}},
Note = {{2nd International Symposium on Radiation Physics, Chihuahua, MEXICO, FEB
   27-MAR 02, 2006}},
Organization = {{Consejo Nacl Cienc Tecnol; Subsecret Educ Super Investigac Cient
   Secreatar Educac Publ; Cent Investigac Mat Fis; Inst Fis; Univ Nacl
   Automoma Mexico}},
Abstract = {{The average angular distributions of neutron emissions have been
   measured in the Fuego Nuevo II (FN-II) dense plasma focus device (50) by
   means of CR-39 plastic nuclear track detectors. When pure deuterium is
   used as the filling gas, the data can be adjusted to a Gaussian
   function, related to anisotropic emission, superposed on a constant
   pedestal, related to isotropic emission. When deuterium-argon admixtures
   are used, the anisotropic contribution is best represented by a
   parabola. The neutron flux at these two angles are used, along with the
   angular distribution obtained from the track detectors, in order to
   estimate the absolute neutron yield of both the isotropic and the
   anisotropic contributions. From examining different groups of shots, it
   is found that the shape of the angular distribution is important in the
   estimation of anisotropy, and that the value usually reported, as the
   ratio of the neutron flux head on and side on, as measured by activation
   counters, may be misleading.}},
