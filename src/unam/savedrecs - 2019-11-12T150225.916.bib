@article{ ISI:000255260300004,
Author = {Dominguez, Hector and Pizio, Orest and Pusztai, Laszlo and Sokolowski,
   Stefan},
Title = {{The structural properties and diffusion of a three-dimensional isotropic
   core-softened model fluid in disordered porous media. Molecular dynamics
   simulation}},
