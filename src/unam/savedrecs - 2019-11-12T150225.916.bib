@inproceedings{ ISI:000250750700027,
Author = {Castanon-Olivares, Laura Rocio and Guerena-Elizalde, Diego and
   Gonzalez-Martinez, Marisela Rocio and Licea-Navarro, Alexei Fedorovich
   and Gonzalez-Gonzalez, Gloria Maria and Aroch-Calderon, Arturo},
Editor = {{Clemons, KV and LaniadoLaborin, R and Stevens, DA}},
Title = {{Molecular identification of Coccidioides isolates from Mexican patients}},
Booktitle = {{COCCIDIOIDOMYCOSIS: SIXTH INTERNATIONAL SYMPOSIUM}},
Series = {{Annals of the New York Academy of Sciences}},
