@article{ ISI:000244532300032,
Author = {Bradler, K.},
Title = {{Eavesdropping of quantum communication from a noninertial frame}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2007}},
Volume = {{75}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{We introduce a relativistic version of the quantum encryption protocol
   by considering two inertial observers who wish to securely transmit
   quantum information encoded in a free scalar quantum field state forming
   Minkowski particles. In a nonrelativistic setting a certain amount of
   shared classical resources is necessary to perfectly encrypt the state.
   We show that in the case of a uniformly accelerated eavesdropper the
   communicating parties need to share (asymptotically in the limit of
   infinite acceleration) just half of the classical resources.}},
