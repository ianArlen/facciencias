@article{ ISI:000243132000009,
Author = {Rosado, M. and Arias, L. and Ambrocio-Cruz, P.},
Title = {{Kinematics of the molecular hydrogen associated with the supernova
   remnant IC 443}},
Journal = {{ASTRONOMICAL JOURNAL}},
Year = {{2007}},
Volume = {{133}},
