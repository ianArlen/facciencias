Title = {{Thermal analysis and microstructure comparison between A356 aluminum
   alloy and A356/15\%vol.SiCp cast composite modified with strontium}},
Booktitle = {{ADVANCED STRUCTURAL MATERIALS III}},
Series = {{Materials Science Forum}},
Year = {{2007}},
Volume = {{560}},
