@article{ ISI:000247473000003,
Author = {Hayashi, Nakao and Kaikina, Elena I. and Naumkin, Pavel I.},
Title = {{Large time asymptotics for the BBM-Burgers equation}},
Journal = {{ANNALES HENRI POINCARE}},
Year = {{2007}},
Volume = {{8}},
Number = {{3}},
Pages = {{485-511}},
Abstract = {{We study large time asymptotics of solutions to the BBM-Burgers equation
   partial derivative(t)(u - u(xx)) + beta u(x) - mu u(xx) + uu(x) = 0. We
   are interested in the large time asymptotics for the case, when the
   initial data have an arbitrary size. Let the initial data u(0) is an
   element of H(1) (R) boolean AND W(1)(1) (R), and 0 = integral(R) u(0)
   (x) dx not equal 0. Then we prove that there exists a unique solution u
   (t, x) is an element of C ({[}0, infinity); H(1) (R) boolean AND W(1)(1)
   (R)) to the Cauchy problem for the BBM-Burgers equation. We also find
   the large time asymptotics for the solutions.}},
