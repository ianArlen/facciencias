@article{ ISI:000251284100006,
Author = {Garcia, Alberto Bravo and Sahu, Sarira},
Title = {{Higher order correction to the neutrino self-energy in a medium and its
   astrophysical applications}},
Journal = {{MODERN PHYSICS LETTERS A}},
Year = {{2007}},
Volume = {{22}},
Number = {{3}},
Pages = {{213-225}},
Month = {{JAN 30}},
Abstract = {{We have calculated the 1/M-4 (M the vector boson mass) order correction
   to the neutrino self-energy in a medium. The possible application of
   this higher order contribution to the neutrino effective potential is
   considered in the context of the Early Universe hot plasma and of the
   cosmological Gamma Ray Burst fireball. We found that, depending on the
   medium parameters and on the neutrino properties (mixing angle and mass
   square difference), the resonant oscillation of active to active
   neutrinos is possible.}},
