@article{ ISI:000243543200025,
Author = {Toledano, O. and Moreno, E. and Koenigsberger, G. and Detmers, R. and
   Langer, N.},
Title = {{Tides in asynchronous binary systems}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
