@article{ ISI:000246981600001,
Author = {Macias-Ruvalcaba, Norma A. and Evans, Dennis H.},
Title = {{Electron transfer and structural change: Distinguishing concerted and
   two-step processes}},
Journal = {{CHEMISTRY-A EUROPEAN JOURNAL}},
Year = {{2007}},
Volume = {{13}},
Number = {{16}},
Pages = {{4386-4395}},
Abstract = {{In electron-transfer reactions accompanied by structural changes, the
   structural change can be concerted with electron transfer or can occur
   in a separate reaction either preceding or following the
   electron-transfer step. In this paper we discuss ways of distinguishing
   concerted reactions from the latter two-step type. Included are recent
   examples in which no intermediates have been detected in the reactions,
   thus precluding the direct assignment to the two-step category. In these
   cases, other means are used to build support for the two-step mechanism
   with respect to the concerted process. These include an example of
   structural change preceding electron transfer, a demonstration that the
   current models of concerted reactions cannot fit the voltammetric data,
