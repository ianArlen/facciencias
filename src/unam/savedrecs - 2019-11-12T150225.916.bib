@article{ ISI:000245015300024,
Author = {Sanchez-Salcedo, F. J. and Santillan, A. and Franco, J.},
Title = {{Cloudy intergalactic accretion flows in the outer discs of galaxies}},
Journal = {{NEW ASTRONOMY REVIEWS}},
Year = {{2007}},
Volume = {{51}},
Number = {{1-2}},
Pages = {{104-107}},
Month = {{FEB}},
Note = {{Workshop on Fate of the Gas in Galaxies, Dwingeloo, NETHERLANDS, JUL
   12-14, 2006}},
Organization = {{ASTRON}},
Abstract = {{High-resolution two-dimensional magnetohydrodynamical simulations have
   been carried out to investigate the role of continuing infall of clumpy
   gas as a driver of turbulence in extended H I galactic discs. We have
   compared the responses of isothermal gas discs with sound speeds 4 and 8
   km/s to infalling, condensed clouds. For mass accretion rates of -0.6
   M-circle dot yr(-1), the turbulent motions in the outer disc become
   slightly faster than transonic. We suggest that the rain of compact high
   velocity clouds on the disc not only can fuel the Milky Way with fresh
   material but is a potential source of random motions in outer regions of
   H I discs. (c) 2006 Elsevier B.V. All rights reserved.}},
