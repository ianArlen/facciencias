@article{ ISI:000244149300012,
Author = {Sandoval-Castellanos, Edson and Uribe-Alcocer, Manuel and Diaz-Jaimes,
   Pindaro},
Title = {{Population genetic structure of jumbo squid (Dosidicus gigas) evaluated
   by RAPD analysis}},
Journal = {{FISHERIES RESEARCH}},
Year = {{2007}},
Volume = {{83}},
Number = {{1}},
Pages = {{113-118}},
Month = {{JAN}},
Abstract = {{The jumbo squid (Dosidicus gigas) supports important fisheries, with the
   maximum landings being recorded in Mexico, Peru and Chile.
   To evaluate genetic structure and to measure the impact of temporal
   variation, randomly amplified polymorphic DNA (RAPD) data were generated
   and analysed from 2 10 samples from eight eastern Pacific sites and on
   two sample sets taken from the same location in consecutive years. The
   temporal variation was shown to have no significant effect on genetic
   diversity. In addition, a genetic structure was detected which divided
   the populations into northern and southern locations. There was marginal
   isolation by distance between populations. Like in other ommastrephids,
   genetic structure is considered evidence of an adaptive radiation. (c)
   2006 Elsevier B.V. All rights reserved.}},
