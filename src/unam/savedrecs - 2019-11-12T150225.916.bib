@article{ ISI:000244599100105,
Author = {Grabski, V. and Brandan, M. -E.},
Title = {{A simple method to estimate coordinate resolution and MTF for pixelized
   detectors}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
Year = {{2007}},
Volume = {{571}},
Number = {{1-2}},
Pages = {{433-436}},
Month = {{FEB 1}},
Note = {{1st International Conference on Molecular Imaging Technology, Marseille,
   FRANCE, MAY 09-12, 2006}},
Organization = {{Univ Mediterranean; CERN; Govt City Marseilles; Bouches du Rhone,Gen
   Council Bouches; CISBIO; SCIONIX}},
Abstract = {{A simple method to estimate the coordinate resolution of pixelized
   detectors, using non-single photon counting readout is proposed. This
   method can be useful particularly for testing detectors used in digital
   imaging systems. The proposal is based on the determination of the
   statistical correlation between detector neighbor pixels, by means of
   the statistical noise measurement of isolated pixels, and the linear
   combination of neighbor pixels. Distributions of differences between
   neighbor pixels are constructed for pixels sharing an edge or only
   having one common vertex. The method is validated using images from a GE
   Senographe 2000D mammographic unit. The pixelized detector has been
   irradiated by X-rays along its entire surface. The correlation asymmetry
   between perpendicular directions has been studied to estimate the
   possible existence of a hardware asymmetry. The coordinate resolution of
   the studied detector, pixel size 100 mu m, is of the order of 50-55 mu
   m, that is, almost twice as large as the value determined by geometry. A
   small asymmetry between the mutually perpendicular X and Y directions
   has been detected. The modulation transfer function (MTF) can be
   estimated from the correlation data, and the reconstructed MTF is in
   good agreement with measurements. (c) 2006 Elsevier B.V. All rights
   reserved.}},
