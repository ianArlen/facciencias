@article{ ISI:000244652700007,
Author = {Miranda, J. and de Lucio, O. G. and Lugo-Licona, M. F.},
Title = {{X-ray production induced by heavy ion impact: challenges and possible
   uses}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2007}},
Volume = {{53}},
Number = {{3, S}},
Pages = {{29-32}},
Month = {{FEB}},
