@article{ ISI:000243670300017,
Author = {Lima, Enrique and Bosch, Pedro and Bulbulian, Silvia},
Title = {{Immobilization of cobalt in collapsed non-irradiated and
   gamma-irradiated X zeolites}},
Journal = {{APPLIED RADIATION AND ISOTOPES}},
Year = {{2007}},
Volume = {{65}},
Number = {{2}},
Pages = {{259-265}},
Month = {{FEB}},
Abstract = {{Cobalt exchanged X zeolites were gamma irradiated and heated until the
   zeolite structure collapsed. Heating destroys the zeolite network as
   found by X-ray-diffraction and Si-29, Al-27 MAS NMR spectroscopy. Gamma
   irradiation treatment diminished the collapsing temperature of zeolite.
   Cobalt leaching from crystalline and amorphized zeolites was verified by
   ion exchange with NaCl solution. Results show that cobalt is not
   released from the amorphous materials. Furthermore adsorption of xenon
   and Xe-129 NMR spectroscopy reveal that cobalt ions are heterogeneously
   distributed in the non irradiated amorphous materials. Gamma irradiation
   causes the mobility of cobalt in the amorphous materials resulting then
   in a more homogeneous distribution. Cobalt is, thus, retained safely in
   the amorphous materials. (c) 2006 Elsevier Ltd. All rights reserved.}},
