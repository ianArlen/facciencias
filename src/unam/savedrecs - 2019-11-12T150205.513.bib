@article{ ISI:000246895100004,
Author = {Bruner, Carlos A.},
Title = {{The many applications of the only psychology}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2007}},
Volume = {{24}},
Number = {{1}},
Pages = {{23-30}},
Month = {{JUN}},
Abstract = {{The themes of the international mobility of psychologists, the
   undergraduate curriculum in psychology, the accreditation of licensure
   programs in psychology and the certification of professional
   psychologists are examined under three thesis: that there is only one
   psychology, that has an enormous number of applications and that
   specializations do not yet exist in psychology. Its is concluded that a
   broad knowledge of generic psychology coupled with creativity in the
   application of this knowledge should enable psychologists to face the
   challenges imposed by the new educational and professional demands.}},
