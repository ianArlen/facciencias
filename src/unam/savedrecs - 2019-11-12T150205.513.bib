@article{ ISI:000247606900016,
Author = {Suazo-Ortuno, Ireri and Alvarado-Diaz, Javier and Raya-Lemus, Elizabeth
   and Martinez-Ramos, Miguel},
Title = {{Diet of the Mexican marbled toad (Bufo marmoreus) in conserved and
   disturbed tropical dry forest}},
Journal = {{SOUTHWESTERN NATURALIST}},
Year = {{2007}},
Volume = {{52}},
Number = {{2}},
Pages = {{305-309}},
Month = {{JUN}},
Abstract = {{We collected data on diet of the marbled toad (Bufo marmoreus) on
   conserved and disturbed areas of tropical dry forest oil the coast of
   Jalisco, Mexico, during 2000 and 2001. Although the diet of B. marmoreus
   consisted of 19 prey taxa, the moderately low dietary diversity measure
   (I-I' = 1.51) reflected the dominance in the diet of only 3 groups of
   prey: ants, beetles, and termites. Toads in the conserved area consumed
   greater proportions of ants (36.7\% by volume), whereas toads in
   disturbed forest consumed greater proportions of beetles (53.1\% by
   volume). Diet diversity was significantly lower in the disturbed area.
   However, abundance, size, and weight of toads was similar in both areas,
   suggesting that prey availability was not affected by disturbance.}},
