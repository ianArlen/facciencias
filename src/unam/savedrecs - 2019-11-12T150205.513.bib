@article{ ISI:000246746100008,
Author = {Alonso, J. C. and Diaz-Bucio, M. and Ortiz, A. and Benami, A. and
   Cheang-Wong, J. C. and Rodriguez-Fernandez, L.},
Title = {{Fluorine content of SiOF films as determined by IR spectroscopy and
   resonant nuclear reaction analysis}},
