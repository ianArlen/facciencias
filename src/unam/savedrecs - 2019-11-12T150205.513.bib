@article{ ISI:000246100100002,
Author = {Shishkova, Svetlana and Garcia-Mendoza, Edith and Castillo-Diaz, Vicente
   and Moreno, Norma E. and Arellano, Jesus and Dubrovsky, Joseph G.},
Title = {{Regeneration of roots from callus reveals stability of the developmental
   program for determinate root growth in Sonoran Desert Cactaceae}},
Journal = {{PLANT CELL REPORTS}},
Year = {{2007}},
Volume = {{26}},
Number = {{5}},
Pages = {{547-557}},
Month = {{MAY}},
Abstract = {{In some Sonoran Desert Cactaceae the primary root has a determinate root
   growth: the cells of the root apical meristem undergo only a few cell
   division cycles and then differentiate. The determinate growth of
   primary roots in Cactaceae was found in plants cultivated under various
   growth conditions, and could not be reverted by any treatment tested.
   The mechanisms involved in root meristem maintenance and determinate
   root growth in plants remain poorly understood. In this study, we have
   shown that roots regenerated from the callus of two Cactaceae species,
   Stenocereus gummosus and Ferocactus peninsulae, have a determinate
   growth pattern, similar to that of the primary root. To demonstrate
   this, a protocol for root regeneration from callus was established. The
   determinate growth pattern of roots regenerated from callus suggests
   that the program of root development is very stable in these species.
   These findings will permit future analysis of the role of certain
   Cactaceae genes in the determinate pattern of root growth via the
   regeneration of transgenic roots from transformed calli.}},
