@article{ ISI:000246575400013,
Author = {Maycock, Jonathan and Hennelly, Bryan M. and McDonald, John B. and
   Frauel, Yann and Castro, Albertina and Javidi, Bahram and Naughton,
   Thomas J.},
Title = {{Reduction of speckle in digital holography by discrete Fourier filtering}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA A-OPTICS IMAGE SCIENCE AND
   VISION}},
Year = {{2007}},
Volume = {{24}},
Number = {{6}},
Pages = {{1617-1622}},
Month = {{JUN}},
Abstract = {{We present a digital signal processing technique that reduces the
   speckle content in reconstructed digital holograms. The method is based
   on sequential sampling of the discrete Fourier transform of the
   reconstructed image field. Speckle reduction is achieved at the expense
   of a reduced intensity and resolution, but this tradeoff is shown to be
   greatly superior to that imposed by the traditional mean and median
   filtering techniques. In particular, we show that the speckle can be
   reduced by half with no loss of resolution (according to standard
   definitions of both metrics). (C) 2007 Optical Society of America}},
