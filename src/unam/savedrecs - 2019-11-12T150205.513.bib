@article{ ISI:000247250700006,
Author = {Cano, Alfredo and Clapp, Monica},
Title = {{Multiple positive and 2-nodal symmetric solutions of elliptic problems
   with critical nonlinearity}},
Journal = {{JOURNAL OF DIFFERENTIAL EQUATIONS}},
Year = {{2007}},
Volume = {{237}},
Number = {{1}},
Pages = {{133-158}},
Month = {{JUN 1}},
Abstract = {{We consider the problem -Delta u + a(x)u = f (x)vertical bar u vertical
   bar(2{*}-2)u in Omega, u = 0 on partial derivative Omega, where Omega is
   a bounded smooth domain in R-N, N >= 4, 2{*} := (2N)(N-2) is the
   critical Sobolev exponent, and a, f are continuous func- tions. We
   assume that Omega, a and f are invariant under the action of a group of
   orthogonal transformations. We obtain multiplicity results which contain
   information about the symmetry and symmetry-breaking properties of the
   solutions, and about their nodal domains. Our results include new
   multiplicity results for the Brezis-Nirenberg problem -Delta u + lambda
   u = vertical bar u vertical bar(2{*}-2)u in Omega, u = 0 on partial
   derivative Omega. (C) 2007 Elsevier Inc. All rights reserved.}},
