@article{ ISI:000249822900007,
Author = {Flores, Enrique and Gonzalez, Raul Eduardo},
Title = {{Malverde: Thank you's to God and ballads (in the voices of Sinaloan
   singers) (Jesus Malverde)}},
Journal = {{CARAVELLE-CAHIERS DU MONDE HISPANIQUE ET LUSO-BRESILIEN}},
Year = {{2007}},
Number = {{88}},
Pages = {{111-138}},
Month = {{JUN}},
Abstract = {{The cult rendered to bandit Jesus Malverde finds one of its most
   traditional expressions in votive offerings. Inscriptions, veladoras and
   photographs proliferate in his chapel of Culiacan, Sinaloa. An even more
   original form to show gratitude to the santo for the miracles taking
   place consists of offering him music or ordering the composition of
   corridos as a tribute to him during his days. Later on, those songs are
   recorded and sold outside the chapel, along with amulets and sacred
   objects. Some musicians of the region -Malverde devotees themselves have
   composed songs in his honour during years and have in this way given
   support to his promeseros.}},
