@article{ ISI:000247572400011,
Author = {Perez Ruiz, S. J. and Iniesta, S. Alcantara and Hernandez, P. R. and
   Castaneda-Guzman, R.},
Title = {{Sound speed resolved by photoacoustic technique}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2007}},
Volume = {{53}},
Number = {{3}},
Pages = {{213-217}},
Month = {{JUN}},
Abstract = {{In this work a new method for measuring the speed of sound in materials
   is reported. This method uses the photoacoustic effect, which is the
   generation of sound waves by pulsed optical radiation incident on a
   material sample. The sound waves generated on the surface of the sample
   travel through the material and are detected with two piezoelectric
   sensors separated by a known distance. An appropriate processing of the
   photoacoustic signal permits the separation of the information of the
   generated longitudinal waves, of their reflections, as well as of other
   types of waves generated (shear, surface, etc). The advantages and
   disadvantages, of this method are discussed in comparison with standard
   methods.}},
Publisher = {{SOC MEXICANA FISICA}},
Address = {{APARTADO POSTAL 70-348, COYOACAN 04511, MEXICO}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Perez Ruiz, SJ (Reprint Author), Inst Politecn Nacl, Sec Bioelect, Dpto Ingn Elect, Ctr Invest \& Estudios Avanzados, Mexico City 07738, DF, Mexico.
