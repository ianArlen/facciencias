@article{ ISI:000246342200004,
Author = {Hidalgo-Gamez, A. M. and Peimbert, A.},
Title = {{Diffuse ionized gas inside the dwarf irregular galaxy NGC 6822}},
Journal = {{ASTRONOMICAL JOURNAL}},
Year = {{2007}},
Volume = {{133}},
Number = {{5}},
Pages = {{1874-1882}},
Month = {{MAY}},
Abstract = {{We have studied the differences between the diffuse ionized gas ( DIG)
   and the H Pi regions along a slit position in the local dwarf irregular
   galaxy NGC 6822. The slit position passes through the two most prominent
   H Pi regions: Hubble V and Hubble X. Important differences have been
   found in the excitation, ionization, and {[}N Pi] lambda 6584/H alpha
   and {[}S Pi] lambda 6717/H alpha line ratios between the DIG and the H
   Pi locations. Moreover, the values of all the line ratios are not
   similar to those in the DIG locations of spiral galaxies but are very
   similar to the values in other irregular galaxies, such as IC 10. We
   also determined the rate of recombination using the He Iota lambda 5875
   line. Finally, we obtained a picture of the ionization sources of the
   DIG. We consider that the leakage of photons from the H Pi regions might
   explain most of the line ratios, except {[}N Pi]/H alpha, which might be
   explained by turbulence.}},
