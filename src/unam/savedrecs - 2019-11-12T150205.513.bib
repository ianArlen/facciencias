@article{ ISI:000247015800030,
Author = {Perez-Esteva, Salvador and Rivera-Noriega, Jorge},
Title = {{Vector-valued Hardy spaces in non-smooth domains}},
Journal = {{JOURNAL OF MATHEMATICAL ANALYSIS AND APPLICATIONS}},
Year = {{2007}},
Volume = {{330}},
Number = {{1}},
Pages = {{388-405}},
Month = {{JUN 1}},
Abstract = {{We characterize the Radon-Nikodym property of a Banach space X in terms
   of the existence of nontangential limits of X-valued harmonic functions
   u defined in a domain D subset of R-n, n > 2, with Lipschitz boundary
   and belonging to maximal Hardy spaces. This extends the same result
   previously known for the unit disk of C. We also prove an atomic
