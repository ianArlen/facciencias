@article{ ISI:000246417900002,
Author = {de Diego, J. A. and Binette, L. and Ogle, P. and Andersen, A. C. and
   Haro-Corzo, S. and Wold, M.},
Title = {{Quasar 3C 298: a test-case for meteoritic nanodiamond 3.5 mu m emission}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{2007}},
Volume = {{467}},
Number = {{1}},
Pages = {{L7-L10}},
Month = {{MAY}},
Abstract = {{Aims. We calculate the dust emission expected at 3.43 and 3.53 mu m if
   meteoritic (i.e. hydrogenated) nanodiamonds are responsible for most of
   the far-UV break observed in quasars.
   Methods. We integrate the UV flux that hydrogenated nanodiamonds must
   absorb to reproduce the far-UV break. Based on laboratory spectra of
   H-terminated diamond surfaces, we analyse the radiative energy budget
   and derive theoretically the IR emission profiles expected for possible
   C-H surface stretch modes of the diamonds.
   Results. Using as test case a spectrum of 3C 298 provided by the Spitzer
   Observatory, we do not find evidence of these emission bands.
   Conclusions. While diamonds without surface adsorbates remain a viable
   candidate for explaining the far-UV break observed in quasars,
   hydrogenated nanodiamonds appear to be ruled out, as they would give
   rise to IR emission bands, which have not been observed so far.}},
