@article{ ISI:000245967700010,
Author = {Alvarez, M. A. and Seyler, D. and Madrigal-Carballo, S. and Vila, A. O.
   and Molina, F.},
Title = {{Influence of the electrical interface properties on the rheological
   behavior of sonicated soy lecithin dispersions}},
