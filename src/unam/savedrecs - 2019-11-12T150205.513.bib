@article{ ISI:000247413300009,
Author = {De la Macorra, Axel and Melchiorri, Alessandro and Serra, Paolo and
   Bean, Rachel},
Title = {{The impact of neutrino masses on the determination of dark energy
   properties}},
Journal = {{ASTROPARTICLE PHYSICS}},
Year = {{2007}},
Volume = {{27}},
Number = {{5}},
Pages = {{406-410}},
Month = {{JUN}},
Abstract = {{Recently, the Heidelberg- Moscow double beta decay experiment has
   claimed a detection for a neutrino mass with high significance. Here we
   consider the impact of this measurement on the determination of the dark
   energy equation of state. By combining the Heidelberg-Moscow result with
   the WMAP 3-years data and other cosmological datasets we constrain the
   equation of state to -1.67 < w < -1.05 at 95\% c.l. Interestingly
   enough, coupled neutrino-dark energy models may be consistent with such
   equation of state. While future data are certainly needed for a
   confirmation of the controversial Heildelberg-Moscow claim, our result
   shows that future laboratory searches for neutrino masses may play a
   crucial role in the determination of the dark energy properties. (c)
   2007 Elsevier B.V. All rights reserved.}},
