@article{ ISI:000245537900019,
Author = {Martinez-Alvarez, Laura Edith and Hernandez-Ceron, Joel and
   Gonzalez-Padilla, Everardo and Perera-Marin, Gerardo and Valencia,
   Javier},
Title = {{Serum LH peak and ovulation following synchronized estrus in goats}},
Journal = {{SMALL RUMINANT RESEARCH}},
