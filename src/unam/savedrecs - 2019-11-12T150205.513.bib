@article{ ISI:000246842500007,
Author = {Salinas, Yeritzia and Oliart, Rosa Maria and Ramirez-Lepe, Mario and
   Navarro-Ocana, Arturo and Valerio-Alfaro, Gerardo},
Title = {{Synthesis of chiral alpha-hydroxy amides by two sequential enzymatic
   catalyzed reactions}},
Journal = {{APPLIED MICROBIOLOGY AND BIOTECHNOLOGY}},
Year = {{2007}},
Volume = {{75}},
Number = {{2}},
Pages = {{297-302}},
Month = {{MAY}},
Abstract = {{Enantiomerically pure Lx-hydroxy amides have been prepared from the
   corresponding Lx-oxo esters by the use of a double sequence reaction
   involving in a first step the highly enantioselective Saccharomyces
   cerevisiae bioreduction and then in a second step, the resulting
   cx-hydroxy esters followed a non-enantiospecific lipase catalyzed
   aminolysis with n-butylamine reaction. In the first non-organic solvent
   process, the moistened baker's yeast reduced seven a-oxo esters with
   high conversions degree (93\% for one substrate and >99\% for the
   others) and high enantioselectivities {[}>99\% for all the substrates
   except for ketopantoyl lactone, which gave 88\% of enantiomeric excess
   (ee)]. At the same way, the isolated resulting chiral oc-hydroxy esters
   were subjected to the second Candida antarctica lipase fraction B
   (CAL-B) catalyzed aminolysis in dioxane conducting to the corresponding
   chiral Lx-hydroxy amides with high conversions degree, between 88 and
   99\%. Both processes were carried out at 28-30 degrees C.}},
