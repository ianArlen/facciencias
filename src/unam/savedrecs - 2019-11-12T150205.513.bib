@article{ ISI:000246895100005,
Author = {Oviedo-Gomez, Maria Teresa and Reidl-Martinez, Lucy Maria},
Title = {{Individual predictors of the quality of life in type 2 diabetes}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2007}},
Volume = {{24}},
Number = {{1}},
Pages = {{31-42}},
Month = {{JUN}},
Abstract = {{The quality of life (QOL) in patients with type 2 diabetes (T2D) is
   affected by its chronic condition. QOL perception is different between
   patients because it involves subjective and objective components related
   to psychological, physical, and social factors. There are no studies
   that evaluate co-relationships among psychological variables, clinical
   indicators of metabolic control, or self-care activities inherent to
   this disease; moreover, nothing is known about the predictive value of
   QOL for Mexican patients. In the present study we examined 263 patients
   with T2D diagnosis at different health services in Mexico City. Their
   perception of QOL is regular and we found that only psychological
   variables such as self-efficacy, external locus of control, evasive
   coping style and wellness index, are positive predictors of QOL, whereas
   depressive coping is a negative predictor. To improve the perception of
   QOL, we propose to strengthen positive treatment goal predictors and to
   modify those that predict negative effects.}},
