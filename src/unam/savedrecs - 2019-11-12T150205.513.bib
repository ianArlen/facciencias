@article{ ISI:000247045200003,
Author = {Rosas Almeida, David I. and Alvarez, Joaquin and Fridman, Leonid},
Title = {{Robust observation and identification of nDOF Lagrangian systems}},
Journal = {{INTERNATIONAL JOURNAL OF ROBUST AND NONLINEAR CONTROL}},
Year = {{2007}},
Volume = {{17}},
Number = {{9}},
Pages = {{842-861}},
Month = {{JUN}},
Abstract = {{A procedure to design a global exponentially stable, second-order,
   sliding-mode observer for nDOF Lagrangian systems is presented. The
   observer converges to the system state in spite of the existence of
   bounded disturbances or parameter uncertainties affecting the system
   dynamics. The generation of sliding modes permits the identification of
   disturbances using the equivalent output injection which, under some
   circumstances, can also be used to identify the system parameters via a
   continuous version of the last-square method. The proposed methodology
   is illustrated with some numerical examples and experiments. Copyright
   (C) 2006 John Wiley \& Sons, Ltd.}},
