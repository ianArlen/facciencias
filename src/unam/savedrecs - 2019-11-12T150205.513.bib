@article{ ISI:000246895100008,
Author = {Sanchez-Castillo, Hugo and Chavez, Alejandro and Miranda, Florencio and
   Velazquez-Martinez, David N.},
Title = {{Effect of D-amphetamine on a retrospective timing task}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2007}},
Volume = {{24}},
Number = {{1}},
Pages = {{65-75}},
Month = {{JUN}},
Abstract = {{The neurophysiological interpretation of the scalar-expectancy theory
   (SET) of timing holds that dopamine regulates the speed of a theorized
   internal clock. Some studies support this account but there is mounting
   contrary evidence. The dopamine theory predicts leftward displacements
   of psychometric functions with the administration of dopamine agonists
   but rightward shifts after dopamine antagonists, excluding changes on
   the slope of psychometric functions. Our aim was to determine the
   effects of d-amphetamine (0.1-1.0 mg/kg) and haloperidol (0.03-0.3
   mg/kg) on the timing performance of rats trained in a temporal bisection
   procedure. Amphetamine and haloperidol both produced a non-significant
   leftward shift in the bisection point. The Weber fraction in the
   temporal bisection task was unaltered by amphetamine, while haloperidol
   produced an inverted U dose-effect. The present study's findings of lack
   of statistical effects and absence of a dose effect cast doubts on the
   suggestion that dopamine modulates the speed of the theorized internal
   clock; instead, modulation of attentional processes may explain these
   effects of dopamine agonists and antagonists.}},
