@article{ ISI:000246571300034,
Author = {Franco, Jose and Garcia-Segura, Guillermo and Kurtz, Stanley E. and
   Arthur, S. Jane},
Title = {{From ultracompact to extended HII regions. II. Cloud gravity and stellar
   motion}},
