@article{ ISI:000467773700005,
Author = {Ramirez Rancano, Mario},
Title = {{Victoriano Huerta's bitter exile and his followers in Spain: 1914-1920}},
Journal = {{ESTUDIOS DE HISTORIA MODERNA Y CONTEMPORANEA DE MEXICO}},
Year = {{2018}},
Volume = {{55}},
Pages = {{159-194}},
Month = {{JAN-JUN}},
Abstract = {{Once Victoriano Huerta resigned as President of Mexico, a large portion
   of politicians and military personnel took refuge in the United States,
   Cuba, and other countries. Huerta was exiled in Spain, as did other
   members of his cabinet. Since Spain was one of the countries whose
   government recognized Huerta, the newcomers assumed that they would
   receive special treatment, but that was not the case. Almost
   simultaneously, the Grat War began, which made the exiles' situation
   even worse. Finaly, almost every one of them returned to the Americas,
   specially to the United States.}},
