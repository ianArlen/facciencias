@article{ ISI:000449763400050,
Author = {Kumar, Santosh and Kumar, M. Vinay and Pattabi, Manjunatha and Asokan,
   K. and Xavier and Nini and Martin and Chandrashekar, B. N. and Chun,
   Cheng and Krishnaveni, S.},
Title = {{Effect of Gamma Irradiation on Electrical Properties of CdTe/CdS Solar
   Cells}},
Journal = {{MATERIALS TODAY-PROCEEDINGS}},
Year = {{2018}},
Volume = {{5}},
Number = {{10, 3}},
Pages = {{22570-22575}},
Note = {{International Conferance on Advances in Science and Enginerring (ICASE),
   Bangkok, THAILAND, JAN 19-22, 2017}},
Abstract = {{The effect of Co-60 Gamma irradiation on electrical characteristics of
   CdTe/CdS solar cell has been analyzed using in-situ current-voltage
   characterization (I-V) in dark condition. The irradiation was done over
   a wide range of doses from 1kGy to 100 kGy. Electrical parameters such
   as ideality factor (n), series resistance (R-S) and reverse bias leakage
   current (I-R) for each dose have been calculated from the I-V
   characteristics. The ideality factor of the pristine solar cell is found
   to be 1.80 and it gradually increased up to 3.38 for the dose of 10 kGy,
   then it is around 3.38-3.61 for higher doses up to 100 kGy. The I-V
   characteristics showed significant increase in forward bias and drastic
   increase in reverse leakage current. The value of I-R is 16.8 mu A for
   pristine solar cell and it increases to 1.46 mA for 10 kGy. Further
   there is four times observable change in the value of I-R. However the
   value of R-s is 0.80 ohm for pristine and continuously decreases to 0.34
   ohm for 50 kGy dose. Further there is slight increase in the series
   resistance to 0.38 ohm for 100 kGy dose. (C) 2018 Elsevier Ltd. All
   rights reserved.}},
