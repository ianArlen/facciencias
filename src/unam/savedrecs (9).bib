Title = {{EFFECTIVENESS TESTING OF AN INVERSE METHOD FOR BALANCING NONLINEAR
   ROTORDYNAMIC SYSTEMS}},
Booktitle = {{PROCEEDINGS OF THE ASME TURBO EXPO: TURBOMACHINERY TECHNICAL CONFERENCE
   AND EXPOSITION, 2018, VOL 7A}},
Year = {{2018}},
