@article{ ISI:000419992700013,
Author = {Zarazua-Villalobos, L. and Tellez-Jurado, L. and Vargas-Becerril, N. and
   Fantozzi, G. and Balmori-Ramirez, H.},
Title = {{Synthesis of magnesium aluminate spinel nanopowder by sol-gel and
   low-temperature processing}},
