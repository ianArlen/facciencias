@article{ ISI:000458490700003,
Author = {Zamora, J. and Betancourt, I and Jimenez, A. and Perez del Real, R. and
   Vazquez, M.},
Title = {{Correlation of Microstructure with Hard Magnetic Properties of
   Glass-Coated MnBi Microwires}},
Journal = {{MRS ADVANCES}},
Year = {{2018}},
Volume = {{3}},
Number = {{62}},
Pages = {{3635-3640}},
Abstract = {{In this work we study the hard-magnetic properties and its influence on
   the microstructure of MnBi-based glass-coated microwires obtained by
   Taylor-Ulitovsky process, as well as their magnetic and thermal
   dependence in the range of 200 K-360 K We obtained glass-coated
   microwires pieces trough of the Taylor-Ulitovsky process. Glass-coated
   microwires exhibited the formation of Low-Temperature Intermetallic
   Phase (LTIP)-MnBi, as confirmed by scanning electron microscopy (SEM).
   Energy Dispersive Spectroscopy (EDS) elemental chemical composition
   analysis showed LTIP regions interspersed within Bi- and Mn-rich areas.
   Magnetic properties were determined by vibrating sample magnetometry
   (VSM), for which a considerable intrinsic coercivity field (H-1(c)) up
   to 6000 Oe at 360 K, together with a saturation magnetization (M-g) of
   57.49 emu/cm(3). Therefore, this combination of properties renders LTIP
   is a promising precursor with potential for applications at high
   temperatures.}},
