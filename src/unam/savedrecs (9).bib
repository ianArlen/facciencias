@inproceedings{ ISI:000455614900227,
Author = {Campos-Roca, Yolanda and Calle-Alonso, Fernando and Perez, Carlos J. and
   Naranjo, Lizbeth},
Book-Group-Author = {{IEEE}},
Title = {{Computational diagnosis of Parkinson's Disease from speech based on
   regularization methods}},
Booktitle = {{2018 26TH EUROPEAN SIGNAL PROCESSING CONFERENCE (EUSIPCO)}},
Series = {{European Signal Processing Conference}},
Year = {{2018}},
Pages = {{1127-1131}},
Note = {{European Signal Processing Conference (EUSIPCO), Rome, ITALY, SEP 03-07,
   2018}},
Organization = {{European Assoc Signal Processing; IEEE Signal Processing Soc; ROMA TRE
   Univ Degli Studi; MathWorks; Amazon Devices}},
Abstract = {{A computational tool to discriminate healthy people from people with
   Parkinson's Disease (PD) is proposed based on acoustic features
   extracted from sustained vowel recordings. Several approaches based on
   different feature sets and regularization methods (LASSO, Ridge, and
   Elastic net) are experimentally compared. The effectiveness of these
   methods has been evaluated on a dataset containing acoustic features of
   40 healthy people and 40 patients with PD, who have been recruited at
   the Regional Association for Parkinson's Disease in Extremadura (Spain).
