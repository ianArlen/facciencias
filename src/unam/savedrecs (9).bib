@article{ ISI:000459602200008,
Author = {Reyes Garcia, Jorge Luis and Morales Castro, Arturo},
Title = {{VALUE AT RISK USING SMOOTHING TECHNIQUES: A PROPOSAL IN THE FOREIGN
   EXCHANGE MARKET}},
Journal = {{DIMENSION EMPRESARIAL}},
Year = {{2018}},
Volume = {{16}},
Number = {{2}},
Pages = {{99-110}},
Abstract = {{One of the main problems that are exposed companies and financial
   institutions in Mexico is the volatility in the exchange rate
   Peso/Dollar when operating or in the valuation of financial assets. This
   article analyzes and compares the behavior of value at risk {[}VAR]
   under three different methodologies: historical simulation, Monte Carlo
   Simulation and straightening. An application to the exchange rate in
