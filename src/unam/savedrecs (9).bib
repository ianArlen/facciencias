@inproceedings{ ISI:000465195200016,
Author = {Ballesteros, M. and Crawford, N. and Fraas, M. and Frohlich, J. and
   Schubnel, B.},
Editor = {{Bonetto, F and Borthwick, D and Harrell, E and Loss, M}},
Title = {{Non-demolition measurements of observables with general spectra}},
Booktitle = {{MATHEMATICAL PROBLEMS IN QUANTUM PHYSICS}},
Series = {{Contemporary Mathematics}},
Year = {{2018}},
Volume = {{717}},
Pages = {{241-256}},
Note = {{13th Conference on Mathematical Results in Quantum Physics (QMATH),
   Georgia Inst Technol, Atlanta, GA, OCT 08-11, 2016}},
Abstract = {{It has recently been established that, in a non-demolition measurement
   of an observable Ar with a finite point spectrum, the density matrix of
   the system approaches an eigenstate of Ar, i.e., it ``purifies{''} over
   the spectrum of N. We extend this result to observables with general
   spectra. It is shown that the spectral density of the state of the
   system converges to a delta function exponentially fast, in an
   appropriate sense. Furthermore, for observables with absolutely
   continuous spectra, we show that the spectral density approaches a
