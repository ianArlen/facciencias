@article{ ISI:000448596700004,
Author = {Yilmaz, Ajda and Kalsbeek, Andries and Buijs, Ruud M.},
Title = {{Functional changes of the SCN in spontaneous hypertension but not after
   the induction of hypertension}},
Journal = {{CHRONOBIOLOGY INTERNATIONAL}},
Year = {{2018}},
