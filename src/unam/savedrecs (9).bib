   and Anselmo Mora-Gutierrez, Roman and Ponsich, Antonin and Prieto
   Rodriguez, Scarlet and Camargo Marin, Lisbeth and Guzman Huerta, Mario},
Editor = {{Romero, E and Lepore, N and Brieva, J}},
Title = {{Classification of Abdominal ECG Recordings for the Identification of
   Fetal Risk Using Random Forest and Optimal Feature Selection}},
Booktitle = {{14TH INTERNATIONAL SYMPOSIUM ON MEDICAL INFORMATION PROCESSING AND
   ANALYSIS}},
Series = {{Proceedings of SPIE}},
