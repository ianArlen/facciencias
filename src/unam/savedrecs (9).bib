@inproceedings{ ISI:000452663000044,
Author = {Carbajal-Degante, Erik and Olveres, Jimena and Escalante-Ramirez, Boris},
Editor = {{Schelkens, P and Ebrahimi, T and Cristobal, G}},
Title = {{A Multiphase Active Contour Model based on the Hermite Transform for
   Texture Segmentation}},
Booktitle = {{OPTICS, PHOTONICS, AND DIGITAL TECHNOLOGIES FOR IMAGING APPLICATIONS V}},
