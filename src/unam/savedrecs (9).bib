@inproceedings{ ISI:000457070900059,
Author = {Al-Ghazal, Ghaith Ghanim and Bonello, Philip and Torres Cedillo, Sergio
   G.},
Book-Group-Author = {{ASME}},
Title = {{Improved Empirical Identification of the Inverse Model of a Squeeze-Film
   Damper Bearing based on a Recurrent Neural Network}},
Booktitle = {{PROCEEDINGS OF THE ASME TURBO EXPO: TURBOMACHINERY TECHNICAL CONFERENCE
   AND EXPOSITION, 2018, VOL 7A}},
