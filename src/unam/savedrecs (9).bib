@article{ ISI:000461286200008,
Author = {Valdivies, Marcela and Peniche, Dubon},
Title = {{Relation between Body Weight and Depression}},
Journal = {{INVESTIGACION CLINICA}},
Year = {{2018}},
Volume = {{59}},
Number = {{4}},
Pages = {{329-331}},
Abstract = {{Cross- sectional study conducted from beginning of December 2015 till
   end of January 2017. Study population included adult of age more than 18
   years. Data were collected using structured interviewer filled
   questionnaire. Anthropometric measurements (height, weight, and Waist
   circumference were taken for all participants. The objective of this
   paper is to investigate the relation between common mental disorders
   (depression, anxiety and stress) and body weight among Zulia females
   attending primary health care centers in, Eastern province, Zulia. The
   results indicate that in community sample of adult females, regardless
   socioeconomic status, reports of depressive, anxiety and stress symptoms
   are not significantly higher in obese than nounal-weight groups, again
   adding to the dilemma of inconsistent relationship between body weight
   and common mental disorders. This may be explained by racial, cultural
   and social differences among different studies.}},
