Title = {{Investigation on Physical Properties of Pb(Zr0.52Ti0.48)O-3 Doped BiFeO3
   Multiferroics Materials}},
Booktitle = {{NATIONAL CONFERENCE ON ADVANCED MATERIALS AND NANOTECHNOLOGY (AMN-2018)}},
Series = {{AIP Conference Proceedings}},
Year = {{2018}},
