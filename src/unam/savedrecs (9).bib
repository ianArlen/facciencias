Author = {Martinez-Aguilar, E. and Mok, H'Linh-H' and Moreno-Armenta, M. G. and
   Siqueiros, J. M.},
Title = {{Tailoring the properties of BiFeO3 for photovoltaic applications through
   first-principles calculations}},
Journal = {{FERROELECTRICS}},
Year = {{2018}},
