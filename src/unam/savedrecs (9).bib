@incollection{ ISI:000452108900008,
Author = {de los Heros, Paola and Pacheco-Alvarez, Diana and Gamba, Gerardo},
Editor = {{Levitane, I and Delpire, E and RasgadoFlores, H}},
Title = {{Role of WNK Kinases in the Modulation of Cell Volume}},
