@inproceedings{ ISI:000455780300023,
Author = {Margarito, Daniela and Gonzalez, Erika and Buendia, Ivette and Selman,
   Moises and Hautefeuille, Mathieu},
Editor = {{He, S and Lee, EH}},
Title = {{Development of a wearable for oximetry and patient physical activity
   correlation}},
Booktitle = {{SMART PHOTONIC AND OPTOELECTRONIC INTEGRATED CIRCUITS XX}},
Series = {{Proceedings of SPIE}},
Year = {{2018}},
Volume = {{10536}},
Note = {{Conference on Smart Photonic and Optoelectronic Integrated Circuits XX,
   San Francisco, CA, JAN 29-FEB 01, 2018}},
Organization = {{SPIE}},
Abstract = {{In this work, we show preliminary advance in the development of a
   low-cost wrist wearable that measures levels of oxygen saturation levels
   in blood (SpO(2)) and physical activity simultaneously. For the
   monitoring of SpO(2), we use pulse oximetry (a non invasive technique)
   by backscattering detection at the wrist. The device consists in an
