@inproceedings{ ISI:000457516200006,
Author = {Sanchez, Salvador and Ascanio, Gabriel and Aguayo, Juan P. and
   Sanchez-Minero, Felipe},
Book-Group-Author = {{ASME}},
Title = {{NUMERICAL ANALYSIS OF THERMAL EFFECTS INDUCED IN THE HYDRODYNAMICS OF
   THE HEAVY OIL TRANSPORT}},
Booktitle = {{PROCEEDINGS OF THE ASME FLUIDS ENGINEERING DIVISION SUMMER MEETING,
   2018, VOL 2}},
