@inproceedings{ ISI:000347614600016,
Author = {Giselle Murillo-Garcia, Franny and Alcantara-Ayala, Irasema},
Editor = {{Wu, W}},
Title = {{Landslide Susceptibility Analysis and Mapping Using Statistical
   Multivariate Techniques: Pahuatlan, Puebla, Mexico}},
