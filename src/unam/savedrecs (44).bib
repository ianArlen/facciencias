@article{ ISI:000357805500095,
Author = {Hernandez-Juarez, M. and Toscano, R. A. and Cortes-Guzman, F. and
   Zuniga-Villarreal, N.},
Title = {{Hydrogen and oxygen activation by an iridium precursor containing the
   4,5-bis(diphenylthiophosphinoyl)-1,2,3-triazolate ligand}},
Journal = {{RSC ADVANCES}},
Year = {{2015}},
Volume = {{5}},
Number = {{71}},
Pages = {{58046-58054}},
Abstract = {{The novel mononuclear complex {[}Ir(kappa(2)-S,
   N-(4,5-(P(S)Ph-2)(2)Tz))(coe)(2)] was prepared starting from the dimer
   {[}Ir(coe)(2)(mu-Cl)](2) (coe = cyclooctene) and potassium
   4,5-bis(diphenylthiophosphinoyl)-1,2,3-triazolate, K
   {[}4,5-(P(S)Ph-2)(2)Tz]. Reaction of {[}Ir(kappa(2)-S,
   N-(4,5-(P(S)Ph-2)(2)Tz))(coe)(2)] with two equivalents of PPh3 led to
   complex {[}Ir(kappa(2)-S, N-(4,5-(P(S)Ph-2)(2)Tz))(PPh3)(2)]; which,
   upon exposure to atmospheric air at room temperature, fixed molecular
   oxygen to produce complex {[}Ir(kappa(2)-S,
   N-(4,5-(P(S)Ph-2)(2)Tz))(PPh3)(2)O-2]. Molecular hydrogen also added
   oxidatively to complex {[}Ir(kappa(2)-S,
   N-(4,5-(P(S)Ph-2)(2)Tz))(PPh3)(2)] to form the Ir(III) dihydride complex
   {[}Ir(kappa(2)-S, N-(4,5-(P(S)Ph-2)(2)Tz))(PPh3)(2)(H)(2)].
   Carbon-hydrogen bond activation was effected when complex
   {[}Ir(kappa(2)-S, N-(4,5-(P(S)Ph-2)(2)Tz))(coe)(2)] was made to react
   with triphenylphosphite to give the ortho-metallated complex
   {[}Ir(kappa(2)-S,
   N-(4,5-(P(S)Ph-2)(2)Tz))(P(OC6H4)(OPh)(2))(P(P(OPh)(3))(H)]. The new
   compounds were characterized by spectroscopic methods and, excepting for
   {[}Ir(kappa(2)-S, N-(4,5-(P(S)Ph-2)(2)Tz))(PPh3)(2)], by single crystal
   X-ray analysis. Oxygen and hydrogen activation by the diphosphane
   complex {[}Ir(kappa(2)-S, N-(4,5-(P(S)Ph-2)(2)Tz))(PPh3)(2)] compared
   with the inertness of the diolefin complex {[}Ir(kappa S-2,
   N-(4,5-(P(S)Ph-2)(2)Tz))(coe)(2)] to such processes was assessed by
   means of a DFT-QTAIM analysis.}},
