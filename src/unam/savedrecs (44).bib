@article{ ISI:000374522300014,
Author = {Gomez-Espinosa, Catalina and Colin-Garcia, Maria and Negron-Mendoza,
   Alicia},
Title = {{The origin of life from a paleontological perspective, a review}},
Journal = {{BOLETIN DE LA SOCIEDAD GEOLOGICA MEXICANA}},
Year = {{2015}},
Volume = {{67}},
Number = {{3}},
Pages = {{413-420}},
Abstract = {{Although the origin of life cannot be dated with precision, life must
   have appeared soon after the cooling of the Earth, when the existence of
   liquid water on the planet enabled primitive oceans to exist. In fact,
   fossil records support the evidence of life on Earth earlier than 3400
   Ma ago. In order to understand the origin of life, it is useful to track
   geochemical factors such as the presence of carbon and isotopic
   evidence, which also suggests the presence of microbials. Eoarchean
   crustal rocks are located on Akilia Island and on the Isua Greenstone
   belt southwest of Greenland. The oldest recognized microfossil record is
   3430 Ma old from Strelley Pool Formation cherts, Pilbara, Australia. In
   Paleontology, it is necessary to reevaluate the outcrops of the oldest
   rock in light of new technologies, new techniques, and a
   multidisciplinary approach. This will help support data about when life
   emerged.}},
