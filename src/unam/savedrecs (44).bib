@article{ ISI:000342530900029,
Author = {Velasco Herrera, V. M. and Mendoza, B. and Velasco Herrera, G.},
Title = {{Reconstruction and prediction of the total solar irradiance: From the
   Medieval Warm Period to the 21st century}},
Journal = {{NEW ASTRONOMY}},
