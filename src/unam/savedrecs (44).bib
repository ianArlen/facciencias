@article{ ISI:000347298100001,
Author = {Morales-Sanchez, Daniela and Martinez-Rodriguez, Oscar A. and Kyndt,
   John and Martinez, Alfredo},
Title = {{Heterotrophic growth of microalgae: metabolic aspects}},
Journal = {{WORLD JOURNAL OF MICROBIOLOGY \& BIOTECHNOLOGY}},
Year = {{2015}},
Volume = {{31}},
Number = {{1}},
Pages = {{1-9}},
Month = {{JAN}},
Abstract = {{Microalgae are considered photoautotrophic organisms, however several
   species have been found living in environments where autotrophic
   metabolism is not viable. Heterotrophic cultivation, i.e. cell growth
   and propagation with the use of an external carbon source under dark
   conditions, can be used to study the metabolic aspects of microalgae
   that are not strictly related to photoautotrophic growth and to obtain
   high value products. This manuscript reviews studies related to the
   metabolic aspects of heterotrophic grow of microalga. From the
   physiological and metabolic perspective, the screening of microalgal
   strains in different environments and the development of molecular and
   metabolic engineering tools, will lead to an increase in the number of
   known microalgae species that growth under strict heterotrophic
   conditions and the variety of carbon sources used by these
   microorganisms.}},
