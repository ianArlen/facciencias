@article{ ISI:000214333700005,
Author = {Mora, Manuel and Gomez, Jorge Marx and O'Connor, Rory and Raisinghani,
   Mahesh and Gelman, Ovsei},
Title = {{An Extensive Review of IT Service Design in Seven International ITSM
   Processes Frameworks: Part II}},
