@inproceedings{ ISI:000456184200037,
Author = {Marinelli, Antonio and Fraija, Nissim},
Editor = {{Massaro, F and Cheung, CC and Lopez, ED and Siemiginowska, A}},
Title = {{Very high energy neutrino expectation from Fanaroff-Riley I sources}},
Booktitle = {{EXTRAGALACTIC JETS FROM EVERY ANGLE}},
Series = {{IAU Symposium Proceedings Series}},
Year = {{2015}},
Volume = {{10}},
Number = {{313}},
Pages = {{169-174}},
