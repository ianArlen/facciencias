@article{ ISI:000348554500009,
Author = {Aurelio Ramirez-Argaez, Marco and Conejo, Alberto N. and Sofia
   Lopez-Cornejo, Monserrat},
Title = {{Mathematical Modeling of the Melting Rate of Metallic Particles in the
   EAF under Multiphase Flow}},
Journal = {{ISIJ INTERNATIONAL}},
