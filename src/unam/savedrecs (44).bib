@article{ ISI:000364024100008,
Author = {Munoz-Rugeles, L. and Alvarez-Idaboy, J. R.},
Title = {{A proton-electron sequential transfer mechanism: theoretical evidence
   about its biological relevance}},
Journal = {{PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
Year = {{2015}},
Volume = {{17}},
Number = {{43}},
Pages = {{28525-28528}},
Abstract = {{Density functional theory calculations, using the SMD continuum model,
   indicate that hydrogen transfer from totally protonated uric to a
   tryptophanyl radical in proteins corresponds to a sequential mechanism.
   Modeling in methyl butanoate indicates that this mechanism is more
   important in a hydrophobic medium than in water.}},
