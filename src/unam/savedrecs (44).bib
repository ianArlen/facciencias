@article{ ISI:000347462300001,
Author = {Raga, A. C. and Reipurth, B. and Castellanos-Ramirez, A. and Chiang,
   Hsin-Fang and Bally, J.},
Title = {{COLLISIONALLY EXCITED FILAMENTS IN HUBBLE SPACE TELESCOPE H alpha AND H
   beta IMAGES OF HH 1/2}},
Journal = {{ASTROPHYSICAL JOURNAL LETTERS}},
Year = {{2015}},
Volume = {{798}},
Number = {{1}},
Month = {{JAN 1}},
Abstract = {{We present new H alpha and H beta images of the HH 1/2 system, and we
   find that the H alpha/H beta ratio has high values in ridges along the
   leading edges of the HH 1 bow shock and of the brighter condensations of
   HH 2. These ridges have H alpha/H beta = 4 -> 6, which is consistent
   with collisional excitation from the n = 1 to the n = 3 and 4 levels of
   hydrogen in a gas of temperatures T = 1.5 -> 10 x 10(4) K. This is
   therefore the first direct evidence that the collisional
   excitation/ionization region of hydrogen just behind Herbig-Haro shock
   fronts is detected.}},
