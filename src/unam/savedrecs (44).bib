@article{ ISI:000347523800033,
Author = {Bernardino-Nicanor, Aurea and Noemi Hinojosa-Hernandez, Emma and
   Simitrio Juarez-Goiz, Jose Mayolo and Luis Montanez-Soto, Jose and
   Eugenia Ramirez-Ortiz, Maria and Gonzalez-Cruz, Leopoldo},
Title = {{Quality of Opuntia robusta and its use in development of mayonnaise-like
   product}},
