@article{ ISI:000346478000002,
Author = {Chans, Guillermo M. and Gomez, Elizabeth and Gomez-Vidales, Virginia and
   Alfredo Toscano, R. and Alvarez-Toledano, Cecilio},
Title = {{Synthesis, molecular structure, and spectral analysis of copper (II)
   complexes derived from pyridinediols}},
Journal = {{JOURNAL OF COORDINATION CHEMISTRY}},
Year = {{2015}},
Volume = {{68}},
Number = {{2}},
Pages = {{206-219}},
Abstract = {{A series of mononuclear copper(II) complexes was synthesized by reaction
   of different 2,6-disubstituted pyridines with elemental Cu in a
   CCl4/DMSO solvent system. Physical properties were analyzed using IR,
   optical spectroscopy, mass spectrometry, and EPR. Single-crystal X-ray
   diffraction for complexes 2a and 4a revealed that the molecular
   structure of 2a is composed of a six-coordinate unit in which two
   ligands are linked to Cu(II) by oxygen and nitrogen donors. Conversely,
   in the molecular structure of 4a, the Cu(II) has a slightly distorted
   trigonal bipyramidal arrangement, where the coordination environment
   around the copper ion is five-coordinate. EPR spectra of Cu(II)
   complexes were illustrated elaborately and some theoretical data were
   abstracted from EPR curves to support the proposed structures.}},
