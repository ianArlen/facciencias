@article{ ISI:000351275300001,
Author = {Esquivel-Velazquez, Marcela and Ostoa-Saloma, Pedro and Isabel
   Palacios-Arreola, Margarita and Nava-Castro, Karen E. and Ivonne Castro,
   Julieta and Morales-Montor, Jorge},
Title = {{The Role of Cytokines in Breast Cancer Development and Progression}},
Journal = {{JOURNAL OF INTERFERON AND CYTOKINE RESEARCH}},
Year = {{2015}},
Volume = {{35}},
Number = {{1}},
Pages = {{1-16}},
Month = {{JAN 1}},
Abstract = {{Cytokines are highly inducible, secretory proteins that mediate
   intercellular communication in the immune system. They are grouped into
   several protein families that are referred to as tumor necrosis factors,
   interleukins, interferons, and colony-stimulating factors. In recent
   years, it has become clear that some of these proteins as well as their
   receptors are produced in the organisms under physiological and
   pathological conditions. The exact initiation process of breast cancer
   is unknown, although several hypotheses have emerged. Inflammation has
   been proposed as an important player in tumor initiation, promotion,
   angiogenesis, and metastasis, all phenomena in which cytokines are
   prominent players. The data here suggest that cytokines play an
   important role in the regulation of both induction and protection in
   breast cancer. This knowledge could be fundamental for the proposal of
   new therapeutic approaches to particularly breast cancer and other
   cancer-related disorders.}},
Publisher = {{MARY ANN LIEBERT, INC}},
Address = {{140 HUGUENOT STREET, 3RD FL, NEW ROCHELLE, NY 10801 USA}},
