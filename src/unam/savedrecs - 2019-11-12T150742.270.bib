@article{ ISI:000165515100008,
Author = {Garcia-Jimenez, F and Khramov, MI and Sanchez-Obregon, R and Collera, O},
Title = {{Formation of J-aggregates of cyanine dyes in bilayer lipid vesicles}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{2000}},
Volume = {{331}},
Number = {{1}},
Pages = {{42-46}},
Month = {{NOV 24}},
Abstract = {{The absorption behavior of cyanine dyes in lipid vesicles prepared from
   egg lecithin and synthetic dipalmitoylphosphatidylcholine was studied.
   The molecules of the cyanine dyes are readily redistributed between the
   vesicles with the characteristic time of several minutes and form
   J-aggregates when the average number of monomeric dye molecules exceeds
   3.2 per vesicle, far below the most common minimum aggregation number,
   which is between 20 and 50, for cyanine dyes in J-aggregates. The
   formation of J-aggregates was followed spectroscopically by the
   appearance of a narrow red-shifted band. (C) 2000 Elsevier Science B.V.
   All rights reserved.}},
