@article{ ISI:000169720900003,
Author = {de Leon, GPP and Prieto, LG and Villa, CR},
Title = {{Helminthofauna of Opisthonema libertate and Harengula thrissina
   (Osteichthyes : Clupeidae) from Chamela Bay, Jalisco, Mexico}},
Journal = {{REVISTA DE BIOLOGIA TROPICAL}},
Year = {{2000}},
Volume = {{48}},
Number = {{4}},
Pages = {{759-763}},
Month = {{DEC}},
Abstract = {{We collected helminths from the ``sardines{''} Harengula thrissina (N =
   61) and Opisthonema libertate (n = 43), from Chamela Bay, Jalisco State,
   Mexico (12 and six species, respectively). The nematode Pseudoterranova
   sp, reached the highest values of prevalence and mean abundance in O.
   libertate (11.6\% and 0.13 worms per host, respectively), while in H.
   thrissina the digenean Parahemiurus merus reached a prevalence of 49.1\%
   and a mean abundance of 1.40 worms per host. Low similarity values
   (qualitative and quantitative) between helminths of both host species is
   a result of their opportunistic feeding habits (100\% of the parasitic
   species in N. thrissina and 668 of those from O. libertate infect their
   host from prey) and differential exposure to helminth larvae.}},
