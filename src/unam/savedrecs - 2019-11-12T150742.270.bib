@article{ ISI:000169676600005,
Author = {Velasco, CS and Rull, MAP and Riano, AB},
Title = {{Equity Judgments: The effect of inflationary context on salary resources
   allocation}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2000}},
Volume = {{17}},
Number = {{2}},
Pages = {{143-150}},
Month = {{DEC}},
Abstract = {{This paper is concerned with the study of the rules of allocation of
   scarce resources, among persons with varying merit. The proportionality
   model of equity judgments describes properly these kind of situations
   (Anderson, 1976). In order td evaluate the generality of this model, 188
   subjects were exposed to a situation where they assigned from a feted
   amount of money, the correspondent salary of two hypothetical faculty
   members with varying merits. Items corresponded to a factorial design of
   two amounts to distribute, seven levels of i's merit and four levels of
   j's merit. Subjects were randomly assigned to one of four groups, which
   represented context of allocation. The factorial design consisted of
   three groups of inflation and a control group. The Proportionality model
   described properly the data of all the groups. There were no contextual
   effects. However, some systematical biases were identified. On the basis
   of a micro-analytic strategy, such biases are discussed.}},
