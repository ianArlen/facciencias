@article{ ISI:000170142500032,
Author = {Jimenez-Cisneros, BE and Maya-Rendon, C and Salgado-Velazquez, G},
Title = {{The elimination of helminth ova, faecal coliforms, Salmonella and
   protozoan cysts by various physicochemical processes in wastewater and
   sludge}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{2001}},
Volume = {{43}},
Number = {{12}},
Pages = {{179-182}},
Note = {{10th International Symposium on Health-Related Water Microbiology,
   PARIS, FRANCE, JUL 03-06, 2000}},
Organization = {{IWA}},
Abstract = {{The removal of helminth ova, faecal coliforms, Salmonella and protozoan
   cysts by the application of physicochemical treatment processes to
   municipal wastewater and sludge was studied. In the first case, the
   advanced primary treatment (APT) process was studied, as well as
   filtration of the APT effluent. The APT sludge was treated with either
   lime or acid. The initial values of helminth ova, faecal coliforms,
   Salmonella and protozoan cysts in the wastewater were 23-27 eggs/L,
   7.8x10(7)-6.5x10(8) MPN/100mL, 4.5x10(5)-2.4x10(6) MPN/100mL, and
   1,007-1,814 cysts/L respectively. After APT treatment, 96\% of the
   helminth ova, 1 log of faecal coliforms and Salmonella, and 67\% of the
   protozoan cysts were eliminated. To reduce the concentration of helminth
   ova from values >1.2 ova/L to < 1 ova/L an additional filtration step
   was required. In the sludge, the initial values of helminth ova, faecal
   coliforms and Salmonella were 65-120 ova/g TS, 8.3x10(7)-1.4x10(11)
   MPN/g TS and 3.6x10(6)-2.4x10(10) MPN/g TS respectively. A 97\%
   reduction of the helminth and an 8.5 log reduction of faecal coliforms
   and Salmonella was achieved by alkaline stabilisation, compared with a
   98\% and 4.5 log reduction by acid treatment.}},
