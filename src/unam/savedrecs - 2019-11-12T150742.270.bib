@article{ ISI:000089711100016,
Author = {Bunge, CF and Ley-Koo, E and Jauregui, R},
Title = {{Variational incorporation of negative-energy orbitals in relativistic
   electronic structure calculations}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
