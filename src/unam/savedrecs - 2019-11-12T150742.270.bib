@article{ ISI:000171303400014,
Author = {Oropeza, MR and Cabirol, N and Ortega, S and Ortiz, LPC and Noyola, A},
Title = {{Removal of fecal indicator organisms and parasites (fecal coliforms and
   helminth eggs) from municipal biologic sludge by anaerobic mesophilic
   and thermophilic digestion}},
