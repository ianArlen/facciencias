Title = {{Cooperative grain boundary sliding at room temperature of a
   Zn-20.2\%Al-1.8\%Cu superplastic alloy}},
Booktitle = {{SUPERPLASTICITY IN ADVANCED MATERIALS, ICSAM-2000}},
Series = {{MATERIALS SCIENCE FORUM}},
Year = {{2001}},
Volume = {{357-3}},
