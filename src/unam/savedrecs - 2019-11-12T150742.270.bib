@article{ ISI:000166356400005,
Author = {Besprosvany, J},
Title = {{Gauge and space-time symmetry unification}},
Journal = {{INTERNATIONAL JOURNAL OF THEORETICAL PHYSICS}},
Year = {{2000}},
Volume = {{39}},
Number = {{12}},
Pages = {{2797-2836}},
Month = {{DEC}},
Abstract = {{Unification ideas suggest an integral treatment of fermion and boson
   spin and gauge-group degrees of freedom. Hence, a generalized quantum
   field equation. based on Dirac's. is proposed and investigated which
   contains gauge and flavor symmetries, determines vector gauge Field and
   fermion solution representations. and Tires their mode of interaction.
   The simplest extension of the theory with a 6-dimensional Clifford
   algebra has an SU(2)(L) X U(1) symmetry, which is associated with the
   isospin and the hypercharge. their vector carriers, two-flavor charged
   and chargeless leptons. and scalar particles. A mass term produces
   breaking of the symmetry to an electromagnetic U(1), and a Weinberg's
   angle theta (w) with sin(2)(theta (w)) = 0.25. A more realistic 8D
   extension gives coupling constants of the respective groups g = 1/root2
   approximate to .707 and g' = 1/root6 approximate to .408. with the same
   theta (w).}},
