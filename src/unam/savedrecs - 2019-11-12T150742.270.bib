@article{ ISI:000166998400051,
Author = {Raga, AC and Curiel, S and Rodriguez, LF and Canto, J},
Title = {{A numerical model for the Serpens radio jet}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{2000}},
Volume = {{364}},
Number = {{2}},
Pages = {{763-768}},
Month = {{DEC}},
Abstract = {{The Serpens ({''}triple source{''}) radio continuum jet shows a series
   of aligned knots with approximate to 1-2 `` angular separations, forming
   a curved structure circumscribed within a cone of similar to 10 degrees
   full opening angle. We present a high resolution, 3D numerical
   simulation of a jet with variable ejection direction and velocity with
   parameters appropriate for the Serpens jet, from which we obtain
   predictions of 3.6 cm radio continuum maps. We find that the general
   morphology of the radio emission, and the time evolution of the
   successive knots, qualitatively agree with 3.6 cm VLA observations of
   the Serpens radio jet.}},
