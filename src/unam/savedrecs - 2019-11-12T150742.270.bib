@article{ ISI:000165285300001,
Author = {Carballo, JL},
Title = {{The genus Hymerhabdia Topsent, 1892 (Porifera : Halichondrida :
   Axinellidae) with some remarks on related genera}},
Journal = {{PROCEEDINGS OF THE BIOLOGICAL SOCIETY OF WASHINGTON}},
