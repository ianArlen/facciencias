@article{ ISI:000169676600001,
Author = {Ortiz, DR and Munoz, JG and Nunez, GP},
Title = {{Optimal electrode placement for electromyographic (EMG) recording of
   facial muscular groups of both sides of the face}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2000}},
Volume = {{17}},
Number = {{2}},
Pages = {{101-109}},
Month = {{DEC}},
Abstract = {{The best sites for placing surface electrodes, where the greatest
   activity of a muscular group was registered when making specific facial
   actions, were identified. Symmetry of such sites in both sides of the
   face was also determined. Fifteen healthy volunteers participated in the
   study(eight women and seven men). They were instructed to make seven
   movements or specific facial actions, and electromyograms were recorded
   in both sides of the face. In six of the seven actions, four placement
   derivations were recorded and only three in the remaining action. An
   ANOVA of randomized blocks was carried out for each facial action and
   each side, followed by a post hoc Tukey test. The data showed
   significant differences between electromyograms of each facial action in
   both sides of the face. The electrode sites showing the greater
   differences were symmetrical.}},
