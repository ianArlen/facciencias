@article{ ISI:000165738100014,
Author = {Barbosa-Garcia, O and Meneses-Nava, MA and Maldonado, JL and Hernandez,
   J and Murrieta, H},
Title = {{Non-radioactive energy transfer processes and luminescent quantum
   efficiency in laser crystals}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2000}},
Volume = {{46}},
Number = {{2}},
Pages = {{59-63}},
Month = {{NOV}},
Note = {{1st Congress on Management of Exact Science Research Projects, ACAPULCO,
   MEXICO, FEB 22-25, 2000}},
Organization = {{Consejo Nacl Ciencia \& Technol}},
Abstract = {{The non-radiative energy transfer processes are identified for the
   luminiscent system Nd(1.1 at.\%):YAG and their effects upon the
   luminiscent quantum efficiency. In comparison to other calculations for
   this system and for the transfer processes, donor and acceptor ions are
   identified among the same neodymium ions. Thus, donor and acceptor ions
   correspond to ions that occupy non-perturbed and perturbed sites,
   respectively. The interactions responsible for the transfer processes
   are dipole-dipole, dipole-quadrupole, quadrupole-quadrupole and
   exchange. Our calculations for the luminiscence of the F-4(3/2) State of
   neodymium ions fit the experimental measured data. The experimental data
   show a fast initial decay followed by a non-exponential decay at
   longtime. The calculated luminiscent quantum efficiency is 0.75.}},
