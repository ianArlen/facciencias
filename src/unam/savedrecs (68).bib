@article{ ISI:000299086400008,
Author = {Bartsch, Thomas and Clapp, Monica and Grossi, Massimo and Pacella,
   Filomena},
Title = {{Asymptotically radial solutions in expanding annular domains}},
Journal = {{MATHEMATISCHE ANNALEN}},
Year = {{2012}},
Volume = {{352}},
Number = {{2}},
Pages = {{485-515}},
Month = {{FEB}},
Abstract = {{In this paper we consider the problem
   -Delta u = up in Omega(R),
   u = 0 on partial derivative Omega(R),
   where p > 1 and Omega(R) is a smooth bounded domain with a hole which is
   diffeomorphic to an annulus and expands as R -> infinity. The main goal
   of the paper is to prove, for large R, the existence of a positive
   solution to (0.1) which is close to the positive radial solution in the
   corresponding diffeomorphic annulus. The proof relies on a careful
   analysis of the spectrum of the linearized operator at the radial
   solution as well as on a delicate analysis of the nondegeneracy of
   suitable approximating solutions.}},
