@inproceedings{ ISI:000309944800148,
Author = {Sabin, Laurence and Vazquez, Roberto and Lopez, Jose A. and Garcia-Diaz,
   Maria-Teresa},
Editor = {{Manchado, A and Stanghellini, L and Schonberner, D}},
Title = {{The multi-polar planetary nebula NGC 5189}},
