@inproceedings{ ISI:000312888700071,
Author = {Snigula, J. M. and Cornell, M. E. and Drory, N. and Fabricius, M. and
   Landriau, M. and Hill, G. J. and Gebhardt, K.},
Editor = {{Radziwill, NM and Chiozzi, G}},
Title = {{Cure-WISE: HETDEX data reduction with Astro-WISE}},
Booktitle = {{SOFTWARE AND CYBERINFRASTRUCTURE FOR ASTRONOMY II}},
