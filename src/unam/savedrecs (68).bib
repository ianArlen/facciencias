@article{ ISI:000313776400005,
Author = {Oyetayo, Victor O. and Nieto-Camacho, Antonio and Esquivel Rodriguez,
   Baldomero and Jimenez, Manuel},
Title = {{Assessment of Anti-inflammatory, Lipid Peroxidation and Acute Toxicity
   of Extracts Obtained From Wild Higher Basidiomycetes Mushrooms Collected
   From Akure (Southwest Nigeria)}},
Journal = {{INTERNATIONAL JOURNAL OF MEDICINAL MUSHROOMS}},
Year = {{2012}},
Volume = {{14}},
Number = {{6}},
Pages = {{575-580}},
Abstract = {{Ethanolic extracts of three wild medicinal mushrooms, namely Lenzites
   betulina (LET), Trametes versicolor (TET), and Coriolopsis polyzona
   (CET), collected from Akure, southwest Nigeria, were assessed for their
   lipid peroxidation, anti-inflammatory, and acute toxicity effects. The
   inhibition of the formation of thiobarbituric acid reactive species
   (TBARS) by extracts was concentration dependent and ranged from 86.99\%
   to 92.18\% at 1000 mu g/mL. The IC50 of the extracts was also in the
   range of 222.81 mu g/mL to 737.13 mu g/mL. The anti-inflammatory effect
   measured by inhibition of mice ear edema was higher and significantly
   different (P <= 0.05) than the control. The acute toxicity test also
   revealed tolerance to the three ethanolic extracts by Artemia sauna at
   concentrations of 10 mu g/mL to 1000 mu g/mL, except for ethanolic
   extracts of LET and TET, which exhibited toxicity against this
   invertebrate at 1000 mu g/mL. This research has shown that ethanolic
   extracts of these three macrofungi could be good sources of safe and
   effective antioxidant and antinflammatory agents for biopharmaceutical
   exploitation.}},
