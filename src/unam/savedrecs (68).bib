@inproceedings{ ISI:000309944800086,
Author = {Garcia-Diaz, Maria-Teresa and Lopez, Jose-Alberto and Steffen, Wofgang
   and Richer, Michael G. and Riesgo, Hortensia},
Editor = {{Manchado, A and Stanghellini, L and Schonberner, D}},
Title = {{A Cat's Eye view of the Eskimo from Saturn}},
Booktitle = {{PLANETARY NEBULAE: AN EYE TO THE FUTURE}},
