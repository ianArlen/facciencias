@inproceedings{ ISI:000312415800174,
Author = {Bernal, Abel and HAWC Collaboration},
Editor = {{Navarro, R and Cunningham, CR and Prieto, E}},
Title = {{Analog front end board electronics proposal for the HAWC observatory}},
Booktitle = {{MODERN TECHNOLOGIES IN SPACE-AND GROUND-BASED TELESCOPES AND
   INSTRUMENTATION II}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8450}},
Note = {{Conference on Modern Technologies in Space-and Ground-Based Telescopes
   and Instrumentation II, Amsterdam, NETHERLANDS, JUL 01-06, 2012}},
Organization = {{SPIE}},
Abstract = {{HAWC (High Altitude Water Cherenkov) is a high energy, second
   generation, gamma ray detector-telescope (energy between 100GeV to
   100TeV) under construction at an altitude of 4100 m in the Sierra Negra
   volcano, Mexico. HAWC is a international Mexico/USA collaboration and
   when completed it will consist of a array of 300 tanks filled with high
   transparency water and three or four photomultipliers tubes near the
   bottom of each tank Charged particles traveling through the water of the
   tanks produce Cherenkov effect blue light that will be captured by the
   photomultiplier tubes. This work discusses some analog electronics
   solutions and the use of high speed differential amplifiers for tracking
   the high frequency pulses from the photomultiplier tubes. It also looks
   towards the update of the present analog front end board electronics of
   the water detector tanks}},
