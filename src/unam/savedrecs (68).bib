@article{ ISI:000321037200002,
Author = {Salgado-Bernal, Irina and Duran-Dominguez, Carmen and Cruz-Arias, Mario
   and Elena Carballo-Valdes, Maria and Martinez-Sardinas, Armando},
Title = {{Rhizosphere bacteria with physiological potentialities for organic
   matter removal from waste waters}},
