@inproceedings{ ISI:000310698100042,
Author = {Delgado-Alvarez, J. and Ramirez-Argaez, Marco A. and Gonzalez-Rivera, C.},
Editor = {{Simos, TE and Psihoyios, G and Tsitouras, C and Anastassi, Z}},
Title = {{Mathematical Modeling of a Gas Jet Impinging on a Two Phase Bath}},
Booktitle = {{NUMERICAL ANALYSIS AND APPLIED MATHEMATICS (ICNAAM 2012), VOLS A AND B}},
Series = {{AIP Conference Proceedings}},
Year = {{2012}},
Volume = {{1479}},
Pages = {{177-180}},
