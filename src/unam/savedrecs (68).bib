Title = {{Nylon 6,6 electrospun fibres reinforced by amino functionalised 1D and
   2D carbon}},
Booktitle = {{INTERNATIONAL CONFERENCE ON STRUCTURAL NANO COMPOSITES (NANOSTRUC 2012)}},
Series = {{IOP Conference Series-Materials Science and Engineering}},
