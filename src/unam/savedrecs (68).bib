@inproceedings{ ISI:000309527200018,
Author = {Corichi, Alejandro and Vukasinac, Tatjana},
Editor = {{UrenaLopez, LA and BecerrilBarcenas, R and LinaresRomero, R}},
Title = {{On the choice of time in the continuum limit of polymeric effective
   theories}},
Booktitle = {{IX WORKSHOP OF THE GRAVITATION AND MATHEMATICAL PHYSICS DIVISION OF THE
   MEXICAN PHYSICAL SOCIETY}},
Series = {{AIP Conference Proceedings}},
