@article{ ISI:000314292300020,
Author = {Elena Aguilera, Maria and Illanes, Alejandro},
Title = {{CONNECTEDNESS PROPERTIES OF THE HYPERSPACES C-epsilon(X)}},
Journal = {{HOUSTON JOURNAL OF MATHEMATICS}},
Year = {{2012}},
Volume = {{38}},
Number = {{4}},
Pages = {{1343-1354}},
Abstract = {{For a metric continuum X and epsilon > 0, let C-epsilon(X) be the
   hyperspace of subcontinua of X of diameter less than or equal to
   epsilon. In this paper we answer several questions posed by Eric L.
   McDowell and B.E. Wilder. We prove that if X is a metric continuum and
   epsilon > 0, then: (a) If X is arcwise connected, then C-epsilon(X) is
   cyclicly connected, (b) C-epsilon(X) is not separated by any of its
   zero-dimensional subsets, and (c) C-epsilon(X) is zero-dimensional
   closed set aposyndetic.}},
