@article{ ISI:000314341500010,
Author = {Ortiz-Bobadilla, L. and Rosales-Gonzalez, E. and Voronin, S. M.},
Title = {{THOM'S PROBLEM FOR DEGENERATE SINGULAR POINTS OF HOLOMORPHIC FOLIATIONS
   IN THE PLANE}},
Journal = {{MOSCOW MATHEMATICAL JOURNAL}},
Year = {{2012}},
Volume = {{12}},
Number = {{4}},
Pages = {{825-862}},
Abstract = {{Let V-n be the class of germs of holomorphic non-dicritic vector fields
   in (C-2, 0) with vanishing (n - 1)-jet at the origin, n >= 2, and
   non-vanishing n-jet. hi the present work the formal normal form (under
   the strict orbital classification) of generic germs in a subclass V-n(o)
   of V-n is given. Any such normal form is given as the sum of three
   terms: a ``principal{''} generic homogeneous term, v(o) is an element of
   V-n a ``hamiltonian{''} term, v(c) (given by a hamiltonian polynomial
   vector field) and a ``radial{''} term.
   For any generic genii v is an element of V-n(o) we define the triplet
   i(v) = (v(o), v(c), {[}G(v)]), where v(o) and v(c) denote are principal
   and hamiltonian terms of its corresponding formal normal form, and
   {[}G(v)] denotes the class of strict analytic conjugacy of its
   projective (hidden or vanishing) monodromy group. We prove that the
   terms appearing in i(v) are Thom's invariants of the strict analytical
   orbital classification of generic germs in V-n(o): two generic germs v
   and (v) over tilde in V-n(o) are strictly orbitally analytically
