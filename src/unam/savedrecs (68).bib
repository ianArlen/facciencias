Title = {{Design of a Prescribed Convergence Time Uniform Robust Exact Observer in
   the Presence of Measurement Noise}},
Booktitle = {{2012 IEEE 51ST ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2012}},
Pages = {{6615-6620}},
