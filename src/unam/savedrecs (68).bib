@article{ ISI:000300272200047,
Author = {Fernandez-Osorio, A. and Pineda-Villanueva, E. and Chavez-Fernandez, J.},
Title = {{Synthesis of nanosized (Zn1-xCox)Al2O4 spinels: New pink ceramic
   pigments}},
Journal = {{MATERIALS RESEARCH BULLETIN}},
Year = {{2012}},
Volume = {{47}},
Number = {{2}},
Pages = {{445-452}},
Month = {{FEB}},
Abstract = {{The Zn1-xCoxAl2O4 system has been reported to show a blue color with
   particles sizes that range from 36 nm to 30 mu m. However, we have
   observed a pink color in this system with nanoparticles of an average
   size of 10 nm.
   Such differences in the optical properties of this nanosystem are
   related to the distribution of the cobalt cations in octahedral sites
   rather than in tetrahedral sites as in standard cobaltite spinels.
   The solid solution Zn(1-x)CQ(x)Al(2)O(4) (x = 0.2, 0.4, 0.6, 0.8 and 1)
   was synthesized by a co-precipitation reaction from the metallic salts
   and subsequent calcinations.
   The color properties and the high thermal stability (1400 degrees C) of
   these nanostructures suggest that they have the potential to be applied
   as satisfactory ceramic pigments. (C) 2011 Elsevier Ltd. All rights
   reserved.}},
