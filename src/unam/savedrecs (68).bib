@article{ ISI:000298613800013,
Author = {de los Santos-Sanchez, O. and Recamier, J.},
Title = {{The f-deformed Jaynes-Cummings model and its nonlinear coherent states}},
Journal = {{JOURNAL OF PHYSICS B-ATOMIC MOLECULAR AND OPTICAL PHYSICS}},
Year = {{2012}},
Volume = {{45}},
Number = {{1}},
Month = {{JAN 14}},
Abstract = {{Based on the f-oscillator formalism, we introduce a nonlinear
   Jaynes-Cummings model (NJCM) which is constructed from the standard JCM
   by deforming the single-mode field operators. Such a generalization of
   the JCM describes the interaction of a two-level atom with a single mode
   of the electromagnetic field in the presence of a nonlinear Kerr-like
   medium. Since the medium is modelled as an f-oscillator, it is possible
   to consider the field f-coherent states (nonlinear coherent states) and
   their evolution.}},
