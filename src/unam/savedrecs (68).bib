@inproceedings{ ISI:000326239300054,
Author = {Aguilar, I. P. and Aguilar, H.},
Editor = {{Chova, LG and Torres, IC and Martinez, AL}},
Title = {{WORKING ALONG, RATHER THAN COMPETING AGAINST MEDIA TOOLS IN THE
   CLASSROOM}},
Booktitle = {{EDULEARN12: 4TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
