@article{ ISI:000263907200004,
Author = {Layer, P. W. and Garcia-Palomo, A. and Jones, D. and Macias, J. L. and
   Arce, J. L. and Mora, J. C.},
Title = {{El Chichon volcanic complex, Chiapas, Mexico: Stages of evolution based
   on field mapping and Ar-40/Ar-39 geochronology}},
Journal = {{GEOFISICA INTERNACIONAL}},
Year = {{2009}},
Volume = {{48}},
Number = {{1}},
Pages = {{33-54}},
Month = {{JAN-MAR}},
