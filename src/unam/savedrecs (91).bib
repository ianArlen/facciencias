@article{ ISI:000265628200005,
Author = {Manuel Mendoza-Nunez, Victor and Sanchez-Rodriguez, Martha A. and
   Correa-Munoz, Elsa},
Title = {{Undernutrition and Oxidative Stress as Risk Factors for High Blood
   Pressure in Older Mexican Adults}},
Journal = {{ANNALS OF NUTRITION AND METABOLISM}},
Year = {{2009}},
Volume = {{54}},
Number = {{2}},
Pages = {{119-123}},
Abstract = {{Objective: To determine the relationship between undernutrition and
   oxidative stress as risk factors for high blood pressure (HBP) in older
   Mexican adults. We carried out a case-control study in a convenience
   sample of 62 older adults, 60-80 years of age; 32 without HBP (mean age
   75.8 +/- 5.1 years) and 30 with HBP (mean age 65.2 +/- 6.8 years).
   Methods: We measured serum lipoperoxide (LPO) levels, antioxidant
   enzymes, total antioxidant activity and lipid profile; in addition, we
   evaluated the nutritional status with anthropometric measurements, the
   Mini Nutritional Assessment and food intake frequency. Data were
   analyzed with Student t and odds ratio (OR) statistical tests. Results:
   We observed significantly higher LPO levels in subjects with HBP than in
   normotensive subjects (0.303 +/- 0.10 vs. 0.256 +/- 0.06 mu mol/l; p <
   0.05). With respect to risk factors for HBP, we found an OR of 12.2
   (95\% CI 1.9-76.4, p < 0.01) in subjects with high LPO levels (>= 0.320
