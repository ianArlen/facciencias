@article{ ISI:000262388200003,
Author = {Mario, Sousa S.},
Title = {{STANDLEYI, A NEW SECTION FOR THE GENUS LONCHOCARPUS (LEGUMINOSAE), NEW
   SPECIES AND SUBSPECIES FOR MESOAMERICA AND SOUTH AMERICA}},
Journal = {{Acta Botanica Mexicana}},
Year = {{2009}},
Volume = {{86}},
Pages = {{39-69}},
Month = {{JAN}},
Abstract = {{A new section, sect. Standleyi, is described for the genus Lonchocarpus
   (Leguminosae, Papilionoideae, Millettieae), based mainly on previously
   proposed concepts, but not validly published. Also six new taxa for
   science: five species, Lonchocarpus martinezii, L. savannicola, L.
   semideserti. L. stenophyllus, L. tuxtepecensis and a new subspecies L.
   lanceolatus subsp. calciphilus are described and illustrated.
   Lonchocarpus pubescens is proposed as an older binomial than L.
   dipteroneurus and four other names associated previously. To contrast
   the differences, two keys for the taxa are given, one for the species in
   Mesoamerica and South America and another for the new section and the
   sect. Lonchocarpus.}},
