@article{ ISI:000262613900019,
Author = {Ramirez, Carlos and Wang, Chumin},
Title = {{Bosonic nature of collective Cooper pairs}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2009}},
Volume = {{373}},
Number = {{2}},
Pages = {{269-271}},
Month = {{JAN 5}},
Abstract = {{Superconductivity is not considered as a Bose-Einstein condensation,
   because the creation and annihilation operators of Cooper pairs do not
   satisfy bosonic commutation relations. However. collective pairs can be
   constructed by a linear combination of Cooper pairs and we demonstrate
   in this Letter that these collective Cooper pairs have bosonic nature.
   In addition, the Bardeen-Cooper--Schrieffer (BCS) superconducting ground
   state can be built by means of these pairs and in consequence. Could be
   treated as a Bose-Einstein condensate. (C) 2008 Elsevier B.V. All rights
   reserved.}},
