@inproceedings{ ISI:000262216100003,
Author = {Makeyev, O. and Sazonov, E. and Schuckers, S. and Lopez-Meyer, P. and
   Baidyk, T. and Melansod, E. and Neuman, M.},
Editor = {{Allen, T and Ellis, R and Petridis, M}},
Title = {{Recognition of Swallowing Sounds Using Time-Frequency Decomposition and
   Limited Receptive Area Neural Classifier}},
Booktitle = {{APPLICATIONS AND INNOVATIONS IN INTELLIGENT SYSTEMS XVI}},
Year = {{2009}},
Pages = {{33+}},
Note = {{28th SGAI International Conference on Innovative Techniques and
   Applications of Artificial Intelligence, Cambridge, ENGLAND, DEC, 2008}},
Organization = {{British Comp Soc Specialist Grp Artificial Intelligence}},
Abstract = {{In this paper we propose a novel swallowing Sound recognition technique
   based on the limited receptive area (LIRA) neural classifier and
   time-frequency decomposition. Time-frequency decomposition methods
   commonly used in sound recognition increase dimensionality of the signal
   and require steps of feature selection and extraction. Quite often
   feature selection is based on a set of empirically chosen statistics,
   making the pattern recognition dependent on the intuition and skills of
   the investigator. A limited set of extracted features is then presented
   to a classifier. The proposed method avoids the steps of feature
   selection and extraction by delegating them to a limited receptive area
   neural (LIRA) classifier. LIRA neural classifier utilizes the increase
   in dimensionality of the signal to create a large number of random
   features in the time-frequency domain that assure a good description of
   the signal without prior assumptions of the signal properties. Features
   that do not provide useful information for separation of classes do not
   obtain significant weights during classifier training. The proposed
   methodology was tested on the task of recognition of swallowing Sounds
   with two different algorithms of time-frequency decomposition,
   short-time Fourier transform (STFT) and continuous wavelet transform
   (CWT). The experimental results suggest high efficiency and reliability
   of the proposed approach.}},
