@article{ ISI:000262701400026,
Author = {Berzunza-Cruz, Miriam and Bricaire, Guadalupe and Salaiza Suazo, Norma
   and Perez-Montfort, Ruy and Becker, Ingeborg},
Title = {{PCR for identification of species causing American cutaneous
   leishmaniasis}},
Journal = {{PARASITOLOGY RESEARCH}},
