@inproceedings{ ISI:000275676600015,
Author = {Czyzowicz, J. and Kranakis, E. and Krizanc, D. and Lambadaris, I. and
   Narayanan, L. and Opatrny, J. and Stacho, L. and Urrutia, J. and
   Yazdani, M.},
Editor = {{Ruiz, PM and GarciaLunaAceves, JJ}},
Title = {{On Minimizing the Maximum Sensor Movement for Barrier Coverage of a Line
   Segment}},
Booktitle = {{AD-HOC, MOBILE AND WIRELESS NETWORKS, PROCEEDINGS}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2009}},
