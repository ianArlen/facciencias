@article{ ISI:000264057700003,
Author = {Martinez-Mayen, Mario and Roman-Contreras, Ramiro},
Title = {{REPRODUCTION OF PERICLIMENES SIANKAANENSIS (DECAPODA, CARIDEA,
   PALAEMONIDAE) IN BAHIA DE LA ASCENSION, QUINTANA ROO, MEXICO}},
Journal = {{CRUSTACEANA}},
Year = {{2009}},
Volume = {{82}},
Number = {{1}},
Pages = {{27-37}},
Abstract = {{The reproduction of Periclimenes siankaanensis was studied from samples
   collected in May 2002 in Bahia de la Ascension, Quintana Roo, Mexico. A
   total of 1076 specimens was analysed, and a sex ratio of 1.56 : 1 in
   favour of females was calculated. The size of ovigerous females varied
   from 1.91 to 3.2 mm cephalothoracic length (CL), and the size at sexual
   maturity in the population was 2.39 mm CL. Average fecundity amounted to
   52.6 eggs with a minimum of 23 and a maximum of 141. The correlation
   coefficient between CL and fecundity was calculated as 0.69. Average egg
   size was 0.4988 x 0.3764 mm during the first phase of development, and
   0.6453 x 0.4486 mm for embryos near hatching, with average volumes of
   0.0560 mm(3) to 0.1024 mm(3).}},
