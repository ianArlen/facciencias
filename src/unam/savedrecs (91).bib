@article{ ISI:000262479100002,
Author = {Raga, A. C. and Henney, W. and Vasconcelos, J. and Cerqueira, A. and
   Esquivel, A. and Rodriguez-Gonzalez, A.},
Title = {{Multiple clump structures within photoionized regions}},
