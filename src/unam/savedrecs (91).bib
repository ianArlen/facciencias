@article{ ISI:000270912600006,
Author = {Contreras-Cristan, Alberto and Gonzalez-Barrios, Jose M.},
Title = {{A Nonparametric Test for Symmetry Based on Freeman and Halton's Ideas on
   Contingency Tables}},
Journal = {{COMMUNICATIONS IN STATISTICS-SIMULATION AND COMPUTATION}},
Year = {{2009}},
Volume = {{38}},
Number = {{9}},
Pages = {{1856-1869}},
Abstract = {{In this article, we propose a nonparametric method to test for symmetry
   in bivariate data. By using the extension of Fisher's exact treatment
   for 2 x 2 contingency tables proposed by Freeman and Halton (1951), we
   can test the hypothesis of equal distribution for two samples of integer
   valued variables. Then, by counting the number of observations belonging
   to each cell of a symmetric, appropriately built grid, we can produce
   the two samples of integers required to use this test for equal
   distribution. The resulting test for symmetry is potentially extendible
   to higher dimensions. A simulation study is performed to compare with
   some known tests (Bowker, 1948; Hollander, 1971; and its improvement
   given in Krampe and Kuhni, 2007). Our proposal represents a competitive
   option as a test for symmetry.}},
