@article{ ISI:000263907200008,
Author = {Nooren, C. A. M. and Hoek, W. Z. and Tebbens, L. A. and Martin Del
   Pozzo, A. L.},
Title = {{Tephrochronological evidence for the late Holocene eruption history of
   El Chichon Volcano, Mexico}},
Journal = {{GEOFISICA INTERNACIONAL}},
Year = {{2009}},
