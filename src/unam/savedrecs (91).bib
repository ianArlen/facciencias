@article{ ISI:000263478600007,
Author = {Vazquez-Elizondo, Genaro and Ponciano-Rodriguez, Guadalupe and Uribe,
   Misael and Mendez-Sanchez, Nahum},
Title = {{Human leukocyte antigens among primary biliary cirrhosis patients born
   in Mexico}},
Journal = {{ANNALS OF HEPATOLOGY}},
Year = {{2009}},
Volume = {{8}},
Number = {{1}},
Pages = {{32-37}},
Month = {{JAN-MAR}},
Abstract = {{Background and aim: Primary biliary cirrhosis (PBC) is a chronic
   cholestatic disease of autoimmune origin and has a genetic component.
   Although it has been reported that the prevalence of the HLA-DRB1{*}08
   allele is high in various populations, the prevalence of HLA alleles in
   Mexican PBC patients has not been described. The aim of this study was
   to quantify the prevalence of HLA-B/-DR alleles in Mexican PBC patients.
   Materials and methods: A case-control transversal study was performed
   during January and July 2008 with adult patients diagnosed with PBC.
   Cases were defined as subjects with PBC and controls were obtained from
   healthy subjects evaluated for bone marrow transplantation. Laboratory
   was performed at the moment of diagnosis. HLA-B/-DR allele frequency was
