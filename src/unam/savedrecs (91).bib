@incollection{ ISI:000276911900009,
Author = {Santos, Maria Josefa},
Editor = {{Butler, JS and Morales, A and Torres, DL}},
Title = {{Knowledge and Networks: Mexican American Entrepreneurship in
   Southwestern Michigan}},
Booktitle = {{AMERICAN STORY: MEXICAN AMERICAN ENTREPRENEURSHIP AND WEALTH CREATION}},
Series = {{International Series on Technology Policy and Innovation}},
Year = {{2009}},
Pages = {{151-174}},
Abstract = {{In this chapter, we show the role that social networks play in providing
   the mechanisms that allow Mexican migrants' access to the social and
   cultural capital and technical knowledge necessary to start their own
   business in southwestern Michigan. the cases analyzed are part of what
   specialists call minority enterprises. the main thesis is that knowledge
   provided by certain members of the network helps migrants to venture
   into new business opportunities. these opportunities have not been
   traditionally explored by members of this group and that is why these
   new enterprises are different from those traditionally owned by Mexican
   Americans. in order to analyze the knowledge flows in these networks, we
   studied three different businesses in Southwest Michigan counties:
   farms, stores, and restaurants.}},
