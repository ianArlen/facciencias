@inproceedings{ ISI:000268997700017,
Author = {Rebolledo-Mendez, Genaro and Dunwell, Ian and Martinez-Miron, Erika A.
   and Dolores Vargas-Cerdan, Maria and de Freitas, Sara and Liarokapis,
   Fotis and Garcia-Gaona, Alma R.},
Editor = {{Jacko, JA}},
Title = {{Assessing NeuroSky's Usability to Detect Attention Levels in an
   Assessment Exercise}},
Booktitle = {{HUMAN-COMPUTER INTERACTION, PT I}},
