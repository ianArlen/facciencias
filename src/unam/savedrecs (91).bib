@article{ ISI:000276898000017,
Author = {Reyes-Salas, A. and Reyes, O. E. and Ortega, F. and Macias, C. and
   Linares, C.},
Title = {{AN ANALYTICAL SEM STUDY OF MORPHOLOGY AND COMPOSITIONS OF METALS AND
   OTHERS OPAQUE GRAINS IN THE STONE METEORITE COSINA; AN ORDINARY
   CHONDRITE H5.}},
Journal = {{ACTA MICROSCOPICA}},
Year = {{2009}},
Volume = {{18}},
Number = {{3}},
Pages = {{351-357}},
Abstract = {{Cosina is an unusual ordinary chondrite H5 with a highly porous
   lithology. SEM images of the Fe-Ni metal phases (kamacite, taenite and
   tetrataenite), and other opaque phases such as troilite and chromite
   show a wide variety of textures, which may occur in euhedral and twinned
   crystals metallic druses, and an unusual morphology of troilite as
   cords. The different metal, sulfide and chromite microstructures
   observed in Cosina indicate two thermal histories: 1) As probable relict
   shock event and rapid cooling evidenced by the presence of melt in the
   form of tiny droplets of FeNi metal, FeS melted veins, chromite and vein
   FeNi across olivine, pyroxene and the assemblage CPA and, 2) As slow
   cooling event shown by Fe-Ni metal and chromite formed prisms with twins
   where well-developed crystal faces are common, druses, and the presence
   of tetrataenite among the metal grains.}},
