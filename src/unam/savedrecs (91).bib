Title = {{Automatic Extraction of Lexical Relations from Analytical Definitions
   Using a Constraint Grammar}},
Booktitle = {{ADVANCES IN ARTIFICIAL INTELLIGENCE, PROCEEDINGS}},
Series = {{Lecture Notes in Computer Science}},
