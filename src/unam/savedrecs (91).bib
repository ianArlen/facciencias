Title = {{Inhibition study of the SOD-Cu/Zn of Taenia solium by computational
   methods}},
Booktitle = {{BIBMW: 2009 IEEE INTERNATIONAL CONFERENCE ON BIOINFORMATICS AND
   BIOMEDICINE WORKSHOP}},
Series = {{IEEE International Conference on Bioinformatics and Biomedicine
   Workshop-BIBMW}},
