@article{ ISI:000265462000008,
Author = {Groeninckx, Inge and Dessein, Steven and Ochoterena, Helga and Person,
   Clases and Motley, Timothy J. and Karehed, Jesper and Bremer, Birgitta
   and Huysmans, Suzy and Smets, Erik},
Title = {{PHYLOGENY OF THE HERBACEOUS TRIBE SPERMACOCEAE (RUBIACEAE) BASED ON
   PLASTID DNA DATA}},
Journal = {{ANNALS OF THE MISSOURI BOTANICAL GARDEN}},
Year = {{2009}},
Volume = {{96}},
Number = {{1}},
