@article{ ISI:000262578600008,
Author = {Castillo, David J. and Ballado, Teresa and Camarena, Laura and Dreyfus,
   Georges},
Title = {{Functional analysis of a large non-conserved region of FlgK (HAP1) from
   Rhodobacter sphaeroides}},
Journal = {{ANTONIE VAN LEEUWENHOEK INTERNATIONAL JOURNAL OF GENERAL AND MOLECULAR
   MICROBIOLOGY}},
