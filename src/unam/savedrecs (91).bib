@article{ ISI:000276033800012,
Author = {Illanes, Alejandro and Martinez-Montejano, Jorge M.},
Title = {{COMPACTIFICATIONS OF {[}0, infinity) WITH UNIQUE HYPERSPACE F-n(X)}},
Journal = {{GLASNIK MATEMATICKI}},
Year = {{2009}},
Volume = {{44}},
Number = {{2}},
Pages = {{457-478}},
Abstract = {{Given a metric continuum X, F-n(X) denotes the hyperspace of nonempty
   subsets of X with at most n elements. In this paper we show the
   following result. Suppose that X is a metric compactification of {[}0,
   infinity), Y is a continuum and F-n(X) is homemorphic to F-n(Y). Then:
   (a) if n not equal 3, then X is homeomorphic to Y, (b) if n = 3 and the
   remainder of X is an ANR, then X is homeomorphic to Y. The question if
   the result in (a) is valid for n = 3 remains open.}},
Publisher = {{CROATIAN MATHEMATICAL SOC}},
