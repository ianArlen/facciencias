@article{ ISI:000265462000009,
Author = {Martinez-Cabrera, Dorismilda and Terrazas, Teresa and Ochoterena, Helga},
Title = {{FOLIAR AND PETIOLE ANATOMY OF TRIBE HAMELIEAE AND OTHER RUBIACEAE}},
