@inproceedings{ ISI:000274787200001,
Author = {Gutierrez-Castrejon, R.},
Editor = {{Velasquez, CE}},
Title = {{Impact of Channel-Spacing on the 100 Gb/s 40-km Ethernet PMD}},
Booktitle = {{2009 IEEE LATIN-AMERICAN CONFERENCE ON COMMUNICATIONS (LATINCOM 2009)}},
Series = {{IEEE Latin American Conference on Communications}},
Year = {{2009}},
Pages = {{1-6}},
Note = {{IEEE Latin-American Conference on Communications, Medellin, COLOMBIA,
   SEP 10-11, 2009}},
Organization = {{IEEE Commun Soc}},
Abstract = {{A numerical analysis that uses the Q-factor to investigate the impact of
   multi-channel semiconductor optical pre-amplifier (SOA) nonlinearities
   on the 4 x 25 Gb/s 40-km Ethernet link is presented for channel-spacing
   values in the 200-800 GHz range. The analysis, based on advanced methods
   to differentiate SOA nonlinear effects, shows that the main deleterious
   impact is produced by cross-gain modulation (XGM). However, four-wave
   mixing (FWM) is responsible for the system performance difference as a
   function of channel-spacing. In this respect, the 800 GHz
   channel-spacing plan should be preferred. For fiber lengths greater than
   30 km, FWM is practically inconsequential and XGM is less severe.
   Although this work represents and aid in the design of next-generation
   Ethernet products, its general nature also applies to similar WDM
   transmission systems.}},
