@article{ ISI:000263574700003,
Author = {Dzib-Perez, L. and Gonzalez-Sanchez, J. and Malo, J. M. and Rodriguez,
   F. J.},
Title = {{The effect of test conditions on the sensitivity and resolution of SRET
   signal response}},
Journal = {{ANTI-CORROSION METHODS AND MATERIALS}},
