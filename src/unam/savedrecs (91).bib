@inproceedings{ ISI:000275700400003,
Author = {Scardua, Bruno and Seade, Jose},
Editor = {{Saldanha, NC and Conlon, L and Langevin, R and Tsuboi, T and Walczak, P}},
Title = {{Compact foliations with Bott-Morse singularities}},
