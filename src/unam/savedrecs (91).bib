@article{ ISI:000265277600016,
Author = {Benitez, Felipe and Kaikina, Elena I. and Ruiz-Paredes, Hector F.},
Title = {{SUBCRITICAL NONLINEAR DISSIPATIVE EQUATIONS ON A HALF-LINE}},
Journal = {{ARABIAN JOURNAL FOR SCIENCE AND ENGINEERING}},
