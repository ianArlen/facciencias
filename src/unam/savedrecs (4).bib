@article{ ISI:000438374100034,
Author = {Nga Thi Thuy Nguyen and Contreras-Moreira, Bruno and Castro-Mondragon,
   Jaime A. and Santana-Garcia, Walter and Ossio, Raul and Robles-Espinoza,
   Carla Daniela and Bahin, Mathieu and Collombet, Samuel and Vincens,
   Pierre and Thieffry, Denis and van Helden, Jacques and Medina-Rivera,
