@article{ ISI:000440205200018,
Author = {Navarro-Guerrero, Gerardo and Tang, Yu},
Title = {{Fractional-Order Closed-Loop Model Reference Adaptive Control for
   Anesthesia}},
Journal = {{ALGORITHMS}},
Year = {{2018}},
Volume = {{11}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{The design of a fractional-order closed-loop model reference adaptive
   control (FOCMRAC) for anesthesia based on a fractional-order model (FOM)
   is proposed in the paper. This proposed model gets around many
   difficulties, namely, unknown parameters, lack of state measurement,
   inter and intra-patient variability, and variable time-delay,
   encountered in controller designs based on the PK/PD model commonly used
   for control of anesthesia, and allows to design a simple adaptive
   controller based on the Lyapunov analysis. Simulations illustrate the
   effectiveness and robustness of the proposed control.}},
