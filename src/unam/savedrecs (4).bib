@article{ ISI:000417367000003,
Author = {Camarena Adame, Maria Elena and Saavedra Garcia, Maria Luisa},
Title = {{The gender perspective in the curriculum of management accountants
   degrees}},
Journal = {{NOESIS-REVISTA DE CIENCIAS SOCIALES Y HUMANIDADES}},
Year = {{2018}},
Volume = {{27}},
Number = {{54}},
Pages = {{39-58}},
Month = {{JUL-DEC}},
Abstract = {{The gender perspective involves visualizing the phenomena of reality in
   the context of differences between men and women in all areas. The aim
   of the present paper is to propose incorporating the gender perspective
   in the curriculum of business administration professional degrees. An
   analysis of the relevant literature led to understanding such issues as
   university and gender, gender in the university curriculum, gender
   perspective in national development plans including education in Mexico
   and gender studies in organizations. A review of relevant publications
   in Mexico addressing the issue of gender studies in organizations, led
   to classifying them by lines of research. Finally the issues that should
   be added to the curricula of these disciplines are presented.}},
