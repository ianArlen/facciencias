@article{ ISI:000438841000003,
Author = {Rendon-Sauz, F. G. and Flores-Reyes, T. and Ponce-Flores, A.},
Title = {{RAPID CLASSIFICATION OF BACTERIA USING LIBS IN MULTI-PULSE LASER REGIME
   AND NEURAL NETWORKS PROCESSING}},
Journal = {{REVISTA CUBANA DE FISICA}},
Year = {{2018}},
Volume = {{35}},
Number = {{1}},
Pages = {{10-14}},
Month = {{JUL}},
Abstract = {{Recently, efforts have been increased for the rapid identification of
   microbiological samples to improve health diagnosis. Existing methods
   are time consuming and require pre-treatment of the sample. For these
   reasons, the Laser Induced Breakdown Spectroscopy (LIBS) technique has
   been proposed as an alternative method to reduce bacterial
   identification times. In this work, spectra were obtained from two types
   of bacteria: Escherichia coli and Staphylococcus aureus, their elemental
   identification, as well as the use of an artificial neural network for
   classification among them. As a source of excitation, a compact laser
   was used in the Q Multi-pulse switch, which favors the intensity of the
   plasma emission and facilitates detection. The results presented show a
   clear identification between the two types of bacteria.}},
