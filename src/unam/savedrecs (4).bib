@article{ ISI:000434903300003,
Author = {Gutierrez-Garcia, Ana G. and Contreras, Carlos M. and Saldivar-Lara,
   Mauricio},
Title = {{An alarm pheromone reduces ventral tegmental area-nucleus accumbens
   shell responsivity}},
