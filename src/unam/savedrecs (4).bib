@article{ ISI:000441443500012,
Author = {Fages, Francois and Martinez, Thierry and Rosenblueth, David A. and
   Soliman, Sylvain},
Title = {{Influence Networks Compared with Reaction Networks: Semantics,
   Expressivity and Attractors}},
Journal = {{IEEE-ACM TRANSACTIONS ON COMPUTATIONAL BIOLOGY AND BIOINFORMATICS}},
Year = {{2018}},
Volume = {{15}},
