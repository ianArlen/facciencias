@article{ ISI:000468553100006,
Author = {Manuel Davila-Alcocer, Victor and Lopez-Ortega, Teresita and Vargas
   Pacheco, Ernesto},
Title = {{Pottery Through its Petrographic Analysis. Study of Pieces from the
   Archaeological Zone of El Tigre in Campeche, Mexico}},
Journal = {{INTERVENCION-REVISTA INTERNACIONAL DE CONSERVACION RESTAURACION Y
   MUSEOLOGIA}},
Year = {{2018}},
Volume = {{9}},
Number = {{18}},
Pages = {{54-64}},
Month = {{JUL-DEC}},
Abstract = {{The petrographic study of six fragments of ceramic pots extracted from
   the archaeological zone of El Tigre, Campeche, Mexico, is presented. The
   aim is to know the deterioration process that they have suffered so that
   it is possible to carry out conservation treatments for this type of
   pots. Assuming that the fragmentation and deterioration were due to the
   pressure of the soil and the in-situ interaction of its materials with
   those of the constituents of the pots, the petrographic analysis was
   carried out to determine the mineralogical composition and the state of
   alteration of the main minerals. Three mineralogical groups are
   reported: the first with a homogeneous composition, chalky; the second
   with volcanic constituents; and the third with a predominance of calcite
   fragments in the degreasers. These mineralogical changes are related to
   different sampling sites and correspond to different types of
   alterations, allowing the restorer to establish the most appropriate
   techniques.}},
