@article{ ISI:000447235800009,
Author = {Hernandez Ozuna, Paola Anahi and Morales Fajardo, Maria Esther},
Title = {{Should Mexico look at Latin America? The possibilities of Mexico's
   foreign policy in the Community of Latin America and Caribbean States}},
Journal = {{OASIS-OBSERVATORIO DE ANALISIS DE LOS SISTEMAS INTERNACIONALES}},
Year = {{2018}},
Number = {{28}},
Pages = {{151-170}},
Month = {{JUL-DEC}},
Abstract = {{This document identifies if there are possibilities for Mexican foreign
   policy within the Community of Latin-American and Carib-bean States
   (Celac) to achieve a major regional projection. Foreign policy is
   analyzed from a procedural perspective and the historical contingency
   and normative and ideational structures of Celac are highlighted, as a
   mechanism of political dialogue at the highest level in the region. It
   is concluded there is no concrete strategy on behalf of Mexican foreign
   policy within Celac because this regional space has not represented a
   tangible and short-term priority for the current government, even though
   Mexico was one of the promoters of its creation.}},
