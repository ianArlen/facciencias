@article{ ISI:000438355200003,
Author = {Xue, Hui-Fang and Fu, Jian-Ning and Fox-Machado, L. and Shi, Jian-Rong
   and Zhou, Yu-Tao and Zhang, Jun-Bo and Michel, R. and Yan, Hong-Liang
   and Niu, Jia-Shu and Zong, Wei-Kai and Su, Jie and Castro, A. and
   Ayala-Loera, C. and Altamirano-Devora, L.},
Title = {{Asteroseismology of the Double-mode High-amplitude delta Scuti Star VX
   Hydrae}},
