@article{ ISI:000439528000023,
Author = {Ovalle-Encinia, Oscar and Pfeiffer, Heriberto and Ortiz-Landeros, Jose},
Title = {{CO2 Separation Improvement Produced on a Ceramic-Carbonate Dense
   Membrane Superficially Modified with Au-Pd}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
Year = {{2018}},
Volume = {{57}},
Number = {{28}},
Pages = {{9261-9268}},
Month = {{JUL 18}},
Abstract = {{Ceramic-carbonate membranes have been proposed for the selective
   separation of CO2. In previous reports, membranes performance has been
   enhanced through the improvement of microstructural features and
   conductivity properties. Different to the aforesaid, this paper was
   focused on modifying the membrane surface by incorporating metallic
   particles to promote the involved surface reactions. First, a composite
   made of a Ce0.85Sm0.15O2-delta and Sm0.6Sr0.4Al0.3Fe0.7O3-delta was
   chemically synthesized. Then porous supports were obtained by pressing
   powders and sintering. Then, dense membranes were fabricated by
   infiltration of the supports with molten carbonates and the subsequent
   deposition of metallic Au-Pd particles on membrane feed side surface.
   Obtained membranes were tested for CO2 separation between 700 degrees C
   and 900 degrees C, using different feed gas mixtures. Membranes show
   excellent CO2 permeance (1.72 x 10(-7) mol m(-2) s(-1) Pa-1) operating
   at low CO2 partial pressure in the feed side (P-CO2 = 0.115 atm),
   wherein about 24\% of the total permeation values resulted from the
   surface modification approach. The permeation mechanism is discussed on
   the basis of these results.}},
