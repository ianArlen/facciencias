@article{ ISI:000439798200002,
Author = {Morales, Luis B. and Bulutoglu, Dursun A.},
Title = {{On E(s(2))-optimal and minimax-optimal supersaturated designs with 20
   rows and 76 columns}},
Journal = {{JOURNAL OF COMBINATORIAL DESIGNS}},
Year = {{2018}},
Volume = {{26}},
Number = {{7}},
Pages = {{344-355}},
Month = {{JUL}},
Abstract = {{We derive a previously unknown lower bound of 41 for the frequency of s
   max =8 of an E(s(2))-optimal and minimax-optimal supersaturated design
   (SSD) with 20 rows and 76 columns. This is accomplished by an exhaustive
   computer search that uses the combinatorial properties of resolvable 2 -
   (20, 10, 36) designs and the parallel class intersection pattern method.
   We also classify all nonisomorphic E(s(2))-optimal 4-circulant SSDs with
   20 rows and s(max) = 8.}},
