@article{ ISI:000435930900009,
Author = {Eduardo Molina-Guerrero, Carlos and de la Rosa, Guadalupe and
   Castillo-Michel, Hiram and Sanchez, Arturo and Garcia-Castaneda,
   Concepcion and Hernandez-Rayas, Angelica and Valdez-Vazquez, Idania and
   Suarez-Vazquez, Santiago},
Title = {{Physicochemical Characterization of Wheat Straw during a Continuous
   Pretreatment Process}},
Journal = {{CHEMICAL ENGINEERING \& TECHNOLOGY}},
Year = {{2018}},
Volume = {{41}},
Number = {{7}},
Pages = {{1350-1358}},
Month = {{JUL}},
Abstract = {{The changes occurring during the depolymerization of lignocellulosic
   biomasses are not yet fully understood. Synchrotron micro-Fourier
   transform infrared (mu-FTIR), Raman spectroscopy (RS), X-ray diffraction
   (XRD), and X-ray fluorescence (XRF) were applied for better
   characterization of wheat straw fibers during a continuous pretreatment
   process in terms of conditioning (C), extrusion (E), steam explosion
   (SE), and enzymatic hydrolysis (EH). mu-FTIR revealed functional groups
   as phenylpropanoid polymers, ethers, and aliphatic alcohol. RS detected
   acetoacetate, methyl and phenol groups after SE. The crystallinity index
