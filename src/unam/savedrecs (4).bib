@article{ ISI:000435047900004,
Author = {Hoelzl, Georg and Sohlenkamp, Christian and Angel Vences-Guzman, Miguel
   and Gisch, Nicolas},
Title = {{Headgroup hydroxylation by OlsE occurs at the C4 position of ornithine
   lipid and is widespread in proteobacteria and bacteroidetes}},
Journal = {{CHEMISTRY AND PHYSICS OF LIPIDS}},
Year = {{2018}},
Volume = {{213}},
Pages = {{32-38}},
Month = {{JUL}},
Abstract = {{Amino acid-containing acyloxyacyl lipids are composed of a 3-hydroxy
   fatty acid amide-bound to the a-amino group of an amino acid. A second
   fatty acid is ester-linked to the 3-hydroxy group of the first fatty
   acid. Most commonly, ornithine is the headgroup of these lipids, but
   glycine, serineglycine, glutamine and lysine have also been described in
   bacteria. Ornithine lipids (OL) can be synthesized by about 50\% of the
   sequenced bacterial species, and several covalent modifications of its
   basic structure have been described. The OL hydroxylase OlsE is
   widespread in Rhizobium and Agrobacterium species and is responsible for
   introducing a hydroxyl group at a hence unknown position within the
   omithine headgroup causing the formation of the OL named S2. Using NMR
   on purified OL S2, we show that the OlsE-mediated hydroxylation takes
   place at the C-4 position of the omithine headgroup. Furthermore, we
   identify a hydroxylase in the genome of Pseudopedobacter saltans,
   distantly related to OlsE from a-proteobacteria, able to hydroxylate the
   headgroup of both omithine lipids and lysine lipids. A homology search
   with the amino acid sequence of this hydroxylase allows us to predict
   that OL headgroup hydroxylation is not restricted to a few
   a-proteobacteria, but is apparently also common in many genera belonging
   to the Cytophaga-Flavobacterium-Bacteroidetes (CFB) group of bacteria.}},
