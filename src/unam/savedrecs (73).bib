@article{ ISI:000292362500005,
Author = {Massin, Claude and Hendrickx, Michel E.},
Title = {{Deep-water Holothuroidea (Echinodermata) collected during the TALUD
   cruises off the Pacific coast of Mexico, with the description of two new
   species}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
