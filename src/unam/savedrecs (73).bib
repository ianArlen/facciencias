@article{ ISI:000292362500003,
Author = {Soto-Trejo, Fabiola and Palomino, Guadalupe and Villasenor, Jose Luis},
Title = {{Chromosome numbers in Asteraceae of the Ecological Reserve of the
   Pedregal de San Angel (REPSA), Mexico City, Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2011}},
Volume = {{82}},
Number = {{2}},
Pages = {{383-393}},
Month = {{JUN}},
Abstract = {{Mitotic or meiotic chromosome numbers are recorded for 31 species of
   Asteraceae growing in the Ecological Reserve of the Pedregal de San
   Angel (REPSA), Mexico City. The chromosome counts of 3 species endemic
   to Mexico are recorded for the first time: Acourtia cordata (2n= 54),
   Ageratina cilyndrica (2n= 34), and Viguiera buddleiiformis (n= 17). The
   results are discussed in relation to the available data for each of the
   analyzed taxa.}},
