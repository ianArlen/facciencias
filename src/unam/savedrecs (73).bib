@article{ ISI:000292021100004,
Author = {Lisboa, Fabio da Silva and Carbajal Arizaga, Gregorio Guadalupe and
   Wypych, Fernando},
Title = {{Esterification of Free Fatty Acids Using Layered Carboxylates and
   Hydroxide Salts as Catalysts}},
Journal = {{TOPICS IN CATALYSIS}},
Year = {{2011}},
Volume = {{54}},
Number = {{8-9}},
Pages = {{474-481}},
Month = {{JUN}},
Abstract = {{Layered lanthanum and copper(II) laurates were tested as catalysts for
   methanolic esterification of lauric acid (C-12). Factorial design
   evidenced a positive effect of reaction temperature and amount of
   catalyst on ester conversion, while the acid: alcohol molar ratio (MR)
   had a negative effect. With lanthanum laurate, 90.5\% conversion was
   reached at 140 degrees C with 10\% catalyst and MR of 6:1. Although the
   highest conversion with copper(II) laurate converted 81.5\% under the
   same conditions, results were better with a lower amount of catalyst
   (78\% ester at 140 degrees C with 2\% catalyst and MR of 6:1). A calcium
   hydroxide succinate was also investigated showing better catalytic
   performance than the well-known CaO, under mild reaction conditions (120
   degrees C with 1\% catalyst and MR of 3:1). Although layered
   carboxylates can be successively reused, the layered calcium hydroxide
   salt transformed into inactive CaCO3 in the second reuse reaction.}},
