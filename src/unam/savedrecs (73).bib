@article{ ISI:000291944700034,
Author = {Chacon-Camacho, Oscar F. and Vazquez, Johanna and Zenteno, Juan C.},
Title = {{Expanding the Phenotype of Gingival Fibromatosis-Mental
   Retardation-Hypertrichosis (Zimmermann-Laband) Syndrome}},
Journal = {{AMERICAN JOURNAL OF MEDICAL GENETICS PART A}},
Year = {{2011}},
Volume = {{155A}},
Number = {{7}},
Pages = {{1716-1720}},
Month = {{JUL}},
Abstract = {{Zimmermann-Laband syndrome (ZLS) is a rare disorder characterized by
   gingival fibromatosis, hypertrichosis, intellectual disability, and
   absence and/or hypoplasia of the nails or terminal phalanges of the
   hands and feet. The syndromic features of ZLS are highly variable and
   can overlap with other entities featuring gingival fibrosis. This study
   describes a patient with ZLS with novel findings, including
   colpocephaly, hemivertebra, polydactyly, hyperpigmentation, and
   hemihyperplasia. Thus, the present report expands the phenotypic
   spectrum of this uncommon syndrome. (C) 2011 Wiley-Liss, Inc.}},
