@article{ ISI:000290322200005,
Author = {Bengochea, Abimael and Falconi, Manuel and Perez-Chavela, Ernesto},
Title = {{Symmetric horseshoe periodic orbits in the general planar three-body
   problem}},
Journal = {{ASTROPHYSICS AND SPACE SCIENCE}},
Year = {{2011}},
Volume = {{333}},
Number = {{2}},
Pages = {{399-408}},
Month = {{JUN}},
Abstract = {{We present some families of horseshoe periodic orbits in the general
   planar three-body problem for the case of two equal masses. The
   considered system is a symmetric version of the one formed by Saturn,
   Janus and Epimetheus. We use a mass ratio equal to 35x10(-5),
   corresponding to 10(5) times the Saturn-Janus mass parameter of the
   restricted case; for this mass ratio the satellites have a significantly
   bigger influence on the planet than in the classical Saturn, Janus and
   Epimetheus system. To obtain periodic orbits, we search those horseshoe
   orbits passing through two reversible configurations. A particular kind
   of periodic orbits where the minor bodies follow the same path is
   discussed.}},
