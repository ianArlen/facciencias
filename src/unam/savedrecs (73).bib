@article{ ISI:000291568100137,
Author = {Rivera-Munoz, E. M. and Huirache-Acuna, R. and Velazquez, R. and
   Alonso-Nunez, G. and Eguia-Eguia, S.},
Title = {{Growth of Hydroxyapatite Nanoparticles on Silica Gels}},
