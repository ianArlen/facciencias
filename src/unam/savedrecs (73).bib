@article{ ISI:000291568100119,
Author = {Tuzovskaya, I. and Lima, E. and Bosch, P. and Bogdanchikova, N. and
   Pestryakov, A. and Fraissard, J.},
Title = {{Influence of Cation Nature on Stabilization of Gold Nanospecies in
   Mordenites}},
