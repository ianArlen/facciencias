@article{ ISI:000292830900008,
Author = {Chakrabarti, Gopal and Shome, Debasish and Kumar, Subhasish and
   Armstrong-Altrin, John S. and Sial, Alcides N.},
Title = {{Carbon and oxygen isotopic variations in stromatolitic dolomites of
   Palaeoproterozoic Vempalle Formation, Cuddapah Basin, India}},
Journal = {{CARBONATES AND EVAPORITES}},
Year = {{2011}},
Volume = {{26}},
Number = {{2}},
Pages = {{181-191}},
Month = {{JUL}},
Abstract = {{The marine stromatolitic dolomites of Palaeoproterozoic Vempalle
   Formation, of Cuddapah Basin, South India show significantly depleted
   delta O-18 (-8.19 to -18.25aEuro degrees) and delta C-13 (-0.03 to
   -5.74) values and are consistent with the global Proterozoic data. A
   comparison of the stable isotope values from major localities of
   Proterozoic Basins of India has also been attempted in this study.
   Significantly depleted delta O-18 values of Vempalle carbonate samples
   could not be considered to be post-depositional modifications as there
   exists an inverse correlation relationship between C and O isotopic
   values. The Vempalle stromatolitic dolomite is likely to be precipitated
   in equilibrium with marine waters significantly depleted in oxygen
   isotopes (-10 to -15aEuro degrees SMOW) with respect to modern sea
   water. The negative delta C-13 values of the Vempalle carbonates are
   likely to be interpreted at the backdrop of global ``icehouse{''} as
   there is an indication of dropstone-like features in the lower part of
   the Vempalle Formation just 2 km, southwest of the study area.}},
