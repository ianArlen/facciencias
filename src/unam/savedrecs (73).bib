@article{ ISI:000292804100005,
Author = {Balvanera, Patricia and Quijas, Sandra and Perez-Jimenez, Alfredo},
Title = {{Distribution Patterns of Tropical Dry Forest Trees Along a Mesoscale
   Water Availability Gradient}},
