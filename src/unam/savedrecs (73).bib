@article{ ISI:000290959000017,
Author = {Ortiz, Victor and Aleman, Gabriela and Escamilla-Del-Arenal, Martin and
   Recillas-Targa, Felix and Torres, Nimbe and Tovar, Armando R.},
Title = {{Promoter characterization and role of CRE in the basal transcription of
   the rat SNAT2 gene}},
Journal = {{AMERICAN JOURNAL OF PHYSIOLOGY-ENDOCRINOLOGY AND METABOLISM}},
Year = {{2011}},
Volume = {{300}},
