@article{ ISI:000293763600005,
Author = {Raymundo-Raymundo, Elias and Lucia Prado-Pano, Blanca and
   Nikolskii-Gavrilov, Iourii and Rodolfo Mendoza-Hernandez, Jose Hipolito},
Title = {{Sorption, degradation and leaching of imazapyr in volcanic soil in
   Mexico}},
