@article{ ISI:000298267000004,
Author = {Garay-Palmett, K. and U'Ren, A. B. and Rangel-Rojo, R.},
Title = {{Tailored photon-pair sources based on inner-loop phasematching in
   fiber-based spontaneous four-wave mixing}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2011}},
Volume = {{57}},
Number = {{3, S}},
Pages = {{15-22}},
Month = {{JUL}},
Abstract = {{We study the generation of photon pairs by spontaneous four-wave mixing
   (SFWM) in photonic crystal optical fibers. We show that it is possible
   to engineer two-photon states with specific spectral entanglement
   properties suitable for quantum information processing applications. In
   this work, we concentrate on inner-loop phasematching, for which signal
   and idler photons can be generated at a small spectral separation from
   the pump, while sufficiently removed from the pump to ensure the absence
   of background photons produced by spontaneous Raman scattering. We
   present specific source designs which permit both symmetric and
   asymmetric factorable states, which allow heralding of pure-state,
   single-photon wavepackets without the need for spectral post-filtering.
   Additionally, we present numerical results for the conversion
   efficiency, where we compare sources based on inner- and outer-loop
   phasematching.}},
