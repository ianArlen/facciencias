@article{ ISI:000294026200004,
Author = {Atakishiyeva, M. K. and Atakishiyev, N. M.},
Title = {{Lifting q-Difference Operators for Askey-Wilson Polynomials and their
   Weight Function}},
Journal = {{PHYSICS OF ATOMIC NUCLEI}},
Year = {{2011}},
Volume = {{74}},
Number = {{6}},
Pages = {{837-845}},
Month = {{JUN}},
Abstract = {{We determine an explicit form of a q-difference operator that transforms
   the continuous q-Hermite polynomials H(n)(x vertical bar q) of Rogers
   into the Askey-Wilson polynomials pn(x; a, b, c, d vertical bar q) on
   the top level in the Askey q-scheme. This operator represents a special
   convolution-type product of four one-parameter q-difference operators of
   the form epsilon(q)(c(q)D(q)) (where c(q) are some constants), defined
   as Exton's q-exponential function epsilon(q)(z) in terms of the
   Askey-Wilson divided q-difference operator D(q). We also determine
   another q-difference operator that lifts the orthogonality weight
   function for the continuous q-Hermite polynomials H(n)(x vertical bar q)
   up to the weight function, associated with the Askey-Wilson polynomials
   p(n)(x; a, b, c, d vertical bar q).}},
