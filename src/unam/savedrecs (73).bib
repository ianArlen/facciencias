@article{ ISI:000292566700005,
Author = {Nielsen, Mogens Brondsted and Andersen, Lars H. and Rocha-Rinza, Tomas},
Title = {{Absorption tuning of the green fluorescent protein chromophore:
   synthesis and studies of model compounds}},
Journal = {{MONATSHEFTE FUR CHEMIE}},
