@article{ ISI:000291922200003,
Author = {van Tussenbroek, Brigitta I. and Barba Santos, M. Guadalupe},
Title = {{Demography of Halimeda incrassata (Bryopsidales, Chlorophyta) in a
   Caribbean reef lagoon}},
