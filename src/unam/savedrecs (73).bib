@article{ ISI:000295064000020,
Author = {Cornejo-Bravo, Jose M. and Ganem-Quintanar, Adriana and Pinon-Segundo,
   Elizabeth and Serrano-Medina, Aracely and Quintanar-Guerrero, David},
Title = {{Release Behavior and Bioadhesivity of a Hydrophobic Polyacid Gel}},
Journal = {{LATIN AMERICAN JOURNAL OF PHARMACY}},
Year = {{2011}},
Volume = {{30}},
Number = {{5}},
Pages = {{965-971}},
Month = {{JUN}},
Abstract = {{The release behavior of salicylic acid and the bioadhesion properties of
   crosslinked polyacid hydrophobic-gel disks, containing methyl
   methacrylate-co-methacrylic acid, were evaluated in simulated fluids. In
   gastric fluid a very low amount of drug was released with a minimal
   swelling and water uptake. The drug release was dependent of the drug
   precipitated in the interconnecting micropores of the disk structure.
   The transport and release type in this medium ranged from Fickian to
   zero order depending of drug loading. In intestinal fluid, disks swelled
   completely and the water absorption capacity was eight-fold its dry
   weight. In this case the released drug showed a biphasic behavior with a
   fast linear release (M-t/M-alpha, <= 0.6), followed by a slow drug
   release controlled by the gel swelling. Bioadhesion was measured by the
   tensiometer method on pig mucosae, the results indicated that these
   polyacid hydrogels are weak bioadhesives on both gastric and intestinal
   mucosae.}},
