@article{ ISI:000293135000031,
Author = {Fisher, David B. and Drory, Niv},
Title = {{DEMOGRAPHICS OF BULGE TYPES WITHIN 11 Mpc AND IMPLICATIONS FOR GALAXY
   EVOLUTION}},
