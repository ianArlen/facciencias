@article{ ISI:000292590100051,
Author = {Mamedov, Tofik and de Llano, Manuel},
Title = {{Generalized Superconducting Gap in an Anisotropic Boson-Fermion Mixture
   with a Uniform Coulomb Field}},
Journal = {{JOURNAL OF THE PHYSICAL SOCIETY OF JAPAN}},
