Title = {{Human fallen pose detection by using feature selection and a generative
   model}},
Booktitle = {{2017 IEEE INTERNATIONAL AUTUMN MEETING ON POWER, ELECTRONICS AND
   COMPUTING (ROPEC)}},
Series = {{IEEE International Autumn Meeting on Power Electronics and Computing}},
