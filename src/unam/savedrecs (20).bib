@article{ ISI:000393644900016,
Author = {Garcia-Huante, Yolanda and Cayetano-Cruz, Maribel and
   Santiago-Hernandez, Alejandro and Cano-Ramirez, Claudia and
   Marsch-Moreno, Rodolfo and Campos, Jorge E. and Aguilar-Osorio,
   Guillermo and Benitez-Cardoza, Claudia G. and Trejo-Estrada, Sergio and
   Eugenia Hidalgo-Lara, Maria},
Title = {{The thermophilic biomass-degrading fungus Thielavia terrestris Co3Bag1
   produces a hyperthermophilic and thermostable beta-1,4-xylanase with
   exo- and endo-activity}},
Journal = {{EXTREMOPHILES}},
Year = {{2017}},
Volume = {{21}},
Number = {{1}},
Pages = {{175-186}},
Month = {{JAN}},
Note = {{11th International Congress on Extremophiles, Kyoto, JAPAN, SEP 12-16,
   2016}},
Abstract = {{A hyperthermophilic and thermostable xylanase of 82 kDa (TtXynA) was
   purified from the culture supernatant of T. terrestris Co3Bag1, grown on
   carboxymethyl cellulose (CMC), and characterized biochemically. TtXynA
   showed optimal xylanolytic activity at pH 5.5 and at 85 A degrees C, and
   retained more than 90\% of its activity at a broad pH range (4.5-10).
   The enzyme is highly thermostable with a half-life of 23.1 days at 65 A
   degrees C, and active in the presence of several metal ions. Circular
   dichroism spectra strongly suggest the enzyme gains secondary structures
   when temperature increases. TtXynA displayed higher substrate affinity
   and higher catalytic efficiency towards beechwood xylan than towards
   birchwood xylan, oat-spelt xylan, and CMC. According to its final
   hydrolysis products, TtXynA displays endo-/exo-activity, yielded
   xylobiose, an unknown oligosaccharide containing about five residues of
   xylose and a small amount of xylose on beechwood xylan. Finally, this
   report represents the description of the first fungal hyperthermophilic
   xylanase which is produced by T. terrestris Co3Bag1. Since TtXynA
   displays relevant biochemical properties, it may be a suitable candidate
