@inproceedings{ ISI:000449997700019,
Author = {Padilla, Pablo and Knights, Francis and Ruiz, Adrian Tonatiuh and
   Tidhar, Dan},
Editor = {{AgustinAquino, OA and LluisPuebla, E and Montiel, M}},
Title = {{Identification and Evolution of Musical Style I: Hierarchical Transition
   Networks and Their Modular Structure}},
Booktitle = {{MATHEMATICS AND COMPUTATION IN MUSIC, MCM 2017}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2017}},
Volume = {{10527}},
Pages = {{259-278}},
Note = {{6th Biennial International Conference for Mathematics and Computation in
   Music (MCM), Univ Nacl Autonoma Mexico, Fac Sci, Mexico, MEXICO, JUN
   26-29, 2017}},
Abstract = {{The problem of identifying musical styles using mathematical tools is
   central not only in musicology and the mathematical theory of music, but
   also in applications to music pattern recognition and automated music
   generation in a particular idiom. In this paper we propose a methodology
   related to the transition network approach developed by D. Cope in his
   Experiments on Musical Intelligence, EMI. This extension allows for the
   possibility of defining stylistic cells at different scales as motifs
   and moduli of networks at the corresponding scale. It can be applied to
   study recursivity aspects of music. We also outline how this methodology
