@article{ ISI:000397106600089,
Author = {Rodriguez-Mendez, A. and Guzman, C. and Elizalde-Pena, Eduardo A. and
   Escobar-Alarcon, L. and Vega, Marina and Alvarado Rivera, Josefina and
   Esquivel, K.},
Title = {{Effluent Disinfection of Real Wastewater by Ag-TiO2 Nanoparticles
   Photocatalysis}},
