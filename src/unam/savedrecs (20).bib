@inproceedings{ ISI:000434340800028,
Author = {Urrutia, L. F.},
Editor = {{Kostelecky, VA}},
Title = {{Emergence of Gauge Invariance from Nambu Models}},
Booktitle = {{PROCEEDINGS OF THE SEVENTH MEETING ON CPT AND LORENTZ SYMMETRY}},
Year = {{2017}},
Pages = {{109-112}},
Note = {{7th Meeting on CPT and Lorentz Symmetry (CPT), Indiana Univ, Phys Dept,
   Bloomington, IN, JUN 20-24, 2016}},
Abstract = {{In the framework of a hamiltonian nonperturbative approach we show that
   after demanding current conservation together with the Gauss constraints
   at some initial time in a nonabelian Nambu model, we recover the
   corresponding Yang-Mills theory. In this way, the spontaneous Lorentz
   symmetry breaking present in the Nambu model becomes unobservable and
   the Goldstone modes can be identified with the corresponding gauge
   bosons.}},
