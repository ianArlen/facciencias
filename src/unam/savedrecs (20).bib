@inproceedings{ ISI:000429975308012,
Author = {Dominguez-Perez, D. N. and Dominguez-Vergara, N. and Dominguez-Perez, R.
   M. and Pantoja-Gallegos, J. L.},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{A FUTURE OF ISOLATION: CONTROL AND AUTOMATION IN THE DEVELOPING
   COUNTRIES}},
