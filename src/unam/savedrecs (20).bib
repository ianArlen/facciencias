Title = {{Subclinical Doses of ATP-Sensitive Potassium Channel Modulators Prevent
   Alterations in Memory and Synaptic Plasticity Induced by Amyloid-beta}},
Journal = {{JOURNAL OF ALZHEIMERS DISEASE}},
Year = {{2017}},
Volume = {{57}},
