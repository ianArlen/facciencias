@inproceedings{ ISI:000432419900041,
Author = {Sanchez, E. and Garcia-Rios, V. N. and Mercado, M.},
Editor = {{ArnalBailera, A and BeltranPellicer, P and DeLaVega, MLC and Yanez, JC}},
