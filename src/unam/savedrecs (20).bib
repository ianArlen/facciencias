@article{ ISI:000434876100005,
Author = {Antonio Davalos-Orozco, Luis},
Title = {{SIDEBAND THERMOCAPILLARY INSTABILITY OF A THIN FILM COATING THE OUTSIDE
   OF A THICK WALLED CYLINDER WITH FINITE THERMAL CONDUCTIVITY IN THE
   ABSENCE OF GRAVITY}},
