@inproceedings{ ISI:000427596701166,
Author = {Maltseva, O. A. and Sergeeva, M. A. and Gonzalez-Esparza, J. -A. and de
   la Luz, V.},
Book-Group-Author = {{IEEE}},
Title = {{Possible Enhancements of TEC and Their Effects on Radio Propagation in
   Mexican Region}},
Booktitle = {{2017 PROGRESS IN ELECTROMAGNETICS RESEARCH SYMPOSIUM - SPRING (PIERS)}},
Year = {{2017}},
Pages = {{1867-1874}},
Note = {{Progress in Electromagnetics Research Symposium - Spring (PIERS), St
   Petersburg, RUSSIA, MAY 22-25, 2017}},
Organization = {{Electromagnet Acad; St Petersburg State Univ; Tomsk Polytechn Univ; Univ
   Gavle; Swedish Inst; Inst Elec \& Elect Engineers; IEEE Geoscience \&
   Remote Sensing Soc; Zhejiang Univ, Coll Informat Sci \& Elect Engn; Sino
   Swedish Joint Res Ctr Photon; Zhejiang Univ, Electromagnet Acad}},
Abstract = {{In this paper, we study the enhancements of the total electron content
   (TEC) according to maps GIM (JPL) in the period of minimum solar
   activity (2006-2009). The difference from previous papers is the choice
   of the Mexican region, the observation period. The total electron
   content of the ionosphere was selected as a parameter. It was revealed
   76 positive cases and found that the TEC enhancements appear already at
   Dst 20 nT. The largest number of cases is related to Dst 40 nT. There is
   a certain linear trend showing an increase in the maximum deviations in
   the amplification of the disturbance, but the dispersion of the values
   is large and the correlation is absent. Enhancement factors for TEC are
   in the range 1.1-2.15 with a mean of 1.46. Maximum relative deviations
   of TEC are in range from 20 to 120\% with a mean of 46.4\%. The presence
   of positive events was confirmed by data of CHAMP satellite. The global
   distribution of the TEC allowed obtaining longitudinal width of the
   enhancement, which depends on latitude, season, geomagnetic conditions.
   Comparison of the appearance of positive events and enhancement factors
   in adjacent areas reveals in some cases significant differences,
   highlighting the need for local monitoring of TEC.}},
