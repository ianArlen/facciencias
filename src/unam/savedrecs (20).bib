@article{ ISI:000391647800033,
Author = {Hansberg-Pastor, Valeria and Gonzalez-Arenas, Aliesha and
   Camacho-Arroyo, Ignacio},
Title = {{CCAAT/enhancer binding protein beta negatively regulates progesterone
   receptor expression in human glioblastoma cells}},
Journal = {{MOLECULAR AND CELLULAR ENDOCRINOLOGY}},
Year = {{2017}},
Volume = {{439}},
Number = {{C}},
Pages = {{317-327}},
Month = {{JAN 5}},
Abstract = {{Many progesterone (P4) actions are mediated by its intracellular
   receptor (PR), which has two isoforms (PR-A and PR-B) differentially
   transcribed from separate promoters of a single gene. In glioblastomas,
   the most frequent and aggressive brain tumors, PR-B is the predominant
   isoform. In an in silico analysis we showed putative CCAAT/Enhancer
   Binding Protein (C/EBP) binding sites at PR-B promoter. We evaluated the
   role of C/EBP beta in PR-B expression regulation in glioblastoma cell
   lines, which expressed different ratios of PR and C/EBP beta isoforms
   (IAP1, LAP2, and LIP). ChIP assays showed a significant basal binding of
   CiEBK3, specific protein 1 (Spl) and estrogen receptor alpha (ER alpha)
   to PR-B promoter. C/EBP beta knockdown increased PR-B expression and
   treatment with estradiol (E2) reduced C/EBP beta binding to the promoter
   and up-regulated PR-B expression. P4 induced genes were differently
   regulated when CEBP/beta was silenced. These data show that C/EBP beta
   negatively regulates PR-B expression in glioblastoma cells. (C) 2016
   Elsevier Ireland Ltd. All rights reserved.}},
