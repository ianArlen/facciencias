@article{ ISI:000432211000006,
Author = {Salazar Lama, Daniel},
Title = {{Maya Lords and the Recreation of Mythical Episodes on Sculptural
   Programs Integrated in Architecture}},
Journal = {{ESTUDIOS DE CULTURA MAYA}},
Year = {{2017}},
Volume = {{49}},
Pages = {{165-199}},
Abstract = {{The progressive incorporation of royal potraits into sculptural programs
   integrated in architecture shows that rulers frequently adopted certain
   behavioral characteristics related to the gods by reenacting
   mythical/cosmological events. This study focuses on the Balamku frieze
   (Campeche) and the stucco reliefs of the Great Central Acropolis in El
   Mirador (Peten, Guatemala). Both monuments provide important information
   about the compositional strategies employed in such scenes. The results
   suggest that in the absence of texts. the use of a visual code with a
   high degree of symbolic expression and the proper construction of the
   image are capable of conveying messages with a strong ideological
   backgrounds that reflect to the social functions of the Maya rulers.}},
