@inproceedings{ ISI:000416372100027,
Author = {Guerrero-Navarro, Gustavo and Bijker, Roelof},
Book-Group-Author = {{IOP}},
Title = {{Contribution of sea quarks to the electromagnetic decay of decuplet
   baryons}},
Booktitle = {{31ST ANNUAL MEETING OF THE DIVISION OF PARTICLES AND FIELDS (DPYC) OF
   THE MEXICAN PHYSICAL SOCIETY}},
Series = {{Journal of Physics Conference Series}},
Year = {{2017}},
