@article{ ISI:000393089600024,
Author = {Alejandra Suarez-Ortiz, G. and Cerda-Garcia-Rojas, Carlos M. and
   Fragoso-Serrano, Mabel and Pereda-Miranda, Rogelio},
Title = {{Complementarity of DFT Calculations, NMR Anisotropy, and ECD for the
   Configurational Analysis of Brevipolides K-O from Hyptis brevipes}},
Journal = {{JOURNAL OF NATURAL PRODUCTS}},
Year = {{2017}},
Volume = {{80}},
