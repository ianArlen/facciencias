@article{ ISI:000428694500001,
Author = {Hernandez-Cervantes, Rosalia and Mendez-Diaz, Monica and
   Prospero-Garcia, Oscar and Morales-Montor, Jorge},
Title = {{Immunoregulatory Role of Cannabinoids during Infectious Disease}},
Journal = {{NEUROIMMUNOMODULATION}},
