Address = {{CAMINO VERA S-N, VALENCIA, 46022, SPAIN}},
Type = {{Proceedings Paper}},
Language = {{Spanish}},
Affiliation = {{Figueroa, LS (Reprint Author), Univ Nacl Autonoma Mexico, Postgrad Artes \& Diseno, Mexico City, DF, Mexico.
   Serrano Figueroa, Luis, Univ Nacl Autonoma Mexico, Postgrad Artes \& Diseno, Mexico City, DF, Mexico.}},
DOI = {{10.4995/ANIAV.2017.5848}},
