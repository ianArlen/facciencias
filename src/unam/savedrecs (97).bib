@article{ ISI:000255801500010,
Author = {Gonzalez-Mellado, Alex Onar and De la Cruz-Reyna, Servando},
Title = {{A simplified equation of state for the density of silicate hydrous
   magmas: An application to the Popocatepetl buoyancy-driven dome growth
   process}},
Journal = {{JOURNAL OF VOLCANOLOGY AND GEOTHERMAL RESEARCH}},
Year = {{2008}},
Volume = {{171}},
Number = {{3-4}},
Pages = {{287-300}},
Month = {{APR 20}},
Abstract = {{We present a simplified equation of state (EOS) for calculating the
   density of hydrous siliceous magmas in a range of pressures,
   temperatures and dissolved water content of interest to study several
   magma conduit processes. A simpler linear form and a more precise
   non-linear, yet simple form of the equation, both valid in the range 0
   to 0.25 GPa, 800 to 1000 K, and 0 to 7 wt.\% H2O are proposed for the
   dacitic and andesitic Popocateptl magma melt compositions. We apply
   these equations to explain some aspects of the dome-forming effusive
   activity that has been observed at Popocatepetl volcano since 1996. This
   model poses a density reduction dependent mainly on the volatile
   contents of the magma filling the volcano conduit system as the driving
   buoyancy force causing magma extrusions and dome growth. (C) 2007
   Elsevier B.V. All rights reserved.}},
