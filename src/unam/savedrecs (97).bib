@article{ ISI:000254368700008,
Author = {de Cos, D. and Alvarez, G. and Garcia-Arribas, A. and Montiel, H. and
   Barandiaran, J. M. and Zamorano, R. and Valenzuela, R.},
Title = {{Low field magnetoimpedance in the GHz range}},
