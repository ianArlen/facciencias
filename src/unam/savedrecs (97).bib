@article{ ISI:000254758700005,
Author = {Rozas, M.},
Title = {{A dynamical model for the spiral galaxy NGC 3359}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2008}},
Volume = {{44}},
Number = {{1}},
Pages = {{71-83}},
Month = {{APR}},
Abstract = {{In this work we have determined a dynamical model for NGC 3359. This is
   a strongly barred spiral galaxy with a complex structure, within which
   we can see an annular zone of intense star formation. In this work we
   present self consistent numerical simulations of molecular gas in NGC
   3359; and using the potential derived from an I band image, we attempt
   to find the parameters responsible for the kinematics and morphology of
   the observed gas. The best results are obtained for a two pattern speed
   model: the central region (up to 15{''}) corresponding to the nuclear
   bar which rotates with a Omega(s)=100 km s(-1) kpc(-1), and the main bar
   + outer spiral, decoupled from the nuclear region, which rotates with a
   angular velocity, Omega(p) = 27 km s(-1) kpc(-1). The model successfully
   reproduces the complex structure in Ha of the galaxy in the internal
   20{''} zone, which cannot really be explained with a unique mode
   pattern.}},
