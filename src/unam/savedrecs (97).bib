@article{ ISI:000256318700010,
Author = {Lopez-Serrano, F. and Lopez-Aguilar, J. E. and Mendizabal, E. and Puig,
   J. E. and Alvarez, J.},
Title = {{Microemulsion polymerization: Building a model using the experimental
   conversion and its derivative}},
Journal = {{REVISTA MEXICANA DE INGENIERIA QUIMICA}},
Year = {{2008}},
Volume = {{7}},
Number = {{1}},
Pages = {{79-87}},
Month = {{APR}},
Abstract = {{A model based on kinetic, thermodynamic and physicochemical concepts, is
   presented and an integrodifferential approach to find the fundamental
   events, is used for microemulsion polymerization (MEP)). The procedure
   was applied to experimental conversion data of hexyl methacrylate
   (C(6)MA) and styrene (STY) in MEP. It was found that: (i) nucleation
   rate is not linear with time, opposed to previous reports, (ii)
   coagulation rate is negligible, (iii) micelles provide monomer to
   reacting particles, (iv) a vitreous effect, even in polymers with a T-g
   lower than the reaction temperature, was found, (v) radical entry to
   particles is negligible confirming previous reports, (vi) the decreasing
   reaction rate interval, besides monomer depletion, is caused by a
   decrease in active reaction sites, and (vii) a three-parameter
   phenomenological model is presented capable of describing the studied
   systems.}},
