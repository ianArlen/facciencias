@article{ ISI:000254758700014,
Author = {Garcia-Diaz, Ma. T. and Henney, W. J. and Lopez, J. A. and Doi, T.},
Title = {{Velocity structure in the orion nebula. II. Emission line atlas of
   partially ionized to fully ionized gas}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2008}},
Volume = {{44}},
Number = {{1}},
Pages = {{181-212}},
Month = {{APR}},
Abstract = {{We present an atlas(4) of three-dimensional (position-position-velocity)
   spectra of the Orion Nebula in optical emission lines from a variety of
   different ionization stages: {[}OI] 6300 angstrom, {[}S II] 6716,6731
   angstrom, {[}N II] 6584 angstrom, {[}S III] 6312 angstrom, Ha 6563
   angstrom, and {[}O III] 5007 angstrom. These transitions provide point
   to point information about the physical structure and kinematics of the
   nebula at an effective resolution of 3{''} x 2{''} x 10 km s(-1),
   clearly showing the large scale behavior of the ionized gas and the
   presence of localized phenomena such as Herbig-Haro outflows. As an
   example application of the atlas, we present a statistical analysis of
   the widths of the H alpha, {[}O III], and {[}N II] lines that permits a
   determination of the mean electron temperature in the nebula of (9200
   +/- 400) K. We also find, in contradiction to previous claims, that the
   non-thermal line broadening is not significantly different between
   recombination lines and collisional lines.}},
