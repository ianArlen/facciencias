@article{ ISI:000257185600076,
Author = {Andrade, E. and Ramirez, E. B. and Alonso, J. C. and Rocha, M. F.},
Title = {{IBA of ZrO2 : Yb/Si thin films produced by the spray pyrolysis method}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
