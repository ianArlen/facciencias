@article{ ISI:000254758700003,
Author = {Jordan, E. and Araya, E. and Hofner, P. and Mateen, M. and Kurtz, S.},
Title = {{Haystack observations of CS and ch(3)oh toward star forming regions}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2008}},
Volume = {{44}},
Number = {{1}},
Pages = {{45-55}},
Month = {{APR}},
Abstract = {{We report (CS)-C-12 J = 1 -> 0 observations toward 20 star forming
   regions conducted with the MIT 37m Haystack telescope. Three of the
   sources were also observed in the (CS)-C-13 j = 1 -> 0 line, and five
   sources where observed in the CH3OH J(K) = 70 -> 6(1)A+ transition at 44
   GHz. We detected (CS)-C-13 and CH3OH emission toward all sources
   observed, and (CS)-C-12 emission toward 19 of the 20 sources in the
   sample, 11 of which are new detections. We found a strong correlation
   between the (CS)-C-12 FWHM and the source distance, indicating the
   potential use of (CS)-C-12 J = 1 -> 0 emission as a tool to estimate
   distances to star forming regions. We also report detection of CH3OH
   44GHz maser variability in most of the observed sources.}},
