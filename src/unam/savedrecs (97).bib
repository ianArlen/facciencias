@article{ ISI:000255827200006,
Author = {Sanchez-Sesma, Francisco J. and Perez-Ruiz, J. Alfonso and Luzon,
   Francisco and Campillo, Michel and Rodriguez-Castellanos, Alejandro},
Title = {{Diffuse fields in dynamic elasticity}},
