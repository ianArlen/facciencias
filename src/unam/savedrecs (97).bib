@article{ ISI:000256239600030,
Author = {Lopez-Sandoval, Horacio and Londono-Lemos, Milton E. and Garza-Velasco,
   Raul and Poblano-Melendez, Israel and Granada-Macias, Pilar and
   Gracia-Mora, Isabel and Barba-Behrens, Norah},
Title = {{Synthesis, structure and biological activities of cobalt(II) and
   zinc(II) coordination compounds with 2-benzimidazole derivatives}},
Journal = {{JOURNAL OF INORGANIC BIOCHEMISTRY}},
Year = {{2008}},
Volume = {{102}},
