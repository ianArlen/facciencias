@article{ ISI:000253106100093,
Author = {Valdez, S. and Genesca, J. and Campillo, B. and Flores, O. and Perez, R.
   and Juarez-Islas, J. A.},
Title = {{Effect of Mg addition on the tau phase distribution of an AlMn alloys
   modified with zinc}},
Journal = {{MATERIALS LETTERS}},
Year = {{2008}},
Volume = {{62}},
Number = {{6-7}},
Pages = {{1139-1142}},
Month = {{MAR 15}},
Abstract = {{The effect of Mg addition on the tau phase formation of an AlMn alloys
   modified with Zn, has been investigated by means of X-ray diffraction,
   scanning electron microscopy and transmission electron microscopy.,
   complemented by micro vickers hardness and galvanic efficiency. The
   results show the tau precipitates are formed when aged at 450 degrees C
   for 5 h in AlMnZn-(Mg) alloy with upper to 5.49\% Mg addition. The
   hardness and galvanic efficiency value of the aged alloy with 5.49\% Mg
   is always distinctly higher than that of the alloy in as-east condition
   with the same composition. Based on microstructural and galvanic
   efficiency results the quantity Of tau phase with Mg addition is
   discussed, in order to relate the tau distribution with the galvanic
   efficiency. (c) 2007 Elsevier B.V. All rights reserved.}},
