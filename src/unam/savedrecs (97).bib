@article{ ISI:000255066400008,
Author = {Francisco-Marquez, Misaela and Alvarez-Idaboy, J. Raul and Galano, Annia
   and Vivier-Bunge, Annik},
Title = {{Quantum chemistry and TST study of the mechanism and kinetics of the
   butadiene and isoprene reactions with mercapto radicals}},
Journal = {{CHEMICAL PHYSICS}},
Year = {{2008}},
Volume = {{344}},
Number = {{3}},
Pages = {{273-280}},
Month = {{MAR 13}},
Abstract = {{The reactions of isoprene and butadiene with SH center dot radicals have
   been investigated by density functional theory and ab initio molecular
   orbital theories. We report the thermodynamics and kinetics of four
   different pathways, involving addition of SH center dot radicals to all
   double-bonded carbon atoms. Calculations have been performed on all
   stationary points using BHandHLYP functional, Moller-Plesset
   perturbation theory to second-order (MP2) and the composite CBS-QB3
   method at the MP2 optimized geometries and frequencies. Pre-reactive
   complexes have been identified. The apparent activation energies are
   negative for SH center dot addition at the terminal carbon atoms and are
   slightly smaller than those for OH center dot addition at the same
   positions. The calculated overall rate coefficient for butadiene + SH
   center dot reaction at 298 K is in excellent agreement with the only
   available experimentally measured value. Activation energies and overall
   rate coefficients at different temperatures are predicted for the first
   time for butadiene + SH center dot and isoprene + SH center dot
   reactions. The reactions of butadiene and isoprene with SH center dot
   radicals were found to be about four times faster than with OH center
   dot radicals. (C) 2008 Elsevier B.V. All rights reserved.}},
