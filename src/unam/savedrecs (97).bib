@article{ ISI:000255457100039,
Author = {Bradler, K. and Jauregui, R.},
Title = {{Relativistically covariant state-dependent cloning of photons}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2008}},
Volume = {{77}},
Number = {{4}},
Month = {{APR}},
Abstract = {{The influence of the relativistic covariance requirement on the
   optimality of the symmetric state-dependent 1 -> 2 cloning machine is
   studied. Namely, given a photonic qubit whose basis is formed from the
   momentum-helicity eigenstates, the change to the optimal cloning
   fidelity is calculated taking into account the Lorentz covariance
   unitarily represented by Wigner's little group. Among other, we found
   states for which the optimal fidelity of the cloning process drops to
   2/3 which corresponds to the fidelity of the optimal classical cloner.
   Also, an implication for the security of the Bennett-Brassard 1984
   protocol is analyzed.}},
