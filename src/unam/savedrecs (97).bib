@article{ ISI:000209891900006,
Author = {Blackburn, Jason K. and Curtis, Andrew and Mujica, Frances Currin and
   Jones, Farrell and Dorn, Patricia and Coates, Rosamond},
Title = {{The Development of the Chagas' Online Data Entry System (CODES-GIS)}},
