@article{ ISI:000256090600014,
Author = {Flores-Romero, E. and Rangel-Rojo, R. and Marquez, H. and Vazquez, G. V.
   and Rickards, J. and Trejo-Luna, R.},
Title = {{Self-pulsing behavior in a Nd : YAG ion-implanted waveguide laser}},
Journal = {{LASER PHYSICS}},
Year = {{2008}},
Volume = {{18}},
Number = {{5}},
Pages = {{637-640}},
Month = {{MAY}},
Abstract = {{We report the observation of sustained self-pulsing behavior of the
   output of a channel waveguide Nd:YAG laser under a CW pump. The duration
   and repetition rate of the pulses observed were dependent on the pumping
   power. At the maximum pumping power available (44 mW of absorbed power),
   the pulse duration was around similar to 40 ns, and the pulse repetition
   rate was similar to 1.4 MHz. We discuss the possible physical mechanisms
   that generate the pulsing behavior, and their implications on the
