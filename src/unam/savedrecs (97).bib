@article{ ISI:000254810300044,
Author = {Mccauley, Ross A. and Cortes-Palomec, Aurea C. and Oyama, Ken},
Title = {{Isolation, characterization, and cross-amplification of polymorphic
   microsatellite loci in Guaiacum coulteri (Zygophyllaceae)}},
Journal = {{MOLECULAR ECOLOGY RESOURCES}},
Year = {{2008}},
Volume = {{8}},
Number = {{3}},
Pages = {{671-674}},
Month = {{MAY}},
Abstract = {{Guaiacum coulteri is a dry forest hardwood species of conservation
   concern endemic to the Pacific coast of Mexico. Fifteen microsatellite
   markers were developed which show high levels of polymorphism across two
   populations with the number of alleles ranging from four to 21. Most
   loci additionally exhibited consistent multiple banding patterns,
   indicating the likely polyploidy of this species. All loci were tested
   for cross-amplification with most found to amplify well across the genus
   Guaiacum, although amplification in other related genera of the
   Zygophyllaceae was limited.}},
