@article{ ISI:000254173800062,
Author = {Ayala, A. and Martinez, M. and Paic, G. and Sanchez, G. Toledo},
Title = {{Modelling the hadronization of quark matter}},
Journal = {{JOURNAL OF PHYSICS G-NUCLEAR AND PARTICLE PHYSICS}},
Year = {{2008}},
Volume = {{35}},
Number = {{4}},
Month = {{APR}},
Note = {{International Conference on Strangeness in Quark Matter, Levoca,
   SLOVAKIA, JUN 24-29, 2007}},
Organization = {{Inst Expt Phys Slovak Acad Sci; Fac Sci PJ Safarik Univ}},
Abstract = {{We perform a variational Monte Carlo simulation to study the quark
   matter evolution to hadronic matter. Incorporating clustering at low
   densities and deconfinement at high densities, we compute the baryons
   and mesons formation as a function of energy density. We find that
   hadronization is achieved distinctly for each case. The variational
   parameter, which is related to the confining scale, for baryons stiffly
   increases when entering into the confining phase transition, while for
   mesons is smoothly increased. Our results may hint on the experimental
   observation of the proton-to-pion rate.}},
