@article{ ISI:000251774300015,
Author = {Jalbout, Abraham F.},
Title = {{Ammonlated solvation of excess electrons on molecular surfaces}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2008}},
Volume = {{108}},
Number = {{3}},
Pages = {{532-537}},
Month = {{MAR 5}},
Abstract = {{As previously asserted, we have proposed a set of hypothetical molecular
   surfaces that possessed an extended hydrogen-bonded network on one of
   the sides and hydrogen atoms on the opposite side. The uneven
   distribution of the OH groups (which increases the total dipole moment
   of the system) coupled to the partial positive charge of the hydrogen
   atoms creates charge pockets capable of trapping excess electrons. In
   this work we consider the ability of ammonia (NH3) in solvating excess
