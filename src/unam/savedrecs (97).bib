@article{ ISI:000254368700015,
Author = {Valenzuela, R. and Fessant, A. and Gieraltowski, J. and Tannous, C.},
Title = {{Effect of the metal-to-wire ratio on the high-frequency magnetoimpedance
   of glass-coated CoFeBSi amorphous microwires}},
