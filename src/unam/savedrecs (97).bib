@article{ ISI:000254657800011,
Author = {Wintergerst, Ana M. and Throckmorton, Gaylord S. and Buschang, Peter H.},
Title = {{Effects of bolus size and hardness on within-subject variability of
   chewing cycle kinematics}},
Journal = {{ARCHIVES OF ORAL BIOLOGY}},
