@article{ ISI:000255607800001,
Author = {Martinez-Camacho, Fernanda and Vazquez-Negrete, Javier and Lima, Enrique
   and Lara, Victor Hugo and Bosch, Pedro},
Title = {{Texture of nopal treated adobe: restoring Nuestra Senora del Pilar
   mission}},
Journal = {{JOURNAL OF ARCHAEOLOGICAL SCIENCE}},
Year = {{2008}},
Volume = {{35}},
Number = {{5}},
Pages = {{1125-1133}},
Month = {{MAY}},
Abstract = {{Materials eroded, non-eroded, and treated with the local nopal mucilage
   adobe, from the Nuestra Senora del Pilar mission located in Sonora
   Mexico, are compared. The treatment of adobe bricks with an
   alcohol-water solution prior to the nopal application is proposed in
   order to increase their resistance to erosion. Using X-ray diffraction,
   small angle X-ray scattering, scanning electron microscopy and nitrogen
   adsorption techniques, we compared the structural and textural
   properties of eroded and non-eroded adobe materials. In the samples
   treated with nopal it is shown how the nopal coats the adobe small
   particles. The nopal impregnated material is stable up to 200 degrees C.
   A non-conventional distinction among these materials is made through
