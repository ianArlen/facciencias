@article{ ISI:000256090600012,
Author = {Castillo-Matadamas, H. and Quintero-Torres, R.},
Title = {{Pump efficiency and beam quality control in an end-pumped Cr : LiSAF
   laser}},
Journal = {{LASER PHYSICS}},
Year = {{2008}},
Volume = {{18}},
Number = {{5}},
Pages = {{625-631}},
Month = {{MAY}},
Abstract = {{The laser radiation production in an end-pumped laser requires taking
   into consideration the following: (a) the optical pumping system, (b)
   the thermal and spectroscopic characteristics of the gain medium, (c)
   the mode overlap, and (d) the interaction with the optical resonator.
   The correlation between the previous four factors is needed to guide the
   design and evaluation of such a laser system. This suggestion is a
   better methodology than focusing the discussion on only one factor of
   the system ignoring interactions. Here, we use these arguments to
   supervise the complete process with a one-watt pump-diode laser largely
   astigmatic and a Cr:LiSAF gain medium highlighting the importance
   between interactions.}},
