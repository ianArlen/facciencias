@article{ ISI:000235227600029,
Author = {Soto-Jimenez, MF and Hibdon, SA and Rankin, CW and Aggarawl, J and
   Ruiz-Fernandez, AC and Paez-Osuna, F and Flegal, AR},
Title = {{Chronicling a century of lead pollution in Mexico: Stable lead isotopic
   composition analyses of dated sediment cores}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2006}},
Volume = {{40}},
Number = {{3}},
Pages = {{764-770}},
Month = {{FEB 1}},
Abstract = {{Analyses of lead isotopic compositions (Pb-204, Pb-206, Pb-207, and
   Pb-208)of dated sediment cores from two coastal estuaries and two inland
   lakes chronicle the predominance of industrial lead emissions in Mexico
   over the past century. These isotopic ratios exhibit a shift in
   composition from the turn of the previous century (1900) that
   corresponds with measurable increases (from 2- to 10-fold) in lead
   concentrations in the cores above their baseline values (3-22 mu
   g/g)-both changes are consistent with the development of Mexican lead
   production for export and the manufacture of tetraethyl lead additives
   for Mexican gasolines. While subsequent changes in lead concentrations
   in the cores correspond with calculated emissions from the combustion of
   leaded gasoline in Mexico, isotopic compositions of the cores remain
   relatively constant throughout most of the 1900s (e.g., Pb-206/Pb-207 =
   1.200 +/- 0.003; Pb-208/Pb-207 = 2.463 +/- 0.004). That isotopic
   constancy is attributed to the widespread pollution from lead production
   in Mexico and the dispersion of some of that lead used as an additive in
   Mexican gasolines.}},
