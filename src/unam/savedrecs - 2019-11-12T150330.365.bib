@article{ ISI:000236014100022,
Author = {Vazquez-Nin, GH and Echeverria, OM and Juarez-Chavero, S},
Title = {{Integration of morphological and cytophysiological studies on estrogen
   receptor}},
Journal = {{MOLECULAR AND CELLULAR ENDOCRINOLOGY}},
Year = {{2006}},
Volume = {{246}},
Number = {{1-2}},
Pages = {{163-164}},
Month = {{FEB 26}},
Note = {{International Symposium on Steroid Hormone Receptor Superfamily and
   Molecular Signaling, Trivandrum, INDIA, NOV 25-27, 2004}},
Abstract = {{Ultrastructural and immunocytochemical studies of an intra-nuclear
   particle, the perichromatin granule (PCG), demonstrated the presence of
   processed mRNA in this structure. Ovariectomy caused an increase in the
   number of PCGs in uterine cells and administration of estradiol
   drastically reduced the nuclear pool of PCGs in 15 min. In vitro studies
   demonstrated that this depletion was accompanied by an increase of the
   export of previously synthesized RNA. Similar quantitative changes of
   the abundance of PCG and of the rate of the export of RNA were found in
   ventral prostate after orchiectomy and testosterone restitution, as well
   as in the target cells of FSH, LH, TSH, and ACTH. These results taken
   together led us to conclude that PCGs constitute an intra-nuclear
   compartment of a few processed mRNA in equilibrium with transcription
   and export. This mRNA is rapidly transferred to the cytoplasm by
   specific hormone signals. (c) 2005 Elsevier Ireland Ltd. All rights
   reserved.}},
