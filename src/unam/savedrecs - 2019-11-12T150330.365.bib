@article{ ISI:000235718200002,
Author = {Gonzalez, S and Huddart, D and Bennett, MR and Gonzalez-Huesca, A},
Title = {{Human footprints in Central Mexico older than 40,000 years}},
Journal = {{QUATERNARY SCIENCE REVIEWS}},
Year = {{2006}},
Volume = {{25}},
Number = {{3-4}},
Pages = {{201-222}},
Month = {{FEB}},
Abstract = {{The timing route and origin of the first colonization to the Americas
   remains one of the most contentious topics in human evolution. A number
   of migration routes have been suggested and there are different views
   Lis to the antiquity of the earliest human occupation. Some believe that
   settlement happened as early as 30 ka BP, but most of the currently
   accepted early sites in North America date to the latest Pleistocene,
   related to the expansion of the Clovis culture, while the oldest
   directly radiocarbon dated human remains are 11.5 ka BP. In this context
   new evidence is presented in this paper, in the form of human footprints
   preserved in indurated volcanic ash, to suggest that Central Mexico was
   inhabited as early Lis over 40 ka BP.
   Human and animal footprints have been found within the upper bedding
   surfaces of the Xalnene volcanic ash layer that outcrops in the
   Valsequillo Basin, south of Puebla, Mexico. This ash layer was produced
   by a subaqueous monogenetic volcano erupting within a palaeo-lake,
   dammed by lava within the Valsequillo Basin during the Pleistocene. The
   footprints were formed during low stands in lake level along the former
   shorelines and indicate the presence Of humans, deer, canids, big
   fields, and probably camels and bovids. The footprints were buried by
   ash and lake sediments as lake levels rose and transgressed across the
   site. The ash has been dated to at least 40 ka BP by OSL dating of
   incorporated, baked lake sediments. (c) 2005 Elsevier Ltd. All rights
   reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Gonzalez, S (Reprint Author), Liverpool John Moores Univ, Sch Biol \& Earth Sci, Liverpool L3 3AF, Merseyside, England.
   Liverpool John Moores Univ, Sch Biol \& Earth Sci, Liverpool L3 3AF, Merseyside, England.
