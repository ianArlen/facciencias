@article{ ISI:000235120600010,
Author = {Sanchez-Gutierrez, M and Delgado-Buenrostro, NL and Zarate-Grande, M and
   Uribe, S and Mujica, A},
Title = {{In guinea pig spermatozoa, the procaine-promoted synchronous acrosome
   reaction results in highly fertile cells exhibiting normal F-actin
   distribution}},
Journal = {{REPRODUCTIVE TOXICOLOGY}},
Year = {{2006}},
Volume = {{21}},
Number = {{2}},
Pages = {{208-215}},
Month = {{FEB}},
Abstract = {{In guinea pig spermatozoa, procaine induces Ca2+ independent
   hyperactivated motility suggestive of sperm capacitation. Nonetheless,
   in the presence of high extracellular Ca2+, procaine increases
   cytoplasmic Ca2+. We analyze the procaine effect on the acrosome
   reaction (AR) processes in guinea pig spermatozoa. Results indicated
   that: (i) in spermatozoa pre-incubated 5-30 min in MCM-PLG medium,
   procaine produced synchronous AR, (ii) the acrosome-reacted sperm number
   increased with the capacitation period before procame, treatment and
   with procaine concentration, (iii) acrosome reaction was blocked when
   Ca2+ was omitted, (iv) plasma membrane-outer acrosomal membrane fusion
   started within 2 min after procaine treatment, (v) in acrosome-reacted
   spermatozoa, actin polymerization occurred and F-actin was located in
   the equatorial and post-acrosomal regions and (vi) procaine treatment
   resulted in highly fertile acrosome-reacted spermatozoa. This is the
   first report indicating that procame promotes synchronic AR in mammalian
   spermatozoa. If procaine promotes premature AR of spermatozoa in vivo,
   it might be a factor for infertility in patients exposed to this local
   anesthetic. (c) 2005 Elsevier Inc. All rights reserved.}},
