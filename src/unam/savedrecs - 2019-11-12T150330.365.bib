@article{ ISI:000235559600004,
Author = {Montellano-Ballesteros, JJ},
Title = {{On totally multicolored stars}},
Journal = {{JOURNAL OF GRAPH THEORY}},
Year = {{2006}},
Volume = {{51}},
Number = {{3}},
Pages = {{225-243}},
Month = {{MAR}},
Abstract = {{Given an edge-coloring of a graph G, a subgraph M of G will be called
   totally multicolored if no two edges of M receive the same color. Let
   h(G, K-1,K-q) be the minimum integer such that every edge-coloring of G
   using exactly h(G, K-1,K-q) colors produces at least one totally
   multicolored copy of K-1,K-q (the q-star) in G. In this article, an
   upper bound of h(G, K-1,K-q) is presented, as well as some applications
   of this upper bound. (c) 2005 Wiley Periodicals, Inc.}},
