@article{ ISI:000235242800008,
Author = {Carreon-Freyre, DC and Cerca, M},
Title = {{Delineating the near-surface geometry of the fracture system affecting
   the Valley of Queretaro, Mexico: Correlation of GPR signatures and
   physical properties of sediments}},
Journal = {{NEAR SURFACE GEOPHYSICS}},
Year = {{2006}},
Volume = {{4}},
Number = {{1}},
Pages = {{49-55}},
Month = {{FEB}},
