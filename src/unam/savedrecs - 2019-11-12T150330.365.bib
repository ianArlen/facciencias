@article{ ISI:000236467700089,
Author = {Castrejon-Pita, JR and Castrejon-Pita, AA and Huelsz, G and Tovar, R},
Title = {{Experimental demonstration of the Rayleigh acoustic viscous boundary
   layer theory}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2006}},
Volume = {{73}},
Number = {{3, 2}},
Month = {{MAR}},
Abstract = {{Amplitude and phase velocity measurements on the laminar oscillatory
   viscous boundary layer produced by acoustic waves are presented. The
   measurements were carried out in acoustic standing waves in air with
   frequencies of 68.5 and 114.5 Hz using laser Doppler anemometry and
   particle image velocimetry. The results obtained by these two techniques
   are in good agreement with the predictions made by the Rayleigh viscous
   boundary layer theory and confirm the existence of a local maximum of
   the velocity amplitude and its expected location.}},
