@article{ ISI:000239819400027,
Author = {Fuentes, L. and Fernandez, J. F. and Fuentes, Ma. E. and Lascano, L. and
   Villafuerte, M. E. and Montero, Ma. E. and Olivera, R. and Mehta, A. and
   Jardiel, T.},
Title = {{Synchrotron radiation study of structural tendencies in aurivillius
   ceramics}},
Journal = {{FERROELECTRICS}},
