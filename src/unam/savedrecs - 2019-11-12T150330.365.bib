@inproceedings{ ISI:000236109700035,
Author = {Cortes, LAP and Castellanos, H and Olguin, SRC and Estrada, V and Lopez,
   F and Lopez, I and Meza, I and Moreno, I and Perez, P and Rodriguez, C},
