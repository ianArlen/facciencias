@inproceedings{ ISI:000241606200006,
Author = {Pena, M. and Lopez, I. and Osorio, R.},
Editor = {{Ceballos, S}},
Title = {{Invariant object recognition robot vision system for assembly}},
Booktitle = {{CERMA2006: ELECTRONICS, ROBOTICS AND AUTOMOTIVE MECHANICS CONFERENCE,
   VOL 1, PROCEEDINGS}},
Series = {{Electronics Robotics and Automotive Mechanics Conference (CERMA)}},
