@article{ ISI:000235667700047,
Author = {Bagnoli, F and Rechtman, R},
Title = {{Synchronization universality classes and stability of smooth coupled map
   lattices}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2006}},
Volume = {{73}},
Number = {{2, 2}},
Month = {{FEB}},
Abstract = {{We study two problems related to spatially extended systems: the
   dynamical stability and the universality classes of the replica
   synchronization transition. We use a simple model of one-dimensional
   coupled map lattices and show that chaotic behavior implies that the
   synchronization transition belongs to the multiplicative noise
   universality class, while stable chaos implies that the synchronization
   transition belongs to the directed percolation universality class.}},
