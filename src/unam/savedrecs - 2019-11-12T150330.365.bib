@article{ ISI:000242272000012,
Author = {Ruiz, Ruben Cejudo and Goguitchaichvili, Avto and Geuna, Silvana E. and
   Alva-Valdivia, Luis M. and Sole, Jesus and Morales, Juan},
Title = {{Early cretaceous absolute geomagnetic paleointensities from Cordoba
   Province (Argentina)}},
Journal = {{EARTH PLANETS AND SPACE}},
Year = {{2006}},
Volume = {{58}},
Number = {{10}},
Pages = {{1333-1339}},
Note = {{Conference on Paleomagnetism and Tectonics in Latinamerica, New Orleans,
   LA, MAY, 2005}},
Organization = {{AGU}},
Abstract = {{We present here new paleointensity and geochronology results from Early
   Cretaceous volcanic rocks of Sierra Chica de Cordoba (Argentina). The
   new K-Ar isotopic ages of 5 samples range from 136 to 122 Ma. Twenty
   five samples from 7 individual flows yielded acceptable paleointensity
   estimates. The mean paleointensity values per flow are ranging from 53.0
   +/- 1.9 to 25.4 +/- 2.6 mu T and the corresponding Virtual Dipole
   Moments (VDMs) are ranging, from 9.3 +/- 1.3 to 4.6 +/- 0.5 (10(22)
   Am-2). This corresponds to the mean value of 7.3 +/- 1.7 x 10(22) Am-2,
   which is compatible to the present geomagnetic axial dipole. Currently
   available selected paleointensity data from 80 to 130 Ma suggest that
   geomagnetic field strength frequently fluctuated before and during the
   Cretaceous Normal Superchron while the magnetic polarity maintained
   stable. The mean paleointensities derived from Cordoba lavas agree
   remarkably well with those obtained from the Parana Magmatic Province
   (133-132 Ma). This reinforces the hypothesis about the unreliability of
   `Mesozoic Dipole Low'.}},
Publisher = {{SPRINGEROPEN}},
Address = {{CAMPUS, 4 CRINAN ST, LONDON, N1 9XW, ENGLAND}},
Type = {{Article; Proceedings Paper}},
Language = {{English}},
