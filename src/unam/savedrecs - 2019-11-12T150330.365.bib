@article{ ISI:000235807800006,
Author = {Lopez-Araiza, H and Ventura, JL and Lopez-Diazguerrero, NE and
   Gonzalez-Marquez, H and Gutierrez-Ruiz, MC and Zentella, A and
   Konigsberg, M and Araiza, P},
Title = {{Organ- and tissue-specific alterations in the anti-apoptotic protein
   Bcl-2 in CD1 female mice of different ages}},
Journal = {{BIOGERONTOLOGY}},
Year = {{2006}},
Volume = {{7}},
Number = {{1}},
Pages = {{63-67}},
Month = {{FEB}},
Abstract = {{The anti-apoptotic protein Bcl-2, which also has cytoprotective and
   antioxidant functions might be one of the crucial factors that
   altogether, establish how a cell may deal with stress and damage,
   contributing to longevity. Among the controversial issues to understand
   Bcl-2 functions in vivo, is to establish its content and variation in
   tissues during an organismal lifespan. In this work we analyzed the
   changes of Bcl-2 levels in lung, liver, heart, kidney, spleen and brain
   homogenates obtained from CD1 mice throughout their lifespan (newborn to
   24 months). A tendency of increment was observed in all the organs
   analyzed, except brain where Bcl-2 was not detected. Bcl-2
   over-expression during aging could be interpreted as a protective
   mechanism preventing cell death, despite the overall accumulated cell
   damage.}},
