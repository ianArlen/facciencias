@article{ ISI:000235633800002,
Author = {Garcia-Oliva, F and Lancho, JFG and Montano, NM and Islas, P},
Title = {{Soil Carbon and nitrogen dynamics followed by a forest-to-pasture
   conversion in western Mexico}},
Journal = {{AGROFORESTRY SYSTEMS}},
Year = {{2006}},
Volume = {{66}},
Number = {{2}},
Pages = {{93-100}},
Month = {{FEB}},
Abstract = {{Gains and losses of soil carbon (C), have been reported when tropical
   forests are converted to pastures. Regional studies are crucial for
   setting regional baselines and explaining each particular trend, in
   order to solve this controversy. Tropical deciduous forest (TDF) is
   under high deforestation pressure, mainly for conversion to pastures.
   The present study compared soil organic C (SOC) and nitrogen (SON) in
   the surface layer (0-5 cm) of forest and pasture soils in a TDF of
   western Mexico. SOC and SON concentrations were 18 and 60\% lower in
   pasture soils than in forest soils, and C:N ratio increased in pasture
   soils. Furthermore, pasture soils had lower labile C and available
   inorganic nitrogen (N) than forest soils. These results can be explained
   as a reduction in C inputs to pasture soils and management-induced
   disruption of soil aggregates. In forest soils, macroaggregates (> 250
   mu m) were predominant (85\%), whereas in pasture soils they were
   reduced to 35\% of dry sand-free soil mass. The estimated SOC and SON
   losses from the top 5 cm of soil were 3 Mg C ha(-1) and 0.9 Mg N ha(-1),
   respectively.}},
