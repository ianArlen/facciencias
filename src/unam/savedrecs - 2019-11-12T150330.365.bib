@inproceedings{ ISI:000236907100104,
Author = {Popa, G and Aprahamian, A and Bruce, AM and Hirsch, JG and Draayer, JP},
Editor = {{Woehr, A and Aprahamian, A}},
Title = {{K=0(+) bands in Gadolinium nuclei}},
Booktitle = {{CAPTURE GAMMA-RAY SPECTROSCOPY AND RELATED TOPICS}},
Series = {{AIP Conference Proceedings}},
Year = {{2006}},
Volume = {{819}},
Pages = {{587+}},
Note = {{12th International Symposium on Capture Gamma-Ray Spectroscopy and
   Related Topics, Univ Notre Dame, Notre Dame, IN, SEP 04-09, 2005}},
Abstract = {{The recent Gd-160(p,t)Gd-158 experiment{[}1] has raised again the
   question about the nature of the low-lying excited 0(+) states in
   deformed nuclei. New (p,t) experiments have resolved typically about ten
   low-lying excited 0(+) states{[}2]. Calculations using the 1BA model
   {[}3], the projected shell model {[}4] and the quasiparticle-phonon
   model {[}5] have answered partially this question and showed the
   complicated nature of these states.
   We study the energy levels, their wave function content, and the
   corresponding B(E2) transitions in (156),(158),Gd-160 nuclei, in the
   framework of the pseudo-SU(3) model. The theory uses a realistic
   Hamiltonian with single-particle energies and monopole pairing
   interactions in addition to the usual quadrupole-quadrupole term, with
   the pairing and quadrupole-quadrupole strengths taken from systematics.
   The calculations are carried out with a set of basis states with
   pseudo-spin zero proton and neutron configurations. The calculated B(E2)
   values suggest that the first excited 0(+) state might have a mixed
   rotional and collective vibrational character, where the
   latter-quasiparticle type excitations-are not considered in this
   calculation.}},
