@article{ ISI:000235327200006,
Author = {Massillon-JL, G and Gamboa-deBuen, I and Brandan, ME},
Title = {{Onset of supralinear response in TLD-100 exposed to Co-60 gamma-rays}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{2006}},
Volume = {{39}},
Number = {{2}},
Pages = {{262-268}},
Month = {{JAN 21}},
Abstract = {{The onset of the supralinear response of peaks 4-9 in LiF:Mg,Ti
   (TLD-100) induced by Co-60 gamma ray irradiation at doses from 4.18 mGy
   to 8.32 kGy has been studied reading the thermoluminescence signal at 8
   degrees C s(-1). Glow curves were decomposed into individual peaks by a
   controlled deconvolution procedure. The response of the low-temperature
   peaks (4 and 5) as a function of dose is linear up to approximate to
   1000-5000 mGy and supralinear for higher doses; the high-temperature
   peaks (6a-9) exhibit a supralinear behaviour at doses higher than about
   50-200 mGy. The f(D) supralinearity function has been evaluated,
   obtaining f(D)(max) equal to 3.6 +/- 0.3 for peak 5 and from 23 up to
   207 for the high temperature peaks. To assess the contribution to the
   glow curve from the high-temperature peaks, two methods were studied:
   ratios of peak heights (peak 7 with respect to peak 5) and ratios of
   areas of the deconvoluted high-temperature peaks (added area of peaks
   6b, 7 and 8) with respect to peak 5. The shape of the glow curve ceases
   to be constant, abruptly, at a dose near 100 mGy, if the ratio of areas
   is evaluated.}},
