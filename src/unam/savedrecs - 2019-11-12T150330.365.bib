@article{ ISI:000237403800003,
Author = {Samaniego-Hernandez, M and De Leon-Rodriguez, A and Aparicio-Fabre, R
   and Arias-Ortiz, C and de la Rosa, APB},
Title = {{Expression and purification of rotavirus proteins NSP5 and NSP6 in
   Escherichia coli}},
