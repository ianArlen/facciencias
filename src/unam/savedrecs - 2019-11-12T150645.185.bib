@article{ ISI:000175777900004,
Author = {Garcia, LE and Galvan, SC and Cardoso, EJ},
Title = {{Phylogenetic distance of Giardia intestinalis isolates from symptomatic
   and asymptomatic children}},
Journal = {{REVISTA DE INVESTIGACION CLINICA-CLINICAL AND TRANSLATIONAL
   INVESTIGATION}},
Year = {{2002}},
Volume = {{54}},
Number = {{2}},
Pages = {{113-118}},
Month = {{MAR-APR}},
Abstract = {{The purpose of the present investigation, was to analyze the sequences
   of 16S ribosomal genes partially amplified from 17 isolated Giardia
   intestinalis obtained from faeces of 13 children with intestinal
   pathology and four symptoms-free children with intestinal pathology.
   Analysis was made also with Giardia Portland-I and four Giardias
   isolated from dogs. Children ages in both groups ranged from six to
   twelve years. Genomic DNA was isolated using the
   phenol-chloro-form-isoamilic alcohol technique, and partial
   amplification of 16S rRNA ribosomal gene was carried out by the
   Polymerase Chain Reaction. Sequences of rDNA were compared with
   Portland-I by using CLUSTAL-W (1.81) and PHYLIP (3.6) software in order
   to determine phylogenic associations. Our results showed that only one
   isolate from symptoms-free children and three from the symptomatic
   children were associated with Portland I. The other isolated Giardias
   were associated between them and with two samples obtained from dogs
   that are phylogenetically distant from Portland-I. Two isolates from dog
   constituted a different group. These results suggest a zoonotic
   infection and presence of symptoms in children from the present
   investigation; probably it was associated with host or special
   characteristics of Giardia strains. The last two situations have a
   special epidemiological and public health interest.}},
Publisher = {{INST NACIONAL NUTRICION}},
Address = {{VASCO DE QUIROZA 15, COLONIA SECCION XVI, TLALPAN, MEXICO 14000 D F,
   MEXICO}},
Type = {{Article}},
