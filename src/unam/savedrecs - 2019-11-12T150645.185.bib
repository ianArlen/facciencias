@article{ ISI:000174401500005,
Author = {Bracho, J and Montejano, L},
Title = {{Helly-type theorems on the homology of the space of transversals}},
Journal = {{DISCRETE \& COMPUTATIONAL GEOMETRY}},
Year = {{2002}},
Volume = {{27}},
Number = {{3}},
Pages = {{387-393}},
Month = {{APR}},
Abstract = {{In this paper we ``measure{''} the size of the set of n-transversals of
   a family F of convex sets in R(n+k) according to its homological
   complexity inside the corresponding Grassmannian manifold. Our main
   result states that the ``measure{''} mu of the set of n-transversals of
   F is greater than or equal to k if and only if every k + I members of F
   have a common point and also if and only if for some integer in, I less
   than or equal to m less than or equal to n, and every subfamily V of F
   with k + 2 members, the ``measure{''} mu of the set of m-transversals of
   P is greater than or equal to k.}},
