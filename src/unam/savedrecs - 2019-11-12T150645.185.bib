@article{ ISI:000177932800022,
Author = {Bokhimi, X and Sanchez-Valente, J and Pedraza, F},
Title = {{Crystallization of sol-gel boehmite via hydrothermal annealing}},
Journal = {{JOURNAL OF SOLID STATE CHEMISTRY}},
