@article{ ISI:000175941700013,
Author = {Fuentes-Noriega, I and Ruiz-Ramirez, L and Tovar, AT and Rico-Morales, H
   and Gracia-Mora, I},
Title = {{Development and validation of a liquid chromatographic method for
   Casiopeina IIIi (R) in rat plasma}},
Journal = {{JOURNAL OF CHROMATOGRAPHY B-ANALYTICAL TECHNOLOGIES IN THE BIOMEDICAL
   AND LIFE SCIENCES}},
Year = {{2002}},
Volume = {{772}},
Number = {{1}},
Pages = {{115-121}},
Month = {{MAY 25}},
Abstract = {{A sensitive and specific liquid chromatographic method using extraction
   with zinc sulfate has been developed for the determination of Casiopeina
   IIIi and validated over the linear range 5-100 mug/ml in 1 ml of rat
   plasma. The analysis was performed on a Symmetry C-18 (5 mum) column.
   The mobile phase was methanol: 0.01 M phosphate buffer pH 6.5 (40:60,
   v/v). The column effluent was monitored at 262 nm. The results showed
   that the assay is sensitive at 5 mug/ml. Maximum intra-day coefficient
   of variation was 10.6\%. The recovery obtained in plasma was 87.2\%. The
   method was used to perform protein binding studies by equilibrium
   dialysis in rat plasma and was found to be satisfactory. (C) 2002
   Elsevier Science B.V. All rights reserved.}},
