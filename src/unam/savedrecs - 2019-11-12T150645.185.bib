@article{ ISI:000174828300010,
Author = {Nieto-Alvarez, DA and Jimenez-Cruz, F and Mancilla, T},
Title = {{Synthesis and characterization of new di-n-butylhydroxytin(IV) derived
   compounds from 1,3-diketone malonates}},
Journal = {{POLYHEDRON}},
Year = {{2002}},
Volume = {{21}},
Number = {{4}},
Pages = {{417-420}},
Month = {{MAR 1}},
Abstract = {{The synthesis of four new di-n-butylhydroxytin(IV) derivatives obtained
   from 1,3-diketone malonates 2a-2d is reported. All compounds were
   characterized by H-1, C-13, Sn-119 NMR, infrared spectroscopy and mass
   spectrometry (FAB). The Sn-119 NMR spectra at room temperature showed
   that in CDCl3 solution the tin atom is hexacoordinated. Moreover, the
   Sn-119 NMR spectra of compound 2a at low temperature exhibits two
   signals in the range of hexacoordinated compounds. suggesting the
   existence of additional species in higher states of aggregation. (C)
   2002 Elsevier Science Ltd. All rights reserved.}},
