@article{ ISI:000173645000005,
Author = {Cifuentes, E and Mazari-Hiriart, M and Carneiro, F and Bianchi, F and
   Gonzalez, D},
Title = {{The risk of enteric diseases in young children and environmental
   indicators in sentinel areas of Mexico City}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENTAL HEALTH RESEARCH}},
