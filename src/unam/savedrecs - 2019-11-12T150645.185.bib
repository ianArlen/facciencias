@article{ ISI:000174978600084,
Author = {Dong, SH and Ma, ZQ},
Title = {{Nonrelativistic levinson's theorem in D dimensions}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2002}},
Volume = {{65}},
Number = {{4, A}},
Month = {{APR}},
Abstract = {{The Levinson theorem for the Schrodinger equation with a spherically
   symmetric potential in D dimensions is uniformly established by the
   Sturm-Liouville theorem. It is shown that the Levinson theorem for the
   cases without a half bound state does not depend on the spatial
   dimension D, namely, the phase-shift delta(l)(0) of the scattering state
   with angular momentum l at zero momentum is equal to the total number
   n(l) of bound states multiplied by pi. When a half bound state occurs
   the Levinson theorem may be modified.}},
