@article{ ISI:000174548100041,
Author = {Corichi, A and Cruz-Pacheco, G and Minzoni, A and Padilla, P and
   Rosenbaum, M and Ryan, MP and Smyth, NF and Vukasinac, T},
Title = {{Quantum collapse of a small dust shell}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2002}},
Volume = {{65}},
Number = {{6}},
Month = {{MAR 15}},
Abstract = {{The full quantum mechanical collapse of a small relativistic dust shell
   is studied analytically, asymptotically and numerically starting from
   the exact finite dimensional classical reduced Hamiltonian recently
   derived by Hajicek and Kuchar. The formulation of the quantum mechanics
   encounters two problems. The first is the multivalued nature of the
   Hamiltonian and the second is the construction of an appropriate
   self-adjoint momentum operator in the space of the shell motion which is
   confined to a half-line. The first problem is solved by identifying and
   neglecting orbits of small action in order to obtain a single valued
   Hamiltonian. The second problem is solved by introducing an appropriate
   lapse function. The resulting quantum mechanics is then studied by means
   of analytical and numerical techniques. We find that the region of total
   collapse has a very small probability. We also find that the solution
   concentrates around the classical Schwarzschild radius. The present work
   obtains from first principles a quantum mechanics for the shell and
   provides numerical solutions, whose behavior is explained by a detailed
   WKB analysis for a wide class of collapsing shells.}},
