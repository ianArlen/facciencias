@article{ ISI:000177064600007,
Author = {Cabrera, FP and Cuevas, SG and Martinez, KM and Tena, RO and Velasquez,
   HA},
Title = {{Evaluation of clinical change in psychological interventions}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2002}},
Volume = {{19}},
Number = {{1}},
Pages = {{73-84}},
Month = {{JUN}},
Abstract = {{The evaluation of the impact of psychological interventions has been
   characterized by the use of statistics as the principal means of
   determining the significance of the behavioral change brought about.
   Without being inadequate, these procedures are insufficient,
   particularly if one wishes to evaluate the quality of the change. The
   present paper submits that statistical significance needs to be
   complemented by the assessment of clinical significance, since
   statistically significant change does not imply that it's clinically or
   socially relevant as well. This article reviews some of the procedures
   used to assess clinical significance; Social Validity, Normative
   Comparisons and the Reliable Change Index. The methods used in these
   procedures are presented as well as a discussion of their relevance in
   assessing clinical significance.}},
