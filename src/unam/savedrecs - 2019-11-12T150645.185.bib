@article{ ISI:000177064600003,
Author = {Rull, MAP},
Title = {{Analogical transfer in four-term problems using a multiple trial design}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2002}},
Volume = {{19}},
Number = {{1}},
Pages = {{23-34}},
Month = {{JUN}},
Abstract = {{Studies suggest that a first episode of analogical transfer may affect
   subsequent ones. The first Study assessed the possibility of
   meta-analogical reasoning using verbal protocols. In a second experiment
   four term problems that have not been used in previous studies were used
   to study the phenomenon. University students solved four consecutive
   letter string problems. Verbal protocols showed clear evidence of
   meta-analogical transfer. Results also showed meta-analogical transfer
   may occur with different types of problems and predicates. Results
   suggest discrete trial models of analogical transfer could be
   substituted by other models that take into consideration history
   effects.}},
