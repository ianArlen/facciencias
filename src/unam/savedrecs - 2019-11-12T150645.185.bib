@article{ ISI:000176784500004,
Author = {Moshinsky, M and Hernandez-Saldana, H and Suarez-Moreno, A},
Title = {{Sturm-Coulomb problem in a magnetic field and its implications for
   quantum chaos}},
Journal = {{PHYSICS OF ATOMIC NUCLEI}},
Year = {{2002}},
Volume = {{65}},
Number = {{6}},
Pages = {{976-983}},
Month = {{JUN}},
Note = {{23rd International Colloquium on Group Theoretical Methods in Physics,
   DUBNA, RUSSIA, JUL 31-AUG 05, 2000}},
Abstract = {{The Sturm-Coulomb problem is an integrable one since its symmetry group
   is O(4). When we apply to it a magnetic field, this symmetry is broken
   and reduced to the O(2) group. The problem is then nonintegrable, but we
   can derive its matrix representation in a basis in which the
   Sturm-Coulomb problem alone is diagonal. We use this matrix
   representation to obtain the corresponding eigenvalues and their nearest
   neighbor spacing distribution. From the histogram of the latter, we
   discuss the presence or absence of quantum chaos as a function of the
   intensity H of the magnetic field and the angular momentum in in the
   direction of this field. (C) 2002 MAIK ``Nauka.Interperiodica{''}.}},
