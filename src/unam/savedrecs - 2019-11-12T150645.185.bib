@article{ ISI:000177064600002,
Author = {Ortiz, DR and Munoz, JG and Nunez, GP},
Title = {{Asymmetry given by a greater activity of right hemiface in the course of
   non-emotional deliberate facial movement}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2002}},
Volume = {{19}},
Number = {{1}},
Pages = {{17-22}},
Month = {{JUN}},
Abstract = {{The present Study examined asymmetry related to a deliberate facial
   non-emotional movement. Changes were assessed through electromyographic
   recording (EMG) Of Muscular activity in both women and men. Twenty-four
   healthy, right-handed women, and 14 healthy, right-handed men
   participated. All were volunteers and were instructed to perform seven
   electromyographically recorded facial actions. The right hemi-face
   showed a significantly greater EMG in 5 of the 7 movements. Both women
   and men showed facial asymmetry. Men showed significant greater muscular
   activity (p < 0.01) for pursing the lips and simling showing the teeth.
   On the other hand, women showed greater muscular activity for frowning
   and whistling. Greater activity of right hemiface could be the result of
   cerebral mechanisms related to deliberate and emotional expressions. Sex
   differences might be due to differences of cerebral organization and/or
   Cultural factors.}},
