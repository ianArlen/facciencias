@article{ ISI:000177064600001,
Author = {Gomez-Perez, E and Ostrosky-Solis, F and Corsi-Cabrera, M and Guevara,
   MA and Aveleyra, E},
Title = {{Quantitative EEG analysis in subjects at risk of inheriting familial
   Alzheimer's disease}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2002}},
Volume = {{19}},
Number = {{1}},
Pages = {{5-15}},
Month = {{JUN}},
Abstract = {{Alzheimer's Disease (AD) is a progressive disorder, therefore
   experimental treatments should he applied in early phases, when deficits
   have not yet progressed. The aim of this Study was to determine if
   electroencephalograph evaluations can provide information about early
   alterations in asymptomatic Subjects at risk of inheriting familial AD
   (FAD). Electroencephalographic activity of a group of asymptomatic
   subjects at risk of inheriting FAD and that of a control group was
   recorded. Four subjects at risk of inheriting FAD had a decrease of in
   power for the alpha and beta bands, an increase for the theta band, and
   a decrease in interhemispheric and intrahemispheric temporal Coupling or
   coherent activity. One year later, electroencephalograph alterations had
   persisted and, in addition, an increase for theta and a decrease for
   beta bands were observed. The electroencephalogram may be an objective
   indicator of disorders in the early stages of FAD.}},
