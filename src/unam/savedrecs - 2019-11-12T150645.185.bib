@article{ ISI:000175371800047,
Author = {Garcia, EA and Casar, I and Magana, LF},
Title = {{On the optimization of the diffraction efficiency by angular selectivity
   for BSO under strong modulations and applied electric fields}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2002}},
Volume = {{204}},
Number = {{1-6}},
Pages = {{363-369}},
Month = {{APR 1}},
Abstract = {{We calculated the diffraction efficiency as a function of the angular
   selectivity for the non-linear regime by solving numerically the beam
   coupling equations (for recording and reading). The refraction index
   variation used in the beam coupling equations was calculated directly
   from the material rate equations via the total space charge field. We
   found that the enhancement of the diffraction efficiency by angular
   selectivity depends on the way the grating is recorded and on the
   thickness of the sample. For the case of a sample of BSO with a
   thickness of 1.7 cm recorded with an applied field of 10 kV/cm and a
   modulation depth at the surface of the sample Of m(0) = 0.9 the
   diffraction efficiency can be increased from 0.1\% to 37\%. (C) 2002
   Elsevier Science B.V. All rights reserved.}},
