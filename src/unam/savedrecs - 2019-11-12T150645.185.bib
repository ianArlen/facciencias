@article{ ISI:000176267400008,
Author = {Hirsch, JG and Castanos, O and Hess, PO and Ceron, VE and Civitarese, O},
Title = {{Double-beta decay in deformed nuclei}},
Journal = {{CZECHOSLOVAK JOURNAL OF PHYSICS}},
Year = {{2002}},
Volume = {{52}},
Number = {{4}},
Pages = {{513-519}},
Month = {{APR}},
Note = {{Workshop on Calculation of Double-Beta-Decay Matrix Elements (MEDEX 01),
   PRAGUE, CZECH REPUBLIC, JUL 11-15, 2001}},
Abstract = {{A brief review of theoretical results for the double-beta decay and the
   double-electron capture in heavy deformed nuclei is presented. The
   betabeta half life of Gd-160 is evaluated using an extended version of
   the pseudo SU(3) model. While the 2nu mode is forbidden when the most
   probable occupations are considered, states with different occupation
   numbers can be mixed through the pairing interaction. The amount of this
   mixing is calculated using perturbation theory. The possibility of
   observing the betabeta decay in Gd-160 is discussed for both the 2nu and
   0nu modes.}},
