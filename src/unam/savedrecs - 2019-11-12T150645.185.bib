Title = {{Refractive index measurement of pure and Er3+-doped ZrO2-SiO2 sol-gel
   film by using the Brewster angle technique}},
Journal = {{OPTICAL MATERIALS}},
Year = {{2002}},
Volume = {{19}},
Number = {{2}},
