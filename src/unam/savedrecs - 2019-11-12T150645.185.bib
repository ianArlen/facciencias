@article{ ISI:000175598300003,
Author = {Torres, M and Aragon, JL and Dominguez, P and Gil, D},
Title = {{Regularity in irregular echinoids}},
Journal = {{JOURNAL OF MATHEMATICAL BIOLOGY}},
Year = {{2002}},
Volume = {{44}},
Number = {{4}},
Pages = {{330-340}},
Month = {{APR}},
Abstract = {{Using the mathematical concept of eutactic star, we prove that it is
   possible to define a morphospace for irregular echinoids by using a
   single parameter. In particular, we have found an extraordinary
   geometric property in the flower-like patterns of the five ambulacral
   petals of these animals. This property is fulfilled with great accuracy
   for a large collection of fossil specimens and provides new insights in
   the study of the viable skeletal designs of extinct and/or living
   organisms.}},
