@article{ ISI:000177064600004,
Author = {Rodriguez, SO and Martinez, LR},
Title = {{Interpersonal perception among teachers and students: The case of street
   children}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2002}},
Volume = {{19}},
Number = {{1}},
Pages = {{37-46}},
Month = {{JUN}},
Abstract = {{Interpersonal perceptions tend to create expectations. When teachers'
   expectations predicts behavior we speak of self-fulfilling prophecies. A
   school that educates street children has problems to keep discipline in
   the classroom. Four disciplined groups were compared with four less
   disciplined ones. Eighty street children, 68 boys and 12 girls, between
   the ages of 10 and 19, and eight teachers participated. Responses given
   by the more disciplined groups were compared through discriminant
   analysis with those by less disciplined groups. Interpersonal
   perceptions by students of teachers and vice-versa was more positive
   among disciplined groups, and ``empathy{''} and ``emotional{''} factors
   suggest that a more positive perception among these groups, should be
   expressly promoted when teaching this type of students.}},
