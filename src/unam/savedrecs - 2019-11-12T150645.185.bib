@article{ ISI:000175943000001,
Author = {Nino-Cabrera, HG and Colin-Barenque, L and Avila-Costa, MR and
   Espinosa-Villanueva, J and Fortoul, TI and Rivas-Arancibia, S},
Title = {{Differences between hippocampus and cerebral cortex in aged rats in an
   oxidative stress model}},
Journal = {{INTERNATIONAL JOURNAL OF NEUROSCIENCE}},
Year = {{2002}},
Volume = {{112}},
Number = {{4}},
Pages = {{373-381}},
Month = {{APR}},
Abstract = {{Ozone exposure increases the production of free radicals that causes
   oxidative stress (OS), a state that also occurs during aging and in
   neuro-degenerative diseases. This study identified ultrastructural
   alterations produced by OS induced by acute ozone exposure in
   hippocampus and prefrontal cortex in aged compared with young rats.
   Animals were exposed to 0.70 ppm ozone for 4 h, and controls to flowing
   air. After the exposure, the tissues were processed for ultrastructural
   analysis. Results showed increased ultrastructural alterations in the
   hippocampus and prefrontal cortex in the aged exposed animals compared
   with controls. OS enhanced the modifications induced by the aging
   process in those areas related with learning and memory functions, which
   are the first where degenerative aging changes are observed.}},
