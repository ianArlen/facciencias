@article{ ISI:000175407300059,
Author = {Brom, S and Girard, L and Garcia-de los Santos, A and Sanjuan-Pinilla,
   JM and Olivares, J and Sanjuan, J},
Title = {{Conservation of plasmid-encoded traits among bean-nodulating Rhizobium
   species}},
Journal = {{APPLIED AND ENVIRONMENTAL MICROBIOLOGY}},
Year = {{2002}},
Volume = {{68}},
Number = {{5}},
Pages = {{2555-2561}},
Month = {{MAY}},
Abstract = {{Rhizobium etli type strain CFN42 contains six plasmids. We analyzed the
   distribution of genetic markers from some of these plasmids in
   bean-nodulating strains belonging to different species (Rhizobium etli,
   Rhizobium gallicum, Rhizobium giardinii, Rhizobium leguminosarum, and
   Sinorhizobium fredii). Our results indicate that independent of
   geographic origin, R. etli strains usually share not only the pSym
   plasmid but also other plasmids containing symbiosis-related genes, with
   a similar organization. In contrast, strains belonging to other
   bean-nodulating species seem to have acquired only the pSym plasmid from
   R. etli.}},
