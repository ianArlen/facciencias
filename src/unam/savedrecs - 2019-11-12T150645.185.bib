@article{ ISI:000174019500016,
Author = {Escobar-Alarcon, L and Villarreal-Barajas, JE and Camps, E and Muhl, S
   and Haro-Poniatowski, E and Romero, S and Salinas, B},
Title = {{Thermoluminescent response of carbon nitride thin films deposited by
   laser ablation}},
