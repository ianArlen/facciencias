@article{ ISI:000396680800001,
Author = {Castillo, J. A. and Cruz-Salas, M. V. and Huelsz, G.},
Title = {{Natural ventilation by windexchangers in a building with a window in
   prevailing winds: design guidelines}},
Journal = {{INTERNATIONAL JOURNAL OF VENTILATION}},
Year = {{2017}},
Volume = {{16}},
Number = {{1}},
Pages = {{1-14}},
Abstract = {{This paper presents design guidelines to enhance natural ventilation
   using windexchangers, in a room with a window in prevailing winds, at
   either windward or leeward. These guidelines are extracted from
   experimental results of windexchangers used in the roof of a single room
   with a window. The results of the cases with the window at windward are
   taken from a previous work. The results of the cases with the window at
   leeward are presented in this paper. In both group of cases, six
   windexchanger configurations with different wind orientations are
   evaluated. The room without any windexchanger is used as the baseline
   case, in each group of cases. The experiments were carried out in a
   water channel using scale models. Flow velocity measurements were
   performed using stereoparticle image velocimetry. The velocity vertical
   profile in the test section corresponds to a small town. The parameters
   used to evaluate the ventilation performance are the flow rate and the
   area percentage with a significant flow speed evaluated at the room
   vertical central plane.}},
