@inproceedings{ ISI:000401591600041,
Author = {Zharikov, S. V. and Miroshnichenko, A. S.},
Editor = {{Miroshnichenko, A and Zharikov, S and Korcakova, D and Wolf, M}},
Title = {{Short-Term Spectral Variability in the Binary FS CMa-Type Object MWC 728}},
Booktitle = {{B(E) PHENOMENON: FORTY YEARS OF STUDIES}},
Series = {{Astronomical Society of the Pacific Conference Series}},
