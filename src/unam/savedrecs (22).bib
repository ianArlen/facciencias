@article{ ISI:000402129400005,
Author = {Albarran, Guadalupe and Mendoza, Edith and Esparza, Maribel},
Title = {{COMPARISON OF LIQUID CHROMATOGRAPHY AND CAPILLARY ELECTROPHORESIS IN
   STUDIES ON RADIATION CHEMISTRY: SEPARATION OF RADIOLYTIC PRODUCTS}},
Journal = {{AVANCES EN CIENCIAS E INGENIERIA}},
Year = {{2017}},
Volume = {{8}},
Number = {{1}},
Pages = {{43-49}},
Month = {{JAN-MAR}},
Abstract = {{The aim of this study is to compare the results obtained from two
   methods for the identification and quantification of the radiolytic
   products of dilute solutions of hydroxybenzoic acids. Aqueous solutions
   of each acid were irradiated and analyzed using a Alltima C18 column
   (50x4.6 mm), of mobile phase a mixture of methanol, 5 mmol/L
   1-hexanesulfonic acid and 1\% acetic acid solution for liquid
   chromatography and for capillary electrophoresis 72cmx75aem capillary
   column, high sensitivity cell, voltage 25 kV and borate buffer solution
   10 mmol/L, pH 9.3 and 50 mmol/L SDS were used. On the kinetics of
   formation of the radiolytic products approximate concentrations were
   obtained. The results obtained provide extensive information to
   radiation chemistry in determining the distribution of load in the
   aromatic ring.}},
