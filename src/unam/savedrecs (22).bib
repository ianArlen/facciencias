@article{ ISI:000393245900043,
Author = {Fernandez, A. M. and Turner, J. A. and Lara-Lara, B. and Deutsch, T. G.},
Title = {{Influence of support electrolytic in the electrodeposition of Cu-Ga-Se
   thin films}},
