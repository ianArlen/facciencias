@article{ ISI:000392843300002,
Author = {Monroy-Barreto, Minerva and Esturau-Escofet, Nuria and Romero-Avila,
   Margarita and Recillas-Mota, Jesus},
Title = {{NMR characterization of pentaerythritol glycolysis products of
   polyethylene terephthalate}},
Journal = {{INTERNATIONAL JOURNAL OF POLYMER ANALYSIS AND CHARACTERIZATION}},
Year = {{2017}},
Volume = {{22}},
Number = {{1}},
Pages = {{11-16}},
Abstract = {{The degradation of polyethylene terephthalate (PET) bottles has been
   successfully achieved by depolymerization through glycolysis with
   pentaerythritol. The reaction was performed at 250 degrees C in the
   presence of an organotin catalyst. The glycolyzed products were
   characterized by 1D and 2D nuclear magnetic resonance (NMR)
   spectroscopy. The existence of the chemical bond between the polyol and
   the PET was detected through heteronuclear multiple bond correlation
   spectroscopy, and the NMR signals of different species were separated
   according to their diffusion coefficient by diffusion-ordered
   spectroscopy. This method offers a rapid quantification of glycolyzed
   products. Furthermore, it was found that the major product was the
   mono-pentaerythritol ester followed by the di-pentaerythritol ester, and
   lastly the tri-pentaerythritol ester.}},
