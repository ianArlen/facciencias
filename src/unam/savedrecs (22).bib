@article{ ISI:000406145600001,
Author = {Beuchot, Mauricio},
Title = {{Epistemology of analogy: Knowledge, society and expression}},
Journal = {{SOCIOLOGIA Y TECNOCIENCIA}},
Year = {{2017}},
Volume = {{7}},
Number = {{2}},
Pages = {{1-12}},
Abstract = {{In this article we expose the bases of analog epistemology. This theory
   of knowledge is between an extreme subjectivism and an extreme
   objectivism. Analog hermeneutics is a realistic hermeneutics. She seeks
   the truth, but incorporates the meaning and emotion. We have separated
   the reason from the experience, the praxis theory, the mind or the soul
   of the body. We have to get them back together, if we do not get lost in
   the rational (which says little of the human being), or we lose
   ourselves in the emotional (without logical consistency). The analogical
   hermeneutic realism is able, thanks to the analogy itself, to mediate in
   this way of union.}},
