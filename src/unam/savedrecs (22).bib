@article{ ISI:000391288200013,
Author = {Soto, Eduardo and Espinosa, Norma and Diaz-Guerrero, Miguel and Gaytan,
   Meztlli O. and Puente, Jose L. and Gonzalez-Pedrajo, Bertha},
Title = {{Functional Characterization of EscK (Orf4), a Sorting Platform Component
   of the Enteropathogenic Escherichia coli Injectisome}},
Journal = {{JOURNAL OF BACTERIOLOGY}},
