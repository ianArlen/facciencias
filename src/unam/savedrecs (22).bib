@inproceedings{ ISI:000405307700074,
Author = {Vazquez-Gomez, Octavio and Lopez-Martinez, Edgar and Ivan
   Gallegos-Perez, Alexis and Santoyo-Aviles, Heber and Javier
   Vergara-Hernandez, Hector and Campillo, Bernardo},
Editor = {{Meyers, MA and Benavides, HAC and Bruhl, SP and Colorado, HA and Dalgaard, E and Elias, CN and Figueiredo, RB and GarciaRincon, O and Kawasaki, M and Langdon, TG and Mangalaraja, RV and Marroquin, MCG and Rocha, AD and Schoenung, JM and Silva, ACE and Wells, M and Yang, W}},
Title = {{Kinetic Study of the Austenite Decomposition During Continuous Cooling
   in a Welding Steel}},
Booktitle = {{PROCEEDINGS OF THE 3RD PAN AMERICAN MATERIALS CONGRESS}},
Series = {{Minerals Metals \& Materials Series}},
Year = {{2017}},
Pages = {{749-760}},
Note = {{3rd Pan American Materials Congress (PAMC), The Minerals, Met \& Mat
   Soc, San Diego, CA, FEB 26-MAR 02, 2017}},
Abstract = {{The kinetics of austenite decomposition during the continuous cooling of
   a low-carbon welding steel was determined by dilatometric analysis.
   Based on the measurements, the critical transformation temperatures of
   austenite decomposition were determined for both ferrite and pearlite.
   The cooling conditions were established from the Stelmor (R) controlled
   cooling process for the manufacturing of wire rod. It was observed that
   the critical temperatures decrease when the cooling time was reduced as
   a result of an increased cooling rate in the range of 600-900 degrees C.
   Using the decomposition temperatures, a continuous cooling
   transformation CCT diagram was created and it may be observed that
   austenite decomposition occurs in two steps. The kinetic parameters for
   each step were determined and compared with the
   Johnson-Mehl-Avrami-Kolmogorov diffusive model. The final microstructure
   was analyzed using an optical microscope and evaluated by
   nanoindentation to determine the effect of the cooling rate on the
   nanohardness of each phase.}},
