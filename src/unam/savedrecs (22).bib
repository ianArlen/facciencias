@article{ ISI:000392371900008,
Author = {Orozco-del-Castillo, M. G. and Cardenas-Soto, M. and Ortiz-Aleman, C.
   and Couder-Castaneda, C. and Urrutia-Fucugauchi, J. and
   Trujillo-Alcantara, A.},
Title = {{A texture-based region growing algorithm for volume extraction in
   seismic data}},
Journal = {{GEOPHYSICAL PROSPECTING}},
Year = {{2017}},
Volume = {{65}},
Number = {{1}},
Pages = {{97-105}},
Month = {{JAN}},
Abstract = {{We present a novel approach to automated volume extraction in seismic
   data and apply it to the detection of allochthonous salt bodies. Using a
   genetic algorithm, we determine the optimal size of volume elements that
   statistically, according to the U-test, best characterize the contrast
   between the textures inside and outside of the salt bodies through a
