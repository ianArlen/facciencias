
@article{ ISI:000174377200006,
Author = {Gomez-Torrente, M},
Title = {{Vagueness and margin for error principles (Epistemicism)}},
Journal = {{PHILOSOPHY AND PHENOMENOLOGICAL RESEARCH}},
Year = {{2002}},
Volume = {{64}},
Number = {{1}},
Pages = {{107-125}},
Month = {{JAN}},
Abstract = {{Timothy Williamson's potentially most important contribution to
   epistemicism about vagueness lies in his arguments for the basic
   epistemicist claim that the alleged cut-off points of vague predicates
   are not knoweable. His arguments for this are based on so-called
   `margins for error principles'. This paper argues that these principles
   fail to provide a good argument for the basic claim. Williamson has
   offered at least two kinds of margin for error principles applicable to
   vague predicates. A certain fallacy of equivocation (on the meaning of
   `knowable') seems to underlie his justification for both kinds of
   principles. Besides, the margin for error principles of the first kind
   can be used in the derivation of unacceptable consequences , while the
   margin for error principles of the second kind can be shown to be
   compatible with the falsity of epistemicism, under a number of
   assumptions acceptable to the epistemicist.}},
