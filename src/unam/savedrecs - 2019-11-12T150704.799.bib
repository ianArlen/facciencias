@article{ ISI:000184976900004,
Author = {Martinez, SF},
Title = {{History and the combinatorial theory of scientific representations.
   Commentary on the propositions of Ibarra and Mormann}},
Journal = {{CRITICA-REVISTA HISPANOAMERICANA DE FILOSOFIA}},
Year = {{2001}},
Volume = {{33}},
Number = {{99}},
Pages = {{75-95}},
Month = {{DEC}},
Abstract = {{Ibarra and Mormann's combinatorial theory of scientific representations
   is subject to critical scrutiny. It is argued that the classification of
   types of representations offered cannot serve as a basis for an analysis
   of the variety of representations that happen to arise in science.
   Empirical studies on ways of representing used in science strongly
   suggest that a formal theory of representations of the kind proposed is
   not able to capture the historical dimension of the structure of
   representations.}},
