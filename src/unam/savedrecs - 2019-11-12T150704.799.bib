@inproceedings{ ISI:000183080300064,
Author = {Betancourt, I and Davies, HA},
Editor = {{Hadjipanayis, GC and Bonder, MJ}},
Title = {{Rare earth-rich, didymium (Nd/Pr)-based nanostructured hard magnetic
   alloys}},
Booktitle = {{RARE EARTH MAGNETS AND THEIR APPLICATIONS}},
Year = {{2002}},
