@article{ ISI:000175684000001,
Author = {Espinosa-Perez, MD and Hendrickx, ME},
Title = {{The genus Paracerceis Hansen, 1905 (Isopoda, Sphaeromatidae) in the
   eastern tropical Pacific, with the description of a new species}},
Journal = {{CRUSTACEANA}},
Year = {{2001}},
Volume = {{74}},
Number = {{11}},
Pages = {{1169-1187}},
Month = {{DEC}},
Abstract = {{Paracerceis spinulosa sp. nov., is described from material collected
   along the Pacific coast of Mexico. This species was previously reported
   in literature as Paracerceis sp. and differs from the other two species
   of Paracerceis from the eastern Pacific (i.e., R sculpta (Holmes, 1904)
   and P richardsonae Lombardo, 1988) by the presence of a large,
   subconical median tubercle on the telson, a much higher uropodal exopod
   partly covered with spiny and rounded tubercles, and a distinct
   spinulation pattern on all pereiopods. Newly-collected material of R
   sculpta and P. richardsonae provides new environmental and
   distributional data. Keys to the males and females of the species of
   Paracerceis of the eastern Pacific are provided.}},
