@article{ ISI:000172479800004,
Author = {Bokhimi, X and Toledo, JA and Navarrete, J and Sun, XC and Portilla, M},
Title = {{Thermal evolution in air and argon of nanocrystalline MoS2 synthesized
   under hydrothermal conditions}},
Journal = {{INTERNATIONAL JOURNAL OF HYDROGEN ENERGY}},
Year = {{2001}},
Volume = {{26}},
Number = {{12}},
Pages = {{1271-1277}},
Month = {{DEC}},
Note = {{4th International Symposium on New Materials, Renewable Energy and
   Environment, CANCUN, MEXICO, AUG 27-31, 2000}},
Abstract = {{Nanocrystalline molybdenum sulfide was synthesized between 150 degreesC
   and 225 degreesC under hydrothermal conditions starting from ammonium
   heptamolybdate and thiourea. Samples were characterized with X-ray
   powder diffraction. electron microscopy, nitrogen adsorption,
   thermoanalysis and infrared spectroscopy. The hydroxyls involved in the
   synthesis and adsorbed on crystals surface hindered crystallization and
   samples still recrystallized after the final dehydroxylation step above
   300 degreesC, just when hydroxyls were isolated from each other. This
   also promoted sulfur bond breaking that gave rise to partial
   transformation of the MoS2 into MoO2 when the annealing atmosphere was
   argon, and to the total transformation of the sulfide into MoO3 when it
   was air. The initial MoS2 crystals were bend; many of them were
   isolated, and others associated in bundles that formed worm-like grains
   interacting with each other to produce spherical grains aggregated in
   clusters. This morphology gave rise to samples with a low specific
   surface area. (C) 2001 International Association for Hydrogen Energy.
   Published by Elsevier Science Ltd. All rights reserved.}},
