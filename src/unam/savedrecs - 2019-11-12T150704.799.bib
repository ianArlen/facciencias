@article{ ISI:000171820900001,
Author = {Imriskova, I and Langley, E and Arreguin-Espinosa, R and Aguilar, G and
   Pardo, JP and Sanchez, S},
Title = {{Rapid purification and biochemical characterization of glucose kinase
   from Streptomyces peucetius var. caesius}},
Journal = {{ARCHIVES OF BIOCHEMISTRY AND BIOPHYSICS}},
Year = {{2001}},
Volume = {{394}},
Number = {{2}},
Pages = {{137-144}},
Month = {{OCT 15}},
Abstract = {{Glucose kinase catalyzes the ATP-dependent phosphorylation of glucose.
   Streptomyces peucetius var. caesius glucose kinase was purified 292-fold
   to homogeneity. The enzyme has cytosolic localization and is composed of
   four identical subunits, each of 31 kDa. The purified enzyme easily
   dissociates into dimers. However, in the presence of 100 mM glucose the
   enzyme maintains its tetrameric form. Maximum activity was found at 42
   degreesC and pH 7.5. Isoelectric focusing of the enzyme showed a pl of
   8.4. The N- and C-terminal amino acid sequences were MGLTIGVD and
   VY-FAREPDPIM, respectively. The kinetic mechanism of S. peucetius var.
   caesius glucose kinase appears to be a rapid equilibrium ordered type,
   i.e., ordered addition of substrates to the enzyme, where the first
   substrate is D-glucose. The K-m values for D-glucose and MgATP(2-) were
   1.6 +/- 0.2 and 0.8 +/- 0.1 mM, respectively. Mg2+ in excess of 10 mM
   inhibits enzyme activity. (C) 2001 Academic Press.}},
