@article{ ISI:000172815100034,
Author = {Garcia-Hipolito, M and Falcony, C and Aguilar-Frutis, MA and Azorin-
   Nieto, J},
Title = {{Synthesis and characterization of luminescent ZrO2 : Mn, Cl powders}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2001}},
Volume = {{79}},
Number = {{26}},
Pages = {{4369-4371}},
Month = {{DEC 24}},
Abstract = {{ZrO2:Mn, Cl luminescent powders have been synthesized at temperatures
   ranging from 250 to 500 degreesC. X-ray diffraction measurements
   indicate changes in the crystallinity of the material as a function of
   the processing temperature. The photoluminescence spectra show bands
   associated with transitions from the T-4(1) level to the (6)A(1) levels
   of the Mn2+ ions. An impurity concentration quenching is observed with
   the Mn contents. In addition, the presence of Cl in the synthesized
   material has been found to have an important role on the
   photoluminescence intensity emission. Preliminary results on
   cathodoluminescent emission are also reported. (C) 2001 American
   Institute of Physics.}},
