@article{ ISI:000173322200006,
Author = {Gonzalez-Acuna, F and Ramirez, A},
Title = {{Two-bridge knots with property Q}},
Journal = {{QUARTERLY JOURNAL OF MATHEMATICS}},
Year = {{2001}},
Volume = {{52}},
Number = {{4}},
Pages = {{447-454}},
Month = {{DEC}},
Abstract = {{A knot k in S-3 has property Q if there is a closed surface S in S-3
   containing k such that k is imprimitive in H(1)A and H1B where A and B
   are the closures of the components Of S-3 - S. If, in addition, lk(k,
   k(+)) not equal 0, where k(+) is contained in S and is parallel to k,
   then k has property Q{*}. We determine the 2-bridge knots K(beta/alpha)
   that have property Q in terms of a continued fraction expansion of
   alpha/beta. We also prove that the following conditions are equivalent:
   (i) K(beta/alpha) has property Q; (ii) K(beta/alpha) has property Q{*};
   (iii) There is an epimorphism from the group of K(beta/alpha) onto a
   free product; (iv) There is an epimorphism from the group of
   K(beta/alpha) onto Z(2) {*} Z(d).}},
