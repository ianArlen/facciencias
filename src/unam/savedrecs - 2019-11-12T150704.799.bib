@article{ ISI:000178899900025,
Author = {Gastelum, S and Osuna, I and Melendrez, R and Cruz-Zaragoza, E and
   Chernov, V and Calderon, T and Barboza-Flores, M},
Title = {{Application of a thermoluminescence method for the detection of
   irradiated spices}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{2002}},
Volume = {{101}},
