@article{ ISI:000171544400016,
Author = {Luca, F and Raggi-Cardenas, AG},
Title = {{Composition factors from the table of marks}},
Journal = {{JOURNAL OF ALGEBRA}},
Year = {{2001}},
Volume = {{244}},
Number = {{2}},
Pages = {{737-743}},
Month = {{OCT 15}},
Abstract = {{Let B(G) be the Burnside ring for a finite group G and let T(G) be the
   table of marks of G. It is known that, in general, neither B(G) nor T(G)
   determines the finite group G. That is, one can easily construct pairs
   of non-isomorphic finite groups such that their table of marks is the
   same (hence, their Burnside rings are isomorphic). In this note, we show
   that the composition factors of G are determined by the table of marks
   T(G). (C) 2001 Academic Press.}},
