@article{ ISI:000172884600004,
Author = {Peresmitre, GG and Alvarado, G and Moreno, L and Saloma, S and Pineda, G},
Title = {{Eating disorders. Risk factors in three different age groups:
   Prepubescent, pubescent and adolescent}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2001}},
Volume = {{18}},
Number = {{3}},
Pages = {{313-324}},
Month = {{DEC}},
Abstract = {{The purpose of this study was to determine if risk factors for eating
   disorders are already present in prepubescent Mexican boys and girls
   that have been found in pubescent or preadolescent and adolescent
   samples, and to compare distributions of such factors by sex and age
   group. The total non probabilistic sample size included N = 8673 male
   and female subjects distributed in three age groups: Prepubescent (n =
   479); preadolescents (n = 2694) and adolescents (n = 5500). Among the
   most important findings we have that younger girls have already adopted
   the lipophobic attitude from their social environment, and that they
   share with the oldest girls not only the desire of a thin body image,
   but also, they feel dissatisfied with their own body image (because they
   would like to be thinner) altering it (over estimating), perceiving
   themselves with a higher weight than the one they really have.}},
