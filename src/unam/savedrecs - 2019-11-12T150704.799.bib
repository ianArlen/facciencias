@inproceedings{ ISI:000179805700233,
Author = {Espinosa, G and Amero, C and Gammage, RB},
Editor = {{Horowitz, YS and Oster, L}},
Title = {{Measurements of alpha particle energy using nuclear tracks in solids
   methodology}},
Booktitle = {{SOLID STATE DOSIMETRY, PTS 1 AND 2, PROCEEDINGS}},
