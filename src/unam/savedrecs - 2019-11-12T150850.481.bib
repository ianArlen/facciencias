@inproceedings{ ISI:000084260300058,
Author = {Dultzin-Hacyan, D and Krongold, Y and Guridi, IF and Marziani, P},
Editor = {{Gaskell, CM and Brandt, WN and Dietrich, M and DultzinHacyan, D and Eracleous, M}},
Title = {{Evidence against the unified scheme for Seyferts and an alternative view}},
