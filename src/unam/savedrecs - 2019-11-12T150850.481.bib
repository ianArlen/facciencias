@inproceedings{ ISI:000080089400080,
Author = {Benitez, E and Webb, JR and Howard, E and Punsly, B},
Editor = {{Takalo, LO and Sillanpaa, A}},
Title = {{The dual jet model for Blazar continuum emission}},
Booktitle = {{BL LAC PHENOMENON}},
Series = {{Astronomical Society of the Pacific Conference Series}},
Year = {{1999}},
Volume = {{159}},
Pages = {{329-332}},
Note = {{Meeting on BL Lac Phenomenon, TURKU, FINLAND, JUN 22-26, 1998}},
Abstract = {{A discussion of the current understanding of the Blazar phenomena is
   presented in the context of the Dual Jet Model recently proposed by
   Punsly (Punsly 1996a; 1996b). This model is capable of explaining many
   features exhibited by Blazars through the formation of two relativistic
   jets: one jet emanating from an ergospheric disk, which supports the
   extended radio lobes, and a second jet emanating from the black hole
   magnetospheric horizon. The interaction between the two jets provides a
   region where gamma-ray emission can be produced. The model seems to work
   for explaining the differences in extended radio emission between FSRQ
   and BL Lac objects, and at the same time, why some Blazars present
   emission in the gamma-ray region.}},
