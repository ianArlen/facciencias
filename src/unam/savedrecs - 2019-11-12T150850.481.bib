@article{ ISI:000077675900058,
Author = {Barranco, AP and Pinar, FC and Martinez, OP and Tera, AH},
Title = {{PMN-PZT: processing schedule and experimental dependence between the
   electromechanical coupling factor and the polarization}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1998}},
Volume = {{44}},
Number = {{3}},
Pages = {{220-223}},
Month = {{DEC}},
Note = {{14th Latin American Symposiumm on Solid State Physics, OAXACA, MEXICO,
   JAN 11-16, 1998}},
Abstract = {{Ferroelectric ceramics of nominal composition
   Pb(Mg1/3Nb2/3)(0.375)Ti0.375Zr0.25O3 (PMN-PZT) were prepared using two
   procedures: the standard ceramic method and the columbite technique. In
   the second one, the columbite structure was obtained and then mixed with
   the other oxides, which permitted to obtain higher density values.
   Dielectric and piezoelectric properties were studied and the results
   were compared for both preparation techniques. The highest mechanical
   and electrical quality factors, electromechanical coupling coefficient
   and maximum dielectric permittivity were obtained by the columbite
   route. The dielectric losses were lower in this case. The experimental
   dependence between the electromechanical coupling factor (k(p)) and the
   polarization (P) was analyzed in the columbite method. The temperature
   dependence of resonance-antiresonance frequencies and minimum impedance
   was obtained. A square dependence was observed between k(p) and P.}},
