@inproceedings{ ISI:000079745200009,
Author = {Leyvraz, F},
Editor = {{Hacyan, S and Jauregui, R and LopezPena, R}},
Title = {{Spectral properties of classically chaotic systems}},
Booktitle = {{LATIN-AMERICAN SCHOOL OF PHYSICS XXXI ELAF: NEW PERSPECTIVES ON QUANTUM
   MECHANICS}},
Series = {{AIP Conference Proceedings}},
Year = {{1999}},
Volume = {{464}},
Pages = {{253-280}},
Note = {{XXXI Latin American School of Physics on New Perspectives on Quantum
   Mechanics (XXXI ELAF), MEXICO CITY, MEXICO, JUL 27-AUG 14, 1998}},
Organization = {{Univ Nacl Autonoma Mexico; Univ Autonoma Metroppolitana, Unidad
   Iztapalapa; Colegio Nacl; Ctr Internacl Ciencias AC; Org Estados Amer;
   Ctr LatinoAmer Fisica, Brazil; Ctr LatinoAmer Fisica, Mexico; Int Ctr
   Theoret Phys Trieste; Ctr Invest Estudios Avanzados Ist Politecnico
   Nacl; Consejo Nacl Ciencia Tecnologia}},
Abstract = {{We describe the connection between quantum systems which have a chaotic
   classical counterpart and random matrix theory. As is well-known it
   consists in the fact that the statistical properties of the spectra of
   such systems in the semiclassical limit are equivalent to those of
   random matrix theory. Here, we first briefly review some propertie of
   random matrices, and then proceed to justify the above-mentioned
   connection in two different ways: First, according to the classic work
   of Berry, we show how the result can be derived from periodic orbit
   theory, of which we give a rapid overview; second, we show how the same
   result can be obtained with greater generality but in a more speculative
   manner using the concept of structural invariance.}},
