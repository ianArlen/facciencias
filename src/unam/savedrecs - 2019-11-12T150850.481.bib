@inproceedings{ ISI:000080192500115,
Author = {Noguez, ME and Salas, G and Robert, T and Pacheco, G and Ramirez, J},
Editor = {{Eckert, CE}},
Title = {{Secondary dendrite arm coarsening during chemical grain refinement in
   some light metal alloys}},
Booktitle = {{LIGHT METALS 1999}},
Year = {{1999}},
Pages = {{793-795}},
Note = {{128th TMS Annual Meeting, SAN DIEGO, CA, FEB 28-MAR 04, 1999}},
Organization = {{Min, Met \& Mat Soc}},
Abstract = {{In certain conditions, chemical grain refinement does not promote a
   refining effect on secondary dendrite arm; on the contrary it tends to
   coarsen. Mondolfo has already pointed out this effect for Al alloys.
   This has practical implications in segregation, properties, cooling rate
   curves, and homogenization times, which have not been enough, discussed
   in the literature.
   A series of grain refined samples using different conditions have been
   cast, with a successful grain refinement. The dendrite coarsening has
   been found in an Al-5\%Zn alloy refined with 0.1-0.5\% Ti-B additions in
   sand molds and markedly in a Mg5\%Zn alloy using 0.03-0.12\%
   hexacloroethane as grain refiner in metallic molds.
   Processing variables and structural parameters are reported, related and
   discussed.}},
