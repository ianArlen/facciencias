@article{ ISI:000079281000006,
Author = {Marquez-Rochal, FJ and Guillen, GK and Sanchez, JE and Vazquez-Duhalt, R},
Title = {{Growth characteristics of Pleurotus ostreatus in bioreactors}},
Journal = {{BIOTECHNOLOGY TECHNIQUES}},
Year = {{1999}},
Volume = {{13}},
Number = {{1}},
Pages = {{29-32}},
Month = {{JAN}},
Abstract = {{Pleurotus ostreatus was cultured in a bioreactor equipped with different
   impeller geometries under non-limiting nutrient conditions. With a
   Rushton turbine impeller the specific growth rate decreased 30\% and
   pellet diameter was reduced 15\% when the aeration rate was increased
   from 1 to 1.5 vvm. Agitation rate reduced the pellet diameter from 5.1
   mm to 2.8 mm using 200 rpm and 400 rpm of agitation, respectively.
   Specific growth rates of 0.036, 0.020 and 0.041 h(-1) were obtained with
   Roshton (disc turbine), Helical Ribbon and InterMIG impellers,
   respectively. Impeller geometry is important to control the pellet size
   and consequently growth rate of P. ostreatus.}},
