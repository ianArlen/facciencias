@article{ ISI:000077321100009,
Author = {Aguilar, MA and Seade, JA and Verjovsky, A},
Title = {{Indices of vector fields and topological invariants of real analytic
   singularities}},
Journal = {{JOURNAL FUR DIE REINE UND ANGEWANDTE MATHEMATIK}},
Year = {{1998}},
Volume = {{504}},
Pages = {{159-176}},
Month = {{NOV 27}},
Abstract = {{In this work we deal with topological invariants of real analytic
   isolated complete intersection singularities. Our invariants arise from
   generalizations to the case of singular varieties, of the classical
   Poincare-Hopf local index for vector fields on smooth manifolds. If the
   vector field in question is radial, then one of the invariants (the real
   GSV-index) coincides with the curvatura integra of Gauss, Hopf and
   Kervaire. When the singular variety is odd dimensional, this invariant
   is also half the Euler-Poincare-characteristic of the link; when the
   dimension is even, the invariant coincides with the semi-characteristic
   of the link. This allows us to define a ``real Milnor number{''} for
   these singularities, which coincides module 2 with the Milnor number for
   real map germs of finite type, studied by C.T.C. Wall {[}30] and others.
   We also study, and classify, the homotopy classes of vector fields on
   isolated complete intersection singularity germs, using certain
   multi-indices.}},
