@article{ ISI:000087163200002,
Author = {Mora-Rios, J and Gonzalez-Forteza, C and Jimenez-Tapia, A and
   Andrade-Palos, P},
Title = {{The meaning of ``family{''} among students through the use of semantic
   networks}},
