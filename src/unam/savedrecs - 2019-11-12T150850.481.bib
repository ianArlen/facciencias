@article{ ISI:000077362300038,
Author = {Mejia-Monasterio, C and Richert, J and Rupp, T and Weidenmuller, HA},
Title = {{Properties of low-lying states in a diffusive quantum dot and Fock-space
   localization}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{1998}},
Volume = {{81}},
Number = {{23}},
Pages = {{5189-5192}},
Month = {{DEC 7}},
Abstract = {{Motivated by an experiment by Sivan er nl. {[}Europhys. Lett. 25, 605
   (1994)] and by subsequent theoretical work on localization in Fock
   space, we study numerically a hierarchical model for a finite many-body
   system of fermions moving in a disordered potential and coupled by a
   two-body interaction. We focus attention on the low-lying states close
   to the Fermi energy. Both the spreading width and the participation
   number depend smoothly on excitation energy. This behavior is in keeping
   with naive expectations and does not display Anderson localization. We
   show that the model reproduces essential features of the experiment by
   Sivan et al. {[}S0031-9007(98)07906-X].}},
