@article{ ISI:000083425100004,
Author = {Avila, R and Bruner, CA},
Title = {{Acquisition of lever pressing by rats with delayed reinforcement in a
   discrete trial procedure}},
Journal = {{REVISTA LATINOAMERICANA DE PSICOLOGIA}},
Year = {{1999}},
Volume = {{31}},
Number = {{1}},
Pages = {{49-64}},
Abstract = {{The acquisition of behavior with delayed reinforcement, has been studied
   in trial by trial and free-operant situations. Different methods and
   dependent variables, however, have hitherto prevented their comparison.
   In the present study, a modified trial by trial procedure was used for
   the acquisition of leverpressing by rats, to assess the viability of a
   system of variables common to both types of situations. With a constant
   8 sec opportunity to leverpress in each trial, the effect of 8- or 24
   Sec delays of reinforcement and of 32- or 64-sec intertrial intervals
   were explored, using a 2 x 2 factorial design, with 3 naive rats per
   cell. For most subjects, the overall and running rate of leverpressing
   were higher under 8- that under 24 sec delays, regardless of intertrial
   interval duration. These two variables were also higher under the 32-
   than under the 64-sec intertrial interval, regardless of delay duration.
   The proportion of trials with responding and the latency of the first
   leverpress, were significantly related to the overall and running rates.
   It was concluded that trial by trial and free-operant procedures can
   bt:viewed as different cases of the same continuum of independent
   variables.}},
