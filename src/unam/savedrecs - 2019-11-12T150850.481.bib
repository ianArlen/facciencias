@article{ ISI:000165298500006,
Author = {Manzanilla-Cano, JA and Reyes-Salas, EO and Barcelo-Quintal, MH},
Title = {{Electrochemical elimination of the pesticide methylparathion in an
   aqueous medium}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENTAL ANALYTICAL CHEMISTRY}},
Year = {{1999}},
Volume = {{75}},
Number = {{4}},
Pages = {{387-405}},
Abstract = {{One of the most important transformation reactions of methylparathion,
   an organophosphorus pesticide, was studied. The redox transformation of
   methylparathion was carried out using different electrochemical methods
   in an aqueous medium. The study was based on direct current (DCP) and
   differential pulse (DPP) polarography, cyclic voltammetry (CV) and
   coulometry. The results show that the pesticide produces a single step
   or two steps depending on the pH and the electrode. The average value
   for the limit of detection (3 s) was (2.05 +/- 0.41) x 10(-8)mol dm(-3)
   (5.4 +/- 1.1 mug l(-1)) for DPP. The electrochemical behavior of
   methylparathion and the nature of the electrolysis products was
   interpreted according to the electrochemical and spectroscopic data. The
   experimental data were employed to predict the fate of the pesticide in
   water samples.}},
