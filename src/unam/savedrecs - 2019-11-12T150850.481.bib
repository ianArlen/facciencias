@article{ ISI:000081959400069,
Author = {Azorin, J and Rivera, T and Falcony, C and Martinez, E and Garcia, M},
Title = {{Ultraviolet thermoluminescent dosimetry using terbium-doped zirconium
   oxide thin films}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{1999}},
Volume = {{85}},
Number = {{1-4}},
Pages = {{317-319}},
Note = {{12th International Conference on Solid State Dosimetry, BURGOS, SPAIN,
   JUL 05-10, 1998}},
Organization = {{Bicron Technol; Bubble Technol Ind; Caja Burgos; Comis Interminist Cienc
   \& Tecnolog; Consejo Seguridad Nucl; Direcc Gen Investigacion Cienc \&
   Tecnol; Empressa Nacl Residuos SA; Excmo Ayuntamiento Burgos; Excma
   Diputac Province Burgos; Landauer Inc; Nucl Technol Publishing; Siemens
   Environm Syst Ltd; Stillwater Sci LLC; Studsvik Instrument AB}},
Abstract = {{The experimental results of studying the thermoluminescent (TL)
   properties of ZrO2 thin films doped with TbCl3 exposed to ultraviolet
   radiation (UVR) of wavelength in the range of 200 to 400 nm are
   reported. The photoluminescence spectrum induced by 275 nm UVR exhibited
   four maxima at 487, 542, 582, and 619 nm with the major emission at 612
   nm. The glow curve exhibited two peaks at 112 degrees C and 270 degrees
   C for all the wavelengths studied. The TL response of Tb-doped ZrO2
   exposed to 260 nm UVR was linear in the range of 40 to 240 mu J.cm(-2)
   spectral irradiance. The results showed that ZrO2:Tb has appropriate
   characteristics for use as a UV dosemeter. The high sensitivity and
