@article{ ISI:000082299000001,
Author = {de Rueda, JR and Rodriguez, R and Cotorogea, M and Castano, VM},
Title = {{Application of Wigner distribution function to fiber optics}},
Journal = {{OPTIK}},
Year = {{1999}},
Volume = {{110}},
Number = {{7}},
Pages = {{305-308}},
Abstract = {{Some of the most relevant phenomena of light propagation in waveguides
   in two dimensions (fibers) are analyzed by using the Wi,oner
   Distribution Function approach. All results, which are cumbersome to
   obtain by traditional optical methods, were simply reproduced by working
   in a four-dimensions phase space. This method reduces the traditional
   optical problem of obtaining the image of an optical system, to a simple
   determination of the transfer matrix associated to the system and simple
   products of matrices. The propagation of plane and gaussian waves within
   a waveguide was considered, and the corresponding changes in the phase
   and wavelength of the output signal are precisely predicted by this
   approach.}},
