@article{ ISI:000080917600003,
Author = {Acevedo-Chavez, R and Costas, ME},
Title = {{Spectral and magnetic studies of the polynuclear compound
   {[}Cu(II)(hypoxanthine(2-))(H2O)(2)](n)}},
Journal = {{POLYHEDRON}},
Year = {{1999}},
Volume = {{18}},
Number = {{11}},
Pages = {{1549-1553}},
Abstract = {{Three different polynuclear coordination compounds have been obtained
   from several purine derivative and isomer-Cu(II) center chemical
   reactions performed in neutral aqueous solution. One of those is
   {[}Cu(II)(hypoxanthine(2-))(H2O)(2)](n). Characterization of this
   compound suggests that hypoxanthine(2-) acts as a bridging ligand, with
   H2O molecules present in the metallic coordination sphere. The magnetic
   studies carried out on this Cu(II) compound were performed considering
   both the linear chain and the tetranuclear magnetic models of S=1/2
   coupled spins. In these studies, the superexchange magnetic coupling
   parameters obtained have been rationalized in terms of the
   hypoxanthine(2-) and the H2O ligand contributions. (C) 1999 Elsevier
   Science Ltd. All rights reserved.}},
