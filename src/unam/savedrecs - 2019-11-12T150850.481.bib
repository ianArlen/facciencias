@inproceedings{ ISI:000083350200048,
Author = {Firmani, C and Avila-Reese, V},
Editor = {{Giuricin, G and Mezzetti, M and Salucci, P}},
Title = {{Disk galaxy formation and evolution: models up to intermediate redshifts}},
Booktitle = {{OBSERVATIONAL COSMOLOGY: THE DEVELOPMENT OF GALAXY SYSTEMS}},
