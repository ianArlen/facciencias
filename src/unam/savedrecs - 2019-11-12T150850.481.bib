@inproceedings{ ISI:000084220800028,
Author = {Mondragon, A and Rodriguez-Jauregui, E},
Editor = {{DOlivo, JC and Castro, GL and Mondragon, M}},
Title = {{Breaking of flavor permutational symmetry and the CKM matrix}},
Booktitle = {{PARTICLES AND FIELDS}},
