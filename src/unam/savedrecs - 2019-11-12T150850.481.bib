@inproceedings{ ISI:000080089400126,
Author = {Dultzin-Hacyan, D},
Editor = {{Takalo, LO and Sillanpaa, A}},
Title = {{Concluding remarks}},
Booktitle = {{BL LAC PHENOMENON}},
Series = {{Astronomical Society of the Pacific Conference Series}},
Year = {{1999}},
Volume = {{159}},
Pages = {{521-525}},
Note = {{Meeting on BL Lac Phenomenon, TURKU, FINLAND, JUN 22-26, 1998}},
Abstract = {{The recent discovery of beamed gamma-rays from BL Lac objects and other
   blazars has helped to confirm the paradigm proposed 20 years ago. Our
   understanding of the underlying physics has turned from qualitative to
   quantitative in many respects; but not in all respects. The beamed jet
   paradigm that unifies BL Lac objects with other radio loud AGN is not
   free of problems and complications. It has become clear that orientation
   only cannot account for all the phenomenology. There are still open
   questions about parent population, and the idea that we seem to have two
   types of blazars (LBL \& HBL) has been questioned in this meeting on the
   basis of selection effects. We still don't know which is the dominant
   parameter of jets (if there is only one dominant parameter), and we need
   much more coordinated multifrequency monitoring, as well as theoretical
   work in order to understand the different physical processes involved in
   the origin and transfer of photons at different frequencies.}},
