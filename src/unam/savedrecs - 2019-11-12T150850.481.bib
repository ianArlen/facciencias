@article{ ISI:000085601400009,
Author = {Gutierrez-Perez, R and Percino, MJ and Chapela, VM and Alvarez, C},
Title = {{Synthesis and characterization of
   (R)-(+)-alpha-methylbenzylmethacryloylamine. A novel and versatile
   monomer}},
Journal = {{DESIGNED MONOMERS AND POLYMERS}},
Year = {{1999}},
Volume = {{2}},
Number = {{1}},
Pages = {{103-107}},
Abstract = {{We synthesized (R)-(+)-alpha-methylbenzylmethacryloylamine from the
   condensation reaction of methacryloyl chloride with
   (R)-(+)-alpha-methylbenzylamine in order to make a prototype molecule
   that can mimic some of the necessary characteristics of nonlinear
   optical (NLO) materials and that could also display potential chiral
