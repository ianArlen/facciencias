@article{ ISI:000081587500064,
Author = {Buenfil, AE and Gamboa-deBuen, I and Ruiz-Trejo, C and Olvera, C and
   Rodriguez-Villafuerte, M and Rogers, RC and Brandan, ME},
Title = {{Response of TLD-300 to heavy charged particles}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{1999}},
Volume = {{84}},
Number = {{1-4, 1}},
Pages = {{273-275}},
Note = {{12th International Conference on Solid State Dosimetry, BURGOS, SPAIN,
   JUL 05-10, 1998}},
Organization = {{Bicron Technol; Bubble Technol Ind; Caja Burgos; Comis Interminist Cienc
   \& Tecnolog; Consejo Seguridad Nucl; Direcc Gen Investigacion Cienc \&
   Tecnol; Empressa Nacl Residuos SA; Excmo Ayuntamiento Burgos; Excma
   Diputac Province Burgos; Landauer Inc; Nucl Technol Publishing; Siemens
   Environm Syst Ltd; Stillwater Sci LLC; Studsvik Instrument AB}},
Abstract = {{The TL response of CaF2:Tm to 0.7 MeV protons, 5.3 MeV alpha particles
   and 120 MeV C-12 ions has been investigated. Maximum fluences were
   typically 10(10)-10(11) cm(-2). Deconvolution techniques were applied to
   separate the glow curve into its component peaks. In all cases, the area
   of peak 3 showed a linear response as a function of fluence, for all the
   fluences studied, while peaks 5 and 6 followed a linear-supralinear
   behaviour. The departure from linearity occurs at high doses, above
   10(2) Gy. Concerning the dependence of the glow curve on type and energy
   of the incident radiation, the sensitivities of peaks 3 and 5 were
   studied as compared to the response to equal doses of gamma rays. The
   results, as a function of LET, follow the same general systematics
   observed previously by other authors. However, the quantitative
   comparison suggests that the linear energy transfer may not be the
   parameter which unifies all the observations.}},
