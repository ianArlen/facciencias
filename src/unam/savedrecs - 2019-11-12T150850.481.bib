@article{ ISI:000077218000004,
Author = {Velazquez, V and Hirsch, JG and Sun, Y},
Title = {{Self-consistency in the projected shell model}},
Journal = {{NUCLEAR PHYSICS A}},
Year = {{1998}},
Volume = {{643}},
Number = {{1}},
Pages = {{39-51}},
Month = {{NOV 16}},
Abstract = {{The projected shell model is a shell-model theory built up over a
   deformed BCS mean field. Ground state and excited bands in even-even
   nuclei are obtained through diagonalization of a pairing plus quadrupole
   Hamiltonian in an angular momentum projected 0-, 2-, and 3-quasiparticle
   basis. The residual quadrupole-quadrupole interaction strength is fixed
   self-consistently with the deformed mean field and the pairing constants
   are the same used in constructing the quasiparticle basis. Taking Dy-160
   as example, we calculate low-lying states and compare them with
   experimental data. We exhibit the effect of changing the residual
   interaction strengths on the spectra. It is clearly seen that there are
   many J(pi) = 0(+), 1(+), 4(+) bandheads whose energies can only be
   reproduced using the self-consistent strengths. It is thus concluded
   that the projected shell model is a model with essentially no free
   parameters. The predicted energy of the 2(+) bandhead lies however in
   nearly twice the experimental value. (C) 1998 Elsevier Science B.V.}},
