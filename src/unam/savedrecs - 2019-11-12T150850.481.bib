@inproceedings{ ISI:000175726300126,
Author = {Luridiana, V and Peimbert, M and Leitherer, C},
Editor = {{VanderHucht, KA and Koenigsberger, G and Eenens, PRJ}},
Title = {{Photo-ionization models of NGC 2363 and their implications}},
Booktitle = {{WOLF-RAYET PHENOMENA IN MASSIVE STARS AND STARBURST GALAXIES}},
