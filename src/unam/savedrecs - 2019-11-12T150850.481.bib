@article{ ISI:000077675900011,
Author = {Taguena-Martinez, J and Lugo, JE and del Rio, JA},
Title = {{Effects of form and surface on optical properties of porous silicon}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1998}},
Volume = {{44}},
Number = {{3}},
Pages = {{39-41}},
Month = {{DEC}},
Note = {{14th Latin American Symposiumm on Solid State Physics, OAXACA, MEXICO,
   JAN 11-16, 1998}},
Abstract = {{An effective medium approximation which includes surface contributions
   has been used to model the optical properties of porous silicon (p-Si)
   {[}1] as a bundle of parallel c-Si wires covered by different materials.
   It was previously shown that such an approach may be in the right
   direction when compared with some experimental measurements, but for
   example the effective absorption coefficient fit was out of range. Since
   the original model did not consider overlapping or interconnectivity
   between the wires, in this work, an interpolation formula {[}2] to study
   the role of the interconnectivity and the morphology on the optical
   properties is used. The new model is used to fit the absorption
   coefficient results on a sample of p-Si made with the standard
   technique.}},
