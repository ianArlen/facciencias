@article{ ISI:000082085600006,
Author = {Raggi-Cardenas, AG and Salmeron, L},
Title = {{On curves and orbits of matrices}},
Journal = {{BOLETIN DE LA SOCIEDAD MATEMATICA MEXICANA}},
Year = {{1999}},
Volume = {{5}},
Number = {{1}},
Pages = {{117-122}},
Abstract = {{Given an algebraically closed field k, we consider the action of the
   general linear group G over the space k(nxn), of n x n matrices, given
   by conjugation. We prove the following geometric fact: Let l: k -->
   k(nxn) be any curve which does not lie in some G-orbit, then there is a
   curve (l) over cap which hits exactly the same orbits as l does (with
   only finitely many possible exceptions) and satisfies the additional
   property (l) over cap(t), (l) over cap(t') are not conjugate for all
   pairs of different points t, t' in a cofinite subset of k.}},
