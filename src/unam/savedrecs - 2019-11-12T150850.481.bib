@inproceedings{ ISI:000084113500012,
Author = {Basiuk, VA and Douda, J and Navarro-Gonzalez, R},
Editor = {{Ehrenfreund, P and Robert, F}},
Title = {{Transport of extraterrestrial biomolecules to the Earth: Problem of
   thermal stability}},
Booktitle = {{LIFE SCIENCES: NEW INSIGHTS INTO COMPLEX ORGANICS IN SPACE}},
Series = {{ADVANCES IN SPACE RESEARCH}},
Year = {{1999}},
