@article{ ISI:000077039900035,
Author = {Duda, Y},
Title = {{Binary system of network-forming fluid: Study of phase stability through
   an analytical solution of the Percus-Yevick equation}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{1998}},
Volume = {{109}},
Number = {{20}},
