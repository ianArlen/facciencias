@article{ ISI:000077857500042,
Author = {Martinez, L and Amaya, M and Porcayo-Calderon, J and Lavernia, EJ},
Title = {{High-temperature electrochemical testing of spray atomized and deposited
   iron aluminides alloyed with boron and reinforced with alumina
   particulate}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{1998}},
Volume = {{258}},
Number = {{1-2}},
Pages = {{306-312}},
