@article{ ISI:000082791100007,
Author = {Gallegos, E and Warren, A and Robles, E and Campoy, E and Calderon, A
   and Sainz, MG and Bonilla, P and Escolero, O},
Title = {{The effects of wastewater irrigation on groundwater quality in Mexico}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{1999}},
Volume = {{40}},
