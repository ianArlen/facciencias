@article{ ISI:000077266400010,
Author = {Lopez-Gutierrez, AU and Riba, L and Ordonez-Sanchez, ML and
   Ramirez-Jimenez, S and Cerrillo-Hinojosa, M and Tusie-Luna, MT},
Title = {{Uniparental disomy for chromosome 6 results in steroid 21-hydroxylase
   deficiency: evidence of different genetic mechanisms involved in the
   production of the disease}},
Journal = {{JOURNAL OF MEDICAL GENETICS}},
Year = {{1998}},
Volume = {{35}},
Number = {{12}},
Pages = {{1014-1019}},
Month = {{DEC}},
Abstract = {{Congenital adrenal hyperplasia (CAH) is an inherited recessive disorder
   of adrenal steroidogenesis caused by mutations in the steroid
   21-hydroxylase gene (CYP21) in more than 90\% of affected patients. The
   CYP21 gene is located within the HLA complex locus on chromosome 6
   (6p21.3). During a molecular characterisation study of a group of 47
   Mexican families with 21-hydroxylase deficiency, we identified nine in
