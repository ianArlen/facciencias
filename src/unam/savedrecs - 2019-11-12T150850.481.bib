@article{ ISI:000077973600025,
Author = {Le Borgne, S and Palmeros, B and Valle, F and Bolivar, F and Gosset, G},
Title = {{pBRINT-T-s: a plasmid family with a temperature-sensitive replicon,
   designed for chromosomal integration into the lacZgene of Escherichia
   coli}},
Journal = {{GENE}},
Year = {{1998}},
Volume = {{223}},
Number = {{1-2}},
Pages = {{213-219}},
Month = {{NOV 26}},
Note = {{Symposium entitled a Wisconsin Gathering Honoring Waclaw Szybalski, UNIV
   WISCONSIN MADISON, MADISON, WISCONSIN, AUG 10-11, 1997}},
Abstract = {{A pBRINT-T-s family of integrative vectors for Escherichia coli was
   constructed by using a temperature-sensitive replicon derived from
   pSC101, a region of homology to the lacZ gene, and various antibiotic
   resistance markers (kanamycin, chloramphenicol and gentamycin) for
   selection of the integrants. The gene or group of genes to be integrated
   can be inserted into a multiple cloning site, flanked by an antibiotic
   resistance marker and lacZ sequences. A simple and rapid procedure was
   developed for the selection of cells where allelic exchange had
   occurred. With this procedure, the efficiency of integration of around
   10(-3) was observed, using several E. coli strains. From colonies with
   an integrated pBRINT-T-s plasmid, we detected an average allelic
   exchange event frequency of 7.5\%. As a test for this system, we
   integrated the amy gene that codes for the cc-amylase from B.
   stearothermophilus, into the lacZ gene of E, coli W3110. Production of
   alpha-amylase was found to be proportional to copy number; at up to 10
   copies per chromosome. (C) 1998 Elsevier Science B.V. All rights
   reserved.}},
