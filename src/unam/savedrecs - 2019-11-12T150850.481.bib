@article{ ISI:000081098300006,
Author = {Rodriguez, C and Campillo, B and Albarran, JL and Genesca, J and
   Caballero, LX and Perez, R},
Title = {{Corrosion behavior of electrolytic NiCoB coatings}},
Journal = {{CORROSION REVIEWS}},
Year = {{1999}},
Volume = {{17}},
Number = {{2}},
Pages = {{137-149}},
Abstract = {{The corrosion characteristics of an electrolytic alloy of NiCoB
   deposited on a low C steel (AISI-1018) have been explored by using
   electrochemical techniques. In this research the corrosion rate was
   obtained a comparison of the corrosion rates gathered with gravimetric
   methods is also included. The solutions used in this study were HCl 0.1M
   H(2)SO(4) 0.1M and KOH at 28\%. All the electrochemical tests were
   performed at room temperature. From the results obtained, the sulfuric
   acid solution showed higher corrosion activity in comparison with the
   other solutions used. Furthermore, the corrosion rates obtained are
   quite low, indicating that this electrolytic alloy has good corrosion
   resistance. This behavior could be related to the formation of one or
   several passivation films.}},
