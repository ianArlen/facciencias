@article{ ISI:000077675900019,
Author = {Mijangos, RR and Camarillo, E and Guerrero, R and Vazquez-Polo, G},
Title = {{New ternary ion crystal and its optical characterization}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1998}},
Volume = {{44}},
Number = {{3}},
Pages = {{71-73}},
Month = {{DEC}},
Note = {{14th Latin American Symposiumm on Solid State Physics, OAXACA, MEXICO,
   JAN 11-16, 1998}},
Abstract = {{A novel ternary compound (KCl)(x)(KBr)(y)(RbCl)(z) with z = y = z = 0.33
   is studied, using the optical absorption F band. The supposition of the
   compound possibility with a complete miscibility and crystallinity, to
   large concentrations, was based on theoretical and experimental studies
   of the F band in mixed binary compounds. This band obtained to room
   temperature and 18 K, reveals an electron trap associated with a
   definite lattice constant of the crystal. Using the Mollwo-Ivey law, we
   compute the lattice constant of the ternary system. Extrapolation for
   the ternary system of a Vegard law is assumed; valid only for binary
   systems. We show that lattice constants obtained with optical F band,
   Vegard law and X-ray diffractometry are in good agreement. Finally we
   calculated values of the lattice constant and F band energy using
   Mollwo-Ivey and Vegard laws for several concentrations of the ternary
   compound.}},
