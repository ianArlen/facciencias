@article{ ISI:000079920900002,
Author = {Licea, AF and Gutierrez, MC and Esparza, J and Estrada-Parra, S and
   Estrada-G, I and Quesada-Pascual, F and Possani, LD},
Title = {{Enhancement of the immunogenicity of Mycobacterium leprae peptides by
   means of polymerization}},
Journal = {{ASIA-PACIFIC JOURNAL OF MOLECULAR BIOLOGY AND BIOTECHNOLOGY}},
