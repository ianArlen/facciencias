@article{ ISI:000079260600010,
Author = {Aguilar-Roblero, R and Escandon-Calderon, J and Salazar-Juarez, A and
   Caldelas, I and Granados-Fuentes, D and Escobar, C},
Title = {{Phase relations between host and grafted SCN depend on graft location in
   rats}},
