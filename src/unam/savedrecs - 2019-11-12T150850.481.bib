@article{ ISI:000078834900012,
Author = {Apatiga, LM and Castano, VM and Alba, F},
Title = {{A study of the phosphorescence mechanism of polycrystalline diamond
   films}},
Journal = {{JOURNAL OF MATERIALS SCIENCE-MATERIALS IN ELECTRONICS}},
Year = {{1998}},
Volume = {{9}},
Number = {{6}},
Pages = {{473-476}},
Month = {{DEC}},
Abstract = {{A study of the phosphorescence mechanisms in polycrystalline diamond
   films was carried out through their thermoluminescent (TL) vanishing
   glow response. The polycrystalline diamond films phosphoresced when kept
   at room or higher temperatures after bei ng excited with a UV light
   source. The observed behaviour of shallow and deep traps during the
   phosphorescence process can be explained with a simple time-dependent
   model. The diamond film phosphorescence was induced by exciting with a
   UV light source of 4W and 254 nm wavelength. The TL vanishing glow
   curves were integrated from room temperature to 350 degrees C at a
   linear heating rate of 10 degrees C s(-1) in a N-2 atmosphere. The
   optical response of the diamond films was studied by means of its
   luminescence spectra, showing a broad emission band centered around 500
   nm.}},
