@article{ ISI:000082825200007,
Author = {Calderon, F and Siqueiros, JM and Portelles, J and Heiras, JL},
Title = {{Diffuse phase transition in (Na,Bi)-doped PbSnxTi1-xO3 ferroelectric
   ceramics}},
Journal = {{SOLID STATE COMMUNICATIONS}},
Year = {{1999}},
Volume = {{112}},
Number = {{4}},
Pages = {{219-221}},
Abstract = {{The temperature dependence of the dielectric behavior of
   Pb-0.85(Na0.5Bi0.5)(0.15)SnxTi1-xO3 (PSNB) with x = 0.40, 0.45, 0.50,
   0.55, 0.60 has been investigated, A careful analysis of the dielectric
   properties reveals a relaxer behavior near T-c where the diffuseness
   parameter (delta) increases as the content of Sn4+. It is observed that
   the temperature of the permittivity maximum shifts toward lower
   temperatures and the permittivity maximum decreases with the increase of
   Sn content in the PSNB solid solutions. The diffuseness (delta) and
   deformation structure (gamma) parameters were determined for each
   composition using the results of the dielectric measurements and
   experimental lattice constant values. (C) 1999 Elsevier Science Ltd. All
   rights reserved.}},
