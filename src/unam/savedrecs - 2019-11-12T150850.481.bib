@article{ ISI:000082498200020,
Author = {Perez-Amador, MC and Amor-Prats, D},
Title = {{Chromatographic analysis of the essential oil and flavonoids in bracts
   of Ipomoea bracteata Cav. (Convolvulaceae)}},
Journal = {{PHYTON-INTERNATIONAL JOURNAL OF EXPERIMENTAL BOTANY}},
Year = {{1999}},
Volume = {{64}},
Number = {{1-2}},
Pages = {{107-109}},
Abstract = {{The bracts of Ipomoea bracteata Cav, were analyzed to determine the
   composition of the essential oil and to identify the flavonoids present
   in this part of the flower. The essential oil composition was determined
   by gas chromatography; linalol is the main component (27.5\%). The
   flavonoids were analyzed by TLC; three anthocyanidins: cyanidin,
   peonidin, delphinidin, and one anthocyanin, pelargonidin-3-glucoside,
   were identified. Luteolin, apigenin, kaempferol and quercetin-3-xyloside
   were also present.}},
