@article{ ISI:000078125600017,
Author = {Gonzalez-Trujano, ME and Navarrete, A and Reyes, B and Hong, E},
Title = {{Some pharmacological effects of the ethanol extract of leaves of Annona
   diversifolia on the central nervous system in mice}},
Journal = {{PHYTOTHERAPY RESEARCH}},
Year = {{1998}},
Volume = {{12}},
Number = {{8}},
Pages = {{600-602}},
Month = {{DEC}},
Abstract = {{In the present study we investigated some neuropharmacological effects
   of the ethanol extract of the leaves of Annona diversifolia
   (Annonaceae). Intraperitoneal administration of the extract delayed the
   onset of clonic seizures induced by pentylenetetrazole and delayed the
   time in the `rota-rod' and `swimming' test. In addition, the extract
   augmented the duration of sleeping time induced by sodium pentobarbital.
   These results indicate that the ethanol extract of the leaves of A.
   diversifolia has depressant activity on the central nervous system. (C)
   1998 John Wiley \& Sons, Ltd.}},
