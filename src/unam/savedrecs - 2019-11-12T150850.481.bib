@article{ ISI:000082153100012,
Author = {Rodriguez-Romo, S},
Title = {{Actions of the Dipper-Donkin quantization GL(2) on the Clifford algebra
   C(1,3).}},
Journal = {{COMMUNICATIONS IN ALGEBRA}},
Year = {{1999}},
Volume = {{27}},
Number = {{9}},
Pages = {{4307-4325}},
Abstract = {{Following the method already developed for studying the actions of
   GL(q)(2, C) on the Clifford algebra C(1, 3) and its quantum invariants
   {[}1], we study the action on C(1,3) of the quantum GL(2) constructed by
   Dipper and Donkin {[}2]. We are able of proving that there exits only
   two non-equivalent cases of actions with nontrivial ``perturbation{''}
   {[}1]. The spaces of invariants are trivial in both cases.
   We also prove that each irreducible finite dimensional algebra
   representation of the quantum GL(2), q(m) not equal 1, is one
   dimensional.
   By studying the cases with zero ``perturbation{''} we find that the
   cases with nonzero ``perturbation{''} are the only ones with maximal
   possible dimension for the operator algebra R.}},
