@article{ ISI:000077675900036,
Author = {Mello, PA},
Title = {{Quantum transport through ballistic chaotic cavities: a statistical
   approach}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1998}},
Volume = {{44}},
Number = {{3}},
Pages = {{136-140}},
Month = {{DEC}},
Note = {{14th Latin American Symposium on Solid State Physics, OAXACA, MEXICO,
   JAN 11-16, 1998}},
Abstract = {{The problem of quantum chaotic scattering is addressed by means of a
   statistical model for the scattering matrix. The model, introduced in
   the past in the context of nuclear physics, describes the problem in
   terms of a prompt and an equilibrated component: it incorporates the
   average value of the scattering matrix to describe the prompt processes
   and satisfies the requirements of flux conservation, causality and
   ergodicity. The model is applied to the analysis of electronic transport
   through ballistic mesoscopic cavities: it describes well the results
   arising from the numerical solution of the Schrodinger equation for
   two-dimensional cavities.}},
