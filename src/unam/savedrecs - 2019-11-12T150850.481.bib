@article{ ISI:000082374700002,
Author = {Bohnel, H and Schnepp, E},
Title = {{Precision of the paleomagnetic method: An example from the Quaternary
   Eifel volcanics (Germany)}},
Journal = {{EARTH PLANETS AND SPACE}},
Year = {{1999}},
Volume = {{51}},
Number = {{6}},
Pages = {{403-412}},
Abstract = {{The total within-site dispersion of paleomagnetic data results from
   natural misalignment processes and experimentally produced dispersion.
   Although some of the sources of dispersion may be determined by
   experiments, this is not possible for others. The total accuracy of the
   paleomagnetic method so far has been determined only on historic lava
   flows. Thirty-seven Quaternary basaltic lava flows from the Eifel,
   Germany, have been re-sampled using the same outcrops as before. The new
   site-mean directions do not differ significantly from those of the
   original study. The angular distance between pairs of site-mean
   directions may be approximated by a Fisher distribution. Assuming that
   both studies are affected by the same natural dispersion processes, we
   obtain as the best value for the experimental dispersion s(exp) = 4.33
   degrees. The total within-site dispersion shows a log-normal
   distribution with a mean of s(tot) = 5.39 degrees. From these values we
   calculate that the dispersion due to natural processes in the Eifel
   volcanic field is s(tot) = 3.21 degrees.}},
