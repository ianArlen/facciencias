@inproceedings{ ISI:000083104400024,
Author = {Moreno, J},
Editor = {{Dion, JM and Dugard, L and Fliess, M}},
Title = {{An extension of Lucas theorem to entire functions}},
Booktitle = {{LINEAR TIME DELAY SYSTEMS (LTDS'98)}},
Year = {{1999}},
Pages = {{141-145}},
Note = {{IFAC Workshop on Linear Time Delay Systems (LTDS 98), GRENOBLE, FRANCE,
   JUL 06-07, 1998}},
Organization = {{Int Federat Automat Control, Tech Comm Linear Syst; Minist Educ Natl
   Rech \& Technol; Ecole Natl Super Ingenieurs Electriciens Grenoble; Inst
   Natl Polytech Grenoble; Ville Grenoble; CNRS, SPI}},
Abstract = {{It is a well-known consequence of Lucas theorem that the derivative of a
   Hurwitz-stable polynomial is also Hurwitz-stable. In this work it will
   be demonstrated that this property is valid for a general entire
   function, if it satisfies a uniform monotone phase increase property. An
   important class of functions for which the result is valid is that of
   quasipolynomials, which are characteristic functions of linear systems
   with delays. Some applications of this fact will be given, which seem to
   be interesting for the study of robust stability of families of entire
   functions. Copyright (C) 1998 IFAC.}},
