@article{ ISI:000076801900005,
Author = {Boldt, A and Takane, M},
Title = {{The spectral classes of unicyclic graphs}},
Journal = {{JOURNAL OF PURE AND APPLIED ALGEBRA}},
Year = {{1998}},
Volume = {{133}},
Number = {{1-2}},
Pages = {{39-49}},
Month = {{DEC 15}},
Note = {{Ring Theory Conference, MISKOLC, HUNGARY, JUL 15-20, 1996}},
Abstract = {{We study the spectral classes of a finite, connected graph without loops
   and with exactly one ``essential{''} cycle. A spectral class consists of
   all those orientations of the graph that do not contain oriented cycles
   and yield the same Coxeter polynomial. We show that, if the essential
   cycle has m vertices, then there are exactly {[}m/2] distinct spectral
   classes; the corresponding spectral radii are distinct in case the graph
   is wild. Furthermore, we give an explicit combinatorial expression for
   the entries of the powers of the Coxeter matrix of a finite quiver
   without oriented cycles. (C) 1998 Published by Elsevier Science B.V. All
   rights reserved.}},
