@article{ ISI:000083222000007,
Author = {Herrera, I and Diaz, M},
Title = {{Indirect methods of collocation: Trefftz-Herrera collocation}},
Journal = {{NUMERICAL METHODS FOR PARTIAL DIFFERENTIAL EQUATIONS}},
Year = {{1999}},
Volume = {{15}},
Number = {{6}},
Pages = {{709-738}},
Month = {{NOV}},
Abstract = {{A nonstandard collocation method (TH-collocation) is presented, where
   collocation is used to construct specialized weighting functions instead
   of the solution itself, as it is usual, so that in this sense it is an
   indirect method. TH-collocation is shown to be as accurate as standard
   collocation, but computationally far more efficient. The present article
   is the first of a series devoted to explore thoroughly collocation
   methods. The following classification of collocation methods is
   introduced: direct-nonoverlapping; indirect-nonoverlapping;
   direct-overlapping; and indirect-overlapping. Most of the effort
   reported in the literature has gone to direct-nonoverlapping methods.
   The procedure presented in this article falls into the
   indirect-nonoverlapping category and it is based on Trefftz-Herrera
   formulation. (C) 1999 John Wiley \& Sons, Inc.}},
