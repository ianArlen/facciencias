@article{ ISI:000083629400007,
Author = {Apatiga, LM and Castano, VM and Golzarri, JI and Garcia, J and Alba, F},
Title = {{Thermoluminescent behaviour of diamond films subjected to UV irradiation}},
Journal = {{MATERIALS RESEARCH INNOVATIONS}},
Year = {{1999}},
Volume = {{3}},
Number = {{3}},
Pages = {{156-159}},
Month = {{OCT}},
Abstract = {{The thermoluminescent behaviour of diamond films subjected to UV
   irradiation was studied by using an UV lamp of 254 nm wavelength. The UV
   irradiation was achieved by placing the samples 15 cm away from an UV
   source for different periods. The thermoluminescent signal was
   integrated from 0 to 350 degrees C at a linear heating rate of 10
   degrees C/s in a N-2 atmosphere. The corresponding luminescence spectra
   show an excitation band centered at 450 nm while the emission band is
   centered around 500 nm at room temperature. The diamond films were
   synthesized on molybdenum substrates by the combustion flame technique
   and characterized by Raman spectroscopy and scanning electron
   microscopy.}},
