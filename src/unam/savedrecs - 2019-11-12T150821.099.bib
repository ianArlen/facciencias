@article{ ISI:000082736600018,
Author = {Jimenez-Mier, J and Schaphorst, S and Caldwell, CD and Krause, MO},
Title = {{Auger decay of the 1s -> np (n=2-6) resonances in beryllium}},
Journal = {{JOURNAL OF PHYSICS B-ATOMIC MOLECULAR AND OPTICAL PHYSICS}},
Year = {{1999}},
Volume = {{32}},
Number = {{17}},
Pages = {{4301-4307}},
Month = {{SEP 14}},
Abstract = {{We studied the decay channels of the photon-excited 1s2s(2)np(P-1) (n =
   2-6) resonances in Be. We confirmed the predominant decay modes for n =
   2, 3 found previously by Caldwell et al (1990 Phys. Rev. A 41 542). The
   decay modes of the higher resonances are admixtures of np' ionic states,
   with n > 2. Evidence for strong shake up and shake down was found. Very
   good agreement was found with the `shake' model, which predicts the
   relative intensities of the exit channels in terms of the projections of
   the excited electron wavefunctions onto the ionic electron
   wavefunctions.}},
