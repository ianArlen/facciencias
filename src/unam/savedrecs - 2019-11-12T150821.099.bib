@article{ ISI:000084440600009,
Author = {Alvarez-Hernandez, S and De Lara-Isassi, G and Arreguin-Espinoza, R and
   Arreguin, B and Hernandez-Santoyo, A and Rodriguez-Romero, A},
Title = {{Isolation and partial characterization of giraffine, a lectin from the
   Mexican endemic alga Codium giraffa Silva}},
Journal = {{BOTANICA MARINA}},
Year = {{1999}},
Volume = {{42}},
Number = {{6}},
Pages = {{573-580}},
Month = {{NOV}},
Abstract = {{A lectin from the alga Codium giraffa, reported as an endemic species of
   Mexico, was purified by a combination of ammonium sulfate fractionation,
   affinity and anion exchange chromatographies. Giraffine, as we called
   this lectin, shares some characteristics with other lectins isolated
   from the same genus. It is a monomeric protein with a relative molecular
   weight of 17,800, srs shown by native and SDS-PAGE and confirmed by mass
   spectrometry; its isoelectric point was 6.4 and it did not show multiple
   molecular forms. The agglutinating activity of this lectin was inhibited
   with low concentrations of N-acetylglucosamine and N-acetylgalactosamine
   and was independent of divalent cations.}},
