@article{ ISI:000084718900017,
Author = {Casas, M and Puente, A and Rigo, A and Davidson, NJ and Quick, RM and
   Fortes, M and Solis, MA and De Llano, M and Navarro, O and Valladares,
   AA and Rojo, O},
Title = {{BEC-driven superconductivity in the cuprates}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{1999}},
Volume = {{13}},
Number = {{29-31}},
Pages = {{3489-3491}},
Month = {{DEC 20}},
Note = {{2nd International Conference on New Theories, Discoveries and
   Applicatins of Superconductors and Relates Materials (New3SC-2), LAS
   VEGAS, NEVADA, MAY 31-JUN 04, 1999}},
Abstract = {{We apply to cuprates a three-fluid ideal boson-fermion statistical model
   of superconductivity in two dimensions (2D) derived from three extrema
   of the system Helmholtz free energy (subject to constant total
   fermion-number) for the BCS model interaction between fermions. The same
   interactions absent in BCS theory are neglected here. As the ensuing
   bosonic Cooper pairs move not in vacuum but in a Fermi sea we employ the
   correct linear-as opposed to the commonly-assumed quadratic-dispersion
   relation in the center-of-mass momentum (CMM). More importantly, pair
   breakup beyond a certain (very small) CMM is accounted for.
   Bose-Einstein condensation (BEC) critical temperatures of about 800 K
   result for moderate coupling with cuprate parameters.}},
