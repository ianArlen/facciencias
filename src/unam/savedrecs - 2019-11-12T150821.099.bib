@inproceedings{ ISI:000087282200011,
Author = {Colome, C},
Editor = {{Sitko, ML and Sprague, AL and Lynch, DK}},
Title = {{First detection of polarization in the CO bandhead emission in young
   stellar objects}},
Booktitle = {{THERMAL EMISSION SPECTROSCOPY AND ANALYSIS OF DUST, DISKS, AND REGOLITHS}},
Series = {{Astronomical Society of the Pacific Conference Series}},
Year = {{2000}},
Volume = {{196}},
Pages = {{91-98}},
Note = {{Meeting on Thermal Emission Spectroscopy and Analysis of Dust, Disks,
   and Regoliths, LUNAR \& PLANETARY INST, HOUSTON, TX, APR 28-30, 1999}},
Organization = {{Lunar \& Planetary Inst; Natl Sci Fdn}},
Abstract = {{We report the first detection of polarization in the CO bandhead
   emission at near-infrared wavelengths (lambda similar to 2.3 mu m). In
   this paper we describe the results for three young stellar objects;
   SSV13, DG Tau and V1331 Cyg. In the first two objects the polarization
   of the CO bandhead emission (P-co) is higher than the polarization of
   their adjacent continua (P-cont), while the reverse is true for V1331
   Cyg. All three objects have dusty envelopes which scatter the emission
   of both the CO bandhead and the stellar continuum. The large differences
   between P-co and P-cont in all three objects indicate that the CO is not
   distributed symmetricaly around the young stars. And in the case of
   V1331 Cyg our polarimetric results confirm the suggestion that we are
   viewing this FU Orionis candidate along one of its poles.}},
