@article{ ISI:000083788400002,
Author = {Pankov, V and Alonso, JC and Ortiz, A},
Title = {{Moisture stability and structure relaxation processes in
   plasma-deposited SiOF films}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY A-VACUUM SURFACES AND FILMS}},
Year = {{1999}},
Volume = {{17}},
Number = {{6}},
Pages = {{3166-3171}},
Month = {{NOV-DEC}},
Abstract = {{Fluorinated silicon dioxide (SiOF) films have been prepared by remote
   plasma enhanced chemical vapor deposition using SiF4, O-2, H-2, and He
   gases. Fourier transform infrared spectroscopy has been used to study
   structural changes in SiOF films caused by interaction with atmospheric
   moisture. It is shown that the increased incorporation of fluorine into
   the SiOF network during film deposition gradually increases the value of
   the average Si-O-Si angle in the SiOF network, {[}theta], from 138
   degrees to 153 degrees. It is concluded that the value of about 144
   degrees corresponds to nearly relaxed SiOF network and therefore can be
   considered as an equilibrium angle for SiOF films. Subsequent increase
   in the fluorine concentration in the film results in {[}theta] larger
   than the equilibrium {[}theta] ({''}overequilibrium{''}) and causes
   structural relaxation of the SiOF network towards the equilibrium
   {[}theta] during the post-deposition period. Both ``under-{''} and
   ``overequilibrium{''} values of {[}theta] seem to be the sources of SiOF
   film structural instability and increased reactivity with moisture.
   However, film hydrolysis and structural relaxation processes in the SiOF
   films characterized by ``underequilibrium{''} values of {[}theta] are
   effectively suppressed by F presence in moderate concentrations. On the
   contrary, SiOF films characterized by ``overequilibrium{''} {[}theta]
   have low density network and heavily absorb atmospheric water, which
   strongly promotes both structural relaxation and hydrolysis during the
   post-deposition period. (C) 1999 American Vacuum Society.
   {[}S0734-2101(99)02406-9].}},
Publisher = {{AMER INST PHYSICS}},
Address = {{CIRCULATION FULFILLMENT DIV, 500 SUNNYSIDE BLVD, WOODBURY, NY 11797-2999
