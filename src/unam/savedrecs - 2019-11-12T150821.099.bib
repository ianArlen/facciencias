@article{ ISI:000084325200036,
Author = {O'Connor, D and Stephens, CR},
Title = {{Universal scaling properties of superconductors in magnetic fields}},
Journal = {{PHYSICS LETTERS A}},
Year = {{1999}},
Volume = {{263}},
Number = {{4-6}},
Pages = {{439-445}},
Month = {{DEC 6}},
Abstract = {{Based on renormalization group arguments we establish that for a
   superconductor in the presence of a weak magnetic field the dependence
   on the magnetic induction, B, and the deviation from the zero-field
   critical temperature, tau, of a thermodynamic quantity, P, takes the
   scaling form P = tau(theta)X(B/Phi(0)tau(-2 nu), q tau(-nu omega e)),
   where theta and nu are XY exponents, q is the scaled electromagnetic
   coupling and nu omega(e) is the associated crossover exponent. For
   q/tau(nu omega e) << 1, the experimentally accessible region in high-T-c
   superconductors, there is a reduction to one-variable scaling plus small
   corrections. In this region we find the shift in the specific heat
   maximum is given by Delta = x(0)(B/Phi(0))(1/2 nu) and that the singular
   part of the free energy at the critical temperature takes the form
   F-sing = c(d)(B/Phi(0))(d/2) where c(d) is a universal amplitude. A one
   loop approximation in three dimensions gives c(3)similar to 0.22. The
   results presented here should have equal applicability to the nematic to
   smectic-A transition. (C) 1999 Published by Elsevier Science B.V. All
   rights reserved.}},
