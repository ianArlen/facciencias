@article{ ISI:000084227300026,
Author = {Ballesteros-Paredes, J and Hartmann, L and Vazquez-Semadeni, E},
Title = {{Turbulent flow-driven molecular cloud formation: A solution to the
   post-T tauri problem?}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1999}},
Volume = {{527}},
Number = {{1, 1}},
Pages = {{285-297}},
Month = {{DEC 10}},
Abstract = {{We suggest that molecular clouds can be formed on short timescales by
   compressions from large scale streams in the interstellar medium (ISM).
   In particular, we argue that in the Taurus-Auriga complex, with
   filaments of 10-20 x 2-5 pc, most have been formed by H I flows in less
   than or similar to 3 Myr, explaining the absence of post-T Tauri stars
   in the region with ages greater than or similar to 3 Myr. Observations
   in the 21 cm line of the H I ``halos{''} around the Taurus molecular gas
   show many features (broad asymmetric profiles, velocity shifts of H I
   relative to (CO)-C-12) predicted by our MHD numerical simulations, in
   which large-scale H I streams collide to produce dense filamentary
   structures. This rapid evolution is possible because the H I hows
   producing and disrupting the cloud have much higher velocities (5-10 km
   s(-1)) than are present in the molecular gas resulting from the
   colliding flows. The simulations suggest that such flows can occur from
   the global ISM turbulence without requiring a single triggering event
   such as a supernova explosion.}},
