@article{ ISI:000085620000010,
Author = {Acuna, L and Bruner, CA},
Title = {{Factorial structure of the social support questionnaire of Sarason,
   Levine, Basham, and Sarason in Mexico}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1999}},
Volume = {{16}},
Number = {{2}},
Pages = {{267-279}},
Month = {{DEC}},
Abstract = {{The study aimed at determining the factor structure of the Social
   Support Questionnaire (SSQ) by Sarason, Levine, Basham, and Sarason
   (1983) using Mexican students Bs subjects. The SSQ consists of two
   independent scales: number of support persons (N score) and satisfaction
   with the support (S score). One hundred and fifty four senior high
   school and 495 undergraduate students responded to a
   transalted-retranslated Spanish version of the SSQ. Separate principal
   component factor analyses for the N and S scores showed that all the
   items loaded .52 or more in the first factor. An oblique rotation for
   the N scores and a varimax rotation for the S scores, showed two factors
   with items loading .30 or more. The first factor seemed to reflect an
   emotional support dimension, and the second factor reflected support
   during crisis situations. The results from the present study showed that
   the Spanish version of the SSQ can be reliably used to asses social
   support. Results are discussed with respect to their similarities with
   the data of the original English version of the SSQ.}},
