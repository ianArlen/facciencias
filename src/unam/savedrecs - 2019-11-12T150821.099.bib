@article{ ISI:000082854800005,
Author = {Eudave-Munoz, M and Wu, YQ},
Title = {{Nonhyperbolic Dehn fillings on hyperbolic 3-manifolds}},
Journal = {{PACIFIC JOURNAL OF MATHEMATICS}},
Year = {{1999}},
Volume = {{190}},
Number = {{2}},
Pages = {{261-275}},
Month = {{OCT}},
Abstract = {{In this paper we will give three infinite families of examples of
   nonhyperbolic Dehn fillings on hyperbolic manifolds. A manifold in the
   first family admits two Dehn fillings of distance two apart, one of
   which is toroidal and annular, and the other is reducible and partial
   derivative-reducible. A manifold in the second family has boundary
   consisting of two tori, and admits two reducible Dehn fillings. A
   manifold in the third family admits a toroidal filling and a reducible
   filling with distance 3 apart. These examples establish the virtual
   bounds for distances between certain types of nonhyperbolic Dehn
   fillings.}},
