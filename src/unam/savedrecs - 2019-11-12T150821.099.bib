@article{ ISI:000085620000011,
Author = {Hernandez-Guzman, L and Lopez-Morales, EG},
Title = {{Grandparents: An ignored resource in psychological assessment, diagnosis
   and intervention}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1999}},
Volume = {{16}},
Number = {{2}},
Pages = {{281-290}},
Month = {{DEC}},
Abstract = {{The influence of the extended family on child development has been
   traditionally ignored by professional psychologists in Latin-American
   countries, where a high percentage of households include one or more
   extended family members, usually grandparents. Psychological assessment,
   diagnosis, and intervention will necessarily have to take into account
   family relations, including relationships with grandparents, in order to
   successfully promote and maintain children's and family well-being.
   Grandparents usually play the role of co-caregivers. Research literature
   recommends participation of grandparents in everyday childrearing, if
   some conditions related to discipline styles are met. Grandparents' as
   co-caregivers seem to act as protective factors promoting child and
   family well being, specially during stressful life events.}},
