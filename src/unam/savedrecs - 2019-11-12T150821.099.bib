@article{ ISI:000085620000006,
Author = {Villalobos, JA and Cruz, AV and Rosenzuaig, MB and Sanchez, PR},
Title = {{Social representations on the political conflict in Chiapas and its
   relationships with some social variables}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1999}},
Volume = {{16}},
Number = {{2}},
Pages = {{227-235}},
Month = {{DEC}},
Abstract = {{The purpose of this study was to determine the social representations on
   the political conflict in Chiapas (Mexico) developed by different
   college student groups as well as the relationships between those
   representations and some social measures. Several measures
   (questionaires and semantic differential scales) were designed by means
   of reliability analysis and factorial analysis, which were applied to
   several samples of college students. In orden to analize the differente
   social representations a cluster analysis of the perception of the
   causes of the conflict and of the positions of principal political
   actors was made. The social representations found correspond
   approximately to pro-governmental, pro-zapatist and neutral positions.
   In addition, significant relations were found between the groups
   associated to each representation and the measures of authoritarism and
   conservatism.}},
