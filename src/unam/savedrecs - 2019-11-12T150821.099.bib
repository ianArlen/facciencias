@article{ ISI:000084151300013,
Author = {Coll, P and Coscia, D and Smith, N and Gazeau, MC and Ramirez, SI and
   Cernogora, G and Israel, G and Raulin, F},
Title = {{Experimental laboratory simulation of Titan's atmosphere: aerosols and
   gas phase}},
Journal = {{PLANETARY AND SPACE SCIENCE}},
Year = {{1999}},
Volume = {{47}},
Number = {{10-11}},
Pages = {{1331-1340}},
