@article{ ISI:000082524800050,
Author = {Armas, B and Herrera, MD and Combescure, C and Sibieude, F and Thenegal,
   D},
Title = {{Low-pressure chemical vapour deposition of mullite coatings in a
   cold-wall reactor}},
Journal = {{JOURNAL DE PHYSIQUE IV}},
