@article{ ISI:000085620000002,
Author = {Cruz-Morales, SE and Cervantes, GR and Romero, JG and Lopez, Y and
   Secundino, I},
Title = {{Scopolamine produces anterograde amnesia in inhibitory avoidance trained
   with different magnitudes of reinforcement}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1999}},
Volume = {{16}},
Number = {{2}},
Pages = {{195-202}},
Month = {{DEC}},
Abstract = {{The administration of anticholinergic drugs post and:pre-training
   induces impairment in retention. Several behavioral variables related
   with training procedures such as magnitude of reinforcement and degree
   of training prevent the amnesic effects of anticholinergic drugs
   administered after training. To find out if the magnitude of
   reinforcement is also important when treatments are given before
   training, male Wistar rats were trained with footshocks of different
   intensities (0.5, 1.5 or 2.5 mA) in inhibitory avoidance after the
   administration of scopolamine (0.5, 1 or 8 mg/kg); retention latencies
   were:evaluated 24 hrs later. Additional tests for state dependent
   learning and motor activity were realized. Scopolamine induced
   anterograde amnesia in all shock intensities; this impairment is not
   attributable to changes in motor activity or state dependency.}},
