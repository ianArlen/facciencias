@article{ ISI:000088475900037,
Author = {Castilla, P and Meraz, M and Monroy, O and Noyola, A},
Title = {{Anaerobic treatment of low concentration waste water in an inverse
   fluidized bed reactor}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
