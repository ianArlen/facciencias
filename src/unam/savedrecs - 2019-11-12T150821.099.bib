@article{ ISI:000083090900009,
Author = {Siguenza, R and Flores, N and Hernandez, G and Martinez, A and Bolivar,
   F and Valle, F},
Title = {{Kinetic characterization in batch and continuous culture of Escherichia
   coli mutants affected in phosphoenolpyruvate metabolism: differences in
   acetic acid production}},
Journal = {{WORLD JOURNAL OF MICROBIOLOGY \& BIOTECHNOLOGY}},
Year = {{1999}},
Volume = {{15}},
Number = {{5}},
Pages = {{587-592}},
Month = {{OCT}},
Abstract = {{The growth kinetics of an Escherichia coli wild type strain and two
   derivative mutants were examined in batch cultures and in
   glucose-limited chemostats. One mutant (PB12) had an inactive
   phosphotranferase transport system and the other (PB25) had interrupted
   pykA and pykF genes that code for the two pyruvate kinase isoenzymes. In
   both batch and continuous culture, important differences in acetic acid
   accumulation and other metabolic activities were found. Compared to the
   wild type strain, we observed a reduction in acetic acid accumulation of
   25 and 80\% in PB25 and PB12 strains respectively, in batch culture.
   Continuous culture experiments revealed that compared to the other two
   strains, PB25 accumulated less acetic acid as a function of dilution
   rate. In continuous cultures, oxidoreductase metabolic activities were
   substantially affected in the two mutant strains. These changes in turn
   were reflected in different levels of biomass and CO2 production, and in
   oxygen consumption.}},
