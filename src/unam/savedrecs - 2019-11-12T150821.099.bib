@article{ ISI:000083007000004,
Author = {Jazcilevich, A and Fuentes-Gea, V},
Title = {{The conservative random choice method for the numerical solution of the
   advection equation}},
Journal = {{MONTHLY WEATHER REVIEW}},
Year = {{1999}},
Volume = {{127}},
Number = {{10}},
Pages = {{2281-2292}},
Month = {{OCT}},
Abstract = {{A version of the random choice method (RCM), which we call the
   conservative random choice method (CRCM), is introduced to solve the
   variable coefficient linear advection equation. CRCM is an advection
   scheme that does not create numerical diffusion nor dispersion, enabling
   it to preserve initial conditions, discontinuities, and other nonsmooth
   features. It is capable of handling the variable coefficients case
   without creating or destroying mass, as is the case of the original RCM.
   These properties make CRCM a useful scheme for applications such as
   atmospheric and water transport.}},
