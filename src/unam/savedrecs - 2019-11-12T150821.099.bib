@inproceedings{ ISI:000185752400004,
Author = {Skiba, YN and Parra-Guevara, D},
Editor = {{Shen, S}},
Title = {{Assessment of pollution concentrations and control of industrial
   emissions}},
Booktitle = {{WORLD RESOURCE REVIEW, VOL 12, NOS 2-4: GLOBAL WARMING SCIENCE \&
   POLICY, PTS 1-3}},
Series = {{GLOBAL WARMING INTERNATIONAL CENTER MONOGRAPH SERIES}},
Year = {{2000}},
Pages = {{253-268}},
Note = {{11th International Conference on Global Warming Science and Policy,
   BOSTON, MA, APR 26-28, 2000}},
Abstract = {{A two-dimensional air pollution transport problem and its adjoint are
   formulated in a limited area using industrial emissions as the pollution
   sources. Solutions of the main and adjoint transport models are used to
   estimate mean pollution concentration in some ecologically important
   zone. Three strategies of control over the pollutants released into the
   atmosphere by industrial plants are suggested to keep mean pollution
   concentration in such a zone below a maximum permissible value (a
   sanitary norm). The first strategy imposes restrictions on the integral
   values of the pollutants emitted by industrial plants over whole time
   interval (0,7). The second strategy determines an optimal temporal
   structure of the emission rates in (0,7), whilst the third one sets the
   maximum allowable invariant emission rates in (0,T). All the strategies
   use the adjoint problem solution and guaranty the fulfillment of the
   sanitary norm in the zone. From the point of view of the environmental
   protection, the first strategy is better. However, it is too complicated
   and restrictive for the industrial activity, since it requires the
   plants to stop working for some periods.}},
