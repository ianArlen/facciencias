@article{ ISI:000083147100008,
Author = {Rodriguez-Romo, S},
Title = {{Dipper-Donkin algebra as global symmetry of quantum chains}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{1999}},
Volume = {{32}},
Number = {{40}},
Pages = {{7017-7030}},
Month = {{OCT 8}},
Abstract = {{We analyse the role of GL(2), a quantum group constructed by Dipper and
   Donkin (1991 Proc. London Math. Sec. 63 165-211), as a global symmetry
   for quantum chains, and show how to construct all possible Hamiltonians
   for four-state quantum chains with GL2 global symmetry. In doing this,
   we search for and present all inner actions of GL2 on the Clifford
   algebra C(1, 3). We also introduce the corresponding operator algebras,
   invariants and Hamiltonians, explicitly.}},
