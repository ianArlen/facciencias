@article{ ISI:000083825300001,
Author = {Cole, GA and Schuster, WJ and Parrao, L and Moreno, E},
Title = {{The mass of the Milky Way from a uvby-beta survey of high-velocity stars}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{1999}},
Volume = {{35}},
Number = {{2}},
Pages = {{111-122}},
Month = {{OCT}},
Abstract = {{The highest-velocity stars from our uvby - beta photometric survey have
   been selected in the Toomre energy diagram and used to estimate a lower
   bound to the local escape speed, in the range 420-470 km s(-1). A
   careful analysis of the observational errors in the stellar distances,
   proper motions, and radial velocities, and their propagation into the
   total speeds has been carried out. This local escape speed plus six
   models for the Galactic mass distribution, from simple one-component
   models, spheroid or disk, to more realistic 2- and S-component ones,
   give estimates for the Galactic size and total mass. A conservative
   measure for the size lies in the range greater than or similar to 20 -
   30 kpc, and for the total mass greater than or similar to 2.0 - 2.5 x
   10(11) M-circle dot. The last observed point of the Galactic rotation
   curve would suggest a Galactic size greater than or similar to 30 - 50
   kpc and a total mass greater than or similar to 3.2 - 3.6 x 10(11)
   M-circle dot. Our fastest star, W7547, with V-T = 509 km s(-1) but
   sigma(VT) similar to 100 km s(-1), implies greater than or similar to 45
   kpc and greater than or similar to 0.5 x 10(12) M-circle dot. Some of
   these models suggest that as much as 85\% of the Galactic mass may be in
   the form of dark matter, and that about 50\% of the total mass may lie
   in an outer dark-halo fall-off region.}},
