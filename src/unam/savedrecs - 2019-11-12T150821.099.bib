@article{ ISI:000083261100012,
Author = {Zhu, YQ and Hsu, WK and Terrones, M and Grobert, N and Hu, WB and Hare,
   JP and Kroto, HW and Walton, DRM and Terrones, H},
Title = {{Microscopy study of the growth process and structural features of
   silicon oxide nanoflowers}},
Journal = {{CHEMISTRY OF MATERIALS}},
Year = {{1999}},
Volume = {{11}},
Number = {{10}},
Pages = {{2709-2715}},
Month = {{OCT}},
Abstract = {{High-quality SiOx nanoflowers have been generated by heating mixed
   SiC/Co powder films in a CO atmosphere. The structural features
   associated with different stages of the growth were monitored by SEM,
   TEM, EDX, and XRD analysis. A plausible mechanism for the formation of
   exquisite flowerlike structures is proposed in which SiC reacts with CO,
   the Co particles acting as a catalytic template, to yield amorphous SiOx
   branches. Numerous loops and branches were also observed which may arise
   either as a result of bifurcation or coalescence. Use of Co2O3 instead
   of Co as catalyst leads to abnormal flowers exhibiting secondary growth.}},
