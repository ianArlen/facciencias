@article{ ISI:000083761300002,
Author = {Mendoza-Anaya, D and Gonzalez-Martinez, P and Rodriguez-Lugo, V and
   Martinez, E and Castano, VM},
Title = {{gamma-radiation-induced thermoluminescence of Fe-doped silica gels}},
Journal = {{JOURNAL OF MATERIALS SCIENCE-MATERIALS IN ELECTRONICS}},
Year = {{1999}},
Volume = {{10}},
Number = {{9}},
Pages = {{623-625}},
Month = {{DEC}},
Abstract = {{Silica gels were prepared by the sol-gel method containing varying
   amounts of Fe. The gels were irradiated with gamma-ray doses in the
   range from 0.3 to 11.5 kGy. The thermoluminescent response was
   characterized before and after irradiation. When the samples were
   irradiated in the range 0.3 to 1.0 kGy, no thermoluminescent response
   was detected. However, when the samples were irradiated in the range
   from 1.0 to 11.5 kGy, they showed a clear thermoluminescent behavior and
   a linear dose-response relationship over the whole irradiation range,
   opening up the possibility of employing this system as a radiation
   dosimeter.}},
