@article{ ISI:000083615800012,
Author = {Lemus, R and Frank, A},
Title = {{General anharmonic local mode approach to molecular vibrations}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
