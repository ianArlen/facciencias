@article{ ISI:000085174800006,
Author = {Bruce, NC},
Title = {{The enhanced backscatter effect with the source and detector at finite
   distances}},
Journal = {{WAVES IN RANDOM MEDIA}},
Year = {{2000}},
Volume = {{10}},
Number = {{1}},
Pages = {{71-82}},
Month = {{JAN}},
Abstract = {{Numerical results are presented of double-scatter Kirchhoff calculations
   for the scattering of light from randomly rough surfaces with the source
   and detector at finite distances. The surfaces considered are
   one-dimensional perfectly conducting surfaces and shadowing is included
   explicitly in the calculations. For the case of the source and detector
   at the same distance, the scattered intensity distribution is
   insensitive to the value of this distance except when it is of the order
   of the surface size. In that case the intensity distribution changes but
   the backscatter enhancement factor stays constant. For the source and
   detector at different distances, the backscatter enhancement is less
   than the enhancement for the case of the source and detector at the same
   distance.}},
