@article{ ISI:000084313100016,
Author = {Hirsch, JG and Hess, PO and Civitarese, O},
Title = {{Boson expansion techniques, the Pauli principle, and the quasiparticle
   random phase approximation phase transition}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{1999}},
Volume = {{60}},
Number = {{6}},
Month = {{DEC}},
Abstract = {{The onset of instabilities, in the standard proton-neutron quasiparticle
   random phase approximation (QRPA), is investigated by using different
   boson mappings and trial states belonging to a representation of
   coherent states. The model describes pairing and proton-neutron
   interactions. It is shown that an exact mapping of the Hamiltonian is
   needed to describe correctly the QRPA phase transition, i.e., that the
   algebra of all operators participant in the Hamiltonian should be
   preserved by the boson mapping. Spurious components of the wave
   functions, which appear if the Pauli principle is violated, are isolated
   by construction {[}S0556-2813(99)05511-9].}},
