@article{ ISI:000082661100002,
Author = {Robledo, A},
Title = {{Renormalization group, entropy optimization, and nonextensivity at
   criticality}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{1999}},
Volume = {{83}},
Number = {{12}},
Pages = {{2289-2292}},
Month = {{SEP 20}},
Abstract = {{The scaling properties of pair correlations at criticality are
   reproduced through an equivalence between random walk distributions and
   order parameter correlations. The shift from Gaussian to fractal walks
   with self-similar clusters corresponds to the changeover from a Gaussian
   to a nontrivial fixed paint with nonvanishing dimensional anomaly. We
   show that the renormalization group trajectories lead to fixed points of
   minimum entropy, and use the Tsallis entropy index q to measure
   nonextensivity as behavior departs from Gaussian.}},
