@article{ ISI:000082424000008,
Author = {Valdez, E and Martinez, MD and Gomez, A and Cedillo, R and Arellano, J
   and Perez, ME and Ramos, F and Moran, P and Gonzalez, E and Valenzuela,
   O and Melendro, EI and Ramiro, M and Kretschmer, R and Munoz, O and
   Ximenez, C},
Title = {{HLA characterization in adult asymptomatic cyst passers of Entamoeba
   histolytica/E. dispar}},
Journal = {{PARASITOLOGY RESEARCH}},
Year = {{1999}},
Volume = {{85}},
Number = {{10}},
Pages = {{833-836}},
Month = {{OCT}},
Abstract = {{The present work aimed at studying: the possible association of HLA
   antigens with Entamoeba histolytica/E. dispar asymptomatic infection in
   a Mexican mestizo population. A case-control design was selected for
   evaluation of the role of genetic markers in parasite infection. For
   this purpose the HLA-A, HLA-B, and HLA-DR profiles of a population of
   asymptomatic E. histolytica/E. dispar adult cyst passers (cases) and a
   corresponding nonparasitized adult group (controls) followed for 12
   months were identified. Entamoeba species were identified through
   zymodeme patterns and/or amplification of species-specific DNA
   sequences. A healthy, nonparasitized group of individuals was included
   as a control. Our results show that apparently, no specific HLA marker
   is associated with the asymptomatic cyst passers' condition. These
   findings have to be added to previous results in which, in contrast to a
   demonstrated association between HLA-DR3 and amebic liver abscess in
   Mexican mestizo adults and infants, no significant association with
   amebic rectocolitis was found.}},
