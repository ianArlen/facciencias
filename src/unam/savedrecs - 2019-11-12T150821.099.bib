@article{ ISI:000085620000004,
Author = {Solana-Figueroa, R and Quirarte, GL and Prado-Alcala, RA},
Title = {{Effects of pre-training systemic administration of p-chloroamphetamine
   on inhibitory avoidance trained with high and low foot-shock}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1999}},
Volume = {{16}},
Number = {{2}},
Pages = {{211-215}},
Month = {{DEC}},
Abstract = {{Acetylcholine is critically involved in memory of aversively-motivated
   conditioned behaviors. In situations where learning involves high levels
   of aversive stimulation, however, blockade of this neurotransmitter does
   not produce memory impairments. The aim of this work was to determine
   whether the known amnesic effects of p-chloroamphetamine, which produces
   acute release of serotonin, can also be prevented by increasing the
   strength of training. Rats were trained in inhibitory avoidance using a
   foot-shock of 1.25 or 2.0 mA. Independent groups were injected, ip, with
   isotonic saline or with 2.5 mg/kg of PCA 30 min before training.
   Retention of the task was measured 24 h afterwards. A retention
   impairment was found after training regardless of footshock intensity.
   These results confirm the amnesic effects of PCA, but there was no
   evidence for a protective effect of a high level of aversive
   stimulation.}},
Publisher = {{SOCIEDAD MEXICANA PSICOLOGIA}},
Address = {{APARTADO POSTAL 22-211, TLALPAN 14000, MEXICO}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Prado-Alcala, RA (Reprint Author), Univ Nacl Autonoma Mexico, Ctr Neurobiol, POB 70-228, Mexico City 04510, DF, Mexico.
