@article{ ISI:000083336300006,
Author = {Bozhevolnyi, SI and Xiao, MF and Hvam, JM},
Title = {{Polarization-resolved imaging with a reflection near-field optical
   microscope}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA A-OPTICS IMAGE SCIENCE AND
   VISION}},
Year = {{1999}},
Volume = {{16}},
Number = {{11}},
Pages = {{2649-2657}},
Month = {{NOV}},
Abstract = {{Using a rigorous microscopic point-dipole description of probe-sample
   interactions, we study imaging with a reflection scanning near-field
   optical microscope. Optical content, topographical artifacts,
   sensitivity window-i.e., the scale an which near-field optical images
   represent mainly optical contrast-and symmetry properties are considered
   for optical images obtained in constant-distance mode for different
   polarization configurations. We demonstrate that images obtained in
   cross-polarized detection mode are free of background and topographical
   artifacts and that the cross-circular polarization configuration is
   preferable to the cross-linear one, since it ensures more isotropic (in
   the surface plane) near-field imaging of surface features. The numerical
   results are supported with experimental near-field images obtained by
   using a reflection microscope with an uncoated fiber tip. (C) 1999
   Optical Society of America {[}S0740-3232(99)01311-3].}},
