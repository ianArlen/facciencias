@article{ ISI:000083453200007,
Author = {Martinez-Morales, JL},
Title = {{Variational data fitting}},
Journal = {{INDIAN JOURNAL OF PURE \& APPLIED MATHEMATICS}},
Year = {{1999}},
Volume = {{30}},
Number = {{11}},
Pages = {{1125-1155}},
Month = {{NOV}},
Abstract = {{Given a dense set of points lying on or near an embedded curve Gamma(0)
   subset of R(n) in Euclidean space, the curve fitting problem is to find
   an embedding F:S(1) --> R(n) that approximates Gamma(0) in the sense of
   least squares. When the dataset is modeled by a probability
   distribution, the fitting problem reduces to that of finding; an
   embedding that minimizes E(d){[}F], the expected square of the distance
   from a point in R(n) to F(S(1)).
   This approach to die fitting problem, however, is guaranteed to fail
   because the functional E(d) has no local minima This problem is
   addressed by adding a small multiple epsilon of the elastic energy of
   the embedding map to the expected square of the distance. Techniques
   from the calculus of variations are then used to study this modified
   functional. In this paper, necessary conditions that a local minimum
   satisfies are given and smooth dependence of critical curves on both the
   probability distribution and the parameter epsilon is demonstrated.}},
