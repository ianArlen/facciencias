@article{ ISI:000084540000006,
Author = {Sepulveda, MLH and Rocha, GFQ and Brumbaugh, GW and Montiel, J and
   Rodriguez, S and de Morales, EC},
Title = {{Lack of beneficial effects of bethanechol, imipramine or furosemide on
   seminal plasma of three stallions with urospermia}},
Journal = {{REPRODUCTION IN DOMESTIC ANIMALS}},
Year = {{1999}},
Volume = {{34}},
Number = {{6}},
Pages = {{489-493}},
Month = {{DEC}},
Abstract = {{Three adult stallions with urospermia were treated with bethanechol,
   imipramine or furosemide in a cross-over manner to evaluate the effect
   of those drugs on biochemical constituents of their seminal plasma.
   Results indicated that contamination of semen by urine was not prevented
   by these drugs. All three drugs increased concentrations of urea and
   proteins in seminal plasma: concentrations of calcium were decreased by
   all three drugs. It was concluded that these drugs, as used, were not
   efficacious for treatment of urospermia of these stallions. More
   information is needed about the pathophysiology of urospermia of
   stallions in order to adequately direct the selection of proper
   treatment. Response to medication may be useful for pathophysiologic
   diagnosis of individual stallions, but treatment should be
   individualized and objectively evaluated.}},
