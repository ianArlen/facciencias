@article{ ISI:000402484000001,
Author = {Shrestha, Sujan and Luis Dominguez-Juarez, Jorge and Vuong, Luat T.},
Title = {{Light-induced electrohydrodynamic instability in plasmonically absorbing
   gold nanofluids}},
Journal = {{PHYSICAL REVIEW FLUIDS}},
Year = {{2017}},
Volume = {{2}},
Number = {{6}},
Month = {{JUN 1}},
Abstract = {{Plasmonically absorbing nanofluids exhibit light-induced
   electrokinetics. We measure an electrical response to the light-induced
   Rayleigh-Benard-Marangoni convective instabilities in
   gold-polyvinylpyrrolidone (PVP) nanoparticles (NPs) suspended in
   isopropanol and water. Microampere current oscillations are measured and
   attributed to the presence of the Au-PVP NPs with negative zeta
   potential, in correspondence with the accompanying thermal lens
   oscillations and a nanofluid thermoelectric effect. The measured
   electrical oscillations represent an electrohydrodynamic stability
   driven by light, one among many that should be observed with plasmonic
   nanoparticles in liquids.}},
