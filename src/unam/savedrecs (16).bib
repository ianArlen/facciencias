@article{ ISI:000402399700005,
Author = {Austrich-Olivares, Joan A. and Garcia-Chung, Angel and David Vergara, J.},
Title = {{Instanton solutions on the polymer harmonic oscillator}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{2017}},
Volume = {{34}},
Number = {{11}},
Month = {{JUN 8}},
Abstract = {{We have computed, using instanton methods, the first allowed energy band
   for the polymer harmonic oscillator. The eigenvalues within the band are
   labelled by a discrete parameter. which results from the
   non-separability of the polymer Hilbert space. It is shown throughout
   the article the role played by. in the full quantization of the polymer
   harmonic oscillator. The result is consistent with the band structure of
   the standard quantum pendulum but with pure point spectrum.
   Consequently, an effective infinite degeneracy emerges in the formal
   limit mu/l(0) -> 0 where l(0) is the characteristic length of the vacuum
   eigenfunction of a quantum harmonic oscillator.}},
