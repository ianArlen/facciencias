@article{ ISI:000403221300003,
Author = {Gavin, J. and de Lucio, O. G. and DuBois, R. D.},
Title = {{Triply differential measurements of single ionization of argon by 1-keV
   positron and electron impact}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2017}},
Volume = {{95}},
Number = {{6}},
Month = {{JUN 14}},
Abstract = {{By establishing coincidences between target ions and scattered
   projectiles, and coincidences between target ions, scattered
   projectiles, and ejected electrons, triply differential cross-section
   (TDCS) information was generated in terms of projectile energy loss and
   scattering angles for interactions between 1-keV positrons and electrons
   and Ar atoms. The conversion of the raw experimental information to the
   TDCS is discussed. The single-ionization TDCS exhibits two
   distinguishable regions (lobes) where binary and recoil interactions can
   be described by two peaks. A comparison of the positron and electron
   impact data shows that the relative intensity of both binary and recoil
   interactions decreases exponentially as a function of the momentum
   transfer and is larger when ionization is induced by positron impact,
   when compared with electron impact.}},
