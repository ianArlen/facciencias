@article{ ISI:000404581500081,
Author = {Campos-Parra, Alma D. and Cuamani Mitznahuatl, Gerardo and
   Pedroza-Torres, Abraham and Vazquez Romo, Rafael and Porras Reyes, Fany
   Iris and Lopez-Urrutia, Eduardo and Perez-Plasencia, Carlos},
Title = {{Micro-RNAs as Potential Predictors of Response to Breast Cancer Systemic
   Therapy: Future Clinical Implications}},
