@article{ ISI:000403633300008,
Author = {Trevino, Samuel and Vazquez-Roque, Ruben A. and Lopez-Lopez, Gustavo and
   Perez-Cruz, Claudia and Moran, Carolina and Handal-Silva, Anabella and
   Gonzalez-Vergara, Enrique and Flores, Gonzalo and Guevara, Jorge and
   Diaz, Alfonso},
Title = {{Metabolic syndrome causes recognition impairments and reduced
   hippocampal neuronal plasticity in rats}},
Journal = {{JOURNAL OF CHEMICAL NEUROANATOMY}},
Year = {{2017}},
Volume = {{82}},
Pages = {{65-75}},
Month = {{JUL}},
Abstract = {{Metabolic syndrome (MS) is a serious public health problem, which can
   promote neuronal alterations in cognitive regions related to learning
   and memory processes, such as the hippocampus. However, up to now there
   has been information of a regional segregation of this damage. In this
   study, we evaluate the MS effect on the neuronal morphology of the
   hippocampus. Our results demonstrate that 90 days of a high-calorie diet
   alters the metabolic energy markers causing the MS and causes memory
   impairments, evaluated by the recognition of novel objects test (NORT).
   In addition, MS animals showed significant differences in dendritic
   order, total dendritic length and density of dendritic spines in CA1,
   CA3 and the dentate gyrus (DG) of the hippocampal area, compared with
   rats fed with a normocaloric diet (vehicle group). Furthermore, the
   immunoreactivity to synaptophysin (Syp) decreased in the hippocampus of
   the MS animals compared to the vehicle group. These results indicate
   that metabolic alterations induced by the MS affect hippocampal
   plasticity and hippocampal dependent memory processes. (C) 2017 Elsevier
   B.V. All rights reserved.}},
