@article{ ISI:000438985600006,
Author = {Rojas-Miranda, Mario and Rodriguez-Medina, Oscar},
Title = {{Two cycles of monetary easing in Mexico: 2009 And 2013-2014}},
Journal = {{CLIO AMERICA}},
