@article{ ISI:000402985800001,
Author = {Garcia-Tecocoatzi, H. and Bijker, R. and Ferretti, J. and Santopinto, E.},
Title = {{Self-energies of octet and decuplet baryons due to the coupling to the
   baryon-meson continuum}},
Journal = {{EUROPEAN PHYSICAL JOURNAL A}},
Year = {{2017}},
Volume = {{53}},
Number = {{6}},
Month = {{JUN 2}},
Abstract = {{We present an unquenched quark model calculation of the mass shifts of
   ground-state octet and decuplet baryons due to the coupling to the
   meson-baryon continuum. All ground-state baryons and pseudoscalar mesons
   are included in our calculation as intermediate states. The q (q) over
   bar pair creation effects are taken explicitly into account through a
   microscopic, QCD-inspired, quark-antiquark pair creation mechanism.}},
