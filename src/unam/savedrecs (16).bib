@article{ ISI:000402709400003,
Author = {Diaz-Damacillo, Lamberto and Reyes, Fidel and Ingalls, Alberto and
   Mendez, Claudio and Plascencia, Gabriel},
Title = {{Mass Transfer Model for the De-oxidation of Molten Copper}},
Journal = {{JOM}},
Year = {{2017}},
Volume = {{69}},
Number = {{6}},
Pages = {{980-986}},
Month = {{JUN}},
Abstract = {{In this paper, we present a mass transfer model that predicts two
   different mechanisms that control copper de-oxidation: (1) the transport
   of the reducing gas from the gas bubbles towards the melt/bubble
   interface, and (2) the transport of dissolved oxygen from the melt
   towards the melt/bubble interface. The model accounts for gas fluid flow
   and other process parameters such as lance submergence and nozzle
   diameter. The model was validated with published data and predictions
   from our model are in good agreement with the values reported. The key
   parameters to determine are the mass transfer coefficients of the
   reducing gas and that of the dissolved oxygen in the melt.}},
