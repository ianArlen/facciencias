@article{ ISI:000403882000001,
Author = {Vazquez-Vergara, Pamela and Torres Rojas, Aimee M. and Guevara-Pantoja,
   Pablo E. and Corvera Poire, Eugenia and Caballero-Robledo, Gabriel A.},
Title = {{Microfluidic flow spectrometer}},
Journal = {{JOURNAL OF MICROMECHANICS AND MICROENGINEERING}},
Year = {{2017}},
Volume = {{27}},
Number = {{7}},
Pages = {{1-10}},
Month = {{JUL}},
Abstract = {{We present a microfluidic device which allows one to study the dynamics
   of oscillatory flows for a frequency range between 1 and 300 Hz. The
   fluid in the microdevice could be Newtonian, viscoelastic, or even a
   biofluid, since the device is made of PMMA, which makes it biocompatible
   and free of elastomeric elements. Coupling a piezoelectric to a
   micropiston allows one to impose periodic movement to the fluid, with
   zero mean flow and amplitudes of up to 20 mu m, within the microchannels
   in which the dynamics is studied. The use of a fast camera coupled to a
   microscope allows one to study the dynamics of 1 mu m tracer particles
   and interfaces at an image acquisition rate as fast as 5000 frames per
   second. The fabrication of the device is easy and cost-effective, since
   it is based on the use of a micromilling machine. The dynamics of a
   Newtonian fluid is studied as a proof of principle.}},
