@article{ ISI:000405541600147,
Author = {Argueta-Figueroa, Liliana and Martinez-Alvarez, O. and Santos-Cruz, J.
   and Garcia-Contreras, R. and Acosta-Torres, L. S. and de la
   Fuente-Hernandez, J. and Arenas-Arrocena, M. C.},
Title = {{Nanomaterials made of non-toxic metallic sulfides: A systematic review
   of their potential biomedical applications}},
