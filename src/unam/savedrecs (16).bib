@article{ ISI:000423271500005,
Author = {Cuetara-Guadarrama, Fabian and Ramirez-Gualito, Karla and Cuevas,
   Gabriel},
Title = {{Study of CH/pi Interactions in the Molecular Recognition between Acetyl
   Galactopyranoside and 6-substituted 2-Methoxypyridines and
   2(1H)-Pyridones}},
Journal = {{JOURNAL OF THE MEXICAN CHEMICAL SOCIETY}},
Year = {{2017}},
Volume = {{61}},
Number = {{3}},
Pages = {{205-216}},
Month = {{JUL-SEP}},
Abstract = {{A series of 6-substituted 2-methoxypyridine and 2(1H)-pyridones was
   designed and synthesized for its evaluation in the molecular recognition
   of acetyl 2,3,4,6-tetra-O-methyl-beta-D-galactopyranoside substrate.
   H-1-NMR titration (affinity constant K-a determination) and chemical
   shift perturbation experiments were performed to evaluate the capacity
   of these receptors to form CH/pi interactions with the substrate. The
   addition of 2-methoxypyridines to the substrate effected up-field shift
   for the H-3, H-4 and H-5 proton signals and down-field shift for the H-2
   proton signal of galactopyranoside substrate. The determined affinity
   constant K-a values for the association between 2(1H)-pyridones and
   galactopyranoside showed that molecular recognition was weak. These
   results have demonstrated the existence of weak CH/pi interactions and
   have reflected their weak intermolecular nature. Finally DFT
   calculations were performed to illustrate the geometry of the molecular
   recognition between 2(1H)-pyridones and galactopyranoside.}},
