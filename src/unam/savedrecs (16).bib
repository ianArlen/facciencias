@article{ ISI:000403534700043,
Author = {Contreras, Victor and Toivonen, Juha and Martinez, Horacio},
Title = {{Enhanced self-mixing interferometry based on volume Bragg gratings and
   laser diodes emitting at 405-nm wavelengths}},
Journal = {{OPTICS LETTERS}},
Year = {{2017}},
Volume = {{42}},
Number = {{11}},
Pages = {{2221-2223}},
Month = {{JUN 1}},
Abstract = {{Self-mixing interferometry (SMI) represents a robust, self-aligned
   technique for metrology applications. Recently, it has been shown that
   the detection of the frequency-modulated (FM) signal enhances the
   conventional SMI signal based on the amplitude modulation. Here, an
   alloptical, simple and effective alternative approach to detect the FM
   self-mixing signal is presented. We demonstrate the enhanced self-mixing
   approach using a laser diode emitting at 405 nm and a volume Bragg
   grating (VBG) to map frequency to intensity modulations for further
   optical detection. Our approach overcomes the spectral range limitation
   of the edge filtering approach based on molecular absorption filters,
   since the VBGs can be fabricated at any spectral range. (C) 2017 Optical
   Society of America}},
