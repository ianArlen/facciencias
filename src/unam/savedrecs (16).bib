@article{ ISI:000404033200001,
Author = {Ossio, Raul and Roldan-Marin, Rodrigo and Martinez-Said, Hector and
   Adams, David J. and Daniela Robles-Espinoza, Carla},
Title = {{Melanoma: a global perspective}},
Journal = {{NATURE REVIEWS CANCER}},
Year = {{2017}},
Volume = {{17}},
Number = {{7}},
Pages = {{393-394}},
Month = {{JUL}},
Abstract = {{Most of our current knowledge of melanoma is derived from the study of
   patients from populations of European descent, for whom public health,
   sun protection initiatives and screening measures have appreciably
   decreased disease mortality. Notably, some melanoma subtypes that most
   commonly develop in other populations are not associated with exposure
   to ultraviolet (UV) light, suggesting a different disease aetiology.
   Further study of these subtypes is necessary to understand their risk
   factors and genomic architecture, and to tailor therapies and public
   health campaigns to benefit patients of all ethnic groups.}},
