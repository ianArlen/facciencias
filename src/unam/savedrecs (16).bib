@article{ ISI:000402819700030,
Author = {Camps-Farina, A. and Zaragoza-Cardiel, J. and Beckman, J. E. and Font,
   J. and Velazquez, P. F. and Rodriguez-Gonzalez, A. and Rosado, M.},
Title = {{Physical properties of superbubbles in the Antennae galaxies}},
