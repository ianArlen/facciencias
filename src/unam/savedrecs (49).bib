@article{ ISI:000336249300029,
Author = {Avila-Reese, Vladimir and Zavala, Jesus and Lacerna, Ivan},
Title = {{The growth of galactic bulges through mergers in Lambda cold dark matter
   haloes revisited - II. Morphological mix evolution}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
