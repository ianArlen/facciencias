@article{ ISI:000333405800011,
Author = {Moreno, L. and Sanchez-Ake, C. and Bizarro, M.},
Title = {{Double-beam pulsed laser deposition for the growth of Al-incorporated
   ZnO thin films}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{2014}},
Volume = {{302}},
Pages = {{46-51}},
Month = {{MAY 30}},
Note = {{5th European-Materials-Research-Society Symposium on Laser Material
   Interactions for Micro- and Nano- Applications, Strasbourg, FRANCE, MAY
   27-31, 2013}},
Organization = {{European Mat Res Soc; Amplitude Syst; Quantel}},
Abstract = {{Pulsed laser deposition in a delayed-double beam configuration is used
   to incorporate in situ Al in ZnO thin films. In this configuration, two
   synchronized pulsed-laser beams are employed to ablate independently a
   ZnO and an Al target. We investigated the effects of relative time delay
   of plasma plumes on the composition of the films with the aim of
   evaluating the performance of this technique to produce doped materials.
   Relative delay between plumes was found to control the incorporation of
   Al in the film in the range from 14\% to 30\%. However, to produce low
   impurity concentration of Al-doped ZnO (with Al incorporation less than
   2\%) the fluence used to produce the plasmas has more influence over the
   film composition than the relative plume delay. The minimum
   incorporation of Al corresponded to a relative delay of 0 mu s, due to
   the interaction between plumes during their expansion. (C) 2014 Elsevier
   B.V. All rights reserved.}},
