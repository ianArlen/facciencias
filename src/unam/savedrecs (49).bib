@article{ ISI:000334523200002,
Author = {Carvalho, Bruno M. and Garduno, Edgar and Santos, Tiago S. and Oliveira,
   Lucas M. and Silva Neto, Jose F.},
Title = {{Fuzzy segmentation of video shots using hybrid color spaces and motion
   information}},
Journal = {{PATTERN ANALYSIS AND APPLICATIONS}},
