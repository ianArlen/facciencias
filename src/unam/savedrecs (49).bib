@article{ ISI:000337252400027,
Author = {Camacho-Medina, Pablo and Angel Olivares-Robles, Miguel and
   Vargas-Almeida, Alexander and Solorio-Ordaz, Francisco},
Title = {{Maximum Power of Thermally and Electrically Coupled Thermoelectric
   Generators}},
