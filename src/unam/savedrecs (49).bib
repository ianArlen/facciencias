@article{ ISI:000335924200039,
Author = {Gabbasov, R. F. and Rosado, M. and Klapp, J.},
Title = {{AN INTERACTION SCENARIO OF THE GALAXY PAIR NGC 3893/96 (KPG 302): A
   SINGLE PASSAGE?}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2014}},
Volume = {{787}},
Number = {{1}},
Month = {{MAY 20}},
Abstract = {{Using the data obtained previously from Fabry-Perot interferometry, we
   study the orbital characteristics of the interacting pair of galaxies
   KPG 302 with the aim to estimate a possible interaction history, the
   conditions necessary for the spiral arm formation, and initial satellite
   mass. We found by performing N-body/smoothed particle hydrodynamics
   simulations of the interaction that a single passage can produce a grand
   design spiral pattern in less than 1 Gyr. Although we reproduce most of
   the features with the single passage, the required satellite to host
   mass ratio should be similar to 1:5, which is not confirmed by the
   dynamical mass estimate made from the measured rotation curve. We
   conclude that a more realistic interaction scenario would require
