@article{ ISI:000335810700038,
Author = {Hernandez-Hernandez, Vicente and Zapata, Luis and Kurtz, Stan and Garay,
   Guido},
Title = {{SMA MILLIMETER OBSERVATIONS OF HOT MOLECULAR CORES}},
