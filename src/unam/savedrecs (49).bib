@article{ ISI:000334912000009,
Author = {Andrade-Dominguez, Andres and Salazar, Emmanuel and Carmen
   Vargas-Lagunas, Maria del and Kolter, Roberto and Encarnacion, Sergio},
Title = {{Eco-evolutionary feedbacks drive species interactions}},
Journal = {{ISME JOURNAL}},
Year = {{2014}},
