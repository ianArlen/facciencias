@article{ ISI:000331928900002,
Author = {Reyes, P. G. and Torres, C. and Martinez, H.},
Title = {{Electron temperature and ion density measurements in a glow discharge of
   an Ar-N-2 mixture}},
Journal = {{RADIATION EFFECTS AND DEFECTS IN SOLIDS}},
Year = {{2014}},
Volume = {{169}},
Number = {{4}},
Pages = {{285-292}},
Month = {{APR 3}},
Abstract = {{A DC glow discharge produced in N-2 gas can generate several species
   that are important in different applications, such as the modification
   of surface properties of materials. A low-pressure glow discharge
   apparatus was used for the the analysis of the Ar-N-2 mixture at a total
   pressure of 2.0Torr, a power of 20W and 40l/min flow rate of gases. The
   emission bands were measured in the wavelength range of 200-1100nm. The
   principal elements are N-2, N (+)(2) and Ar I. The electron temperature
   was found in the range of 1.72-2.08eV, and the ion density was in the
   order of 10(10)cm(-3).}},
