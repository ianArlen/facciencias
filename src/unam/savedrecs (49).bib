@article{ ISI:000338806000024,
Author = {Vargas, A. and Moreno, J. A. and Vande Wouwer, A.},
Title = {{A weighted variable gain super-twisting observer for the estimation of
   kinetic rates in biological systems}},
Journal = {{JOURNAL OF PROCESS CONTROL}},
Year = {{2014}},
Volume = {{24}},
Number = {{6, SI}},
Pages = {{957-965}},
Month = {{JUN}},
Abstract = {{The knowledge of kinetic reaction rates is important for monitoring and
   controlling biotechnological processes. However, the lack of on-line
   sensors for this purpose and the inherent problems with numerical
   differentiation make observers indispensable. In this work, we propose
   the use of a weighted variable gain super-twisting observer (WVGSTO),
   applicable to a class of second-order nonlinear systems that include a
   measurable weight on the unmeasured variable and the possibility of
   bounding the perturbations with measurable functions. This estimation
   method is illustrated with an academic example and then applied to a
   fed-batch bioprocess. (C) 2014 Elsevier Ltd. All rights reserved.}},
Publisher = {{ELSEVIER SCI LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, OXON, ENGLAND}},
Type = {{Article}},
