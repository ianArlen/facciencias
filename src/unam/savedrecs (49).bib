@article{ ISI:000335096100010,
Author = {Ramirez-Jimenez, Alejandro and Alvarez-Lorenzo, Carmen and Concheiro,
   Angel and Bucio, Emilio},
Title = {{Temperature-responsiveness and biocompatibility of DEGMA/OEGMA
   radiation-grafted onto PP and LDPE films}},
Journal = {{RADIATION PHYSICS AND CHEMISTRY}},
Year = {{2014}},
Volume = {{99}},
Pages = {{53-61}},
Month = {{JUN}},
Abstract = {{Polypropylene (PP) and low density polyethylene (LOPE) were modified by
   gamma-ray grafting of di(ethylene glycol) methyl ether methacrylate
   (DEGMA) and oligo (ethylene glycol) methyl ether methacrylate (0EGMA300
   or OEGMA475 with Mn 300 and 475 respectively) with different monomer
   concentrations and mol ratios. The grafting percentage was evaluated as
   a function of the solvent, irradiation dose, reaction time, temperature,
   and monomers concentration. The grafted materials were more hydrophilic
   than the pristine polymers, as observed by contact angle and swelling in
   water. Temperature-responsive behavior was evaluated using DSC showing
   transitions between 34 and 48 C. In vitro hemocompatibility, protein
   adsorption, cytotoxicity and bacteria adhesion tests were also carried
   out. Overall, the DEGMA/ OEGMA grafting provides hemo and cytocompatible
   materials that exhibit temperature-responsive hydrophilic features and
   decreased protein adsorption. (C) 2014 Elsevier Ltd. All rights
   reserved.}},
