@article{ ISI:000337089400009,
Author = {de la Pena, Jose A.},
Title = {{Algebras Whose Coxeter Polynomials are Products of Cyclotomic
   Polynomials}},
Journal = {{ALGEBRAS AND REPRESENTATION THEORY}},
