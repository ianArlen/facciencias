@article{ ISI:000337093800002,
Author = {Phillips, George E. and Nyborg, Torrey and Vega, Francisco J.},
Title = {{Icriocarcinidae: a family of portunoid crabs from the Upper Cretaceous
   of North America}},
Journal = {{PALAEONTOLOGISCHE ZEITSCHRIFT}},
