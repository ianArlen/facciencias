@article{ ISI:000335220600041,
Author = {Loria, Antonio and Espinosa-Perez, Gerardo and Chumacero, Erik},
Title = {{Exponential Stabilization of Switched-Reluctance Motors Via
   Speed-Sensorless Feedback}},
Journal = {{IEEE TRANSACTIONS ON CONTROL SYSTEMS TECHNOLOGY}},
Year = {{2014}},
Volume = {{22}},
Number = {{3}},
Pages = {{1224-1232}},
Month = {{MAY}},
Abstract = {{We solve the control problem of switched-reluctance motors without
   velocity measurements. Our controller is composed of a loop in the
   mechanical dynamics which consists of a (PID)-D-2 controller and a
   tracking controller closing an inner loop with the stator currents
   dynamics. The (PID)-D-2 controller consists of a linear proportional
   derivative controller in which the measurement of velocities is replaced
   by approximate derivatives of angular position. Then a double integrator
   is added, composed of an integral of the angular position errors and a
   second integral correction term in function of the approximate
   derivative. We show global exponential stability and illustrate the
   performance of our controller in numerical simulations.}},
