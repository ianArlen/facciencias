@article{ ISI:000334293200035,
Author = {Jagdale, Pravin and Castellino, Micaela and Marrec, Francoise and Rodil,
   Sandra E. and Tagliaferro, Alberto},
Title = {{Nano Sized bismuth oxy chloride by metal organic chemical vapour
   deposition}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{2014}},
Volume = {{303}},
Pages = {{250-254}},
Month = {{JUN 1}},
Abstract = {{Metal organic chemical vapour deposition (MOCVD) method was used to
   prepare thin films of bismuth based nano particles starting from bismuth
   salts. Nano sized bismuth oxy chloride (BiOCl) crystals were synthesized
   from solution containing bismuth chloride (BiCl3) in acetone
   (CH3-CO-CH3). Self-assembly of nano sized BiOCl crystals were observed
   on the surface of silicon, fused silica, copper, carbon nanotubes
   aluminium substrates. Various synthesis parameters and their significant
   impact onto the formation and of self-assembled nano-crystalline BiOCl
   were investigated. BiOCl nano particles were characterized by dispersive
   X-ray spectroscopy and Micro-Raman spectroscopy. These analyses confirm
   that bismuth nanometer-sized crystal structures showing a single
