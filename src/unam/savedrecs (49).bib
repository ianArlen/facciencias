@article{ ISI:000336743900004,
Author = {Montes de Oca Balderas, Marco A. and Sienra Loera, Guillermo J. F. and
   King Davalos, Jefferson E.},
Title = {{Baker Domains of Period Two for the Family f(lambda,mu)(Z) = lambda e(z)
   + mu/z}},
Journal = {{INTERNATIONAL JOURNAL OF BIFURCATION AND CHAOS}},
Year = {{2014}},
Volume = {{24}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{Our main theorem establishes that the Fatou set of the functions
   f(lambda,mu) (z) = lambda e(z) vertical bar mu/z contains a two-cycle of
   Baker domains \{F-infinity, F-0\} if Re(lambda) < 0 and |Im(lambda)| <
   1/2|Re(lambda)|. We show that under f(lambda,mu)(2)every point in
   F-infinity tends to infinity and in F-0 tends to zero. Moreover, if
   |Im(lambda)| < 1/2|Re(lambda)| - 4, the set F-infinity contains
   infinitely many critical points of f(lambda,mu) and F-0 contains
   infinitely many critical values; also, infinitely many critical values
   of f(lambda,mu)(2) are contained in both F-infinity and F-0. Finally,
   the images of the Baker domains are displayed for some parameters.}},
Publisher = {{WORLD SCIENTIFIC PUBL CO PTE LTD}},
Address = {{5 TOH TUCK LINK, SINGAPORE 596224, SINGAPORE}},
Type = {{Article}},
Language = {{English}},
