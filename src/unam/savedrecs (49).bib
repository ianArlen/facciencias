@article{ ISI:000337248100002,
Author = {Rodriguez Plaza, Jonathan G. and Morales-Nava, Rosmarbel and Diener,
   Christian and Schreiber, Gabriele and Gonzalez, Zyanya D. and Lara
   Ortiz, Maria Teresa and Ortega Blake, Ivan and Pantoja, Omar and
   Volkmer, Rudolf and Klipp, Edda and Herrmann, Andreas and Del Rio,
   Gabriel},
Title = {{Cell Penetrating Peptides and Cationic Antibacterial Peptides TWO SIDES
   OF THE SAME COIN}},
Journal = {{JOURNAL OF BIOLOGICAL CHEMISTRY}},
Year = {{2014}},
Volume = {{289}},
Number = {{21}},
Pages = {{14448-14457}},
Month = {{MAY 23}},
Abstract = {{Cell penetrating peptides (CPP) and cationic antibacterial peptides
   (CAP) have similar physicochemical properties and yet it is not
   understood how such similar peptides display different activities. To
   address this question, we used Iztli peptide 1 (IP-1) because it has
   both CPP and CAP activities. Combining experimental and computational
   modeling of the internalization of IP-1, we show it is not internalized
   by receptor-mediated endocytosis, yet it permeates into many different
   cell types, including fungi and human cells. We also show that IP-1
   makes pores in the presence of high electrical potential at the
   membrane, such as those found in bacteria and mitochondria. These
   results provide the basis to understand the functional redundancy of
   CPPs and CAPs.}},
