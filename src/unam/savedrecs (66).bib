@article{ ISI:000302886000013,
Author = {Velasco-Rodriguez, V. and Cornejo-Mazon, M. and Flores-Flores, J. O. and
   Gutierrez-Lopez, G. F. and Hernandez-Sanchez, H.},
Title = {{PREPARATION AND PROPERTIES OF ALPHA-LIPOIC ACID-LOADED CHITOSAN
   NANOPARTICLES}},
Journal = {{REVISTA MEXICANA DE INGENIERIA QUIMICA}},
Year = {{2012}},
Volume = {{11}},
Number = {{1}},
Pages = {{155-161}},
Month = {{APR}},
Abstract = {{The aim of this study was to prepare alpha-lipoic acid (ALA) loaded
   chitosan nanoparticles by the ionotropic gelation between chitosan and
   sodium pyrophosphate or tripolyphosphate and the use of the surfactant
   poloxamer 188 as a stabilizer. Their particle size and distribution
   along with their encapsulation efficiency and zeta potential was
   determined. The smallest particle size (180 nm) were obtained when
   sodium pyrophosphate was used as the polyanion. The addition of the
   poloxamer incresased the particle size independently of the polyanionic
   compound used. All the polydispersity indexes were > 0.2, indicating
   that all the chitosan nanoparticle systems were polydisperse. The
   highest encapsulation efficiency (62.4\%) was obtained in the case of
   the tripolyphosphate and poloxamer system. The isoelectric point (pI)
