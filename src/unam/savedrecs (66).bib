@article{ ISI:000304343300003,
Author = {Luna-Ortiz, Kuauhyama and Gueemes-Meza, Agustin and
   Villavicencio-Valencia, Veronica and Mosqueda-Taylor, Adalberto},
Title = {{Upper lip malignant neoplasms. A study of 59 cases}},
