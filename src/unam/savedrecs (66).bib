@article{ ISI:000303446500015,
Author = {Noriega-Crespo, A. and Raga, A. C.},
Title = {{SPITZER OBSERVATIONS OF THE HH 1/2 SYSTEM: THE DISCOVERY OF THE
   COUNTERJET}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2012}},
Volume = {{750}},
Number = {{2}},
Month = {{MAY 10}},
Abstract = {{We present unpublished Spitzer IRAC observations of the HH 1/2 young
   stellar outflow processed with a high angular resolution deconvolution
   algorithm that produces subarcsecond (similar to 0 `'.6-0 `'.8) images.
   In the resulting mid-infrared images, the optically invisible counterjet
   is detected for the first time. The counterjet is approximately half as
   bright as the jet at 4.5 mu m (the IRAC band that best traces young
   stellar outflows) and has a length of similar to 10 `'. The NW optical
   jet itself can be followed back in the mid-IR to the position of the
   exciting VLA 1 source. An analysis of the IRAC colors indicates that the
   jet/counterjet emission is dominated by collisionally excited H-2 pure
   rotational lines arising from a medium with a neutral hydrogen gas
   density of similar to 1000-2000 cm(-3) and a temperature of similar to
   1500 K. The observed jet/counterjet brightness asymmetry is consistent
   with an intrinsically symmetric outflow with extinction from a dense,
   circumstellar structure of similar to 6 `' size (along the outflow
   axis), and with a mean visual extinction, A(V) similar to 11 mag.}},
