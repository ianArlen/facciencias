@article{ ISI:000306256400003,
Author = {Oviedo, M. J. and Contreras, O. and Vazquez-Duhalt, R. and
   Carbajal-Arizaga, G. G. and Hirata, G. A. and McKittrick, J.},
Title = {{Photoluminescence of Europium-Activated Hydroxyapatite Nanoparticles in
   Body Fluids}},
Journal = {{SCIENCE OF ADVANCED MATERIALS}},
