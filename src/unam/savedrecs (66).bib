@article{ ISI:000304175700006,
Author = {Jiang, Yu and Martinez-Mares, M. and Castano, E. and Robledo, A.},
Title = {{Mobius transformations and electronic transport properties of large
   disorderless networks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2012}},
Volume = {{85}},
Number = {{5, 2}},
Month = {{MAY 17}},
Abstract = {{We show that the key transport states, insulating and conducting, of
   large regular networks of scatterers can be described generically by
   negative and zero Lyapunov exponents, respectively, of Mobius maps that
   relate the scattering matrix of systems with successive sizes. The
   conductive phase is represented by weakly chaotic attractors that have
   been linked with anomalous transport and ergodicity breaking. Our
   conclusions, verified for serial as well as parallel stub and ring
   structures, reveal that mesoscopic behavior results from a drastic
   reduction of degrees of freedom.}},
