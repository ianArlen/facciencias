Title = {{Regioselective cleavage of 22-oxo-23-spiroketals. Novel cholestanic
   frameworks with pyranone and cyclopentenone E rings on the side chain}},
Journal = {{STEROIDS}},
Year = {{2012}},
Volume = {{77}},
Number = {{5}},
Pages = {{534-541}},
Month = {{APR}},
Abstract = {{The regioselective opening of the F ring of 22-oxo-23-spiroketals using
