@article{ ISI:000303799400003,
Author = {Deciga-Campos, Myrna and Arriaga-Alba, Myriam and Ventura-Martinez, Rosa
   and Aguilar-Guadarrama, Berenice and Yolanda Rios, Maria},
Title = {{Pharmacological and Toxicological Profile of Extract from Heliopsis
   longipes and Affinin}},
Journal = {{DRUG DEVELOPMENT RESEARCH}},
