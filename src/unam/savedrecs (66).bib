@article{ ISI:000303544900002,
Author = {Haro-Garcia, Luis and Villa-Ibarra, Martin and de Jesus Chain-Castro,
   Teresita and Lastra-Rodriguez, Angel and Arturo Juarez-Perez, Cuauhtemoc
   and Aguilar-Madrid, Guadalupe and Crystal Sanchez-Escalante, Vanessa and
   Rosa Brito-Zurita, Olga},
Title = {{Are Mexican Agricultural Farmlands PCDD/F Soil Reservoirs?}},
Journal = {{BULLETIN OF ENVIRONMENTAL CONTAMINATION AND TOXICOLOGY}},
Year = {{2012}},
Volume = {{88}},
Number = {{6}},
Pages = {{813-815}},
Month = {{JUN}},
Abstract = {{Our aim was to identify polychlorinated dibenzo-p-dioxins (PCDDs) and
   polychlorinated dibenzo-p-furans (PCDFs) in agricultural farmland soils
   in the Northwest of Mexico. We obtained a parts per thousand 50 g of
   soil in five Yaqui Valley (VY) agricultural fields in the north-western
   Mexican State of Sonora and in five Culiacan Valley (VC) agricultural
   fields in the north-western Mexican State of Sinaloa. Fields with
   minimal tillage, with ferti-irrigation, and those with intensive aerial
   and manual tillage were included. All soil samples were subjected to the
   chemical activated luciferase gene expression (CALUX(A (R))) test to
   determine PCDD/F. On average, samples contained 4.2 +/- A 1.2 PCDD/F ppt
   TEQ; VY soil samples contained 4.72 +/- A 1.23 PCDD/F ppt TEQ, while VC
   soil samples showed 3.6 +/- A 1.1 PCDD/F ppt TEQ (p = 0.47). On
   considering tillage-type, in agricultural fields catalogued as intensive
   tillage, PCDD/F concentrations were 4.40 +/- A 0.43 in agricultural
   fields catalogued as intensive tillage, while in farmlands of another
   tillage-type these concentrations were slightly higher (5.53 +/- A 0.8).}},
