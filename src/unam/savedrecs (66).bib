@article{ ISI:000303541900041,
Author = {Chigo Anota, Ernesto and Bautista Hernandez, Alejandro and Castro,
   Miguel and Hernandez Cocoletzi, Gregorio},
Title = {{Investigating the electronic properties of silicon nanosheets by
   first-principles calculations}},
Journal = {{JOURNAL OF MOLECULAR MODELING}},
