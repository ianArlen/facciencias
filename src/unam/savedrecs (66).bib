@article{ ISI:000302046800021,
Author = {Bravo-Castillero, Julian and Guinovart-Diaz, Raul and Rodriguez-Ramos,
   Reinaldo and Sabina, Federico J. and Brenner, Renald},
Title = {{Unified analytical formulae for the effective properties of periodic
   fibrous composites}},
Journal = {{MATERIALS LETTERS}},
Year = {{2012}},
Volume = {{73}},
Pages = {{68-71}},
Month = {{APR 15}},
Abstract = {{Two-phase fibre-reinforced elastic composites with transversely
   isotropic constituents and two types of fibres periodic distribution
   (square and hexagonal arrays) with perfect contact conditions at the
   interface are considered. Unified simple analytical formulae for their
   effective properties are given. Such formulae involve two orders of
   approximation of semi-analytical expressions obtained via asymptotic
   homogenization. The limit case of empty fibres is also presented. The
   influence of each approximation is illustrated. Theoretical and
   experimental comparisons are included. The objective is to provide
   information at hand which could be useful for applications to bone
   mechanics and ultrasonic transducers, and for validation of numerical
   codes. (C) 2012 Elsevier B.V. All rights reserved.}},
