@article{ ISI:000302020300014,
Author = {Luca, Florian and Young, Paul Thomas},
Title = {{ON THE LOG-CONCAVITY OF THE DEGENERATE BERNOULLI NUMBERS}},
Journal = {{INTERNATIONAL JOURNAL OF NUMBER THEORY}},
Year = {{2012}},
Volume = {{8}},
Number = {{3}},
Pages = {{789-800}},
Month = {{MAY}},
Abstract = {{The degenerate Bernoulli numbers beta(n)(lambda) are polynomials with
   rational coefficients of degree n in the variable lambda, which arise in
   several combinatorial settings. An appropriate change of variable
   transforms beta(n)(lambda) into a polynomial whose coefficients are all
   positive. Here, we prove that this transformed polynomial is
   log-concave, and therefore unimodal. As a consequence, we deduce bounds
   on the absolute values of the roots of beta(n)(lambda).}},
