@article{ ISI:000304221300001,
Author = {Poveda, Juan C. and Molina, Daniel R.},
Title = {{Average molecular parameters of heavy crude oils and their fractions
   using NMR spectroscopy}},
Journal = {{JOURNAL OF PETROLEUM SCIENCE AND ENGINEERING}},
Year = {{2012}},
Volume = {{84-85}},
Pages = {{1-7}},
Month = {{APR}},
Abstract = {{In this work, a new procedure for obtaining a full set of average
   molecular parameters (AMPS) of heavy crude oils and their fractions is
   proposed. Initially, a definition of the nomenclature for the
   identification of structural groups is presented, followed by the
   development of the scheme to obtain the AMPs. Corrections to parameters
   f(a), R-N, RT, (n(Alk)) over bar which are common in the literature, and
   new parameters for samples with a high grade of aromatic condensation
   and alkyl ramification, eta, phi, and xi, are revisited. The aromatic
   factor, f(a), has been revised in detail to include the heteroaromatic
   species sulfur, nitrogen, and oxygen, which were not considered in any
   previous work. This new procedure allows the calculation of the AMPs
   from convenient analytical data, including the average molecular weight,
   elemental analyses results, and H-1- and C-13-NMR spectroscopy data. (C)
   2012 Elsevier B.V. All rights reserved.}},
