@article{ ISI:000303370000012,
Author = {Sanchez-Basurto, Beatriz E. and Ramirez-Gilly, Mariana and Tecante,
   Alberto and Severiano-Perez, Patricia and Wacher, Carmen and
   Valdivia-Lopez, Maria A.},
Title = {{Effect of High Hydrostatic Pressure Treatment on the Preservation of
   Beef Meat}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
Year = {{2012}},
Volume = {{51}},
Number = {{17}},
Pages = {{5932-5938}},
Month = {{MAY 2}},
Note = {{1st International Congress and 32nd National Meeting of the Mexican
   Association for Research and Teaching in Chemical Engineering (AMIDIQ),
   Riviera Maya, MEXICO, MAY 03-06, 2011}},
Abstract = {{The effect of high hydrostatic pressure was investigated to determine
   the best treatment conditions to preserve beef meat. The meat was
   pressurized from 172 to 620 MPa for 1-5 min at 25 +/- 2 degrees C.
   Intensive color degradation occurred above 345 MPa. No significant
   reduction in aerobic mesophilic count was observed below 310 MPa.
   Therefore, 310, 324, and 345 MPa were applied to the meat for 1 min. The
   pH, water-holding capacity, total volatile bases, CFU/g, color, texture,
   and tenderness of pressurized and unpressurized meats were assessed for
   42 days, while meat was stored at 4 degrees C. Data showed that pressure
   treatment produced good results. The color, texture, and tenderness of
   treated and untreated meats were not statistically different (p > 0.05).
   High hydrostatic pressure preserved meat over a longer time period and
   is a promising technology for extending the shelf life of raw meat.}},
