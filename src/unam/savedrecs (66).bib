@article{ ISI:000303119900010,
Author = {Olin-Sandoval, Viridiana and Gonzalez-Chavez, Zabdi and Berzunza-Cruz,
   Miriam and Martinez, Ignacio and Jasso-Chavez, Ricardo and Becker,
   Ingeborg and Espinoza, Bertha and Moreno-Sanchez, Rafael and Saavedra,
   Emma},
Title = {{Drug target validation of the trypanothione pathway enzymes through
   metabolic modelling}},
Journal = {{FEBS JOURNAL}},
