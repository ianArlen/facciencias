@article{ ISI:000304574400014,
Author = {Gelover-Santiago, Adriana and Fowler, Michael A. and Yip, Jamie and
   Duhamel, Jean and Burillo, Guillermina and Rivera, Ernesto},
Title = {{Unexpected Absorbance Enhancement upon Clustering Dyes in a Polymer
   Matrix}},
