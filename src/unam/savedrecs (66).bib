@article{ ISI:000303700900010,
Author = {Clapp, Carmen and Martinez de la Escalera, Lucia and Martinez de la
   Escalera, Gonzalo},
Title = {{Prolactin and blood vessels: A comparative endocrinology perspective}},
Journal = {{GENERAL AND COMPARATIVE ENDOCRINOLOGY}},
