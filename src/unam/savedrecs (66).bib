@article{ ISI:000302990900005,
Author = {Honey-Escandon, Magali and Laguarda-Figueras, Alfredo and Solis-Marin,
   Francisco A.},
Title = {{Molecular phylogeny of the subgenus Holothuria (Selenkothuria)
   Deichmann, 1958 (Holothuroidea: Aspidochirotida)}},
