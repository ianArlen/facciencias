@article{ ISI:000303043500009,
Author = {Ramirez-Paredes, J. G. and Garduno-Lugo, M. and Munoz-Cordova, G.},
Title = {{Productive performance of a new synthetic red tilapia population
   `Pargo-UNAM' compared with that of wild-type Nile tilapia (Oreochromis
   niloticus L.)}},
