@article{ ISI:000303028600009,
Author = {Angel Betancourt, Miguel and de Aluja, Aline S. and Sciutto, Edda and
   Hernandez, Marisela and Bobes, Raul J. and Rosas, Gabriela and
   Hernandez, Beatriz and Fragoso, Gladis and Hallal-Calleros, Claudia and
   Aguilar, Liliana and Flores-Perez, Ivan},
Title = {{Effective protection induced by three different versions of the porcine
   S3Pvac anticysticercosis vaccine against rabbit experimental Taenia
   pisiformis cysticercosis}},
Journal = {{VACCINE}},
Year = {{2012}},
Volume = {{30}},
Number = {{17}},
Pages = {{2760-2767}},
Month = {{APR 5}},
Abstract = {{In an effort to develop an effective and affordable oral vaccine against
   porcine Taenia solium cysticercosis, the S3Pvac anti-cysticercosis
   vaccine was expressed in papaya calli. Taenia pisiformis experimental
   rabbit cysticercosis was used as a model to compare the efficacy of the
   oral vaccine vs. the injectable S3Pvac-synthetic and S3Pvac-phage
   versions.
   Oral S3Pvac-papaya significantly reduced the expected number of hepatic
   lesions and peritoneal cysticerci to a similar extent than the
   injectable vaccines. This study reports for the first time an effective
   oral vaccine against T. pisiformis cysticercosis, possibly useful
   against porcine T. solium cysticercosis. (c) 2012 Elsevier Ltd. All
   rights reserved.}},
