@article{ ISI:000304187100035,
Author = {Carrillo, Silvia and Hugo Rios, Victor and Calvo, Concepcion and Elena
   Carranco, Maria and Casas, Margarita and Perez-Gil, Fernando},
Title = {{n-3 Fatty acid content in eggs laid by hens fed with marine algae and
   sardine oil and stored at different times and temperatures}},
