@article{ ISI:000303282400043,
Author = {Qureshi, Naser and Kolokoltsev, Oleg V. and Ordonez-Romero, Cesar L. and
   Lopez-Maldonado, Guillermo},
Title = {{An active resonator based on magnetic films for near field microwave
   microscopy}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2012}},
Volume = {{111}},
Number = {{7}},
Month = {{APR 1}},
Note = {{56th Annual Conference on Magnetism and Magnetic Materials, Scottsdale,
   AZ, OCT 30-30, 2011}},
Abstract = {{An active resonator perturbation method is introduced as a sensitive and
   versatile way to probe material properties in near field microwave
   microscopy. An active ring microwave oscillator based on magnetostatic
   excitations in a yittrium iron garnet thin film has been developed with
   a coaxial near field probe connected directly to the resonator ring. The
   probe tunes the resonator's emission frequency, and the high Q-factor of
   the magnetostatic oscillations allows for a very sensitive spatially
   resolved probe of surface impedance and material properties with a much
   larger dynamic range than conventional resonant probes for microwave
   microscopy. (C) 2012 American Institute of Physics.
   {[}doi:10.1063/1.3672081]}},
