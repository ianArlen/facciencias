@article{ ISI:000302766000013,
Author = {Munoz, I. C. and Cruz-Zaragoza, E. and Favalli, A. and Furetta, C.},
Title = {{Thermoluminescence property of LiMgF3 erbium activated phosphor}},
Journal = {{APPLIED RADIATION AND ISOTOPES}},
Year = {{2012}},
Volume = {{70}},
Number = {{5}},
Pages = {{893-896}},
Month = {{MAY}},
Abstract = {{The perovskite-like LiMgF3:ErF3 pellets were obtained from the melt
   formed by LiF and MgF2 mixed salts in the stoichiometric ratio. The
   perovskite material was doped with 1, 2 and 4 mol\% of ErF3 impurity.
   The pellets samples were Co-60 gamma irradiated and their
   thermoluminescence (TL) properties were analyzed, i.e., dose-response,
   fading at RT and under UV irradiation, TL signal reproducibility, and
   kinetic parameters. The intensity of the TL response against irradiation
   dose was increased remarkably by the high concentration of impurity, and
   a linear dose-response was observed in the range of 1-10 Gy. The fading
