@article{ ISI:000303605000009,
Author = {Cornejo, Cecilia and Alvarez-Icaza, Luis},
Title = {{Passivity based control of under-actuated mechanical systems with
   nonlinear dynamic friction}},
Journal = {{JOURNAL OF VIBRATION AND CONTROL}},
Year = {{2012}},
Volume = {{18}},
Number = {{7}},
Pages = {{1025-1042}},
Month = {{JUN}},
Abstract = {{Passivity-based control of under-actuated mechanical systems with
   nonlinear friction effects in the generalized coordinates of motion is
   analyzed in this paper. Nonlinear friction is modeled with a modified
   LuGre dynamic friction model. The internal states of the dynamic
   friction model are incorporated as generalized coordinates in a
   port-controlled Hamiltonian formulation for the complete mechanical
   system in such a way that all passivity properties of this formulation
   are preserved for the extended generalized coordinates system.
   Interconnection and damping assignment passivity-based control laws are
   developed for the models of two case studies: a building with a
   magneto-rheological damper and a double pendulum. Simulation results are
   also presented.}},
