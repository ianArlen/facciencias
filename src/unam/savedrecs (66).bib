@article{ ISI:000303879700105,
Author = {Elena Martinez-Perez, M. and Espinosa-Romero, Arturo},
Title = {{Three-dimensional reconstruction of blood vessels extracted from retinal
   fundus images}},
Journal = {{OPTICS EXPRESS}},
Year = {{2012}},
Volume = {{20}},
Number = {{10}},
Pages = {{11451-11465}},
Month = {{MAY 7}},
Abstract = {{We present a 3D reconstruction of retinal blood vessel trees using two
   views of fundus images. The problem is addressed by using well known
   computer vision techniques which consider: 1) The recovery of
   camera-eyeball model parameters by an auto-calibration method. The
   camera parameters are found via the solution of simplified Kruppa
   equations, based on correspondences found by a LMedS optimisation
   correlation between pairs of eight different views. 2) The extraction of
   blood vessels and skeletons from two fundus images. 3) The matching of
   corresponding points of the two skeleton trees. The trees are previously
   labelled during the analysis of 2D binary images. Finally, 4) the lineal
   triangulation of matched correspondence points and the surface modelling
   via generalised cylinders using diameter measurements extracted from the
   2D binary images. The method is nearly automatic and it is tested with 2
   sets of 10 fundus retinal images, each one taken from different
   subjects. Results of 3D vein and artery trees reconstructions are shown.
   (C) 2012 Optical Society of America}},
