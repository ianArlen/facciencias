@article{ ISI:000302204000013,
Author = {Alvarado, Blanca G. and Sandin, Bonifacio and Valdez-Medina, Jose L. and
   Gonzalez-Arratia, Norma and Rivera, Sofia},
Title = {{Confirmatory factor analysis of the SA-45 Questionnaire in a Mexican
   sample}},
