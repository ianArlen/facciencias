@article{ ISI:000304501900001,
Author = {Alvarez-Padilla, Fernando and Ubick, Darrell and Griswold, Charles E.},
Title = {{Noideattella and Tolegnaro, Two New Genera of Goblin Spiders from
   Madagascar, with Comments on the Gamasomorphoid and Silhouettelloid
   Oonopids (Araneae, Oonopidae)}},
Journal = {{AMERICAN MUSEUM NOVITATES}},
