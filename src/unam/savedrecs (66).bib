@article{ ISI:000300503100005,
Author = {Alvarez-Falcon, Leny and Vicente, Luis},
Title = {{Dynamic monte carlo simulations of NO decomposition on Pt(100):
   Temperature-programmed desorption spectra}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2012}},
Volume = {{112}},
Number = {{7, SI}},
Pages = {{1803-1809}},
