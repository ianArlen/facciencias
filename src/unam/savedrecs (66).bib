@article{ ISI:000310395300002,
Author = {Beukers, Frits and Luca, Florian and Oort, Frans},
Title = {{Power Values of Divisor Sums}},
Journal = {{AMERICAN MATHEMATICAL MONTHLY}},
Year = {{2012}},
Volume = {{119}},
Number = {{5}},
Pages = {{373-380}},
Month = {{MAY}},
Abstract = {{We consider positive integers whose sum of divisors is a perfect power.
   This problem had already caught the interest of mathematicians from the
   17th century like Fermat, Wallis, and Frenicle. In this article we study
   this problem and some variations. We also give an example of a cube,
