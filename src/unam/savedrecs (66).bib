@article{ ISI:000302456000005,
Author = {Carabali, G. and Chavira, E. and Castro, I. and Bucio, E. and Huerta, L.
   and Jimenez-Mier, J.},
Title = {{Novel sol-gel methodology to produce LaCoO3 by acrylamide polymerization
   assisted by gamma-irradiation}},
Journal = {{RADIATION PHYSICS AND CHEMISTRY}},
Year = {{2012}},
Volume = {{81}},
Number = {{5}},
Pages = {{512-518}},
Month = {{MAY}},
Abstract = {{In this paper we report the synthesis of LaCoO3 (LCO) nano-particles
   with two methodologies: the conventional sol-gel reaction of acrylamide
   (AA) polymerization using a cross-linking agent (methylenebisacrylamide
   or MBA) with the activation of the polymerization reaction by
   thermo-chemical initiator (azobisisobutyrnitrile or AIBN). The second
   was a novel sot-gel methodology in which the polymerization of AA
   monomers was done without MBA and the initiation was achieved by gamma
   radiation. With thermochemical initiator a xerogel with a foam and
   porous structure was obtained, while the gamma-irradiation of the
   mixture leads to the formation of a compact resin with entrapped
   cations. X-ray diffraction (XRD) shows that formation of the product
   begins around 500 degrees C and according to analysis of microscopy
   images of powders calcined in 700 degrees C the average sizes of
   particles are 20 nm and 42 nm for samples obtained using
   gamma-irradiation and AIBN as initiators, respectively. TEM images also
   show differences in particle morphology. Those synthesized using AIBN as
   initiator are dispersed, while those with gamma-irradiation are in
   aggregates. (C) 2012 Elsevier Ltd. All rights reserved.}},
