@article{ ISI:000317346600001,
Author = {Ackermann, Nils and Clapp, Monica and Pacella, Filomena},
Title = {{Alternating Sign Multibump Solutions of Nonlinear Elliptic Equations in
   Expanding Tubular Domains}},
Journal = {{COMMUNICATIONS IN PARTIAL DIFFERENTIAL EQUATIONS}},
Year = {{2013}},
Volume = {{38}},
Number = {{5}},
Pages = {{751-779}},
Month = {{MAY 4}},
Abstract = {{Let denote a smooth simple curve in N , N2, possibly with boundary. Let
   R be the open normal tubular neighborhood of radius 1 of the expanded
   curve R: =\{Rx|x\textbackslash{}\}. Consider the superlinear problem
   u+u=f(u) on the domains R , as R, with homogeneous Dirichlet boundary
   condition. We prove the existence of multibump solutions with bumps
   lined up along R with alternating signs. The function f is superlinear
   at 0 and at , but it is not assumed to be odd. If the boundary of the
   curve is nonempty our results give examples of contractible domains in
   which the problem has multiple sign changing solutions.}},
