@article{ ISI:000319292800126,
Author = {Acevedo, Ulises and Gaudisson, Thomas and Ortega-Zempoalteca, Raul and
   Nowak, Sophie and Ammar, Souad and Valenzuela, Raul},
Title = {{Magnetic properties of ferrite-titanate nanostructured composites
   synthesized by the polyol method and consolidated by spark plasma
   sintering}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
