@article{ ISI:000317437500001,
Author = {Perez-Leon, J. E. and Hiriart, D. and Mendoza-Torres, J. E.},
Title = {{MILLIMETER AND SUBMILLIMETER COUNTERPARTS OF THE 2009 SEPTEMBER 26 SOLAR
   PROMINENCE}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2013}},
Volume = {{49}},
Number = {{1}},
Pages = {{3-10}},
Month = {{APR}},
Abstract = {{We present observations of the millimeter and submillimeter wavelength
   radio emission from an extended solar prominence. The data were obtained
   at the Solar Submillimeter Telescope (SST) during regular observations
   on 2009 September 26. The prominence signals were detected at 1.41 mm
   (212 GHz) and 0.74 mm (405 GHz) in an east-west raster practiced on the
   solar disk. Detection was confirmed by the Hell image from the Extreme
   Ultraviolet Imaging Telescope of the Solar and Heliospheric Observatory
   (EIT/S0110). The peak brightness temperature at the 1.41 mm and 0.74 ram
   are separated by similar to 4 arcmin with the 0.74 mm one closer to the
   solar limb. Assuming a free-free emission mechanism, both peak
   intensities correspond to an excess of brightness temperature of T-b
   similar to 2 x 10(3) K and they are consistent with an electron
   temperature and density of T-e similar to 5.6 x 10(3) K and n(e) similar
   to 10(10) cm(-3), respectively.}},
