@article{ ISI:000320684600025,
Author = {Betancourt, I. and Morales-Hernandez, A.},
Title = {{Magnetic properties of mixed valence La2/3Sr1/3Mn1-xTxO3 (T = Fe,Cr)
   manganites obtained by Pechini method}},
Journal = {{JOURNAL OF CHEMICAL SCIENCES}},
Year = {{2013}},
Volume = {{125}},
Number = {{3}},
Pages = {{667-672}},
Month = {{MAY}},
Abstract = {{Polycrystalline manganites of composition La2/3Sr1/3Mn1-xTxO3 (T = Cr3+
   or Fe3+, x = 0.0-0.10) were obtained by the Pechini method. Their
   magnetic properties exhibited a marked dependence on Fe/Cr content, with
   significant reduction of the magnetic moment per formula unit and their
   Curie temperatures. In addition, the magnetocaloric effect, determined
   by isothermal magnetization measurements, displayed a decreasing
   tendency with increasing Fe/Cr concentration. Results were interpreted
   on the basis of a deleterious effect on the double exchange interaction
   provoked by the presence of Fe3+ and Cr3+ ions within the crystal
   structure.}},
