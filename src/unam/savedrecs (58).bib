@article{ ISI:000320363800019,
Author = {Cardenas-Rodriguez, N. and Coballase-Urrutia, E. and Huerta-Gertrudis,
   B. and Garcia-Cruz, M. E. and Pedraza-Chaverri, J. and Coria-Jimenez, R.
   and Bandala, C. and Ruiz-Garcia, M.},
Title = {{Antioxidant activity of topiramate: an antiepileptic agent}},
Journal = {{NEUROLOGICAL SCIENCES}},
Year = {{2013}},
Volume = {{34}},
Number = {{5}},
Pages = {{741-747}},
Month = {{MAY}},
Abstract = {{A number of experimental and clinical reports suggest the involvement of
   oxidative stress in pathophysiology of epilepsy. Topiramate, a new
   antiepileptic drug, induces antioxidant effect in epileptic animals.
   However, to date, no further studies appear to be carried out in order
   to demonstrate the ability of topiramate to act as antioxidant.
   Therefore, the objective of this work was to evaluate the in vitro
   superoxide (O-2(center dot-)), hydroxyl radical (OH center dot),
   hypochlorous acid (HOCl), hydrogen peroxide (H2O2), singlet oxygen
   (O-1(2)) and peroxynitrite (ONOO-) scavenging capacity of topiramate in
   comparison with reference compounds. In addition, we investigated the
   possible antitumour activity of this compound in some cancer cell lines.
   Topiramate displays a scavenging capacity compared to the reference
   compound, with the exception of ONOO-, although it was less efficient
   than nordihydroguaiaretic acid, dimethylthiourea, ascorbic acid, sodium
   pyruvate and glutathione for O-2(center dot-), OH center dot, HOCl, H2O2
   and O-1(2) (P < 0.0001), respectively, and not induced significant
   growth inhibition in cancer cell lines. The direct antioxidant
   properties of topiramate could explain the neuroprotective effects
   attributed to this compound and suggest its use as chemopreventive agent
   in a future.}},
