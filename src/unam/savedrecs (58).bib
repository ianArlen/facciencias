@article{ ISI:000316454800004,
Author = {Guven, Jemal and Antonio Santiago, Jose and Vazquez-Montejo, Pablo},
Title = {{Confining spheres within hyperspheres}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2013}},
Volume = {{46}},
Number = {{13}},
Month = {{APR 5}},
Abstract = {{The bending energy of any freely deformable closed surface is quadratic
   in its curvature. In the absence of constraints, it will be minimized
   when the surface adopts the form of a round sphere. If the surface is
   confined within a hypersurface of smaller size, however, this spherical
   state becomes inaccessible. A framework is introduced to describe the
   equilibrium states of the confined surface. It is applied to a
   two-dimensional surface confined within a three-dimensional hypersphere
   of a smaller radius. If the excess surface area is small, then the
   equilibrium states are represented by harmonic deformations of a
   2-sphere: the ground state is described by a quadrupole; all higher
   multipoles are shown to be unstable.}},
