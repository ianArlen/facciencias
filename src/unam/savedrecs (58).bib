@article{ ISI:000316862500001,
Author = {Edinho Robles-Gomez, Edson and Citlali Flores-Villegas, Mirelle and
   Gonzalez-Manjarrez, Alicia and Soriano-Garcia, Manuel},
Title = {{Structural Analysis As an Alternative to Identify and Determine Mode of
   Action of Antimicrobial Peptides: Proposition of a Kinetic Model Based
   on Molecular Dynamics Studies}},
