@article{ ISI:000320000000024,
Author = {Basu, Debjyoti and Pal, Rabindranath and Martinell, Julio J. and Ghosh,
   Joydeep and Chattopadhyay, Prabal K.},
Title = {{Disruption avoidance in the SINP-Tokamak by means of electrode-biasing
   at the plasma edge}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2013}},
Volume = {{20}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{Control of plasma disruption by a biased edge electrode is reported in
   SINP-Tokamak. The features that characterize a plasma disruption are
   reduced with increasing bias potential. The disruption can be completely
   suppressed with the concomitant stabilization of observed MHD modes that
   are allegedly precursors of the disruption. An m = 3/n = 1 tearing mode,
   which apparently causes disruption can be stabilized when a negative
   biasing potential is applied near the edge. These changes in the
   disruptive behavior with edge biasing are hypothesized to be due to
   changes in the current density profile. (C) 2013 AIP Publishing LLC.}},
