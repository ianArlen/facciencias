@article{ ISI:000317126100020,
Author = {Koch, Wolfhard H. G.},
Title = {{Explicit and implicit multi-center integrations}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2013}},
Volume = {{113}},
Number = {{10}},
Pages = {{1573-1583}},
Month = {{MAY 15}},
Abstract = {{We present truncated expansions of multicenter one-electron nuclear
   attraction and two-electron repulsion integrals over localized basis
   functions in terms of one- and two-center integrals of Coulomb,
   exchange, and hybrid type. Two variants are discussed: the Explicit
   Multi-center Integrations and the Implicit Multi-Center Integrations
   (abbreviated as EMCI and IMCI, respectively). While EMCI also deals with
   individual integrals, the IMCI option is the more appealing one: it
   enables us to evaluate the entire matrix elements of Restricted
   HartreeFock-type in a very effective and chemically meaningful way. Due
   to the diatomic nature of our expansions, integrations over Slater-Type
   Orbitals become well-feasible, too. (c) 2012 Wiley Periodicals, Inc.}},
