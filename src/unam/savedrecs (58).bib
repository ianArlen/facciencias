@article{ ISI:000317523900005,
Author = {Solis-Castillo, Berenice and Solleiro-Rebolledo, Elizabeth and Sedov,
   Sergey and Liendo, Rodrigo and Ortiz-Perez, Mario and Lopez-Rivera, Sara},
Title = {{Paleoenvironment and Human Occupation in the Maya Lowlands of the
   Usumacinta River, Southern Mexico}},
