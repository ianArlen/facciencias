@article{ ISI:000315322200022,
Author = {Sanchez-Enriquez, J. and Reyes-Gasga, J.},
Title = {{Obtaining Ca(H2PO4)(2)center dot H2O, monocalcium phosphate monohydrate,
   via monetite from brushite by using sonication}},
Journal = {{ULTRASONICS SONOCHEMISTRY}},
