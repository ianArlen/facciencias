@article{ ISI:000317585200009,
Author = {Fernandez-Garcia, N. and Hernandez, E. and Jauregui, A. and Mondragon,
   A.},
Title = {{Exceptional points of a Hamiltonian of von Neumann-Wigner type}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2013}},
Volume = {{46}},
Number = {{17}},
Month = {{MAY 3}},
Abstract = {{In this paper, we present and discuss an example of exceptional points
   in the continuous spectrum of a Hamiltonian of von Neumann-Wigner type
   associated with a double pole in the normalized Jost eigenfunctions, as
   functions of the wave number. The two unnormalized Jost energy
   eigenfunctions are analytic functions of the wave number k for all k
   complex, but at the exceptional points k = +/- q, they coalesce to give
   rise to Jordan cycles of two generalized bound state quadratically
   integrable eigenfunctions. The Green function has simple poles at the
   exceptional points, but the regular scattering eigenfunction vanishes at
   those points. The scattering matrix is a regular function of the wave
   number at the exceptional points, that is, S(k) does not have a pole at
   the exceptional points.}},
