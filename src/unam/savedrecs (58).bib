@article{ ISI:000317652200001,
Author = {Munoz-Gutierrez, Ivan and Martinez, Alfredo},
Title = {{Polysaccharide hydrolysis with engineered Escherichia coli for the
   production of biocommodities}},
Journal = {{JOURNAL OF INDUSTRIAL MICROBIOLOGY \& BIOTECHNOLOGY}},
