@article{ ISI:A1996VT26600004,
Author = {German, G},
Title = {{The Nambu-Goto string occupied by scalar fields: A two-loop calculation}},
Journal = {{JOURNAL OF PHYSICS G-NUCLEAR AND PARTICLE PHYSICS}},
Year = {{1996}},
Volume = {{22}},
Number = {{11}},
Pages = {{1567-1574}},
Month = {{NOV}},
Abstract = {{We study the properties of the Nambu-Goto string populated by a set of
   self-interacting scalar fields. The contributions to the static
   quark-antiquark potential coming from the string, fields and
   string-field interactions are obtained in a two-loop calculation. For an
   infinitely-long string or massless scalar fields, the interaction
   between string and fields disappears. Also we observe that the energy of
   the system increases and the deconfinement radius (the value of R for
   which the potential vanishes) decreases as the value of the coupling
   parameter grows.}},
