@article{ ISI:A1996VC07100008,
Author = {Xiao, MF},
Title = {{Evanescent field coupling of dipole to a surface: Configurational
   resonance at long distances}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{1996}},
Volume = {{258}},
Number = {{3-4}},
Pages = {{363-368}},
Month = {{AUG 16}},
Abstract = {{Light excited dipole interacts with nearby surfaces and, at certain
   dipole-surface distances, configurational resonances happen. The
   evanescent field coupling can be separately calculated and the
   configurational resonances due to the evanescent coupling may happen at
   long dipole-surface distances. The numerical results of field
   enhancement for silver particle-surface system are provided and
   discussed. In the light of the theory, we also predict that selective
   (from the homogeneous field) detection of the evanescent field may be
   realizable, especially at these resonance distances.}},
