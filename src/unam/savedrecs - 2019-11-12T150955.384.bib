@article{ ISI:A1996UV89400026,
Author = {Busso, M and Origlia, L and Marengo, M and Persi, P and FerrariToniolo,
   M and Silvestro, G and Corcione, L and Tapia, M and Bohigas, J},
Title = {{Mid-infrared imaging of AGB star envelopes .1. Colors, classification
   and mass-loss estimates}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{1996}},
Volume = {{311}},
Number = {{1}},
Pages = {{253-263}},
Month = {{JUL}},
Abstract = {{Mid-infrared images obtained with the TIRCAM camera are presented for a
   sample of 16 sources including both O-rich and C-rich AGB and post-AGB
   stars. Photometry and colors are derived using 10\% band with filters at
   8.8, 9.8, 11.7 and 12.5 mu m. These are compared with photometric data
   and Low Resolution Spectra (LRS) from IRAS. The observations form the
   basis for developing photometric criteria suitable to discriminate
   between O-rich and C-rich sources and to derive mass loss rates. The
   evolutionary status of the observed stars and the consequences for the
   mass loss history on the AGB are discussed. Finally, for the C-rich star
   CW Leo we show evidence for a geometrical extension of the envelope at
   12.5 mu m.}},
