@article{ ISI:A1996UR60500101,
Author = {Hacyan, S},
Title = {{Squeezed states and uncertainty relations in rotating frames and Penning
   traps}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{1996}},
Volume = {{53}},
Number = {{6}},
Pages = {{4481-4487}},
Month = {{JUN}},
Abstract = {{The quantum harmonic oscillator in a nonuniformly rotating frame is
   studied. It is shown that squeezed states and particles are generated by
   inertial effects. The analytic forms of the squeezing and Bogoliubov
   coefficients are given explicitly in terms of the solutions of the
   classical equations of motion. The squeezed states have the minumum
   dispersion allowed by the Robertson uncertainty relations for an
   arbitrary number of observables (the explicit form of these relations is
   obtained). The problem of an `'abrupt twist'' is worked out in detail as
   an example. The formalism can also be applied to the problem of a
   charged particle in a Penning trap, where it is shown that squeezed
   states are produced, with a squeezing frequency of the order of the
   magnetron frequency. In both examples, analytic expressions for the
   variances of the position and momentum are obtained.}},
