Type = {{Article}},
Language = {{English}},
Affiliation = {{UNIV NACL AUTONOMA MEXICO,INST BIOTECNOL,DEPT GENET \& FISIOL MOLEC,CUERNAVACA 62250,MORELOS,MEXICO.
   UNIV NACL AUTONOMA MEXICO,INST FISIOL CELULAR,DEPT BIOFIS,MEXICO CITY,DF,MEXICO.
   COLUMBIA UNIV COLL PHYS \& SURG,DEPT ANAT CELL BIOL \& UROL,NEW YORK,NY 10032.
   COLUMBIA UNIV COLL PHYS \& SURG,CTR REPROD SCI,NEW YORK,NY 10032.}},
DOI = {{10.1016/0014-5793(96)00515-7}},
ISSN = {{0014-5793}},
