@article{ ISI:A1996VK69700005,
Author = {Rivera, RE and Bouzat, G and Poirier, JE and Bessiere, J},
Title = {{Double ionic flotation phenomenon of the barium octyl-benzene-sulphonate
   in aqueous medium.}},
Journal = {{AFINIDAD}},
Year = {{1996}},
Volume = {{53}},
Number = {{464}},
Pages = {{246-250}},
Month = {{JUL-AUG}},
Abstract = {{The ion flotation process of the barium ions with lauryl-sulfate ions in
   aqueous solution has been studied and modeled by Bernasconi et al. The
   barium recovery curves as a function of the laurylsulfate concentration
   are typically curves to one maximum, due to the adsorption of the excess
   of tensoactive over the precipitated phase, which confer it hydrophobic
   properties firstly and then hydrophilic. In this work we show that in
   the same medium and concentration conditions, the flotation behavior of
   the barium ions it's different when sodium octylbenzenesulfonate is used
   as collector, presenting a double flotation phenomena, which has never
   been observed. The solubility diagram and the electrokinetic potential,
   turbidity and conductivity measures, show signs of tensoactive
   adsorption over the precipitated phase.}},
