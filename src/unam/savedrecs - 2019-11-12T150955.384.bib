@article{ ISI:A1996VB61800003,
Author = {Loske, AM and Ballesteros, L and Castano, VM},
Title = {{Two-dimensional spatial correlator for lens-free image processing}},
Journal = {{OPTIK}},
Year = {{1996}},
Volume = {{103}},
Number = {{2}},
Pages = {{59-65}},
Month = {{AUG}},
Abstract = {{A lens-free image-processing device for improving poorly-contrasted
   images and enhancing periodicities by image averaging, is described: The
   apparatus has the capabilites of both, direct visualization and
   performing signal processing tasks like convolutions, filtering and
   cross-correlations without the use of lenses. Some examples show the
   efficiency of the device in filtering `'defects'' and Moire patterns and
   detecting periodicities hidden in nonperiodic structures. Being easy to
   build and of low cost, this optical correlator is also interesting as a
   didactic instrument. The theoretical background related to this device
   is also discussed.}},
