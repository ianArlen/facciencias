@article{ ISI:A1996VU66500009,
Author = {GarciaSainz, JA and CruzMunoz, ME and RomeroAvila, MT},
Title = {{Characterization of the beta(2) adrenoceptors of dog liver}},
Journal = {{COMPARATIVE BIOCHEMISTRY AND PHYSIOLOGY C-PHARMACOLOGY TOXICOLOGY \&
   ENDOCRINOLOGY}},
Year = {{1996}},
Volume = {{115}},
Number = {{1}},
Pages = {{61-65}},
Month = {{SEP}},
Abstract = {{The beta-adrenoceptors present in dog liver were characterized by
   radioligand binding techniques. {[}H-3]Dihydroalprenolol bound with high
   affinity (K-D 0.9 nM) to a relatively low number of sites (approximate
   to 68 fmol/mg protein) in partially purified dog liver plasma membranes.
   These sites were characterized by binding competition; the orders of
   affinity were: (a) for agonists: (-)isoproterenol greater than or equal
   to salbutamol > epinephrine greater than or equal to terbutaline >
   norepinephrine > (+)isoproterenol and (b) for antagonists: propranolol >
   ICI 118551 much greater than metoprolol much greater than atenolol.
   These data indicate that dos liver expresses beta-adrenoceptors of the
   beta(2) subtype. Copyright (C) 1996 Elsevier Science Inc.}},
