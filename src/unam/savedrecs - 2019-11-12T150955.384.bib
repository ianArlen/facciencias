@article{ ISI:A1996VN26100065,
Author = {Miranda, J},
Title = {{Low energy PIXE: Advantages, drawbacks, and applications}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{1996}},
Volume = {{118}},
Number = {{1-4}},
Pages = {{346-351}},
Month = {{SEP}},
Note = {{12th International Conference on Ion Beam Analysis (IBA-12), ARIZONA
   STATE UNIV CAMPUS, TEMPE, AZ, MAY 22-26, 1995}},
Abstract = {{The use of particle induced X-ray emission (PIXE) with proton beams
   having energies below 1 MeV is discussed. The possible advantages it
   presents, such as tow background radiation, good sensitivity for light
   elements, and lower secondary excitation in thick targets, are
   explained. Also, some of the major problems are indicated, related to
   high uncertainties in fundamental constants (stopping and ionization
   cross sections), influence of energy or lateral straggling, and sample
   damage. Finally, a few applications are shown, supporting the attention
   raised by low energy PIXE. They include light element detection, thin
   film thickness measurement, and possibilities of depth profiling.}},
