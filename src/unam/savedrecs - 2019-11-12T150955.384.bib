@article{ ISI:A1996VC98400005,
Author = {Garcia, C and Pardo, JP and MorenoSanchez, R},
Title = {{Control of oxidative phosphorylation supported by nad-linked substrates
   in rat brain mitochondria}},
Journal = {{BIOCHEMICAL ARCHIVES}},
Year = {{1996}},
Volume = {{12}},
Number = {{3}},
Pages = {{157-176}},
Month = {{AUG}},
Abstract = {{The flux control coefficients of various steps of state 3 respiration
   and ATP synthesis were determined in brain mitochondria, in conditions
   that led to changes in the NAD-linked dehydrogenases activity i.e. 0.5
   and 5 mM pyruvate (+ malate), 1 and 10 mM glutamate (+ malate), and 5
   and 427 nM external free Ca2+. Control of flux was homogeneously
   distributed among the several steps evaluated, without the presence of a
   predominant controlling step, under each of the above-mentioned
   conditions. The increase in flux induced by the high concentration of
   Ca2+ or substrate did not modify the contribution of the respiratory
   chain to the control of oxidative phosphorylation; however, a tendency
   to higher values for the control exerted by the phosphorylating system
   and an apparent diminution in the contribution attributed to the Krebs
   cycle enzymes, was also observed. The matrix free Ca2+ and NAD(P)H
   steady state levels were also increased by high external Ca2+, whereas
   the magnitude of the membrane potential remained unchanged. Thus, the
   results of this study may be interpreted in terms of a Ca2+ stimulation
   of oxidative phosphorylation at the NAD-linked dehydrogenases level and,
   perhaps, on sites in the H+ gradient-consuming branch of the pathway.}},
