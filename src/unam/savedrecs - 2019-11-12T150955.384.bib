@article{ ISI:A1996UW27600011,
Author = {Balbas, P and Alexeyev, M and Shokolenko, I and Bolivar, F and Valle, F},
Title = {{A pBRINT family of plasmids for integration of cloned DNA into the
   Escherichia coli chromosome}},
Journal = {{GENE}},
Year = {{1996}},
Volume = {{172}},
Number = {{1}},
Pages = {{65-69}},
Month = {{JUN 12}},
Abstract = {{Plasmid pBRINT is an efficient vector for chromosomal integration of
   cloned DNA into the lacZ gene of Escherichia coli {[}Balbas et al., Gene
   136(1993) 211-213]. A family of related plasmids containing different
   antibiotic-resistance markers (Cm-R or Gm(R) or Km(R)) and a larger
   multiple cloning site (MCS) has been constructed. This set of plasmids,
   whose integration efficiencies are as good as those obtained with the
   prototype plasmid pBRINT, constitutes a collection of tools that allow
   rapid and easy integration of cloned DNA, at the chromosomal level.
   Their functionality as integration vectors has been ascertained by
   integrating the Vitreoscilla sp. hemoglobin-encoding gene and the
   Photobacterium leiognathi lux genes. To evaluate the level of expression
   obtained after chromosomal integration, we constructed strains carrying
   one or two copies of the cat gene integrated in the chromosome, and
   compared their enzymatic activities with those obtained from a strain
   carrying cat on a multicopy plasmid.}},
