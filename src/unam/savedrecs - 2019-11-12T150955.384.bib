@article{ ISI:A1996VX61800007,
Author = {Frank, A and Lemus, R and Bijker, R and PerezBernal, F and Arias, JM},
Title = {{A general algebraic model for molecular vibrational spectroscopy}},
Journal = {{ANNALS OF PHYSICS}},
Year = {{1996}},
Volume = {{252}},
Number = {{1}},
Pages = {{211-238}},
Month = {{NOV 25}},
Abstract = {{We introduce the anharmonic oscillator symmetry model to describe
   Vibrational excitations in molecular systems exhibiting a high degree of
   symmetry. A systematic procedure is proposed to establish the relation
   between the algebraic and configuration space formulations, leading to
   new interactions in the algebraic model. This approach incorporates the
   full power of group theoretical techniques and provides reliable
   spectroscopic predictions. We illustrate the method for the case of
   D-3h-triatomic molecules. (C) 1996 Academic Press, Inc.}},
