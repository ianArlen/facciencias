@article{ ISI:A1996VJ64500005,
Author = {Plata, AM and Ponce, MA and Rios, JM and delaRosa, F and Castano, VM},
Title = {{Degradation of ZnO based surge arresters under field conditions}},
Journal = {{IEE PROCEEDINGS-SCIENCE MEASUREMENT AND TECHNOLOGY}},
