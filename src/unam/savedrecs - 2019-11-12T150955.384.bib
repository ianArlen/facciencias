@article{ ISI:A1996VH18600009,
Author = {Cardenas, L and Dominguez, J and Santana, O and Quinto, C},
Title = {{The role of the nodI and nodJ genes in the transport of Nod metabolites
   in Rhizobium etli}},
Journal = {{GENE}},
Year = {{1996}},
Volume = {{173}},
Number = {{2}},
Pages = {{183-187}},
Month = {{SEP 16}},
Abstract = {{A kinetic analysis of secretion of lipo-chitin oligosaccharides (LCO)
   produced by Rhizobium etli (Re) wild-type (wt) strain and derivatives
   carrying disrupted nodI or nodJ genes was performed. LCO were detected
   in the growth media of the wt strain as early as 1 h after nod gene
   induction. In contrast strains carrying nodI or nodJ mutations secreted
   less LCO, and accumulated LCO metabolites intracellularly after 4 h of
   induction, These Re mutants presented a delayed nodulation phenotype and
   a reduction in the maximum number of nodules formed in Phaseolus
   vulgaris roots.}},
