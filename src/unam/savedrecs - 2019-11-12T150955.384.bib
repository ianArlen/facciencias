@article{ ISI:A1996VB77800006,
Author = {Santoyo, C and Espinosa, MC and Maciel, O},
Title = {{Reciprocity and coercive patterns of preschool children}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1996}},
Volume = {{13}},
Number = {{1}},
Pages = {{63-74}},
Month = {{JUN}},
Abstract = {{On the basis of behavioral criteria the study conducted a cross-section
   comparison of coercive patterns by three and four year olds identified
   as agressive or non agressive. Behavior was recorded in two settings:
   classroom and playground. Trained observers used the Behavioral
   Observation of Social Interaction system achieving reliability superior
   to 79\%. The Kappa concordance analysis revealed excellent levels on the
   basis of the criteria proposed by Bakeman \& Gottman (1986). In general,
   the behavioral criteria for subject selection allowed for the
   differential identification of coercive interactive patterns between
   children classified as agressive and those from a comparison group.
   Younger preschool children showed more coercive behavior than older
   preschoolers and those from the comparison group.}},
