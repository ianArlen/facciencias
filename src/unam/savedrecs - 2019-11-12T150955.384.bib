@article{ ISI:A1996VB77800001,
Author = {Aveleyra, E and Gomez, C and OstroskySolis, F and Rigalt, C and Cruz, F},
Title = {{Adaptation of nonverbal stimuli of Snodgrass and Vanderwart in a Spanish
   speaking population: Judgments for name agreement, image agreement,
   familiarity and visual complexity}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1996}},
Volume = {{13}},
Number = {{1}},
Pages = {{5-19}},
Month = {{JUN}},
Abstract = {{The present study involved the Spanish adaptation and standardization of
   the Snodgrass \& Vanderwart (1980) drawings test. Parameters used to
   evaluate drawings included: name agreement, familiarity, visual
   complexity and image agreement. Results showed that 75\% of pictures
   yielded perfect name agreement among subjects. Correlations among all
   four measures were low, suggesting that most stimuli were highly
   familiar, high in image agreement and low in visual complexity. Results
   yielded reliable and valid data on non-verbal stimuli which can be used
