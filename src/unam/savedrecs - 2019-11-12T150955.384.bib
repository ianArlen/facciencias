@article{ ISI:A1996VN69700001,
Author = {Sebastian, PJ and Ocampo, M},
Title = {{A photodetector based on ZnCdS nanoparticles in a CdS matrix formed by
   screen printing and sintering of CdS and ZnCl2}},
Journal = {{SOLAR ENERGY MATERIALS AND SOLAR CELLS}},
Year = {{1996}},
Volume = {{44}},
Number = {{1}},
Pages = {{1-10}},
Month = {{OCT 30}},
Abstract = {{High photosensitive CdS:ZnCdS structure was formed by screen printing
   and high temperature sintering of CdS and ZnCl2 powders. The sintering
   of the film at high temperatures resulted in the formation of ZnCdS
   nanoparticles in a CdS matrix by an ion interchange reaction followed by
   a solid state reaction. The photosensitivity of the structure depends on
   parameters like initial film composition (ZnCl2 to CdS ratio), sintering
