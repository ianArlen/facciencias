@article{ ISI:A1996VJ94200023,
Author = {GarciaSainz, JA and RomeroAvila, MT and GonzalezEspinosa, C},
Title = {{Coexpression of alpha(1A)- and alpha(1B)-adrenoceptors in the liver of
   the rhesus monkey (Macaca mulatta)}},
Journal = {{EUROPEAN JOURNAL OF PHARMACOLOGY}},
Year = {{1996}},
Volume = {{311}},
Number = {{2-3}},
Pages = {{277-283}},
Month = {{SEP 12}},
Abstract = {{The alpha(1)-adrenoceptors present in the liver of rhesus monkeys was
   characterized using {[}H-3]prazosin. This radioligand binds to monkey
   liver membranes with high affinity (K-D 0.33 nM) to a moderately
   abundant number of sites (97 fmol/mg of protein). These sites were
   characterized pharmacologically, by binding competition, observing two
   affinities for most ligands. The order of potency for agonists was: (a)
   for the high affinity sites: oximetazoline > epinephrine =
   norepinephrine > methoxamine; and (b) for the other sites (low affinity
   for the alpha(1A)-adrenoceptor-selective agonists): oximetazoline
   greater than or equal to epinephrine = norepinephrine >> methoxamine.
   For antagonists the orders of potency were: (a) for the high affinity
   sites:
   R-(-)-5{[}2-{[}{[}2-(ethoxyphenoxy)ethyl]amino]propyl]-2-metoxybenzenesu
   lfonamide HCl (tamsulosin) greater than or equal to
   2-(2,6-dimethoxyphenoxyethyl)-aminomethyyl-1,4-benzodioxane (WB4101)
   greater than or equal to prazosin greater than or equal to (+)-
   niguldipine > 5-methylurapidil = benoxathian > phentolamine >
   8-{[}2-{[}4-(2-methoxyphenyl)-1-piperazinyl]ethyl]-8-azaspiro{[}4,5]deca
   ne-7,9-dione dihydrochloride (BMY 7378); (b) for the other sites (low
   affinity for the alpha(1A)-adrenoceptor-selective antagonists): prazosin
   > tamsulosin > phentolamine = WB4101 > (+)-niguldipine greater than or
   equal to 5-methyl-urapidil = benoxathian > BMY 7378. These data strongly
   suggest that Macaca mulatta liver cells coexpress alpha(1A)- and
   alpha(1B)-adrenoceptors. Expression of the mRNA for these receptors was
   confirmed by reverse transcriptase-polymerase chain reactions.}},
Publisher = {{ELSEVIER SCIENCE BV}},
