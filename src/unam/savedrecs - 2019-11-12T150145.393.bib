@article{ ISI:000248786700018,
Author = {Hess, Peter O. and Greiner, Walter},
Title = {{Shift of the GZK limit in the cosmic ray spectrum due to a smallest
   length scale}},
Journal = {{JOURNAL OF PHYSICS G-NUCLEAR AND PARTICLE PHYSICS}},
Year = {{2007}},
Volume = {{34}},
Number = {{9}},
Pages = {{2091-2098}},
Month = {{SEP}},
Abstract = {{The effect of a finite smallest length scale on the Greisen - Zatsepin -
   Kuzmin limit ( GZK) is studied. A scenario is presented on how to shift
   the predicted cutoff in the cosmic ray spectrum at 1020 eV to larger
   energies without breaking the Lorentz invariance. The formulation is
   based on a pseudo- complex extension of standard field theory which
   implies a minimal length scale. The dispersion relation of particles can
   be changed, leading to a modification of the GZK limit. Maximal shifts
   are determined.}},
