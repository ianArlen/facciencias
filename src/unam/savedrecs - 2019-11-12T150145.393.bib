@article{ ISI:000249227100032,
Author = {Yadava, M. G. and Ramesh, R. and Pandarinath, K.},
Title = {{Positive `amount effect' in the Sahayadri (Western Ghats) rainfall}},
Journal = {{CURRENT SCIENCE}},
Year = {{2007}},
Volume = {{93}},
Number = {{4}},
Pages = {{560-564}},
Month = {{AUG 25}},
Abstract = {{We present stable oxygen isotopic ratios (delta O-18) of rainfall
   collected over three years (July to October 200002) at Mangalore. We
   observe an apparent positive correlation between the monthly,
   amount-weighted mean oxygen isotopic ratio of the rainwater and monthly
   total rainfall, contrary to the generally expected negative trend in
   many island and continental stations. We offer an explanation for this
   observation and propose that the `amount effect' in the annual rainfall
   still remains with a negative slope, and thus can be useful in
   palaeomonsoon reconstruction using oxygen isotopic variations of
   annually laminated speleothems.}},
