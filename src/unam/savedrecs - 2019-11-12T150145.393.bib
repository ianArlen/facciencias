@article{ ISI:000248150000133,
Author = {Santana Gil, A. and Sanchez Ll, J. L. and Valenzuela, R. and Hernando,
   B. and Perez, M. J. and Santos, J. D.},
Title = {{In-plane magnetic properties of as-spun YxPr1-xCo5 ribbons (x=0.25,
   0.50, 0.75)}},
