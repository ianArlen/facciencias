@article{ ISI:000249591300011,
Author = {Veliz-Enriquez, Mayra Y. and Gonzalez, Gonzalo and Pfeiffer, Heriberto},
Title = {{Synthesis and CO2 capture evaluation of Li2-xKxZrO3 solid solutions and
   crystal structure of a new lithium-potassium zirconate phase}},
