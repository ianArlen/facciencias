@article{ ISI:000250388600035,
Author = {Meza-Laguna, V. and Basiuk, E. V. and Alvarez-Zauco, E. and
   Acosta-Najarro, D. and Basiuk, V. A.},
Title = {{Cross-linking of C-60 films with 1,8-diaminooctane and further
   decoration with silver nanoparticles}},
Journal = {{JOURNAL OF NANOSCIENCE AND NANOTECHNOLOGY}},
