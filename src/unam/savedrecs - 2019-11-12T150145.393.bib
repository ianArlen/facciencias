@article{ ISI:000248783200036,
Author = {Pineda-Pinon, J. and Mendoza-Lopez, M. L. and Manzano-Ramirez, A. and
   Perez-Robles, J. F. and Vega-Duran, J. T.},
Title = {{Water diffusion in clays with added organic surfactants}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{2007}},
Volume = {{40}},
Number = {{16}},
Pages = {{4991-4997}},
Month = {{AUG 21}},
Abstract = {{Tensoactive agents may decrease water absorption in clay products like
   adobes. They modify the characteristics of the surface of clay
   particles. Characterization of water diffusion through the pores of
   modified clays is important to apply appropriate surface modifiers and
   to improve their performance. We established a simple model for water
   diffusion in test samples of defined dimensions to estimate real
   physical parameters and their effect on water absorption. Adsorption
   mechanisms are examined based on experimental results. The fitting of
   the experimental data to the model provides a deep understanding of
   water adsorption in chemically modified clays. A better agreement
   between the model and the experimental data is achieved for complex
   molecules.}},
