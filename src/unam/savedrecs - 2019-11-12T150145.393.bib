@article{ ISI:000250790500008,
Author = {Escalante-Sandoval, Carlos},
Title = {{Design rainfall estimation using exponentiated and mixed exponentiated
   distributions in the Coast of Chiapas}},
Journal = {{INGENIERIA HIDRAULICA EN MEXICO}},
Year = {{2007}},
Volume = {{22}},
Number = {{4}},
Pages = {{103-113}},
Month = {{OCT-DEC}},
Abstract = {{The assessment of extreme precipitation is an important problem in
   hydrologic risk analysis and design. One of the objectives in hydrologic
   analysis is the statistical uncertainty reduction of design events. In
   this paper, Gumbel, Weibull, and Frechet exponentiated distributions,
   along with three more mixed exponentiated distributions are used to
   model the annual maximum daily rainfall from 19 gauging stations located
   on the coast of Chiapas. In general, mixed exponentiated distributions
   provided the best fits and they were measured using the standard
   likelihood ratio. It was also shown that it is very important to
   consider the exponentiated and mixed exponentiated distributions as
   additional mathematical tools when modeling hydrometeorological series.}},
