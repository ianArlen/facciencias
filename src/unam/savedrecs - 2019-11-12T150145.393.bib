@article{ ISI:000249528000022,
Author = {Anguiano, Gerardo and Llera-Herrera, Raul and Rojas, Emilio and
   Vazquez-Boucard, Celia},
Title = {{Subchronic organismal toxicity, cytotoxicity, genotoxicity, and feeding
   response of Pacific oyster (Crassostrea Gigas) to lindane (gamma-HCH)
   exposure under experimental conditions}},
Journal = {{ENVIRONMENTAL TOXICOLOGY AND CHEMISTRY}},
Year = {{2007}},
Volume = {{26}},
Number = {{10}},
Pages = {{2192-2197}},
Month = {{OCT}},
Abstract = {{This study evaluated organismal toxicity, cytotoxicity, and genotoxicity
   and the filtration rate in response to different concentrations of
   subchronic lindane (gamma-hexachlorocyclohexane {[}gamma-HCH]), exposure
   (12 d) in adult Pacific oysters Crassostrea gigas. Oysters were exposed
   in vivo in laboratory aquaria to 10 different concentrations (0.0- 10.0
   mg/L) of gamma-HCH. Tile median lethal concentration (LC50) after 12 d
   was calculated as 2.22 mg/L. Cytotoxic effects were observed in
   hemocytes. where the mean cell viability was significantly decreased at
   1.0 mg/L of -y-HCH after 12 d. Genotoxicity of gamma-HCH measured by
   single cell gel electrophoresis assay, in hemocytes was evident at 0.7
   mg/L of gamma-HCH after 12 d. After 4 It of exposure to gamma-HCH,
   filtration rates were reduced compared with controls to 65.8 and 38,2\%
   at concentrations of 0.3 and 0.7 mg/L, respectively, and after 11 d of
   exposure, filtration rates were reduced to 60.4 and 30.9\% at
