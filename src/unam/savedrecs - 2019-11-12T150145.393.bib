@article{ ISI:000248612300011,
Author = {Martinez, T. and Navarrete, M. and Cabrera, L. and Juarez, F. and Ramos,
   A. and Vazquez, K.},
Title = {{K-40 activities and potassium concentrations in tobacco samples of
   Mexican cigarettes}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2007}},
Volume = {{273}},
Number = {{3}},
Pages = {{569-572}},
Month = {{SEP}},
Abstract = {{Nine brands of tobacco cigarettes manufactured and distributed in the
   Mexican market were analyzed by gamma-spectrometry to certify their
   non-artificial radioactive contamination. Since natural occurring
   radioactive materials (NORM) K-40, Th-232, U-235, and U-239 (and decay
   products from the latter three nuclides) are the main sources for human
   radiation exposure, the aim of this work was to determine the activity
   of K-40 and potassium concentration. Averages of (40)supercript stopK
   and potassium concentration were of 1.29 +/- 0.18 Bq center dot g(1),
   and 4.0 +/- 0.57\%. The annual dose equivalents to the whole body from
   ingestion and inhalation of 26 Bq K-40 were 0.23 mu Sv and 15.8 mu Sv,
   respectively. The corresponding 50 years committed dose equivalents was
   0.23 mu Sv. The total committed dose to the lungs due to inhalation of
   K-40 in tobacco was 16 mu Sv. Potassium concentrations obtained in this
   work were in the same range of those obtained by INAA, so showing that
   the used technique is acute, reproducible, and accessible to
   laboratories equipped with low background scintillation detectors.}},
