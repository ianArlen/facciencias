@article{ ISI:000248150000287,
Author = {Alvarez, G. and Zamorano, R. and Heiras, J. and Castellanos, M. and
   Valenzuela, R.},
Title = {{An electron paramagnetic resonance and magnetically modulated microwave
   absorption characterization of thermochromic (Ba, Li)-Mn oxides}},
