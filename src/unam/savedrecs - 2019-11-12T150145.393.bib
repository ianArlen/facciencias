@article{ ISI:000249929500010,
Author = {Godinez-Martinez, Arturo and Watson, Alan M. and Matthews, Lynn D. and
   Sparke, Linda S.},
Title = {{Photometry of polar-ring galaxies}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2007}},
Volume = {{43}},
Number = {{2}},
Pages = {{315-327}},
Month = {{OCT}},
Abstract = {{We have obtained photometry in B and R for seven confirmed or probable
   polar-ring galaxies from the Polar-Ring Catalog of Whitmore et al.
   (1990). The rings show a range of colors from B - R approximate to 0.6
   to B - R approximate to 1.7. The bluest rings have bright H II regions,
   which are direct evidence for recent star formation. The minimum age of
   the reddest ring, that in PRC B-20, is somewhat uncertain because of a
   lack of knowledge of the internal reddening and metallicity, but appears
   to be at least 1.2 Gyr. As such, this ring is likely to be stable for at
   least several rotation periods. This ring is an excellent candidate for
   future studies that might better determine if it is truly old.}},
