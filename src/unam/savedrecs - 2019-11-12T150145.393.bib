@article{ ISI:000250733900007,
Author = {Romero, David and Sanchez-Arroyo, Abdon},
Title = {{Adding evidence to the Erdos-Faber-Lovasz conjecture}},
Journal = {{ARS COMBINATORIA}},
Year = {{2007}},
Volume = {{85}},
Pages = {{71-84}},
Month = {{OCT}},
Abstract = {{A hypergraph is linear if no two distinct edges intersect in more than
   one vertex. A long standing conjecture of Erdos, Faber and Lovasz states
   that if a linear hypergraph has n edges, each of size n, then its
   vertices can be properly colored with n colors. We prove the correctness
   of the conjecture for a new, infinite class of linear hypergraphs.}},
