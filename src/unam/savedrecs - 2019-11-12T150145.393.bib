@article{ ISI:000250379400013,
Author = {Zolotukhin, Mikhail G. and Fomine, Serguie and Lazo, Luz Maria and
   Hernandez, Ma. Del Carmen G. and Guzman-Gutierrez, M. T. and
   Ruiz-Trevino, Alberto and Fritsch, Detlev and Cuellas, David Cuellas and
   Fernandez-G, Juan M.},
Title = {{A novel approach to the synthesis of high performance and functional
   polymers}},
