@article{ ISI:000250071300006,
Author = {Abarca-Arenas, L. G. and Franco-Lopez, J. and Peterson, M. S. and
   Brown-Peterson, N. J. and Valero-Pacheco, E.},
Title = {{Sociometric analysis of the role of penaeids in the continental shelf
   food web off Veracruz, Mexico based on by-catch}},
