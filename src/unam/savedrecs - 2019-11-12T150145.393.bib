@article{ ISI:000248773400015,
Author = {Leon-Silva, U. and Nicho, M. E. and Hu, Hailin and Cruz-Silva, Rodolfo},
Title = {{Effect of modified ITO substrate on electrochromic properties of
   polyaniline films}},
Journal = {{SOLAR ENERGY MATERIALS AND SOLAR CELLS}},
