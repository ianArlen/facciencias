@article{ ISI:000258564600005,
Author = {Lisbona Guillen, Miguel},
Title = {{The Beginnings of Anti-Church Policies in Chiapas during the Mexican
   Revolution (1910-1920)}},
Journal = {{HISTORIA MEXICANA}},
Year = {{2007}},
Volume = {{57}},
Number = {{2}},
Pages = {{491+}},
Month = {{OCT-DEC}},
Abstract = {{In Chiapas, as in other states of South-Eastern Mexico, the 1930s were
   the years of most intense religious persecution. However, anti-Church
   policies had begun before that, supported by successive Mexican
   governments, mainly during Carranza's administration. This paper
   describes the circumstances that prevailed during this decade, as well
   as some of the Catholic Church's reactions in Chiapas, for public
