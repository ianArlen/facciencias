@article{ ISI:000251318800003,
Author = {Bugeaud, Yann and Luca, Florian and Mignotte, Maurice and Siksek, Samir},
Title = {{Perfect powers from products of terms in Lucas sequences}},
Journal = {{JOURNAL FUR DIE REINE UND ANGEWANDTE MATHEMATIK}},
Year = {{2007}},
Volume = {{611}},
Pages = {{109-129}},
Month = {{OCT}},
Abstract = {{Suppose that \{U-n\}(n >= 0) is a Lucas sequence, and suppose that l(1),
   ..., l(t) are primes. We show that the equation
   U-n1 ... U-nm =+/- l(1)(x1) ... l(t)(x1)y(p), p prime, m<p, has only
   finitely many solutions. Moreover, we explain a practical method of
   solving these equations. For example, if \{F-n\}(n >= 0) is the
   Fibonacci sequence, then we solve the equation
   F-n1 ... F-nm = 2(x1). 3(x2) . 5(x3) ... 541(x100)y(p)
   under the restrictions: p is prime and m < p.}},
