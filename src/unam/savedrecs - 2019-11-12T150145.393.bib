@article{ ISI:000250900700009,
Author = {Rivera, C. and Pilatowsky, I. and Mendez, E. and Rivera, W.},
Title = {{Experimental study of a thermo-chemical refrigerator using the barium
   chloride-ammonia reaction}},
Journal = {{INTERNATIONAL JOURNAL OF HYDROGEN ENERGY}},
Year = {{2007}},
Volume = {{32}},
Number = {{15, SI}},
Pages = {{3154-3158}},
Month = {{OCT}},
Note = {{International Symposium on Solar-Hydrogen-Fuel Cells, Cancun, MEXICO,
   2005}},
Abstract = {{An experimental intermittent thermochemical refrigeration system
   operating with barium chloride-ammonia reaction is described. The barium
   chloride is used as solid absorbent and ammonia as refrigerant. The
   equipment components and the experimental preliminary results are also
   presented. The temperature range of the heating fluid from 70 to 95
   degrees C was established. The main results showed that the generation
   temperature was 53 degrees C for a condensation temperature of 23
   degrees C. In the evaporation-absorption process, the evaporating
   temperature was between -10 and 0 degrees C. The results showed the
   technical feasibility to operate this refrigeration system with low cost
   solar flat plate collectors in remote areas. (C) 2007 International
   Association for Hydrogen Energy. Published by Elsevier Ltd. All rights
   reserved.}},
