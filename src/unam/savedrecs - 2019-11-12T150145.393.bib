@article{ ISI:000250790500002,
Author = {Mendoza, Rosalva and Rodriguez, Katya and Alvarez-Icaza, Luis},
Title = {{Surface generation by genetic programming}},
Journal = {{INGENIERIA HIDRAULICA EN MEXICO}},
Year = {{2007}},
Volume = {{22}},
Number = {{4}},
Pages = {{21-31}},
Month = {{OCT-DEC}},
Abstract = {{The use of evolutionary computing (EC) for solving the problem of
   surface interpolation from a set of topographic data is proposed. The
   goal is to obtain a mathematical function that may represent, in a
   precise and compact fashion, a surface represented by samples of
   topographic data. EC is a very promising alternative to polynomial
   interpolation techniques, because it is able to directly deal with
   nonlinear behavior, very common in complex interpolation problems. In
   this paper, one specific EC technique-genetic programming (GP)-is
   applied to a set of topographic data from the Colorado River, located in
   the northwestern region of Mexico. A function set consisting of six
   operations was defined: four arithmetic and two trigonometric functions.
   Several tests were performed to determine the number of tree nodes,
   taking care of reducing the error size in the best individual of the
   population. Another set of tests to find the optimal number of
   generations was also performed. Results, obtained by implementing the
   algorithm in MATLAB, indicate a substantial improvement in the quality
   of the approximation when compared with a traditional polynomial
   approximation method applied to the same topographic region, both in
   total approximation error and in the size of the maximum error produced.}},
