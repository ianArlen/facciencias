@article{ ISI:000249474000110,
Author = {Castro-Garay, P. and Reyes, J. Adrian and Ramos-Garcia, R.},
Title = {{Band structure controlled by chiral imprinting}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2007}},
Volume = {{91}},
Number = {{11}},
Month = {{SEP 10}},
Abstract = {{Using the configuration of an imprinted cholesteric elastomer immersed
   in a racemic solvent, the authors find the solution of the
   boundary-value problem for the reflection and transmission of incident
   optical waves due to the elastomer. They show a significant width
   reduction of the reflection band for certain values of nematic
   penetration depth, which depends on the volume fraction of molecules
   from the solvent, whose handedness is preferably absorbed. The
   appearance of nested band gaps of both handednesses during the sorting
   mixed chiral process is also obtained. This suggests the design of
   chemically controlled optical filters and optically monitored chiral
   pumps.}},
