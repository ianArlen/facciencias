@article{ ISI:000266093800054,
Author = {Rodriguez-Gonzalez, Vicente and Zanella, Rodolfo and Calzada, Lina A.
   and Gomez, Ricardo},
Title = {{Low-Temperature CO Oxidation and Long-Term Stability of Au/In2O3-TiO2
   Catalysts}},
