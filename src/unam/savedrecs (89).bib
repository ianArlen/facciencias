@article{ ISI:000264573900012,
Author = {Li, Xiao-Jun and Li, Chun-Ping and Yang, Ju-Cai and Jalbout, Abraham F.},
Title = {{The Structures, Thermochemistry, and Electron Affinities of Hydrogenated
   Silicon Clusters Si6Hn/Si6Hn- (n=3-14)}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2009}},
Volume = {{109}},
Number = {{6}},
Pages = {{1283-1301}},
Month = {{MAY}},
Abstract = {{The hydrogenated silicon clusters structures, electron affinities, and
   dissociation energies of the Si6Hn/Si6Hn- (n = 3-14) species have been
   systematically investigated by means of three density functional theory
   (DFT) methods. The basis set used in this work is of double-zeta Plus
   polarization quality with additional diffuse s- and p-type functions,
   denoted DZP++. The geometries are fully optimized with each DFT method
   independently. Three different types of energy separations presented in
   this work are the adiabatic electron affinity (EA(ad)) the vertical
   electron affinity (EA(vert)), and the vertical detachment energy (VIDE).
   The first Si-H dissociation energies D-e (Si6Hn -> Si6Hn-1+H) for the
   neutral Si6Hn and D-e (Si6Hn--> Si6Hn-1-+H) for the anionic Si6Hn-+H)
   species have also been reported. (C) 2008 Wiley Periodicals, Inc. Int J
   Quantum Chem 109: 1283-1301, 2009}},
