@article{ ISI:000266326400017,
Author = {Garcia, J. and Hallen, J. and Esparza, R. and Perez, R.},
Title = {{Microstructural aspects of the brittleness phenomena in steels induced
   by mill-annealing}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2009}},
Volume = {{55}},
Number = {{1, S}},
