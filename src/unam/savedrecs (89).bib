@article{ ISI:000265714000007,
Author = {Juarez-Calderon, J. M. and Negron-Mendoza, A. and Gomez-Vidales, V. and
   Ramos-Bernal, S.},
Title = {{Study of dosimetric properties of acetylsalicylic acid in pharmaceutical
   preparations by EPR spectroscopy}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2009}},
Volume = {{280}},
Number = {{2}},
Pages = {{245-249}},
Month = {{MAY}},
Note = {{2nd International Nuclear Chemistry Congress (2nd-INCC), Cancun, MEXICO,
   APR 13-17, 2008}},
Abstract = {{Electron paramagnetic resonance (EPR) was used to investigate the
   dosimetric properties of two pharmaceutical preparations containing
   acetylsalicylic acid, AspirinA (R) and CafiaspirinA (R). The EPR spectra
   of the irradiated samples were found to have an asymmetric absorption
   characterized by a major resonance at g = 2.0033. Dose response was
   investigated between dose ranges of 2 to 95 kGy for Co-60-gamma rays.
   Fading characteristics and dependence on temperature irradiation were
   also studied. We suggest that commercial AspirinA (R) and CafiaspirinA
   (R) tablets can be used as dosimeters in the case of a short accident.}},
