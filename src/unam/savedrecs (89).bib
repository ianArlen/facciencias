@article{ ISI:000267254600023,
Author = {Sousa S, Mario},
Title = {{Punctati Sect. of the genus Lonchocarpus (Leguminosae, Papilionoidae,
   Milettieae) for Mesoamerica}},
Journal = {{NOVON}},
Year = {{2009}},
Volume = {{19}},
Number = {{2}},
Pages = {{239-255}},
Month = {{JUN}},
Abstract = {{An account of section Punctati Bentham for the genus Lonchocarpus Kunth
   in Mesoamerica is given. Of the 12 species treated, five are new to
