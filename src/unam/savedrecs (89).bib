@article{ ISI:000265208300008,
Author = {Escalante-Mancera, Edgar and Silva-Casarin, Rodolfo and Mendoza-Baldwin,
   Edgar and Marino-Tapia, Ismael and Ruiz-Renteria, Francisco},
Title = {{Sea level variation and currents induced by hurricane Wilma in Puerto
   Morelos, Quintana Roo, Mexico}},
Journal = {{INGENIERIA HIDRAULICA EN MEXICO}},
Year = {{2009}},
Volume = {{24}},
Number = {{2}},
Pages = {{111-126}},
Month = {{APR-JUN}},
Abstract = {{From the number and intensity of tropical cyclones recorded in 2005, it
   can be seen that this was not a typical year; several historical records
   were established. An acoustic wave and current profiler, located off
   Puerto Morelos, Q.R., recorded information from part of this
   record-breaking year and the analysis of these results is presented
   here. This paper analyzes the storm surge effect through the separation
   of the meteorological and astronomic components of sea level evolution,
   its relation to the changes in atmospheric pressure, and wind intensity
   and direction. In addition, the vertical and horizontal currents
   generated by Wilma are analysed. From the results it is clear that there
   is a correlation between these currents and the severe decrease in sea
   bottom temperatures as the hurricane passed over the area.}},
