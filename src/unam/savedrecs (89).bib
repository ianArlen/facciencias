@article{ ISI:000264455900019,
Author = {Licona-Limon, Paula and Aleman-Muench, German and Chimal-Monroy, Jesus
   and Macias-Silvac, Marina and Garcia-Zepeda, Eduardo A. and Matzuk,
   Martin M. and Fortoul, Teresa I. and Soldevila, Gloria},
Title = {{Activins and inhibins: Novel regulators of thymocyte development}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{2009}},
Volume = {{381}},
Number = {{2}},
Pages = {{229-235}},
Month = {{APR 3}},
Abstract = {{Activins and inhibins are members of the transforming growth factor-p
   superfamily that act on different cell types and regulate a broad range
   of cellular processes including proliferation, differentiation, and
   apoptosis. Here, we provide the first evidence that activins and
   inhibins regulate specific checkpoints during thymocyte development. We
   demonstrate that both activin A and inhibin A promote the DN3-DN4
   transition in vitro, although they differentially control the transition
   to the DIP stage. Whereas activin A induces the accumulation of a
   CD(8+)CD24(+)TCR beta(10) intermediate subpopulation, inhibin A promotes
   the differentiation of DN4 to DIP. In addition, both activin A and
   inhibin A appear to promote CD8(+)SP differentiation. Moreover, inhibin
   alpha null mice have delayed in vitro T cell development, showing both a
   decrease in the DN-DP transition and reduced thymocyte numbers, further
   Supporting a role for inhibins in the control of developmental signals
   taking place during T cell differentiation in vivo. (C) 2009 Elsevier
   Inc. All rights reserved.}},
