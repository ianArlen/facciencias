@article{ ISI:000270993500001,
Author = {Nakano, Hirotaka},
Title = {{The Second Half of the Transcendental Deduction in Kant's Critique of
   Pure Reason (B)}},
Journal = {{IDEAS Y VALORES}},
Year = {{2009}},
Volume = {{58}},
Number = {{139}},
Pages = {{5-20}},
Month = {{APR}},
Abstract = {{The Transcendental Deduction in the second edition of Kant's Critique of
   Pure Reason is divided in two parts. Nevertheless, the role of the
   second half is not immediately clear. This article intends to examine
   the argument presented in the second half after clarifying its purpose.
   Based on this approach, we sustain an interpretation according to which
   Kant tries to establish the validity of categories for all intuition
   given through sensibility. This interpretation seeks to confirm a
   conceptual articulation among sensible intuitions that is not regarded
   as a result of objective judgment.}},
