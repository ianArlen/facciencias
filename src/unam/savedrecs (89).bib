@article{ ISI:000265721100022,
Author = {Gomez-Bernal, H. and Cedeno-Caero, L. and Gutierrez-Alejandre, Aida},
Title = {{Liquid phase oxidation of dibenzothiophene with alumina-supported
   vanadium oxide catalysts: An alternative to deep desulfurization of
