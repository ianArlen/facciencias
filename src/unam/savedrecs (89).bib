@article{ ISI:000264295800051,
Author = {Afraimovich, V. and Rechtman, R.},
Title = {{Local complexity functions of interval exchange transformations}},
Journal = {{COMMUNICATIONS IN NONLINEAR SCIENCE AND NUMERICAL SIMULATION}},
Year = {{2009}},
Volume = {{14}},
Number = {{4}},
Pages = {{1454-1460}},
Month = {{APR}},
Abstract = {{We studied numerically complexity functions for interval exchange
   transformations. We have shown that they grow linearly in time as well
   as the c-complexity function. Moreover, we found out that they depend
   also linearly on E where E is the Lebesgue measure of a set of initial
   points. This allows us to hypothesize that the dimension of the measure
   related to the E-complexity function could be determined by studying the
   dependence of local complexity functions on C. (c) 2008 Elsevier B.V.
   All rights reserved.}},
