@article{ ISI:000264881300005,
Author = {Elliott, Geoffrey N. and Chou, Jui-Hsing and Chen, Wen-Ming and
   Bloemberg, Guido V. and Bontemps, Cyril and Martinez-Romero, Esperanza
   and Velazquez, Encarna and Young, J. Peter W. and Sprent, Janet I. and
   James, Euan K.},
Title = {{Burkholderia spp. are the most competitive symbionts of Mimosa,
   particularly under N-limited conditions}},
Journal = {{ENVIRONMENTAL MICROBIOLOGY}},
Year = {{2009}},
Volume = {{11}},
Number = {{4}},
Pages = {{762-778}},
Month = {{APR}},
Abstract = {{Bacteria isolated from Mimosa nodules in Taiwan, Papua New Guinea,
   Mexico and Puerto Rico were identified as belonging to either the alpha-
   or beta-proteobacteria. The beta-proteobacterial Burkholderia and
   Cupriavidus strains formed effective symbioses with the common invasive
   species Mimosa diplotricha, M. pigra and M. pudica, but the
   alpha-proteobacterial Rhizobium etli and R. tropici strains produced a
   range of symbiotic phenotypes from no nodulation through ineffective to
   effective nodulation, depending on Mimosa species. Competition studies
   were performed between three of the alpha-proteobacteria (R. etli TJ167,
   R. tropici NGR181 and UPRM8021) and two of the beta-rhizobial symbionts
   (Burkholderia mimosarum PAS44 and Cupriavidus taiwanensis LMG19424) for
   nodulation of these invasive Mimosa species. Under flooded conditions,
   B. mimosarum PAS44 out-competed LMG19424 and all three
   alpha-proteobacteria to the point of exclusion. This advantage was not
   explained by initial inoculum levels, rates of bacterial growth,
   rhizobia-rhizobia growth inhibition or individual nodulation rate.
   However, the competitive domination of PAS44 over LMG19424 was reduced
   in the presence of nitrate for all three plant hosts. The largest
   significant effect was for M. pudica, in which LMG19424 formed 57\% of
   the nodules in the presence of 0.5 mM potassium nitrate. In this host,
   ammonium also had a similar, but lesser, effect. Comparable results were
   also found using an N-containing soil mixture, and environmental N
   levels are therefore suggested as a factor in the competitive success of
   the bacterial symbiont in vivo.}},
