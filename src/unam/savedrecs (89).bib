@article{ ISI:000265314900011,
Author = {Diaz, Lorenza and Ceja-Ochoa, Irais and Restrepo-Angulo, Ivan and
   Larrea, Fernando and Avila-Chavez, Euclides and Garcia-Becerra, Rocio
   and Borja-Cacho, Elizabeth and Barrera, David and Ahumada, Elias and
