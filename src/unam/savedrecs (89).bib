@article{ ISI:000268163400002,
Author = {Arroyo, Elsa},
Title = {{The cultural biography of a premature ruin: the Virgen del Perdon by
   Simon Pereyns}},
Journal = {{GOYA}},
Year = {{2009}},
Number = {{327}},
Pages = {{95+}},
Month = {{APR-JUN}},
Abstract = {{The study discusses the multiple meanings and responses that the
   beholder has assigned to the painting Virgen del Perdon through out
   cultural conditions along history. The Flemish Simon Pereyns painted
   this panel in 1568 for the first Cathedral of Mexico. During the fire of
   1967, over 70\% of the panel painting was carbonized, and even though it
   suffered great deterioration, it has been conserved and exposed in
   recent art exhibitions. The results from the technical examination of
   the Virgen del Perdon gave arguments to relate the artistic production
   of Simon Pereyns in New Spain, with the tradition of panel paintings at
   the royal court of Felipe II. The artist's intention, the status of the
   original image, and its transformation into a `premature ruin' is
   explored under art historical interpretation and material analysis.}},
