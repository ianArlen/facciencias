@article{ ISI:000267509800005,
Author = {Quevedo, M. C. and Genesca, J.},
Title = {{Influence of turbulent flow on the corrosion of Al-Zn-Mg galvanic anode
   in artificial seawater media}},
Journal = {{MATERIALS AND CORROSION-WERKSTOFFE UND KORROSION}},
Year = {{2009}},
Volume = {{60}},
Number = {{6}},
Pages = {{424-430}},
Month = {{JUN}},
Abstract = {{The effect of hydrodynamics on the corrosion of AI(14 wt\%)-Zn(8
   wt\%)-Mg alloy in artificial seawater media at room temperature was
   studied in a rotating cylinder electrode (RCE) system under turbulent
   flow conditions. Five different rotation rates were studied: 100, 1000,
   3060, 5000, and 7000 rpm. The corrosion rates were measured by
   electrochemical impedance spectroscopy (EIS). For the system studied,
   the steady-state corrosion potential increased with increase in rotation
   rate. The effect of increasing the rotation rate is to increase the
   availability of oxygen at the surface, which in turn will polarize the
   corrosion reaction in the more noble direction. The corrosion rate also
   increases with increase in RCE rotation rate. This reflects the fact
   that the rate of corrosion is controlled, at least in part, by the rate
   of mass transfer. In this case, the effect of increase in the rotation
   rate on the corrosion rate is to increase the interfacial concentration
   of the reactant (oxygen).}},
