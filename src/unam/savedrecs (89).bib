   Prospero-Garcia, Oscar},
Title = {{BCL-2 and BAX Proteins Expression Throughout the Light-Dark Cycle and
   Modifications Induced by Sleep Deprivation and Rebound in Adult Rat
   Brain}},
Journal = {{JOURNAL OF NEUROSCIENCE RESEARCH}},
Year = {{2009}},
Volume = {{87}},
