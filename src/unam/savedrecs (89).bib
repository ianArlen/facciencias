@article{ ISI:000265683700003,
Author = {Loza-Mejia, Marco A. and Olvera-Vazquez, Susana and Maldonado-Hernandez,
   Karina and Guadarrama-Salgado, Teresita and Gonzalez-Sanchez, Ignacio
   and Rodriguez-Hernandez, Fernando and Solano, Jose D. and
   Rodriguez-Sotres, Rogelio and Lira-Rocha, Alfonso},
Title = {{Synthesis, cytotoxic activity, DNA topoisomerase-II inhibition,
   molecular modeling and structure-activity relationship of
   9-anilinothiazolo{[}5,4-b]quinoline derivatives}},
Journal = {{BIOORGANIC \& MEDICINAL CHEMISTRY}},
Year = {{2009}},
Volume = {{17}},
