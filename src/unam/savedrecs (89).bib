@article{ ISI:000267254600020,
Author = {Scheinvar, Leia and Manzanero, Gladys},
Title = {{Opuntia chiangiana, a new species of Cactaceae in Oaxaca, Mexico}},
Journal = {{NOVON}},
Year = {{2009}},
Volume = {{19}},
Number = {{2}},
Pages = {{222-228}},
Month = {{JUN}},
Abstract = {{A new species of Opuntia (L.) Miller (Cactaceae), endemic to the region
   of Cuicatalan in the state of Oaxaca, Mexico, is described. The fruits
   are red-violet with long spines, funiculi that are almost dry, and fruit
   walls that are edible and do not fall off when mature. However, birds
   have not been observed to eat the fruits of this sweet-acidic cactus
   pear (known as xoconostle or xoco-tuna). The new species is compared to
   three sympatric species formerly described: O. tehuacana S. Arias \& U.
   Guzman, O. parviclada S. Arias \& Gama, and O. olmeca Joel Perez, J.
   Reyes \& F. Brachet. Opuntia, chiangiana Scheinvar \& Manzanero is
   assigned to series Heliabravoanae Scheinvar on the basis of its acidic
   fruits.}},
