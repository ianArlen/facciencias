@article{ ISI:000266475000041,
Author = {Xu, Zhiyong and Smyth, Noel F. and Minzoni, Antonmaria A. and Kivshar,
   Yuri S.},
Title = {{Vector vortex solitons in nematic liquid crystals}},
Journal = {{OPTICS LETTERS}},
Year = {{2009}},
Volume = {{34}},
Number = {{9}},
Pages = {{1414-1416}},
Month = {{MAY 1}},
Abstract = {{We analyze the existence and stability of two-component vector solitons
   in nematic liquid crystals for which one of the components carries
   angular momentum and describes a vortex beam. We demonstrate that the
   nonlocal, nonlinear response can dramatically enhance the field coupling
   leading to the stabilization of the vortex beam when the amplitude of
   the second beam exceeds some threshold value. We develop a variational
   approach to describe this effect analytically. (C) 2009 Optical Society
   of America}},
