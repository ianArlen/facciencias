@article{ ISI:000266369300012,
Author = {Reyes, C. M. and Urrutia, L. F. and Vergara, J. D.},
Title = {{The photon sector in the quantum Myers-Pospelov model: An improved
   description}},
Journal = {{PHYSICS LETTERS B}},
