@article{ ISI:000265128600041,
Author = {Range-Rojo, R. and McCarthy, J. and Bookey, H. T. and Kar, A. K. and
   Rodriguez-Fernandez, L. and Cheang-Wong, J. C. and Crespo-Sosa, A. and
   Lopez-Suarez, A. and Oliver, A. and Rodriguez-Iglesias, V. and
   Silva-Pereyra, H. G.},
Title = {{Anisotropy in the nonlinear absorption of elongated silver nanoparticles
   in silica, probed by femtosecond pulses}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2009}},
Volume = {{282}},
Number = {{9}},
Pages = {{1909-1912}},
Month = {{MAY 1}},
Abstract = {{We present measurements of the anisotropy of the nonlinear absorption of
   a silica matrix doped with aligned spheroidal silver nanoparticles,
   produced by a double ion-implantation process of silver nanoparticles
   followed by an irradiation with Si ions. The nonlinear response was
   studied using the z-scan technique with fs pulses at 530 nm, which lies
   very close to the surface-plasmon absorption peak of the sample. The
   observed saturable absorption is studied for different angles of the
   input linear polarization of the pulses, showing a strong anisotropy,
   which is consistent with the fact that the nanorods are aligned. (C)
   2009 Elsevier B.V. All rights reserved.}},
