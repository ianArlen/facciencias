@article{ ISI:000267045400012,
Author = {Soto, Gerardo and Moreno-Armenta, Maria G.},
Title = {{The most probable structures of platinum nitride as a function of
   composition}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
Year = {{2009}},
Volume = {{246}},
Number = {{6}},
Pages = {{1221-1224}},
Month = {{JUN}},
Abstract = {{So far two methods have been successful for the production of platinum
   nitride. One is the laser ablation method, the other the
   high-temperature high-pressure method. Spectro scopic examinations of
   films produced by the first method reveal that nitrogen is in
   interstitial sites of platinum as N-1 units. In contrast, the accepted
   configuration for the material grown by the second method is that
   nitrogen is an N-2 units in a pyrite-like PtN2 structure. To elucidate
   these conflicting results we carried out density functional calculations
   to correlate composition with nitrogen sites in PtNx. We found that for
   dilute nitrogen concentrations M 0.2, nitrogen is in six-fold
   coordinated sites (octahedral interstices) as N-1 units. For 0.2 < x <
   1.5 nitrogen is in tetrahedral intersitces as N-1 units. Only for x <
   1.5 is the N-2 configuration in octahedral sites attained. (C) 2009
   WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim}},
