@article{ ISI:000267494600009,
Author = {Rosales-Zarate, L. E. C. and Jauregui, R.},
Title = {{Trapping effects on short-range two-body interactions}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2009}},
Volume = {{55}},
Number = {{3}},
Pages = {{221-225}},
Month = {{JUN}},
Abstract = {{In contrast to conventional ideal Bose and Fermi degenerate gases, atoms
   in experimental ultracold gases interact among themselves and are
   trapped by an external potential. In this work, we consider two
   particles of mass m trapped by a 3D harmonic potential of frequency w;
   and interacting through an isotropic short range potential of intensity
   V-0 and range b/2, with b/2 << root h/mw. Eigenfunctions and
   eigenenergies are obtained and compared with those resulting (a) from an
   effective contact interaction and (b) in the absence of the trapping
   potential. Concepts like zero-resonance and binding energy, usually
   introduced for problems that allow a continuum spectra, are discussed
   for trapped particles.}},
