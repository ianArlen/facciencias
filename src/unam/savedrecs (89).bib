@article{ ISI:000266168800023,
Author = {Morera, Claudia and Villanueva, Marco A.},
Title = {{Heat treatment and viability assessment by Evans blue in cultured
   Symbiodinium kawagutii cells}},
Journal = {{WORLD JOURNAL OF MICROBIOLOGY \& BIOTECHNOLOGY}},
Year = {{2009}},
Volume = {{25}},
Number = {{6}},
Pages = {{1125-1128}},
Month = {{JUN}},
Abstract = {{Evans blue was used to determine viability in Symbiodinium kawagutii
   cultures but heat treatments at up to 80A degrees C for 1 h were
   required for detecting blue-stained dead cells. About 3 h at this
   temperature was necessary to obtain similar to 50\% blue-stained dead
   cells, and more than 6 h for 100\%. In comparison, Trypan blue showed
   unusual elevated numbers of viable cells at times when more than 50\%
   cells were dead by Evans blue parameters. Respiration decreased to 17
   and 13.2\% after 1 and 2 h at 80A degrees C, respectively, and a
   marginal but still statistically significant 7.5\% was scored after 3 h.
   Thus, cultured S. kawagutii cells were more resistant to heat-induced
   death than expected, and Evans blue was reliable for assessment of their
   viability.}},
