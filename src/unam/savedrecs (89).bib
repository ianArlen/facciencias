@article{ ISI:000264292300005,
Author = {Calogero, F. and Leyvraz, F.},
Title = {{How to embed an arbitrary Hamiltonian dynamics in a superintegrable (or
   just integrable) Hamiltonian dynamics}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2009}},
Volume = {{42}},
Number = {{14}},
Month = {{APR 10}},
Abstract = {{Given an arbitrary (autonomous) Hamiltonian h((p) over right arrow; (q)
   over right arrow), where the N components p(n) of the N-vector (p) over
   right arrow are the canonical momenta, the N components q(n) of the
   N-vector (q) over right arrow are the corresponding canonical
   coordinates and N is an arbitrary positive integer, we show how to
   manufacture (autonomous) Hamiltonians (H) over tilde(<(<(p)over
   tilde>)over right arrow>, (P) over tilde; <(<(q)over tilde>)over right
   arrow>, (Q) over tilde), featuring the N+1 canonical momenta (p) over
   tilde (n), (P) over tilde and the corresponding canonical coordinates
   (q) over tilde (n) , (Q) over tilde, and having the following two
   properties: (i) the generic solutions of (H) over tilde (<(<(p)over
   tilde>)over right arrow>, (P) over tilde; <(<(q)over tilde>)over right
   arrow>, (Q) over tilde) are periodic-entailing that the dynamics yielded
   by this Hamiltonian (H) over tilde (<(<(p)over tilde>)over right arrow>,
   ; <(<(q)over tilde>)over right arrow>, (Q) over tilde) is (maximally)
   superintegrable, namely, it features 2N+1 functionally independent
   constants of motion, N+1 of which in involution. (ii) On the manifold
   characterized by the condition (H) over tilde (<(<(p)over tilde>)over
   right arrow>, ; <(<(q)over tilde>)over right arrow>, (Q) over tilde) =
   0, the coordinates (P) over tilde and (Q) over tilde evolve trivially,
   (P) over tilde (t) = (P) over tilde (0) and (Q) over tilde (t) = (Q)
   over tilde (0) = t, while the evolution of the 2N coordinates (p) over
   tilde (n)(t), (q) over tilde (n) (t) is that determined by the
   (arbitrary!) Hamiltonian h(<(<(p)over tilde>)over right arrow>;
   <(<(q)over tilde>)over right arrow>). This is related to an earlier
   finding by Bolsinov and Taimanov.}},
