@article{ ISI:000265429200029,
Author = {Zarate-Perez, Francisco and Chanez-Cardenas, Maria Elena and Arreola,
   Rodrigo and Torres-Larios, Alfredo and Vazquez-Contreras, Edgar},
Title = {{Different catalytic properties of two highly homologous triosephosphate
   isomerase monomers}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{2009}},
Volume = {{382}},
Number = {{3}},
Pages = {{626-630}},
Month = {{MAY 8}},
Abstract = {{It is assumed that amino acid sequence differences in highly homologous
   enzymes would be found at the peripheral level, subtle changes that
   would not necessarily affect catalysis. Here, we demonstrate that, using
   the same set of mutations at the level of the interface loop 3, the
   activity of a triosephosphate isomerase monomeric enzyme is ten times
   higher than that of a homologous enzyme with 74\% identity and 86\%
   similarity, whereas the activity of the native, dimeric enzymes is
   essentially the same. This is an example of how the dimeric biological
   unit evolved to compensate for the intrinsic differences found at the
   monomeric species level. Biophysical techniques of size exclusion
   chromatography, dynamic light scattering, X-ray crystallography,
   fluorescence and circular dichroism, as well as
   denaturation/renaturation assays with guanidinium hydrochloride and ANS
   binding, allowed us to fully characterize the properties of the new
   monomer. (C) 2009 Elsevier Inc. All rights reserved.}},
