@article{ ISI:000266112900005,
Author = {Lara-Nunez, Aurora and Sanchez-Nieto, Sobeida and Luisa Anaya, Ana and
   Cruz-Ortega, Rocio},
Title = {{Phytotoxic effects of Sicyos deppei (Cucurbitaceae) in germinating
   tomato seeds}},
Journal = {{PHYSIOLOGIA PLANTARUM}},
Year = {{2009}},
Volume = {{136}},
Number = {{2}},
Pages = {{180-192}},
Month = {{JUN}},
Abstract = {{The phytotoxic effect of allelochemicals is referred to as
   allelochemical stress and it is considered a biotic stress. Sicyos
   deppei G. Don (Cucurbitaceae) is an allelopathic weed that causes
   phytotoxicity in Lycopersicon esculentum, delaying seed germination and
   severely inhibiting radicle growth. This paper reports in in vitro
   conditions, the effects of the aqueous leachate of S. deppei-throughout
   tomato germination times-on (1) the dynamics of starch and sugars
   metabolism, (2) activity and expression of the cell wall enzymes
   involved in endosperm weakening that allows the protrusion of the
   radicle, and (3) whether abscisic acid (ABA) is involved in this altered
   metabolic processes. Results showed that S. deppei leachate on tomato
   seed germination mainly caused: (1) delay in starch degradation as well
   as in sucrose hydrolysis; (2) lower activity of sucrose phosphate
   synthase, cell wall invertase, and alpha-amylase; being sucrose
   phosphate synthase (SPS) gene expression down-regulated, and the last
   two up regulated; (3) also, lower activity of endo beta-mannanase,
   beta-1,3 glucanase, alpha-galactosidase, and exo-polygalacturonase with
   altered gene expression; and (4) higher content of ABA during all times
   of germination. The phytotoxic effect of S. deppei aqueous leachate is
   because of the sum of many metabolic processes affected during tomato
   seed germination that finally is evidenced by a strong inhibition of
   radicle growth.}},
