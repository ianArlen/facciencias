@article{ ISI:000267405700090,
Author = {Alva-Sanchez, H. and Martinez-Davalos, A. and Moreno-Barbosa, E. and
   Hernandez-Reyes, B. and Murrieta, T. and Ruiz-Trejo, C. and Brandan, M.
   E. and Rodriguez-Villafuerte, M.},
Title = {{Initial characterization of a benchtop microPET system based on LYSO
   crystal arrays and Hamamatsu H8500 PS-PMTs}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
