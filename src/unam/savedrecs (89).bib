@article{ ISI:000264920900012,
Author = {Dunbar, William D. and Greenwald, Sarah J. and McGowan, Jill and Searle,
   Catherine},
Title = {{Diameters of 3-sphere quotients}},
Journal = {{DIFFERENTIAL GEOMETRY AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{27}},
Number = {{2}},
Pages = {{307-319}},
Month = {{APR}},
Abstract = {{Let G subset of 0(4) act isometrically on S-3. In this article we
   calculate a lower bound for the diameter of the quotient spaces S-3/G.
   We find it to be 1/2 arccos(tan (3 pi/10)/root 3). which is exactly the
   value of the lower bound for diameters of the spherical space forms. In
   the process, we are also able to find a lower bound for diameters for
   the spherical Aleksandrov spaces, S-n/G, of cohomogeneities I and 2, as
   well as for cohomogeneity 3 (with some restrictions on the group type).
   This leads us to conjecture that the diameter of S-n/G is increasing as
   the cohomogeneity of the group G increases. (C) 2008 Elsevier B.V. All
   rights reserved.}},
