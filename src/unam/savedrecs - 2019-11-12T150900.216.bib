@article{ ISI:000075771100010,
Author = {Lewis, RR and Gongora-T, A},
Title = {{Stark-induced anapole magnetic fields in n=2 hydrogen}},
Journal = {{JOURNAL OF PHYSICS B-ATOMIC MOLECULAR AND OPTICAL PHYSICS}},
Year = {{1998}},
Volume = {{31}},
Number = {{16}},
Pages = {{3565-3574}},
Month = {{AUG 28}},
Abstract = {{This paper presents results for the anapole magnetic field moment due to
   the mixing of states with opposite parity in the n = 2 hydrogen atom in
   an external constant electric field. The evaluation of the current
   densities of the four n = 2 states is carried out to first order in the
   field strength. These current densities and their associated magnetic
   fields are expanded in multipoles, including both the ordinary poloidal
   and the toroidal fields. The dominant contribution to the toroidal
   dipole (anapole) magnetic field comes from the strong mixing of the
   degenerate 2S, 2P states within the: n = 2 shell. A brief discussion is
   also presented of the electronic scattering from the anapole held of a
   Rydberg atom. The experimental conditions under which the atomic anapole
   field could be detected are critically considered, in particular, in
   terms of the quasi-resonant dependence of the anapole field on the
   applied electric field.}},
