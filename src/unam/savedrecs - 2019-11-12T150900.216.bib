@article{ ISI:000075442300015,
Author = {Negrete, MAM},
Title = {{Eleven semesters of administering the Moreira-Axt test in thermodynamics
   to physics majors}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1998}},
Volume = {{44}},
Number = {{4}},
Pages = {{397-401}},
Month = {{AUG}},
Abstract = {{The test on thermodynamics designed by M.A. Moreira and R. Art has been
   applied during eleven undergraduated terms on that subject in the
   Physics Department of the Faculty of Sciences of the National Autonomous
   University of Mexico, as a tool of the so called meaningful learning.
   The following are some of the main results obtained, averaged over the
   eleven terms: 1) only 54\% of the scores are correct when the test is
   applied at the beginning of the term; 2) a relative increment of 40\% is
   obtained as a result of the course in thermodynamics; 3) the final test
   score and the final score by conceptual ordinary examinations of the
   course almost coincide. This fact could be due to the emphasis given in
   the classroom both to the learning of the basic concepts of
   thermodynamics as to the understanding of the ``alternative{''} or
