@article{ ISI:000077488500005,
Author = {Vazquez, L and Sierra, C and Juarez, S and Agundis, C and Zavala, A and
   Zenteno, E},
Title = {{Mechanisms of immunity in Crustacea}},
Journal = {{INTERCIENCIA}},
Year = {{1998}},
Volume = {{23}},
Number = {{6}},
Pages = {{344+}},
Month = {{NOV-DEC}},
Abstract = {{It has been shown that edible crustasceans can be infected by several
   pathogenic agents, such as viruses or bacteria. Some of these infections
   also affect humans, for this reason, knowledge of the defense mechanisms
   in invertebrates is an important tool to propose alternatives to prevent
   and treat these infectious diseases. Decapods as well as invertebrates
   possess in common innate immunity, mediated by phagocytosis or by
   humoral mechanisms driven by proteins with lytic activity or by lectins,
   which possess opzonizating activity and show specificity for
   carbohydrate residues on foreign molecules or cells. In the present work
   we analyze the main biochemical and cellular defense mechanisms
   occurring in marine decapods.}},
