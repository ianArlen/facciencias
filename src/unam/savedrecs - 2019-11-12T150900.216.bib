@article{ ISI:000075509400007,
Author = {Santos, SJ and Bohon, LM and Sanchez-Sosa, JJ},
Title = {{Childhood family relationships, marital and work conflict, and mental
   health distress in Mexican immigrants}},
Journal = {{JOURNAL OF COMMUNITY PSYCHOLOGY}},
