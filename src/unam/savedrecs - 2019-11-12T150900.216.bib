@article{ ISI:000075425300006,
Author = {Ortiz, E and Estrada, G and Lizardi, PM},
Title = {{PNA molecular beacons for rapid detection of PCR amplicons}},
Journal = {{MOLECULAR AND CELLULAR PROBES}},
Year = {{1998}},
Volume = {{12}},
Number = {{4}},
Pages = {{219-226}},
Month = {{AUG}},
Abstract = {{The authors have developed a method for rapid detection of polymerase
   chain reaction (PCR) amplicons based on surface immobilized PNA-DNA
   hybrid probes ('molecular beacons') that undergo a fluorescent-linked
   conformational change in the presence of a complementary DNA target.
