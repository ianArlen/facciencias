@article{ ISI:000075876500020,
Author = {Robledo, A and Quintana, J},
Title = {{Landau density functional theory for one-dimensional inhomogeneities}},
Journal = {{PHYSICA A}},
Year = {{1998}},
Volume = {{257}},
Number = {{1-4}},
