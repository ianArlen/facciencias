@article{ ISI:000077291000022,
Author = {Scaglia, HE and Timossi, CM and Carrere, CA and Zylbersztein, C and
   Colombani, ME and Rey-Valzacchi, G and Aquilano, DRA},
Title = {{Altered luteinizing hormone pulsatility in infertile patients with
   idiopathic oligoasthenozoospermia}},
Journal = {{HUMAN REPRODUCTION}},
Year = {{1998}},
Volume = {{13}},
Number = {{10}},
Pages = {{2782-2786}},
Month = {{OCT}},
Abstract = {{Tn a previous study, we demonstrated that oligoasthenozoo-spermic (OAZ)
   patients had two types of testosterone response to human chorionic
   gonadotrophin (HCG) administration: group 1 (OAZ-1) had an altered,
   monophasic (no first peak) response, and group 2 (OAZ-2) had a normal
   biphasic response, The objective of the present work was to study the
   luteinizing hormone (LH) pulsatility in OAZ-1 compared with both OAZ-2
   and men of proven fertility (PF), in order partly to determine the
   possible aetiology of the blunted acute testosterone response to HCG in
   these patients, LH pulsatility was measured in 10 PF, 10 OAZ-1 and 10
   OAZ-2 patients, in blood samples taken every 5 min for 6 h in PF, and
   for 4 h in OAZ patients. LH values were determined by a time-resolved
   immunofluorometric assay. Frequency and amplitude of the LW pulses were
   determined by a computer program. LN pulse frequency, expressed as
   pulses/4 h, was significantly lower in OAZ-1 (1.5 +/- 0.97) than in PF
   (2.4 +/- 0.63) and OAZ-2 (2.4 +/-: 0.84) patients. Tn six OAZ-1 and two
   OAZ-2 patients, LH pulsatility was diminished, as they showed less than
   two pulses/4 h, No statistically significant differences in LH pulse
   amplitude mere found. These results, together with a higher number of
   OAZ-1 cases found with decreased LH pulsatility, suggest that, at least
