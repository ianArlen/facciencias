@article{ ISI:000076266800010,
Author = {Villatoro, J and Garcia-Valenzuela, A},
Title = {{Measuring optical power transmission near the critical angle for sensing
   beam deflection}},
Journal = {{APPLIED OPTICS}},
Year = {{1998}},
Volume = {{37}},
Number = {{28}},
Pages = {{6648-6653}},
Month = {{OCT 1}},
Abstract = {{We investigate an internal-transmission method for measuring
   microdeflections of an optical beam as a potential tool for the
   development of new compact and stable optical sensors. We calculate the
   detection limits of the internal-transmission method when an ideal
   coherent optical source and an ideal quasi-monochromatic thermallike
   source are used. The proposed method is compared with an
   internal-reflection method previously studied. It is found theoretically
   and verified experimentally that the transmission method may have better
   resolution than the reflection method. We also compare the calculated
   sensitivity as a function of the angle of incidence with experimental
   results for both methods. (C) 1998 Optical Society of America. OCIS
   codes: 120.4640, 130.6010, 260.6970, 230.0040, 230.5480.}},
