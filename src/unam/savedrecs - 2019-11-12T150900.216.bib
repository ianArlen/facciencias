@article{ ISI:000076130500008,
Author = {Takeuchi, N},
Title = {{Stability of the c(4x8) structure in the adsorption of Pb in the
   (100)surface of elemental semiconductors}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{1998}},
Volume = {{58}},
Number = {{12}},
Pages = {{R7504-R7507}},
Month = {{SEP 15}},
Abstract = {{Using first-principles total-energy calculations, we have studied the
   atomic structure and stability of the c(4 x 8) phase of Pb on Ge(100).
   The structure consists of rows of Pb ad-dimers, with one missing row out
   of every four, yielding a coverage of 75\% of a monolayer. The dimers
   are asymmetric: there is a relative vertical separation of similar to
   0.81 Angstrom between the two atoms. Surface formation energies show
   that this configuration is more stable than the full monolayer (2 x 1)
   structure. Opposite results are found for Sb on Ge(100), where the (2 x
   1) reconstruction is preferred over the c(4 x 8). Our calculations
   provide some insights into the microscopic origin of the difference in
   stability between group V and heavy group IV elements adsorbed on the
   (100) surfaces of the elemental semiconductors Si and Ge.
   {[}S0163-1829(98)52136-4].}},
