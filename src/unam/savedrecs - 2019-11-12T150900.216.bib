@article{ ISI:000075896700014,
Author = {Vargas, C and Hirsch, JG and Hess, PO and Draayer, JP},
Title = {{Interplay between the quadrupole-quadrupole and spin-orbit interactions
   in nuclei}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{1998}},
Volume = {{58}},
Number = {{3}},
Pages = {{1488-1499}},
Month = {{SEP}},
Abstract = {{The interplay between the quadrupole-quadrupole and spin-orbit
   interactions in nuclei is analyzed in an SU(3) basis. Results for two
   ds-shell nuclei Ne-20 and Ne-22 and one fp-shell nucleus Ti-44 are
   considered for realistic values of the strength parameters of these two
   interactions. The ground state band is built from the S=0 leading
   irreducible representation (irrep), which mixes strongly with the
   leading S=1 irreps in the proton and neutron subspaces. The
   quadrupole-quadrupole interaction is found to give dominant weights to
   the ``stretched'' coupled representations, which supports a strong
   truncation of the Hilbert space.}},
