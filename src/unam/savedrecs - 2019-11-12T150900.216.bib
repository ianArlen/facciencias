@article{ ISI:000078290000003,
Author = {Cuevas, G and Renugopalakrishnan, V and Garduno-Juarez, R and Hagler, AT},
Title = {{Acetamide planarity revisited: Density functional and second order
   Moeller-Plesset perturbation studies}},
Journal = {{INDIAN JOURNAL OF CHEMISTRY SECTION B-ORGANIC CHEMISTRY INCLUDING
   MEDICINAL CHEMISTRY}},
Year = {{1998}},
Volume = {{37}},
Number = {{11}},
Pages = {{1092-1096}},
Month = {{NOV}},
Abstract = {{Density functional theory (DFT) with nonlocal functionals, BLYP, BP86,
   B3LYP and ACM, with split-valence basis sets has been applied to the
   prediction of molecular structure and torsional barrier of acetamide, a
   peptide mimic. Moeller-Plesset calculations truncated to the second
   order (MP2) have been performed on acetamide for comparison with DFT
   results. The conformation of the methyl group observed in MP2
   calculations is different from that in DFT calculations. DFT promises to
   be a powerful method for molecular structure determination and
