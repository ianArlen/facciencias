@article{ ISI:000075590700057,
Author = {Baca, E and Chacon, M and Lopera, W and Gomez, ME and Prieto, P and
   Heiras, J and Di Leo, R and Romano, P and Cucolo, AM},
Title = {{Planar-type tunnel junctions with Bi2Sr2CaCu2O8+delta high-temperature
   superconducting electrodes and Bi2Sr2YCu2O8+delta artificial barriers}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{1998}},
Volume = {{84}},
Number = {{5}},
Pages = {{2788-2794}},
Month = {{SEP 1}},
Abstract = {{We performed a detailed study of the structure and transport properties
   of Bi2212/22Y2/2212 planar-type tunnel junctions. Both high-temperature
   superconducting electrodes and semiconducting barriers are highly
   epitaxial thin films deposited onto SrTiO3 single-crystal (001)
   substrates. Deposition of the films was carried out by a high oxygen
   pressure de-sputtering technique, which produces high-quality epitaxial
   thin films, as determined by x-ray diffraction, lattice resolution
   transmission electron microscopy, and Rutherford backscattering.
   Critical temperatures for the superconducting electrodes of 85 K were
   determined by transport measurements (rho and chi versus T). A study of
   resistivity as a function of temperature of the semiconducting barriers
   was performed. Clear quasiparticle tunneling indicating a gap structure
   at about 30-35 mV, a zero-bias peak, as well as linear and flat
   background at high voltages have been observed. For junctions with very
   thin barriers weak-link-type behavior was observed. An analysis of the
   I-V curves for these junctions has been made based on the resistively
   shunted junction model. (C) 1998 American Institute of Physics.
   {[}S0021-8979(98)00516-7]}},
