@article{ ISI:000076086700031,
Author = {Takeuchi, N},
Title = {{First-principles calculations of low-coverage phases of Pb on the (100)
   surface of Ge}},
Journal = {{SURFACE SCIENCE}},
Year = {{1998}},
Volume = {{412-13}},
Pages = {{358-363}},
Month = {{SEP 3}},
Abstract = {{We have performed first-principles total energy calculations to
   determine the atomic structure of the Ge(100) surface with half a
   monolayer of Pb. As in the case of Si(100) covered with similar amounts
   of group III elements, the optimum structure consists of rows of Pb
   ad-dimers parallel to the underlying Ge dimers. In the present case, the
   Pb dimers are buckled: there is a vertical separation of similar to 0.67
   Angstrom between the two atoms. The Pb-Pb bond length is similar to 3.16
   Angstrom, and the buckling angle is similar to 12 degrees. The first Ge
   layer is formed by slightly asymmetric dimers with a bond length of
   similar to 2.53 Angstrom. (C) 1998 Published by Elsevier Science B.V.
   All rights reserved.}},
