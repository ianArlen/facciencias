@article{ ISI:000076647200003,
Author = {Calderon, F and Lopez-Colome, AM},
Title = {{Spermine inhibits {[}H-3]glycine binding at the NMDA receptors from
   plexiform layers of chick retina}},
Journal = {{NEUROCHEMICAL RESEARCH}},
Year = {{1998}},
Volume = {{23}},
Number = {{11}},
Pages = {{1363-1369}},
Month = {{NOV}},
Abstract = {{Saturable specific binding of glycine to synaptosomal membranes from
   plexiform layers of the retina has been described, which seems to
   correspond to the modulatory site on NMDA-receptors (26). Spermine
   inhibited specific {[}H-3]glycine binding to membranes from synaptosomal
   fractions from the outer (P-1) and the inner (P-2) plexiform layers of
   1-3 day-old chick retinas in a dose-dependent manner with an IC50 = 35
   mu M for the P-1 fraction and 32 mu M for the P-2 fraction. Kinetic
   experiments and non-linear regression analysis of {[}3H]glycine-specific
   binding showed a K-d similar to 100-150 nM in both fractions, and a
   higher B-max (4.11 +/- 0.47 pmol/mg protein) for the inner plexiform
   layer compared to the outer plexiform layer (B-max = 2.76 +/- 0.25
   pmol/mg protein). Strychnine-insensitive {[}H-3]glycine binding was
   inhibited by 100 mu M spermine, due to a reduction in B-max (P-1 = 0.84
   +/- 0.16 pmol/mg protein; P-2 = 0.81 +/- 0.16 pmol/mg protein) without
   affecting the K-d. Association and dissociation constants in the absence
   and presence of 50 mu M spermine remained unchanged. Results demonstrate
   the presence of a single modulatory site for spermine on NMDA receptors,
   in both synaptic layers of the chick retina.}},
