@article{ ISI:000076473800004,
Author = {Vazquez, GF and Diaz, RA and Salvador, LG and Sharma, VK},
Title = {{Dissolved metals in Alvarado Lagoon, Mexico}},
Journal = {{ENVIRONMENT INTERNATIONAL}},
Year = {{1998}},
Volume = {{24}},
Number = {{7}},
Pages = {{721-727}},
Month = {{OCT}},
Abstract = {{Concentrations of major and trace elements (temperature, salinity,
   conductivity, Na(+), K(+), Mg(2+), Ca(2+), Cl(-), Br(-), F(-),
   SO(4)(2-), pH, A(T), Cd, Cu, Pb, Fe, and Cr) were determined in the
   Alvarado Lagoon at fifteen stations in four seasons during November 1992
   - September 1993. The salinities, and concentrations of major cations
   and anions, were found to be a function of seasons. The dissolved metal
   concentrations depend on the season. Cd concentrations were higher in a
   winter season and lower in a rainy season. The concentrations of Cu and
   Pb were higher in the dry season with low values in a rainy season. Cr
   and Fe concentrations were found higher in the rainy seasons and lowest
   in a dry season. The concentrations of Cd, Cu, and Pb were found to be
   positively related to the salinity, and Fe and Cr did not change
   significantly with salinity. The results of dissolved metal
   concentrations in Alvarado Lagoon are compared with other lagoon waters
   in Mexico and are briefly discussed. (C) 1998 Elsevier Science Ltd.}},
