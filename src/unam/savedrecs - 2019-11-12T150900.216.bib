@article{ ISI:000074448500002,
Author = {Skiba, YN and Adem, J and Morales-Acoltzi, T},
Title = {{On the structure of the stability matrix in the normal mode stability
   study of zonal incompressible flows on a sphere}},
Journal = {{ATMOSFERA}},
Year = {{1998}},
Volume = {{11}},
Number = {{3}},
Pages = {{143-155}},
Month = {{JUL 1}},
Abstract = {{A structure of the matrix resulting from the normal mode stability of
   zonal non-divergent hows on a sphere is analyzed. The analysis is based
   on using the recurrent formula derived for the nonlinear triad
   interaction coefficients. As an application, it is shown that a zonal
   how of the form of a Legendre polynomial of degree j is exponentially
   and algebraically stable to all the small-seal perturbations whose zonal
   wave number is greater than j.}},
