@article{ ISI:000074611500011,
Author = {Naumis, GG and Kerner, R},
Title = {{Stochastic matrix description of glass transition in ternary
   chalcogenide systems}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{1998}},
Volume = {{231}},
Number = {{1-2}},
Pages = {{111-119}},
Month = {{JUL}},
Abstract = {{A method using stochastic transfer matrices is applied to description of
   microcluster growths in typical ternary chalcogenide glasses (Ge-As-Se
   and Ge-Sb-Se systems) during glass transition. By finding the unit
   eigenvalue and eigenvector of the stochastic matrix, an analytical
   relation between the glass transition temperature and the concentration
   of atomic species is found. In the chalcogen-rich region this formula
   reproduces the experimentally observed modified Gibbs-DiMarzio equation,
   which is a semi-empirical rule that relates the average coordination
   number with the glass transition temperature, using a parameter beta
   which is fixed by the experimental data. The present approach allows us
   to predict a beta close to the observed one. (C) 1998 Published by
   Elsevier Science B.V. All rights reserved.}},
