@article{ ISI:000076333800006,
Author = {Hernandez-Becerril, DU and Granados, CF},
Title = {{Species of the diatom genus Chaetoceros (Bacillariophyceae) in the
   plankton from the southern Gulf of Mexico}},
Journal = {{BOTANICA MARINA}},
Year = {{1998}},
Volume = {{41}},
Number = {{5}},
Pages = {{505-519}},
Month = {{SEP}},
Abstract = {{Examination of material obtained by net and taken on 11 cruises
   (1983-1989) in the southern Gulf of Mexico has shown this region to
   contain a very diverse phytoplankton community. In this paper the taxa
   of the planktonic diatom genus Chaetoceros are reported. Forty-two taxa
   (38 species and 4 varieties) were identified. They were studied by light
   and electron microscopy, and most are illustrated. Brief morphological
   details, as well as comments on the taxonomy of some species are
   provided. A few species are typically tropical, but most exhibit a wider
   distribution. Six new records for the Gulf are noted, with four species
   of those being of tropical to subtropical distribution. Taxonomic
   proposals include two new sections within the subgenus Chaetoceros:
   Rostrata and Tetrastichona, to accomodate the species C. rostratus, and
   C. dadayi and C. tetrastichon, respectively. A brief discussion is also
   provided on the morphological intergrades among three taxa of C.
   didymus, two of them are considered to be synonyms.}},
