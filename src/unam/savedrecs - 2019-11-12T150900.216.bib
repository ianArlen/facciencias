@article{ ISI:000076081100008,
Author = {Palma, V and Buentello, L and Arellano, J and Salamanca, F},
Title = {{Cell cycle kinetics in blood lymphocytes from patients with progeria and
   from normal subjects}},
Journal = {{MEDICAL SCIENCE RESEARCH}},
Year = {{1998}},
Volume = {{26}},
Number = {{8}},
Pages = {{531-534}},
Month = {{AUG}},
Abstract = {{We studied the duration of the cell cycle in peripheral blood
   lymphocytes from three children with progeria and from normal subjects
   of different ages, using the average generation time (AGT) and the
   analytical method (A-cct) recently described by our group. There were
   significant differences between the studied groups, progeria patients
   showing higher values than healthy controls matched by chronological
   ages. These facts are probably related to an impaired proliferation
   response or to a decreased sensitivity to growth factors, since we found
   less cell proliferation and less interleukin-2 (IL-2) production in the
   lymphocyte cultures of normal old individuals and children with
   progeria. Med Sci Res 26:531-534 (C) 1998 Lippincott Williams \&
   Wilkins.}},
