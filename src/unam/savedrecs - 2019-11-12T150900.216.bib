@article{ ISI:000075902500020,
Author = {Kaplan, IG},
Title = {{Many-body interactions, symmetry adapted perturbation theory and
   chemical bonding in beryllium clusters}},
Journal = {{POLISH JOURNAL OF CHEMISTRY}},
Year = {{1998}},
Volume = {{72}},
Number = {{7, S}},
Pages = {{1454-1463}},
Month = {{JUL}},
Abstract = {{The general formulae for the decomposition of many-body interaction
   energies in the double SAFT and the ab initio model potential for the
   Be-3 cluster are presented. The analysis of exchange and dispersion
   contributions to the model potential allows to elucidate the nature of
   binding in beryllium clusters. Two kind of stabilizing forces are
   responsible for binding: the additive local van der Waals forces and the
   nonadditive delocalized exchange forces.}},
