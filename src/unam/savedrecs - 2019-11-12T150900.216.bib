@article{ ISI:000075958900005,
Author = {Elizondo, EJ and Lima-Filho, P},
Title = {{Chow quotients and projective bundle formulas for Euler-Chow series}},
Journal = {{JOURNAL OF ALGEBRAIC GEOMETRY}},
Year = {{1998}},
Volume = {{7}},
Number = {{4}},
Pages = {{695-729}},
Month = {{OCT}},
Abstract = {{Given a projective algebraic variety X, let II(p) (X) denote the monoid
   of effective algebraic equivalence classes of effective algebraic cycles
   on X. The p-th Euler-Chow series of X is an element in the formal
   monoid-ring Z{[}II(p) (X)] defined in terms of Euler characteristics of
   the Chow varieties C(p), alpha (X) of X, with alpha epsilon II(p) (X).
   We provide a systematic treatment of such series, and give projective
   bundle formulas which generalize previous results by {[}LY87] and
   {[}Eli94]. The techniques used involve the Chow quotients introduced in
   {[}KSZ91], and this allows the computation of various examples including
   some Grassmannians and flag varieties. There are relations between these
   examples and representation theory, and further results point to
   interesting connections between Euler-Chow series for certain varieties
   and the topology of the moduli spaces (M) over bar(0 n+1).}},
