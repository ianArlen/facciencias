@article{ ISI:000075838800111,
Author = {Del Castillo, LF and Hernandez, SI and Garcia-Zavala, A and
   Diaz-Calleja, R},
Title = {{Effects of symmetric and antisymmetric stress tensors on N- and
   alpha-dielectric relaxations}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{1998}},
Volume = {{235}},
Pages = {{677-681}},
Month = {{AUG}},
Note = {{3rd International Discussion Meeting on Relaxations in Complex Systems,
   VIGO, SPAIN, JUN 30-JUL 11, 1997}},
Organization = {{J Non Crystalline Solids}},
Abstract = {{We present a generalized hydrodynamic model for dielectric relaxation to
   describe the effect of spatial inhomogeneities in the polarization
   fluctuations in viscoelastic fluids. The model is a generalization of
   the simple Debye relaxation equation with the addition of the gradient
   of the total stress tensor. The two known modes of relaxation, N- and
   alpha-absorption can be incorporated in the model semiquantitatively,
   since we do not know the actual parameters involved. The comparison
   between our results and the experimental response reported for the N-
   and alpha-modes in polymers, shows that the predicted intensities of the
   loss peaks are well represented. However, at lower and higher
   frequencies the predicted curves deviate from the experimental ones. (C)
   1998 Elsevier Science B.V. All rights reserved.}},
