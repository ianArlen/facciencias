@article{ ISI:000076962200043,
Author = {Banerjee, PK and Olsen, RW and Tillakaratne, NJK and Brailowsky, S and
   Tobin, AJ and Snead, OC},
Title = {{Absence seizures decrease steroid modulation of
   t-{[}S-35]Butylbicyclophosphorothionate binding in thalamic relay nuclei}},
Journal = {{JOURNAL OF PHARMACOLOGY AND EXPERIMENTAL THERAPEUTICS}},
Year = {{1998}},
Volume = {{287}},
Number = {{2}},
Pages = {{766-772}},
Month = {{NOV}},
Abstract = {{Interaction of gamma-aminobutyric acid (GABA), pentobarbital and two
   neuroactive steroids on t-butylbicyclophosphorothionate ({[}S-35]TBPS)
   binding to GABA(A) receptors in thalamus was studied during absence
   seizures. In control brain sections, the steroids alphaxalone and
   tetrahydrodeoxycorticosterone (at low 0.1-1 mu M concentrations)
   increased {[}S-35]TBPS binding in thalamic relay nuclei. Both GABA and
   pentobarbital dose-dependently decreased {[}S-35]TBPS binding in these
   nuclei. A significant decrease in the ability of steroids to increase
   {[}S-35]TBPS binding in thalamic relay nuclei was observed during
   absence seizures induced by gamma-hydroxybutyric acid (GHB). This loss
   of steroid effect on binding was 1) selective to steroids only as GABA
   and pentobarbital modulation of {[}S-35]TBPS binding in these nuclei did
   not change significantly and 2) not causally related to the generation
   of GHB-induced absence seizures as it was not observed at the onset of
   GHB-seizures but developed 30 min after the seizure-onset. We tested
   whether absence seizures were critical for the development of this loss
   of steroid effect on {[}S-35]TBPS binding in thalamic relay nuclei. The
   ability of the steroids to increase {[}S-35]TBPS binding in relay nuclei
   was preserved when GHB-seizures were blocked. When the duration of
   GHB-seizures was prolonged, the loss of steroid effect on {[}S-35]TBPS
   binding in thalamus persisted throughout the seizure-duration. These
   findings suggest that absence seizures cause a rapid loss of steroid
   effect on {[}S-35]TBPS binding to GABA(A) receptors in thalamic relay
   nuclei.}},
