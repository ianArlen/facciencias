@article{ ISI:000076887200002,
Author = {Moshinsky, M and Nikitin, AG and Sharma, A and Smirnov, YF},
Title = {{Analysis of relativistic particles of arbitrary spin through different
   chains of groups}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1998}},
Volume = {{44}},
Number = {{2}},
Pages = {{1-10}},
Month = {{OCT}},
Note = {{XXI Symposium on Nuclear Physics, OAXTEPEC, MEXICO, JAN 05-08, 1998}},
Organization = {{Univ Nacl Autonoma Mexico, Inst Fis; Univ Nacl Autonoma Mexico, Inst
   Ciencias Nucl; Inst Politecn Nacl, Ctr Invest \& Estudios Avanzados;
   Inst Nacl Invest Nucl; Soc Mexicana Fis; Consejo Nacl Ciencia \& Tecnol;
   Ctr Latinoamer Fis}},
Abstract = {{In a previous paper we analyzed the equation of particles of arbitrary
   spins characterized by the chain of groups SU(4) superset of {[}S (U)
   over cap(2) x SU(2)]. In the present paper we find that the symmetry
   group of the problem rather than SU(4) is its smaller subgroup Sp(4)
   which is a unitary symplectic one. As Sp(4) is isomorphic to O(5) we can
   replace it by the latter and write our equation in terms of the
   generators of O(5) superset of O(4) superset of O(3) superset of O(2)
   groups. These are more convenient as there are no multiplicity indices
   and the matrix elements of the generators can be given explicitly for an
   arbitrary irrep (n(1)n(2)) of O(5). The analysis is applied
   variationally to particles in an harmonic oscillator potential
   corresponding to the irreps (1/2 1/2 ), (11), (10), (3/2 3/2) of O(5).}},
