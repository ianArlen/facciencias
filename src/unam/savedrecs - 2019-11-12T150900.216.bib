@article{ ISI:000075082500008,
Author = {Toledo-Cuevas, M and Barquera, B and Gennis, RB and Wikstrom, M and
   Garcia-Horsman, JA},
Title = {{The cbb(3)-type cytochrome c oxidase from Rhodobacter sphaeroides, a
   proton-pumping heme-copper oxidase}},
Journal = {{BIOCHIMICA ET BIOPHYSICA ACTA-BIOENERGETICS}},
Year = {{1998}},
Volume = {{1365}},
Number = {{3}},
Pages = {{421-434}},
Month = {{JUL 20}},
Abstract = {{Rhodobacter sphaeroides expresses a bb(3)-type quinol oxidase, and two
   cytochrome c oxidases, cytochrome aa(3) and cytochrome cbb(3). We report
   here the characterization of the genes encoding this latter oxidase. The
   ccoNOQP gene cluster of R. sphaeroides contains four open reading frames
   with high similarity to all ccoNOQP/fixNOQP gene clusters reported so
   far. CcoN has the six highly conserved histidines proposed to be
   involved in binding the low spin heme, and the binuclear center metals.
   ccoO and ccoP code for membrane bound mono- and diheme cytochromes c.
   ccoQ codes for a small hydrophobic protein of unknown function. Upstream
   from the cluster there is a conserved Fnr/FixK-like box which may
   regulate its expression. Analysis of a R. sphaeroides mutant in which
   the ccoNOQP gene cluster was inactivated confirms that this cluster
   encodes the cbb(3)-type oxidase previously purified. Analysis of proton
   translocation in several strains shows that cytochrome cbb(3) is a
   proton pump. We also conclude that cytochromes cbb(3) and aa(3) are the
   only cytochrome c oxidases in the respiratory chain of R sphaeroides.
   (C) 1998 Elsevier Science B.V. All rights reserved.}},
