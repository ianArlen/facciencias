@article{ ISI:000074854200006,
Author = {Ferro, AA and Arellano, ER and Gonzalez-Bedolla, S and Rosenzweig, P},
Title = {{uvby beta photometric data and Fourier coefficients for galactic
   population I and population II cepheids}},
Journal = {{ASTROPHYSICAL JOURNAL SUPPLEMENT SERIES}},
Year = {{1998}},
Volume = {{117}},
Number = {{1}},
Pages = {{167-208}},
Month = {{JUL}},
Abstract = {{Photometric data in the u upsilon by beta system are presented for a
   sample of 98 Population I Cepheids and seven W Virginis or Population II
   Cepheids. The importance of the Fourier decomposition technique in the
   study of the structure of pulsating stars is stressed. Mean values and
   Fourier decomposition coefficients for the V, b - y, m(1), and c,
   variations are calculated. Also, mean values of HE are provided. New
   times of maximum I/ light are reported for the majority of the stars in
   the sample. Significant shifts of the light and color curves were found
   in some Cepheids; these are explained by their period variations. These
   stars are highlighted in the text.}},
