@article{ ISI:000247666000295,
Author = {Perez, L. A. and Millan, J. S. and Dominguez, B. C. and Wang, C.},
Title = {{Electronic specific heat of anisotropic superconductors and its doping
   dependence}},
Journal = {{JOURNAL OF MAGNETISM AND MAGNETIC MATERIALS}},
Year = {{2007}},
Volume = {{310}},
Number = {{2, 1}},
Pages = {{E129-E131}},
Month = {{MAR}},
Note = {{17th International Conference on Magnetism (ICM 2006), Kyoto, JAPAN, AUG
   20-25, 2006}},
Organization = {{Int Union Pure \& Appl Phys}},
Abstract = {{The specific heat of electrons in p- and d-symmetry superconducting
   states (CS) is comparatively studied within the BCS framework. This
   study is performed on a square lattice described by a generalized
   Hubbard model, in which correlated-hopping interactions are included in
   addition to the repulsive Coulomb ones. The temperature dependences of
   C-S show a power law behavior, instead of the exponential behavior found
   in the s channel, and for a given gap amplitude the number of nodes in
   the gap enhances CS in the low temperature regime. Moreover, the
   normalized discontinuity of C-S at the critical temperature (T-c) is
   smaller than the BCS analytical result of 1.43 for a wide range of
   electron densities. Finally, the results show that the single-particle
   van Hove singularity determines the location of the maximum gap, and
   then the position of the maximum discontinuity of the specific heat at
   Tc. (c) 2006 Elsevier B.V. All rights reserved.}},
