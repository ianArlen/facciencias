@article{ ISI:000246351000065,
Author = {Lopez Vieyra, Juan C. and Turbiner, Alexander V. and Guevara, Nicolais
   L.},
Title = {{The ion H-3(+) in a strong magnetic field - Linear configuration}},
Journal = {{ASTROPHYSICS AND SPACE SCIENCE}},
Year = {{2007}},
Volume = {{308}},
Number = {{1-4}},
Pages = {{499-503}},
Month = {{APR}},
Abstract = {{A first detailed study of the low- lying electronic states of the H-3(+)
   molecular ion in linear configuration, parallel to a magnetic field, is
   carried out for B = 0-4.414 x 10(13) G in the Born-Oppenheimer
   approximation. The variational method is employed with a single,
   physically adequate trial function which includes, in particular,
   explicitly the electronic correlation term in the form exp (gamma
   r(12)), where gamma is a variational parameter. The state of the lowest
   total energy (ground state) depends on the magnetic field strength. It
   evolves from spin- singlet (1)Sigma(g) for small magnetic fields B empty
   set 5 x 10(8) G to weakly-bound spin- triplet (3)Sigma(u) for
   intermediate fields and eventually to spin-triplet (3)Pi(u) state for B
   empty set 5 x 10(10) G.}},
