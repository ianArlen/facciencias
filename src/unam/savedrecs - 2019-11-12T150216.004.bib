@article{ ISI:000246568900006,
Author = {Alvarez-Madrigal, M. and Perez-Peraza, J.},
Title = {{Differences in the monthly evolution of the Antarctic ozone hole size}},
Journal = {{ATMOSFERA}},
Year = {{2007}},
Volume = {{20}},
Number = {{2}},
Pages = {{215-221}},
Month = {{APR 1}},
Abstract = {{We study here the evolution of the ozone hole size (OHS) with the aim of
   contributing to understand the long term evolution and the origin of the
   discrepancies between modeled and observed size. Using the September,
   October and November monthly average data of the OHS, for the years
   1982-2003, we separate the series into two components: a linear and a
   residual one. The residual OHS components for October-November, shows a
   tendency to decrease after 1997, whereas the residual component of
   September does not show a clear decrease. The OHS during September
   evolve different, in relation to the other months. Therefore, the
   earlier results for the OHS analysis obtained with an annual approach
   may be refined.}},
