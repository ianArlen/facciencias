@article{ ISI:000245337100007,
Author = {Zhang, Xue-Guang and Dultzin-Hacyan, Deborah and Wang, Ting-Gui},
Title = {{The properties of optical FeII emission lines of AGN with double-peaked
   broad emission lines}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2007}},
Volume = {{43}},
Number = {{1}},
Pages = {{101-121}},
Month = {{APR}},
Abstract = {{We study the Fell properties of double-peaked broad low-ionization
   emission line AGN (dbp emitters) using a sample of 27 dbp emitters from
   SDSS (DR4). Our first result is that the line spectra in the wavelength
   range from 4100 angstrom to 5800 angstrom can be best fitted by an
   elliptical accretion disk model. The best fitted results indicate that
   the optical Fell emission lines of dbp emitters originate from the same
   region in the accretion disk where the double-peaked Balmer emission
   lines originate. Some correlations between Fell emission lines and the
   other broad emission lines for normal AGN can be confirmed for dbp
   emitters. However,. these results should be taken with caution due to
   the small number of objects and the bias in selecting strong Fell
   emitters. We show that for dbp emitters, BH masses seems to have more
   influence on Fell properties than dimensionless accretion rate.}},
