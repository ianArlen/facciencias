@article{ ISI:000245337100015,
Author = {Huerta, E. A. and Mendoza, S.},
Title = {{A simple accretion model of a rotating gas sphere onto a Schwarzschild
   black hole}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2007}},
Volume = {{43}},
Number = {{1}},
Pages = {{191-202}},
Month = {{APR}},
Abstract = {{We construct a simple accretion model of a rotating gas sphere onto a
   Schwarzschild black hole. We show how to build analytic solutions in
   terms of Jacobi elliptic functions. This construction represents a
   general relativistic generalisation of the Newtonian accretion model
   first proposed by Ulrich (1976). In exactly the same form as it occurs
   for the Newtonian case, the flow naturally predicts the existence of an
   equatorial rotating accretion disc about the hole. However, the radius
   of the disc increases monotonically without limit as the flow reaches
   its minimum allowed angular momentum for this particular model.}},
