@article{ ISI:000245406000004,
Author = {Sanchez-Salcedo, F. J. and Lora, V.},
Title = {{Bounds on the mass and abundance of dark compact objects and black holes
   in dwarf spheroidal galaxy halos}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2007}},
Volume = {{658}},
Number = {{2, 2}},
Pages = {{L83-L86}},
Month = {{APR 1}},
Abstract = {{We establish new dynamical constraints on the mass and abundance of
   compact objects in the halo of dwarf spheroidal (dSph) galaxies. In
   order to preserve kinematically cold the second peak of the Ursa Minor
   (UMi) dSph against gravitational scattering, we place upper limits on
   the density of compact objects as a function of their assumed mass. The
   mass of the dark matter constituents cannot be larger than 10(3) M
   circle dot at a halo density in UMi's core of 0.35 M circle dot pc(-3).
   This constraint rules out a scenario in which dark halo cores are formed
   by two-body relaxation processes. Our bounds on the fraction of dark
   matter in compact objects with masses greater than or similar to 3 x
   10(3) M circle dot improve those based on dynamical arguments in the
   Galactic halo. In particular, objects with masses similar to 10(5) M
   circle dot can comprise no more than a halo mass fraction similar to
   0.01. Better determinations of the velocity dispersion of old overdense
   regions in dSph galaxies may result in more stringent constraints on the
   mass of halo objects. For illustration, if the preliminary value of 0.5
   km s(-1) for the secondary peak of UMi is confirmed, compact objects
   with masses above similar to 100 M circle dot could be excluded from
   comprising all its dark matter halo.}},
