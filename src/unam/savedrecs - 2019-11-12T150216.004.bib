@article{ ISI:000244797200004,
Author = {Medina-Valtierra, Jorge and Frausto-Reyes, Claudio and Calixto, Sergio
   and Bosch, Pedro and Lara, Victor Hugo},
Title = {{The influence of surfactants on the roughness of titania sol-gel films}},
