@article{ ISI:000245047900012,
Author = {Franco, Alfredo and Valverde-Aguilar, Guadalupe and Garcia-Macedo, Jorge},
Title = {{Orientational dynamics of DR1 molecules in sol-gel films}},
Journal = {{OPTICAL MATERIALS}},
Year = {{2007}},
Volume = {{29}},
Number = {{7}},
Pages = {{814-820}},
Month = {{MAR}},
Abstract = {{A physical model for the induced orientation of azochromophores in
   sol-gel films is presented. This model describes the poling time
   dependence of the order parameter Phi based on a damped oscillator. This
   model provides a different approach to the chromophore orientational
   dynamics, with the property that all its parameters are physical
   meaningful. The model results have been experimentally verified in DRI
   doped sol-gel films, by means of optical absorption measurements. We.
   considered two kinds of sol-gel films: amorphous with a side-chain
   doping and mesostructured ones with a guest-host doping. The
   experimental results were fitted successfully with our model. (c) 2006
   Elsevier B.V. All rights reserved.}},
