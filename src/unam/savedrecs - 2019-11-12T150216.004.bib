@article{ ISI:000244774700011,
Author = {Martinez-Becerra, Francisco and Silva, Daniel-Adriano and
   Dominguez-Ramirez, Lenin and Mendoza-Hernandez, Guillermo and
   Lopez-Vidal, Yolanda and Soldevila, Gloria and Garcia-Zepeda, Eduardo A.},
Title = {{Analysis of the antimicrobial activities of a chemokine-derived peptide
   (CDAP-4) on Pseudomonas aeruginosa}},
