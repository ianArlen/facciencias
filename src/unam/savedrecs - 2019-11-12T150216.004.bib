@article{ ISI:000245795800051,
Author = {Alvarez, G. and Montiel, H. and de Cos, D. and Zamorano, R. and
   Garcia-Arribas, A. and Barandiaran, J. M. and Valenzuela, R.},
Title = {{Experimental and theoretical correlation between low-field power
   absorption and magnetoimpedance in amorphous materials}},
