@article{ ISI:000245866100014,
Author = {Chippaux, Jean-Philippe and Ramos-Cerrillo, Blanca and Stock, Roberto P.},
Title = {{Study of the efficacy of the black stone on envenomation by snake bite
   in the murine model}},
Journal = {{TOXICON}},
Year = {{2007}},
Volume = {{49}},
Number = {{5}},
Pages = {{717-720}},
Month = {{APR}},
Abstract = {{The black stone (BS) has been used since antiquity to treat
   envenomations. Since no actual clinical trial has ever been performed we
   used an experimental approach to evaluate its efficacy against the
   venoms of Bitis arietans, Echis ocellatus and Naja nigricollis. Local
   application of BS after intramuscular venom injection had no
   demonstrable effect on the outcome of envenomationa and it did not
   change the LD50 of B. arietans venom. Our results show that, contrary to
   widespread belief, no efficacy to treat envenomation may be expected of
   the BS. (c) 2006 Elsevier Ltd. All rights reserved.}},
