@article{ ISI:000244210400002,
Author = {Castaneda, L. and Maldonado, A. and Cheang-Wong, J. C. and Terrones, M.
   and Olvera, M. de la L.},
Title = {{Composition and morphological characteristics of chemically sprayed
   fluorine-doped zinc oxide thin films deposited on Si(100)}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
Year = {{2007}},
Volume = {{390}},
Number = {{1-2}},
Pages = {{10-16}},
Month = {{MAR 1}},
Abstract = {{Fluorine-doped zinc oxide thin films (ZnO:F) were deposited on Si(100)
   substrates by the chemical spray technique (CST) from an aged-solution.
   The effect of the substrate temperature on the morphology and
   composition of the ZnO:F thin films was studied. The films were
   polycrystalline, with a preferential growth along the ZnO (002) plane,
   irrespective of the deposition temperature. The average crystal size
   within the films was ca. 35 nm and the morphology of the surface was
   found to be dependent on the substrate temperature. At low substrate
   temperatures irregular-shaped grains were observed, whereas at higher
   temperatures uniform flat grains were obtained. Elemental analysis
   showed that the composition of the films is close to stoichiometric ZnO
   and that samples contain quite a low fluorine concentration, which
   decreases as a function of the deposition temperature. (c) 2006 Elsevier
   B.V. All rights reserved.}},
