@article{ ISI:000243982000071,
Author = {Jasso-Gastinel, Carlos F. and Lopez-Ureta, Luz C. and Gonzalez-Ortiz,
   Luis J. and Reyes-Gonzalez, Ignacio and Lopez-Dellamary T, Fernando A.
   and Manero-Brito, Octavio},
Title = {{Synthesis and characterization of styrene-butyl acrylate polymers,
   varying feed composition in a semicontinuous emulsion process}},
Journal = {{JOURNAL OF APPLIED POLYMER SCIENCE}},
Year = {{2007}},
Volume = {{103}},
Number = {{6}},
Pages = {{3964-3971}},
Month = {{MAR 15}},
Abstract = {{Two-stage polymerization has been used to improve properties of
   two-component polymers. In this work, a semicontinuous emulsion process
   varying feed composition along the reaction is used to produce copolymer
   chains of different composition as conversion proceeds. Polymer
   composition and evidence of branching were determined by H-1-NMR.
   Polymer molecular weight values determined by GPC are beyond the range
   where mechanical properties depend on molecular weight. Mechanodynamic
   properties show a copolymer type behavior with a synergistic effect
   around the 50/50 styrene/butyl acrylate composition ratio. For such
   composition, mechanical performance superiority of variable composition
   copolymer with respect to two-stage polymer was confirmed with
   stress-strain tests carried out at several temperatures. (c) 2006 Wiley
   Periodicals, Inc.}},
