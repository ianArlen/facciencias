@article{ ISI:000246059600019,
Author = {Sanchez-Mendoza, Maria Elena and Torres, Gabriela and Arrieta, Jesus and
   Aguilar, Abigail and Castillo-Henkel, Carlos and Navarrete, Andres},
Title = {{Mechanisms of relaxant action of a crude hexane extract of Gnaphalium
   liebmannii in guinea pig tracheal smooth muscle}},
Journal = {{JOURNAL OF ETHNOPHARMACOLOGY}},
Year = {{2007}},
Volume = {{111}},
Number = {{1}},
Pages = {{142-147}},
Month = {{APR 20}},
Abstract = {{We investigated the mechanisms of action of Gnaphalium liebmannii which
   is used as a folk medicine in Mexico for treating various respiratory
   diseases such as gripe, fever, asthma, cough, cold, bronchitis,
   expectorating, and bronchial affections. The tension changes of guinea
   pig tracheal segments were isometrically recorder on a polygraph. Hexane
   extract of Gnaphalium liebmannii was the most active relaxant extract
   (IC30 = 54.23 +/- 19.79 mu g/mL with 99.5 +/- 3.2 \% of relaxation),
   followed by dichloromethane extract (IC30 = 120.22 +/- 5.27 mu g/mL) and
   methanol extract (IC30 = 190.25 +/- 30.02 mu g/mL). Hexane extract
   produced a parallel rightward shift of the concentration-response curve
   of carbachol in a competitive manner (pA(2) = -2.4), but did not modify
   the concentration-response curves for histamine. The relaxant effect of
   hexane extract of Gnaphalium liebmannii was unaffected by the presence
   of propranolol (3 x 10(-6) M) or glibenclamide (10 mu M). However hexane
   extract produced a leftward shifts of the concentration-response curve
   of forskolin (10(-8) to 10(-3) M), nitroprusside (10-(10) to 10(-6) M),
   isoproterenol (3 x 10(-10) to 3 x 10(-5) M) and aminophylline (10(-11)
   to 10(-2) M). The above results suggest that Gnaphalium liebmannii
   induce relaxation of the tracheal muscle, probably via phosphodiesterase
   inhibition. The bronchodilator effect of Gnaphalium liebmannii might
   explain in part their traditional use as anti-asthmatic remedy. (c) 2006
   Elsevier Ireland Ltd. All rights reserved.}},
