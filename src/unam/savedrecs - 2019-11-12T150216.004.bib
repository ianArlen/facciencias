@article{ ISI:000246351000073,
Author = {Zharikov, S. and Mennickent, R. E. and Shibanov, Yu. and Komarova, V.},
Title = {{Optical spectroscopy of the radio pulsar PSR B0656+14}},
Journal = {{ASTROPHYSICS AND SPACE SCIENCE}},
Year = {{2007}},
Volume = {{308}},
Number = {{1-4}},
Pages = {{545-549}},
Month = {{APR}},
Abstract = {{We have obtained the spectrum of a middle-aged PSR B0656+14 in the
   4300-9000 angstrom range with the ESO/VLT/FORS2. Preliminary results
   show that at 4600-7000 angstrom the spectrum is almost featureless and
   flat with a spectral index alpha(v) similar or equal to -0.2 that
   undergoes a change to a positive value at longer wavelengths. Combining
   with available multiwavelength data suggests two wide, red and blue,
   flux depressions whose frequency ratio is about 2 and which could be the
   1st and 2nd harmonics of electron/positron cyclotron absorption formed
   at magnetic fields similar to 10(8) G in upper magnetosphere of the
   pulsar.}},
