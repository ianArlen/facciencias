@article{ ISI:000245155800029,
Author = {Velasco, V. R. and Mora-Ramos, M. E.},
Title = {{Electronic structure of (001) AlN/GaN quantum wells by means of a
   sp(3)s{*}d(5) empirical tight-binding Hamiltonian}},
Journal = {{SURFACE SCIENCE}},
