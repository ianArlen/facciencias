@article{ ISI:000072984600013,
Author = {Galeana-Sanchez, H and Li, XL},
Title = {{Semikernels and (k,l)-kernels in digraphs}},
Journal = {{SIAM JOURNAL ON DISCRETE MATHEMATICS}},
Year = {{1998}},
Volume = {{11}},
Number = {{2}},
Pages = {{340-346}},
Month = {{MAY}},
Abstract = {{Let D be a digraph with minimum indegree at least one. The following
   results are proved: a digraph D has a semikernel if and only if its line
   digraph L(D) does; the number of (k, 1)-kernels in L(D) is less than or
   equal to that in D; if the number of (k,l)-kernels in D is less than or
   equal to the number of (2, l)-kernels in L(D), and if L(D) has a (k,
   l)-kernel, then D has a (k', l')-kernel for k' + l less than or equal to
   k, l less than or equal to l'. As a consequence, it obtains previous
   results about kernels and quasikernels in the line digraph.
   It is also proved that any diagraph has a (k, l)-kernel with l greater
   than or equal to 2k - 2, k greater than or equal to 1, generalizing a
   previous result on the existence of quasikernels in digraphs.}},
