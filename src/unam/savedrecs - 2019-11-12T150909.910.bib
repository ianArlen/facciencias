@article{ ISI:000075370900011,
Author = {Maldonado, G and Gorocica, P and Agundis, C and Perez, A and Molina, J
   and Zenteno, E},
Title = {{Inhibition of phagocytic activity by the
   N-acetyl-D-galactosamine-specific lectin from Amaranthus leucocarpus}},
Journal = {{GLYCOCONJUGATE JOURNAL}},
Year = {{1998}},
Volume = {{15}},
Number = {{6}},
Pages = {{615-622}},
Month = {{JUN}},
Abstract = {{Amaranthus leucocarpus lectin (ALL), specific for
   N-acetyl-D-galactosamine, induces inhibition of the erythrophagocytic
   activity of resident murine peritoneal macrophages and of the
   macrophage-like cell line J-774. This effect was observed only in
   macrophages that were Mac-2 (CD11c/CD18 or CR4) negative, indicating
   that macrophage activation induces important modification to the
   glycosylation (mainly O-glycosylation) of the membrane. Receptors for
   IgM and C3b remain unaltered after lectin treatment. Ultrastructural
   analysis revealed (a) that ALL induced the formation of pinocytic
   vacuoles, and (b) a regular distribution over the macrophage membrane as
   well as endosomal vesicles of the gold labeled ALL. Our results suggest
   that macrophage membrane glycoproteins with constitutive
   N-acetyl-D-galactosamine residues participate in the regulation of
   pinocytic-phagocytic vacuole formation.}},
