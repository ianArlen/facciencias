@article{ ISI:000074150500003,
Author = {Xiao, MF},
Title = {{Numerical study on near field imaging of fluorescence decay of
   near-surface molecule cluster}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY A}},
Year = {{1998}},
Volume = {{16}},
Number = {{3, 2}},
