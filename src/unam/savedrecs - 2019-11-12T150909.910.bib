Author = {Nair, PK and Nair, MTS and Garcia, VM and Arenas, OL and Pena, Y and
   Castillo, A and Ayala, IT and Gomezdaza, O and Sanchez, A and Campos, J
   and Hu, H and Suarez, R and Rincon, ME},
Title = {{Semiconductor thin films by chemical bath deposition for solar energy
   related applications}},
