@article{ ISI:000075548700032,
Author = {Gentile, JM and Gentile, GJ and Nannenga, B and Johnson, M and
   Blankespoor, H and Montero, R},
Title = {{Enhanced liver cell mutations in trematode-infected Big Blue (R)
   transgenic mice}},
Journal = {{MUTATION RESEARCH-FUNDAMENTAL AND MOLECULAR MECHANISMS OF MUTAGENESIS}},
Year = {{1998}},
Volume = {{400}},
Number = {{1-2, SI}},
Pages = {{355-360}},
Month = {{MAY 25}},
Abstract = {{Parasite infections in humans have long been associated with specific
   types of cancers. Schistosoma hematobium is a known inducer of urinary
   bladder cancer, Helicobacter pylori is a gastric carcinogen, and
   hepatitis B virus and Opisthorchis viverrini are causative agents of
   liver cell cancers. Another liver fluke, Fasciola hepatica, has also
   been identified as a neoplastic risk agent, primarily in animals. We
   used F. hepatica as a model agent to determine if the presence of an
   aggressive liver fluke could induce mutagenic events in mammalian
   tissue. Using the Big Blue(R) transgenic mouse assay, we found a
   two-fold increase in lad mutations in cells harvested from mice
   harboring F. hepatica worms when compared to uninfected control animals.
   These data indicate that biological infections can cause increased
   genetic damage in surrounding host tissue. (C) 1998 Elsevier Science
   B.V. All rights reserved.}},
