@article{ ISI:000073262800013,
Author = {Martinez, A and Ramirez, OT and Valle, F},
Title = {{Effect of growth rate on the production of beta-galactosidase from
   Escherichia coli in Bacillus subtilis using glucose-limited
   exponentially fedbatch cultures}},
Journal = {{ENZYME AND MICROBIAL TECHNOLOGY}},
Year = {{1998}},
Volume = {{22}},
Number = {{6}},
Pages = {{520-526}},
Month = {{MAY 1}},
Abstract = {{The effect of specific growth rate (mu) oil the production of
   beta-galactosidase from Escherichia coli in Bacillus subtilis was
   assessed using glucose-limited exponentially feedback cultures (EFBC). A
   B subtilis strain carrying a chromosomal copy of the lacZ gene under the
   control of the subtilisin (aprE) regulatory region was used in this
   study. This strain also carries the hpr2 and degU32(Hy) mutations which
   are known to overexpress the subtilisin (aprE) gene. Constant mu in the
   range of 0.058-0.35 h(-1) were maintained using maintained media and
   computer-controlled predetermined glucose feeding profiles. At all
   growth rates tested, a pseudo-steady state in mu was obtained; however,
   a sudden decrease in the slope of log of biomass concentration against
   time and accumulation of glucose and acetic acid indicated that control
   on mu was lost at all growth rates tested Cultures maintained at a mu
   value close to the maximum obtained in batch cultures had a specific
   enzymatic activity relatively constant during the fedbatch phase. In
   contrast, cultures maintained at a mu value lower than 0.2 h(-1) had
   specific and volumetric beta-galactosidase activities that decreased
   during the fedbatch phase, reaching a value close to zero before glucose
   feeding was stopped. The maximum beta-galactosidase activity; after the
   EFBC phase was obtained at the maximum mu tested whereas it strongly
   decreased at lower mu values. This suggests that glucose limitation and
   mu reaction during exponential growth phase represses beta-galactosidase
   production during the exponential and stationary phases. (C) 1998
   Elsevier Science Inc.}},
