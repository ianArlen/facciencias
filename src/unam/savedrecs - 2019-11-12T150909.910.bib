@article{ ISI:000074817800010,
Author = {Ramirez-Nino, J and Mendoza, D and Castano, VM},
Title = {{A comparative study on the effect of gamma and UV irradiation on the
   optical properties of chlorophyll and carotene}},
Journal = {{RADIATION MEASUREMENTS}},
Year = {{1998}},
Volume = {{29}},
Number = {{2}},
Pages = {{195-202}},
Month = {{APR}},
Abstract = {{The optical characterization of chlorophyll and carotene, both dissolved
   in acetone-ethanol, embedded into an amorphous silicon oxide gel matrix
   (prepared by the Sor-gel method) and irradiated with UV and gamma
   radiation, is presented. First, the basic physical-hemical processes
   involved in the degradation of these organic compounds are reviewed. The
   extraction procedure of these natural dyes, as well as the gel
   preparation, are described. Then the irradiation methodology and optical
   characterization are depicted. A simple electronic device was
   specifically designed to be used as a UV or gamma radiation dosimeter
   based on absorption changes of both chlorophyll or carotene. (C) 1998
   Elsevier Science Ltd. All rights reserved.}},
