@article{ ISI:000074548000077,
Author = {Negron-Mendoza, A and Ramos-Bernal, S},
Title = {{Radiolysis of carboxylic acids adsorbed in clay minerals}},
Journal = {{RADIATION PHYSICS AND CHEMISTRY}},
Year = {{1998}},
Volume = {{52}},
Number = {{1-6}},
Pages = {{395-399}},
Month = {{JUN}},
Note = {{10th International Meeting on Radiation Processing (IMRP-10), ANAHEIM,
   CA, MAY 11-16, 1997}},
Abstract = {{This research is aimed at studing the effect of ionizing radiation in an
   heterogeneous system formed by a carboxylic acid adsorbed in a clay
   mineral. The study is focussed to discriminate if the presence of a
   solid surface alters the formation and distribution of radiolytic
   products in relation to the radiolysis of the carboxylic acid without
   the surface (clay). The results showed that the radiolysis of the system
   clay-acid goes along a defined path rather than showing various pathways
   of decomposition as in the case of simple aqueous solutions The main
   pathway was the decarboxylation of the target compound rather than
   condensation/dimerization reactions.}},
