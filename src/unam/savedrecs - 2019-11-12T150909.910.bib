@article{ ISI:000073234900015,
Author = {Julin, KR and Segura, G and Hinckley, TM and Shaw, CG and Farr, WA},
Title = {{The fluted western hemlock of southeast Alaska III. Six growing seasons
   after treatment}},
Journal = {{FOREST ECOLOGY AND MANAGEMENT}},
Year = {{1998}},
Volume = {{103}},
Number = {{2-3}},
Pages = {{277-285}},
Month = {{APR 20}},
Abstract = {{The cause of stem fluting on young, rapidly growing western hemlock
   (Tsuga heterophylla (Raf.) Sarg.) was evaluated six growing seasons
   after manipulative treatments. Branch girdling, stem drilling and
   plugging, and root pruning were used to examine relations between
   transport of substances in vascular tissue and stem fluting. Treatments
   appeared to have affected stem growth by altering the distribution of
   growth substances. Over time these effects disappeared as the original
   treatments were compensated for (e.g., root pruning), diminished in
   impact (e.g., branch girdling), or were completely overcome (e.g., stem
   drilling and plugging). No differences in the expression of fluting
   resulting from the manipulative treatments were noted in a comparison of
   a coastal site and an inland site. Stem guying demonstrated a relation
   between mechanical bending of the stem induced by wind and flute
   formation. (C) 1998 Elsevier Science B.V.}},
