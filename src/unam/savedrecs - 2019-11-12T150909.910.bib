Author = {Batina, N and Huerta, A and Pizio, O and Sokolowski, S and Trokhymchuk,
   A},
Title = {{A primitive model for hexamethylpararosaniline (crystal violet)
   monomolecular adlayer: a Monte Carlo simulation study}},
Journal = {{JOURNAL OF ELECTROANALYTICAL CHEMISTRY}},
