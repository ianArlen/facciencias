@article{ ISI:000072753600005,
Author = {Rodriguez, ME and Santiago, SA},
Title = {{Simulated seismic load tests on two-story waffle-flat-plate structure
   rehabilitated by jacketing}},
Journal = {{ACI STRUCTURAL JOURNAL}},
Year = {{1998}},
Volume = {{95}},
Number = {{2}},
Pages = {{129-141}},
Month = {{MAR-APR}},
Abstract = {{A retrofitted waffle-flat-plate RC structure was tested subjected to
   cyclic loading simulating a severe earthquake. The specimen was
   constructed at one-third scale and retrofitted with RC jacketing of
   columns and injection of epoxy resin in the original columns and waffle
   slabs. At the end of testing, the specimen showed severe flexural damage
   in both columns and waffle slabs. It also showed high flexibility and
   low measured displacement ductility capacity, which were nor much
   different to those measured in the as-built structure. Recommendations
   are given for evaluating the seismic performance of this type of
   structure.}},
