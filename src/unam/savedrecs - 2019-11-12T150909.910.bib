@article{ ISI:000073328000019,
Author = {Martinez, H},
Title = {{Single-electron loss and capture for 1.0-5.0 keV Ar+ colliding with He}},
Journal = {{JOURNAL OF PHYSICS B-ATOMIC MOLECULAR AND OPTICAL PHYSICS}},
Year = {{1998}},
Volume = {{31}},
Number = {{7}},
Pages = {{1553-1562}},
Month = {{APR 14}},
Abstract = {{Absolute differential and total cross sections for single-electron loss
   and capture were measured from Arf ions on He in the energy range of
   1.0-5.0 keV. The reduced differential cross sections (rho = (d sigma/d
   Omega)theta sin theta versus E(lab)theta = tau) are shown to scale
   reasonably well for each of the two processes studied. For
   single-electron loss, our cross sections are found to be of the order of
   magnitude of 10(-19) cm(2). An extrapolation of the high-energy results
   to low energies shows good agreement with the present data. The total
   cross section for single-electron capture is compared with other
   available measurements. These results give a general shape of the whole
   curve of single-electron capture cross sections for the Ar+-He system
   over a wide range of energy.}},
