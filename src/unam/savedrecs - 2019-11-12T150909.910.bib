@article{ ISI:000071884000006,
Author = {Arellano, RO and Garay, E and Miledi, R},
Title = {{Cl- currents activated via purinergic receptors in Xenopus follicles}},
Journal = {{AMERICAN JOURNAL OF PHYSIOLOGY-CELL PHYSIOLOGY}},
