@article{ ISI:000073691700001,
Author = {Trager, SC and Worthey, G and Faber, SM and Burstein, D and Gonzalez, JJ},
Title = {{Old stellar populations. VI. Absorption-line spectra of galaxy nuclei
   and globular clusters}},
