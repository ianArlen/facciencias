@article{ ISI:000077134200006,
Author = {Tlapanco, PM and Sanudo, MGM and Cardenas, SJ},
Title = {{Effect of a behavioral-cognitive treatment of symptoms and subjective
   well-being of patients with vertigo problems}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1998}},
Volume = {{15}},
Number = {{1}},
Pages = {{49-57}},
Month = {{JUN}},
Abstract = {{In order to asses the effect of a group of behavioral cognitive
   techniques on vertigo symptoms, in this exploratory study, a sample of
   14 subjects, previously diagnosed as vertigo patients by the National
   Institute of Human Comunication's otoneurologists, were randomly
   assigned to two groups; experimental (N=7) and control (N=7). The
   experimental group underwent six two hour groupal sessions, of theory
   and practice, once a week learning breath control, autogenic relaxation,
   Rational Emotive Therapy and problem solving. The control group
   underwent several informative sessions without any action
   ``treatment{''}. For ethical reasons the control group received the
   identical treatment as the experimental group, after the sixth session.
   Wilcoxon test rendered significant diferences for the self reports of
   intensity (Chi square= 26.1, fd 5, p<.0001), duration (Chi square= 25.1,
   5 fd, p<.0001) and frequency (Chi square=18.9, 5 fd, p<.001) of vertigo
   symptoms. All the patients showed improvement according to the well
   being verbal report. Although the findings can not be generalized they
   encourage more research.}},
