@article{ ISI:000074150400078,
Author = {Soto, G and Samano, EC and Machorro, R and Cota, L},
Title = {{Growth of SiC and SiCxNy films by pulsed laser ablation of SiC in Ar and
   N-2 environments}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY A}},
Year = {{1998}},
Volume = {{16}},
Number = {{3, 1}},
