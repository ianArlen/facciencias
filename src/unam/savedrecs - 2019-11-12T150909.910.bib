@article{ ISI:000074516700005,
Author = {Vountesmeri, V and Martynyuk, A},
Title = {{Magnetoresistive thin film sensor for active RF power}},
Journal = {{SENSORS AND ACTUATORS A-PHYSICAL}},
Year = {{1998}},
Volume = {{69}},
Number = {{1}},
Pages = {{21-26}},
Month = {{JUN 30}},
Abstract = {{A new thin film sensor for active RF power based on a magnetoresistive
   effect in a ferromagnetic film has been developed. The sensitive element
   of the sensor was created by the deposition of 80Ni-20Fe alloy on a
   glass substrate. In a 50-Omega coaxial line of size 1.6 x 0.65 mm(2),
   the sensitivity 1.2 mV/W at the frequency 13.56 MHz has been achieved.
   The dynamic range is 82 dB, the upper level is 1.3 W with
   self-absorption less then 5\%. The temperature coefficient of
   sensitivity (TCS) is - 4 pm degrees C-1. The uncertainty in measurements
   of the active RF power due to complex load is no more than 1.8\% for
   reactive circuits with power factor up to cos phi = 0.23 or for
   unmatched transmission lines with VSWR up to 8.5. (C) 1998 Published by
   Elsevier Science S.A. All rights reserved.}},
