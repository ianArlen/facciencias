@article{ ISI:000074273500020,
Author = {Vazquez-Nava, RA and Cocoletzi, GH and del Castillo-Mussot, M and
   Mochan, WL},
Title = {{Electron energy-loss spectroscopy on the surface of conducting
   superlattices in the presence of plasma waves}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{1998}},
Volume = {{57}},
Number = {{23}},
Pages = {{14642-14645}},
Month = {{JUN 15}},
Abstract = {{We present calculations for the inelastic scattering of electrons by the
   surface of binary conductor-conductor periodic superlattices described
   by local and nonlocal models. The conducting layers consist of metals or
   highly doped semiconductors and we include their spatial dispersion
   through the presence of longitudinal plasmons described by a
   hydrodynamic model. These modes manifest themselves as a series of peaks
   in the electron-energy-loss spectroscopy spectrum superimposed on the
   main structure due to the excitation of two coupled surface-plasmon
   bands. These peaks depend upon the nature of the first layer and are
   sensitive to the first few layers.}},
