@article{ ISI:000072783100004,
Author = {Levine, SE and Sparke, LS},
Title = {{A model for lopsided galactic disks}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1998}},
Volume = {{496}},
Number = {{1, 2}},
Pages = {{L13-L16}},
Month = {{MAR 20}},
Abstract = {{Many disk galaxies are lopsided; their brightest inner parts are
   displaced from the center of the outer isophotes or the outer contours
   of the H I disk. This asymmetry is particularly common in small,
   low-luminosity galaxies. We argue here that long-lived lopsidedness is a
   consequence of the disk lying off-center in the potential of the
   galaxy's extended dark halo and spinning in a sense retrograde to its
   orbit about the halo center. The stellar velocity field predicted by our
   gravitational N-body simulations is clearly asymmetric.}},
