@article{ ISI:000074150500073,
Author = {Takeuchi, N},
Title = {{First principles calculations of the different structures of a monolayer
   of Sb on Si (111)}},
Journal = {{JOURNAL OF VACUUM SCIENCE \& TECHNOLOGY A-VACUUM SURFACES AND FILMS}},
