@article{ ISI:000074380400213,
Author = {Rodriguez-Fernandez, L and Lennard, WN and Massoumi, GR and Xia, H and
   Huang, LJ and Hung, Y and Chang, S},
Title = {{Sub-monolayer phosphorus coverage measurements on CrCoTa/NiP hard disks
   using PIXE}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{1998}},
Volume = {{136}},
Pages = {{1191-1195}},
Month = {{MAR}},
Note = {{13th International Conference on Ion Beam Analysis (IBA-13), LISBON,
   PORTUGAL, JUL 27-AUG 01, 1997}},
Abstract = {{The absolute concentration of P in a cyclic phosphazene used as a
   lubricant additive for hard disk surfaces has been determined by
   measuring the He-4(+)-ion induced K X-ray yield. A new method utilized
   the detection of X-rays emitted at grazing angles with respect to the
   sample surface in order to differentiate between those P K X-rays
   originating from the additive at the surface and those from the hard
   disk NiP substrate. Under these conditions, P K X-rays emanating from
   the substrate were totally self-absorbed in the sample. A saturated
   coverage of 2 x 10(14) atoms/cm(2) was measured for the surface film,
   with a minimum detection limit estimated to be 5 x 10(13) atoms/cm(2).
   Results for a variety of phosphazene concentrations yielded reasonable
   agreement with relative values measured by X-ray photoelectron
   spectroscopy. (C) 1998 Elsevier Science B.V.}},
