@article{ ISI:000073707600001,
Author = {Ortega, A and Maldonado, E and Diaz, E and Reynolds, WF},
Title = {{Variable temperature NMR studies on the conformations of tonalensin in
   solution}},
Journal = {{SPECTROCHIMICA ACTA PART A-MOLECULAR AND BIOMOLECULAR SPECTROSCOPY}},
Year = {{1998}},
Volume = {{54}},
Number = {{5}},
Pages = {{659-670}},
Month = {{MAY}},
Abstract = {{NMR studies on tonalensin 1, a diterpene containing a ten membered ring,
   made evident the presence of three conformational isomers in solution.
   At room temperature compound 1 exists as a mixture of the conformers 1A
   and 1B in a ratio 1:1 in a CDCl3 solution and 1.5:1 in a
   Me2CO-d(6)/DMSO-d(6) solution. At lower temperatures a third conformer,
   1C, was detected. It was responsible for the line broadening observed
   for 1A. Temperature dependent 2D NMR experiments have been employed to
