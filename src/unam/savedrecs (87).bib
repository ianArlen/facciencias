@article{ ISI:000267367300007,
Author = {Ramirez-Meneses, E. and Chavira, E. and Torres-Huerta, A. M. and Banos,
   L. and Guzman, J.},
Title = {{Comparative study of the synthesis of KReO4 using acrylamide sol-gel and
   solid-state reaction methods}},
Journal = {{JOURNAL OF SOL-GEL SCIENCE AND TECHNOLOGY}},
Year = {{2009}},
Volume = {{51}},
Number = {{2}},
Pages = {{175-181}},
Month = {{AUG}},
Abstract = {{KReO4 was synthesized by solid-state reaction and for the first time by
   sol-gel method via acrylamide polymerization. X-ray diffraction analysis
   showed that a single phase of KReO4 was obtained by both methods at
   500-505 A degrees C having a tetragonal unit cell. The solid-state
   reaction samples had a grain size of 5 mu m and the sol-gel samples had
   fibrous aspect, agglomerated between 10 and 100 mu m. By TEM it was
   observed nanocrystals of 100 nm, it is suggested that the xerogel fibers
   are formed by nanocrystals. From the results obtained, we concluded that
   the morphology is strongly influenced by the method of synthesis used.}},
