@article{ ISI:000269480500009,
Author = {Pedraza-Chaverri, Jose and Maria Reyes-Fermin, Laura and Guadalupe
   Nolasco-Amaya, Eva and Orozco-Ibarra, Marisol and Noel Medina-Campos,
   Omar and Gonzalez-Cuahutencos, Octavio and Rivero-Cruz, Isabel and Mata,
   Rachel},
Title = {{ROS scavenging capacity and neuroprotective effect of alpha-mangostin
   against 3-nitropropionic acid in cerebellar granule neurons}},
Journal = {{EXPERIMENTAL AND TOXICOLOGIC PATHOLOGY}},
Year = {{2009}},
Volume = {{61}},
Number = {{5}},
Pages = {{491-501}},
Month = {{SEP}},
Abstract = {{alpha-Mangostin is a xanthone with antioxidant properties isolated from
   mangosteen fruit. The reactive oxygen species (ROS) scavenging capacity
   and the potential protective effect of alpha-mangostin against the
   mitochondrial toxin 3-nitropropionic acid (3-NP) in primary cultures of
   cerebellar granule neurons (CGNs) were studied in the present work. It
   was found that alpha-mangostin was able to scavenge in a
   concentration-dependent way singlet oxygen, superoxide anion and
   peroxynitrite anion. In contrast, alpha-mangostin was unable to scavenge
   hydroxyl radicals and hydrogen peroxide. Furthermore, alpha-mangostin
   was able to ameliorate in a concentration-dependent way the neuronal
   death induced by 3-NP. This protective effect was associated with an
   amelioration of 3-NP-induced reactive oxygen species formation. It is
   concluded that alpha-mangostin is able to scavenge directly several ROS
   and has a neuroprotective effect against 3-NP in primary cultures of
   CGNs, which is associated with its ability to ameliorate 3-NP-induced
   ROS production. (C) 2008 Elsevier GmbH. All rights reserved.}},
