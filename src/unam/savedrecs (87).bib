Author = {Garcia-Villegas, Refugio and Lopez-Alvarez, Luz E. and Arni, Stephan and
   Rosenbaum, Tamara and Morales, Marco A.},
Title = {{Identification and Functional Characterization of the Promoter of the
   Mouse Sodium-Activated Sodium Channel Na-x Gene (Scn7a)}},
Journal = {{JOURNAL OF NEUROSCIENCE RESEARCH}},
