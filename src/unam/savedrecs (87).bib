@article{ ISI:000267994100027,
Author = {Ochoa-Olmos, Omar E. and Montero-Montoya, Regina and Serrano-Garcia,
   Luis and Basiuk, Elena V.},
Title = {{Genotoxic Properties of Nylon-6/MWNTs Nanohybrid}},
Journal = {{JOURNAL OF NANOSCIENCE AND NANOTECHNOLOGY}},
Year = {{2009}},
Volume = {{9}},
Number = {{8}},
Pages = {{4727-4734}},
Month = {{AUG}},
Abstract = {{We performed the micronucleus test to determine the level of
   biocompatibility of pristine multi-walled carbon nanotubes (MWNTs) and
   MWNTs functionalized with nylon-6, (referred to as the nylon-6/MWNTs
   nanohybrid), when they interact with human lymphocytes in a cell culture
   medium. A comparative genotoxic analysis demonstrated a better degree of
   biocompatibility of nylon-6/MWNT with human lymphocytes, as compared to
   pristine MWNTs, for the concentration range of 10-60 mu g/ml. An
   evidence was found that pristine MWNTs act as clastogenic agents and
   possibly as aneuploidogenic agents, increasing the frequency of
   genotoxic bioindicators. On the other hand, nylon-6/MWNTs nanohybrid
   were observed to induce cell death via apoptosis, which could be
   attributed to residual impurities of epsilon-caprolactam.}},
