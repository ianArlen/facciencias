@article{ ISI:000266145400009,
Author = {Aichholzer, Oswin and Bereg, Sergey and Dumitrescu, Adrian and Garcia,
   Alfredo and Huemer, Clemens and Hurtado, Ferran and Kano, Mikio and
   Marquez, Alberto and Rappaport, David and Smorodinsky, Shakhar and
   Souvaine, Diane and Urrutia, Jorge and Wood, David R.},
Title = {{Compatible geometric matchings}},
Journal = {{COMPUTATIONAL GEOMETRY-THEORY AND APPLICATIONS}},
Year = {{2009}},
Volume = {{42}},
Number = {{6-7}},
Pages = {{617-626}},
Month = {{AUG}},
Note = {{Conference on Topological and Geometric Graph Theory (TGGT), Paris,
   FRANCE, MAY 19-23, 2008}},
Abstract = {{This paper studies non-crossing geometric perfect matchings. Two such
   perfect matchings are compatible if they have the same vertex set and
   their union is also non-crossing. Our first result states that for any
   two perfect matchings M and M' of the same set of n points, for some k
   is an element of O(log n), there is a sequence of perfect matchings M =
   M-0, M-1, ... M-k = M', such that each M-i is compatible with Mi+1. This
   improves the previous best bound of k <= n - 2. We then study the
   conjecture: every perfect matching with an even number of edges has an
   edge-disjoint compatible perfect matching. We introduce a sequence of
   stronger conjectures that imply this conjecture, and prove the strongest
   of these conjectures in the case of perfect matchings that consist of
   vertical and horizontal segments. Finally, we prove that every perfect
   matching with n edges has an edge-disjoint compatible matching with
   approximately 4n/5 edges. (C) 2009 Elsevier B.V. All rights reserved.}},
