@article{ ISI:000270440200009,
Author = {Zasche, P. and Uhlar, R.},
Title = {{A DETAILED LIGHT CURVE ANALYSIS OF TY DELPHINI}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2009}},
Volume = {{45}},
Number = {{2}},
Pages = {{205-214}},
Month = {{OCT}},
Abstract = {{A detailed light curve analysis of the eclipsing binary TY Del is
   presented for the first time. The B, V, R, and I light curves were
   observed and also new minima times have been derived. The secondary
   minimum has been observed for the first time. The system is probably a
   detached one with rather unequal components. Eccentricity of the system
   is still uncertain, but not ruled out. Also the influence of a possible
   third component is discussed together with a potential apsidal motion.
   if the hypothesis of apsidal motion is confirmed, it will be the
   shortest apsidal motion period among eclipsing binaries.}},
