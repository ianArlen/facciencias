@article{ ISI:000269142000017,
Author = {Pinon-Gimate, Alejandra and Soto-Jimenez, Martin F. and Julia
   Ochoa-Izaguirre, Maria and Garcia-Pages, Eynar and Paez-Osuna, Federico},
Title = {{Macroalgae blooms and delta N-15 in subtropical coastal lagoons from the
   Southeastern Gulf of California: Discrimination among agricultural,
   shrimp farm and sewage effluents}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2009}},
Volume = {{58}},
Number = {{8}},
Pages = {{1144-1151}},
Month = {{AUG}},
Abstract = {{Macroalgae blooms of Gracilaria vermiculophylla, Hypnea spinella and
   Spyridia filamentosa have been found in coastal lagoons in the SE Gulf
   of California. Agriculture, livestock, shrimp and poultry farms and
   sewage contribute anthropogenic nitrogen to the systems. The delta N-15
   of these sources, water column and macroalgae were studied in order to
   identify the N supply for macroalgae blooms. delta N-15 of three species
   of macroalgae (4.3-13.6 parts per thousand) were enriched compared to
   the water column (delta N-15-NO3- 3.7-6.8\%), probably because of
   fractioning from the macroalgae. delta N-15 of POM (1.4-10.3\%) was
   similar to the water column but the relationship was unclear. Depending
   on the site, macroalgae showed different delta N-15 values since some
   sites receive more or less influence from one given source of the
   associated watershed, which is reflected in the different delta N-15
   values of the macroalgae of the same system and in the relative
   contributions of the sources. (C) 2009 Elsevier Ltd. All rights
   reserved.}},
