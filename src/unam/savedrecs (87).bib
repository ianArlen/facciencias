@article{ ISI:000269179300002,
Author = {Avila-Carrera, R. and Rodriguez-Castellanos, A. and Sanchez-Sesma, F. J.
   and Ortiz-Aleman, C.},
Title = {{Rayleigh-wave scattering by shallow cracks using the indirect boundary
   element method}},
