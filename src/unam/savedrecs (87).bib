@article{ ISI:000269455800011,
Author = {Lenis, Carolina and Garcia-Prieto, Luis},
Title = {{POLYSTOMOIDES MAGDALENENSIS N. SP (MONOGENOIDEA: POLYSTOMATIDAE), A
   PARASITE OF BUCCAL CAVITY OF TRACHEMYS CALLIROSTRIS CALLIROSTRIS
   (TESTUDINATA: EMYDIDAE) FROM COLOMBIA}},
Journal = {{JOURNAL OF PARASITOLOGY}},
Year = {{2009}},
Volume = {{95}},
Number = {{4}},
Pages = {{850-854}},
Month = {{AUG}},
Abstract = {{Polystomoides magelalenensis n. sp. is described from the buccal cavity
   of the Colombian slider Trachemys callirostris callirostris (Gray, 1856)
   in the middle of Magdalena River drainage, Colombia. The new species is
   characterized by a combination of traits, i.e., 29-35 genital spines,
   outer and inner hamuli length (123-150 and 54-63, respectively), testis
   size (261.4-554.5 X 388.2768.4), marginal hook lengths (45-51), and
   pharynx length (150-333), as well as testis length/oral sucker length,
   testis length/pharynx length, and testis width/pharyrix width ratios.
   This is the first species described from turtles in Colombia and the
   fifth reported in chelonians from South America.}},
