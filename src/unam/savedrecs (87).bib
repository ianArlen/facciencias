@article{ ISI:000271352100054,
Author = {Hahn-Herrera, Otto and Aburto, Andrea and Orgaz, Emilio},
Title = {{Hydrogen occupancy in the RNi4Mg (R=Y, La, Ce, and Nd) intermetallic
   compounds and hydrides}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2009}},
Volume = {{80}},
Number = {{16}},
Month = {{OCT}},
Abstract = {{We have investigated the effect of hydrogen on the electronic strtucture
   of the RNi4Mg (R=Y, La, Ce, Pr, and Nd) intermetallics. By means of a
   two-step approach, the projected plane-wave and linearized plane-waves
   methods, we studied the hydrogen-insertion energetics on the
   intermetallic matrix and the H-vacancy formation in the hydride
   compound. We found that particular interstitial sites in the
   intermetallics are suitable to allocate hydrogen and form a solid
   solution. The effect of these interstitials on the electronic structure
   is discussed. In the other hand, the hydrogen-occupied sites in the
   hydride are found to be energetically equivalent.}},
