@article{ ISI:000269970200004,
Author = {Rivas-Caicedo, Ana and Soldevila, Gloria and Fortoul, Teresa I. and
   Castell-Rodriguez, Andres and Flores-Romo, Leopoldo and Garcia-Zepeda,
   Eduardo A.},
Title = {{Jak3 Is Involved in Dendritic Cell Maturation and CCR7-Dependent
   Migration}},
Journal = {{PLOS ONE}},
Year = {{2009}},
Volume = {{4}},
Number = {{9}},
Month = {{SEP 17}},
Abstract = {{Background: CCR7-mediated signalling is important for dendritic cell
   maturation and homing to the lymph nodes. We have previously
   demonstrated that Jak3 participates in the signalling pathway of CCR7 in
   T lymphocytes.
   Methodology and Principal Findings: Here, we used Jak3(-/-) mice to
   analyze the role of Jak3 in CCR7-mediated dendritic cells migration and
   function. First, we found no differences in the generation of DCs from
   Jak3(-/-) bone marrow progenitors, when compared to wild type cells.
   However, phenotypic analysis of the bone marrow derived DCs obtained
   from Jak3(-/-) mice showed reduced expression of co-stimulatory
   molecules compared to wild type (Jak3(+/+)). In addition, when we
   analyzed the migration of Jak3(-/-) and Jak3(+/+) mature DCs in response
   to CCL19 and CCL21 chemokines, we found that the absence of Jak3 results
   in impaired chemotactic responses both in vitro and in vivo. Moreover,
   lymphocyte proliferation and contact hypersensitivity experiments showed
   that DC-mediated T lymphocyte activation is reduced in the absence of
   Jak3.
   Conclusion/Significance: Altogether, our data provide strong evidence
   that Jak3 is important for DC maturation, migration and function,
   through a CCR7-mediated signalling pathway.}},
