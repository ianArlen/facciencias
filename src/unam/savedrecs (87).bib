@article{ ISI:000269724300004,
Author = {Robledo, A. and Moyano, L. G.},
Title = {{Dynamics towards the Feigenbaum attractor}},
Journal = {{BRAZILIAN JOURNAL OF PHYSICS}},
Year = {{2009}},
Volume = {{39}},
Number = {{2A}},
