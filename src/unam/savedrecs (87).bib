@article{ ISI:000268906300001,
Author = {Warren, Andrew D. and Austin, George T.},
Title = {{Variation of Stinga Evans, 1955, with description of a new species from
   Mexico (Lepidoptera: Hesperiidae: Hesperiinae: Hesperiini)}},
Journal = {{ZOOTAXA}},
Year = {{2009}},
Number = {{2197}},
Pages = {{1-19}},
Month = {{AUG 13}},
Abstract = {{Stinga Evans, 1955, has been considered monotypic since its description
   despite geographical variation that has not been elucidated. The
   variation of Stinga morrisoni (W. H. Edwards, 1878) described and
   figured here includes a range of superficial phenotypes segregated by a
   combination of geography and elevation, but united by a common genital
   morphology of both sexes. In addition, a second species of the genus
   occurring in southern Mexico (Tlaxcala, Puebla and Oaxaca), with
   different genitalia and superficial traits outside the range of
   variation seen in S. morrisoni, is described and named as a new species.}},
