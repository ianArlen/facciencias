@article{ ISI:000268258000013,
Author = {Escudero, R. and Morales, F. and Bernes, S.},
Title = {{Specific heat studies of pure Nb3Sn single crystals at low temperature}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2009}},
Volume = {{21}},
Number = {{32}},
Month = {{AUG 12}},
Abstract = {{Specific heat measurements performed on high purity vapor-grown Nb3Sn
   crystals show clear features related to both the martensitic and
   superconducting transitions. Our measurements indicate that the
   martensitic anomaly does not display hysteresis, meaning that the
   martensitic transition could be a weak first-order or a second-order
   thermodynamic transition. Careful measurements of the two transition
   temperatures display an inverse correlation between them. At low
   temperature, specific heat measurements show the existence of a single
   superconducting energy gap feature.}},
