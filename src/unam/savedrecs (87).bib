@article{ ISI:000270440200011,
Author = {Cruz, Fidel and Aceves, Hector},
Title = {{A PRE-MERGER IN THE LUMINOUS INFRARED SYSTEM IRAS 02290+2533}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2009}},
Volume = {{45}},
Number = {{2}},
Pages = {{231-238}},
Month = {{OCT}},
Abstract = {{We report optical and near-infrared imaging and photometry for the IRAS
   02290+2533 source. Observations show that this source is a system of
   three galaxies. Two galaxies (A and B), which are separated in
   projection approximate to 10kpc, show some evidence of tidal interaction
   between them. The spiral-like Galaxy B presents a disk-wide H alpha
   emission and a high infrared luminosity, from near to mid-infrared
   wavelengths. We suggest that such luminosity is due to a probable
   disk-wide star-burst triggered by the ongoing gravitational interaction
   with Galaxy A. Galaxy B can be classified as a Luminous Infrared Galaxy
   (LIRG). We estimate that a binary merger in the system is likely to
   occur in less than or similar to 1 Gyr. It is probable that this IRAS
   source will harbor in the future an Ultra LIRG, according to the
   evolutionary scheme of Sanders \& Mirabel (1996).}},
Publisher = {{UNIV NACIONAL AUTONOMA MEXICO, INST DE ASTRONOMIA}},
Address = {{APDO POSTAL 70-264, MEXICO CITY 04510, MEXICO}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Cruz, F (Reprint Author), Bahia San Quintin, Ensenada, Baja California, Mexico.
   Cruz, Fidel, Bahia San Quintin, Ensenada, Baja California, Mexico.
