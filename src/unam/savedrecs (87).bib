@article{ ISI:000273162100074,
Author = {Espinosa, G. and Golzarri, J. I. and Ponciano-Rodriguez, G. and Gaso, M.
   I. and Mena, M. and Segovia, N. and Vazquez-Lopez, C. and Sajo-Bohus, L.},
Title = {{Population vulnerability due to the exposure to radon and airborne
   particulate matter (PM10), in Mexico City}},
Journal = {{RADIATION MEASUREMENTS}},
Year = {{2009}},
Volume = {{44}},
Number = {{9-10, SI}},
Pages = {{1028-1031}},
