Author = {Zeferino Gonzalez, I and Valenzuela-Muniz, A. M. and Alonso-Nunez, G.
   and Farias, M. H. and Verde Gomez, Y.},
Title = {{Influence of the Synthesis Parameters in Carbon Nanotubes Doped with
   Nitrogen for Oxygen Electroreduction}},
