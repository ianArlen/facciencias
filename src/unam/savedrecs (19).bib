@article{ ISI:000393258300010,
Author = {Negrette-Guzman, Mario and Huerta-Yepez, Sara and Vega, Mario I. and
   Carlos Leon-Contreras, Juan and Hernandez-Pando, Rogelio and Noel
   Medina-Campos, Omar and Rodriguez, Esteban and Tapia, Edilia and
   Pedraza-Chaverri, Jose},
Title = {{Sulforaphane induces differential modulation of mitochondrial biogenesis
   and dynamics in normal cells and tumor cells}},
Journal = {{FOOD AND CHEMICAL TOXICOLOGY}},
Year = {{2017}},
Volume = {{100}},
Pages = {{90-102}},
Month = {{FEB}},
Abstract = {{Antioxidant-based chemotherapy has been intensely debated. Herein, we
   show that sulforaphane (SFN) induced mitochondrial biogenesis followed
   by mitochondrial fusion in a kidney cell line commonly used in
   nephroprotective models. At the same concentration and exposure time,
   SFN induced cell death in prostate cancer cells accompanied by
   mitochondrial biogenesis and fragmentation. Stabilization of the nuclear
   factor E2-related factor-2 (Nrf2) could be associated with these effects
   in the tumor cell line. An increase in the peroxisome
   proliferator-activated receptor-gamma co-activator-1 alpha (PGC1 alpha)
   level and a decrease in the hypoxia-inducible factor-1 alpha (H1F1
   alpha) level would suggest a possible metabolic shift. The knockdown in
   the nuclear respiratory factor-1 (NRF1) attenuated the SFN-induced
   effect on prostate cancer cells demonstrating that mitochondrial
   biogenesis plays an important role in cell death for this kind of tumor
   cells. This evidence supports SFN as a potential antineoplastic agent
   that could inhibit tumor development and could protect normal tissues by
   modulating common processes. (C) 2016 Elsevier Ltd. All rights reserved.}},
