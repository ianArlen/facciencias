@article{ ISI:000393443400013,
Author = {Pablo-Pedro, Ricardo and Lopez-Rios, Hector and Fomine, Serguei and
   Dresselhaus, Mildred S.},
Title = {{Detection of Multiconfigurational States of Hydrogen-Passivated Silicene
   Nanoclusters}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY LETTERS}},
Year = {{2017}},
Volume = {{8}},
Number = {{3}},
Pages = {{615-620}},
Month = {{FEB 2}},
Abstract = {{Utilizing density functional theory (DET) and a complete active space
   self-consistent field (CASSCF) approach,we study the electronic
   properties of rectangular silicene nano clusters with hydrogen
   passivated edges denoted by H-SiNCs (n(nu)n(a)), with n(z) and n(a)
   representing the zigzag and armchair directions, respectively. The
   results show that in the nz direction, the H-SiNCs prefer to be in a
   singlet (S = 0) ground state for n(z) > n(a). However, a transition from
   a singlet (S = 0) to a triplet (S = 1) ground state is revealed for n(a)
   > n(z) Through the calculated Raman spectrum, the S = 0 and S = 1 ground
   states can be observed by the E-2g (G) and A (D) Raman modes.
   Furthermore, H-SiNC clusters are shown to have HOMO-LUMO (HL) energy
   gaps, which decrease as a function of n(a) and n(z) for S = 0 and S = I
   states. The H-SiNC with a S = 1 ground state can be potentially used for
   silicene-based spintronic devices.}},
