@article{ ISI:000397106500007,
Author = {Fattorini, Giorgia and Melone, Marcello and Victoria Sanchez-Gomez,
   Maria and Arellano, Rogelio O. and Bassi, Silvia and Matute, Carlos and
   Conti, Fiorenzo},
Title = {{GAT-1 Mediated GABA Uptake in Rat Oligodendrocytes}},
