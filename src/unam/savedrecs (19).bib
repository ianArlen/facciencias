@article{ ISI:000395210400013,
Author = {Panathula, Chandrasekhara Bharath and Rosales, Antonio and Shtessel,
   Yuri and Basin, Michael},
Title = {{Chattering analysis of uniform fixed-time convergent Sliding Mode
   Control}},
Journal = {{JOURNAL OF THE FRANKLIN INSTITUTE-ENGINEERING AND APPLIED MATHEMATICS}},
Year = {{2017}},
Volume = {{354}},
Number = {{4}},
Pages = {{1908+}},
Month = {{MAR}},
Abstract = {{In this paper, chattering in systems controlled by a super-twisting
   based fixed-time convergent controller is analyzed. The analysis is
   performed by introducing dynamical perturbations into the system driven
   by the fixed-time convergent controller that yields self-sustained
   oscillations with a finite frequency and a finite amplitude. Describing
   Function-Harmonic Balance technique is used to estimate the amplitude
   and the frequency of those self-sustained oscillations. Then, the
   stability of the self-sustained oscillations is analyzed. Furthermore,
   considering a DC motor as a case study, chattering of super-twisting
   based fixed time convergent controller is compared with chattering of
   the traditional super-twisting controller having the same gains. (C)
   2017 The Franklin Institute. Published by Elsevier Ltd. All rights
   reserved.}},
