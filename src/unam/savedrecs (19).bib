@article{ ISI:000393744900003,
Author = {Mercati, Flavio and Gomes, Henrique and Koslowski, Tim and Napoletano,
   Andrea},
Title = {{Gravitational collapse of thin shells of dust in asymptotically flat
   shape dynamics}},
