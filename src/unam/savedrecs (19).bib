@article{ ISI:000426530700008,
Author = {Belmont, Jafet and Sanchez-Coronado, Maria E. and Osuna-Fernandez, Helia
   R. and Orozco-Segovia, Alma and Pisanty, Irene},
Title = {{Priming effects on seed germination of two perennial herb species in a
   disturbed lava field in central Mexico}},
