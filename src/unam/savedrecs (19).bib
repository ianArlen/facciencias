@article{ ISI:000395496700011,
Author = {Arce, H. and Torres, A. and Falcon-Neri, A. and Mimila, O. and Gonzalez,
   G. H.},
Title = {{Border collision bifurcations in tantalus oscillator}},
Journal = {{REVISTA MEXICANA DE FISICA}},
