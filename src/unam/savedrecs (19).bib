@article{ ISI:000398046700009,
Author = {Alatorre-Fernandez, Pamela and Mayoral-Teran, Claudia and
   Velazquez-Acosta, Consuelo and Franco-Rodriguez, Cecilia and
   Flores-Moreno, Karen and Angel Cevallos, Miguel and Lopez-Vidal, Yolanda
   and Volkow-Fernandez, Patricia},
Title = {{A polyclonal outbreak of bloodstream infections by Enterococcus faecium
   in patients with hematologic malignancies}},
Journal = {{AMERICAN JOURNAL OF INFECTION CONTROL}},
Year = {{2017}},
Volume = {{45}},
Number = {{3}},
Pages = {{260-266}},
Month = {{MAR 1}},
Abstract = {{Background: Enterococcus faecium causes bloodstream infection (BSI) in
   patients with hematologic malignancies (HMs). We studied the clinical
   features and outcomes of patients with HM with vancomycinsensitive E
   faecium (VSE) and vancomycin-resistant E faecium (VRE) BSI and
   determined the genetic relatedness of isolates and circumstances
   associated with the upsurge of E faecium BSI.
   Methods: Case-control study of patients with HM and E faecium-positive
   blood culture fromJanuary 2008-December 2012; cases were patients with
   VRE and controls were VSE isolates. The strains were tested for Van
   genes by polymerase chain reaction amplification and we performed
   pulsed-field gel electrophoresis to determine genetic relatedness.
   Results: Fifty-eight episodes of E faecium BSI occurred: 35 sensitive
   and 23 resistant to vancomycin. Mortality was 46\% and 57\%,
   attributable 17\% and 40\%, respectively. Early stage HM was associated
   with VSE (P = .044), whereas an episode of BSI within the 3 months
   before the event (P = .039), prophylactic antibiotics (P = .013), and
   vancomycin therapy during the previous 3 months (P = .001) was
   associated with VRE. The VanA gene was identified in 97\% of isolates
   studied. E faecium isolates were not clonal.
   Conclusions: E faecium BSI was associated with high mortality. This
   outbreak of VRE was not clonal; it was associated with antibiotic-use
   pressure and highly myelosuppressive chemotherapy. (C) 2017 Association
   for Professionals in Infection Control and Epidemiology, Inc. Published
   by Elsevier Inc. All rights reserved.}},
Publisher = {{MOSBY-ELSEVIER}},
Address = {{360 PARK AVENUE SOUTH, NEW YORK, NY 10010-1710 USA}},
