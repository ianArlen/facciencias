@article{ ISI:000396223500132,
Author = {Sanal, K. C. and Nair, P. K. and Nair, M. T. S.},
Title = {{Band offset in zinc oxy-sulfide/cubic-tin sulfide interface from X-ray
   photoelectron spectroscopy}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{2017}},
Volume = {{396}},
Pages = {{1092-1097}},
Month = {{FEB 28}},
Abstract = {{Zinc oxy-sulfide, ZnOxS1-x, has been found to provide better band
   alignment in thin film solar cells of tin sulfide of orthorhombic
   crystalline structure. Here we examine ZnOxS1-x/SnS-CUB interface, in
