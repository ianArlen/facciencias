
@article{ ISI:000396988400012,
Author = {Suarez-Cerda, Javier and Espinoza-Gomez, Heriberto and Alonso-Nunez,
   Gabriel and Rivero, Ignacio A. and Gochi-Ponce, Yadira and Flores-Lopez,
