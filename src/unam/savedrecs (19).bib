@article{ ISI:000393785500087,
Author = {Tsapras, Y. and Arellano Ferro, A. and Bramich, D. M. and Jaimes, R.
   Figuera and Kains, N. and Street, R. and Hundertmark, M. and Horne, K.
   and Dominik, M. and Snodgrass, C.},
Title = {{Variable stars in the bulge globular cluster NGC 6401}},
