@article{ ISI:000395078600019,
Author = {Gracida, Jorge and Ortega-Ortega, Josue and Torres B, Luis G. and
   Romero-Avila, Margarita and Abreu, Arturo},
Title = {{Synthesis of Anionic Surfactant and Their Application in Washing of
   Oil-Contaminated Soil}},
Journal = {{JOURNAL OF SURFACTANTS AND DETERGENTS}},
Year = {{2017}},
Volume = {{20}},
Number = {{2}},
Pages = {{493-502}},
Month = {{MAR}},
Abstract = {{In this study, we report the synthesis of an anionic surfactant
   5-(3,4,5-tris(hexyloxy)benzyloxy)isophthalic acid disodium salt (AS-5)
   and its application to wash off a contaminated soil with total petroleum
   hydrocarbons (TPH). The synthesis of the anionic surfactant was achieved
   in five steps with a 56\% overall yield. AS-5 was characterized by H-1
   nuclear magnetic resonance (NMR) spectroscopy while its precursor was
   characterized by H-1 and C-13 NMR. Some surfactant properties such as
   surface tension (ST), critical miceilar concentration (CMC) and E24 test
   were determined. AS-5 showed a significant capability to decrease the
   water surface tension, the minimum value of the surface tension obtained
   was 25.9 mN/m while the CMC value was 54.6 mg/L (8.87 x 10(-3) mM). The
   capability of AS-5 to remove TPH from a contaminated soil with
   concentrations higher than 24 000 mg TPH/kg soil was demonstrated and
   the percentage removed ranged from 52 to 76\% as determined at different
   concentrations of AS-5.}},
