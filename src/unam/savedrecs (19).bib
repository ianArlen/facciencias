@article{ ISI:000395634000017,
Author = {Gomez-Valdes, Jorge A. and Menendez Garmendia, Antinea and
   Garcia-Barzola, Lizbeth and Sanchez-Mejorada, Gabriela and Karam, Carlos
   and Pablo Baraybar, Jose and Klales, Alexandra},
Title = {{Recalibration of the Klales et al. (2012) method of sexing the human
   innominate for Mexican populations}},
Journal = {{AMERICAN JOURNAL OF PHYSICAL ANTHROPOLOGY}},
Year = {{2017}},
Volume = {{162}},
Number = {{3}},
Pages = {{600-604}},
Month = {{MAR}},
Abstract = {{ObjectivesThe aim of this study was to test the accuracy of the Klales
   et al. (2012) equation for sex estimation in contemporary Mexican
   population.
   Materials and MethodsOur investigation was carried out on a sample of
   203 left innominates of identified adult skeletons from the
   UNAM-Collection and the Santa Maria Xigui Cemetery, in Central Mexico.
   The Klales' original equation produces a sex bias in sex estimation
   against males (86-92\% accuracy versus 100\% accuracy in females). Based
   on these results, the Klales et al. (2012) method was recalibrated for a
   new cutt-of-point for sex estimation in contemporary Mexican
   populations.
   Results and DiscussionThe results show cross-validated classification
   accuracy rates as high as 100\% after recalibrating the original
   logistic regression equation. Recalibration improved classification
   accuracy and eliminated sex bias. This new formula will improve sex
   estimation for Mexican contemporary populations.}},
