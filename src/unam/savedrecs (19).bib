@article{ ISI:000395671600007,
Author = {Ford, Kevin and Garaev, Moubariz Z. and Konyagin, Sergei V.},
Title = {{On the smallest simultaneous power nonresidue modulo a prime}},
Journal = {{FORUM MATHEMATICUM}},
Year = {{2017}},
Volume = {{29}},
Number = {{2}},
Pages = {{347-355}},
Month = {{MAR}},
Abstract = {{Let p be a prime and let p(1),..., p(r) be distinct prime divisors of p
   - 1. We prove that the smallest positive integer n which is a
   simultaneous p(1),..., p(r)-power nonresidue modulo p satisfies
   n < p(1/4-cr+o(1)) ( p ->infinity)
   for some positive cr satisfying cr = e(-(1+o(1))r) as r ->infinity.}},
