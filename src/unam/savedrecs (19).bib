@article{ ISI:000390731800105,
Author = {Arcos-Ramos, Rafael and Maldonado-Dominguez, Mauricio and
   Ordonez-Hernandez, Javier and Romero-Avila, Margarita and Farfan,
   Norberto and del Pilar Carreon-Castro, Maria},
Title = {{3-Substituted-7-(diethylamino)coumarins as molecular scaffolds for the
   bottom-up self-assembly of solids with extensive pi-stacking}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE}},
Year = {{2017}},
Volume = {{1130}},
Pages = {{914-921}},
Month = {{FEB 15}},
Abstract = {{In this study, a set of molecular crystals derived from
   3-substituted-7-(diethylamino)-2H-chromen-2-ones 1-8 were studied to
   sample the aggregation of coumarins into ordered solids. Crystals of
   parent compound la and its brominated derivative 2 were obtained and
   solved in the P-1 and C2/c space groups, respectively. All the
   crystalline coumarins studied display extensive pi-stacking in the
   solid-state. Theoretical valence-conduction band gaps for derivatives 3b
   and 5 are close to crystalline rubrene, highlighting the importance of
   cooperativity and periodicity of pi-stacking, in organic semiconductors;
   given their synthetic accessibility, electronic tunability and
   self-assembly via stacking, dipolar and H-bonding interactions, these
   systems arise as candidates for the bottom-up construction of organic
   crystals with extensive pi-stacking and high polarizability. (C) 2016
   Elsevier B.V. All rights reserved.}},
