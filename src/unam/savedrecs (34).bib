@article{ ISI:000371995100004,
Author = {Alfredo Contreras-Valdez, Jose and Hernandez-Guzman, Laura and Freyre,
   Miguel-Angel},
Title = {{Construct validity of the Beck Depression Inventory II in adolescents}},
Journal = {{TERAPIA PSICOLOGICA}},
Year = {{2015}},
Volume = {{33}},
Number = {{3}},
Pages = {{195-203}},
Month = {{DEC}},
Abstract = {{Few studies have analyzed theoretical model of depression through the
   Beck Depression Inventory II (BDI-II) in Spanish-speaking adolescents.
   This research had two aims: 1) to know the fit of two BDI-II factor
   models in community Mexican adolescents, the one-dimension model and the
   Beck et al. (1996)'s original model posing the Cognitive-Affective and
   Somatic-Vegetative factors, and 2) to examine model invariance across
   sexes with the best-fitting model. In two studies, 1010 and 610
   adolescents participated, respectively, who completed the BDI-II.
   Through CFA the two factor structures were tested and, by means of
   multi-group CFA, invariance analysis across sexes was performed. Results
   revealed superiority of the two-factor model in both studies, without
   differences between women and men. These findings lead to evidence for
   Beck's theory of depression and construct validity of the BDI-II in
   adolescents.}},
