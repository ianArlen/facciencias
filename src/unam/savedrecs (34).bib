@article{ ISI:000379443200001,
Author = {Reyes-Garcia, Jorge and Flores-Soto, Edgar and Solis-Chagoyan, Hector
   and Sommer, Bettina and Diaz-Hernandez, Veronica and Maria
   Garcia-Hernandez, Luz and Montano, Luis M.},
Title = {{Tumor Necrosis Factor Alpha Inhibits L-Type Ca2+ Channels in Sensitized
   Guinea Pig Airway Smooth Muscle through ERK 1/2 Pathway}},
Journal = {{MEDIATORS OF INFLAMMATION}},
Year = {{2016}},
Abstract = {{Tumor necrosis factor alpha (TNF-alpha) is a potent proinflammatory
   cytokine that plays a significant role in the pathogenesis of asthma by
   inducing hyperresponsiveness and airway remodeling. TNF-alpha diminishes
   the L-type voltage dependent Ca2+ channel (L-VDCC) current in cardiac
   myocytes, an observation that seems paradoxical. In guinea pig
   sensitized tracheas KCl responses were lower than in control tissues.
   Serum from sensitized animals (Ser-S) induced the same phenomenon. In
   tracheal myocytes fromnonsensitized (NS) and sensitized (S) guinea pigs,
   an L-VDCC current (ICa) was observed and diminished by Ser-S. Thesame
   decrease was detected in NS myocytes incubated with TNF-alpha, pointing
   out that this cytokine might be present in Ser-S. We observed that a
   small-molecule inhibitor of TNF-alpha (SMI-TNF) and a TNF-alpha receptor
   1 (TNFR1) antagonist (WP9QY) reversed ICa decrease induced by Ser-S in
   NS myocytes, confirming the former hypothesis. U0126 (a blocker of ERK
   1/2 kinase) also reverted the decrease in ICa. Neither cycloheximide (a
   protein synthesis inhibitor) nor actinomycin D (a transcription
   inhibitor) showed any effect on the TNF-alpha -induced ICa reduction. We
   found that CaV 1.2 and CaV 1.3 mRNA and proteins were expressed in
   tracheal myocytes and that sensitization did not modify them. In cardiac
   myocytes, ERK 1/2 phosphorylates two sites of the L-VDCC, augmenting or
   decreasing ICa; we postulate that, in guinea pig tracheal smooth muscle,
   TNF-alpha diminishes ICa probably by phosphorylating the L-VDCC site
   that reduces its activity through the ERK1/2 MAP kinase pathway.}},
