@article{ ISI:000367561700049,
Author = {Pelayo, J. Jesus and Whetten, Robert L. and Garzon, Ignacio L.},
Title = {{Geometric Quantification of Chirality in Ligand-Protected Metal Clusters}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
