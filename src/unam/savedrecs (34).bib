Title = {{A Late Cretaceous marine long snout ``pejelagarto{''} fish
   (Lepisosteidae, Lepisosteini) from Muzquiz, Coahuila, northeastern
   Mexico}},
Journal = {{CRETACEOUS RESEARCH}},
