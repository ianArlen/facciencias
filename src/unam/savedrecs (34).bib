@inproceedings{ ISI:000387747500037,
Author = {Contreras Martinez, Ramiro and Garduno Mejia, Jesus and Rosete Aguilar,
   Martha and Roman Moreno, Carlos J.},
Editor = {{Creath, K and Burke, J and Goncalves, AA}},
Title = {{Spectral resolution measurement technique for Czerny-Turner
   spectrometers based on spectral interferometry.}},
Booktitle = {{INTERFEROMETRY XVIII}},
Series = {{Proceedings of SPIE}},
Year = {{2016}},
Volume = {{9960}},
Note = {{18th Interferometry Conference, San Diego, CA, AUG 30-SEP 01, 2016}},
Organization = {{SPIE}},
Abstract = {{We propose the design of a new technique for measuring the spectral
   resolution of a Czerny-Turner Spectrometer based on spectral
   interferometry of ultrashort laser pulses. It is well known that
   ultrashort pulse measurement like SPIDER and TADPOLE techniques requires
   a precise and well characterized spectrum, especially in fringe
   resolution. We developed a new technique, to our knowledge, in which by
   measuring the nominal fringe spacing of a spectral interferogram one can
   characterize the spectral resolution in a Czerny-Turner spectrometer
   using Ryleighs criteria. This technique was tested in a commercial
   Czerny-Turner spectrometer. The results demonstrate a consistent
   spectral resolution between what was reported by the manufacturer. The
