@inproceedings{ ISI:000376262000024,
Author = {Basurto, Eduardo and Vargas, Carlos A. and Haro-Perez, Catalina and
   Odriozola, Gerardo and Rojas, Braulio and de Urquijo, Jaime and
   Bekstein, Alexandre},
Editor = {{Gitler, I and Klapp, J}},
Title = {{Using a Parallel Genetic Algorithm to Fit a Pulsed Townsend Discharge
   Simulation to Experiments}},
