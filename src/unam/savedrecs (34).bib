@article{ ISI:000367491800015,
Author = {Cuevas-Muniz, F. M. and Gurrola, M. P. and Tellez-Vazquez, O. and
   Esparza, R. and Guerra-Balcazar, M. and Arriaga, L. G. and
   Ledesma-Garcia, J.},
Title = {{Correlation between theoretical data and experimental selective
   properties of PtAg core-shell nanoparticles for oxygen reduction
   reactions}},
