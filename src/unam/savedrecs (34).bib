@article{ ISI:000446266600001,
Author = {Estrada-Gonzalez, Luis},
Title = {{Some Clearer Reactions: Gauker on the Validity of Universal
   Instantiation}},
Journal = {{PHILOSOPHICAL INQUIRIES}},
Year = {{2016}},
Volume = {{4}},
Number = {{1}},
Pages = {{9-24}},
Abstract = {{It is part of the logical orthodoxy that quantifiers are interdefinable
   and that the rules of Universal Instantiation (UI) and Existential
   Generalization (EG) hold or fail together. Christopher Gauker has
   presented some cases which seemingly undermine the validity of UI but
   nonetheless leave EG untouched, and has developed a very sophisticated
   theory to explain why this is so. In the process, he has rejected
   several attempts to explain the asymmetry, especially those aiming at
   saving the logical orthodoxy by showing what is wrong with the
   counterexamples to UI. In this paper I argue that some of those
   proposals are better grounded than Gauker thinks and that ultimately
   they should be preferred over his since they explain satisfactorily the
   apparent counterexamples.}},
Publisher = {{EDIZIONI ETS}},
Address = {{PIAZZA CARRARA 16-19, 56126 PISA, ITALY}},
Type = {{Article}},
Language = {{English}},
