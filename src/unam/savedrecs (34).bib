@article{ ISI:000365759900001,
Author = {Raggi, Miguel},
Title = {{Forbidden configurations: Finding the number predicted by the
   Anstee-Sali conjecture is NP-hard}},
Journal = {{ARS MATHEMATICA CONTEMPORANEA}},
Year = {{2016}},
Volume = {{10}},
Number = {{1}},
Pages = {{1-8}},
Abstract = {{Let F be a (possibly non-simple) hypergraph and let for b (m, F) denote
   the maximum number of edges a simple hypergraph with m vertices can have
   if it doesn't contain F as a subhypergraph. A conjecture of Anstee and
   Sali predicts the asymptotic behaviour of for b (m, F) for fixed F. In
   this paper we prove that even finding this predicted asymptotic
   behaviour is an NP-hard problem, meaning that if the Anstee-Sali
   conjecture were true, finding the asymptotics of for b (m, F) would be
   NP-hard.}},
