@article{ ISI:000391008300003,
Author = {Enriquez Negrete, David Javier and Sanchez Medina, Ricardo and CDID Ctr
   Documentacion Invest},
Title = {{``SEXUAL BEHAVIOUR OF UNDERGRADUATES WITH PREGNANCY HISTORY: THE ROLE OF
   CONDOM USE INTENTION AND OF ITS' NEGOTIATION WITH THE PARTNER{''}}},
Journal = {{EUREKA-REVISTA CIENTIFICA DE PSICOLOGIA}},
Year = {{2016}},
Volume = {{13}},
Number = {{2}},
Pages = {{186-201}},
Abstract = {{The purpose of the present study was to compare the sexual behavior
   pattern, knowledge of HIV/STD, proper condom use, condom use negotiation
   styles and condom use intentions in undergraduates who have or do not
   have a pregnancy history. We evaluated 197 undergraduates through a self
   reporting questionnaire, 50\% reported having been pregnant. The results
   show that the sexual behavior pattern is riskier on those who reported
   having a pregnancy history. The intention of using a condom on the next
   sexual encounter was lower on men and the negotiation style based on
   mutual adjustment was higher on women who reported having been pregnant.
   Finally, the correlations found between the variables are shown. The
   risk profile importance of undergraduates who have a pregnancy history
