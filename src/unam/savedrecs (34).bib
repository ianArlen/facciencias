Title = {{Sensitivity analysis in the test of a parabolic trough solar collector
   (PTSC) with flat null-screens}},
Booktitle = {{Optical System Alignment, Tolerancing, and Verification X}},
Series = {{Proceedings of SPIE}},
Year = {{2016}},
Volume = {{9951}},
