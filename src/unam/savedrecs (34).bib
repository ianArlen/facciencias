@article{ ISI:000366339500002,
Author = {Harasewych, M. G. and Windsor, Amanda M. and Lopez-Vera, Estuardo and
   Thompson, Fred G.},
Title = {{On the phylogenetic relationships of the genus Mexistrophia and of the
   family Cerionidae (Gastropoda: Eupulmonata)}},
Journal = {{NAUTILUS}},
Year = {{2015}},
Volume = {{129}},
Number = {{4}},
Pages = {{156-162}},
Month = {{DEC 10}},
Abstract = {{Phylogenetic analyses of partial DNA sequences of the mitochondrial COI
   and 16S rDNA genes derived from Mexistrophia reticulata Thompson, 2011,
   the type species of the genus Mexistrophia, indicate that this genus is
   sister taxon to all remaining living Cerionidae, and that the family
   Cerionidae is most closely related to Urocoptidae. Relationships among
   representative cerionid taxa are consistent with the zoogeographic
   hypothesis that Mexistrophia has been isolated from the remaining living
