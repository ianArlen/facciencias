@article{ ISI:000367491800009,
Author = {Guevara-Lopez, Eliane and Buitron, German},
Title = {{Evaluation of different support materials used with a photo-fermentative
   consortium for hydrogen production}},
Journal = {{INTERNATIONAL JOURNAL OF HYDROGEN ENERGY}},
Year = {{2015}},
Volume = {{40}},
Number = {{48, SI}},
Pages = {{17231-17238}},
Month = {{DEC 28}},
