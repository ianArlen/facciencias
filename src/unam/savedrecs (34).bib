@inproceedings{ ISI:000456234800125,
Author = {Cardiel, Nicolas and Marino, Raffaella A. and Pascual, Sergio and Teresa
   Ceballos, M. and Gil de Paz, Armando and Sanchez, Sebastian F.},
Editor = {{DePaz, AG and Knapen, JH and Lee, JC}},
Title = {{Understanding biases when fitting disk truncations}},
Booktitle = {{FORMATION AND EVOLUTION OF GALAXY OUTSKIRTS}},
Series = {{IAU Symposium Proceedings Series}},
Year = {{2016}},
Volume = {{11}},
Number = {{S321}},
