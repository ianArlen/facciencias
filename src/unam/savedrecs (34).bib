@inproceedings{ ISI:000468137600061,
Author = {Flores-Saviaga, Claudia and Savage, Saiph and Taraborelli, Dario},
Book-Group-Author = {{ACM}},
Title = {{LeadWise: Using Online Bots to Recruit and Guide Expert Volunteers}},
Booktitle = {{PROCEEDINGS OF THE 19TH ACM CONFERENCE ON COMPUTER SUPPORTED COOPERATIVE
   WORK AND SOCIAL COMPUTING COMPANION}},
Year = {{2016}},
Pages = {{257-260}},
Note = {{19th ACM Conference on Computer-Supported Cooperative Work and Social
   Computing (CSCW), San Francisco, CA, FEB 27-MAR 02, 2016}},
Organization = {{Assoc Comp Machinery; ACM Special Interest Grp Human Interact}},
Abstract = {{In order to help non-profits recruit volunteers with specialized
   knowledge we propose LeadWise, a system that uses social media bots to
   recruit and guide contributions from experts to assist non-profits in
   reaching their goals. We test the feasibility of using the system to
   recruit specialized volunteers for Wikipedia. We focus in particular on
   experts who can help Wikipedia in its objective of reducing the gender
   gap by covering more women in its articles. Results from our first pilot
   show that LeadWise was able to obtain a noteworthy number of expert
   participants in a two week period with limited requests to targeted
   specialists.}},
