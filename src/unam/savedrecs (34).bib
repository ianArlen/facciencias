@article{ ISI:000367561200001,
Author = {Rodriguez, Ramiro and Morales-Arredondo, Ivan and Rodriguez, Isaias},
Title = {{Geological Differentiation of Groundwater Threshold Concentrations of
   Arsenic, Vanadium and Fluorine in El Bajio Guanajuatense, Mexico}},
