@article{ ISI:000369115800018,
Author = {Guisbiers, Gregory and Mendoza-Cruz, Ruben and Bazan-Diaz, Lourdes and
   Velazquez-Salazar, J. Jesus and Mendoza-Perez, Rafael and Antonio
   Robledo-Torres, Jose and Rodriguez-Lopez, Jose-Luis and Martin
   Montejano-Carrizales, Juan and Whetten, Robert L. and Jose-Yacaman,
   Miguel},
Title = {{Electrum, the Gold-Silver Alloy, from the Bulk Scale to the Nanoscale:
   Synthesis, Properties, and Segregation Rules}},
Journal = {{ACS NANO}},
