@article{ ISI:000364327500006,
Author = {Carlos Ruiz-Segura, Juan and Guillermo Mendoza-Reyes, Leon and de San
   Miguel, Eduardo Rodriguez and Lilia Ocampo, Ana and de Gyves, Josefina},
Title = {{Synthesis and characterization of hybrid membranes based on sulfonated
   poly (ether ether ketone) (SPEEK) and polysiloxane}},
Journal = {{DESALINATION AND WATER TREATMENT}},
Year = {{2015}},
Volume = {{56}},
Number = {{12}},
Pages = {{3153-3159}},
