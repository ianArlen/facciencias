@article{ ISI:000227371000030,
Author = {Pozas, R and Carballo, J and Castro, C and Rubio, J},
Title = {{Synthesis and in vitro antitrypanosomal activity of novel Nifurtimox
   analogues}},
Journal = {{BIOORGANIC \& MEDICINAL CHEMISTRY LETTERS}},
Year = {{2005}},
Volume = {{15}},
Number = {{5}},
Pages = {{1417-1421}},
Month = {{MAR 1}},
Abstract = {{Eight novel analogues of Nifurtimox,
   4-{[}(5-nitrofurfurylidene)amino]-3-methylthiomorpholine-1,1-dioxide,
   containing alfa-beta unsaturated amides, were synthesized and evaluated
   for their in vitro activity against Trypanosoma cruzi epimastigotes.
   Four derivatives bearing a nitro group at the 5-position of the furan
   ring were the most active in inhibiting culture growth and provoking
   cell death, showing trypanocidal activity more than threefold the
   potency of Nifurtimox, our positive control. Two derivatives lacking a
   nitro group were less potent than the positive control. Active nitro
   derivatives very efficiently caused epimastigote death, which suggests a
   nitro reduction mechanism of action. (c) 2005 Elsevier Ltd. All rights
   reserved.}},
