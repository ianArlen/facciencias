@article{ ISI:000228661100001,
Author = {Toledo-Antonio, JA and Pedraza, F and Bokhimi, X},
Title = {{The effect of doping low temperature rutile with tin}},
Journal = {{JOURNAL OF NEW MATERIALS FOR ELECTROCHEMICAL SYSTEMS}},
Year = {{2005}},
Volume = {{8}},
Number = {{2}},
Pages = {{85-90}},
Month = {{APR}},
Abstract = {{Rutile with needle-like crystallites, prepared at low temperature from
   titanium trichloride was doped with tin. Samples were characterized by
   X-ray powder diffraction, and by Transmission electron microscopy; the
   crystalline structure was refined with the Rietveld method. Tin addition
   hindered the growth of the rutile crystallites by occupying the titanium
   vacancy sites generated by hydroxyls incorporated into the lattice
   during the synthesis of pure rutile. Tin introduction gave rise to an
   increase in lattice parameter and changes in oxygen atom positions in
   unit cell, modifying the symmetry of the representative rutile
   titanium-oxygen octahedron. A maxima octahedron deformation was attained
   after annealing the sample at 800 degrees C, collapsing rutile structure
   and segregating SnO2.}},
