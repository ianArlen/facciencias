@article{ ISI:000228706000009,
Author = {Moreno-Corral, MA and Chavarria-K, C and de Lara, E},
Title = {{Low-resolution spectroscopy and uvby-beta photometry of selected stars
   in Haffner 18}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2005}},
Volume = {{41}},
Number = {{1}},
Pages = {{69-76}},
Month = {{APR}},
Abstract = {{We present MK spectral types of a sample of selected (bright) stars
   associated with the galactic cluster Haffner 18 in Puppis. They were
   obtained from low-resolution (lambda/Delta lambda = 1720) spectra by Ha
   slit spectroscopy. Some of the selected stars were also observed
   photoelectrically in the uvby-beta system. Our spectroscopic data show
   that over 1/4 of the estimated 50 star members of the cluster are O and
   early B stars. We give MK spectral types and discuss the individual
   stars observed. Finally, based on these spectroscopic results, on our
   photometry, and on published data we briefly reinspect the physical
   parameters of the cluster.}},
