@article{ ISI:000227341600007,
Author = {Ortega, A},
Title = {{On the moduli space of rank 3 vector bundles on a genus 2 curve and the
   Coble cubic}},
Journal = {{JOURNAL OF ALGEBRAIC GEOMETRY}},
Year = {{2005}},
Volume = {{14}},
Number = {{2}},
Pages = {{327-356}},
Month = {{APR}},
Abstract = {{We prove a conjecture about the moduli space SU(C)(3) of semi-stable
   rank 3 vector bundles with trivial determinant on a genus 2 curve C, due
   to I. Dolgachev. Given C a smooth projective curve of genus 2, and the
   embedding of the Jacobian JC into
   \textbackslash{}3Theta\textbackslash{}, A. Coble proved, at the
   beginning of the 20th century, that there exists a unique cubic
   hypersurface C in \textbackslash{}3Theta\textbackslash{}{*} similar or
   equal to P(8), JC{[}3]-invariant and singular along JC. On the other
   hand, we have a map of degree 2 from SU(C)(3) over
   \textbackslash{}3Theta\textbackslash{} similar or equal to P(8){*},
   ramified along a sextic hypersurface B. Dolgachev's conjecture affirms
   that the sextic B is the dual variety of Coble's cubic C.}},
