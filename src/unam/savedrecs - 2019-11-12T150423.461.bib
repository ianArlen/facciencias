@article{ ISI:000227523100013,
Author = {Holwerda, BW and Gonzalez, RA and Allen, RJ and van der Kruit, PC},
Title = {{The opacity of spiral galaxy disks. III. Automating the synthetic field
   method}},
Journal = {{ASTRONOMICAL JOURNAL}},
Year = {{2005}},
