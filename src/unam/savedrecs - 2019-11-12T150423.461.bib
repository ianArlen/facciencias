@article{ ISI:000230022100002,
Author = {Dostal, J and Keppie, JD and Hamilton, MA and Aarab, EM and Lefort, JP
   and Murphy, JB},
Title = {{Crustal xenoliths in Triassic lamprophyre dykes in western Morocco:
   tectonic implications for the Rheic Ocean suture}},
Journal = {{GEOLOGICAL MAGAZINE}},
Year = {{2005}},
Volume = {{142}},
Number = {{2}},
Pages = {{159-172}},
Month = {{MAR}},
Abstract = {{Dykes of calc-alkaline lamprophyre cutting granite of the Hercynian
   Jebilet Massif of the Moroccan Meseta (western Morocco) contain crustal
   xenoliths. The xenoliths range in composition from mafic (cognate
   cumulates) and upper crustal granitic rocks through gneisses to middle
   crustal felsic granulites. SHRIMP U-Th-Pb zircon analyses of these rocks
   indicate that the dykes were likely intruded during Middle Triassic
   times (similar to 235 Ma), whereas the xenoliths contain zircons with
   concordant Carboniferous-Early Permian, Neoproterozoic and
   Palaeoproterozoic ages (280-328 Ma, c. 540-615 Ma, 700 Ma and similar to
   2000 Ma). The 280-328 Ma ages appear to record synchronous intrusive and
   high-grade (up to granulite facies) Variscan metamorphic events,
   suggesting that high-grade metamorphism may have facilitated the S-type
   granitic magmatism. On the other hand, the similar to 540-615 Ma, 700 Ma
   and 2000 Ma ages correspond with Pan-African and Eburnian orogenic
   events recorded in the West African Craton. In a Triassic
   reconstruction, Morocco is juxtaposed against Nova Scotia (Canada), and
   some have proposed that the basement of the easternmost terrane (Meguma
   terrane) is a piece of the West African craton. However, lower crustal
   xenoliths from Devonian dykes (similar to 370 Ma) cutting the Meguma
   terrane have yielded Late Devonian, Neo- and Mesoproterozoic ages (378
   Ma, 575-629 Ma, similar to 880-1050 Ma and similar to 1530 Ma). The
   presence of similar to I Ga ages suggests that the basement of the
   Meguma terrane is Avalonian rather than West African, implying that in a
   Pangean reconstruction, the Rheic Ocean suture between NW Africa and
   Maritime Canada coincides with the Atlantic Ocean.}},
