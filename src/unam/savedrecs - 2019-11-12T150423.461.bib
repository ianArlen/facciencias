@article{ ISI:000227372600014,
Author = {Zenit, R},
Title = {{Computer simulations of the collapse of a granular column}},
Journal = {{PHYSICS OF FLUIDS}},
Year = {{2005}},
Volume = {{17}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{Recently, two independent groups reported experimental results on the
   process of collapse of a cylindrical granular column. It was found that
   the shape of the final deposit depended mostly on column aspect ratio;
   surprisingly, the frictional properties of the material appeared not to
   influence the results significantly. In this investigation, making use
   of discrete element code, simulations of an equivalent two-dimensional
   system were carried out. The numerical results qualitatively reproduce
   the behavior observed in experiments. Performing an energy balance of
   the system, the different deposit regimes can be discerned. (C) 2005
   American Institute of Physics.}},
