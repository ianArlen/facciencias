@article{ ISI:000228510900001,
Author = {Balcar, B and Hrusak, M},
Title = {{Distributivity of the algebra of regular open subsets of ss
   R\textbackslash{}R}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2005}},
Volume = {{149}},
Number = {{1-3}},
Pages = {{1-7}},
Month = {{APR 28}},
Abstract = {{We compare the structure of the algebras P(omega)/fin and
   Alpha(omega)/Fin, where A denotes the algebra of clopen subsets of the
   Cantor set. We show that the distributivity number of the algebra
   Alpha(omega)/Fin is bounded by the distributivity number of the algebra
   P(omega)/fin and by the additivity of the meager ideal on the reals. As
   a corollary we obtain a result of A. Dow, who showed that in the
