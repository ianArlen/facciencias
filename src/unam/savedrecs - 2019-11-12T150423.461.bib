@article{ ISI:000227735300008,
Author = {Macias, JL and Siebe, C},
Title = {{Popocatepetl's crater filled to the brim: significance for hazard
   evaluation}},
Journal = {{JOURNAL OF VOLCANOLOGY AND GEOTHERMAL RESEARCH}},
Year = {{2005}},
Volume = {{141}},
Number = {{3-4}},
Pages = {{327-330}},
Month = {{MAR 15}},
Abstract = {{After similar to 67 years of quiescence Popocatepetl volcano reawoke on
   December 24, 1994 emitting vigorous ash plumes. Juvenile magma appeared
   for the first time on March 29, 1996, with the emplacement of a dacitic
   lava dome inside the crater. Since then, more than 25 dacitic domes have
   been extruded and destroyed by subsequent explosions. On January 22,
   2001 the activity peaked with a strong explosion that produced
   pyroclastic flows that reached the timberline and ignited fires at a
   distance of 4.5 kin from the crater. These pyroclastic flows were
   contained within the red zone of the volcano hazards map. After 10 years
   of discontinuous activity Popocatepel's crater is almost completely
   filled with lava dome rock. This scenario poses a new threat to the
   populations settled in the orange zone (intermediate hazard level)
   because future explosions will not be contained by the crater walls and
   pyroclastic flows will probably reach greater distances from the crater.
   (c) 2004 Elsevier B.V. All rights reserved.}},
