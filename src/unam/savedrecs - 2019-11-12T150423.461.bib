@article{ ISI:000229654400176,
Author = {Perez, LA and Millan, JS and Wang, CM},
Title = {{A unified description of s-, p- and d-wave superconductivity}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
Year = {{2005}},
Volume = {{359}},
Pages = {{569-571}},
Month = {{APR 30}},
Note = {{International Conference on Strongly Correlated Electron Systems (SCES
   04), Karlsruhe, GERMANY, JUL 26-30, 2004}},
Abstract = {{We report a comparative study of the s-, p- and d-symmetry
   superconducting states in square lattices within a Hubbard model, in
   which correlated-hopping interactions are considered in addition to the
   repulsive Coulomb interactions. This study is carried out by means of
   the BCS formalism. The results show that the first and second neighbors
   correlated hoppings favour s- and d-symmetry pairings, respectively.
   Moreover, an infinitesimal distortion of the right angles in the square
   lattice induces the appearance of a p-wave (spin-triplet)
   superconducting state. (c) 2005 Elsevier B.V. All rights reserved.}},
