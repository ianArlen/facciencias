@article{ ISI:000228706400004,
Author = {Dagdug, L and Garcia-Colin, LS and Goldstein, P},
Title = {{An entropy based theory for the viscosity of strong glasses}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2005}},
Volume = {{51}},
Number = {{2}},
Pages = {{144-148}},
Month = {{APR}},
Abstract = {{One of the main applications of the stochastic matrix method is the
   evaluation of the probability of finding boroxol rings and dendrites in
   a boron oxide glass. In this work, we calculate the configurational
   entropy of B2O3 using this probability and Shannon's definition of
   entropy. The isentropic temperature is evaluated obtaining a very good
   agreement with experimental data. Entropy is introduced into the
   Adam-Gibbs equation to calculate viscosity. The resulting expression
   fits the complete range of temperatures for the supercooled liquid.}},
