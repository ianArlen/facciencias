@article{ ISI:000227602700026,
Author = {Gonzalez, A and Wong, A and Delgado-Salinas, A and Papa, R and Gepts, P},
Title = {{Assessment of inter simple sequence repeat markers to differentiate
   sympatric wild and domesticated populations of common bean}},
