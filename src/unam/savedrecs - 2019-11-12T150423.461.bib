@article{ ISI:000231206500008,
Author = {Aceves, JL and Archundia, A and Diaz, G and Paez, A and Masso, F and
   Alvarado, M and Lopez, M and Aceves, R and Ixcamparij, C and Puente, A
   and Vilchis, R and Montano, LF},
Title = {{Stem cell perspectives in myocardial infarctions}},
Journal = {{REVISTA DE INVESTIGACION CLINICA-CLINICAL AND TRANSLATIONAL
   INVESTIGATION}},
Year = {{2005}},
Volume = {{57}},
Number = {{2}},
Pages = {{156-162}},
Month = {{MAR-APR}},
Abstract = {{Myocardial infarction is the leading cause of congestive heart failure
   and death in industrializated countries. The cellular cardiomyoplasty
   has emerged as an alternative treatment in the regeneration of infarted
   myocardial tissue. In animals' models, differents cellular lines such as
   cardiomyocites, sheletal myoblast, embryonic stem cells and adult
   mesenchymal stem cells has been used, resulting in an improvement in
   ventricular function and decrease in amount of infarted tissue. The
   first three cells line have disvantages as they are allogenics and are
   difficult to obtain. The adult mesenchymal stem cells are autologous and
   can be obtained throught the aspiration of bone marrow or from
   peripherical circulation, prior to stimulating with cytokines (G-CSF).
   The implantation in humans with recent and old myocardial infarction
   have shown improvements similar to those shown in animal models. These
   findings encourage the continued investigation in the mechanism of
   cellular differentiation and implantation metods in infarted myocardial
   tissue.}},
