@article{ ISI:000229008400016,
Author = {El Albani, A and Vachard, D and Fursich, F and Buitron, B and de Dios,
   AF},
Title = {{Depositional environment and biofacies characterization of the Upper
   Pennsylvanian-Lower Permian deposits of the San Salvador Patlanoaya
   section (Puebla, mexico)}},
