@article{ ISI:000228621300003,
Author = {Lopez de la Torre, L and Winkler, B and Schreuer, J and Knorr, K and
   Avalos-Borja, M},
Title = {{Elastic properties of tantalum carbide (TaC)}},
Journal = {{SOLID STATE COMMUNICATIONS}},
Year = {{2005}},
Volume = {{134}},
Number = {{4}},
Pages = {{245-250}},
Month = {{APR}},
Abstract = {{Elastic properties of TaC have been investigated experimentally and by
   model calculations. The elastic stiffness coefficients c(11)=597(11) GPa
   and c(44)=153(2) GPa were determined on a (100)-oriented disc-shaped
   monocrystal at room temperature using a plane-wave ultrasound method.
   The corresponding theoretical values (c(11)=621(3), c(44)=166.8(3) GPa)
   agree within 4 and 8\%, respectively. Therefore, we are confident that
   the predicted value for c(12) is equally accurate, and this allows the
   prediction of the Bulk and Young's moduli and the Poisson ratio. Data
   published earlier are critically reviewed and predictions concerning the
   possibility to synthesize extremely incompressible carbides are made.
   (c) 2005 Elsevier Ltd. All rights reserved.}},
