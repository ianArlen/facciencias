@article{ ISI:000228234200013,
Author = {Alvarez-Rubio, S and Benito, JJ and Sanchez-Sesma, FJ and Alarcon, E},
Title = {{The use of direct boundary element method for gaining insight into
   complex seismic site response}},
Journal = {{COMPUTERS \& STRUCTURES}},
Year = {{2005}},
Volume = {{83}},
Number = {{10-11}},
Pages = {{821-835}},
Month = {{APR}},
Abstract = {{The boundary element method is specially well suited for the analysis of
   the seismic response of valleys of complicated topography and
   stratigraphy. In this paper the method's capabilities are illustrated
   using as an example an irregularity stratified (test site) sedimentary
   basin that has been modelled using 2D discretization and the Direct
   Boundary Element Method (DBEM).
   Site models displaying different levels of complexity are used in
   practice. The multi-layered model's seismic response shows generally
   good agreement with observed data amplification levels, fundamental
   frequencies and the high spatial variability. Still important features
   such as the location of high frequencies peaks are missing. Even 2D
   simplified models reveal important characteristics of the wave field
   that 1D modelling does not show up. (c) 2005 Elsevier Ltd. All rights
   reserved.}},
