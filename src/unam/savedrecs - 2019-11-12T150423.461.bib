@article{ ISI:000228126500010,
Author = {Cruz, SA and Diaz-Garcia, C and Pathak, AP and Soullard, J},
Title = {{Pressure dependence of the mean excitation energy of atomic systems}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2005}},
Volume = {{230}},
Number = {{SI}},
Pages = {{46-52}},
Month = {{APR}},
Note = {{21st International Conference on Atomic Collisions in Solids, Genova,
   ITALY, JUL 04-09, 2004}},
Abstract = {{Changes in the mean ionization energy of atoms in compressed matter are
   estimated through cage models of atomic confinement whereby pressure is
   obtained in terms of the rate of change of total atomic energy with
   volume. Resort is taken to a recently implemented shellwise
   Thomas-Fermi-Dirac Weizsacker theory for confined atoms to construct the
   atomic energy functional, which is self-consistently optimized for
   different confinement conditions. The resulting modified atomic orbital
   densities are then used within the Local Plasma Approximation to
   evaluate the corresponding orbital and total mean excitation energies.
   Good agreement is obtained with accurate calculations for free atoms.
   For compressed atoms agreement is found with a previously derived
   universal expression {[}J.M. Peek, Phys. Rev. A 36 (1987) 54291 for
   total mean excitation energies suggesting its adequacy for this class of
   studies. (c) 2004 Elsevier B.V. All rights reserved.}},
