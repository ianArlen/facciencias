@article{ ISI:000228194200038,
Author = {Sole, J and Pi, T},
Title = {{An empirical calibration for He-4 quantification in minerals and rocks
   by laser fusion and noble gas mass spectrometry using Cerro de Mercado
   (Durango, Mexico) fluorapatite as a standard}},
Journal = {{ANALYTICA CHIMICA ACTA}},
Year = {{2005}},
Volume = {{535}},
Number = {{1-2}},
Pages = {{325-330}},
Month = {{APR 11}},
Abstract = {{An empirical calibration with a natural mineral standard (fluorapatite
   from Cerro de Mercado, Mexico) is proposed as a method to determine the
   He-4 concentration of mineral and rock samples. The procedure is based
   on the fusion of several aliquots of the fluorapatite standard with a
   well-spaced weight distribution in order to obtain a good correlation in
   coordinates of 4 He peak height versus fluorapatite weight. The weight
   is then converted to moles using the accepted mineral age (31.4 Ma) and
   appropriate formula. Experimental peak height of 4 He for the unknown
   samples are converted to moles with the regression determined for
   fluorapatite. The procedure is fast and inexpensive, and both precision
   and accuracy are always below 10\% and usually about 3-5\%. (c) 2004
   Elsevier B.V. All rights reserved.}},
