@article{ ISI:000228313100004,
Author = {Jorquera, AP and Martinez, GDM and Pelaez, CV and Zinn, RA},
Title = {{Factors that influence the feed value of supplemental fats used in
   feedlot cattle: A review}},
Journal = {{INTERCIENCIA}},
Year = {{2005}},
Volume = {{30}},
Number = {{3}},
Pages = {{134+}},
Month = {{MAR}},
Abstract = {{Except for associative interactions, the NE value of dietary fat is
   largely a function of its intestinal digestibility. Thus, attention has
   been directed to understanding intrinsic (nature and fat quality) and
   extrinsic (fat method addition and inclusion level) factors that
   influence its value as feed. Among these factors, fat intake level has
   the greatest impact on its energy value. When total fat intake exceeds
   0.96g/kg BW, the NE of fat decreases linearly, as a result of the
   decline in postruminal fatty acid digestibility, mainly of C18:0; the
   latter probably explained by a limited capacity of bile production in
   the ruminant at high levels of fat intake.}},
