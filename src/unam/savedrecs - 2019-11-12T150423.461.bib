@article{ ISI:000228791300025,
Author = {Mejia-Uriarte, EV and Camarillo, E and Hernandez, AJ and Navarrete, M
   and Villagran-Muniz, M and Murrieta, SH},
Title = {{Thermal dissolution of Eu2+ Suzuki phase nanostructures in KBr crystals
   monitored by pulsed photoacoustic and photoluminescence techniques}},
Journal = {{OPTICAL MATERIALS}},
Year = {{2005}},
Volume = {{27}},
Number = {{7}},
Pages = {{1316-1319}},
Month = {{APR}},
