@article{ ISI:000228740500009,
Author = {Debeila, MA and Raphulu, MC and Mokoena, E and Avalos, M and
   Petranovskii, V and Coville, NJ and Scurrell, MS},
Title = {{The influence of gold on the optical properties of sol-gel derived
   titania}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2005}},
Volume = {{396}},
Number = {{1-2}},
Pages = {{70-76}},
Month = {{APR 15}},
Abstract = {{The absorption spectra of TiO(2) and Au-TiO(2) have been measured as a
   function of calcination temperature in order to study the effect of Au
   on the band gap energy of TiO(2). Our results indicate that a signature
   of the presence of Au is a decrease in the band gap energy of TiO(2) to
   a value less than the commonly quoted band gap energies of bulk anatase
   and rutile. In addition, a drastic decrease in the band gap energy of
   Au-TiO(2) is observed with samples heated at 700 degrees C in an argon
   atmosphere, which is attributed to the influence of defects in the
   structure of TiO(2). The reduction in the band gap energy of TiO(2) in
   the presence of Au is correlated to enhanced photo chemical processes
   previously described and is associated with a strong interaction between
   Au and TiO(2). (c) 2005 Elsevier B.V. All rights reserved.}},
