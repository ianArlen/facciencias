@article{ ISI:000228661100008,
Author = {Sanchez-Ramirez, JF and Diaz, G and Vazquez, A and Pal, U},
Title = {{Preparation and growth mechanism study of polymer protected Au/Pd
   bimetallic nanoparticles by simultaneous reduction of HAuCl4 and PdCl2}},
Journal = {{JOURNAL OF NEW MATERIALS FOR ELECTROCHEMICAL SYSTEMS}},
Year = {{2005}},
Volume = {{8}},
Number = {{2}},
Pages = {{127-131}},
Month = {{APR}},
Abstract = {{We report on the preparation of polymer-protected gold-palladium
   bimetallic particles in colloidal form by the reduction of corresponding
   metal ions simultaneously Formation of nanoparticles and their
   constitution were studied by x-ray diffraction (XRD), transmission
   electron microscopy (TEM) and optical absorption spectroscopy
   techniques. The average size of the particles depended strongly on the
   molar ratio of two metal ions in the reaction mixture. XRD study
   revealed that the Au/Pd colloids prepared with different molar ratios
   form some partially alloyed structure of those metals. A high
   concentration of Pd ions in the reaction mixture produces non-dispersed
   small bimetallic colloids. Mechanism of formation of bimetallic Au/Pd
   particles has been studied.}},
