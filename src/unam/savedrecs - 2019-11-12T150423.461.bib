@article{ ISI:000227255300009,
Author = {Soto, G and Aparicio, E and Avalos-Borja, M},
Title = {{Correlation functions between specific volume and stoichiometry for
   transition metal nitrides}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2005}},
Volume = {{389}},
Number = {{1-2}},
Pages = {{42-46}},
Month = {{MAR 8}},
Abstract = {{A methodology is proposed to correlate the structural aspects of
   transition metal nitrides (TMN) to the stoichiometric ratio: x =
   {[}N]/{[}M]. The method is based on a numeric figure, v, given by the
   difference between the atomic concentrations of nitride and parent metal
   normalized to the atomic concentration of parent metal. Numerical
   regression is used to construct interpolating functions for v(x) using
   as input the available data for TMN in two well-recognized databases
   (ICDD and ICSD). In summary we obtain functions of x that describe the
   deformation caused in the parent metal lattice by the nitrogen
   assimilation. The results are attractive, since TMN show remarkable
   trends. (C) 2004 Elsevier B.V. All rights reserved.}},
