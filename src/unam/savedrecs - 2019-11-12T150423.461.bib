@article{ ISI:000227766900047,
Author = {Portelles, J and Garcia, S and Martinez, E and Raymond, O and Almodovar,
   NS and Heiras, JL and Siqueiros, JM},
Title = {{Aging behavior in Ba0.7Sr0.3Ti0.97Zr0.03O ceramics}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2005}},
Volume = {{97}},
Number = {{5}},
Month = {{MAR 1}},
Abstract = {{The polarization aging behavior of the Ba0.7Sr0.3Ti0.97Zr0.03O3 ceramic
   that presents diffuse phase transition, was investigated at different
   temperatures. The aging of this system is explained by assuming the
   existence of two different relaxation regimes, one associated with
   rapidly relaxing polarized macroregions (tau(1)) and the other to long
   time relaxing polarized microregions (tau(2)). With these
   considerations, a modified expression to describe the aging behavior is
   proposed, leading to a good fit to the experimental measurements of the
   polarization. Hysteresis loop, permittivity, and dielectric loss
   measurements were performed on the samples to support the aging study.
   (C) 2005 American Institute of Physics.}},
