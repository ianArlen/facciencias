@article{ ISI:000229119800015,
Author = {Pal, U and Santiago, P and Chavez, J and Ascencio, JA},
Title = {{Structure and growth mechanism study of wurtzite CdSe nanorods grown by
   solvothermal techniques}},
Journal = {{JOURNAL OF NANOSCIENCE AND NANOTECHNOLOGY}},
Year = {{2005}},
Volume = {{5}},
Number = {{4}},
Pages = {{609-614}},
Month = {{APR}},
Abstract = {{Hexagonal cadmium selenide nanorods 32-170 nm in length and 12-28 nm in
   diameter were grown by a solvothermal technique by reacting cadmium
   chloride and selenium powders in ethylenediamine at 140 \&DEG; C. Using
   electron microscopy, the shape, size, and crystal structure of the
   nanorods were determined. A detailed microscopy analysis revealed that
   the nanorods grow through the coalescence of very small particles or
   fiberlike structures of the compound. The size, shape and strain
   structures in the nanorods are defined by their growth mechanism.}},
