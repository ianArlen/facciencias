@article{ ISI:000226858300006,
Author = {Avendano-Vazquez, SEA and Garcia-Caballero, A and Garcia-Sainz, JA},
Title = {{Phosphorylation and desensitization of the lysophosphatidic acid
   receptor LPA(1)}},
Journal = {{BIOCHEMICAL JOURNAL}},
Year = {{2005}},
Volume = {{385}},
Number = {{3}},
Pages = {{677-684}},
Month = {{FEB 1}},
Abstract = {{In C9 cells, LPA (lysophosphatidic acid) induced inositol phosphate
   production, increased intracellular calcium concentration and inhibited
   adenylate cyclase activity. These responses were abolished in cells
   challenged with active phorbol esters. Action of phorbol e;ters was
   blocked by inhibitors of PKC (protein kinase C) and by its
   down-regulation. LPA(1) receptor phosphorylation was observed in
   response to phorbol esters. The effect was rapid (t(1/2) similar to 1
   min), intense (2-fold) and sustained (at least 60 min). PKC inhibitors
   markedly decreased the LPA(1) receptor phosphorylation induced by
   phorbol esters. LPA(1) receptor tagged with the green fluorescent
   protein internalized in response to PKC activation. In addition, LPA and
   angiotensin 11 were also capable of inducing LPA(1) receptor
   phosphorylation, showing that LPA(1) receptor can be subjected to
   homologous and heterologous desensitization.}},
