@article{ ISI:000357906400020,
Author = {Lopez-Aguilar, J. E. and Webster, M. F. and Tamaddon-Jahromi, H. R. and
   Manero, O.},
Title = {{High-Weissenberg predictions for micellar fluids in
   contraction-expansion flows}},
