@article{ ISI:000358097200024,
Author = {Alvarez, Guzman and Martinez, Jennyfer and Varela, Javier and Birriel,
   Estefania and Cruces, Eugenia and Gabay, Martin and Leal, Sandra M. and
   Escobar, Patricia and Aguirre-Lopez, Beatriz and Cabrera, Nallely and
   Tuena de Gomez-Puyou, Marietta and Gomez Puyou, Armando and
   Perez-Montfort, Ruy and Yaluff, Gloria and Torres, Susana and Serna,
   Elva and Vera de Bilbao, Ninfa and Gonzalez, Mercedes and Cerecetto,
   Hugo},
Title = {{Development of bis-thiazoles as inhibitors of triosephosphate isomerase
   from Trypanosoma cruzi. Identification of new non-mutagenic agents that
   are active in vivo}},
Journal = {{EUROPEAN JOURNAL OF MEDICINAL CHEMISTRY}},
Year = {{2015}},
Volume = {{100}},
Pages = {{246-256}},
Month = {{JUL 15}},
Abstract = {{The neglected disease American trypanosomiasis is one of the major
   health problems in Latin America. Triosephosphate isomerase from
   Trypanosoma cruzi (TcTIM), the etiologic agent of this disease, has been
   proposed as a druggable target. Some bis-benzothiazoles have been
   described as irreversible inhibitors of this enzyme. On the other hand,
   new bioactive furane-containing thiazoles have been described as
   excellent in vivo anti-T. cruzi agents. This encouraged us to design and
   develop new bis-thiazoles with potential use as drugs for American
   trypanosomiasis. The bis-thiazol 5,
   3,3'-allyl-2,2'-bis{[}3-(2-furyl)-2-propenylidenehydrazono]-2,2',3,3'-te
   trahydro-4,4'-bisthiazole, showed the best in vitro anti-T. cruzi
   profile with a higher selectivity index than the reference drugs
   Nifurtimox and Benznidazole against amastigote form of the parasite.
   This derivative displayed marginal activity against TcTIM however the
   bis-thiazol 14,
   3-allyl-2{[}3-(2-furyl)-2-propenylidenehydrazono]-3'-phenyl-2-(3-phenyl-
   2-propenylidenehydrazono]-2,2',3,3'-tetrahydro-4,4'-bisthiazole, was an
   excellent inhibitor of the enzyme of the parasite. The absence of both
   in vitro mutagenic and in vivo toxicity effects, together with the
   activity of bis-thiazol 5 in vivo, suggests that this compound is a
   promising anti-T cruzi agent surpassing the ``hit-to-lead{''} stage in
   the drug development process. (C) 2015 Elsevier Science Ltd. All rights
   reserved. (C) 2015 Published by Elsevier Masson SAS.}},
