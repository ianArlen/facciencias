@article{ ISI:000358657100004,
Author = {Paz-Martinez, Gaudencio and Garduno-Mejia, Jesus and Kolokoltsev, Oleg
   V. and Trevino-Palacios, Carlos G. and Qureshi, Naser},
Title = {{Focus and Alignment Tolerance in a Photoconductive Terahertz Source}},
Journal = {{JOURNAL OF INFRARED MILLIMETER AND TERAHERTZ WAVES}},
Year = {{2015}},
Volume = {{36}},
Number = {{9}},
Pages = {{830-837}},
Month = {{SEP}},
Abstract = {{Robust coupling between a pulsed laser beam and a photoelectric circuit
   is an important issue in the development of miniaturized, integrated,
   and embedded terahertz instrumentation. Here, we present a study of the
   effect of varying the focus and alignment parameters of an excitation
   laser pulse on the emission characteristics of a standard
   Hertzian-dipole type terahertz photoelectric source. The objective is to
   quantify the tolerance of a terahertz time-domain spectroscopy system,
   and we study the variation of peak amplitude, waveform, phase, and
   energy distribution as a function of excitation position and defocus. We
   find that a terahertz source can be made relatively tolerant to
   variations in focus, alignment, and details of the geometry of the
   photoelectric system, providing a window for a more robust field
   operation.}},
