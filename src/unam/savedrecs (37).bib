@article{ ISI:000357798100005,
Author = {Guerrero-Vargas, Natali N. and Guzman-Ruiz, Mara and Fuentes, Rebeca and
   Garcia, Joselyn and Salgado-Delgado, Roberto and del Carmen Basualdo,
   Maria and Escobar, Carolina and Markus, Regina P. and Buijs, Ruud M.},
Title = {{Shift Work in Rats Results in Increased Inflammatory Response after
   Lipopolysaccharide Administration: A Role for Food Consumption}},
Journal = {{JOURNAL OF BIOLOGICAL RHYTHMS}},
