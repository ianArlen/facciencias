@article{ ISI:000356751500018,
Author = {Garcia, C. R. and Diaz-Torres, L. A. and Salas, P. and Guzman, M. and
   Angeles-Chavez, C.},
Title = {{Photoluminescent and photocatalytic properties of bismuth doped
   strontium aluminates blended with titanium dioxide}},
Journal = {{MATERIALS SCIENCE IN SEMICONDUCTOR PROCESSING}},
Year = {{2015}},
Volume = {{37}},
Number = {{SI}},
Pages = {{105-111}},
Month = {{SEP}},
Note = {{23rd International-Materials-Research-Congress (IMRC 2014), Cancun,
   MEXICO, AUG 17-21, 2014}},
Abstract = {{Bismuth co-doped long persistent phosphor (LPP) powders were obtained by
   a combustion synthesis technique followed by a post-annealing under
   carbon atmosphere. Bismuth content was varied from 1.0 to 15.0 mol\%.
   X-ray diffraction analysis revealed that the powders show mainly a
   mixture of three phases: the SrAl2O4, the SrAl14O25 and the Sr2Al6O11
   crystalline phases. Photocatalyst composites were obtained by wet mixing
   of TiO2 anatase and LIP powders followed by annealing in air at 450
   degrees C. Photoluminescence measured spectra under 380 nm excitation
   show a tunable emission from green (510 nm) to greenish-blue (463 nm) in
   which peak wavelength localization is related to the Bi content.
   Photoluminescence intensity decreases as Bi content increases.
   Degradation of methylene blue solutions, irradiated by UV light (254
   nm), was monitored by the decrease of its 650 nm absorption peak in
   regular periods. The best photocatalytic activity is observed when in
   the composite blend a 2.0 mol\% of Bi content was used, and complete
   methylene degradation is reached after 210 mm. These photocatalyst
   composite powders are potential candidates to clean-up wastewater
   applications, and might be potential candidates for photocatalytic
   hydrogen generation in aqueous solutions. (C) 2015 Elsevier Ltd. All
   rights reserved.}},
