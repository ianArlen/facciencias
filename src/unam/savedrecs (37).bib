@article{ ISI:000361655100063,
Author = {Perez-Villegas, A. and Pichardo, B. and Moreno, E.},
Title = {{STELLAR ORBITAL STUDIES IN NORMAL SPIRAL GALAXIES. II. RESTRICTIONS ON
   STRUCTURAL AND DYNAMICAL PARAMETERS ON SPIRAL ARMS}},
