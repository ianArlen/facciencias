Author = {Pereza, Eduardo and Ayele, Lijalem and Getachew, Girum and Fetter,
   Geolar and Bosch, Pedro and Mayoral, Alvaro and Diaz, Isabel},
Title = {{Removal of chromium(VI) using nano-hydrotalcite/SiO2 composite}},
Journal = {{JOURNAL OF ENVIRONMENTAL CHEMICAL ENGINEERING}},
