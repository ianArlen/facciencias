@article{ ISI:000360071600009,
Author = {Rovito, Sean M. and Parra-Olea, Gabriela and Recuero, Ernesto and Wake,
   David B.},
Title = {{Diversification and biogeographical history of Neotropical plethodontid
   salamanders}},
Journal = {{ZOOLOGICAL JOURNAL OF THE LINNEAN SOCIETY}},
Year = {{2015}},
Volume = {{175}},
Number = {{1}},
Pages = {{167-188}},
Month = {{SEP}},
Abstract = {{The Neotropical bolitoglossine salamanders represent an impressive
   adaptive radiation, comprising roughly 40\% of global salamander species
   diversity. Despite decades of morphological studies and molecular work,
   a robust multilocus phylogenetic hypothesis based on DNA sequence data
   is lacking for the group. We estimated species trees based on multilocus
   nuclear and mitochondrial data for all major lineages within the
   bolitoglossines, and used our new phylogenetic hypothesis to test
   traditional biogeographical scenarios and hypotheses of morphological
   evolution in the group. In contrast to previous phylogenies, our results
   place all Central American endemic genera in a single clade and suggest
   that Central America played a critical role in the early biogeographical
   history of the group. The large, predominantly Mexican genus
   Pseudoeurycea is paraphyletic, and analyses of the nuclear data place
   two lineages of Pseudoeurycea as the sister group of Bolitoglossa. Our
   phylogeny reveals extensive homoplasy in morphological characters, which
   may be the result of truncation or alteration of a shared developmental
   trajectory. We used our phylogenetic results to revise the taxonomy of
   the genus Pseudoeurycea.(c) 2015 The Linnean Society of London}},
