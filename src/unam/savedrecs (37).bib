@article{ ISI:000369713700013,
Author = {Garcia-Valles, M. and Pi, T. and Alfonso, P. and Canet, C. and Martinez,
   S. and Jimenez-Franco, A. and Tarrago, M. and Hernandez-Cruz, B.},
Title = {{Kaolin from Acoculco (Puebla, Mexico) as raw material: Mineralogical and
   thermal characterization}},
