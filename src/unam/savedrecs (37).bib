@article{ ISI:000358391000011,
Author = {Gabriela Gomez-Vasconcelos, Martha and Hugo Garduno-Monroy, Victor and
   Luis Macias, Jose and Layer, Paul W. and Benowitz, Jeff A.},
Title = {{The Sierra de Mil Cumbres, Michoacan, Mexico: Transitional volcanism
   between the Sierra Madre Occidental and the Trans-Mexican Volcanic Belt}},
Journal = {{JOURNAL OF VOLCANOLOGY AND GEOTHERMAL RESEARCH}},
Year = {{2015}},
Volume = {{301}},
Pages = {{128-147}},
Month = {{AUG 15}},
Abstract = {{The Sierra de Mil Cumbres is a Miocene volcanic range located in central
   Mexico, in the north-eastern part of the State of Michoacan, near the
   city of Morelia. Structurally it is a ENE-trending horst that covers an
   area of 1022 km(2) (approximately 20 km wide x 60 km long) and contains
   exposures of chemically-bimodal volcanism in the form of ignimbrites,
   lava domes, lava flows, cinder cones, and related deposits. The main
   volcanic manifestations of this range are the La Escalera Caldera
   (16.3-23 Ma), the Garnica Volcanic Complex (18.3-17.9 Ma), the Atecuaro
   Caldera (16.3-19.4 Ma), and the Indaparapeo Volcanic Complex (14.1-17.5
   Ma). The Sierra de Mil Cumbres stands in space and time at the
   intersection between the Miocene-Recent Trans-Mexican Volcanic Belt and
   the Late Cretaceous-Early Miocene Sierra Madre Occidental, and so
   provides new insights into the geological evolution of central Mexico.
   Arc volcanism in the Sierra de Mil Cumbres was initiated by a massive
   NNW-SSE extension, probably during the counterclockwise rotation of the
   Sierra Madre Occidental. New geological mapping, stratigraphic analysis,
   detailed geochemistry and Ar-40/Ar-39 geochronology demonstrates that
   this intra-plate volcanism was emplaced between 14 and 23 Ma. (C) 2015
   Elsevier B.V. All rights reserved.}},
