@article{ ISI:000453157400065,
Author = {Chirino, Yolanda I. and Sanchez-Perez, Yesennia and Roman
   Osornio-Vargas, Alvaro and Rosas, Irma and Maria Garcia-Cuellar, Claudia},
Title = {{Sampling and composition of airborne particulate matter (PM10) from two
   locations of Mexico City}},
Journal = {{DATA IN BRIEF}},
Year = {{2015}},
Volume = {{4}},
Pages = {{353-356}},
Month = {{SEP}},
Abstract = {{The PM10 airborne particulate matter with an aerodynamic diameter <= 10
   mu m is considered as a risk factor of various adverse health outcomes,
   including lung cancer. Here we described the sampling and composition of
   PM to collected from an industrial zone (IZ), and a commercial zone (CZ)
   of Mexico City. The PM10 was collected with a high-volume sampler in the
   above mentioned locations and both types of PM10 sampled were
   characterized by the content of polycyclic aromatic hydrocarbons (PAHs),
   metals, and endotoxin. The endotoxin PM10 content from IZ and CZ
   displayed 138.4 UE/mg and 170.4 UE/mg of PM10, respectively. (C) 2015
   The Authors. Published by Elsevier Inc.}},
