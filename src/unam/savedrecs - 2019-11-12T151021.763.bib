@article{ ISI:A1995QK33700005,
Author = {JUNG, C and MEJIAMONASTERIO, C and SELIGMAN, TH},
Title = {{SCATTERING ONE-STEP FROM CHAOS}},
Journal = {{PHYSICS LETTERS A}},
Year = {{1995}},
Volume = {{198}},
Number = {{4}},
Pages = {{306-314}},
Month = {{MAR 6}},
Abstract = {{An infinite set of aligned hard discs of decreasing radius and distance
   is an example of a scattering system with a compact scatterer that has
   an infinite number of periodic orbits with the special property that no
   trajectory leads from one of these orbits to the next. This makes the
   system integrable; that is, there is no topological chaos despite the
   infinite number of unstable periodic orbits. We shall see that some of
   the usual features of chaotic scattering are present while others are
   not.}},
