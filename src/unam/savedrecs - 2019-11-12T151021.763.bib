@article{ ISI:A1995QL15400033,
Author = {ZHANG, LM and AGUILARROBLERO, R},
Title = {{ASYMMETRICAL ELECTRICAL-ACTIVITY BETWEEN THE SUPRACHIASMATIC NUCLEI
   IN-VITRO}},
Journal = {{NEUROREPORT}},
Year = {{1995}},
Volume = {{6}},
Number = {{3}},
Pages = {{537-540}},
Month = {{FEB 15}},
Abstract = {{IN order to test the hypothesis that each side of the suprachiasmatic
   nuclei (SCN) contains independent circadian oscillators, pairs of cells
   were recorded from contralateral SCN alternating each nucleus, in
   hypothalamic slices prepared for extracellular single unit recording.
   The analysis was made by separating the data from right and left nuclei.
   Circadian variations in the electrical activity of neurones were present
   in either side of the SCN. The right SCN presented a bimodal waveform,
   without apparent change of phase between slices prepared from animals
   under LD or DD. In contrast, the fluctuations of the left SCN were
   unimodal in slices of both groups. There was also a difference of 2 h in
   the interval of maximal activity between the groups from LD and DD.
   These asymmetrical wave forms are consistent with previous studies
   suggesting that the contralateral SCN may act as independent
   oscillators.}},
