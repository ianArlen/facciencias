@article{ ISI:A1995RF69000008,
Author = {CAMPIGOTTO, C and SMIRNOV, YF and ENIKEEV, SG},
Title = {{Q-ANALOG OF THE KRAWTCHOUK AND MEIXNER ORTHOGONAL POLYNOMIALS}},
