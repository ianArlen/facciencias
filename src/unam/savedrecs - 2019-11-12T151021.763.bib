@article{ ISI:A1995QE97400004,
Author = {PAEZOSUNA, F and RUIZFERNANDEZ, C},
Title = {{COMPARATIVE BIOACCUMULATION OF TRACE-METALS IN PENAEUS-STYLIROSTRIS IN
   ESTUARINE AND COASTAL ENVIRONMENTS}},
Journal = {{ESTUARINE COASTAL AND SHELF SCIENCE}},
Year = {{1995}},
Volume = {{40}},
Number = {{1}},
Pages = {{35-44}},
Month = {{JAN}},
Abstract = {{Trace metal concentrations (Fe, Mn, Ni, Cu, Co, Cd, Cr and Zn) have been
   measured in estuarine and marine shrimp P. stylirostris collected in the
   Pacific coast of Mexico. Estuarine individuals (juveniles) had higher
   concentrations of Fe and Mn than marine individuals (adults).
   Size-dependent relationships were observed and differed among the
   elements examined. A negative slope was found for Co, Fe, Mn and Ni in
   estuarine juvenile shrimps, while for Cu the opposite tendency occurred.
   In marine adults a positive slope was observed for Cd, Co, Cr and Cu.
   These findings may be due to two factors: (1) that P. stylirostris
   spends part of its life-cycle in estuarine/lagoon environments where it
   is more likely to be exposed to higher levels of bioavailable trace
   metals (natural and anthropogenic contributions) and/or (2) different
   metabolic requirements of young and older specimens, which is especially
   applicable to copper.}},
