@article{ ISI:A1995QE10600007,
Author = {HUELSZ, G and RAMOS, E},
Title = {{ON THE PHASE DIFFERENCE OF THE TEMPERATURE AND PRESSURE WAVES IN THE
   THERMOACOUSTIC EFFECT}},
Journal = {{INTERNATIONAL COMMUNICATIONS IN HEAT AND MASS TRANSFER}},
Year = {{1995}},
Volume = {{22}},
Number = {{1}},
Pages = {{71-80}},
Month = {{JAN-FEB}},
Abstract = {{In this comment, the single-plate, linear theory for the thermoacoustic
   phenomenon in ideal conditions, Prandtl Number (sigma) zero and a plate
   with infinite heat capacity, presented by Swift {[}I] has been used to
   find expressions for the phase difference alpha between the temperature
   and pressure waves. The effect of Prandtl Number different from zero and
   a plate with a finite heat capacity has also been analyzed. It has been
   found that the behavior is governed by the relation between a
   nondimensional temperature gradient Gamma and sigma. Attenuation occurs
   for Gamma < 1 + root sigma and excitation for Gamma > 1 + root sigma
   which correspond to \textbackslash{}alpha\textbackslash{} < pi/2 and
   \textbackslash{}alpha\textbackslash{} > pi/2 respectively. Explicit
   expressions for alpha as a function of the transverse coordinate are
   given. A physical interpretation of the results is presented in the
   context of concepts offered by Lord Rayleigh.}},
