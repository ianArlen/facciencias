@inproceedings{ ISI:A1995BD87Y00040,
Author = {BRAVO, S and STEWART, GA},
Editor = {{Culhane, JL and Hiei, E}},
Title = {{THE SOLAR MAGNETIC CYCLE AND THE PRESENCE OF AN INCLINED RELIC FIELD IN
   THE SUN}},
Booktitle = {{SOLAR FLARE, CORONAL AND HELIOSPHERIC DYNAMICS}},
Series = {{ADVANCES IN SPACE RESEARCH}},
Year = {{1995}},
Volume = {{17}},
Number = {{4/5}},
Pages = {{221-223}},
Note = {{E2.2 Symposium of COSPAR Scientific Commission E on Solar Flare, Coronal
   and Heliospheric Dynamics, at the 13th COSPAR Scientific Assembly,
   HAMBURG, GERMANY, JUL 11-21, 1994}},
Organization = {{Comm Space Res; Int Astron Union; Sci Comm Solar Terrestrial Phys}},
Abstract = {{The evolution of the solar magnetic field through its 22 year cycle
   shows a varying inclination of the magnetic equator at 2.5 Rs from about
   0 degrees during solar minimum up to 90 degrees during solar maximum, as
   measured with respect to the solar equator. We show that this behaviour
   could be explained by the presence of a small dipolar relic field which
   has a high inclination with respect to the solar rotation axis and
   points southward. This fossil field would lead to a larger polar field
   during the negative polarity phase of the cycle, in accordance with
   observations. It may also help to explain the asymmetry observed in the
   solar activity of the northern and southern hemispheres, the appearance
   of some particularly active longitudes on the Sun, as well as other
   asymmetrical characteristics of the solar activity cycles.}},
