@article{ ISI:A1995QE81600003,
Author = {ZHANG, LM and AGUILARROBLERO, R and BARRIO, RA and MAINI, PK},
Title = {{RHYTHMIC FIRING PATTERNS IN SUPRACHIASMATIC NUCLEUS (SCN) - THE ROLE OF
   CIRCUIT INTERACTIONS}},
Journal = {{INTERNATIONAL JOURNAL OF BIO-MEDICAL COMPUTING}},
Year = {{1995}},
Volume = {{38}},
Number = {{1}},
Pages = {{23-31}},
Month = {{JAN}},
Abstract = {{The suprachiasmatic nucleus (SCN) is believed to contain the main
   generator of circadian rhythmicity in mammals. In order to obtain
   further functional details of this, electrophysiological extracellular
   measurements in vitro were made. By means of an interspike interval
   distribution analysis, it is shown that there is a novel kind of
   neuronal firing pattern: the harmonic pattern. From these observations,
   we have developed a theoretical model based on possible filtering
   processes occurring during synaptic transmission. The model suffices to
