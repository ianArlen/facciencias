@article{ ISI:A1995QG50800012,
Author = {MUSLIMOV, A and PAGE, D},
Title = {{DELAYED SWITCH-ON OF PULSARS}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1995}},
Volume = {{440}},
Number = {{2, 2}},
Pages = {{L77-L80}},
Month = {{FEB 20}},
Abstract = {{We consider the ohmic diffusion of the internal magnetic flux through
   the outermost nonmagnetic layers of the crust of a newly born neutron
   star. We assume that at the moment of star formation or soon after, the
   magnetic field of strength similar to 10(12) G is submerged in a closed
   configuration at similar to 100-150 m (similar to 200-300 m) under the
   stellar surface. Due to outward ohmic diffusion of an internal field the
   initial surface magnetic field, of strength similar to 3 x 10(8) G, is
   enhanced by a factor of similar to 3 x 10(3) in a few hundred (thousand)
   years. As a consequence of the enhancement of the surface magnetic
   field, the spin-down luminosity of a similar to 2 ms pulsar increases
   from its initial value of 2 x 10(35) ergs s(-1) by a factor of a few
   times 10(5)-10(6) at ages similar to 10(2)-10(3) yr and then decreases.
   The calculations were performed for a model of thermal evolution of a
   1.4 M. neutron star cooling via the standard modified Urea process. The
   implication of this effect for young neutron stars as potential
   gamma-ray sources is briefly discussed.}},
