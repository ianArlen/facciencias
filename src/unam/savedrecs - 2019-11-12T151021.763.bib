@article{ ISI:A1995UH46400003,
Author = {Guevara, Y and Garrido, A and Reyes, A and Mares, G and Rueda, E},
Title = {{Interbehavioral program for correcting articulation, reading and writing}},
Journal = {{REVISTA INTERAMERICANA DE PSICOLOGIA}},
Year = {{1995}},
Volume = {{29}},
Number = {{2}},
Pages = {{177-190}},
Abstract = {{A program for simultaneous correction of articulation, reading and
   writing problems was applied to, fifteen children of first grade. Tile
   close relationship between learning school problems, articulation and
   linguistic mistakes is emphasized. This program is based on the
   principles of behavior theory and the use of a modified interbehavioral
   model. Activities, materials and words were outlined to promote
   interactions between teachers, children and contexts. Results showed
   that progress achieved by all children was significant. The principles
   of interbehavioral theory regarding education are discussed and
   analized.}},
