@article{ ISI:A1995QD64800025,
Author = {PORTA, H and LIZARDI, PM},
Title = {{AN ALLOSTERIC HAMMERHEAD RIBOZYME}},
Journal = {{BIO-TECHNOLOGY}},
Year = {{1995}},
Volume = {{13}},
Number = {{2}},
Pages = {{161-164}},
Month = {{FEB}},
Abstract = {{We have constructed an RNA molecule containing a hammerhead ribozyme
   that is under allosteric control. In the inactive state, the RNA enzyme
   is unable to cleave a suitable substrate. The formation of the active
   state of the ribozyme is triggered by a specific interaction with a DNA
   oligonucleotide effector that is complementary to a single-stranded loop
   in the RNA enzyme molecule. Other DNA or RNA molecules containing
   unrelated nucleotide sequences do not function as allosteric effecters.
   This work demonstrates the feasibility of designing RNA enzymes that are
   specifically activated in response to an artificially designed molecular
   recognition event. Such enzymes may have practical applications.}},
