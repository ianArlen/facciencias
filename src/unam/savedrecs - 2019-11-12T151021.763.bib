@article{ ISI:A1994PZ24700004,
Author = {BRACHO, F and DROSTE, M},
Title = {{LABELED DOMAINS AND AUTOMATA WITH CONCURRENCY}},
Journal = {{THEORETICAL COMPUTER SCIENCE}},
Year = {{1994}},
Volume = {{135}},
Number = {{2}},
Pages = {{289-318}},
Month = {{DEC 12}},
Abstract = {{We investigate an operational model of concurrent systems, called
   automata with concurrency relations. These are labelled transition
   systems A in which the event set is endowed with a collection of binary
   concurrency relations which indicate when two events, in a particular
   state of the automation, commute. This model generalizes asynchronous
   transition systems, and as in trace theory we obtain, through a
   permutation equivalence for computation sequences of A, an induced
   domain (D(A), less-than-or-equal-to). Here, we construct a categorical
   equivalence between a large category of (''cancellative'') automata with
   concurrency relations and the associated domains. We show that each
   cancellative automation can be reduced to a minimal cancellative
   automation generating, up to isomorphism, the same domain. Furthermore,
   when fixing the event set, this minimal automation is unique.}},
