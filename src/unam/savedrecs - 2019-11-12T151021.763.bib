@article{ ISI:A1995QJ93300056,
Author = {SOSA, A and CELIS, H},
Title = {{H+/PPI STOICHIOMETRY OF MEMBRANE-BOUND PYROPHOSPHATASE OF
   RHODOSPIRILLUM-RUBRUM}},
Journal = {{ARCHIVES OF BIOCHEMISTRY AND BIOPHYSICS}},
Year = {{1995}},
Volume = {{316}},
Number = {{1}},
Pages = {{421-427}},
Month = {{JAN 10}},
Abstract = {{Two kinetic methods have been used to measure the H+/PPi stoichiometry
   in the chomatophores of the photosynthetic bacteria Rhodospirillum
   rubrum. In the first method, the fluorescent probe acridine orange was
   employed to infer the proton pump activity at the steady state of the
   Delta pH generation. At this point the translocation of protons by the
   H+-PP(i)ase in one direction is balanced exactly by the leak of protons
   in the opposite direction. Pyrophosphatase activity was then quickly
   stopped by adding EDTA, producing a relaxation of Delta pH. From the
   initial rate of this relaxation and the rate of PPi hydrolysis measured
   under the same condition, the H+/PPi stoichiometry was obtained. In the
   second method, a mathematical model was used to describe the time course
   of Delta pH formation. In the two methods an apparent H+/PPi
   stoichiometry of nearly 2 was obtained, The H+/ATP stoichiometry was
   determined also as an internal control, giving a value of nearly 3.6,
   which is in agreement with the value in different F-type H+-ATPases. (C)
   1995 Academic Press, Inc.}},
