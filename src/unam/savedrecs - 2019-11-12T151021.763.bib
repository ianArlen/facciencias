@article{ ISI:A1994PV55200038,
Author = {FRANCO, J and SHORE, SN and TENORIOTAGLE, G},
Title = {{ON THE MASSIVE STAR-FORMING CAPACITY OF MOLECULAR CLOUDS}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1994}},
Volume = {{436}},
Number = {{2, 1}},
Pages = {{795-799}},
Month = {{DEC 1}},
Abstract = {{Assuming that photoionization is the self-limiting process for continued
   star formation, we estimate the maximum number of massive (OB) stars
   that can form within a molecular cloud. The most efficient cloud
   destruction mechanism in the early stages of H II region evolution is
   the evaporation of the cloud by stars located near the cloud boundary.
   The maximum number of OB stars is of order 1 per 10(4) M. of average
   molecular gas, or 10 per 10(4) M. of dense molecular gas. The resulting
   star-forming efficiencies within cloud complexes range from 2\% to 16\%,
   depending on both the location of the stars in the cloud and the details
   of the initial mass function, with an overall value of about 5\% for
   average molecular gas.}},
