@article{ ISI:A1995QP13100001,
Author = {TERRONES, H},
Title = {{THE FLIPPING OF CORANNULENE C20H10 THROUGH SURFACE TRANSFORMATIONS AND
   THE STABILITY OF C-20 ISOMERS}},
Journal = {{FULLERENE SCIENCE AND TECHNOLOGY}},
Year = {{1995}},
Volume = {{3}},
Number = {{2}},
Pages = {{107-114}},
Abstract = {{The inversion of corannulene C20H10 has been studied using two geometric
   transformations and the Monte Carlo method. The transformations explain
   the possible energetic states followed by the molecule during its
   flattening. It is found that the inversion of corannulene is
   probabilistic when the flat state has been reached. The stability of
   C-20 (cage), corannulene-like, ring of 20 atoms and a negatively curved
   segment with 20 atoms is discussed. We find that the corannulene-like
   arrangement is the most stable. This has important implications in the
   formation of C-60 and other fullerenes.}},
