@article{ ISI:A1995UN14500005,
Author = {Siebe, C},
Title = {{Heavy metal availability to plants in soils irrigated with wastewater
   from Mexico City}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{1995}},
Volume = {{32}},
Number = {{12}},
Pages = {{29-34}},
