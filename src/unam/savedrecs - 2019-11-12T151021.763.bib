@article{ ISI:A1995QE79400005,
Author = {ARITA, IH and CASTANO, VM and WILKINSON, DS},
Title = {{SYNTHESIS AND PROCESSING OF HYDROXYAPATITE CERAMIC TAPES WITH CONTROLLED
   POROSITY}},
Journal = {{JOURNAL OF MATERIALS SCIENCE-MATERIALS IN MEDICINE}},
Year = {{1995}},
Volume = {{6}},
Number = {{1}},
Pages = {{19-23}},
Month = {{JAN}},
Abstract = {{Hydroxyapatite (Ca-10(PO4)(6)(OH)(2)) ceramic sheets with a wide range
   of porosities (up to 62\%) have been prepared. The process is based on
   the reaction between dicalcium phosphate (CaHPO4) and calcium carbonate
   (CaCO3). When mixed with the appropriate Ca/P ratio, this proves to be a
   reliable new method for obtaining hydroxyapatite. Moreover, CaCO3 serves
   as a gas-forming agent (due to the evolution of carbon dioxide and water
   during the reaction), which leads to the development of highly porous
   microstructures. Alternatively, CaHPO4 and CaCO3 can be reacted by
   calcining at 1000 degrees C to produce pure hydroxyapatite powders. When
   processed in a similar way, a dense ceramic results. By mixing 50 vol\%
   of CaCO3 and CaHPO4 with precalcined powders, hydroxyapatite with an
   intermediate porosity was obtained. Moreover, it should be possible to
   achieve porosity control by mixing different amounts of uncalcined and
   precalcined powders. All of these powders are colloidally processed
   using tape casting to produce thin sheets 150-200 mu m thick. This
   technique can be used to make laminates, with or without porosity
   gradients, up to several millimetres thick.}},
