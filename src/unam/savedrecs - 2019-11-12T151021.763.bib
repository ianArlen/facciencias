@article{ ISI:A1994PU79100008,
Author = {DULTZINHACYAN, D and BENITEZ, E},
Title = {{NEAR-INFRARED AND FAR-INFRARED EMISSION OF SEYFERT-2 GALAXIES AND
   VARIOUS TYPES OF STAR-FORMING GALAXIES}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{1994}},
Volume = {{291}},
Number = {{3}},
Pages = {{720-724}},
Month = {{NOV}},
Abstract = {{In order to study the origin of the near and far infrared emission of
   Seyfert 2 galaxies, we carried out near (J, H, K) infrared photometry of
   a small sample of Seyfert 2 and various types of star-forming galaxies,
   selected according to a criterium developed previously (papers I and II)
   that reveals enhanced star formation. All the galaxies have also good
   detections in IRAS bands 25, 60 and 100 mu m.
   We found a strong correlation between all the near and far luminosities
   for the Seyfert 2 galaxies. In order to explain the position of the
   galaxies in the J-H, H-K diagram as well as the above mentioned
   correlation, we conclude that their near and far infrared emission is
   dominated by a post-starburst emission.}},
