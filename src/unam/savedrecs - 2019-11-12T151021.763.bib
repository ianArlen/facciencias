@inproceedings{ ISI:A1995BG94E00017,
Author = {Miroshnichenko, LI},
Book-Group-Author = {{INT UNION PURE \& APPL PHYS}},
Title = {{Upper intensity-energy limits and utmost spectrum for solar cosmic rays}},
Booktitle = {{24TH INTERNATIONAL COSMIC RAY CONFERENCE, VOL 4: SH SESSIONS -
   CONTRIBUTED PAPERS}},
Year = {{1995}},
Pages = {{62-65}},
Note = {{24th International Cosmic Ray Conference (XXIV ICRC), ROME, ITALY, AUG
   28-SEP 08, 1995}},
Organization = {{Minist Univ Ric Sci; Ist Nazl Fis Nucl; Consiglio Nazl Ric; Univ
   Sapienza, Roma; Univ Roma 3; Accad Nazl Lincei; Int Union Pure \& Appl
   Phys; Int Ctr Theoret Phys; Commiss European Communities, DG XII; ICSC,
   World Lab; Comune Roma; Prov Roma; Banca Roma; PolHiTech; CAEN}},
Abstract = {{Based on direct and indirect observation data at the Earth's orbit the
   integral energy spectra of solar cosmic rays (SCR) are analyzed within
   entire energy range of order 1 MeV - 10 GeV (for protons) for the most
   powerful solar proton events (SPE) of 1942-1992 period. The methodical
   difficulties of such an analysis are discussed: observation accuracy,
   SCR anisotropy, proton energy density in non-relativistic region, upper
   energy limit for registered protons etc. Taking into account a set of
   methodical uncertainties and energy dependence of spectral index we
   present an empirical curve for the utmost SCR spectrum near the Earth.
   The curve may be fitted by a power-law function, the index being depend
   on the energy and the spectrum is significantly steepening at high
   energies.}},
Publisher = {{ARGALIA EDITORE DELLE ARTI GRAFICHE EDITORIALI SRL}},
Address = {{ 61029 URBINO, ITALY}},
