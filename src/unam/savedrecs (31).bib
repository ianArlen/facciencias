@article{ ISI:000367277000043,
Author = {Martinez-Merlin, I. and Guzman-Mendoza, J. and Garcia-Hipolito, M. and
   Sanchez-Resendiz, V. M. and Lartundo-Rojas, L. and Fragoso, R. J. and
   Falcony, C.},
Title = {{Transparent and low surface roughness HfO2: Tb3+, Eu3+ luminescent thin
   films deposited by USP technique}},
Journal = {{CERAMICS INTERNATIONAL}},
Year = {{2016}},
Volume = {{42}},
Number = {{2, A}},
Pages = {{2446-2455}},
Month = {{FEB 1}},
Abstract = {{In this work, the optical and morphological properties of HfO2: Tb3+,
   Eu3+ thin films deposited by the Ultrasonic Spray Pyrolysis technique
   from metal-organic precursors are reported. The films showed optical
   average transmittance values in the visible region greater than 90\%,
   with surface roughness lower than 3.9 nm. The films deposited at 500
   degrees C showed the lowest average roughness with a value of 0.9 nm,
   and the smallest thickness was 35 nm for sample deposited at 500 degrees
   C during 45 s. XRD measurements indicate a hafnium oxide monoclinic
   phase for films deposited at substrate temperatures higher than 500
   degrees C. All films deposited showed the luminescent emissions (PL and
   CL) characteristic of Tb3+ and Eu3+ ions. A luminescence concentration
   quenching was observed for both Tb3+ and Eu3+ ions. The HfO2: Tb3+ (5
   at\%) and HfO2: Eu3+ (10 at\%) films deposited at 500 degrees C, showed
   the highest PL and CL emission intensity. Quantum Efficiency
   measurements (up to about 35\%) were measured for these films which have
   a refractive index between 1.97 and 2.04 and band gap of 5.4 eV. The
   chemical composition of the films as measured by XPS is also reported.
   In addition, decay time measurements were performed on some HfO2: Tb3+,
   Eu3+ samples. (C) 2015 Elsevier Ltd and Techna Group S.r.l. All rights
   reserved.}},
