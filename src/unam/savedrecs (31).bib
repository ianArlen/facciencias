@article{ ISI:000367608000005,
Author = {Illanes, A.},
Title = {{Connected Open Neighborhoods in Products}},
Journal = {{ACTA MATHEMATICA HUNGARICA}},
Year = {{2016}},
Volume = {{148}},
Number = {{1}},
Pages = {{73-82}},
Month = {{FEB}},
Abstract = {{Let X and Y be metric continua. We consider the following property ({*})
   if M is a subcontinuum of X x Y such that pi (X) (M) = X and pi (Y) (M)
   = Y, where pi (X) and pi (Y) are the respective projections on X and Y,
   then M has small open connected neighborhoods in X x Y. Answering
   questions by D. P. Bellamy and J. M. Aysko, we prove that (a) the
   product of the pseudo-arc and any Knaster continuum has property ({*}),
   and (b) there exist chainable indecomposable continua X and Y such that
   X x Y does not have property ({*}).}},
Publisher = {{SPRINGER}},
