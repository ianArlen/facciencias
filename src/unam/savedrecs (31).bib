@article{ ISI:000406489800009,
Author = {Ramirez Rancano, Mario},
Title = {{Miguel Palomar y Vizcarra and the secret society ``Justice and
   Action{''}: the assassination attempt on archbishop Pascual Diaz Barreto}},
Journal = {{BOLETIN AMERICANISTA}},
Year = {{2016}},
Number = {{73}},
Pages = {{159-179}},
Abstract = {{In 1932, Alberto Maria Carreno wrote a book defending Pascual Diaz
   Barreto, Archbishop of Mexico, against previous attacks by the Liga
   Nacional Defensora de la Libertad Religiosa (Religious Liberty National
   Defense League) headed by Miguel Palomar y Vizcarra. This institution
   accused the Archbishop of making a deal with the Mexican government in
   the context of the Cristero movement cease fire. Carreno noted that the
   League's directives had created a secret society called Justicia y
   Accion, and that their aim was to assassinate the Archbishop during the
   presidential elections of 1929. This paper's focus is the acquiescence
   of Diaz Barreto to put a torch to Carreno's book to stop the criticism
   put forth by the Liga Nacional.}},
