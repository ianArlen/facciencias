@article{ ISI:000366205900024,
Author = {Cuellar Martinez, Tomasa del Carmen and Rodriguez, Rosalba Alonso and
   Voltolina, Domenico and Morquecho, Lourdes},
Title = {{Effectiveness of coagulants-flocculants for removing cells and toxins of
   Gymnodinium catenatum}},
Journal = {{AQUACULTURE}},
Year = {{2016}},
Volume = {{452}},
Pages = {{188-193}},
Month = {{FEB 1}},
Abstract = {{Laboratory experiments compared the removal efficiency of cells and
   toxins of the dinoflagellate Gymnodinium catenatum, using three
   concentrations of cells and three concentrations of red clay,
   montmorillonite, clinoptilolite, calcium oxide, calcium hydroxide,
   aluminium sulfate, chitosan, activated silica, and polyaluminium
   chloride, as coagulating or flocculating agents. Chitosan, calcium
   oxide, and calcium hydroxide were selected using the criterion of
   minimum concentration for maximum efficiency for removing cells. These
   were combined in pairs to measure their joint performance. In all cases,
   removal efficiency of cells was 95-99\%. A combination of calcium oxide
   + chitosan effectively removed the cells, but failed to remove
   extracellular toxins produced by G. catenatum. (C) 2015 Elsevier B.V.
   All rights reserved.}},
