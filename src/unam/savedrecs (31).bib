@article{ ISI:000389711100001,
Author = {Solis Avila, Juan Carlos and Sheinbaum Pardo, Claudia},
Title = {{Energy consumption and CO2 emission from road transport in Mexico and
   mitigation scenarios}},
Journal = {{REVISTA INTERNACIONAL DE CONTAMINACION AMBIENTAL}},
Year = {{2016}},
Volume = {{32}},
Number = {{1}},
Pages = {{7-23}},
Month = {{FEB}},
Abstract = {{In 2012, transportation accounted for 39 \% of CO2 emissions associated
   with the production and use of energy in Mexico, which road transport
   accounted for 92 \%. Based on a ``bottom-up{''} model, this article
   presents baseline and mitigation scenarios of CO2 emissions by 2050. An
   improvement in vehicle fuel efficiency and the shift from the private
   transportation to public transportation systems are analyzed as
   mitigation measures. However, to achieve a significant decrease in the
   growth rate of emissions, the vehicle fleet (or its usage) should be
   reduced.}},
