@article{ ISI:000369066300016,
Author = {Garcia-Ramirez, E. V. and Almaguer-Valenzuela, S. and Sanchez-Dena, O.
   and Baldovino-Pantaleon, O. and Reyes-Esqueda, J. A.},
Title = {{Third-order nonlinear optical properties of colloidal Au nanorods
   systems: saturable and reverse-saturable absorption}},
