@article{ ISI:000367776500008,
Author = {Reynoso-Cereceda, Greta I. and Garcia-Cabrera, Ramses I. and
   Valdez-Cruz, Norma A. and Trujillo-Roldan, Mauricio A.},
Title = {{Shaken flasks by resonant acoustic mixing versus orbital mixing: Mass
   transfer coefficient k(L)a characterization and Escherichia coli
   cultures comparison}},
Journal = {{BIOCHEMICAL ENGINEERING JOURNAL}},
Year = {{2016}},
Volume = {{105}},
