@article{ ISI:000412536900010,
Author = {Arcos Casarrubias, J. A. and Reyes-Mayer, A. and Guardian-Tapia, R. and
   Castillo-Ocampo, P. and Romo-Uribe, A.},
Title = {{Rubber Nanodomains Reinforced Epoxy Resin}},
