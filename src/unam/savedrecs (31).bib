@inproceedings{ ISI:000397958000011,
Author = {Lopez, J. A. and Richer, M. G. and Pereyra, M. and Garcia-Diaz, M. T.},
Editor = {{Kwok, S and Leung, KC}},
Title = {{The kinematic evolution of the ionized shells of planetary nebulae}},
Booktitle = {{11TH PACIFIC RIM CONFERENCE ON STELLAR ASTROPHYSICS: PHYSICS AND
   CHEMISTRY OF THE LATE STAGES OF STELLAR EVOLUTION, PTS 1-6}},
Series = {{Journal of Physics Conference Series}},
