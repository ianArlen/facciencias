@article{ ISI:000371398600014,
Author = {Clapp, Monica and Tiwari, Sweta},
Title = {{Multiple solutions to a pure supercritical problem for the p-Laplacian}},
Journal = {{CALCULUS OF VARIATIONS AND PARTIAL DIFFERENTIAL EQUATIONS}},
Year = {{2016}},
Volume = {{55}},
Number = {{1}},
Month = {{FEB}},
Abstract = {{Under some symmetry assumptions, we establish existence and multiplicity
   of positive and sign changing solutions to the quasilinear problem
   -Delta pv = vertical bar v vertical bar(q-2) v in Omega, v=0 on partial
   derivative Omega,
   in some bounded smooth domains Omega in R-N, for 1 < p < N and some
   supercritical exponents q > p{*} := Np/N-p. Related to this question is
   the existence of solutions to the anisotropic critical problem
   -div(a(x) vertical bar del u vertical bar(p-2) del u) = c(x) vertical
   bar u vertical bar(p{*}-2) u in Omega, u = 0 on partial derivative
   Omega,
   where a and c are positive continuous functions on (Omega) over bar. We
   study the lack of compactness for this problem in a symmetric setting,
   and establish existence and multiplicity of positive and sign changing
   solutions by means of a suitable variational principle.}},
