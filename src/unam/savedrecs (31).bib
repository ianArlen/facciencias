@inproceedings{ ISI:000400182000003,
Author = {Yanez, David and Botero, Eduardo},
Editor = {{Shehata, HF and Santillan, DY and Shehata, MF}},
Title = {{Implications of the Seismic Coefficients in High Dams Constructed in
   Mexico}},
Booktitle = {{GEO-CHINA 2016: NEW FRONTIERS IN CIVIL INFRASTRUCTURE}},
Series = {{Geotechnical Special Publication}},
Year = {{2016}},
Number = {{267}},
Pages = {{18-25}},
