@inproceedings{ ISI:000398425500067,
Author = {Fabian Rosales-Ortega, F. and Castillo, Edgar and Sanchez, Sebastian F.
   and Iglesias-Paramo, Jorge and Molla, Mercedes and Chavez, Miguel},
Editor = {{Skillen, I and Balcells, M and Trager, SC}},
Title = {{Beyond MOS and Fibers: Wide-FoV Imaging Fourier Transform Spectroscopy -
   an Instrumentation Proposal for the Present and Future Mexican
   Telescopes}},
Booktitle = {{MULTI-OBJECT SPECTROSCOPY IN THE NEXT DECADE: BIG QUESTIONS, LARGE
   SURVEYS, AND WIDE FIELDS}},
Series = {{Astronomical Society of the Pacific Conference Series}},
Year = {{2016}},
Volume = {{507}},
Pages = {{435-438}},
Note = {{Conference on Multi-Object Spectroscopy in the Next Decade: Big
   Questions, Large Surveys, and Wide Fields, Teatro Circo de Marte, Santa
   Cruz de la Palma, SPAIN, MAR 02-06, 2015}},
Organization = {{Isaac Newton Grp Telescopes}},
Abstract = {{In order to extend the current suite of instruments offered in the
   Observatorio Astroffsico Guillermo Haro (OAGH) in Cananea, Mexico
   (INAOE), and to explore a second-generation instrument for the future
   6.5 m Telescopio San Pedro Martir (TSPM), we propose a prototype
   instrument that will provide un-biased wide-field (few arcmin)
   spectroscopic information, with the flexibility of operating at
   different spectral resolutions (R similar to 1 - 10(4)), with a spatial
   resolution limited by seeing, and therefore to be used in a wide range
   of astronomical problems. This instrument will make use of the Fourier
   Transform Spectroscopy technique, which has been proved to be feasible
   in the optical wavelength range. Here we give the basic technical
