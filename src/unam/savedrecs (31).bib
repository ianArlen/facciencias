@article{ ISI:000368042200014,
Author = {Perez-Campuzano, Enrique and Avila-Foucat, V. S. and Perevochtchikova,
   Maria},
Title = {{Environmental policies in the pen-urban area of Mexico City: The
   perceived effects of three environmental programs}},
Journal = {{CITIES}},
Year = {{2016}},
Volume = {{50}},
Pages = {{129-136}},
Month = {{FEB}},
Abstract = {{Managing pen-urban areas is complicated due to the unique rural and
   urban characteristics and because sectorial policies are not always
   compatible. In Mexico City, pen-urban spaces (particularly the
   Conservation Zone in the south of the city) provide important ecosystem
   services for urban residents. However, despite this environmental
   importance, the Conservation Zone suffers from land-use changes as a
   result of the economic transition from rural to urban activities.
   Different government agencies have implemented environmental programs
   attempting to address this problem. The present paper focuses on the
   beneficiaries' perceptions of the effects of three such programs. The
   results demonstrate the importance of the conservation programs but at
   the same time show numerous unresolved issues, including excessive
   administrative fulfillments, social and political conflicts, and a lack
   of coherence among programs. An altemative could be an integrated
   spatial and environmental planning process in which federal and local
   authorities, beneficiaries, and city inhabitants participate. (C) 2015
   Elsevier Ltd. All rights reserved.}},
