Title = {{SOFTWARE FOR LAND EVALUATION (SOIL, WATER AND CLIMATE)}},
Booktitle = {{PROCEEDINGS FROM INTERNATIONAL CONFERENCE: SOIL - THE NON-RENEWABLE
   ENVIRONMENTAL RESOURCE}},
Year = {{2016}},
