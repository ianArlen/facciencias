@article{ ISI:000366388100279,
Author = {Diaz-Jaimes, Pindaro and Uribe-Alcocer, Manuel and Hinojosa-Alvarez,
   Silvia and Sandoval-Laurrabaquio, Nadia and Adams, Douglas H. and Garcia
   De Leon, Francisco J.},
Title = {{The complete mitochondrial DNA of the bull shark (Carcharhinus leucas)}},
Journal = {{MITOCHONDRIAL DNA PART A}},
Year = {{2016}},
Volume = {{27}},
Number = {{1}},
Pages = {{717-718}},
Month = {{JAN 2}},
Abstract = {{The bull shark mitochondrial structure is similar to that of other
   elasmobranchs; it has a total length of 16,100 bp, the base composition
   of the genomes was as follows: A (31.35\%), T (31.35\%), C (24.38\%) and
