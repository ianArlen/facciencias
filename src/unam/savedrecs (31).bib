@article{ ISI:000371248600018,
Author = {Danehkar, A. and Parker, Q. A. and Steffen, W.},
Title = {{FAST, LOW-IONIZATION EMISSION REGIONS OF THE PLANETARY NEBULA M2-42}},
Journal = {{ASTRONOMICAL JOURNAL}},
