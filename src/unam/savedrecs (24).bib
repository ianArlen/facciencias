@article{ ISI:000387521000016,
Author = {Carrillo-Vargas, Armando and Perez-Enriquez, Roman and Lopez-Montes,
   Rebeca and Rodriguez-Martinez, Mario and Gerardo Ugalde-Calvillo, Luis},
Title = {{Radio signal anomalies detected with MEXART in 2012 during the recovery
   phase of geomagnetic storms}},
Journal = {{ADVANCES IN SPACE RESEARCH}},
Year = {{2016}},
Volume = {{58}},
Number = {{10}},
Pages = {{2057-2066}},
Month = {{NOV 15}},
Abstract = {{In this work we present MEXART observations in 2012 from 17 radio
   sources in which we detected anomalies in the radio signal of these
   sources occurring during the recovery phase of some geomagnetic storms.
   We performed FFT and wavelet analysis of the radio signals during these
   periods and found that rather than IPS the anomalies seem to originate
   in the ionosphere, especially because of the frequencies at which they
   are observed. We discuss this results under the view that the source of
   the geomagnetic storm is no longer in the interplanetary medium. (C)
   2016 COSPAR. Published by Elsevier Ltd. All rights reserved.}},
