@article{ ISI:000386488200010,
Author = {Bower, Geoffrey C. and Loinard, Laurent and Dzib, Sergio and Galli,
   Phillip A. B. and Ortiz-Leon, Gisela N. and Moutou, Claire and Donati,
   Jean-Francois},
Title = {{VARIABLE RADIO EMISSION FROM THE YOUNG STELLAR HOST OF A HOT JUPITER}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2016}},
Volume = {{830}},
Number = {{2}},
Month = {{OCT 20}},
Abstract = {{We report the discovery of variable radio emission associated with the T
   Tauri star, V830 Tau, which was recently shown to host a hot Jupiter
   companion. Very Large Array observations at a frequency of 6 GHz reveal
   a detection on 2011 May 1 with a flux density 919 +/- 26 mu Jy, along
   with non-detections in two other epochs at <66 and <150 mu Jy.
   Additionally, Very Long Baseline Array observations include one
   detection and one non-detection at comparable sensitivity, demonstrating
   that the emission is nonthermal in origin. The emission is consistent
   with the gyro-synchrotron or synchrotron mechanism from a region with a
   magnetic field greater than or similar to 30 G and is likely driven by
   an energetic event such as magnetic reconnection that accelerated
   electrons. With the limited data we have, we are not able to place any
   constraint on the relationship between the radio emission and the
   rotational or orbital properties of V830 Tau. This is the first
   detection of radio emission from a non-degenerate star known to host an
   exoplanet.}},
