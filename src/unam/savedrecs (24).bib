@article{ ISI:000385175000006,
Author = {Manuel Guevara-Vela, Jose and Romero-Montalvo, Eduardo and Costales,
   Aurora and Martin Pendas, Angel and Rocha-Rinza, Tomas},
Title = {{The nature of resonance-assisted hydrogen bonds: a quantum chemical
   topology perspective}},
