@article{ ISI:000387521000002,
Author = {Denardini, Clezio Marcos and Dasso, Sergio and Americo Gonzalez-Esparza,
   J.},
Title = {{Review on space weather in Latin America. 1. The beginning from space
   science research}},
Journal = {{ADVANCES IN SPACE RESEARCH}},
