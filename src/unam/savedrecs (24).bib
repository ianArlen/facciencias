@article{ ISI:000390101200023,
Author = {Escamilla-Herrera, Lenin F. and Mena-Barboza, Eri A. and Torres-Arenas,
   Jose},
Title = {{Thermodynamics of Noncommutative Quantum Kerr Black Holes}},
Journal = {{ENTROPY}},
Year = {{2016}},
Volume = {{18}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{The thermodynamic formalism for rotating black holes, characterized by
   noncommutative and quantum corrections, is constructed. From a
   fundamental thermodynamic relation, the equations of state and
   thermodynamic response functions are explicitly given, and the effect of
   noncommutativity and quantum correction is discussed. It is shown that
   the well-known divergence exhibited in specific heat is not removed by
   any of these corrections. However, regions of thermodynamic stability
   are affected by noncommutativity, increasing the available states for
   which some thermodynamic stability conditions are satisfied.}},
Publisher = {{MDPI AG}},
Address = {{ST ALBAN-ANLAGE 66, CH-4052 BASEL, SWITZERLAND}},
Type = {{Article}},
Language = {{English}},
