@article{ ISI:000388473700029,
Author = {Bonillaa, Cesar and Ma, Ernest and Peinado, Eduardo and Valle, Jose W.
   F.},
Title = {{Two-loop Dirac neutrino mass and WIMP dark matter}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2016}},
Volume = {{762}},
Pages = {{214-218}},
Month = {{NOV 10}},
Abstract = {{We propose a ``scotogenic{''} mechanism relating small neutrino mass and
   cosmological dark matter. Neutrinos are Dirac fermions with masses
   arising only in two-loop order through the sector responsible for dark
   matter. Two triality symmetries ensure both dark matter stability and
   strict lepton number conservation at higher orders. A global
   spontaneously broken U(1) symmetry leads to a physical Diraconthat
   induces invisible Higgs decays which add up to the Higgs to dark matter
   mode. This enhances sensitivities to spin-independent WIMP dark matter
   search below m(h)/2. (C) 2016 The Authors. Published by Elsevier B.V.}},
