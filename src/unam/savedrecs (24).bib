@article{ ISI:000388542100005,
Author = {Michelitsch, T. M. and Collet, B. A. and Riascos, A. P. and Nowakowski,
   A. F. and Nicolleau, F. C. G. A.},
Title = {{A fractional generalization of the classical lattice dynamics approach}},
