Author = {Friis, Guillermo and Aleixandre, Pau and Rodriguez-Estrella, Ricardo and
   Navarro-Siguenza, Adolfo G. and Mila, Borja},
Title = {{Rapid postglacial diversification and long-term stasis within the
   songbird genus Junco: phylogeographic and phylogenomic evidence}},
Journal = {{MOLECULAR ECOLOGY}},
