@article{ ISI:000389284800011,
Author = {Podolak, Morris and Flandes, Alberto and Della Corte, Vincenzo and
   Krueger, Harald},
Title = {{A simple model for understanding the DIM dust measurement at comet
   67P/Churyumov-Gerasimenko}},
Journal = {{PLANETARY AND SPACE SCIENCE}},
Year = {{2016}},
Volume = {{133}},
Number = {{SI}},
Pages = {{85-89}},
Month = {{NOV 15}},
Abstract = {{We present a simple model for gas and dust flow from
   67P/Churyumov-Gerasimenko that can be used to understand the grain
   impact observed by the DIM instrument on Philae (Kruger et al., 2015).
   We show how model results when applied to the GIADA measurements
   (Rotundi et al., 2015; Della Corte et al., 2015) can be used, in
   conjunction with the results found by the MIRO (Schloerb et al., 2015)
   and VIRTIS (De Sanctis et al., 2015) instruments to infer surface
   properties such as surface temperature and surface ice fraction. (C)
   2016 Elsevier Ltd. All rights reserved.}},
