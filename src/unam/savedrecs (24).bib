Title = {{Exploring the different facets of plant endemism in the South-Eastern
   Carpathians: a manifold approach for the determination of biotic
   elements, centres and areas of endemism}},
Journal = {{BIOLOGICAL JOURNAL OF THE LINNEAN SOCIETY}},
Year = {{2016}},
Volume = {{119}},
Number = {{3}},
