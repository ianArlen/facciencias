@article{ ISI:000386363500006,
Author = {Fraija, N. and Marinelli, A.},
Title = {{NEUTRINO, gamma-RAY, AND COSMIC-RAY FLUXES FROM THE CORE OF THE CLOSEST
   RADIO GALAXIES}},
