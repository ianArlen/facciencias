@article{ ISI:000397324600010,
Author = {Dell'Aquila, D. and Acosta, L. and Auditore, L. and Cardella, G. and De
   Filippo, E. and De Luca, S. and Francalanza, L. and Gnoffo, B. and
   Lanzalone, G. and Lombardo, I. and Martorana, N. S. and Norella, S. and
   Pagano, A. and Pagano, E. V. and Papa, M. and Pirrone, S. and Politi, G.
   and Quattrocchi, L. and Rizzo, F. and Russotto, P. and Trifiro, A. and
   Trimarchi, M. and Verde, G. and Vigilante, M.},
Title = {{New experimental investigation of cluster structures in Be-10 and C-16
   neutron-rich nuclei}},
Journal = {{NUOVO CIMENTO C-COLLOQUIA AND COMMUNICATIONS IN PHYSICS}},
Year = {{2016}},
Volume = {{39}},
Number = {{6}},
Month = {{NOV-DEC}},
Abstract = {{The existence of cluster structures in (10) Be and C-16 neutron-rich
   isotopes is investigated via projectile break-up reactions induced on
   polyethylene (CH2) target. We used a fragmentation beam constituted by
   55MeV/u Be-10 and 49 MeV/u C-16 beams provided by the FRIBs facility at
   INFN-LNS. Invariant mass spectra of He-4+He-6 and He-6+Be-10 breakup
   fragments are reconstructed by means of the CHIMERA 4 pi detector to
   investigate the presence of excited states of projectile nuclei
   characterized by cluster structure. In the first case, we suggest the
   presence of a new state in Be-10 at 13.5 MeV. A non-vanishing yield
   corresponding to 20.6 MeV excitation energy of C-16 was observed in the
   He-6+Be-10 cluster decay channel. To improve the results of the present
   analysis, a new experiment has been performed recently, taking advantage
   of the coupling of CHIMERA and FARCOS. In the paper we describe the data
   reduction process of the new experiment together with preliminary
   results.}},
