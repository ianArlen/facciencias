@article{ ISI:000388073300057,
Author = {Aguilar-Arnal, Lorena and Ranjit, Suman and Stringari, Chiara and
   Orozco-Solis, Ricardo and Gratton, Enrico and Sassone-Corsi, Paolo},
Title = {{Spatial dynamics of SIRT1 and the subnuclear distribution of NADH
