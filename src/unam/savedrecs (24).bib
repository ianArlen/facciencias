@article{ ISI:000397324600029,
Author = {Pagano, E. V. and Acosta, L. and Auditore, L. and Baran, V. and Cap, T.
   and Cardella, G. and Colonna, M. and De Luca, S. and De Filippo, E. and
   Dell'Aquila, D. and Francalanza, L. and Gnoffo, B. and Lanzalone, G. and
   Lombardo, I. and Maiolino, C. and Martorana, N. S. and Norella, S. and
   Pagano, A. and Papa, M. and Piasecki, E. and Pirrone, S. and Politi, G.
   and Porto, F. and Quattrocchi, L. and Rizzo, F. and Rosato, E. and
   Russotto, P. and Siwek-Wilczynska, K. and Trifiro, A. and Trimarchi, M.
   and Verde, G. and Vigilante, M. and Wilczynsky, J.},
Title = {{Signals of dynamical and statistical process from IMF-IMF correlation
   function}},
Journal = {{NUOVO CIMENTO C-COLLOQUIA AND COMMUNICATIONS IN PHYSICS}},
Year = {{2016}},
Volume = {{39}},
Number = {{6}},
Month = {{NOV-DEC}},
Abstract = {{In this paper we briefly discuss about a novel application of the IMFIMF
   correlation function to the physical case of binary massive
   projectile-like (PLF) splitting for dynamical and statistical
   breakup/fission in heavy ion collisions at Fermi energy. Theoretical
   simulations are also shown for comparisons with the data. These
   preliminary results have been obtained for the reverse kinematics
   reaction Sn-124+Ni-64 at 35 A MeV that was studied using the forward
   part of CHIMERA detector. In that reaction a strong competition between
   a dynamical and a statistical components and its evolution with the
   charge asymmetry of the binary break up was already shown. In this work
   we show that the IMF-IMF correlation function can be used to pin down
   the timescale of the fragments production in binary fission-like
   phenomena. We also made simulations with the CoMDII model in order to
   compare to the experimental IMF-IMF correlation function. In future we
   plan to extend these studies to different reaction mechanisms and
   nuclear systems and to compare with different theoretical transport
   simulations.}},
