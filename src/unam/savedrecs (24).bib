@article{ ISI:000386107600038,
Author = {Francisco, Hector and Bertin, Virineya and Soto, Jorge R. and Castro,
   Miguel},
Title = {{Charge and Geometrical Effects on the Catalytic N2O Reduction by Rh-6(-)
   and Rh-6(+) Clusters}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
