@article{ ISI:000387101100011,
Author = {Vazquez-Estrada, Omar and Garcia-Valenzuela, Augusto},
Title = {{Collimated light reflection and transmission of a surface partially
   covered by large and tenuous particles}},
Journal = {{APPLIED OPTICS}},
Year = {{2016}},
Volume = {{55}},
