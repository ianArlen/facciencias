@article{ ISI:000388462900012,
Author = {Mendoza, Jonatan and Adrian Reyes, J. and Avendano, Carlos G.},
Title = {{Optical band gap in a nanocomposite structurally chiral medium}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2016}},
Volume = {{94}},
Number = {{5}},
Month = {{NOV 21}},
Abstract = {{We analyzed the optical band gaps for axially propagating
   electromagnetic waves throughout a nanocomposite structurally chiral
   medium. This medium is made of metallic nanoballs (silver) randomly
   dispersed in a structurally chiral material whose dielectric properties
   can be represented by a resonant effective uniaxial tensor. The
   structurally chiral material is taken to possess locally a (4) over bar
   2m point group symmetry. We found that the band-gap properties of the
   periodic system depends strongly on the volume fraction of nanoparticles
   in the chiral matrix. Particularly, we observed splitting of the bands
   and the creation of new sub band gaps when the resonance frequency of
   the composite medium lies within the band gap without inclusions.}},
