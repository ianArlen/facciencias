
@article{ ISI:000387683200004,
Author = {Becerra, Guillermo and Alvarez-Icaza, Luis and Pantoja-Vazquez, Alfonso},
Title = {{Power flow control strategies in parallel hybrid electric vehicles}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART D-JOURNAL OF
   AUTOMOBILE ENGINEERING}},
