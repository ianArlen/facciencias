@article{ ISI:000384384800040,
Author = {Rodriguez-Aranda, M. C. and Calderon-Pinar, F. and Hernandez-Landaverde,
   M. A. and Heiras, J. and Zamorano-Ulloa, R. and Ramirez-Rosales, D. and
   Yanez-Limon, J. M.},
Title = {{Photoluminescence of sol-gel synthesized PZT powders}},
Journal = {{JOURNAL OF LUMINESCENCE}},
Year = {{2016}},
Volume = {{179}},
Pages = {{280-286}},
Month = {{NOV}},
Abstract = {{A wide band of photoluminescence (PL) emission, in structurally
   disordered lead zirconate titanate (PZT) powders, prepared by sol-gel
   route, was observed at room temperature excited with a laser line (488
   nm). Powders with PbZro.53Tio.47O3 nominal composition annealed at
   different temperatures were studied by X-ray diffraction, Raman
   spectroscopy, Luminescence, Diffuse Reflectance and Electronic
   Paramagnetic Resonance Spectroscopy (EPR). Our results indicate that the
   PL response can be associated to order-disorder degree in the perovskite
   structure, with the exception of samples annealed at low temperature,
   where a mixture of oxides precursors' phases was observed. Furthermore,
   in quasi crystalline ordered samples (95\% of crystallinity) a small
   generation of PL remains. In these experiments, the band gap increases
   with the formation of crystalline structure. EPR experiments were
   conducted in order to follow the evolution of paramagnetic species with
   thermal treatment from the mixture of oxides precursors to the
   perovskite phase and paramagnetic point defects were identified (Pb+3
   and Ti+3). EPR data suggest the presence of order-disorder within the
   lattice network. Paramagnetic species are similar in samples treated at
   700 and 800 degrees C, nevertheless the emission intensity decreases by
   a factor of 6, indicating that the defects associated with PL are not
   paramagnetic at both temperatures. (C) 2016 Elsevier B.V. All rights
   reserved.}},
