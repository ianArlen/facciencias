@article{ ISI:000400388500001,
Author = {Moctezuma-Flores, M. and Parmiggiani, F.},
Title = {{SAR observations of the Nansen Ice Shelf fracture}},
Journal = {{EUROPEAN PHYSICAL JOURNAL PLUS}},
Year = {{2016}},
Volume = {{131}},
Number = {{11}},
Month = {{NOV 4}},
Abstract = {{This paper presents a study, by means of of Synthetic Aperture Radar
   (SAR) images, of the fracture of the Nansen Ice Shelf, from its first
   appearance in SAR images to the final collapse on 7 April 2016. Both
   Sentinel-1 and Cosmo-SkyMed images have been used. First, the images
   were remapped onto an equidistant cylindrical projection; from these a
   subset, or imagette, only covering the fracture was extracted. A
   segmentation scheme was then applied to the sequence of imagettes in
   order to produce a sequence of binary imagettes with only the fracture
   area enhanced; from these, the computation of fracture area became a
   trivial task.}},
