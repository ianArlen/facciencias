@article{ ISI:000389417000012,
Author = {Alarcon, Tomas and Castillo, Jorge and Garcia-Ponce, Berenice and Angel
   Herrero, Miguel and Padilla, Pablo},
Title = {{Growth rate and shape as possible control mechanisms for the selection
   of mode development in optimal biological branching processes}},
Journal = {{EUROPEAN PHYSICAL JOURNAL-SPECIAL TOPICS}},
Year = {{2016}},
Volume = {{225}},
Number = {{13-14}},
Pages = {{2581-2589}},
Month = {{NOV}},
Note = {{International Workshop on Advanced Computational and Experimental
   Techniques in Nonlinear Dynamics, Cusco, PERU, AUG, 2015}},
Abstract = {{Recently three branching modes were characterized during the formation
   of the lung in mice. These modes are highly stereotyped and correspond
   to domain formation, planar bifurcation and three dimensional branching
   respectively. At the same time it is proved that although genetic
   control mechanisms are presumably related to the selection of any of
   these modes, other external factors will most probably be involved in
   the branching process during development. In this paper we propose that
   the underlying controling factors might be related to the rate at which
   the tubes that form the lung network grow. We present a mathematical
   model that allows us to formulate specific experimental predictions on
   these growth rates. Moreover we show that according to this formulation,
   there is an optimization criterion which governs the branching process
   during lung development, namely, efficient local space filling
   properties of the network. If there is no space limitation the branches
   are allowed to grow freely and faster, selecting one branching mode,
   namely, domain formation. As soon as volume constraints appear the
   growth rate decreases, triggering the selection of planar and orthogonal
   bifurcation.}},
