@article{ ISI:000384676000027,
Author = {O'Connell, Ross and Eisenstein, Daniel and Vargas, Mariana and Ho,
   Shirley and Padmanabhan, Nikhil},
Title = {{Large covariance matrices: smooth models from the two-point correlation
   function}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2016}},
Volume = {{462}},
Number = {{3}},
Pages = {{2681-2694}},
Month = {{NOV 1}},
Abstract = {{We introduce a new method for estimating the covariance matrix for the
   galaxy correlation function in surveys of large-scale structure. Our
   method combines simple theoretical results with a realistic
   characterization of the survey to dramatically reduce noise in the
   covariance matrix. For example, with an investment of only approximate
   to 1000 CPU hours we can produce a model covariance matrix with noise
   levels that would otherwise require similar to 35 000 mocks.
   Non-Gaussian contributions to the model are calibrated against mock
   catalogues, after which the model covariance is found to be in
   impressive agreement with the mock covariance matrix. Since calibration
   of this method requires fewer mocks than brute force approaches, we
   believe that it could dramatically reduce the number of mocks required
   to analyse future surveys.}},
