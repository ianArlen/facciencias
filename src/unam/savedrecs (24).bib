@article{ ISI:000387544700003,
Author = {Omar Mendoza-Llerenas, Edgar and Javier Perez, David and Gomez-Sandoval,
   Zeferino and Escalante-Minakata, Pilar and Ibarra-Junquera, Vrani and
   Said Razo-Hernandez, Rodrigo and Capozzi, Vittorio and Russo, Pasquale
   and Spano, Giuseppe and Fiocco, Daniela and Alberto Osuna-Castro, Juan
   and Moreno, Abel},
Title = {{Lactobacillus plantarum WCFS1 beta-Fructosidase: Evidence for an Open
   Funnel-Like Channel Through the Catalytic Domain with Importance for the
   Substrate Selectivity}},
Journal = {{APPLIED BIOCHEMISTRY AND BIOTECHNOLOGY}},
Year = {{2016}},
Volume = {{180}},
Number = {{6}},
Pages = {{1056-1075}},
Month = {{NOV}},
Abstract = {{beta-Fructosidase, a glycoside hydrolase of a biotechnologically
   important strain, was studied for its biochemical, physicochemical, and
   three-dimensional structure characteristics. This enzyme was
   heterologously expressed in Escherichia coli as a C-terminal His-tagged
   protein (SacB). beta-Fructosidase catalyzes the cleavage of glycoside
   bonds toward certain carbohydrates with beta-fructofuranosyl linkages;
   however, SacB exhibited selectivity toward sucrose and an optimum
   activity at pH 6.0-6.5 and 37 A degrees C. In such optimum enzymatic
   activity conditions, the SacB was commonly observed as a monodisperse
   protein by dynamic light scattering (DLS). As beta-fructosidase belongs
   to glycoside hydrolase family 32 (GH32), a beta-sandwich and a
   five-bladed beta-propeller domain are typical predicted folds in its
   structure. Docking and molecular dynamic simulations revealed for the
   first time a funnel-like channel perfectly exposed in the beta-propeller
   domain of the Lactobacillus plantarum beta-fructosidase (this allows the
   interaction between its entire catalytic triad and substrates that are
   larger than sucrose). In contrast, SacB showed a closed central tunnel
   collaterally induced by its His-tag.}},
