@article{ ISI:000387520700013,
Author = {Osnaya-Romero, N. and Acosta-Saavedra, L. C. and Goytia-Acevedo, R. and
   Lares-Asseff, I. and Basurto-Celaya, G. and Perez-Guille, G. and
