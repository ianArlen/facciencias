Author = {Almeda-Valdes, Paloma and Aguilar-Salinas, Carlos A. and Uribe, Misael
   and Canizales-Quinteros, Samuel and Mendez-Sanchez, Nahum},
Title = {{Impact of anthropometric cut-off values in determining the prevalence of
   metabolic alterations}},
