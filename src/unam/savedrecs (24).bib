@article{ ISI:000405608100009,
Author = {Salin-Pascual, Rafael J.},
Title = {{New pharmacological strategies for the management of insomnia :
   Suvorexant, one receptor antagonists orexin}},
Journal = {{REVISTA MEXICANA DE NEUROCIENCIA}},
