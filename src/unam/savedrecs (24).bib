@article{ ISI:000387910200010,
Author = {Cortes-Berrueco, Luis E. and Gershenson, Carlos and Stephens,
   Christopher R.},
Title = {{Traffic Games: Modeling Freeway Traffic with Game Theory}},
Journal = {{PLOS ONE}},
Year = {{2016}},
Volume = {{11}},
Number = {{11}},
Month = {{NOV 17}},
Abstract = {{We apply game theory to a vehicular traffic model to study the effect of
   driver strategies on traffic flow. The resulting model inherits the
   realistic dynamics achieved by a two-lane traffic model and aims to
   incorporate phenomena caused by driver-driver interactions. To achieve
   this goal, a game-theoretic description of driver interaction was
   developed. This game-theoretic formalization allows one to model
   different lane-changing behaviors and to keep track of mobility
   performance. We simulate the evolution of cooperation, traffic flow, and
   mobility performance for different modeled behaviors. The analysis of
   these results indicates a mobility optimization process achieved by
   drivers' interactions.}},
