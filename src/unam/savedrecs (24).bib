@article{ ISI:000388047400065,
Author = {Carrillo, Julian and Rico, Alejandro and Alcocer, Sergio},
Title = {{Experimental study on the mechanical properties of welded-wire meshes
   for concrete reinforcement in Mexico City}},
Journal = {{CONSTRUCTION AND BUILDING MATERIALS}},
Year = {{2016}},
Volume = {{127}},
Pages = {{663-672}},
Month = {{NOV 30}},
Abstract = {{Previous experimental studies on the seismic behavior of concrete
   elements reinforced with welded-wire meshes (WWM) have suggested that
   WWM do not comply with the mechanical properties specified in Mexican
   material standards. To thoroughly assess the mechanical properties of
   WWM available in the Metropolitan Mexico City, an experimental research
   program was developed. Based on a statistical analysis of measured
   mechanical properties, main parameters (yield stress and yield strain,
   ultimate strength and ultimate strain, elongation after fracture,
   reduction of cross-sectional area, shear strength of welding, and
   bending capacity) are reported and discussed. Values of the mechanical
   properties of WWM are proposed for computing stress-strain curves. (C)
   2016 Elsevier Ltd. All rights reserved.}},
