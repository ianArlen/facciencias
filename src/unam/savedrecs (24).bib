@article{ ISI:000386611500003,
Author = {Monroy-Ruz, Jorge and Garay-Palmett, Karina and U'Ren, Alfred B.},
Title = {{Counter-propagating spontaneous four wave mixing: photon-pair
   factorability and ultra-narrowband single photons}},
