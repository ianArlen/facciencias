@article{ ISI:000385598300042,
Author = {Melendez-Ceballos, A. and Fernandez-Valverde, S. M. and Albin, V. and
   Lair, V. and Chavez-Carvayar, J. A. and Ringuede, A. and Cassir, M.},
Title = {{Investigation on niobium oxide coatings for protecting and enhancing the
   performance of Ni cathode in the MCFC}},
Journal = {{INTERNATIONAL JOURNAL OF HYDROGEN ENERGY}},
Year = {{2016}},
Volume = {{41}},
Number = {{41}},
Pages = {{18721-18731}},
Month = {{NOV 2}},
Note = {{3rd International Workshop on Molten Carbonates and Related Topics
   (IWMC), NE Univ, Shenyang, PEOPLES R CHINA, JUN 11-13, 2015}},
Abstract = {{This study addresses the problem of the nickel cathode dissolution,
   decreasing the lifetime of molten carbonate fuel cells, by a protective
   coating of Nb2O5 processed by atomic layer deposition ALD onto the
   porous cathode substrate. Samples of different thicknesses were tested
   electrochemically in molten Li2CO3-K2CO3 eutectic at 650 degrees C
   during 230 h by means of chronopotentiometry and electrochemical
   impedance spectroscopy. A significant decrease in the stabilization time
   with respect to the oxygen reduction potential is observed with niobium
   coatings, which could indicate an electrocatalytic process favoured by
   the presence of Nb. The structure and morphology of the coated samples
   are characterised by XRD, SEM and XPS before and after the
   electrochemical tests. Mixed lithiated Ni and Nb oxides are likely to be
   formed at the cathode surface. Interestingly, this Nb-containing mixed
   oxide not only can protect Ni cathode but also maintains its good
   performance.. (C) 2016 Hydrogen Energy Publications LLC. Published by
   Elsevier Ltd. All rights reserved.}},
