Author = {Torchynska, T. V. and Casas Espinola, J. L. and Jaramillo Gomez, J. A.
   and Douda, J. and Gazarian, K.},
Title = {{Photoluminescence of double core/shell infrared (CdSeTe)/ZnS quantum
   dots conjugated to Pseudo rabies virus antibodies}},
