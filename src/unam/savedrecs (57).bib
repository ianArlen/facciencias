@article{ ISI:000320954600014,
Author = {Salazar-Salinas, Karim and Kubli-Garfias, Carlos and Seminario, Jorge M.},
Title = {{Computational design of a CNT carrier for a high affinity bispecific
   anti-HER2 antibody based on trastuzumab and pertuzumab Fabs}},
Journal = {{JOURNAL OF MOLECULAR MODELING}},
