@article{ ISI:000320092100011,
Author = {Mathews, N. R. and Angeles-Chavez, C. and Cortes-Jacome, M. A. and
   Toledo Antonio, J. A.},
Title = {{Physical properties of pulse electrodeposited lead sulfide thin films}},
Journal = {{ELECTROCHIMICA ACTA}},
