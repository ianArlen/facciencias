@article{ ISI:000322719200002,
Author = {Cesar Morales-Mejia, Julio and Angeles-de-la Cruz, Lucero and Almanza,
   Rafael},
Title = {{Thick TiO2 Sol-gel Films for Water Detoxification: Synthesis,
   Characterization and Use for Photocatalytic Oxidation}},
