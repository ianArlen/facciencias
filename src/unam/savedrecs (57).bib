@article{ ISI:000322373200010,
Author = {Aparicio, R. and Barrera, G. and Trapaga, G. and Ramirez-Argaez, M. and
   Gonzalez-Rivera, C.},
Title = {{Solidification kinetics of a near eutectic Al-Si alloy, unmodified and
   modified with Sr}},
Journal = {{METALS AND MATERIALS INTERNATIONAL}},
Year = {{2013}},
Volume = {{19}},
Number = {{4}},
Pages = {{707-715}},
Month = {{JUL}},
Abstract = {{The purpose of this work was to explore the differences in
   solidification kinetics between unmodified and Sr modified eutectic
   Al-Si alloy as revealed by Fourier Thermal Analysis (FTA) and
   grain-growth kinetics characterization. Thermal analysis were performed
   in cylindrical stainless steel cups coated with a thin layer of boron
   nitride, using two type-K thermocouples connected to a data acquisition
   system. Grain growth kinetics characterization was carried out using
   solid fraction evolution and grain density data. FTA results for the non
   modified and modified alloys suggest that there are changes in the
   solidification rate during eutectic nucleation followed, during growth,
