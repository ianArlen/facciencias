@article{ ISI:000325217100006,
Author = {Le, Guan and Chi, Peter J. and Blanco-Cano, Xochitl and Boardsen, Scott
   and Slavin, James A. and Anderson, Brian J. and Korth, Haje},
Title = {{Upstream ultra-low frequency waves in Mercury's foreshock region:
   MESSENGER magnetic field observations}},
