@article{ ISI:000320322400048,
Author = {Itzel Balderas-Martinez, Yalbi and Savageau, Michael and Salgado,
   Heladia and Perez-Rueda, Ernesto and Morett, Enrique and Collado-Vides,
   Julio},
Title = {{Transcription Factors in Escherichia coli Prefer the Holo Conformation}},
Journal = {{PLOS ONE}},
Year = {{2013}},
Volume = {{8}},
