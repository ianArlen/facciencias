@article{ ISI:000320161000003,
Author = {Lopez-Marin, Luz M. and Tamariz, Elisa and Acosta-Torres, Laura S. and
   Castano, Victor M.},
Title = {{Therapeutic Proteins and Nanotechnology: Immune Response and Stealth
   Bioengineered Constructs}},
