@article{ ISI:000320555200011,
Author = {Olvera-Martinez, M. E. and Mendoza-Flores, J. and Rodriguez-Gomez, F. J.
   and Palomar-Pardave, M. E. and Genesca, J.},
Title = {{Effects of turbulent flow on the corrosion inhibition properties of
   2-mercaptobenzimidazole}},
Journal = {{MATERIALS AND CORROSION-WERKSTOFFE UND KORROSION}},
Year = {{2013}},
Volume = {{64}},
Number = {{6}},
Pages = {{522-529}},
Month = {{JUN}},
Abstract = {{The electrochemical techniques, that is, polarization resistance (Rp)
   and potentiodynamic polarization curves, were used in order to determine
   the effect of turbulent flow on the corrosion inhibiting effect of
   2-mercaptobenzimidazole (2-MBI) on API 5L X52 grade steel samples
   immersed in a 3\% NaCl aqueous solution saturated with CO2 at 60 degrees
   C. Turbulent flow conditions were controlled using a rotating cylinder
   electrode (RCE). An inhibition efficiency of 98\% was measured at a
   concentration of 10ppm of 2-MBI at a rotation rate of 5000rpm. This
   efficiency value is similar to those efficiency values measured at 25
   and 40ppm 2-MBI and at the same rotation rate. These observations
   suggest that as the turbulent flow conditions increase the corrosion
   inhibiting effect of 2-MBI is enhanced. 2-MBI follows a Langmuir
   adsorption isotherm. The calculated values of adsorption equilibrium
   constant (Kads) and adsorption free energy (\$\textbackslash{}Delta
   G\_\{\{\textbackslash{}rm ads\}\}<\^{}>\{\{\textbackslash{}rm o\}\} \$)
   suggest that the adsorption process taking place is chemical. The
   polarization curves indicate that the 2-MBI does not modify the
   electrochemical mechanism of the anodic (Fe dissolution) and cathodic
   (hydrogen evolution) reactions. It is suggested that 2-MBI decreases the
   rate at which these reactions occur, blocking the active sites on the
   steel surface.}},
