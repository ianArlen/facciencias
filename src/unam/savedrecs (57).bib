@article{ ISI:000338352400011,
Author = {Claudia Hernandez-Mejia, Blanca and Flores Gallardo, Adrian and
   Llorente-Bousquets, Jorge},
Title = {{Morphological Comparison of the Chorion of the Genera Pieriballia,
   Itaballia, and Perrhybris (Lepidoptera: Pieridae: Pierinae), and their
   Phylogenetic Implications}},
Journal = {{SOUTHWESTERN ENTOMOLOGIST}},
Year = {{2013}},
Volume = {{38}},
Number = {{2}},
Pages = {{275-291}},
Month = {{JUN}},
Abstract = {{We describe and compare the egg chorion of five species of three genera
   of Pierinae: Pieriballia, Itaballia, and Perrhybris. Their
   characterizations and illustrations include the main structural
   features: number and arrangement of longitudinal axes (short and long),
   and quantification of ribs or cells, projections and vestiges or
   reminiscent ribs. Also, we describe the color and size of the egg. We
   typify two egg models: elongated ellipsoidal or fusiform and oblong
   ellipsoidal, both with projections in the apical area, around the
   micropyle. Comparisons are made between features present in different
   species and genera. We discuss their phylogenetic and taxonomic
   implications.}},
