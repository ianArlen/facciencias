@article{ ISI:000323141000004,
Author = {Contreras, Alejandra V. and Torres, Nimbe and Tovar, Armando R.},
Title = {{PPAR-alpha as a Key Nutritional and Environmental Sensor for Metabolic
   Adaptation}},
Journal = {{ADVANCES IN NUTRITION}},
