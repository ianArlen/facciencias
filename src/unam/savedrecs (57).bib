@article{ ISI:000321491200012,
Author = {Borja, Karina and Ostrosky, Feggy},
Title = {{Early Traumatic Events in Psychopaths}},
Journal = {{JOURNAL OF FORENSIC SCIENCES}},
Year = {{2013}},
Volume = {{58}},
Number = {{4}},
Pages = {{927-931}},
Month = {{JUL}},
Abstract = {{The relationship between diverse early traumatic events and psychopathy
   was studied in 194 male inmates. Criminal history transcripts were
   revised, and clinical interviews were conducted to determine the level
   of psychopathy using the Psychopathy Checklist-Revised (PCL-R) Form, and
   the Early Trauma Inventory was applied to assess the incidence of abuse
   before 18 years of age. Psychopathic inmates presented a higher
   victimization level and were more exposed to certain types of intended
   abuse than sociopathic inmates, while the sum of events and emotional
   abuse were associated with the PCL-R score. Our studies support the
   influence of early adverse events in the development of psychopathic
   offenders.}},
