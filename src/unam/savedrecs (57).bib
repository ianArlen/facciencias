@article{ ISI:000319805300002,
Author = {Sahu, Sarira and Oliveros, Andres Felipe Osorio and Sanabria, Juan
   Carlos},
Title = {{Hadronic-origin orphan TeV flare from 1ES 1959+650}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2013}},
Volume = {{87}},
Number = {{10}},
Month = {{MAY 31}},
Abstract = {{The 1ES 1959 + 650 is a high-peaked BL Lacertae object. On the 4th of
   June, 2002, it exhibited a strong TeV flare without any low energy
   counterpart, providing for the first time an example of an orphan flare
   from a blazar. Observation of this orphan flare is in striking
   disagreement with the predictions of the leptonic models thus
   challenging the conventional synchrotron self-Compton interpretation of
   the TeV emission. Here we propose that the low energy tail of the
   synchrotron self-Compton photons in the blazar jet serve as the target
   for the Fermi-accelerated high energy protons of energy less than or
   similar to 100 TeV within the jet to produce the TeV photons through the
   decay of neutral pions from the delta resonance. Our model explains very
   nicely the observed TeV flux from this orphan flare and we also estimate
   the high energy neutrino flux from this flaring event.}},
