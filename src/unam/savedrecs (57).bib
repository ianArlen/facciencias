@article{ ISI:000322048800018,
Author = {Marulanda Arevalo, Jose Luddey and Remolina Millan, Aduljay and Alberto
   Baron, Jairo},
Title = {{STEAM OXIDATION IN STAINLESS STEEL AISI 317 AT 700 AND 750 degrees C}},
Journal = {{DYNA-COLOMBIA}},
