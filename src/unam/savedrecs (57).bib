@article{ ISI:000320213700015,
Author = {Garcia-Uriostegui, Lorena and Dionisio, Nestor and Burillo, Guillermina},
Title = {{Evaluation of 2-vinylnaphthalene and 4-vinylbiphenyl as antirads to
   increase the radiation resistance of poly(vinyl chloride)}},
Journal = {{POLYMER DEGRADATION AND STABILITY}},
Year = {{2013}},
Volume = {{98}},
Number = {{7}},
Pages = {{1407-1412}},
Month = {{JUL}},
Abstract = {{The effects of gamma radiation on pristine poly(vinyl chloride) in the
   presence of the antirads 2-vinylnaphthalene and 4-vinylbiphenyl have
   been studied. External protection derived from the addition of antirads
   at 3 and 5\% wt concentration in the PVC film and internal protection
   from grafted antitrades was evaluated. The grafted antirads were
   obtained through irradiation with a Co-60 gamma source at a dose rate of
   10.5 kGy h(-1). Doses ranged from 10 to 40 kGy. Radiation resistance of
   the modified and pristine PVC (PVC-g-antirads and blends) was evaluated
   in the range of 600-3000 kGy at a dose rate of 10 kGy h(-1). The
   radiation resistance was characterized from crosslinking measurement,
   infrared spectroscopy, thermogravimetric analysis, and differential
   scanning calorimetry. (C) 2013 Published by Elsevier Ltd.}},
