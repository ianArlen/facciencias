@article{ ISI:000322067100003,
Author = {Escotto-Cordova, Eduardo-Alejandro},
Title = {{Inner language: Considerations from the recordings of children's ludic
   soliloquies}},
Journal = {{CULTURA Y EDUCACION}},
Year = {{2013}},
Volume = {{25}},
Number = {{2}},
Pages = {{153-165}},
Month = {{JUN}},
Abstract = {{Inner language (IL) is a kind of dialogical, spoken or silent soliloquy,
   in which one is, at the same time, both or all of the interlocutors.
   Some contemporary studies on IL are reviewed, emphasizing that a number
   of these studies assume Vygotsky's characterization that IL is a
   compacted, elliptical and predicate language whose transcription would
   be incomprehensible. The aim of the study was to analyze a corpus of
   preschool children's ludic soliloquies and establish if they meet
   Vygotsky's criteria. We analyzed 408 voice emissions of ludic
   soliloquies from 7 girls and 4 boys while they were playing with toys in
   a controlled environment. These soliloquies were video-recorded and
   transcribed in a format that allowed us to undertake their spoken and
   enunciative analysis in context. 73\% of the enunciations had sentences,
   and 27\% did not, so the latter could only be understood within the
   dialogical context. Most of the soliloquies did not correspond to
   Vygotsky's characterization, i.e., the subject and the verb were clearly
   identified, and their transcript was understandable. Since the IL
   modality was of ludic soliloquy, the data suggest the need to expand the
   body of soliloquies under other conditions, such as non-ludic soliloquy,
   in order to confirm or refine the characterization of IL.}},
