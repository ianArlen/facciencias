@article{ ISI:000318293100017,
Author = {Arcangeli, Jesica and Cervantes, Fernando A. and Lance, Stacey L. and
   Isabel Salazar, Ma. and Ortega, Jorge},
Title = {{Twenty-four microsatellite markers for the gray mouse opossum
   (Tlacuatzin canescens): development from illumina paired-end sequences}},
Journal = {{CONSERVATION GENETICS RESOURCES}},
Year = {{2013}},
Volume = {{5}},
Number = {{2}},
Pages = {{367-370}},
Month = {{JUN}},
Abstract = {{Tlacuatzin canescens is an opossum species endemic to Mexico from
   southern Sonora to Oaxaca, with populations in Tres Marias Islands and
   in central Peninsula of Yucatan. A technique based on Illumina
   paired-end sequencing of a library highly enriched for microsatellite
   repeats was used to develop loci. Twenty-four polymorphic (tri- and
   tetranucleotide) microsatellites were developed and tested as markers in
   the target species. All markers were genotyped on 18 different
   individuals from distinct locations. We observed medium to low genetic
   variation across most loci (mean number of alleles per locus = 8).
   Levels of expected heterozygosity across all markers was fairly low
   (mean H-E = 0.477, mean H-O = 0.480).}},
