@article{ ISI:000182032600038,
Author = {Baez, G and Larralde, H and Leyvraz, F and Mendez-Sanchez, RA},
Title = {{Fluctuation-dissipation theorem for metastable systems}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2003}},
Volume = {{90}},
Number = {{13}},
Month = {{APR 4}},
Abstract = {{We show that an appropriately defined fluctuation-dissipation theorem,
   connecting generalized susceptibilities and time correlation functions,
   is valid for times shorter than the nucleation time of the metastable
   state of Markovian systems satisfying detailed balance. This is done by
