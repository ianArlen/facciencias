@article{ ISI:000182480400001,
Author = {Ferraes, SG},
Title = {{The conditional probability of earthquake occurrence and the next large
   earthquake in Tokyo, Japan}},
Journal = {{JOURNAL OF SEISMOLOGY}},
Year = {{2003}},
Volume = {{7}},
Number = {{2}},
Pages = {{145-153}},
Month = {{APR}},
Abstract = {{The use of probability distribution of recurrence times as described by
   the exponential, Weibull and Rayleihg probability densities form the
   core of the probabilistic seismic prediction analysis presented in this
   paper. Using these three probabilistic models we derive three formulas
   to calculate the conditional probability P(Deltat|t) than an earthquake
   event will occur in the time interval (t, t + Deltat), provide that it
   has not occurred in the elapsed time t since the last large earthquake
   (M greater than or equal to 6.4) in the Tokyo area. This paper proposes
   a new method to estimate the time interval Deltat for occurrence of a
   new large earthquake in Tokyo area. This time interval is measured after
   the elapsed time (t) since the last large earthquake. To do this we use
   the three formulas for the conditional probability P(Deltat|t) and the
   criterion of the maximum conditional probability of earthquake
   occurrence. Using a list of historical earthquakes which have occurred
   in the Tokyo area as given by Usami (1976, pp. 235-243), we found that:
   (1) Using the exponential model, it is estimated that a highly damaging
   earthquake magnitude M greater than or equal to 6.4, may occur before
   the year 2009.50, or equivalently before June 2009; (2) Using the
   Weibull model, it is estimated that the damaging earthquake (M greater
   than or equal to 6.4) may occur before the year 2129.80, or equivalently
   before October 2129.}},
