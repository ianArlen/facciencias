@article{ ISI:000183109100012,
Author = {Caballero, R and Valdes-Galicia, JF},
Title = {{Statistical analysis of the fluctuations detected in high-altitude
   neutron monitor, solar and interplanetary parameters}},
Journal = {{SOLAR PHYSICS}},
Year = {{2003}},
Volume = {{213}},
Number = {{2}},
Pages = {{413-426}},
Month = {{APR}},
Abstract = {{Galactic cosmic ray fluctuations from six mountain altitude neutron
   monitors around the world are analyzed during the period 1990 - 1999.
   The period comprises the maximum and declining phase of solar cycle 22
   and the beginning of cycle 23. The evolution of the most significant
   periodicities and comparisons with solar activity and interplanetary
   indicators are presented. We found a 38-day variation present in all
   neutron monitors, solar activity parameters, and IMF fluctuations. The
   possible origin of this and other stable periodicities of cosmic ray
   variations in the analyzed period are discussed.}},
