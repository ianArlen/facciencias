@article{ ISI:000180197500014,
Author = {Fomine, S and Vargas, SM and Tlenkopatchev, MA},
Title = {{Molecular Modeling of ruthenium alkylidene mediated olefin metathesis
   reactions. DFT study of reaction pathways}},
Journal = {{ORGANOMETALLICS}},
Year = {{2003}},
Volume = {{22}},
Number = {{1}},
Pages = {{93-99}},
Month = {{JAN 6}},
Abstract = {{Three possible reaction pathways have been studied for the
   Cl-2(PMe3)(2)Ru=CH2 and Cl-2(PCY3)(2)Ru=CH2 (Cy = cyclohexyl) mediated
   metathesis reaction of propylene by quantum-mechanical DFT calculations.
   It was found that in all cases the metathesis reaction proceeds via
   dissociative substitution of a phosphine ligand with propylene, giving a
   monophosphine complex. This tendency increases with ligand volume, and
   in the case of smaller PMe3 groups it is the entropy contribution to the
   reaction energetics that causes the monophosphine complex to participate
   in the metathesis reaction, according to the modeling data.}},
