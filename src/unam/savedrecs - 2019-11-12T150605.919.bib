@article{ ISI:000181398800018,
Author = {Garcia-Cruz, I and Martinez-Magadan, JM and Guadarrama, P and Salcedo, R
   and Illas, F},
Title = {{Electronic structure properties of carbazole-like compounds:
   Implications for asphaltene formation}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY A}},
Year = {{2003}},
Volume = {{107}},
Number = {{10}},
Pages = {{1597-1603}},
Month = {{MAR 13}},
Abstract = {{A series of carbazole derivatives have been studied by means of ab
   initio density functional theory. The molecular structure of these
   compounds has been obtained by means of geometry optimization and
   characterized as potential energy surface minima. The analysis of the
   frontier orbitals together with the study of aromaticity permits one to
   make quantitative predictions about the reactivity of these molecules.
   This could be used to predict the influence of substituents on the
   chemical reactivity of carbazole-like moieties in asphaltenes.}},
