@article{ ISI:000181842900013,
Author = {Castro, A and Clapp, M},
Title = {{The effect of the domain topology on the number of minimal nodal
   solutions of an elliptic equation at critical growth in a symmetric
   domain}},
Journal = {{NONLINEARITY}},
Year = {{2003}},
Volume = {{16}},
Number = {{2}},
Pages = {{579-590}},
Month = {{MAR}},
Abstract = {{We consider the Dirichlet problem Deltau + lambdau +
   \textbackslash{}u\textbackslash{}(2{*}-2)u = 0 in Omega, u = 0 on
   thetaOmega where Omega is a bounded smooth domain in R-N, N greater than
   or equal to 4, and 2{*} = 2N/(N - 2) is the critical Sobolev exponent.
   We show that if Omega is invariant under an orthogonal involution then,
   for lambda > 0 sufficiently small, there is an effect of the equivariant
   topology of Omega on the number of solutions which change sign exactly
   once.}},
