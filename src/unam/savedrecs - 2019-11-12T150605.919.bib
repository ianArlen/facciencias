@article{ ISI:000181857900007,
Author = {Chryssomalakos, C and Sudarsky, D},
Title = {{On the geometrical character of gravitation}},
Journal = {{GENERAL RELATIVITY AND GRAVITATION}},
Year = {{2003}},
Volume = {{35}},
Number = {{4}},
Pages = {{605-617}},
Month = {{APR}},
Abstract = {{The issue of whether some manifestations of gravitation in the quantum
   domain, are indicative or not of a non-geometrical aspect in gravitation
   is discussed. We focus on gedanken experiments, involving
   generalizations of the flavor-oscillation clocks of Ahluwalia and
   Burgard, and provide a critical analysis of previous interpretations. A
   detailed quantum mechanical description of the inner workings of these
   clocks reveals that there are, at this time, no indications of any
   departure of this nature from the geometrical character of gravitation.}},
