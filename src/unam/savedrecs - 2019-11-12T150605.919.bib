@article{ ISI:000181526700017,
Author = {Trujillo-Roldan, MA and Moreno, S and Segura, D and Galindo, E and
   Espin, G},
Title = {{Alginate production by an Azotobacter vinelandii mutant unable to
   produce alginate lyase}},
Journal = {{APPLIED MICROBIOLOGY AND BIOTECHNOLOGY}},
Year = {{2003}},
Volume = {{60}},
Number = {{6}},
Pages = {{733-737}},
Month = {{FEB}},
Abstract = {{Alginate is an industrially relevant linear copolymer composed of
   beta-1,4-linked D-mannuronic acid and its C-5 epimer L-guluronic acid.
   The rheological and gel-forming properties of alginates depend on the
   molecular weight and the relative content of the two monomers. Alginate
   produced by Azotobacter vinelandii was shown to be degraded towards the
   end of the culture, an undesirable situation in terms of potential
   alginate applications. A gene (algL) encoding the alginate lyase
   activity AlgL is present within the alginate biosynthetic gene cluster
   of A. vinelandii. We constructed strain SML2, an A. vinelandii strain
   carrying a non-polar mutation within algL. No alginate lyase activity
   was detected in SML2. Under 3\% dissolved oxygen tension, higher values
   of maximum mean molecular weight alginate were obtained (1,240 kDa) with
   strain SML2, compared to those from the parental strain ATCC 9046 (680
   kDa). These data indicate that AlgL activity causes the drop in the
   molecular weight of alginate produced by A. vinelandii.}},
