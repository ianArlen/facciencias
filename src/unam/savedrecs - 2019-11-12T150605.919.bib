@article{ ISI:000182389300035,
Author = {Castillo-Garza, R and Romero-Rochin, V},
Title = {{Thermodynamics of a gas of bosons in a harmonic trap in the
   Gross-Pitaevskii approximation}},
Journal = {{LASER PHYSICS}},
Year = {{2003}},
Volume = {{13}},
Number = {{4}},
Pages = {{628-633}},
Month = {{APR}},
Note = {{11th International Laser Physics Workshop (LPHYS 02), BRATISLAVA,
   SLOVAKIA, JUL 01-05, 2002}},
Abstract = {{With the extension of the Gross-Pitaevskii approximation to non-ground
   states for weakly interacting dilute atomic oases in magnetic traps, as
   introduced by Yukalov, Yukalova, and Bagnato, we study the thermodynamic
   properties of such a system. We analyze the number of particles in the
   condensate and the heat capacity. Strong dependences on the scattering
   length and the number of particles are found. For systems with positive
   scattering lengths, the shifts on critical temperature and the increase
   of the discontinuity in the heat capacity are predicted to be
   observable. We study systems with the parameter values of the JILA, MIT,
   and RQI traps.}},
