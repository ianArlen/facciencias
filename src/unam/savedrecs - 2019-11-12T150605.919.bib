@article{ ISI:000182159400023,
Author = {Watson, C and Araya, E and Sewilo, M and Churchwell, E and Hofner, P and
   Kurtz, S},
Title = {{Resolution of distance ambiguities of inner galaxy massive star
   formation regions. I}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2003}},
Volume = {{587}},
Number = {{2, 1}},
Pages = {{714-726}},
Month = {{APR 20}},
Abstract = {{Fifty-four ultracompact (UC) H II regions in the GLIMPSE survey region
   (\textbackslash{}b\textbackslash{} < 1 degrees and 30 degrees < l < 70
   degrees) were observed in H(2)CO and H110 alpha using the 305 m Arecibo
   telescope. By analyzing H(2)CO absorption against the UC H II region
   continuum emission, we resolve the distance ambiguity toward 44 sources.
   This determination is critical to measure global physical properties of
   UC H II regions (e.g., luminosity, size, mass) and properties of the
   Galaxy (e.g., spiral structure, abundance gradients). We find that the
   distribution of UC H II regions in this survey is consistent with a
   ``local spur,'' the Perseus, Sagittarius, and Scutum arms as delineated
   by Taylor \& Cordes. However, departures from model velocities produce
   distance uncertainties only slightly smaller than the proposed arm
   separations.}},
