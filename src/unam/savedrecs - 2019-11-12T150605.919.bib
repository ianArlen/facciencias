@article{ ISI:000182798000011,
Author = {Hernandez, S and Miura, H and Beristain, MF and Ogawa, T and Watanabe, T
   and Miyata, S},
Title = {{Novel diacetylene-containing polymers for second order NLO applications:
   Effect of main chains and structure property relationships}},
Journal = {{MACROMOLECULAR SYMPOSIA}},
Year = {{2003}},
Volume = {{192}},
Pages = {{123-133}},
Month = {{MAR}},
Note = {{7th Pacific Polymer Conference, OAXACA, MEXICO, DEC 03-07, 2001}},
Abstract = {{In order to obtain polymers having high second order nonlinear optical
   (NLO) response, various novel polymers that contain polar dye molecules
   in side chains and diacetylenic groups in the main chains were
   synthesized, and their second order NLO properties were studied. Some of
   these polymers consisting of para-benzoate and para-cinnamate main
   chains, showed extremely high NLO coefficients, d(33) of 200 - 350 pm/V,
   and the corresponding meta polymers showed much inferior NLO
   coefficients. The polymers having aliphatic main chains had very little
   NLO response probably because of their low glass transition temperatures
   and flexibility of main chains. The high SHG responses of these polymers
   are attributed to their facile orientation of both chromophores and main
   chains. In the case of para-polymers, the main chain orientation of
   alkoxybenzoate and cinnamate along the chromophore is thought to be a
   reason for their high NLO coefficients.}},
