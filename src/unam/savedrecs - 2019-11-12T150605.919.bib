@article{ ISI:000185910400028,
Author = {Lemos-Espinal, JA and Smith, GR and Ballinger, RE},
Title = {{Ecology of Xenosaurus grandis agrenon, a knob-scaled lizard from Oaxaca,
   Mexico}},
Journal = {{JOURNAL OF HERPETOLOGY}},
Year = {{2003}},
Volume = {{37}},
Number = {{1}},
Pages = {{192-196}},
Month = {{MAR}},
Abstract = {{We studied the ecology of a population of the mountain knob-scaled
   lizard, Xenosaurus grandis agrenon, from Oaxaca, Mexico. Males and
   females did not differ in body size (SVL), but males did have larger
   heads and hind limbs than females. Mean litter size was 3.2. Sex ratio
   did not differ from 1:1. All lizards were found associated with
   crevices. The size of the crevice (thickness and depth) increased with
   lizard SVL. Mean body temperature was 25.6degreesC. Body temperature was
   related to air and substrate temperature. Body temperature was
   influenced by the amount of cover, and the lizard's body position in its
   crevice but not by sex, month, vegetation type, or any crevice
   characteristics. We compare the ecology of this population to that of
   other Xenosaurus.}},
