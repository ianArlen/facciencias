@article{ ISI:000181079200008,
Author = {Colavita, E and Hacyan, S},
Title = {{Wigner functions of free ``Schrodinger cat{''} states}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2003}},
Volume = {{49}},
Number = {{1}},
Pages = {{45-52}},
Month = {{FEB}},
Abstract = {{We study the evolution of a free ``Schrodinger cat{''} state (a
   superposition of two coherent correlated states moving in opposite
   directions) using tile formalism of the Wigner function. Two possible
   mechanisms to spatially separate the two states are considered: a
   ``quantum sling{''} and a Paul trap that produces unstable motion. The
   numerical analysis shows how the two superposed states move away of each
   other, while keeping an interference term between them that is typical
   of quantum phenomena.}},
