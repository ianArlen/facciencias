@article{ ISI:000181101000004,
Author = {Xiao, MF and Rakov, N},
Title = {{Enhanced optical near-field transmission through subwavelength holes
   randomly distributed in a thin gold film}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2003}},
Volume = {{15}},
Number = {{4}},
Pages = {{L133-L137}},
Month = {{FEB 5}},
Abstract = {{Randomly distributed small holes of various subwavelength sizes were
   fabricated in a thin gold film. We study the optical near-field
   transmission of the film. In the wavelength spectrum from 350 to 650 nm,
   a number of strongly enhanced transmission peaks were observed. These
   transmission peaks can only be observed in the near field. We attribute
