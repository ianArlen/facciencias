@article{ ISI:000300549700030,
Author = {Teresa Guzman-Gutierrez, M. and Humberto Rios-Dominguez, M. and Alberto
   Ruiz-Trevino, F. and Zolotukhin, Mikhail G. and Balmaseda, Jorge and
   Fritsch, Detlev and Prokhorov, Evgen},
Title = {{Structure-properties relationship for the gas transport properties of
   new fluoro-containing aromatic polymers}},
Journal = {{JOURNAL OF MEMBRANE SCIENCE}},
Year = {{2011}},
Volume = {{385}},
Number = {{1-2}},
Pages = {{277-284}},
Month = {{DEC 1}},
Abstract = {{Super acid catalysis offers the syntheses of a rich variety of similar
   but different polymer structures otherwise not easily attainable. By
   this synthetic method, a set of 22 polymers was prepared and their gas
   permeability coefficients for pure gases, with their associated ideal
   selectivity, were measured. An analysis of their selectivity and
   permeability combination protocol revealed that their membranes have
   combinations, specifically for the O-2/N-2, CO2/CH4 and CO2/N-2
   separations, that are in the same order of magnitude as those reported
   for the polysulfone and some polyimide families. For CO2/N-2
   separations, polymers CF3PhSO3H-T and CF3Ph-T offer membranes with CO2
   permeability coefficients of 83 and 220 Barrer, with corresponding
   selectivity of 31 and 24. In addition, by the systematic incorporation
   of Me, Ph, flouro atoms or CF3 groups into their chemical structure it
   is possible to understand the structure/properties relationship of gas
   transport for this series of polymers. (C) 2011 Elsevier B. V. All
   rights reserved.}},
