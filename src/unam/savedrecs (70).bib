@article{ ISI:000209151600015,
Author = {Lemanska, Magdalena and Zuazua, Rita},
Title = {{CONVEX UNIVERSAL FIXERS}},
Journal = {{DISCUSSIONES MATHEMATICAE GRAPH THEORY}},
Year = {{2012}},
Volume = {{32}},
Number = {{4}},
Pages = {{807-812}},
Abstract = {{In {[}1] Burger and Mynhardt introduced the idea of universal fixers.
   Let G = (V, E) be a graph with n vertices and G' a copy of G. For a
   bijective function pi : V(G) -> V(G'), define the prism pi G of G as
   follows: V(pi G) = V(G) U V(G') and E(pi G) = E(G) U E(G') U M-pi,M-
   where M pi = \{u pi r(u) u is an element of V(G)\}. Let gamma(G) be the
   domination number of G. If gamma(pi G) = gamma(G) for any bijective
   function it, then G is called a universal fixer. In {[}9] it is
   conjectured that the only universal fixers are the edgeless graphs (K)
   over bar (n).
   In this work we generalize the concept of universal fixers to the convex
   universal fixers. In the second section we give a characterization for
   convex universal fixers (Theorem 6) and finally, we give an in infinite
   family of convex universal fixers for an arbitrary natural number n >=
   10.}},
