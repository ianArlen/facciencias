@article{ ISI:000310153300034,
Author = {Galindo-Murillo, Rodrigo and Ruiz-Azuara, Lena and Moreno-Esparza,
   Rafael and Cortes-Guzman, Fernando},
Title = {{Molecular recognition between DNA and a copper-based anticancer complex}},
Journal = {{PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
Year = {{2012}},
Volume = {{14}},
Number = {{44}},
Pages = {{15539-15546}},
Abstract = {{The aim of this work is to describe the specific recognition site
   between DNA and an anticancer copper complex by means of computational
   methods. Molecular dynamics were used to find the preferred site of
   binding between selected DNA chains and
   {[}Cu(2,2'-bipyridine)(acetylacetonate)(H2O)](+) (Cas). Full DFT
   optimizations of selected geometries extracted from simulations,
   followed by a topological analysis of electron density allowed us to
   define the specific interactions inside the recognition site. Cas links
   deoxyribose-phosphate by a coordination bond between metal and the
   phosphate group. There are several C-H center dot center dot center dot
   pi, O center dot center dot center dot pi(C) and O center dot center dot
   center dot pi(N) interactions between the sugar group and the aromatic
