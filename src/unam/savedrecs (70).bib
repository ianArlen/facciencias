@article{ ISI:000298363500039,
Author = {Martinez, M. A. and Solis, C. and Andrade, E. and Mondragon, M. A. and
   Isaac-Olive, K. and Rocha, M. F.},
Title = {{Ionic liquids as passive monitors of an atmosphere rich in mercury}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
