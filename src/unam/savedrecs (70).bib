@article{ ISI:000299148200058,
Author = {Martinez-Vargas, Sergio and Valdes-Martinez, Jesus and Martinez, Arturo
   I.},
Title = {{Supramolecular architectures of Cu(II) with terpyridine and
   pyridyl-carboxylates}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE}},
Year = {{2011}},
Volume = {{1006}},
Number = {{1-3}},
Pages = {{425-433}},
Month = {{DEC 14}},
Abstract = {{New self assembled supramolecular structures of Cu(II) with terpyridine
   (trpy)and connected by pyridyl-carboxylates ligands were studied. In the
   supramolecular structures, Cu centers exhibit square pyramid geometry
   with a distortion caused by an (4 + 1) elongation. The square pyramid
   geometry in Cu centers was induced by trpy ligands, while the
   supramolecular architectures were self assembled and developed by
   3-pyridine or 4-pyridine carboxylates ligands and counteranions.
   Different arrangements were found in the synthesized complexes such as
   channels and 1-D chains. It was found that the supramolecular structures
   were developed by intermolecular hydrogen bonds and pi-pi stacking. (C)
   2011 Elsevier B.V. All rights reserved.}},
