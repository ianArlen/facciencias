Booktitle = {{XXXV SYMPOSIUM ON NUCLEAR PHYSICS}},
Series = {{Journal of Physics Conference Series}},
Year = {{2012}},
Volume = {{387}},
Note = {{35th Symposium on Nuclear Physics, Cocoyoc, MEXICO, JAN 03-06, 2012}},
Abstract = {{The contribution of strange quarks to the radius and the magnetic moment
   of the proton is discussed in the framework of the unquenched quark
   model in which the effects of the ss pairs (strange sea) are taken into
   account through a P-3(0) quark-antiquark pair creation mechanism. The
   results for the the strange magnetic moment and the strangeness radius
   of the proton are found to be compatible with zero, in agreement with
   the latest experimental results from parity-violating electron
   scattering experiments and recent lattice calculations.}},
