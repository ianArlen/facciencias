@article{ ISI:000301020500009,
Author = {Bryson, Jr., Robert W. and Omar Garcia-Vazquez, Uri and Riddle, Brett R.},
Title = {{Diversification in the Mexican horned lizard Phrynosoma orbiculare
   across a dynamic landscape}},
