@article{ ISI:000299148200095,
Author = {Granifo, Juan and Gavino, Ruben and Freire, Eleonora and Baggio, Ricardo},
Title = {{The new sulphur-containing ligand 4 `-(4-methylthiophenyl)-3,2 `:6 `,3
   `'-terpyridine (L1) and the supramolecular structure of the dinuclear
   complex {[}Zn-2(mu-L1)(acac)(4)] (acac = acetylacetonato): The key role
   of non-covalent S center dot center dot center dot O contacts and C-H
   center dot center dot center dot S hydrogen bonds}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE}},
Year = {{2011}},
Volume = {{1006}},
Number = {{1-3}},
Pages = {{684-691}},
Month = {{DEC 14}},
Abstract = {{The new terpyridyl ligand 4'-(4-methylthiophenyl)-3,2':6',3
   `'-terpyridine (Li)) combines with Zn(acac)(2) to form the dizinc
   complex {[}Zn-2(u-Li)(acac)(4)] (1) (acac = acetylacetonato). The single
   crystal structure of 1 shows the existence of two different Zn(acac)2
   centres bridged by the Li ligand through its two 3-pyridylnitrogen
   atoms. The acac and L1 ligands allow the generation of a series of
   intra- and intermolecular hydrogen bonds of the C-H center dot center
   dot center dot N, C-H center dot center dot center dot O and C-H center
   dot center dot center dot pi type. In addition, the presence of the
   CH3-S group in Li plays a crucial role by providing the non-covalent S
   center dot center dot center dot O, S center dot center dot center dot H
   and H(CH3)center dot center dot center dot O contacts, which permit to
   describe fully this unusual supramolecular network. The NMR spectra of 1
   in CDCI3 solution confirm the presence of the two zinc atoms joined by
   the bridging Li ligand, but, differently to the solid state, their
   environments are identical. (C) 2011 Elsevier B.V. All rights reserved.}},
