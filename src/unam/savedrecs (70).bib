@article{ ISI:000300884800005,
Author = {Esther Morales, Ma and Mejia, Pablo and de Jesus Gutierrez, Raul and
   Angel Diaz, Miguel and Vergara, Reyna},
Title = {{Economic and financial interactions Brazil and Mexico: What is your
   degree of integration?/Economic and Financial Interactions Between
   Brazil and Mexico: Which Degree of Integration?}},
Journal = {{PERFILES LATINOAMERICANOS}},
Year = {{2012}},
Volume = {{20}},
Number = {{39}},
Pages = {{117-150}},
Month = {{JAN-JUN}},
Abstract = {{This paper analyzes trade and financial transactions between Brazil and
   Mexico in order to evaluate the magnitude of their reciprocal
   integration. Our results suggest that both countries have successfully
   got inserted into the international economy, which can be observed in
   their high volumes of trade, in their receiving foreign direct
   investment and in the size of their capital markets, as well as in the
