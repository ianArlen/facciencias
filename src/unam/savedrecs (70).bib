@article{ ISI:000298076300001,
Author = {Olson, Mark E. and Fahey, Jed W.},
Title = {{Moringa oleifera: a multipurpose tree for the dry tropics}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
