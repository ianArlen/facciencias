@inproceedings{ ISI:000326239306017,
Author = {Veleros Valverde, Maria del Carmen and Garcia Hernandez, Monica},
Editor = {{Chova, LG and Torres, IC and Martinez, AL}},
Title = {{THE CASE METHOD ONLINE FOR THE DEVELOPMENT OF DIDACTIC SKILLS}},
Booktitle = {{EDULEARN12: 4TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
