@inproceedings{ ISI:000339479900123,
Author = {Barbachano-Torres, A. and Ramos-Valdivia, A. C. and Cerda-Garcia-Rojas,
   C. M. and Salgado-Rodriguez, L. M. and Flores-Ortiz, C. and
   Ponce-Noyola, T.},
