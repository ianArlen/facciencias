@article{ ISI:000297536100013,
Author = {Guenther, Jeannette and Diaz, Veronica and Pedernera, Enrique and
   Mendez, Carmen},
Title = {{Gonadotropins regulate cadherin expression in the ovary of the chicken
   embryo (Gallus gallus)}},
Journal = {{GENERAL AND COMPARATIVE ENDOCRINOLOGY}},
Year = {{2011}},
Volume = {{174}},
Number = {{3}},
Pages = {{344-347}},
Month = {{DEC 1}},
Abstract = {{Cadherins are adhesion molecules that play a crucial role in tissue
   morphogenesis. Studies on N-cadherin and E-cadherin in the ovary of
   fetal hamster suggest that these adhesion molecules are involved in
   primordial follicle formation. In chicken embryo, present results
   demonstrate that N-cadherin is located on the surface epithelium and in
   the cortical cords of the ovary. Moreover, N-cadherin is identified in
   germ cells on day 14 of chicken embryo development. Quantification of
   mRNA of N-cadherin and E-cadherin demonstrates that treatments with
   follicle-stimulating hormone (FSH) or human chorionic gonadotropin
   (hCG), increase N-cadherin expression. Whereas. E-cadherin expression is
   decreased by gonadotropin treatments. The negative correlation between
   both cadherins expression is demonstrated after 18 h of hormonal
   treatment. Regulation of cadherin expression by gonadotropins and the
   presence of N-cadherin in the ovarian cortex suggest that these adhesion
   molecules are involved in ovarian morphogenesis in the chicken embryo.
   (C) 2011 Elsevier Inc. All rights reserved.}},
