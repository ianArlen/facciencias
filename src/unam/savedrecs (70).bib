@article{ ISI:000298198100010,
Author = {Ramirez-Mendoza, A. and Perez-Silva, J. L. and Lara-Rosano, F.},
Title = {{Electronic Implementation of a Fuzzy Neuron Model With a Gupta
   Integrator}},
Journal = {{JOURNAL OF APPLIED RESEARCH AND TECHNOLOGY}},
Year = {{2011}},
Volume = {{9}},
Number = {{3}},
Pages = {{380-393}},
Month = {{DEC}},
Abstract = {{In this paper the electronic circuit implementation of a fuzzy neuron
   model with a fuzzy Gupta integrator is presented. This neuron model
   simulates the performance and the fuzzy response of a fast-spiking
   biological neuron. The fuzzy neuron response is analyzed for two
   classical (non-fuzzy) input signals, the results are spike trains with
   relative and absolute refractory period and an axonal delay. A
   comparison between the response of the proposed fuzzy neuron model and
   the intracellular registers of biological fast-spiking cortical
   interneurons is made, as well as the transients presented at the
   beginning of each spike train. Also the results obtained from the
   electronic circuit of the fuzzy neuron model with the Matlab (TM)
   simulation of the mathematical model are compared.}},
