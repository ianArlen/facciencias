@article{ ISI:000435649100025,
Author = {Muro-Torres, Victor M. and Amezcua, Felipe and Lara-Mendoza, Raul E. and
   Buszkiewicz, John T. and Amezcua-Linares, Felipe},
Title = {{Trophic ecology of the chihuil sea catfish (Bagre panamensis) in the
   south-east Gulf of California, Mexico}},
Journal = {{JOURNAL OF THE MARINE BIOLOGICAL ASSOCIATION OF THE UNITED KINGDOM}},
Year = {{2018}},
Volume = {{98}},
Number = {{4}},
Pages = {{885-893}},
Month = {{JUN}},
Abstract = {{The trophic ecology of the chihuil sea catfish Bagre panamensis was
   studied through high-resolution variations in its feeding habits and
   trophic position (TP) in the SE Gulf of California, relevant to sex,
   size and season. The combined use of stomach content (SCA) and stable
   isotope analysis (SIA) allowed us to perform these analyses and also
   estimate the TP of its preys. Results of this study show that the
   chihuil sea catfish is a generalist and opportunistic omnivore predator
   that consumes primarily demersal fish and peneid shrimps. Its diet did
   not vary with climatic season (rainy or dry), size or sex. Results from
   the SIA indicated high plasticity in habitat use and prey species. The
   estimated TP value was 4.19, which indicates a tertiary consumer from
   the soft bottom demersal community in the SE Gulf of California, preying
   on lower trophic levels, which aids in understanding the species'
   trophic role in the food web. Because this species and its prey are
   important to artisanal and industrial fisheries in the Gulf of
   California, diet assimilation information is useful for the potential
   establishment of an ecosystem-based fisheries management in the area.}},
