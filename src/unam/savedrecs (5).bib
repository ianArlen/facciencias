@article{ ISI:000447343200001,
Author = {Cherednichenko, Kirill D. and Kiselev, Alexander V. and Silva, Luis O.},
Title = {{FUNCTIONAL MODEL FOR EXTENSIONS OF SYMMETRIC OPERATORS AND APPLICATIONS
   TO SCATTERING THEORY}},
