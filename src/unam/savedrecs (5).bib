@article{ ISI:000431651600019,
Author = {Ding, Chenliang and Wei, Jingsong and Xiao, Mufei},
Title = {{Super-resolution imaging based on the temperature-dependent
   electron-phonon collision frequency effect of metal thin films}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2018}},
Volume = {{123}},
Number = {{17}},
Month = {{MAY 7}},
Abstract = {{We herein propose a far-field super-resolution imaging with metal thin
   films based on the temperature-dependent electron-phonon collision
   frequency effect. In the proposed method, neither fluorescence labeling
   nor any special properties are required for the samples. The 100 nm
   lands and 200 nm grooves on the Blu-ray disk substrates were clearly
   resolved and imaged through a laser scanning microscope of wavelength
   405 nm. The spot size was approximately 0.80 mu m, and the imaging
   resolution of 1/8 of the laser spot size was experimentally obtained.
   This work can be applied to the far-field super-resolution imaging of
   samples with neither fluorescence labeling nor any special properties.
   Published by AIP Publishing.}},
