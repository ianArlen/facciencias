@article{ ISI:000444752100043,
Author = {Schoenfelder, Stefan and Javierre, Biola-Maria and Furlan-Magaril, Mayra
   and Wingett, Steven W. and Fraser, Peter},
Title = {{Promoter Capture Hi-C: High-resolution, Genome-wide Profiling of
   Promoter Interactions}},
