@article{ ISI:000430147200004,
Author = {Alias, Luis J. and Melendez, Josue and Palmas, Oscar},
Title = {{Hypersurfaces with constant scalar curvature in space forms}},
Journal = {{DIFFERENTIAL GEOMETRY AND ITS APPLICATIONS}},
Year = {{2018}},
Volume = {{58}},
Pages = {{65-82}},
Month = {{JUN}},
Abstract = {{In this paper we study the rigidity of complete hypersurfaces with
   constant scalar curvature in Riemannian space forms. Under an
   appropriate constraint on Phi, the traceless part of its second
   fundamental form, we prove that either the hypersurface is totally
   umbilical or it holds a sharp estimate for the supremum of the norm of
   Phi, with equality if and only if the hypersurface is isoparametric with
   two distinct principal curvatures. Moreover, we also construct complete
   non-isoparametric rotational examples which show that our constraint on
   Phi is sharp and necessary. (C) 2018 Elsevier B.V. All rights reserved.}},
