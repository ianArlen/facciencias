@article{ ISI:000433001000001,
Author = {Pablo-Pedro, Ricardo and Lopez-Rios, Hector and Mendoza-Cortes, Jose-L
   and Kong, Jing and Fomine, Serguei and Van Voorhis, Troy and
   Dresselhaus, Mildred S.},
Title = {{Exploring Low Internal Reorganization Energies for Silicene Nanoclusters}},
Journal = {{PHYSICAL REVIEW APPLIED}},
Year = {{2018}},
Volume = {{9}},
Number = {{5}},
Month = {{MAY 9}},
Abstract = {{High-performance materials rely on small reorganization energies to
   facilitate both charge separation and charge transport. Here, we perform
   density-functional-theory calculations to predict small reorganization
   energies of rectangular silicene nanoclusters with hydrogen-passivated
   edges denoted by H-SiNC. We observe that across all geometries, H-SiNCs
   feature large electron affinities and highly stabilized anionic states,
   indicating their potential as n-type materials. Our findings suggest
   that fine-tuning the size of H-SiNCs along the ``zigzag{''} and
   ``armchair{''} directions may permit the design of novel n-type
   electronic materials and spintronics devices that incorporate both high
   electron affinities and very low internal reorganization energies.}},
