@article{ ISI:000423247900003,
Author = {Medina-Barcenas, Mauricio and Morales-Callejas, Lorena and Shaid
   Sandoval-Miranda, Martha Lizbeth and Zaldivar-Corichi, Angel},
Title = {{Attaching topological spaces to a module (I): Sobriety and spatiality}},
Journal = {{JOURNAL OF PURE AND APPLIED ALGEBRA}},
Year = {{2018}},
Volume = {{222}},
Number = {{5}},
Pages = {{1026-1048}},
Month = {{MAY}},
Abstract = {{In this paper we study some frames associated to an R-module M. We
   define semiprimitive submodules and we prove that they form an spatial
   frame canonically isomorphic to the topology of Max(M). We characterize
   the soberness of Max(M) in terms of the point space of that frame.
   Beside of this, we study the regularity of an spatial frame associated
   to M given by annihilator conditions. (C) 2017 Elsevier B.V. All rights
   reserved.}},
