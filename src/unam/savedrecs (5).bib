Title = {{Dahlia tamaulipana (Asteraceae, Coreopsideae), a new species from the
   Sierra Madre Oriental biogeographic province in Mexico}},
Journal = {{PHYTOTAXA}},
Year = {{2018}},
Volume = {{349}},
