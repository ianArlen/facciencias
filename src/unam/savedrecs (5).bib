@article{ ISI:000429175700071,
Author = {Singh, Vikash and Sharma, Subhash and Dwivedi, R. K.},
Title = {{Improved dielectric, magnetic and optical properties of Pr and Ti
   co-substituted BFO ceramics}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2018}},
Volume = {{747}},
Pages = {{611-620}},
Month = {{MAY 30}},
Abstract = {{Bi1-xPrxFe1-xTixO3 ceramics with x <= 0.20 were synthesized by solid
   state reaction method. XRD patterns confirm single phase formation for
   all the samples. Rietveld refinement of diffraction data reveals the
   structural transition from rhombohedral (R3c) phase for x <= 0.10 to
   orthorhombic (Pnma) phase for x >= 0.15. Temperature dependence
   dielectric plots exhibit two dielectric anomalies one at low temperature
   (similar to 250 degrees C) and other at high temperature (similar to 370
   degrees C) corresponding to the magnetic transition temperature TN.
   Dielectric properties of samples were improved by controlling the amount
   of co doping to help ferroelectric measurements. Room temperature M - H
   measurements have shown an increase in magnetization with x which may be
