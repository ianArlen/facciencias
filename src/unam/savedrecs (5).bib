@article{ ISI:000439194400004,
Author = {Hernandez-Santamaria, Victor and de Teresa, Luz},
Title = {{ROBUST STACKELBERG CONTROLLABILITY FOR LINEAR AND SEMILINEAR HEAT
   EQUATIONS}},
Journal = {{EVOLUTION EQUATIONS AND CONTROL THEORY}},
Year = {{2018}},
Volume = {{7}},
Number = {{2}},
Pages = {{247-273}},
Month = {{JUN}},
Abstract = {{In this paper, we present a Stackelberg strategy to control a
   semi-linear parabolic equation. We use the concept of hierarchic control
   to combine the concepts of controllability with robustness. We have a
   control named the leader which is responsible for a controllability to
   trajectories objective. Additionally, we have a control named the
   follower, that solves a robust control problem. That means we solve for
   the optimal control in the presence of the worst disturbance case. In
   this way, the follower control is insensitive to a broad class of
   external disturbances.}},
