@article{ ISI:000432457600051,
Author = {Reyes-Coronado, Alejandro and Morales-Luna, Gesuri and Vazquez-Estrada,
   Omar and Garcia-Valenzuela, Augusto and Barrera, Ruben G.},
Title = {{Analytical modeling of optical reflectivity of random plasmonic
   nano-monolayers}},
