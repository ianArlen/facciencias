@article{ ISI:000431873800012,
Author = {Barriga-Montoya, C. and Huanosta-Gutierrez, A. and Reyes-Vaca, A. and
   Hernandez-Cruz, A. and Picones, A. and Gomez-Lagunas, F.},
Title = {{Inhibition of the K+ conductance and Cole-Moore shift of the oncogenic
   Kv10.1 channel by amiodarone (vol 470, pg 491, 2018)}},
Journal = {{PFLUGERS ARCHIV-EUROPEAN JOURNAL OF PHYSIOLOGY}},
Year = {{2018}},
Volume = {{470}},
Number = {{6}},
Pages = {{981-993}},
Month = {{JUN}},
Abstract = {{The ectopic overexpression of the voltage-dependent Eag1 (Kv10.1) K+
   channel is associated with the cancerous phenotype in about 70\% of
   human cancers and tumor cell lines. Recent reports showed that, compared
   with the canonical Shaker-related Kv family, Kv10.1 presents unique
   structural and functional properties. Herein, we report the interaction
   of the class III antiarrhythmic compound amiodarone with Kv10.1. Using
   whole-cell patch clamp, we found that amiodarone inhibits Kv10.1 channel
   conductance with nanomolar affinity. Additionally, and interestingly, we
   also report that amiodarone inhibits the characteristic Cole-Moore shift
   of Eag1 channels. Our observations are interpreted considering the
   structural-functional characteristics of these channels. We conclude
   that amiodarone possibly binds with high affinity to the voltage sensor
   module, altering the gating of Kv10.1.}},
