@article{ ISI:000304792000008,
Author = {Correa Cuevas, Laura Paloma and Aguilar Franco, Manuel and Hernandez
   Baltazar, Efren},
Title = {{Indirect microwave heating to pharmaceutical excipients: Lactose hydrate}},
