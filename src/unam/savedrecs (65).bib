@article{ ISI:000302935900006,
Author = {Klimova, Elena I. and Sanchez Garcia, Jessica J. and Klimova, Tatiana
   and Ramirez Apan, Teresa and Vazquez Lopez, Eduardo A. and Flores-Alamo,
   Marcos and Martinez Garcia, Marcos},
Title = {{Synthesis and biological evaluation of novel ethyl
   2-amino-6-ferrocenyl-1,6-dihydropyrimidine-5-carboxylates and ethyl
   2-amino-6-ferrocenylpyrimidine-5-carboxylates}},
Journal = {{JOURNAL OF ORGANOMETALLIC CHEMISTRY}},
Year = {{2012}},
Volume = {{708}},
Pages = {{37-45}},
Month = {{JUN 15}},
Abstract = {{Reactions of ethyl 2-acyl-3-ferrocenylacrylates (acyl = acetyl, benzoyl,
   p-nitrobenzoyl) with guanidine and 1,1-dimethylguanidine furnish ethyl
   2-amino-6-ferrocenyl-4-methyl(aryl)-1,6-dihydropyrimidine-5-carboxylates
   . Their oxidative dehydrogenation with PhI(OAc)(2) results in the
   corresponding ethyl 2-amino-6-ferrocenyl-4-methyl(aryl)
   pyrimidine-5-carboxylates. The structures of the synthesized compounds
   were established on the basis of the data from H-1 and C-13 NMR
   spectroscopy and confirmed by X-ray diffraction analysis. All compounds
   were tested in vitro against six human tumor cell lines U-251, PC-3,
   K-562, HCT-15, MCF-7 and SKLU-1 to assess their in vitro antitumor
   activity. The results suggest biological specificity towards PC-3 and
   K-562 cells for compounds 4a-c at doses 50 mu M, which are lower than
   cis-platin IC'(50)s in the two cell lines. Additionally, peritoneal
   mouse macrophages (M circle divide) were also evaluated for compounds
   4a-f and 5a-f. (C) 2012 Elsevier B.V. All rights reserved.}},
