@article{ ISI:000308537900003,
Author = {de Donato Rodriguez, Xavier and Arroyo Santos, Alfonso},
Title = {{The Structure of Idealization in Biological Theories: The Case of the
   Wright-Fisher Model}},
Journal = {{JOURNAL FOR GENERAL PHILOSOPHY OF SCIENCE}},
Year = {{2012}},
Volume = {{43}},
Number = {{1}},
Pages = {{11-27}},
Month = {{JUL}},
Abstract = {{In this paper we present a new framework of idealization in biology. We
   characterize idealizations as a network of counterfactual and
   hypothetical conditionals that can exhibit different ``degrees of
   contingency{''}. We use this idea to say that, in departing more or less
   from the actual world, idealizations can serve numerous epistemic,
   methodological or heuristic purposes within scientific research. We
   defend that, in part, this structure explains why idealizations, despite
   being deformations of reality, are so successful in scientific practice.
   For illustrative purposes, we provide an example from population
   genetics, the Wright-Fisher Model.}},
