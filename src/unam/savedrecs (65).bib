@article{ ISI:000304217000004,
Author = {Aronov, Boris and Garijo, Delia and Nunez-Rodriguez, Yurai and
   Rappaport, David and Seara, Carlos and Urrutia, Jorge},
Title = {{Minimizing the error of linear separators on linearly inseparable data}},
Journal = {{DISCRETE APPLIED MATHEMATICS}},
Year = {{2012}},
Volume = {{160}},
Number = {{10-11}},
Pages = {{1441-1452}},
Month = {{JUL}},
Abstract = {{Given linearly inseparable sets R of red points and B of blue points, we
   consider several measures of how far they are from being separable.
   Intuitively, given a potential separator ({''}classifier{''}), we
   measure its quality ({''}error{''}) according to how much work it would
   take to move the misclassified points across the classifier to yield
   separated sets. We consider several measures of work and provide
   algorithms to find linear classifiers that minimize the error under
   these different measures. (C) 2012 Elsevier B.V. All rights reserved.}},
