@article{ ISI:000326710500010,
Author = {Ortega-Martinez, Areli and Juarez-Lopez, Katy and Solorza-Feria, Omar
   and Ponce-Noyola, Maria T. and Rios-Leal, Elvira and
   Rinderknecht-Seijas, Noemi F. and Poggi-Varaldo, Hector M.},
Title = {{Parallel Connection and Sandwich Electrodes Lower the Internal
   Resistance in a Microbial Fuel Cell}},
Journal = {{JOURNAL OF NEW MATERIALS FOR ELECTROCHEMICAL SYSTEMS}},
Year = {{2012}},
Volume = {{15}},
Number = {{3}},
Pages = {{187-194}},
Month = {{JUL}},
Abstract = {{The aim of this work was to design and characterize a novel, multiface
   parallelepiped MFC in the perspective of decreasing the internal
   resistance (Rin) and increasing the volumetric power (Pr) output. The
   cell was fitted with a `sandwich' cathode-membraneanode assemblage in
   five of its faces, and possessed a ratio electrode surface
   area-to-volume (csi).of 19 m(-1). When the 5 faces of the MFC-P were
   connected in series, the Ri, was 601 Q with a voltage of 0.52 V.
   Characterization of the cell with the 5 faces connected in parallel gave
   a Rint of 62 Q with a voltage of 0.5 V that corresponded to external
   resistance of 56 1d2 in the polarization procedure. This result was
   ascribed to both the changes in cell architecture and decrease of the
   inter-electrode distance as well as the parallel connection. The Pv of
   the new MFC-P achieved values of 62 and 570 mW/m(-3) for series and
   parallel connection, respectively. Molecular ecological techniques were
   used to analyze the bacterial diversity of biocatalyst used in new
   design MFC-P. They showed a low species richness and low-to-moderate
   evenness. The community consisted primarily of 6-Proteobacteria and
   Firmicutes, bacteria that are recognized to be capable of exocellular
   electron transfer.}},
