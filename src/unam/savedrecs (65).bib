@article{ ISI:000306343400011,
Author = {Gomez-Larranaga, J. C. and Gonzalez-Acuna, F. and Heil, Wolfgang},
Title = {{Higher dimensional manifolds with S-1-category 2}},
Journal = {{MATHEMATISCHE ZEITSCHRIFT}},
Year = {{2012}},
Volume = {{271}},
Number = {{1-2}},
Pages = {{167-173}},
Month = {{JUN}},
Abstract = {{A closed topological n-manifold M-n is of S-1-category 2 if it can be
   covered by two open subsets W-1, W-2 such that the inclusions W-i -> M-n
   factor homotopically through maps W-i -> S-1. We show that for n > 3, if
   cat(S1) (M-n) = 2 then M-n is homeomorphic to S-n or Sn-1 x S-1 or the
   non-orientable Sn-1-bundle over S-1. We also obtain an unknotting
   theorem for locally flat knots of S-n 2 in S-n and a characterization of
   S-1 x S-n (1).}},
