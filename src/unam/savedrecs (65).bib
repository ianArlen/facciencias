@article{ ISI:000305658900009,
Author = {Gomez-Ortiz, Antonio and Palacios, David and Palade, Bogdan and
   Vazquez-Selem, Lorenzo and Salvador-Franch, Ferran},
Title = {{The deglaciation of the Sierra Nevada (Southern Spain)}},
