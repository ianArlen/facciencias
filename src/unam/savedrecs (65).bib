@article{ ISI:000304291600018,
Author = {Hernandez-Covarrubias, Carlos and Vilchis-Reyes, Miguel A. and
   Yepez-Mulia, Lilian and Sanchez-Diaz, Remedios and Navarrete-Vazquez,
   Gabriel and Hernandez-Campos, Alicia and Castillo, Rafael and
   Hernandez-Luis, Francisco},
Title = {{Exploring the interplay of physicochemical properties, membrane
   permeability and giardicidal activity of some benzimidazole derivatives}},
Journal = {{EUROPEAN JOURNAL OF MEDICINAL CHEMISTRY}},
Year = {{2012}},
Volume = {{52}},
Pages = {{193-204}},
Month = {{JUN}},
Abstract = {{This study evaluated the relationship between the physicochemical
   properties, membrane permeability and in vitro giardicidal activity of
   twenty nine benzimidazole derivatives (1-7n). The retention time data
   from reverse phase high performance chromatography (RP-HPLC) were used
   to estimate aqueous solubility and lipophilicity of these compounds. The
   apparent permeability was determined using Caco-2 cell monolayer. The
   calculation of some descriptors, such as Clog P. PSA, was performed
   using ACD labs software. For benzimidazole derivatives with NHCOOCH3,
   CH3, NH2, SH and SCH3 groups at the 2-position, a quadratic type of
   regression model was obtained with giardicidal activity and aqueous
   solubility or lipophilicity. On the other hand, giardicidal activity of
   2-(trifluoromethyl)-1H-benzimidazole derivatives was influenced by
   lipophilicity, hydrogen bond donors and molecular volume but it was not
   determined by their apparent permeability in Caco-2 cell line. (C) 2012
   Elsevier Masson SAS. All rights reserved.}},
