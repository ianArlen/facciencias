@article{ ISI:000306406700022,
Author = {Rodriguez Plaza, Jonathan G. and Villalon Rojas, Amanda and Herrera, Sur
   and Garza-Ramos, Georgina and Torres Larios, Alfredo and Amero, Carlos
   and Zarraga Granados, Gabriela and Gutierrez Aguilar, Manuel and Lara
   Ortiz, Maria Teresa and Polanco Gonzalez, Carlos and Uribe Carvajal,
   Salvador and Coria, Roberto and Pena Diaz, Antonio and Bredesen, Dale E.
   and Castro-Obregon, Susana and del Rio, Gabriel},
Title = {{Moonlighting Peptides with Emerging Function}},
Journal = {{PLOS ONE}},
Year = {{2012}},
Volume = {{7}},
Number = {{7}},
Month = {{JUL 13}},
Abstract = {{Hunter-killer peptides combine two activities in a single polypeptide
   that work in an independent fashion like many other multi-functional,
   multi-domain proteins. We hypothesize that emergent functions may result
   from the combination of two or more activities in a single protein
   domain and that could be a mechanism selected in nature to form
   moonlighting proteins. We designed moonlighting peptides using the two
   mechanisms proposed to be involved in the evolution of such molecules
   (i.e., to mutate non-functional residues and the use of natively
   unfolded peptides). We observed that our moonlighting peptides exhibited
   two activities that together rendered a new function that induces cell
   death in yeast. Thus, we propose that moonlighting in proteins promotes
   emergent properties providing a further level of complexity in living
   organisms so far unappreciated.}},
