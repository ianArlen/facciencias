@article{ ISI:000306727800006,
Author = {McMichael, C. H. and Correa-Metrio, A. and Bush, M. B.},
Title = {{Pre-Columbian fire regimes in lowland tropical rainforests of
   southeastern Peru}},
Journal = {{PALAEOGEOGRAPHY PALAEOCLIMATOLOGY PALAEOECOLOGY}},
