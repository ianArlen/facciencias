@article{ ISI:000307609900005,
Author = {Oeckl, Robert},
Title = {{The Schrodinger representation and its relation to the holomorphic
   representation in linear and affine field theory}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2012}},
Volume = {{53}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{We establish a precise isomorphism between the Schrodinger
   representation and the holomorphic representation in linear and affine
   field theory. In the linear case, this isomorphism is induced by a
   one-to-one correspondence between complex structures and Schrodinger
   vacua. In the affine case we obtain similar results, with the role of
   the vacuum now taken by a whole family of coherent states. In order to
   establish these results we exhibit a rigorous construction of the
   Schrodinger representation and use a suitable generalization of the
   Segal-Bargmann transform. Our construction is based on geometric
   quantization and applies to any real polarization and its pairing with
   any Kahler polarization. (C) 2012 American Institute of Physics.
   {[}http://dx.doi.org/10.1063/1.4731770]}},
