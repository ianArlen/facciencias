@article{ ISI:000307626100007,
Author = {Chan, Kai M. A. and Guerry, Anne D. and Balvanera, Patricia and Klain,
   Sarah and Satterfield, Terre and Basurto, Xavier and Bostrom, Ann and
   Chuenpagdee, Ratana and Gould, Rachelle and Halpern, Benjamin S. and
   Hannahs, Neil and Levine, Jordan and Norton, Bryan and Ruckelshaus, Mary
   and Russell, Roly and Tam, Jordan and Woodside, Ulalia},
Title = {{Where are Cultural and Social in Ecosystem Services? A Framework for
   Constructive Engagement}},
Journal = {{BIOSCIENCE}},
Year = {{2012}},
Volume = {{62}},
Number = {{8}},
Pages = {{744-756}},
Month = {{AUG}},
Abstract = {{A focus on ecosystem services (ES) is seen as a means for improving
   decisionmaking. In the research to date, the valuation of the material
   contributions of ecosystems to human well-being has been emphasized,
   with less attention to important cultural ES and nonmaterial values.
   This gap persists because there is no commonly accepted framework for
   eliciting less tangible values, characterizing their changes, and
   including them alongside other services in decisionmaking. Here, we
   develop such a framework for ES research and practice, addressing three
   challenges: (1) Nonmaterial values are ill suited to characterization
   using monetary methods; (2) it is difficult to unequivocally link
   particular changes in socioecological systems to particular changes in
   cultural benefits; and (3) cultural benefits are associated with many
   services, not just cultural ES. There is no magic bullet, but our
   framework may facilitate fuller and more socially acceptable
   integrations of ES information into planning and management.}},
