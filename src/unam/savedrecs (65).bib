@article{ ISI:000309333600001,
Author = {Chavez-Bejar, Maria I. and Baez-Viveros, Jose L. and Martinez, Alfredo
   and Bolivar, Francisco and Gosset, Guillermo},
Title = {{Biotechnological production of L-tyrosine and derived compounds}},
Journal = {{PROCESS BIOCHEMISTRY}},
