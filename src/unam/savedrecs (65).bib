@article{ ISI:000304386400004,
Author = {Elizondo, E. Javier and Kimura, Shun-ichi},
Title = {{Rationality of Motivic Chow series modulo A(1)-homotopy}},
Journal = {{ADVANCES IN MATHEMATICS}},
Year = {{2012}},
Volume = {{230}},
Number = {{3}},
Pages = {{876-893}},
Month = {{JUN 20}},
Abstract = {{Consider the formal power series Sigma{[}C-p,C-alpha(X)]t(alpha) (called
   the Motivic Chow series), where C-p(X) = coproduct C-p,C-alpha(X) is the
   Chow variety of X parametrizing the p-dimensional effective cycles on X
   with C-p,C-alpha (X) its connected components, and {[}C-p,C-alpha(X)]
   its class in K(ChM)(A1), the K-ring of Chow motives modulo A(1)
   homotopy. Using the Picard product formula and torus action, we will
   show that the Motivic Chow series is rational in many cases. (C) 2012
   Elsevier Inc. All rights reserved.}},
