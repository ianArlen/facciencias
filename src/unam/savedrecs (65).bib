@article{ ISI:000312940900005,
Author = {Reyes-Soto, Gervith and Perez-Cruz, Julio and Delgado-Reyes, Luis and
   Ortega-Gutierrez, Cesar and Tellez-Palacios, Daniela},
Title = {{Association of three anatomic variants of the anterior cerebral
   circulation}},
Journal = {{CIRUGIA Y CIRUJANOS}},
Year = {{2012}},
Volume = {{80}},
Number = {{4}},
Pages = {{308-313}},
Month = {{JUL-AUG}},
Abstract = {{Background: As part of a study of the microsurgical anatomy of the
   pericallosal artery, we describe one brain with three unusual anatomic
   variants.
   Methods: From the autopsy findings of a 45-year-old female, we extracted
   the brain. All the arterial blood vessels were washed with saline
   solution to be injected afterwards with red latex. The brain was then
   immersed in 10\% formalin for 2 months. Finally, we dissected and
   measured the internal carotid artery segments using a digital Vernier
   caliper under a Carl Zeiss OPMI surgical microscope (magnification x6 to
   x40).
   Results: Weight of the brain was 1250 g and three rare anatomic variants
   were found: 1) right accessory middle cerebral artery (MCA-r), 2) right
   bihemispheric anterior cerebral artery (ACABihem-r), 3) median artery of
   the corpus callosum (MACC).
   Conclusion: Association of the anatomic variations described previously
   is inconsistent; furthermore, their appearance in a single case is rare.}},
