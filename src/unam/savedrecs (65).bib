@article{ ISI:000306916900023,
Author = {Gonzalez, Tenoch and Moreno, Jaime A. and Fridman, Leonid},
Title = {{Variable Gain Super-Twisting Sliding Mode Control}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
Year = {{2012}},
Volume = {{57}},
Number = {{8, SI}},
Pages = {{2100-2105}},
Month = {{AUG}},
Abstract = {{In this note, a novel, Lyapunov-based, variable-gain super-twisting
   algorithm (STA) is proposed. It ensures for linear time invariant
   systems the global, finite-time convergence to the desired sliding
   surface, when the matched perturbations/uncertainties are
   Lipschitz-continuous functions of time, that are bounded, together with
   their derivatives, by known functions. The proposed algorithm has
   similar properties to the variable-gain first-order sliding mode
   control, but it provides alleviation to the chattering phenomenon. The
   results are verified experimentally.}},
