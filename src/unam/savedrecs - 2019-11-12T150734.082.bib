@inproceedings{ ISI:000171689400012,
Author = {Moshinsky, M},
Editor = {{SaintAubin, Y and Vinet, L}},
Title = {{The relativistic oscillator and the mass spectra of baryons}},
Booktitle = {{ALGEBRAIC METHODS IN PHYSICS: SYMPOSIUM FOR THE 60TH BIRTHDAYS OF JIRI
   PATERA AND PAVEL WINTERNITZ}},
Series = {{CRM SERIES IN MATHEMATICAL PHYSICS}},
Year = {{2001}},
Pages = {{173-191}},
Note = {{Symposium in Honour of Jiri Patera and Pavel Winternitz on Algebraic
   Methods in Physics, MONTREAL, CANADA, JAN, 1997}},
Abstract = {{An analysis of the baryon spectra using harmonic oscillator states has
   been carried out by many authors, usually in a nonrelativistic
   approximation, and employing a great number of parameters. In the
   present paper we perform this analysis using a single relativistic
   equation that has only one parameter, the frequency omega of the
   oscillator. By eliminating the center of mass motion, the problem can be
   formulated in terms of the generators of an SU(2) group, and Racah
   algebra allows us then to obtain a reasonable energy spectra for
   nonstrange baryons, by appropriately choosing our single variational
   parameter omega.}},
