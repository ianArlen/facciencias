@article{ ISI:000166580800012,
Author = {Buenrostro, O and Bocco, G and Vence, J},
Title = {{Forecasting generation of urban solid waste in developing countries - A
   case study in Mexico}},
Journal = {{JOURNAL OF THE AIR \& WASTE MANAGEMENT ASSOCIATION}},
Year = {{2001}},
Volume = {{51}},
Number = {{1}},
Pages = {{86-93}},
Month = {{JAN}},
Abstract = {{Based on a study of the composition of urban solid waste (USW) and of
   socioeconomic variables in Morelia, Mexico, generation rates were
   estimated. In addition, the generation of residential solid waste (RSW)
   and nonresidential solid waste (NRSW) was forecasted by means of a
   multiple linear regression (MLR) analysis. For residential sources, the
   independent variables analyzed were monthly wages, persons per dwelling,
   age, and educational level of the heads of the household. For
   nonresidential sources, variables analyzed were number of employees,
   area of facilities, number of working days, and working hours per day.
   The forecasted values for residential waste were similar to those
   observed. This approach may be applied to areas in which available data
   are scarce, and in which there is an urgent need for the planning of
   adequate management of USW.}},
