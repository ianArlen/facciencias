@article{ ISI:000174249900006,
Author = {Jimenez-Juarez, R and Larque-Saavedra, A and Albores-Velasco, M},
Title = {{Inhibitory activity of 1,4-oxathiins on tobacco mosaic virus}},
Journal = {{PHYTON-INTERNATIONAL JOURNAL OF EXPERIMENTAL BOTANY}},
Year = {{2001}},
Number = {{SI}},
Pages = {{47-56}},
Abstract = {{Vitavaz (5,6-dihydro-2-methyl-carboxanilide-1,4-oxatiin) has been widely
   reported as a systemic fungicide. The compound has four of the five
   proposed characteristics for a good viricidal compound. Besides, it has
   the C=C-C(O)-NH-unit which has been reported in most viricides. In this
   work, Vitavaz and six substituted Vitavaz analogs varying in electronic
   properties were synthesized and their activity against the tobacco
   mosaic virus was assesed using three conditions. Oxatiines inhibited to
   a certain degree the growth of the virus although not completely. The
   results of the bioassays seem to indicate that the inhibition results
   from a metabolic response of the plant to oxatiins.}},
