@inproceedings{ ISI:000172834600190,
Author = {Huizar, AR and Carrillo, MM and Hernandez-Garcia, G and Glz, JER},
Editor = {{Seiler, KP and Wohnlich, S}},
Title = {{Structure and groundwater flow in the Zumpango-Pachuca region, central
   Mexico}},
Booktitle = {{NEW APPROACHES CHARACTERIZING GROUNDWATER FLOW, VOLS 1 AND 2}},
Year = {{2001}},
Pages = {{961-964}},
Note = {{31st International-Association-of-Hydrogeologists Congress, MUNICH,
   GERMANY, SEP 10-14, 2001}},
Organization = {{Int Assoc Hydrogeologists; Assoc Tracer Hydrologists}},
Abstract = {{Geological and structural studies were carried out in the region
   Zumpango-Pachuca (Zumpango sub-basin), to characterise the main shallow
   structure of the region. The structure is interpreted as a series of
   horsts limited by normal faults. An upper aquitard, an unconfined upper
   aquifer, a lower aquitard and a deep aquifer have been defined. The
   upper aquifer is exploited in an uncontrolled manner. In the aquitard
   processes of compaction and fracturing appear to be due to abstraction
   that produces drainage from the aquitard, inducing also changes in the
   quality of the obtained groundwater in the upper aquifer. The proposed
   geologic structure and the hydraulic gradient suggests that this region
   constitutes an important aquifer system, where the vertical component of
   groundwater flows appears to be significant.}},
