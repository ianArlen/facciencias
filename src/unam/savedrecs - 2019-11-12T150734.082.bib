@article{ ISI:000167992100003,
Author = {Garcia-Guzman, G and Wennstrom, A},
Title = {{Interactions between two rust fungi and their host plant Anemone
   nemorosa}},
Journal = {{ECOGRAPHY}},
Year = {{2001}},
Volume = {{24}},
Number = {{1}},
Pages = {{25-32}},
Month = {{FEB}},
Abstract = {{In a field study we found that two rust fungi, Tranzchelia anemones and
   Ochropsora ariae on the shared host. Anemone nemorosa varied in
   abundance among populations. Tranzchelia anemones was favoured in high
   light habitats and was particularly common in mixed deciduous forests as
   well as productive grasslands, whereas the abundance and occurrence of
   O. ariae was dependent on the presence of the alternate host, Sorbus
   aucuparia When found in the same population, the two rusts rarely
   attacked the same ramet. Both T, anemones and O. ariae had a negative
   effect on plant performance by reducing flower production, and rhizome
   length. Diseased shoots were also elongated.}},
