Title = {{Study of fragmented fossil diatoms using an invariant correlation method}},
Booktitle = {{4TH IBEROAMERICAN MEETING ON OPTICS AND 7TH LATIN AMERICAN MEETING ON
   OPTICS, LASERS, AND THEIR APPLICATIONS}},
Series = {{Proceedings of SPIE-The International Society for Optical Engineering}},
