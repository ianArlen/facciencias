@article{ ISI:000166681200002,
Author = {Sebastian, PJ and Ocampo, AL and Moreira, J},
Title = {{Chemical synthesis and characterization of MoxSy (CO)(n) and
   Mo-x(CO)(n): Possible application as catalysts for oxygen reduction
   reaction, hydrogen evolution and supercapacitors}},
Journal = {{JOURNAL OF NEW MATERIALS FOR ELECTROCHEMICAL SYSTEMS}},
Year = {{2001}},
Volume = {{4}},
Number = {{1, SI}},
Pages = {{3-6}},
Month = {{JAN}},
Abstract = {{Mo-x(CO)(n) and MoxSy(CO)(n) were formed by a chemical process in Xylene
   medium at 140 degreesC. The catalysts were characterized using
   techniques such as XRD, SEM, TEM, FTIR spectroscopy and cyclic as well
   as linear voltametry measurements. The results indicated that chemically
   synthesized Mo-x(CO)(n) does not possesses much catalytic activity for
   oxygen reduction. But it may be used as catalyst for hydrogen evolution
   reaction. Incorporation of sulfur to the carbonyl compound seems to
   nullify the catalytic activity of the material. Mo-x(CO)(n) is formed in
   amorphous form, where as, addition of sulfur to this material transforms
   it into polycrystalline in nature. The cyclic voltammograms indicated
   their possible application in electrochemical supercapacitors.}},
