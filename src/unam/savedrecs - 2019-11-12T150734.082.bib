@inproceedings{ ISI:000173496300002,
Author = {Blanco-Cano, X and Russell, CT and Strangeway, RJ and Kivelson, MG and
   Khurana, KK},
Editor = {{Thomas, N and Coustenis, A and LeliwaKopysty, J and Kossacki, KJ}},
Title = {{Galileo observations of ion cyclotron waves in the Io torus}},
Booktitle = {{IO, EUROPA, TITAN AND CRATERING OF ICY SURFACES}},
