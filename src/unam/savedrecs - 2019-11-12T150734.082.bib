@article{ ISI:000167081600004,
Author = {Alvarez, J and Folch-Gabayet, M and Perez-Esteva, S},
Title = {{Banach spaces of solutions of the Helmholtz equation in the plane}},
Journal = {{JOURNAL OF FOURIER ANALYSIS AND APPLICATIONS}},
Year = {{2001}},
Volume = {{7}},
Number = {{1}},
Pages = {{49-62}},
Abstract = {{The purpose of this article is to study the Hilbert space W-2 consisting
   of all solutions of the Helmholtz equation Deltau + u = 0 in R-2 that
   are the image under the Fourier transform of L-2 densities in the unit
   circle. We characterize this space as a close subspace of the Hilbert
   space H-2 of all functions belonging to
   L-2(\textbackslash{}x\textbackslash{}(-3)dx) jointly with their angular
   and radial derivatives, in the complement of the unit disk in R-2 We
   calculate the reproducing kernel of W2 and study its reproducing
   properties in the corresponding spaces H-p, for p > 1.}},
