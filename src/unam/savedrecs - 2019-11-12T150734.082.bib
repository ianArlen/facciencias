@inproceedings{ ISI:000175097400003,
Author = {Jauregui, J and Valderrama, B and Vazquez-Duhalt, R},
Editor = {{Magar, VS and VonFahnestock, FM and Leeson, A}},
Title = {{Biocatalytic oxidation of organophosphorous pesticides by white rot
   fungi}},
Booktitle = {{EX SITU BIOLOGICAL TREATMENT TECHNOLOGIES}},
Series = {{BIOREMEDIATION SERIES}},
Year = {{2001}},
Volume = {{6}},
Number = {{6}},
Pages = {{17-25}},
Note = {{6th International In Situ and On-Site Bioremediation Symposium, SAN
   DIEGO, CA, JUN 04-07, 2001}},
Organization = {{Battelle Mem Inst; Geomatrix Consultants Inc; IT Grp Inc; Parsons;
   Regenesis; USAF, Ctr Environm Excellence; USN, Fac Engn Command}},
Abstract = {{The enzymatic mechanism for the degradation of organophosphorus (OPs)
   pesticides by different white-rot fungi strains was studied. A
   collection of 17 different fungi metabolized 10\% to 97\% of added
   parathion in 96 hrs. Three strains showing the highest activities were
   selected for the degradation of other OPs pesticides: Bjerkandera adusta
   8258, Pleurotus ostreatus 7989 and Phanerocheate chrysosporium 3641. All
   strains degraded terbufos, azinphos-methyl, phosmet and DEF, from 50\%
   to 96\% of these compounds in 96 hrs. In order to identify the origin of
   the degradation activity, extracellular fungal extracts induced for
   ligninolytic enzymes were evaluated. While the activity of lignin
   peroxidase, manganese peroxidase, and laccase were detected in the
   extracts, no enzymatic modification of five OPs pesticides tested could
   be found suggesting the intracellular origin of the degradation
   activity, Microsomal preparations from B. adusta 8258 were obtained and
   tested, the degregadation rates for the microsomal fraction were: 10
   mumol mg-prot(-1) h(-1) for phosmet, 5.7 mumol mg-prot-1 h-1 for
   terbufos, and 2,2 mumol mg-prot-1 h(-1) for azinphos-methyl. These
   results indicate that in ligninolytic fungi the OP pesticides
   degradation activity is intracellular. The stringent requirement for
   NADPH during the degradation suggest cytochrome P450 might be involved.}},
