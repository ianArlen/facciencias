@article{ ISI:000167772000011,
Author = {Cespedes, CL and Calderon, JS and Salazar, JR and Lotina-Hennsen, B and
   Segura, R},
Title = {{Plant-growth inhibitory activity of cedrelanolide from Cedrela
   salvadorensis}},
Journal = {{JOURNAL OF CHEMICAL ECOLOGY}},
Year = {{2001}},
Volume = {{27}},
Number = {{1}},
Pages = {{137-149}},
Month = {{JAN}},
Abstract = {{The effect of cedrelanolide, the most abundant limonoid isolated from
   Cedrela salvadorensis (Meliaceae), was assayed as a plant-growth
   inhibitory compound against monocotyledonous and dicotyledonous seeds.
   This compound inhibited germination. seed respiration, and seedling dry
   weights of some plant species (Lolium multiflorum, var. Hercules,
   Triticum vulgare. var Salamanca, Physalis ixocarpa, and Trifolium
   alexandrinum). Our results indicate that cedrelanolide interferes with
   monocot preemergence properties, mainly energy metabolism of the seeds
   at the level of respiration. In addition, the compound inhibits
   photophosphorylation. H+ uptake. and noncyclic electron Row. This
   behavior might be responsible for its plant-growth inhibitory properties
   and its possible role as an allelopathic agent.}},
