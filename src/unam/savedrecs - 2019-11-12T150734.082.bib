@article{ ISI:000089296600001,
Author = {Perez, G and Jimenez-Gonzalez, AE},
Title = {{Electrical circuit model for thermionic converters}},
Journal = {{ENERGY CONVERSION AND MANAGEMENT}},
Year = {{2001}},
Volume = {{42}},
Number = {{1}},
Pages = {{1-9}},
Month = {{JAN}},
Abstract = {{An electrical circuit model which accounts for the internal electrical
   characteristics of a real vacuum thermionic converter (VTC) is
   presented. Using this model, the expected theoretical behavior of the
   VTC was determined in its equilibrium state. Besides this, its dynamic
   behavior was found when the VTC was operated under transient conditions.
   It is shown that the theoretical asymptotic tendency defined by the
   electrical model approaches the experimental behavior when an adequate
   choice is made for fitting the employed values of the internal elements
   in the circuit model. In this study, we also included theoretical
   results obtained by the electrical model by variation of the temperature
   and work function of the emitter and also by variation of the
   interelectrode impedance. (C) 2000 Elsevier Science Ltd. All rights
   reserved.}},
