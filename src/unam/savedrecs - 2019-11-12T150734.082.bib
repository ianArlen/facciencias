@article{ ISI:000166756700003,
Author = {Basiuk, VA and Basiuk, EV and Gomez-Lara, J},
Title = {{Molecular modeling of octahedral complex cations composed of
   {[}Ni(II)(rac-Me-6{[}14]aneN(4))](2+) units and bidentate carboxylate
   ligands}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE-THEOCHEM}},
