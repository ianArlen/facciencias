@article{ ISI:000180441100014,
Author = {Echeverria, AB and Robles, ALG},
Title = {{The history of science and the introduction of plant genetics in Mexico}},
Journal = {{HISTORY AND PHILOSOPHY OF THE LIFE SCIENCES}},
