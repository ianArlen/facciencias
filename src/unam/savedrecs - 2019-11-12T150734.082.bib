@inproceedings{ ISI:000174014500261,
Author = {Nocetti, FG and Gonzalez, JS and Stojmenovic, I and Stojmenovic, M},
Editor = {{Arabnia, HR}},
Title = {{Addressing and routing in higher dimensional hexagonal networks}},
Booktitle = {{PDPTA'2001: PROCEEDINGS OF THE INTERNATIONAL CONFERENCE ON PARALLEL AND
   DISTRIBUTED PROCESSING TECHNIQUES AND APPLICATIONS}},
Year = {{2001}},
Pages = {{1726-1732}},
Note = {{International Conference on Parallel and Distributed Processing
   Techniques and Applications (PDPTA 2001), LAS VEGAS, NV, JUN 25-28, 2001}},
Organization = {{Comp Sci Res Educ \& Applicat Press; Int Technol Inst; Korea Informat
   Processing Soc; World Acad Sci Informat Technol; PACT Corp}},
Abstract = {{We define the higher dimensional hexagonal graphs as a generalization of
   triangular plane tessellation, and consider it as a multiprocessor
   interconnection network, Nodes in a k-D hexagonal network are placed at
   the vertices of a k-D triangular tessellation, so that each node has up
   to 2k+2 neighbors. In this paper, we propose a simple addressing scheme
   for nodes, which leads to a straightforward formula for distance between
   nodes and a very simple and elegant routing algorithm. The number of
   shortest path between any two nodes and their description are also
   provided in this paper. We then derived closed formulas for the surface
   area (volume) of these networks, which are defined as the number of
   nodes located at given distance (up to given distance, respectively)
   from the origin node. The number of nodes and the network diameter under
   a more symmetrical border conditions are also derived.}},
