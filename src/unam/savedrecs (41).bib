@article{ ISI:000348377000007,
Author = {Lopez-Tellez, Juan M. and Bruce, Neil C. and Delgado-Aguillon, Jesus and
   Garduno-Mejia, Jesus and Avendano-Alejo, Maximino},
Title = {{Experimental method to characterize the retardance function of optical
   variable retarders}},
Journal = {{AMERICAN JOURNAL OF PHYSICS}},
Year = {{2015}},
Volume = {{83}},
Number = {{2}},
Pages = {{143-149}},
Month = {{FEB}},
Abstract = {{In this work, we present an experimental method to characterize variable
   optical retarders, which can have linear or non-linear behavior of the
   retardance variation. A theoretical analysis of such is presented using
   a combination of Stokes vectors and Mueller matrixes for three different
   optical retarders. A straightforward method for phase unwrapping, or
   reconstructing the original phase from the measured retardance, is
   proposed that yields high-accuracy results. This work can be used in an
   undergraduate optics lab to help students understand the concepts of
   retardance and its control and also how variable retardance devices
   work. (C) 2015 American Association of Physics Teachers.}},
