@article{ ISI:000351560100024,
Author = {Sanchez-Morgado, Hector},
Title = {{HYPERBOLICITY AND EXPONENTIAL LONG-TIME CONVERGENCE FOR SPACE-TIME
   PERIODIC HAMILTON-JACOBI EQUATIONS}},
Journal = {{PROCEEDINGS OF THE AMERICAN MATHEMATICAL SOCIETY}},
Year = {{2015}},
Volume = {{143}},
Number = {{2}},
Pages = {{731-740}},
Month = {{FEB}},
Abstract = {{In this note we prove exponential convergence to time-periodic states of
   the solutions of space-time periodic Hamilton-Jacobi equations, assuming
   that the Aubry set is the union of a finite number of hyperbolic
   periodic orbits of the Euler-Lagrange flow. The period of limiting
   solutions is the least common multiple of the periods of the orbits in
   the Aubry set. This extends a result that was obtained by Iturriaga and
   the author for the autonomous case.}},
