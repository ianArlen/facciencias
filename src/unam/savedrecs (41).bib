@article{ ISI:000409758100015,
Author = {Nava Estrada, Gabriela},
Title = {{Stripping of identity: the experience of dressing carnival in Pipa, by
   Leopoldo Alas, `Clarin'}},
Journal = {{ESTUDIOS DE TEORIA LITERARIA-REVISTA DIGITAL-ARTES LETRAS HUMANIDADES}},
Year = {{2015}},
Volume = {{4}},
Number = {{7}},
Pages = {{155-167}},
Month = {{MAR}},
Abstract = {{The continued relevance of Carnival could be largely attributed to the
   possibility of establishing a new reality - one based on the disregard
   for social differences- where the subject can overlook the daily norms
   and act with no material restriction. For a person, this universe is
   some kind of escape from everyday life. In a sense, the key to enter
   that alternative world is a costume. In Pipa (1879), Leopoldo Alas,
   Clarin. dresses the main character of the novella, a poor boy, with a
   carnivalesque costume. Rather than preluding a festive practice, the
   masking becomes the source of new experiences through which the author
   depicts the striking economic contrasts of the 19th century. The
   symbolic dimension of the costume allows us to better understand the
   meaning of the journey made by the character after adopting a
   carnivalesque outfit; at the same time, it highlights the criticism
   found in the novella to the Spain where the author born and lived.}},
