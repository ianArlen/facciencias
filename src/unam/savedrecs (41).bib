@article{ ISI:000351004700002,
Author = {Sahu, Sarira and Palacios, Eddie},
Title = {{Hadronic origin of the TeV flare of M87 in April 2010}},
Journal = {{EUROPEAN PHYSICAL JOURNAL C}},
Year = {{2015}},
Volume = {{75}},
Number = {{2}},
Month = {{FEB 4}},
Abstract = {{M87 is a giant radio galaxy with FR-I morphology. It underwent three
   episodes of TeV flaring in recent years with the strongest one in April
   2010 which was jointly monitored by MAGIC, VERITAS and H.E.S.S. We
   explain its spectral energy distribution in the energy range 0.3-5 TeV
   by assuming that the flaring occurs in the innermost region of the jet.
   In this region the low energy SSC photons serve as the target for the
   Fermi-accelerated high energy protons of energy <= 30 TeV to form a
   delta resonance. The TeV photons are produced from the subsequent decay
   of the delta resonance to neutral pions. In this scenario the observed
   TeV flux of April 2010 flare is fitted very well.}},
