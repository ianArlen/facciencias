@article{ ISI:000349700200006,
Author = {Abadallan Guzman, Ali and Joya, Daniel Chirivi and Francke, Oscar F.},
Title = {{The first troglomorphic species of the genus Phrynus Lamarck, 1801
   (Amblypygi: Phrynidae) from Mexico}},
Journal = {{ZOOTAXA}},
Year = {{2015}},
Volume = {{3920}},
Number = {{3}},
Pages = {{474-482}},
Month = {{FEB 23}},
Abstract = {{A new troglomorphic species, Phrynus perrii sp. nov., is described from
   two adult females from Cueva del Naranjo, Municipio Cintalapa, Chiapas,
   Mexico. This is the first continental record of a troglomorphic Phrynus
   species, and the second troglomorphic species of the genus. With the
   description of this species, in Mexico there are ten extant species,
   plus one fossil of the genus Phrynus, and it is the seventh species of
   troglobitic whip spiders from Mexico, making it the country with the
   highest richness of amblypygids species worldwide.}},
