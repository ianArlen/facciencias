@article{ ISI:000351629100001,
Author = {Loss, Roberto},
Title = {{HOW TO CHANGE THE PAST IN ONE-DIMENSIONAL TIME}},
Journal = {{PACIFIC PHILOSOPHICAL QUARTERLY}},
Year = {{2015}},
Volume = {{96}},
Number = {{1}},
Pages = {{1-11}},
Month = {{MAR}},
Abstract = {{The possibility of changing the past by means of time-travel appears to
   depend on the possibility of distinguishing the past as it is before'
   and after' the time-travel. So far, all the metaphysical models that
   have been proposed to account for the possibility of past-changing
   time-travels operate this distinction by conceiving of time as
   multi-dimensional, and thus by significantly inflating our metaphysics
   of time. The aim of this article is to argue that there is an intuitive
   sense in which past-changing time-travels are metaphysically possible
   also in one-dimensional time.}},
