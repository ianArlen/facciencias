@article{ ISI:000350086700004,
Author = {Casarrubias-Segura, Fidel and Rojas-Hernandez, Reynaldo},
Title = {{On some monotone properties}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2015}},
Volume = {{182}},
Pages = {{36-52}},
Month = {{MAR 1}},
Abstract = {{In this paper we deal with some classes of spaces defined by networks
   and retractions, in particular we prove: Any closed subspace in a
   Sigma-product of cosmic spaces is monotonically stable. A space X is
   monotonically retractable if and only if it is monotonically
   omega-stable and has a full retractional skeleton. Any monotonically
   retractable and monotonically omega-monolithic space is monotonically
   Sokolov, and as a consequence, any monotonically Sokolov and
   monotonically omega-stable space is monotonically retractable. Any
   closed subspace of a countably compact monotonically retractable space X
   is a W-set in X. These results generalize some results obtained in
   {[}18,6,8,10]. (C) 2014 Elsevier EN. All rights reserved.}},
