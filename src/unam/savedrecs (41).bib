@article{ ISI:000350654800018,
Author = {Soriano, Diana and Alvarado-Lopez, Sandra and Zuniga-Sanchez, Esther and
   Orozco-Segovia, Alma and Gamboa-deBuen, Alicia},
Title = {{Analysis of nitrogen seed reserves of ten tree species of the tropical
   dry forest}},
Journal = {{SOUTH AFRICAN JOURNAL OF BOTANY}},
Year = {{2015}},
Volume = {{97}},
Pages = {{149-153}},
Month = {{MAR}},
Abstract = {{Nitrogen is a component of many seed reserve compounds, which play a
   remarkable role during germination and have great value for predators. A
   predefined coefficient factor is used to determine the protein
   concentration from the seed nitrogen concentration. However, this
   calculation may not reflect the variety of nitrogen compounds present in
   seeds and their ecological role. Understanding how the seed nitrogen
   content is distributed between amino acids and proteins will help
   explain how nitrogen compounds are involved in seed metabolism. We
   studied the heat-stable protein and total amino acid concentrations in
   ten tree species in a tropical dry forest in northwest Mexico. The seed
   nitrogen concentrations formed a continuum in the studied species. The
   total amino acid concentration was positively correlated with the
   nitrogen concentration. During germination, a differential decrease in
   the nitrogen, total amino acid, and heat-stable protein contents was
   detected among the species. Functional diversity in the partitioning of
   nitrogen inside seeds is discussed. (C) 2015 SAAB. Published by Elsevier
   B.V. All rights reserved.}},
