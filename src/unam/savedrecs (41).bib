@article{ ISI:000430839600003,
Author = {Anderson, Jill},
Title = {{``Tagged as a criminal{''}: Narratives of deportation and return
   migration in a Mexico City call center}},
Journal = {{LATINO STUDIES}},
Year = {{2015}},
Volume = {{13}},
Number = {{1}},
Pages = {{8-27}},
Month = {{MAR}},
Abstract = {{In this essay, I describe the nexus of global capitalism, the
   precipitous rise in US deportations and return migration over the last
   decade, and the criminalization of Latino youth and immigrants in Mexico
   and the United States as manifested in the ``call center narratives{''}
   of call center employees in Mexico City. The call center is the setting
   for an emergent transnational subculture within the economic imperatives
   of neoliberal globalization. For a disproportionate number of returning
   and deported youth, the transnational call center figures as an
   uncertain safe haven, a community of support and a temporary economic
   safety net. Even as the presence of former and active Latino gang
   members threaten the seeming safety in numbers, the call center
   narrators deconstruct the dominant discourse of ``every immigrant is a
   (potential) criminal{''} in both the United States and in Mexico. And,
   as testimonios, the call center narratives speak from the
   ``elsewhere{''} of deportation and return to US-based readers, voicing
   new perspectives on transnational and immigrant identity and solidarity.}},
