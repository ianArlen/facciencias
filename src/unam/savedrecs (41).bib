@article{ ISI:000349556500005,
Author = {Delgado, Carlos and Kroetzsch, Edgar and Jimenez-Alvarez, Luis A. and
   Ramirez-Martinez, Gustavo and Marquez-Garcia, Jose E. and Cruz-Lagunas,
   Alfredo and Moran, Juan and Hernandez, Carmen and Sierra-Vargas,
   Patricia and Avila-Moreno, Federico and Becerril, Carina and Montano,
   Martha and Banales-Mendez, Jose L. and Zuniga, Joaquin and
   Buendia-Roldan, Ivette},
Title = {{Serum Surfactant Protein D (SP-D) is a Prognostic Marker of Poor Outcome
   in Patients with A/H1N1 Virus Infection}},
Journal = {{LUNG}},
Year = {{2015}},
Volume = {{193}},
Number = {{1}},
Pages = {{25-30}},
Month = {{FEB}},
Abstract = {{Surfactant protein D (SP-D) plays an important role in the innate
   responses against pathogens and its production is altered in lung
   disorders.
   We studied the circulating levels of SP-D in 37 patients with acute
   respiratory distress syndrome due to the A/H1N1 virus infection and in
   40 healthy controls. Cox logistic regression models were constructed to
   explore the association of SP-D levels and risk of death.
   Mortality rate after a 28-day was 32.42 \%. Significant higher levels of
   SP-D were detected in A/H1N1 patients with fatal outcome (p < 0.05).
   After adjusting for confounding variables, levels of SP-D a parts per
   thousand yen250 ng/mL were associated with increased the risk of death
   (HR = 8.27, 95 \% CI 1.1-64.1, p = 0.043).
   Our results revealed that higher circulating levels of SP-D are
   associated with higher mortality risk in critically ill A/H1N1 patients.
   SP-D might be a predictive factor of poor outcomes in viral pneumonia.}},
