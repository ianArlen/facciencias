@article{ ISI:000353070200019,
Author = {Breton, Nora and Ramirez-Codiz, C. E.},
Title = {{On the NUT-Born-Infeld-Lambda spacetime}},
Journal = {{ANNALS OF PHYSICS}},
Year = {{2015}},
Volume = {{353}},
Pages = {{252-270}},
Month = {{FEB}},
Abstract = {{The stationary axisymmetric spacetime coupled to nonlinear Born-Infeld
   electrodynamics is studied; the solution was derived by Plebanski et al.
   (1984) in terms of integrals. It is characterized by six free
   parameters: mass, NUT charge, electric and magnetic charge, Born-Infeld
   parameter and cosmological constant. The geodesic and Lorentz force
   equations are integrated on the equatorial plane, and the effect of
   varying the parameters in the effective potential is analyzed. The
   trajectories of light and those for charged particles are discussed as
   well. Finally the conditions for the extreme black hole are determined,
   obtaining bounds between the charges, the Born-Infeld parameter and the
   cosmological constant. (C) 2014 Elsevier Inc. All rights reserved.}},
