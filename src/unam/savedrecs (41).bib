@article{ ISI:000348965700007,
Author = {Garcia-Ferreira, S. and Rodriguez-Lopez, Y. and Uzcategui, C.},
Title = {{Iterates of dynamical systems on compact metrizable countable spaces}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2015}},
Volume = {{180}},
Pages = {{100-110}},
Month = {{FEB 1}},
Abstract = {{Given a dynamical system (X, f), we let E(X, f) denote its Ellis
   semigroup and E(X, f){*} = E(X, f) \textbackslash{} \{f(n) : n is an
   element of N\}. We analyze the Ellis semigroup of a dynamical system
   having a compact metric countable space as a phase space. We show that
   if (X, f) is a dynamical system such that X is a compact metric
   countable space and every accumulation point of X is periodic, then
   either all functions of E(X, f) {*} are continuous or all functions of
   E(X, f){*} are discontinuous. We describe an example of a dynamical
   system (X, f) where X is a compact metric countable space, the orbit of
   each accumulation point is finite and E(X, f){*} contains both
   continuous and discontinuous functions. (C) 2014 Elsevier B.V. All
   rights reserved.}},
