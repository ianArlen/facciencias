@article{ ISI:000348826100013,
Author = {Martinez-Armenta, Miriam and Diaz de Leon-Guerrero, Sol and Catalan, Ana
   and Alvarez-Arellano, Lourdes and Maria Uribe, Rosa and Subramaniam,
   Malayannan and Charli, Jean-Louis and Perez-Martinez, Leonor},
Title = {{TGF beta 2 regulates hypothalamic Trh expression through the TGF beta
   inducible early gene-1 (TIEG1) during fetal development}},
Journal = {{MOLECULAR AND CELLULAR ENDOCRINOLOGY}},
Year = {{2015}},
Volume = {{400}},
Number = {{C}},
Pages = {{129-139}},
Month = {{JAN 15}},
Abstract = {{The hypothalamus regulates the homeostasis of the organism by
   controlling hormone secretion from the pituitary. The molecular
   mechanisms that regulate the differentiation of the hypothalamic
   thyrotropin-releasing hormone (TRH) phenotype are poorly understood. We
   have previously shown that Klf10 or TGF beta inducible early gene-1
   (TIEG1) is enriched in fetal hypothalamic TRH neurons. Here, we show
   that expression of TGF beta isoforms (1-3) and both TGF beta receptors
   (T beta RI and II) occurs in the hypothalamus concomitantly with the
   establishment of TRH neurons during late embryonic development. TGF beta
   2 induces Trh expression via a TIEG1 dependent mechanism. TIEG1
   regulates Trh expression through an evolutionary conserved GC rich
   sequence on the Trh promoter. Finally, in mice deficient in TIEG1, Trh
   expression is lower than in wild type animals at embryonic day 17. These
   results indicate that TGF beta signaling, through the upregulation of
   TIEG1, plays an important role in the establishment of Trh expression in
   the embryonic hypothalamus. (C) 2014 Published by Elsevier Ireland Ltd.}},
