@article{ ISI:000346828700013,
Author = {Hernandez-Hernandez, M. and Cruz-Mendez, W. and Gonzalez-Rivera, C. and
   Ramirez-Argaez, M. A.},
Title = {{Effect of Process Variables on Kinetics and Gas Consumption in
   Rotor-Degassing Assisted by Physical and Mathematical Modeling}},
Journal = {{MATERIALS AND MANUFACTURING PROCESSES}},
Year = {{2015}},
Volume = {{30}},
Number = {{2}},
Pages = {{216-221}},
Month = {{FEB 1}},
Abstract = {{Mathematical and physical models of water deoxidation in a batch
   aluminum degassing reactor using the rotor-injector technique were
   developed. The mathematical model was successfully validated against
   measured degassing kinetics. The physical model was employed to perform
   a process analysis using a two-level factorial experimental design to
   determine the influence of gas flow rate, impeller angular velocity, and
   gas injection points on gas consumption efficiency and degassing
   kinetics. A combination of higher rotor speeds and gas flow rates
   results in fast degassing kinetics. However, moderate gas flow rates are
