@article{ ISI:000349458400054,
Author = {Vazquez-Victorio, Genaro and Caligaris, Cassandre and Del
   Valle-Espinosa, Eugenio and Sosa-Garrocho, Marcela and Gonzalez-Arenas,
   Nelly R. and Reyes-Cruz, Guadalupe and Briones-Orta, Marco A. and
   Macias-Silva, Marina},
Title = {{Novel Regulation of Ski Protein Stability and Endosomal Sorting by Actin
   Cytoskeleton Dynamics in Hepatocytes}},
Journal = {{JOURNAL OF BIOLOGICAL CHEMISTRY}},
Year = {{2015}},
Volume = {{290}},
Number = {{7}},
Pages = {{4487-4499}},
Month = {{FEB 13}},
Abstract = {{TGF-beta-induced antimitotic signals are highly regulated during cell
   proliferation under normal and pathological conditions, such as liver
   regeneration and cancer. Up-regulation of the transcriptional cofactors
   Ski and SnoN during liver regeneration may favor hepatocyte
   proliferation by inhibiting TGF-beta signals. In this study, we found a
   novel mechanism that regulates Ski protein stability through TGF-beta
   and G protein-coupled receptor (GPCR) signaling. Ski protein is
   distributed between the nucleus and cytoplasm of normal hepatocytes, and
   the molecular mechanisms controlling Ski protein stability involve the
   participation of actin cytoskeleton dynamics. Cytoplasmic Ski is
   partially associated with actin and localized in cholesterol-rich
   vesicles. Ski protein stability is decreased by TGF-beta/Smads, GPCR/Rho
   signals, and actin polymerization, whereas GPCR/cAMP signals and actin
   depolymerization promote Ski protein stability. In conclusion, TGF-beta
   and GPCR signals differentially regulate Ski protein stability and
   sorting in hepatocytes, and this cross-talk may occur during liver
   regeneration.}},
