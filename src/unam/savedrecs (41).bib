@article{ ISI:000350881300081,
Author = {Gaso, M. I. and Gonzalez, P. R. and Segovia, N. and Quintero, E.},
Title = {{Gamma dose rate and Ra-226 activity concentrations in the soil around a
   Mexican radioactive waste-storage center}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2015}},
Volume = {{303}},
Number = {{3}},
Pages = {{2321-2331}},
Month = {{MAR}},
Abstract = {{The environmental gamma dose rate has been determined at the Mexican
   Storage Centre for Radioactive Waste (SCRW) and surrounding communities.
   The gamma dose rates were evaluated using thermoluminescent dosimeters
   and a portable gamma-ray spectrometer. The activity concentrations of
   U-235, Ra-226, Th-232, K-40, and Cs-137 in soil samples were determined
   by gamma spectrometry. The gamma dose rates and the annual effective
   doses were higher at the SCRW and at border line, as compared with the
   surrounding communities. A site specific lineal model to describe the
   relationship between the outdoor gamma dose rate and the Ra-226 activity
   concentrations in the soil is proposed.}},
