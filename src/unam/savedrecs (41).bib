@article{ ISI:000347263800125,
Author = {Vargas, A. V. and Zenon, E. and Oswald, U. and Islas, J. M. and
   Gueereca, L. P. and Manzini, F. L.},
Title = {{Life cycle assessment: A case study of two wind turbines used in Mexico}},
Journal = {{APPLIED THERMAL ENGINEERING}},
Year = {{2015}},
Volume = {{75}},
Number = {{SI}},
