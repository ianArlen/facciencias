@article{ ISI:000348825500018,
Author = {Resendiz Barragan, Aida Monserrat and Hernandez Altamirano, Sheila
   Viridiana and Sierra Murguia, Mariana Alejandra and Torres Tamayo,
   Margarita},
Title = {{EATING HABITS OF PATIENTS WITH SEVERE OBESITY}},
