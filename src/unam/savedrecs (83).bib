@article{ ISI:000274805900004,
Author = {Rodriguez-Monroy, M. A. and Moreno-Fierros, L.},
Title = {{Striking Activation of NALT and Nasal Passages Lymphocytes Induced by
   Intranasal Immunization with Cry1Ac protoxin}},
Journal = {{SCANDINAVIAN JOURNAL OF IMMUNOLOGY}},
