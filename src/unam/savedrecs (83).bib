@article{ ISI:000275031400015,
Author = {Martinez-Carrillo, M. A. and Solis, C. and Andrade, E. and Isaac-Olive,
   K. and Beltran-Hernandez, R. I. and Medina Moreno, S. A. and
   Martinez-Resendiz, G. and Ramirez Reyes, A. and Lucho-Constantino, C. A.
   and Del Razo, L. M.},
