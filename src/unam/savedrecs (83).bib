@article{ ISI:000272614200002,
Author = {Patten, Michael A. and Gomez de Silva, Hector and Smith-Patten, Brenda
   D.},
Title = {{Long-term changes in the bird community of Palenque, Chiapas, in
   response to rainforest loss}},
