@article{ ISI:000272899500005,
Author = {Tremblay, Frederick and Turbiner, Alexander V. and Winternitz, Pavel},
Title = {{Periodic orbits for an infinite family of classical superintegrable
   systems}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2010}},
Volume = {{43}},
Number = {{1}},
Month = {{JAN 8}},
Abstract = {{We show that all bounded trajectories in the two-dimensional classical
   system with the potential V (r, phi) = omega(2)r(2) + alpha
   k(2)/r(2)cos(2)k phi + beta k(2)/r(2)sin(2)k phi are closed for all
   integer and rational values of k. The period is T = pi/2 omega. and does
   not depend on k. This agrees with our earlier conjecture suggesting that
