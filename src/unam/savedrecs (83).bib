@article{ ISI:000273400400078,
Author = {Macias-Ruvalcaba, Norma A. and Evans, Dennis H.},
Title = {{Association Reactions of the Anion Radicals of Some Hydroxyquinones:
   Evidence for Formation of pi- and sigma-Dimers As Well As a
   Neutral-Anion Radical Complex}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2010}},
Volume = {{114}},
Number = {{2}},
Pages = {{1285-1292}},
Month = {{JAN 21}},
Abstract = {{The electrochemical reduction of nine quinones has been Studied ill
   acetonitrile and, in one case, in dimethyl sulfoxide. Included are seven
   hydroxyquinones plus 1,4-naphthoquinone, 8, and 9,10-anthraquinone, 9.
   Each quinone is reduced ill two steps, first to the anion radical and
   then, at more negative potentials, to the dianion. However, digital
   simulations show that the voltammetric data cannot be explained by these
   two reactions alone. For three of the hydroxyquinones, 2-4, Plus 8 and
   9, the fast disproportionation/comproportionation reaction connecting
   the neutral, anion radical, and dianion must be included along with,I
   diffusion coefficient of the anion radical that is smaller than that of
   the neutral quinone and a still smaller diffusion coefficient for the
   dianion. Three other hydroxyquinones, 5-7, require, in addition, the
   formation of a neutral-anion radical complex. Finally,
   5-hydroxy-1,4-naphthoquinone, 1 in acetonitrile involves both sigma- and
   pi-dimerization of the anion radical With Subsequent reduction of the
   pi-complex, whereas in dimethyl sulfoxide the sigma-dimer is replaced by
   the aforementioned formation of the neutral-anion radical complex. The
   differences in behavior are discussed in terms of distribution of spin
   density in the anion radicals and intermolecular and intramolecular
   hydrogen bonding.}},
