Author = {Perez de la Mora, Miguel and Gallegos-Cari, Andrea and Arizmendi-Garcia,
   Yexel and Marcellino, Daniel and Fuxe, Kjell},
Title = {{Role of dopamine receptor mechanisms in the amygdaloid modulation of
   fear and anxiety: Structural and functional analysis}},
