@article{ ISI:000275031400002,
Author = {Tommasino, L. and Tommasino, M. C. and Espinosa, G.},
Title = {{Radon film-badges based on radon-sortpion in solids: A new field for
   solving long-lasting problems}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2010}},
Volume = {{56}},
Number = {{1, S}},
Pages = {{1-4}},
