@inproceedings{ ISI:000291568800008,
Author = {Ayala, Alejandro and Jalilian-Marian, Jamal and Magnin, Javier and
   Ortiz, Antonio and Paic, Guy and Elena Tejeda-Yeomans, Maria},
Editor = {{Ayala, A and Cuautle, E and Hess, PO and Paic, G}},
Title = {{The broad away side of azimuthal correlations: 3 vs 2 final state
   particles in high energy nuclear collisions}},
Booktitle = {{5TH INTERNATIONAL WORKSHOP ON HIGH-PT PHYSICS AT LHC}},
Series = {{AIP Conference Proceedings}},
Year = {{2010}},
Volume = {{1348}},
