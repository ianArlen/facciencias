Title = {{Solvent extracted organic matter and polycyclic aromatic hydrocarbons
   distributed in size-segregated airborne particles in a zone of Mexico
   City: Seasonal behavior and human exposure}},
Journal = {{ATMOSPHERIC ENVIRONMENT}},
