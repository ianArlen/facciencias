@article{ ISI:000273567600005,
Author = {Mockford, Edward L. and Garcia Aldrete, Alfonso N.},
Title = {{A new species of Lachesilla Westwood (Psocoptera: Lachesillidae) in the
   andra group with a proposed classification of the andra group}},
Journal = {{ZOOTAXA}},
Year = {{2010}},
Number = {{2335}},
Pages = {{49-58}},
Month = {{JAN 14}},
Abstract = {{We describe Lachesilla texana Mockford and Garcia Aldrete, n. sp., with
   populations in northeastern Mexico, central and southern Texas, and
   Great Smoky Mountains National Park, Tennessee. Several minor
   differences are noted between the Smoky Mountains and the Texas-Mexican
   populations. We assign the new species to the andra group (Garcia
   Aldrete 1974) and in order to place it within this large and complex
   species group, we propose a classification of the group with designation
   of four subgroups based primarily on male external genitalic and
   paragenitalic structures. Probable apomorphies are noted for each of the
   subgroups.}},
