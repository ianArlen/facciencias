@article{ ISI:000274064900005,
Author = {Perez-Campos, Citiali and Raul Gonzalez-Alonso, Jose and Castanos,
   Octavio and Lopez-Pena, Ramon},
Title = {{Entanglement and localization of a two-mode Bose-Einstein condensate}},
