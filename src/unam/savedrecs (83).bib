@inproceedings{ ISI:000324571900035,
Author = {Meza, Ivan V. and Salinas, Lisset and Venegas, Esther and Castellanos,
   Hayde and Chavarria, Alejandra and Pineda, Luis A.},
Editor = {{KuriMorales, A and Simari, GR}},
Title = {{Specification and Evaluation of a Spanish Conversational System Using
   Dialogue Models}},
Booktitle = {{ADVANCES IN ARTIFICIAL INTELLIGENCE - IBERAMIA 2010}},
Series = {{Lecture Notes in Artificial Intelligence}},
Year = {{2010}},
Volume = {{6433}},
Pages = {{346-355}},
Note = {{12th Ibero-American Conference on AI, Bahia Blanca, ARGENTINA, NOV
   01-05, 2010}},
Organization = {{Univ Nacl Sur, Dept Comp Sci \& Engn; Ibero Amer Artificial Intelligence
   \& Comp Sci Soc; Artificial Intelligence Journal}},
Abstract = {{In this paper, we present a new specification, implementation and
   evaluation of the ``guess the card{''} system. This is a conversational
   system with spoken Spanish and vision capabilities that plays a game
   with members of the general public in a permanent stand at a science
   museum. The system has been built using a methodology and programming
   environment based on the notion of dialogue model specification and
   interpretation, that we have been developing over the years. The present
   system uses the latest version of the formalism and improves
   considerably over previous versions {[}8]. In particular, the present
