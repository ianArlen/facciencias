@article{ ISI:000273978400030,
Author = {Chavez-Cabrera, Cipriano and Flores-Bustamante, Zoila R. and Marsch,
   Rodolfo and del Carmen Montes, Maria and Sanchez, Sergio and Carlos
   Cancino-Diaz, Juan and Bernardo Flores-Cotera, Luis},
Title = {{ATP-citrate lyase activity and carotenoid production in batch cultures
   of Phaffia rhodozyma under nitrogen-limited and nonlimited conditions}},
Journal = {{APPLIED MICROBIOLOGY AND BIOTECHNOLOGY}},
Year = {{2010}},
Volume = {{85}},
Number = {{6}},
Pages = {{1953-1960}},
Month = {{FEB}},
Abstract = {{ATP-citrate lyase (ACL) is the key cytoplasmic enzyme which supplies
   acetyl-CoA for fatty acids in oleaginous yeast. Although it has been
   suggested that fatty acid and carotenoid biosynthesis may have a common
   source of acetyl-CoA in Phaffia rhodozyma, the source for carotenoids is
   currently unknown. The purpose of this work was to analyze the
   development of ACL activity during batch cultures of P. rhodozyma under
   ammonium-limited and nonammonium-limited conditions and study its
   possible relationship with carotenoid synthesis. Every experiment showed
   carotenoid accumulation linked to an increasing ACL activity. Moreover,
   the ACL activity increased with dissolved oxygen (DO), i.e., ACL
   responded to DO in a similar way as carotenoid synthesis. Additionally,
   in the ammonium-limited culture, ACL activity increased upon ammonium
   depletion. However, the contribution to carotenoid accumulation in that
   case was negligible. This suggests that P. rhodozyma has developed two
   components of ACL, each one responsive to a different environmental
   stimulus, i.e., DO and ammonium depletion. The role of each component is
   still unknown; however, considering that the former responds to DO and
   the known role of carotenoids as antioxidants, it may be a provider of
   acetyl-CoA for carotenoid synthesis.}},
