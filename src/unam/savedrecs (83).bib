@article{ ISI:000276194200002,
Author = {Erler, Jens},
Title = {{Mass of the Higgs boson in the standard electroweak model}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2010}},
Volume = {{81}},
Number = {{5}},
Month = {{MAR 1}},
Abstract = {{An updated global analysis within the standard model (SM) of all
   relevant electroweak precision and Higgs boson search data is presented
   with special emphasis on the implications for the Higgs boson mass, M-H.
   Included are, in particular, the most recent results on the top quark
   and W boson masses, updated and significantly shifted constraints on the
   strong coupling constant, alpha(s), from tau decays and other low-energy
   measurements such as from atomic parity violation and neutrino deep
   inelastic scattering. The latest results from searches for Higgs
   production and decay at the Tevatron are incorporated together with the
   older constraints from LEP 2. I find a trimodal probability distribution
   for M-H with a fairly narrow preferred 90\% C. L. window, 115 GeV <= M-H
   <= 148 GeV.}},
