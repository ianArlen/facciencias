@article{ ISI:000273554600019,
Author = {Benard, Claudine and Cultrone, Antonietta and Michel, Catherine and
   Rosales, Carlos and Segain, Jean-Pierre and Lahaye, Marc and Galmiche,
   Jean-Paul and Cherbut, Christine and Blottiere, Herve M.},
Title = {{Degraded Carrageenan Causing Colitis in Rats Induces TNF Secretion and
   ICAM-1 Upregulation in Monocytes through NF-kappa B Activation}},
Journal = {{PLOS ONE}},
Year = {{2010}},
Volume = {{5}},
Number = {{1}},
Month = {{JAN 13}},
Abstract = {{Carrageenan (CGN) is a high molecular weight sulphated polysaccharide
   derived from red seaweeds. In rodents, its degraded forms (dCGN) can
   induce intestinal inflammation associated with macrophage recruitment
   and activation. The aim of this study was: 1) to analyze the
   size-dependent effects of dCGN on colon inflammation in vivo, and 2) to
   correlate these effects with monocyte/macrophage proliferation, cytokine
   production and expression of various cell surface antigens including
   ICAM-1 adhesion molecule. Peripheral blood monocytes (PBM) and THP-1
   monocytic cells were cultured in the presence of either 10 or 40 kDa,
   dCGN. The 40 kDa, but not the 10 kDa dCGN, induced colitis in in vivo.
   Degraded CGN inhibited THP-1 cell proliferation in vitro, arresting the
   cells in G1 phase. In addition, dCGN increased ICAM-1 expression in both
   PBM and THP-1 cells with a major effect seen after 40 kDa dCGN exposure.
   Also, dCGN stimulated monocyte aggregation in vitro that was prevented
   by incubation with anti-ICAM-1 antibody. Finally, dCGN stimulated
   TNF-alpha expression and secretion by both PBM and THP-1 cells. All
   these effects were linked to NF-kappa B activation. These data strongly
   suggest that the degraded forms of CGN have a pronounced effect on
   monocytes, characteristic of an inflammatory phenotype.}},
Publisher = {{PUBLIC LIBRARY SCIENCE}},
Address = {{185 BERRY ST, STE 1300, SAN FRANCISCO, CA 94107 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Benard, C (Reprint Author), Univ Nantes, INRA, UMR PHAN 1280, CHU Hotel Dieu, Nantes, France.
