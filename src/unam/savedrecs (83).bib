@inproceedings{ ISI:000295547400015,
Author = {Pfeiffer, Heriberto},
Editor = {{Hu, YH}},
Title = {{Advances on Alkaline Ceramics as Possible CO2 Captors}},
Booktitle = {{ADVANCES IN CO2 CONVERSION AND UTILIZATION}},
