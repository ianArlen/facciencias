@article{ ISI:000273199500003,
Author = {Moreno, Ivan and Bermudez, David and Avendano-Alejo, Maximino},
Title = {{Light-emitting diode spherical packages: an equation for the light
   transmission efficiency}},
Journal = {{APPLIED OPTICS}},
Year = {{2010}},
Volume = {{49}},
Number = {{1}},
Pages = {{12-20}},
Month = {{JAN 1}},
Abstract = {{Virtually all light-emitting diodes (LEDs) are encapsulated with a
   transparent epoxy or silicone gel. Here we analyze the optical
   efficiency of spherical encapsulants. We develop a quasi-radiometric
   equation for the light transmission efficiency, which incorporates some
   ideas of Monte Carlo ray tracing into the context of radiometry. The
   approach includes the extended source nature of the LED chip and the
   chip radiance distribution. The equation is an explicit function of the
   size and the refractive index of the package, and also of several chip
   parameters such as shape, size, radiance, and location inside the
   package. To illustrate the use of this equation, we analyze several
   packaging configurations of practical interest, for example, a
