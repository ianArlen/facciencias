@inproceedings{ ISI:000287700900037,
Author = {Rosales-Garcia, Andrea and Morse, T. F. and Hernandez-Cordero, Juan},
Editor = {{HernandezCordero, J and TorresGomez, I and Mendez, A}},
Title = {{Intra-cavity Fiber Laser Polarization Mode Beating Sensing}},
Booktitle = {{2ND WORKSHOP ON SPECIALTY OPTICAL FIBERS AND THEIR APPLICATIONS WSOF-2}},
Series = {{Proceedings of SPIE}},
Year = {{2010}},
Volume = {{7839}},
Note = {{2nd Workshop on Specialty Optical Fibers and Their Applications, Oaxaca,
   MEXICO, OCT 13-15, 2010}},
Organization = {{Fibercore Ltd; CONACYT; Opt Soc Amer (OSA); SPIE}},
Abstract = {{We demonstrate a highly-sensitive fiber optic sensor based on
   polarization mode beating (PMB) techniques for measuring changes in the
   optical pathlength (i.e., length and refractive index) of a laser
   cavity. This technique employs the two independent orthogonal modes from
   within the fiber laser to measure relative phase changes. By
   heterodyning the modes it is possible to obtain a beating signal and
   perturb only one of the modes so frequency changes can be measured. This
   results in the elimination of common intra-cavity noise and sensitivity
   enhancement. Frequency changes of the PMB signal are evaluated as a
   function of displacement, intra-cavity pressure and air density and
   monitored in real-time with high precision and accuracy. The high
   sensitivity and narrow laser linewidth show a potential application for
   ultra-sensitive biological measurements, chemical sensing and explosive
   detection.}},
Publisher = {{SPIE-INT SOC OPTICAL ENGINEERING}},
Address = {{1000 20TH ST, PO BOX 10, BELLINGHAM, WA 98227-0010 USA}},
