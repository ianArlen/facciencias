@inproceedings{ ISI:000287165200108,
Author = {Fu, J. -N. and Dolez, N. and Vauclair, G. and Machado, L. Fox and Kim,
   S. -L. and Li, C. and Alvarez, M. and Su, J. and Chevreton, M. and
   Michel, R. and Yang, X. H. and Li, Y. and Zhang, Y. P. and Paparo, M.
   and Molnar, L. and Plachy, E.},
