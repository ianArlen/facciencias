@inproceedings{ ISI:000406956500033,
Author = {Colorado-Garrido, D. and Serna, S. and Cruz-Chavez, M. and Hernandez, J.
   A. and Campillo, B.},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{Artificial neural networks for electrochemical impedance spectroscopy
   sour corrosion predictions of nano-modified microalloyed steels}},
Booktitle = {{2010 IEEE ELECTRONICS, ROBOTICS AND AUTOMOTIVE MECHANICS CONFERENCE
   (CERMA 2010)}},
Series = {{Electronics Robotics and Automotive Mechanics Conference (CERMA)}},
Year = {{2010}},
