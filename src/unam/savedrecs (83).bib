@article{ ISI:000281959500002,
Author = {Juan-Pineda, Daniel and Millan-Lopez, Silvia},
Title = {{The Whitehead group and the lower algebraic K-theory of braid groups on
   S-2 and RP2}},
Journal = {{ALGEBRAIC AND GEOMETRIC TOPOLOGY}},
Year = {{2010}},
Volume = {{10}},
Number = {{4}},
Pages = {{1887-1903}},
Abstract = {{Let M = S-2 or RP2. Let PBn(M) and B-n (M) be the pure and the full
   braid groups of M respectively. If Gamma is any of these groups, we show
   that Gamma satisfies the Farrell-Jones Fibered Isomorphism Conjecture
   and use this fact to compute the lower algebraic K-theory of the
   integral group ring Z Gamma, for Gamma = PBn(M). The main results are
   that for Gamma = PBn(S-2), the Whitehead group of Gamma, (K) over tilde
   (0)(Z Gamma) and K-i(Z Gamma) vanish for i <= -1 and n > 0. For Gamma =
   PBn(RP2), the Whitehead group of Gamma vanishes for all n > 0, (K) over
   tilde (0)(Z Gamma) vanishes for all n > 0 except for the cases n = 2, 3
   and K-i(Z Gamma) vanishes for all i <= -1}},
