@article{ ISI:000272919400009,
Author = {Chaos-Cador, Lorea and Garcia-Calderon, Gaston},
Title = {{Theory of resonant scattering in two dimensions}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2010}},
Volume = {{43}},
Number = {{3}},
Month = {{JAN 22}},
Abstract = {{We present an analytical approach to elastic scattering processes in two
   dimensions which is based on a formalism of resonant states for
   potentials having cylindrical symmetry. We obtain an expression for the
   elastic cross section in terms of the resonant states of the problem.
   Using the unitarity condition on the S-matrix we also derive an exact
   relationship between the elastic cross section and the local density of
   states. We exemplify our findings by considering a delta-potential.}},
