Author = {Espinosa, G. and Golzarri, J. I. and Castano, V. M. and Gaso, I. and
   Mena, M. and Segovia, N.},
Title = {{Measurement of the energy spectrum of Cf-252 fission fragments using
   nuclear track detectors and digital image processing}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2010}},
Volume = {{56}},
