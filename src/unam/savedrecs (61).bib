@article{ ISI:000318064600018,
Author = {Camargo, Javier and Pellicer-Covarrubias, Patricia},
Title = {{ARC-SMOOTHNESS IN SYMMETRIC PRODUCTS}},
Journal = {{HOUSTON JOURNAL OF MATHEMATICS}},
Year = {{2013}},
Volume = {{39}},
Number = {{1}},
Pages = {{287-316}},
Abstract = {{A space X is arc-smooth at a point p provided there exists a continuous
   function a : X -> C(X) satisfying the following conditions: (1) a(p) =
   \{p\}; (2) for each x epsilon X \textbackslash{} \{p\}, the set alpha(x)
   is an arc from p to x; (3) if x E alpha(y), then alpha(x) subset of
   alpha(y). A space is arc-smooth if it is arcsmooth at some point p. In
   this paper we prove that, among hereditarily unicoherent continua, X is
   arc-smooth if and only if the nth symmetric product F-n(X) is arc-smooth
   for each n epsilon N. This allows us to give a positive answer to a
   question by J.T.Goodykoontz when we replace the hyperspace of
   subcontinua of a continuum X with F-n(X). We also give a
   characterization of the arc-smoothness of the hyperspace F-2(X) when X
   is 1-dimensional, and another of F-n(X) when X is a locally connected,
   1-dimensional continuum. Further, we give some conditions on a continuum
   X that imply the arcsmoothness of the hyperspace F-n(X) at some
   particular elements. Finally, we characterize the arc-smoothness at
   elements of F-3(X) in the class of hereditarily unicoherent continua and
   of F-2(X) in the class of 1-dimensional continua.}},
