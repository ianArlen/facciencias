Booktitle = {{SENSORS \& THEIR APPLICATIONS XVII}},
Series = {{Journal of Physics Conference Series}},
Year = {{2013}},
Volume = {{450}},
Note = {{17th Conference on Sensors and Their Applications, Dubrovnik, CROATIA,
   SEP 16-18, 2013}},
Organization = {{Inst Phys, Instrument Sci \& Technol Grp}},
Abstract = {{This paper presents the design of a heat flux sensor using an optical
   fiber system to measure heat conduction in biological tissues. This
   optoelectronic device is based on the photothermal beam deflection of a
   laser beam travelling in an acrylic slab this deflection is measured
   with a fiber optic angle sensor. We measure heat conduction in
   biological samples with high repeatability and sensitivity enough to
   detect differences in tissues from three chicken organs. This technique
   could provide important information of vital organ function as well as
   the detect modifications due to degenerative diseases or physical damage
   caused by medications or therapies.}},
