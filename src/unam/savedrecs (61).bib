@article{ ISI:000312197000034,
Author = {Granados-Oliveros, Gilma and Gomez-Vidales, Virginia and Nieto-Camacho,
   Antonio and Antonio Morales-Serna, Jose and Cardenas, Jorge and Salmon,
   Manuel},
Title = {{Photoproduction of H2O2 and hydroxyl radicals catalysed by natural and
   super acid-modified montmorillonite and its oxidative role in the
   peroxidation of lipids}},
Journal = {{RSC ADVANCES}},
Year = {{2013}},
Volume = {{3}},
Number = {{3}},
Pages = {{937-944}},
Abstract = {{The reactivity of both natural montmorillonite and montmorillonite
   modified with super acids (CF3SO3H and HClO4) to generate hydrogen
   peroxide (H2O2) and hydroxyl radicals ((OH)-O-center dot) under both
   darkness and UV-light irradiation was investigated using EPR
   spin-trapping spectroscopy. We used the free-radical trapping action of
   5,5-dimethyl-1-pyrroline-N-oxide (DMPO) to indicate the production of
   (OH)-O-center dot from natural and modified clays. The recognition and
   characterisation of the formed DMPO-OH adduct established that super
   acid montmorillonites were able to generate hydroxyl radicals at 25
   degrees C in the presence of O-2 and H2O. When natural and acid
   montmorillonite were irradiated with UV light, hydroxyl radical
   generation was greater than under dark conditions. In addition, the
   peroxidation of lipids in biological matrices with natural and super
   acid montmorillonite was studied using a thiobarbituric acid reactive
   substances (TBARS) assay, which demonstrated that only (OH)-O-center dot
   generated from the super acid montmorillonite induced the oxidative
   degradation of lipids. This study reports, for the first time, the
   generation of (OH)-O-center dot and the peroxidation of lipids by a
   typical super acid-modified Mexican montmorillonite.}},
