@article{ ISI:000317331600004,
Author = {Geiss, Christof and Leclerc, Bernard and Schroeer, Jan},
Title = {{Factorial Cluster Algebras}},
Journal = {{DOCUMENTA MATHEMATICA}},
Year = {{2013}},
Volume = {{18}},
Pages = {{249-274}},
Abstract = {{We show that cluster algebras do not contain non-trivial units and that
   all cluster variables are irreducible elements. Both statements follow
   from Fomin and Zelevinsky's Laurent phenomenon. As an application we
   give a criterion for a cluster algebra to be a factorial algebra. This
   can be used to construct cluster algebras, which are isomorphic to
   polynomial rings. We also study various kinds of upper bounds for
   cluster algebras, and we prove that factorial cluster algebras coincide
   with their upper bounds.}},
