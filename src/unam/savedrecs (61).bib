@article{ ISI:000321229900004,
Author = {Barot, Michael and Kussin, Dirk and Lenzing, Helmut},
Title = {{EXTREMAL PROPERTIES FOR CONCEALED-CANONICAL ALGEBRAS}},
Journal = {{COLLOQUIUM MATHEMATICUM}},
Year = {{2013}},
Volume = {{130}},
Number = {{2}},
Pages = {{183-219}},
Abstract = {{Canonical algebras, introduced by C. M. Ringel in 1984, play an
   important role in the representation theory of finite-dimensional
   algebras. They also feature in many other mathematical areas like
   function theory, 3-manifolds, singularity theory, commutative algebra,
   algebraic geometry and mathematical physics. We show that canonical
   algebras are characterized by a number of interesting extremal
   properties (among concealed-canonical algebras, that is, the
   endomorphism rings of tilting bundles on a weighted projective line). We
   also investigate the corresponding class of algebras antipodal to
   canonical ones. Our study yields new insights into the nature of
   concealed-canonical algebras, and sheds a new light on an old question:
   Why are the canonical algebras canonical?}},
