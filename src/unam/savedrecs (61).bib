@article{ ISI:000321299900003,
Author = {Albarran, Alberto Falcon and Canto, Elda A. Alva and Rodriguez, Antonio
   Franco},
Title = {{Intralexic segmentation and categorization by 9-to-12-month-old Spanish
   learning infants.}},
Journal = {{PSICOLOGICA}},
Year = {{2013}},
Volume = {{34}},
Number = {{1}},
Pages = {{37-58}},
Abstract = {{Infants become sensitive to phonological regularities that are more
   consistent in their language input. This sensibility enhances the
   development of perceptual abilities that will serve as the basis for
   language acquisition. The aim of this research was to evaluate Spanish
   learning infants' basic cognitive abilities for the categorical
   discrimination of words that coincide in their initial or final segment.
   The head-turn preference procedure was used in two experiments with
   infants from 9 to 12 months of age. Infants listened to two types of
   lists of CVCV words. Items in the experimental lists shared a particular
   syllable (i.e. onset syllable, Experiment 1 and ending syllable,
   Experiment 2), whereas items in the control lists were unrelated.
   Infants showed significant preferences between the lists. These results
   revealed that Spanish learning infants categorically discriminate not
   only the words that have a common onset, but also those that share a
   final syllable, despite the variability in the adjacent syllable. Given
   the morphology of Spanish, discussion is made on its relationship with
   the development of these perceptual abilities.}},
