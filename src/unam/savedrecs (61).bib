@article{ ISI:000314013300006,
Author = {Rosales-Roldan, Luis and Cedillo-Hernandez, Manuel and Nakano-Miyatake,
   Mariko and Perez-Meana, Hector and Kurkoski, Brian},
Title = {{Watermarking-based image authentication with recovery capability using
   halftoning technique}},
Journal = {{SIGNAL PROCESSING-IMAGE COMMUNICATION}},
