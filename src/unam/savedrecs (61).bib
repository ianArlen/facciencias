@article{ ISI:000322019700020,
Author = {Antonyan, Sergey A.},
Title = {{LOCALLY COMPACT SUBGROUP ACTIONS ON TOPOLOGICAL GROUPS}},
Journal = {{HOUSTON JOURNAL OF MATHEMATICS}},
Year = {{2013}},
Volume = {{39}},
Number = {{2}},
Pages = {{701-716}},
Abstract = {{Let X be a Hausdorff topological group and G a locally compact subgroup
   of X. We show that X admits a locally finite sigma-discrete
   G-functionally open cover each member of which is G-homeomorphic to a
   twisted product G x (H) S-i, where H is a compact large subgroup of G
   (i.e., the quotient G/H is a manifold). If, in addition, the space of
   connected components of G is compact and X is normal, then X itself is
   G-homeomorphic to a twisted product G x (K) S, where K is a maximal
   compact subgroup of G. This implies that X is K-homeomorphic to the
   product G/K x S, and in particular, X is homeomorphic to the product R-n
   x S, where n = dim G/K. Using these results we prove the inequality dim
   X <= dim X/G + dim G for every Hausdorff topological group X and a
   locally compact subgroup G of X.}},
