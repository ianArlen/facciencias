@article{ ISI:000325377000001,
Author = {Valero-Pacheco, Nuriban and Arriaga-Pizano, Lourdes and Ferat-Osorio,
   Eduardo and Maria Mora-Velandia, Luz and Pastelin-Palacios, Rodolfo and
   Angel Villasis-Keever, Miguel and Alpuche-Aranda, Celia and Enid
   Sanchez-Torres, Luvia and Isibasi, Armando and Bonifaz, Laura and
   Lopez-Macias, Constantino},
Title = {{PD-L1 Expression Induced by the 2009 Pandemic Influenza A(H1N1) Virus
   Impairs the Human T Cell Response}},
Journal = {{CLINICAL \& DEVELOPMENTAL IMMUNOLOGY}},
Year = {{2013}},
Abstract = {{PD-L1 expression plays a critical role in the impairment of T cell
   responses during chronic infections; however, the expression of PD-L1 on
   T cells during acute viral infections, particularly during the pandemic
   influenza virus (A(H1N1)pdm09), and its effects on the T cell response
   have not been widely explored. We found that A(H1N1) pdm09 virus induced
   PD-L1 expression on human dendritic cells (DCs) and T cells, as well as
   PD-1 expression on T cells. PD-L1 expression impaired the T cell
   response against A(H1N1) pdm09 by promoting CD8(+) T cell death and
   reducing cytokine production. Furthermore, we found increased PD-L1
   expression on DCs and T cells from influenza-infected patients from the
   first and second 2009 pandemic waves in Mexico City. PD-L1 expression on
   CD8(+) T cells correlated inversely with T cell proportions in patients
   infected with A(H1N1) pdm09. Therefore, PD-L1 expression on DCs and T
   cells could be associated with an impaired T cell response during acute
   infection with A(H1N1) pdm09 virus.}},
