@article{ ISI:000318663700004,
Author = {Baran, A. S. and Winiarski, M. and Siwak, M. and Fox-Machado, L. and
   Koziel-Wierzbowska, D. and Krzesinski, J. and Drozdz, M. and Winans, A.},
Title = {{Mt. Suhora Survey - Searching for Pulsating M Dwarfs. III}},
Journal = {{ACTA ASTRONOMICA}},
Year = {{2013}},
Volume = {{63}},
Number = {{1}},
Pages = {{41-52}},
Abstract = {{We present our final report on the search for pulsating M dwarfs. We
   used moderate (< 1 m) ground-based telescopes. Our detection was limited
   to 1 ppt, which is roughly 0.1 \% of flux variation. We employed both
   the Fourier technique and Phase Dispersion Minimization (PDM) method.
   Our data analyses revealed no detection of pulsations in M dwarfs down
   to the above amplitude level. The results of our survey cannot reject or
   confirm the theoretical calculations suggesting the M dwarfs can
   pulsate, however, if the pulsations exist in these stars their
   amplitudes must be lower than 1 ppt. As a by-product of our search we
   found several new variable stars, including pulsating stars, binary
   systems and flare stars.}},
