@article{ ISI:000314726700013,
Author = {Aktosun, Tuncay and Weder, Ricardo},
Title = {{High-energy analysis and Levinson's theorem for the selfadjoint matrix
   Schrodinger operator on the half line}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2013}},
Volume = {{54}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{The matrix Schrodinger equation with a selfadjoint matrix potential is
   considered on the half line with the general selfadjoint boundary
   condition at the origin. When the matrix potential is integrable, the
   high-energy asymptotics are established for the related Jost matrix, the
   inverse of the Jost matrix, and the scattering matrix. Under the
   additional assumption that the matrix potential has a first moment,
   Levinson's theorem is derived, relating the number of bound states to
   the change in the argument of the determinant of the scattering matrix.
   (C) 2013 American Institute of Physics.
