@article{ ISI:000314263700012,
Author = {Alejandra Suarez-Ortiz, G. and Cerda-Garcia-Rojas, Carlos M. and
   Hernandez-Rojas, Adriana and Pereda-Miranda, Rogelio},
Title = {{Absolute Configuration and Conformational Analysis of Brevipolides,
   Bioactive 5,6-Dihydro-alpha-pyrones from Hyptis brevipes}},
Journal = {{JOURNAL OF NATURAL PRODUCTS}},
