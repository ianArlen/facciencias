Title = {{Solar-like Oscillations in the Stars MC 5390438 and KIC 5701829 Observed
   by Kepler}},
Booktitle = {{NEW QUESTS IN STELLAR ASTROPHYSICS III: A PANCHROMATIC VIEW OF
   SOLAR-LIKE STARS, WITH AND WITHOUT PLANETS}},
Series = {{Astronomical Society of the Pacific Conference Series}},
