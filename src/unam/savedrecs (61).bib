@article{ ISI:000312295700005,
Author = {de la O, Claudio and Mevis, Lieke and Richter, Christin and
   Malaivijitnond, Suchinda and Ostner, Julia and Schuelke, Oliver},
Title = {{Reconciliation in Male Stump-tailed Macaques (Macaca arctoides):
   Intolerant Males Care for Their Social Relationships}},
