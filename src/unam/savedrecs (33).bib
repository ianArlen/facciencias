@article{ ISI:000379428800002,
Author = {Gamborino, Diana and del-Castillo-Negrete, Diego and Martinell, Julio J.},
Title = {{Multi-scale statistical analysis of coronal solar activity}},
Journal = {{NONLINEAR PROCESSES IN GEOPHYSICS}},
Year = {{2016}},
Volume = {{23}},
Number = {{4}},
Pages = {{175-188}},
Abstract = {{Multi-filter images from the solar corona are used to obtain temperature
   maps that are analyzed using techniques based on proper orthogonal
   decomposition (POD) in order to extract dynamical and structural
   information at various scales. Exploring active regions before and after
   a solar flare and comparing them with quiet regions, we show that the
   multi-scale behavior presents distinct statistical properties for each
   case that can be used to characterize the level of activity in a region.
   Information about the nature of heat transport is also to be extracted
   from the analysis.}},
