@article{ ISI:000371951700005,
Author = {Angel Sebastian, Miguel},
Title = {{Consciousness and Theory of Mind: a Common Theory?}},
Journal = {{THEORIA-REVISTA DE TEORIA HISTORIA Y FUNDAMENTOS DE LA CIENCIA}},
Year = {{2016}},
Volume = {{31}},
Number = {{1}},
Pages = {{73-89}},
Month = {{JAN}},
Abstract = {{Many have argued that the difference between phenomenally conscious
   states and other kind of states lies in the implicit self-awareness that
   conscious states have. Higher-Order-Representationalist (HOR) theories
   attempt to explain such self-awareness by means of higher-order
   representation. Consciousness depends on our capacity to represent our
   own mental states: our Theory of Mind. It is generally agreed that such
   an ability can be decomposed into another two: mindreading and
   metacognition.
   I will argue that consciousness cannot depend on mindreading. The
   tenability of HOR theories depends, therefore, on the relation between
   mindreading and metacognition. I analyze several views on such a
   relation and argue that none of them seem to be a plausible option for
   HOR theories.}},
