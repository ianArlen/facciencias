@article{ ISI:000377638500006,
Author = {de Lourdes Serrano-Sanchez, Maria and Guerao, Guillermo and
   Centeno-Garcia, Elena and Vega, Francisco J.},
Title = {{Crabs (Brachyura: Grapsoidea: Sesarmidae) as inclusions in Lower Miocene
   amber from Chiapas, Mexico}},
Journal = {{BOLETIN DE LA SOCIEDAD GEOLOGICA MEXICANA}},
Year = {{2016}},
Volume = {{68}},
Number = {{1}},
Pages = {{37-43}},
Abstract = {{Nine crab remains preserved in Miocene amber from Chiapas are reported.
   Systematic affinities of the crab remains indicate that they belong to
   the Sesarmidae family. Semi-terrestrial sesarmid species are frequently
   found in mangroves and thus their presence in Lower Miocene (Aquitanian)
   amber from mines of Campo La Granja, Chiapas reinforce previous
   interpretations of a mangrove paleoenvironment near the ancient Gulf of
   Mexico coast.}},
