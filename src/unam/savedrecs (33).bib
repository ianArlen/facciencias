@article{ ISI:000376731600004,
Author = {Garcia Ugarte, Marta Eugenia and Rosas Salas, Sergio Francisco},
Title = {{The Catholic Church in Mexico according to its historians (1960-2010)}},
Journal = {{ANUARIO DE HISTORIA DE LA IGLESIA}},
Year = {{2016}},
Volume = {{25}},
Pages = {{91-161}},
Abstract = {{This paper discusses in detail the various traditions, both clerical and
   secular, that have dominated the historiography of the Catholic Church
   in Mexico. The text does recount the historiographies published by
   different authors, to underline the efforts to systematize the studies
   on the Catholic Church. To facilitate the analysis and understanding of
   the publications of the last 50 years, the narrative is articulated
   around axes, both thematic and chronological. This methodology allowed
   presenting the historiographical advances, between the 16th century to
   the 21st century, and the large gaps that still exist.}},
