@article{ ISI:000367261600004,
Author = {Alvarez, G. and Montiel, H. and Conde-Gallardo, A. and Zamorano, R.},
Title = {{Non-resonant Microwave Absorption in Terbium Powders}},
Journal = {{JOURNAL OF SUPERCONDUCTIVITY AND NOVEL MAGNETISM}},
Year = {{2016}},
Volume = {{29}},
Number = {{1}},
Pages = {{15-18}},
Month = {{JAN}},
Abstract = {{The non-resonant microwave absorption is reported in terbium powders at
   X-band (8.8-9.8 GHz) in the 160-300 K temperature region. Two microwave
   absorption-type responses are observed as follows: the magnetically
   modulated microwave absorption spectroscopy (MAMMAS) and the low-field
   microwave absorption (LFMA). MAMMAS response detects the paramagnetic to
   antiferromagnetic-helical and to ferromagnetic transition at T (N) = 227
   K and T (C) = 214 K, respectively, being confirmed by means of magnetic
   measurements. Additionally, LFMA response is only observed at
   temperatures below similar to 230 K, and it is associated with the
   magnetic order (antiferromagnetic-helical/ferromagnetic) at low
   temperature in terbium powders.}},
