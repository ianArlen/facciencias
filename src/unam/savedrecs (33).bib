@article{ ISI:000368045000008,
Author = {Jaimes, Miguel A. and Reinoso, Eduardo and Ordaz, Mario and Huerta,
   Benjamin and Silva, Rodolfo and Mendoza, Edgar and Rodriguez, Juan C.},
Title = {{A new approach to probabilistic earthquake-induced tsunami risk
   assessment}},
