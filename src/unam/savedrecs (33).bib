@article{ ISI:000376486900005,
Author = {Martinez, Matilde and Matsumoto, Shigenori and Verjovsky, Alberto},
Title = {{HOROCYCLE FLOWS FOR LAMINATIONS BY HYPERBOLIC RIEMANN SURFACES AND
   HEDLUND'S THEOREM}},
Journal = {{JOURNAL OF MODERN DYNAMICS}},
Year = {{2016}},
Volume = {{10}},
Pages = {{113-134}},
Abstract = {{We study the dynamics of the geodesic and horocycle flows of the unit
   tangent bundle ((M) over cap, T-1 F) of a compact minimal lamination (M,
   F) by negatively curved surfaces. We give conditions under which the
   action of the affine group generated by the joint action of these flows
   is minimal and examples where this action is not minimal. In the first
