@article{ ISI:000374416800001,
Author = {Sanchez, Tonametl and Moreno, Jaime A. and Ortiz-Ricardez, Fernando A.},
Title = {{Construction of a Smooth Lyapunov Function for the Robust and Exact
   Second-Order Differentiator}},
Journal = {{MATHEMATICAL PROBLEMS IN ENGINEERING}},
Year = {{2016}},
Abstract = {{Differentiators play an important role in (continuous) feedback control
   systems. In particular, the robust and exact second-order differentiator
   has shown some very interesting properties and it has been used
   successfully in sliding mode control, in spite of the lack of a Lyapunov
   based procedure to design its gains. As contribution of this paper, we
   provide a constructive method to determine a differentiable Lyapunov
   function for such a differentiator. Moreover, the Lyapunov function is
   used to provide a procedure to design the differentiator's parameters.
   Also, some sets of such parameters are provided. The determination of
   the positive definiteness of the Lyapunov function and negative
   definiteness of its derivative is converted to the problem of solving a
   system of inequalities linear in the parameters of the Lyapunov function
   candidate and also linear in the gains of the differentiator, but
   bilinear in both.}},
