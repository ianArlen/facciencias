@article{ ISI:000370351800008,
Author = {Barbara, Loic and Crosta, Xavier and Leventer, Amy and Schmidt, Sabine
   and Etourneau, Johan and Domack, Eugene and Masse, Guillaume},
Title = {{Environmental responses of the Northeast Antarctic Peninsula to the
   Holocene climate variability}},
