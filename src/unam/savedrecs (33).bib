@article{ ISI:000366629400015,
Author = {Rouland, Daniel and Legrand, Denis and Cisternas, Armando and Streng,
   Daniel and Gir, Roopa and Souriau, Annie},
Title = {{Historical seismicity in oceans from sailors' testimonies}},
