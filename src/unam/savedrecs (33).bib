@article{ ISI:000367749700002,
Author = {Magana, Mario and Massone, Cesare and Magana, Pablo and Cerroni, Lorenzo},
Title = {{Clinicopathologic Features of Hydroa Vacciniforme-Like Lymphoma: A
   Series of 9 Patients}},
Journal = {{AMERICAN JOURNAL OF DERMATOPATHOLOGY}},
