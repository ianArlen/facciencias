@article{ ISI:000379988300010,
Author = {Freidenberg, Flavia and Javier Aparicio, Francisco},
Title = {{Mexico 2015: Between Partisan Fragmentation and Public Disaffection}},
Journal = {{REVISTA DE CIENCIA POLITICA}},
Year = {{2016}},
Volume = {{36}},
Number = {{1}},
Pages = {{219-238}},
Abstract = {{Mexico had a complicated year in 2015. It was a critical mid-term
   election year, organized under new electoral rules, amidst social
   unrest, insecurity and political violence accompanied by an increasing
   social disaffection with politics. After three years in office,
   President Pena Nieto faced adverse economic conditions and a significant
   decrease in approval rates after damaging events such as the Ayotzinapa
   tragedy, corruption scandals and low economic growth. However, due to an
   unprecedented vote fragmentation, the ruling coalition was able to
   retain its majority in the Chamber of Deputies.}},
