@article{ ISI:000375445500099,
Author = {Islas, S. R. and Zanella, R. and Saniger, J. M.},
Title = {{Thermal activation process of Au/TiO2 system: a molecular spectroscopy
   study}},
Journal = {{RSC ADVANCES}},
Year = {{2016}},
Volume = {{6}},
Number = {{48}},
Pages = {{42554-42560}},
Abstract = {{The activation process of Au nanoparticles deposited on TiO2 (Au/TiO2)
   was studied by in situ diffuse reflectance UV-VIS and IR spectroscopy.
   The deposition-precipitation method with urea was used to support the
   gold nanoparticle precursor (AuPrs) on TiO2 Degussa P25 and the
   activation process was carried out in H-2-Ar atmosphere. The results
   shows the existence of strong correlations during the different stages
   of thermal activation which include the decomposition of the Au complex
   delivering molecular reactive species, the formation of metallic AuNPs
   and the change in the effective optical bandgap of the TiO2 support as a
   result of charge transfer processes.}},
