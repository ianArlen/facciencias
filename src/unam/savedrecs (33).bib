@article{ ISI:000374995100010,
Author = {Ovando-Shelley, Efrain and Santoyo-Villa, Enrique and Hernandez, Julio},
Title = {{Mexico City's Metropolitan Cathedral and Sagrario Church 13 Years After
   Underexcavation and Soil Hardening}},
Journal = {{INTERNATIONAL JOURNAL OF ARCHITECTURAL HERITAGE}},
Year = {{2016}},
Volume = {{10}},
Number = {{2-3, SI}},
Pages = {{346-359}},
Abstract = {{In this article, we briefly discuss the main features and the most
   relevant achievements of the Project for the Geometrical Correction of
   Mexico City's Cathedral and Sagrario church during which these two
   monuments were underexcavated to correct differential settlements and
   where the underlying subsoil was later hardened with mortar grouts.
   Based upon instrumental observations and field test data, we describe
   the effects on the subsoil of the selective soil hardening that took
   place when underexcavation finished. We also look at the behaviour of
   both churches after more than 13 years after the end of underexcavation
   and soil hardening. Topographical data show that soil hardening has
   positively modified the response of both temples and that further
   interventions are not likely to be needed in the short and mid term.}},
