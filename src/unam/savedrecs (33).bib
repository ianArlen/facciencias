@inproceedings{ ISI:000380810000027,
Author = {Hernandez-Guzman, Abel and Aragon-Martinez, Nestor and Gomez-Munoz,
   Arnulfo and Massillon-JL, Guerda},
Editor = {{MassillonJL, G and Fossion, R and RosadoMendez, IM and AvilaRodriguez, MA and LopezPerez, DA}},
Title = {{Absorbed Dose Distribution in Liquid Water for a CyberKnife VSI Using
   Radiochromic EBT3 Film}},
Booktitle = {{MEDICAL PHYSICS: FOURTEENTH MEXICAN SYMPOSIUM ON MEDICAL PHYSICS}},
Series = {{AIP Conference Proceedings}},
