@inproceedings{ ISI:000378119600007,
Author = {Carlos Flores-Bustamante, Mario and Rosete-Aguilar, Martha and Calixto,
   Sergio},
Editor = {{Levitz, D and Ozcan, A and Erickson, D}},
Title = {{Mechanical and optical behavior of a tunable liquid lens using a
   variable cross section membrane. Modeling results}},
Booktitle = {{OPTICS AND BIOPHOTONICS IN LOW-RESOURCE SETTINGS II}},
Series = {{Proceedings of SPIE}},
Year = {{2016}},
Volume = {{9699}},
Note = {{Conference on Optics and Biophotonics in Low-Resource Settings II, San
   Francisco, CA, FEB 13-14, 2016}},
Organization = {{SPIE}},
Abstract = {{A lens containing a liquid medium and having at least one elastic
   membrane as one of its components is known as an elastic membrane lens
   (EML). An EML may change its optical properties by changing the profiles
   of the surface of the membrane and/or changing the refractive index of
   one of its components. An EML may use elastic membranes with constant or
   variable thickness. EML formed with elastic membranes of constant
   thickness have been studied extensively; however, the information of EML
   using elastic membranes of variable thickness is limited. In this work,
   we present simulation results of the mechanical and optical behavior of
   two EML that use elastic membranes of variable thickness (convex-plane
   membranes). Profiles of the surface of the membrane were virtually
   modified by increases of liquid volume and pressure inside the chamber
   of the EML. The model of the convex-plane membranes, as well as the
   simulation of their mechanical behavior were performed using Solidworks
   (R) software; and points of the deformed elastic lens were obtained.
   Algebraic expressions, (Schwarzschild formula, up to four deformation
   coefficients, in a cylindrical coordinate system (r, z)), of the
   meridional profiles of the first and second surfaces of the deformed
   convex-plane membranes, were obtained using the results from Solidworks
   (R) and a program in Mathematica (R) software. The optical performance
   of the EML was obtained by simulation using OSLO (R) software and the
   algebraic expressions obtained in Mathematica (R).}},
