@article{ ISI:000313793200003,
Author = {Luis Villalobos, Jose and Alvarez, Fernando},
Title = {{Two new genera and three new species of freshwater crabs (Crustacea:
   Pseudothelphusidae: Potamocarcinini) from Chiapas, Mexico}},
Journal = {{ZOOTAXA}},
