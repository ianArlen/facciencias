@inproceedings{ ISI:000328117300070,
Author = {Santoyo-Villa, E. and Ovando-Shelley, E.},
Editor = {{Bilotta, E and Flora, A and Lirer, S and Viggiani, C}},
Title = {{Permanence and preservation of the Metztitlan convent in Hidalgo, Mexico}},
Booktitle = {{GEOTECHNICAL ENGINEERING FOR THE PRESERVATION OF MONUMENTS AND HISTORIC
   SITES}},
Year = {{2013}},
Pages = {{633-640}},
Note = {{2nd International Symposium on Geotechnical Engineering for the
   Preservation of Monuments and Historic Sites, Napoli, ITALY, MAY 30-31,
   2013}},
Abstract = {{This paper describes the case of the Metztitlan convent, built in the
   XVIth century some 136 km north of Mexico City. We postulate that the
   survival of both the convent and the village nearby are threatened by an
   ongoing geological instability condition associated to a massive ancient
   landslide. We describe the general geological and geotechnical setting,
   the damages produced in the convent and the cracking patterns in the
   ground. We also analyze the measures that have been taken up to now to
   stabilize the slopes surrounding the convent. Based on the results of
   geotechnical data, precision topographic levellings and inclinometric
   data, we suggest that some of these measures have had beneficial effects
   but others have been ineffectual. In the Authors' view, the case of the
   Metztitlan convent must remain open and we suggest some new measures to
   ensure its survival.}},
