@inproceedings{ ISI:000332889700007,
Author = {Santoyo, Francisco and Chavez, Edgar and Tellez, Eric S.},
Book-Group-Author = {{IEEE}},
Title = {{Compressing Locality Sensitive Hashing Tables}},
Booktitle = {{2013 MEXICAN INTERNATIONAL CONFERENCE ON COMPUTER SCIENCE (ENC 2013)}},
