@article{ ISI:000316032300087,
Author = {Toledo-Cervantes, Alma and Morales, Marcia and Novelo, Eberto and Revah,
   Sergio},
Title = {{Carbon dioxide fixation and lipid storage by Scenedesmus obtusiusculus}},
Journal = {{BIORESOURCE TECHNOLOGY}},
