@article{ ISI:000314554300047,
Author = {Gutierrez-Pineres, Antonio C. and Lopez-Monsalvo, Cesar S.},
Title = {{A static axisymmetric exact solution of f(R)-gravity}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2013}},
Volume = {{718}},
Number = {{4-5}},
Pages = {{1493-1499}},
Month = {{JAN 29}},
Abstract = {{We present an exact, axially symmetric, static, vacuum solution for f
   (R)-gravity in Weyl's canonical coordinates. We obtain a general
   explicit expression for the dependence of df(R)/dR upon the r and z
   coordinates and then the corresponding explicit form of f(R), which must
   be consistent with the field equations. We analyze in detail the
   modified Schwarzschild solution in prolate spheroidal coordinates.
   Finally, we study the curvature invariants and show that, in the case of
   f(R) not equal R, this solution corresponds to a naked singularity. (C)
   2012 Elsevier B.V. All rights reserved.}},
