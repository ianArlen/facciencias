@inproceedings{ ISI:000332514600016,
Author = {Alvarado-Barrientos, M. S. and Asbjornsen, H. and Holwerda, F.},
Editor = {{Steppe, K}},
Title = {{Nighttime Transpiration Enhanced after Fog Events?}},
Booktitle = {{IX INTERNATIONAL WORKSHOP ON SAP FLOW}},
