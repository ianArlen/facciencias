@inproceedings{ ISI:000326708600031,
Author = {Li, Changqing and Martinez Davalos, Arnulfo and Cherry, Simon R.},
Editor = {{Tromberg, BJ and Yodh, AG and SevickMuraca, EM}},
Title = {{Numerical and Experimental Studies of X-ray Luminescence Optical
   Tomography for Small Animal Imaging}},
