@inproceedings{ ISI:000326235301054,
Author = {Froese, Tom and Gershenson, Carlos and Rosenblueth, David A.},
Book-Group-Author = {{IEEE}},
Title = {{The Dynamically Extended Mind A Minimal Modeling Case Study}},
