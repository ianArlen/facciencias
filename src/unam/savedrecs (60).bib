@article{ ISI:000315368500020,
Author = {Charpentier, Isabelle and Sarocchi, Damiano and Rodriguez Sedano, Luis
   Angel},
Title = {{Particle shape analysis of volcanic clast samples with the Matlab tool
   MORPHEO}},
Journal = {{COMPUTERS \& GEOSCIENCES}},
Year = {{2013}},
Volume = {{51}},
Pages = {{172-181}},
Month = {{FEB}},
Abstract = {{This paper presents a modular Matlab tool, namely MORPHEO, devoted to
   the study of particle morphology by Fourier analysis. A benchmark made
   of four sample images with different features (digitized coins, a pebble
   chart, gears, digitized volcanic clasts) is then proposed to assess the
   abilities of the software. Attention is brought to the Weibull
   distribution introduced to enhance fine variations of particle
   morphology. Finally, as an example, samples pertaining to a lahar
   deposit located in La Lumbre ravine (Colima Volcano, Mexico) are
   analysed. MORPHEO and the benchmark are freely available for research
   purposes. (c) 2012 Elsevier Ltd. All rights reserved.}},
