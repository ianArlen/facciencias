@inproceedings{ ISI:000343671600079,
Author = {Benitez, Erika and Sorcia, Marco and Hiriart, David},
Editor = {{Gomez, JL}},
Title = {{A large rotation of the polarization angle of the TeV Blazar W Comae}},
Booktitle = {{INNERMOST REGIONS OF RELATIVISTIC JETS AND THEIR MAGNETIC FIELDS}},
