@article{ ISI:000331502800016,
Author = {Gomez-Romero, M. and de la Barrera, E. and Villegas, J. and
   Lindig-Cisneros, R.},
Title = {{Fertilization and association with pioneer herbaceous species on the
   performance of Pinus pseudostrobus}},
Journal = {{PHYTON-INTERNATIONAL JOURNAL OF EXPERIMENTAL BOTANY}},
Year = {{2013}},
Volume = {{82}},
Pages = {{135-143}},
Abstract = {{When degradation is severe, as it is often the case on acrisols, it is
   necessary to test the effect of plant-plant interactions in their
   survival and growth. An experiment was conducted, with substrate from an
   eroded site (acrisols with gullies) in a shaded greenhouse (30\% shade)
   to evaluate the effect of fertilization and the presence of pioneer
   herbaceous species in the performance of Pinus. pseudostrobus. The
   pioneer species Lupinus mexicanus and Tithonia tubiformis were used in
   three experimental treatments and a control. To fertilize, KH2PO4 was
   applied in four concentration levels. There were a total of 16 treatment
   combinations when considering both factors. Results showed significant
   differences (p <= 0.01) among the treatments of presence of pioneer
   herbaceous species. The species that was more influential in pine
   performance was the legume Lupinus mexicanus; when it was present, the
   pines had a higher survival (98\%) than pines under control conditions
   (78\%), being the difference statistically significant. Fertilization
   had no effect neither in the survival nor the performance of P.
   pseudostrobus. These results suggest that in these soils, reestablishing
   biotic interactions is necessary for improving the performance of tree
   species.}},
