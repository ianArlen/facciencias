@inproceedings{ ISI:000332553000012,
Author = {da Silva, M. L. R. B. and Santos, D. C. and de Lyra, M. C. C. P. and
   Mondragon-Jacobo, C. and Martinez-Romero, E.},
Editor = {{Nefzaoui, A and Inglese, P and Liguori, G}},
Title = {{Isolation and Molecular Characterization of Endophytic Bacteria
   Associated with Forage Cactus (Opuntia spp.)}},
Booktitle = {{VII INTERNATIONAL CONGRESS ON CACTUS PEAR AND COCHINEAL}},
Series = {{Acta Horticulturae}},
