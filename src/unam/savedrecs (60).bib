@article{ ISI:000314008900013,
Author = {Otero, Jose A. and Rodriguez-Ramos, Reinaldo and Bravo-Castillero,
   Julian and Guinovart-Diaz, Raul and Sabina, Federico J. and Monsivais,
   G.},
Title = {{Semi-analytical method for computing effective properties in elastic
   composite under imperfect contact}},
Journal = {{INTERNATIONAL JOURNAL OF SOLIDS AND STRUCTURES}},
Year = {{2013}},
Volume = {{50}},
Number = {{3-4}},
Pages = {{609-622}},
Month = {{FEB}},
Abstract = {{A parallel fiber-reinforced periodic elastic composite is considered
   with transversely iso-tropic constituents. Fibers with circular cross
   section are distributed with the same periodicity along the two
   perpendicular directions to the fiber orientation, i.e., the periodic
   cell of the composite is square. The composite exhibits imperfect
   contact, in particular, spring type at the interface between the fiber
   and matrix is modeled. Effective properties of this composite for
   in-plane and anti-plane local problems are calculated by means of a
   semi-analytic method, i.e. the differential equations that described the
   local problems obtained by asymptotic homogenization method are solved
   using the finite element method. Numerical computations are implemented
   and comparisons with exact solutions are presented. (C) 2012 Elsevier
   Ltd. All rights reserved.}},
