@article{ ISI:000222681600049,
Author = {Reimann, B and Brugmann, B},
Title = {{Late time analysis for maximal slicing of Reissner-Nordstrom puncture
   evolutions}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2004}},
Volume = {{69}},
Number = {{12}},
Month = {{JUN}},
Abstract = {{We perform an analytic late time analysis for maximal slicing of the
   Reissner-Nordstrom black hole spacetime. In particular, we discuss the
   collapse of the lapse in terms of its late time behavior at the throat
   and at the event horizon for the even and the puncture lapse. In the
   latter case we also determine the value of the lapse at the puncture.
   Furthermore, in the limit of late times slice stretching effects are
   studied as they arise for maximal slicing of puncture evolutions. We
   perform numerical experiments for a Schwarzschild black hole with
   puncture lapse and find agreement with the analytical results.}},
