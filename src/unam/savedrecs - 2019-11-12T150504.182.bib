@article{ ISI:000221586300010,
Author = {Wang, XY and Luo, H and Yang, HP and Sebastian, PJ and Gamboa, SA},
Title = {{Oxygen catalytic evolution reaction on nickel hydroxide electrode
   modified by electroless cobalt coating}},
