Author = {Andrade, E and Flores, M and Muhl, S and Barradas, NP and Murillo, G and
   Zavala, EP and Rocha, MF},
Title = {{Ion beam analysis of TiN/Ti multilayers deposited by magnetron
   sputtering}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
