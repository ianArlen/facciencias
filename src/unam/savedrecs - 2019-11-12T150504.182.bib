@article{ ISI:000221895800145,
Author = {Andrade, E and Chromik, A and Jergel, M and Jergel, M and Falcony, C and
   Strbik, V and Rocha, MF},
Title = {{Elemental depth profiles of MgB2/Si precursor and superconducting films}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2004}},
Volume = {{219}},
Pages = {{768-772}},
