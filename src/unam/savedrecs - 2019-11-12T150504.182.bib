@article{ ISI:000224051700078,
Author = {Abatal, M and Chavira, E and Tlamani-Amador, J and Garcia-Vazquez, V},
Title = {{Study of the RuSr2(Gd(1-x)Ln(x))Cu2O8 +/- z system, for 0.0 < x < 0.7
   with Ln = Dy, Ho, Er, Yb, and Lu}},
Journal = {{PHYSICA C-SUPERCONDUCTIVITY AND ITS APPLICATIONS}},
Year = {{2004}},
Volume = {{408}},
Pages = {{185-186}},
Month = {{AUG}},
Note = {{7th International Conference on Materials and Mechanisms of
   Superconductive and High Temperature Superconductors, Rio de Janeiro,
   BRAZIL, MAY 25-30, 2003}},
Abstract = {{We present preliminary results on the RuSr2(Gd(1-x)Ln(x))Cu2O8+/-z
   system for 0.0 < x < 0.7 with Ln = Dy, Ho, Er, Yb, and Lu. The samples
   were prepared by the solid-state reaction method in air at ambient
   pressure between 650 and 1060 degreesC. The X-ray powder diffraction
   patterns show a solid solution from x = 0.0 to 0.7. This system is
   isomorphous to the RuSr2GdCu2O8 composition. Resistance measurements
   from 7 to 300 K show a semiconducting, insulating behaviour. Studies by
   scanning electron microscopy (SEM) give a particle size around 1-10 mum.
   Transmission electron microscopy (TEM) data corroborate a tetragonal
   unit cell with a space group P4/mmm. We discuss the preparation
   procedure and the influence of the ionic size in obtaining the single
   phase and the resistance results. (C) 2004 Elsevier B.V. All rights
   reserved.}},
