@article{ ISI:000223390600007,
Author = {da Silva, CM and Monter, AV and Garcia, PS and Gonzalez, GA and Mendoza,
   MNR and Posadas, LDMR},
Title = {{Effect of culture medium osmotic potential and CA content on CA(2+) and
   K+ distribution, dry weight yield and shoot tip necrosis in grapevine
   ``R110{''}}},
Journal = {{INTERCIENCIA}},
Year = {{2004}},
Volume = {{29}},
Number = {{7}},
Pages = {{384-388}},
Month = {{JUL}},
Abstract = {{The osmotic potential, ionic balance and the nutrient ratios of culture
   medium are determinant for in vitro propagated plants growth; however,
   in most investigations, these factors are not considered. The effect of
   different osmotic potentials (-0.106, -0.113, -0.120, -0.128, -0.143 and
   -0.157MPa) and Ca2+ contents of culture medium. salts on the
   distribution of Ca2+ and K+, and on dry biomass of explants of grapevine
   were evaluated. The Ca2+ and K+ concentrations were measured with a low
   vacuum scanning electron microscope (MEB-LV). Ca2+ and K+ translocation
   from stems to leaves was favored by the higher osmotic potential of
   culture medium (-0.106MPa), whereas the larger contents of these
   elements at the base of the stems were found at the most negative
   osmotic potential (-0.157MPa). The greater dry biomass weight (57mg) and
   the smaller percentage of explants with Ca2+ deficiency symptoms (20\%)
   were obtained at -0.106MPa, the lower osmotic potential, and 1.5mM Ca2+
   in the culture medium.}},
Publisher = {{INTERCIENCIA}},
Address = {{APARTADO 51842, CARACAS 1050A, VENEZUELA}},
