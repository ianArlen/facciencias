@article{ ISI:000221364700010,
Author = {Nunez-Santiago, MC and Bello-Perez, LA and Tecante, A},
Title = {{Swelling-solubility characteristics, granule size distribution and
   rheological behavior of banana (Musa paradisiaca) starch}},
