@article{ ISI:000223309700007,
Author = {Noguez, C and Roman-Velazquez, CE and Esquivel-Sirvent, R and
   Villarreal, C},
Title = {{High-multipolar effects on the Casimir force: The non-retarded limit}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{2004}},
Volume = {{67}},
Number = {{2}},
Pages = {{191-197}},
Month = {{JUL}},
Abstract = {{We implement a spectral representation formalism to calculate exactly
   the non-retarded Casimir force or dispersive van der Waals force,
   between a spherical nanoparticle and a planar substrate beyond the
   dipolar approximation. For a sphere of radius R separated a distance z
   from the substrate, we find that high-order multipole interactions
   induce extra factors R/z in the force, as compared to the dipole power
   law of similar toR(3)/z(3). As a consequence, at small separations the
   non-retarded Casimir force increases by several orders of magnitude with
   respect to that estimated in the usual dipole approximation.}},
