@article{ ISI:000222987500016,
Author = {Bretherton, CS and Uttal, T and Fairall, CW and Yuter, SE and Weller, RA
   and Baumgardner, D and Comstock, K and Wood, R and Raga, GB},
Title = {{The EPIC 2001 stratocumulus study}},
