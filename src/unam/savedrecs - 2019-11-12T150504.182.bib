@article{ ISI:000223749200011,
Author = {Carrero, JC and Lugo, H and Perez, DG and Ortiz-Martinez, C and
   Laclette, JP},
Title = {{Cyclosporin A inhibits calcineurin (phosphatase 2B) and P-glycoprotein
   activity in Entamoeba histolytica}},
Journal = {{INTERNATIONAL JOURNAL FOR PARASITOLOGY}},
Year = {{2004}},
Volume = {{34}},
Number = {{9}},
Pages = {{1091-1097}},
Month = {{AUG}},
Abstract = {{Cyclosporin A (CsA) inhibits the proliferation of several protozoan
   parasites through blocking the activity of calcineurin (Cn) or
   P-glycoproteins (Pgp). We report here, that inhibition of the
   proliferation of Entarnoeba histolytica trophozoites, the causal agent
   of human amebiasis, is due to interference of the phosphatase activity
   of Cn, in a similar fashion to the effect of this immunosuppressive drug
   on T lymphocytes. The non-immunosuppressive CsA analog PSC-833, which
   binds Pgp without interfering the function of Cn, did not inhibit the
   proliferation of HMI:HMl:IMSS trophozoites. Moreover, phosphatase
   activity of amebic Cn, detected using the phosphopeptide RII was
   drastically affected by incubation with CsA, but not with PSC-833. On
   the other hand, both drugs were also tested on clone C2 trophozoites,
   which grow in the presence of emetine due to over-expression of Pgp. The
   effect of CsA was similar to that observed on HMl:IMSS trophozoites,
   whereas PSC-833 only affected the proliferation and viability of clone
   C2 when the trophozoites were grown in the presence of 40 muM of
   emetine, suggesting an interference of the Pgp activity. This suggestion
   was confirmed by results from experiments of Pgpdependent effux of
   rhodamine from pre-loaded trophozoites, in the presence of either of
   these drugs. Therefore, CsA inhibition of E. histolytica trophozoite
   proliferation is more likely due to Cn than Pgp activity inhibition. (C)
   2004 Australian Society for Parasitology Inc. Published by Elsevier Ltd.
   All rights reserved.}},
