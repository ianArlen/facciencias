@article{ ISI:000221561000049,
Author = {Avila, O and Brandan, ME},
Title = {{Low energy ion tracks in lithium fluoride}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
