@article{ ISI:000230254900013,
Author = {Gomez-Gama, M and Avila-Corona, R and Gomez-Gama, MA and Puente, SA and
   Ojeda-Morales, I},
Title = {{Further studies on the effects of tonal languages, non-tonal languages,
   and musical training on music perception of twelve Mexican communities}},
Journal = {{BULLETIN OF THE COUNCIL FOR RESEARCH IN MUSIC EDUCATION}},
Year = {{2004}},
Number = {{161-62}},
Pages = {{99-105}},
Month = {{SUM-FAL}},
Note = {{20th ISME Research Seminar, Las Palmas de Gran Canaria, SPAIN, JUL, 2004}},
Organization = {{ISME}},
Abstract = {{Many scholars have studied the development of auditory perception. The
   purpose of this investigation was to study the effect of the tonal
   languages, non-tonal languages, and music training, on music perception.
   This work was carried out in four years. In the first part of this
   study, a total of 413 Mexican bilingual students, between 8 and 22 years
   of age, from two urban communities, four tonal language native
   communities, and three non-tonal language native communities,
   participated in this study. On the second part of this study, we added
   the data obtained from 62 students from two other Mexican student
   groups, the first group from a tonal language community and other group
   from a non-tonal language community. Two tests were applied to evaluate
   all the participants' aural perception abilities, the Robert Walker ``16
   sounds{''} visual auditory test, and the Harold Fiske ``Visual Auditory
   Melodic Contour Test.{''} The Walker test was concerned with auditory
   stimulus differentiation among four acoustic parameters -frequency,
   waveform, amplitude, and duration - with options of visual metaphors for
   each. The Fiske test was concerned with melodic contour differentiation
   with choices of four single line drawn contours. Musical training and
   the tonal language effect determined consistency of choice.}},
