@article{ ISI:000222097000011,
Author = {Raggi-Cardenas, AG and Valero-Elizondo, L},
Title = {{Normalizing isomorphisms between Burnside rings}},
Journal = {{JOURNAL OF ALGEBRA}},
Year = {{2004}},
Volume = {{277}},
Number = {{2}},
Pages = {{643-657}},
Month = {{JUL 15}},
Abstract = {{We prove that if two finite groups G and G' have isomorphic Burnside
   rings, then there is a normalized isomorphism between these rings, that
   is, a ring isomorphism theta: B(G) --> B(G') such that theta(G/1) =
   G'/1. We use this to prove that if two finite groups have isomorphic
   Burnside rings, then there is a one-to-one correspondence between their
   families of soluble subgroups which preserves order and conjugacy class
   of subgroups. (C) 2004 Elsevier Inc. All rights reserved.}},
