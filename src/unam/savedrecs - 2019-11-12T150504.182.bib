@article{ ISI:000222445600005,
Author = {Mendoza-Anaya, D and Salas, P and Angeles-Chavez, C and Perez-Hernandez,
   R and Castano, VM},
Title = {{Microstructural characterization and morphology of TiO2 for
   thermoluminescent applications}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2004}},
Volume = {{50}},
Number = {{1}},
Pages = {{12-16}},
Month = {{JUN}},
Note = {{6th National Congress of the Asociacion-Mexicana-de-Microscopia,
   Chihuahua, MEXICO, SEP, 2002}},
Organization = {{Asociacion Mexicana Microscopia}},
Abstract = {{Titanium oxide (TiO2) is a ceramic material that presents polymorphism
   reflected in different phases. These phases are anatase, rutile and
   brookite, which have the same chemical composition but different
   crystalline structure and physical properties. These features play an
   important roll in the final application for the crystalline phases of
   TiO2, such as we presented in this work. TiO2 sample was prepared by
   Sol-Gel method, subsequently samples were subject to thermal treatment
   at 500, 600, 700 and 800degreesC. The structural and morphological
   characteristics were obtained using DRX, SEM and TEM. Superficial area
   was determined by use of nitrogen adsorption. Results showed an
   important effect by thermal treatment on the surface area, crystalline
   structure and morphological characteristics. Presence of anatase with a
   surface area of 54 m(2) /gr, while rutile with 1.54 m(2) /gr were
   determined for thermal treatment at 500 and 800degreesC, respectively.
   Also different thermoluminescence properties in the anatase and rutile
   phases were also observed. This result is important in radiation physics
   because TiO2 could be applied for radiation dosimetry.}},
