@article{ ISI:000221895800036,
Author = {Mireles, A and Solis, C and Andrade, E and Lagunas-Solar, M and Pina, C
   and Flocchini, RG},
Title = {{Heavy metal accumulation in plants and soil irrigated with wastewater
   from Mexico city}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2004}},
Volume = {{219}},
Pages = {{187-190}},
