@article{ ISI:000222445600016,
Author = {Mancera, GG and Suarez, VJC and Vieyra, JGR},
Title = {{Effect of the hydrogen on the fractographycs characteristics of ALSI
   1018 steel}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2004}},
Volume = {{50}},
Number = {{1}},
Pages = {{60-62}},
Month = {{JUN}},
Note = {{6th National Congress of the Asociacion-Mexicana-de-Microscopia,
   Chihuahua, MEXICO, SEP, 2002}},
Organization = {{Asociacion Mexicana Microscopia}},
Abstract = {{The effect of hydrogen on a fracture mode in low carbon steel was
   studied in the present investigation using scanning electron microscopy.
   Hydrogen was introduced in a steel tension test, through cathodic
   charging, applying a current density of 50 mA/cm(2). It was found that
   the presence of hydrogen in the steel changes the fracture mode, passing
   from the microvoid coalescence to quasi-cleavage, accelerating the
   fracture process. The non metallic inclusions in the steel, act as sinks
   of hydrogen, therefore when a critical concentration of hydrogen is
   reached the crack propagates, following a transgranular path. The
   mechanical behavior showed that the transition in the fracture mode is
   accompanied lay an appreciable decrease in the ductility of the steel.}},
