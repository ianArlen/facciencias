@article{ ISI:000220451100002,
Author = {Chicurel, R and Rios, S},
Title = {{Stress analysis of an embedded fibre by superposition of virtual forces}},
Journal = {{MECHANICS OF MATERIALS}},
Year = {{2004}},
Volume = {{36}},
Number = {{7}},
Pages = {{573-583}},
Month = {{JUL}},
Abstract = {{A method is presented for the determination of the stresses in a fibre
   embedded in a matrix. The fibre is imaginarily removed and substituted
   by the matrix material to which a number of virtual forces and
   2D-centres of force are applied along the fibre's axis. Equations for
   these virtual loads are obtained by applying conditions of strain and
   displacement continuity at the interface. Solutions for an isolated
   fibre in an infinite matrix subjected to tension are obtained. As the
   aspect ratio of the fibre is increased, the axial stress curve tends to
   become flat in the central portion of the fibre, this effect being less
   immediate as the ratio of matrix to fibre rigidity is decreased. The
   maximum shear stress at the interface increases at first as the aspect
   ratio is increased and then decreases. (C) 2003 Elsevier Ltd. All rights
   reserved.}},
