@article{ ISI:000221146400025,
Author = {Robles, M and de Haro, ML and Santos, A},
Title = {{Equation of state of a seven-dimensional hard-sphere fluid.
   Percus-Yevick theory and molecular-dynamics simulations}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{2004}},
Volume = {{120}},
Number = {{19}},
Pages = {{9113-9122}},
Month = {{MAY 15}},
Abstract = {{Following the work of Leutheusser {[}Physica A 127, 667 (1984)], the
   solution to the Percus-Yevick equation for a seven-dimensional
   hard-sphere fluid is explicitly found. This allows the derivation of the
   equation of state for the fluid taking both the virial and the
   compressibility routes. An analysis of the virial coefficients and the
   determination of the radius of convergence of the virial series are
   carried out. Molecular-dynamics simulations of the same system are also
   performed and a comparison between the simulation results for the
   compressibility factor and theoretical expressions for the same quantity
   is presented. (C) 2004 American Institute of Physics.}},
