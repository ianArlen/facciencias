@article{ ISI:000222939300007,
Author = {Urrutia-Fucugauchi, J and Soler-Arechalde, AM and Rebolledo-Vieyra, M
   and Vera-Sanchez, P},
Title = {{Paleomagnetic and rock magnetic study of the Yaxcopoil-1 impact breccia
   sequence, Chicxulub impact crater (Mexico)}},
Journal = {{METEORITICS \& PLANETARY SCIENCE}},
Year = {{2004}},
Volume = {{39}},
