Title = {{Patterns and universals of adult romantic attachment across 62 cultural
   regions - Are models of self and of other pancultural constructs?}},
Journal = {{JOURNAL OF CROSS-CULTURAL PSYCHOLOGY}},
Year = {{2004}},
Volume = {{35}},
