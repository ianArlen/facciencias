@article{ ISI:000223053300018,
Author = {Galvan, DH and Samaniego, C},
Title = {{Electronic structure calculations for PrOs4Sb12 filled skutterudite. A
   theoretical approach using extended Huckel tight-binding method}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2004}},
Volume = {{70}},
Number = {{4}},
Month = {{JUL}},
Abstract = {{To gain insight into the electronic properties of PrOs4Sb12 filled
   skutterudite, electronic structure calculations were performed. The
   calculated energy bands depict a semimetallic behavior. The
   superconductivity observed in this compound is likely due to the
   electrons associated to the (Os4Sb12)(3-) sublattice, with small
   contributions of Pr f-electrons. On the other hand, a strong
   hybridization occurs between Pr f-orbitals with Os d and Sb p orbitals,
   which make us believe that this hybridization is responsible for the
   heavy Fermion behavior for this compound.}},
