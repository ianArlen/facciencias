@article{ ISI:000222195400046,
Author = {Delgado-Olguin, P and Rosas-Vargas, H and Recillas-Targa, F and
   Zentella-Dehesa, A and de Leon, MB and Cisneros, B and Salamanca, F and
   Coral-Vazquez, R},
Title = {{NFI-C2 negatively regulates alpha-sarcoglycan promoter activity in C2C12
   myoblasts}},
