@article{ ISI:000222416800009,
Author = {Bali, S and Singh, AK and Sharma, P and Toscano, RA and Drake, JE and
   Hursthouse, MB and Light, ME},
Title = {{2-{[}2-(4-Methoxyphenyltelluro)ethyl]thiophene (L-1)
   bis{[}2-(2-thienyl)ethyl] telluride (L-2) and their metal complexes;
   crystal structure of
   trans-dichlorobis\{2-(2-(4-methoxyphenyltelluro)ethyl)
   thiophene-Te\}palladium(II) and \{bis{[}2-(2-thienyl)ethyll telluride\}
   dichloro(p-cymene)ruthenium(II)}},
