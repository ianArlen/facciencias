@article{ ISI:000224232700017,
Author = {Gorostiza, LG and Porter, RM},
Title = {{A stochastic model for transport of size-structured particulate matter
   subject to fragmentation}},
Journal = {{MATHEMATICAL AND COMPUTER MODELLING}},
Year = {{2004}},
Volume = {{40}},
Number = {{1-2}},
Pages = {{193-199}},
Month = {{JUL}},
Abstract = {{A stochastic model for transport of particles subject to random motion,
   fragmentation and change of size was proposed in {[}1] at an abstract
   mathematical level. The motivation derived from air pollution by
   suspended particulate matter, and the aim was to find the asymptotic
   high-density probability distribution of spatial concentrations of
   particles of different sizes. In this note, we carry out computer
   calculations and simulations in order to observe the behavior of the
   model quantitatively and graphically in special cases. This type of
   model also has potential applications in other fields where the main
   effect is the random distribution of sizes (or masses) caused by
   fragmentation. (C) 2004 Elsevier Ltd. All rights reserved.}},
