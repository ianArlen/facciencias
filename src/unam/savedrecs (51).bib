@article{ ISI:000353299900008,
Author = {Jimenez Flores, Juan and Flores Herrera, Luz Maria},
Title = {{Design and psychometric validation of the Harsh Discipline Practice List}},
Journal = {{Revista Iberoamericana de Diagnostico y Evaluacion-e Avaliacao
   Psicologica}},
Year = {{2014}},
Volume = {{2}},
Number = {{38}},
Pages = {{137-153}},
Abstract = {{The aim of this study was to develop and validate an instrument to asses
   a set of items of Harsh Discipline Practices. It was an exploratory
   study supported by parental discipline practices theories. The sample
   was 155 mothers, 25 to 45 years old, their children was public schools
   pupils. The socioeconomic status was working class. Factor analyses used
   to establish the construct validity yielded 5 scales: hostile
   discipline, maltreatment, abuse, moderate corporal punishment and
   moderate verbal punishment. Reliability was evaluated by estimating
   internal consistency, the Cronbach's Alpha coefficient was 87 for the
   total list. These findings must be extrapolated to the broader
   population to improve reliability and evaluate if the same factors
   maintained.}},
