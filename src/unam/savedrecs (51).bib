@inproceedings{ ISI:000354534900001,
Author = {Fraigniaud, Pierre and Gafni, Eli and Rajsbaum, Sergio and Roy, Matthieu},
Editor = {{Kuhn, F}},
Title = {{Automatically Adjusting Concurrency to the Level of Synchrony}},
Booktitle = {{DISTRIBUTED COMPUTING (DISC 2014)}},
