@article{ ISI:000331965600029,
Author = {Moreno-Ahedo, Luis and Collado, Joaquin and Vazquez, Carlos},
Title = {{Parametric Resonance Cancellation Via Reshaping Stability Regions:
   Numerical and Experimental Results}},
Journal = {{IEEE TRANSACTIONS ON CONTROL SYSTEMS TECHNOLOGY}},
Year = {{2014}},
Volume = {{22}},
Number = {{2}},
Pages = {{753-760}},
Month = {{FEB}},
Abstract = {{In this brief, a vibrational control law is designed and applied to an
   unstable and parametrically excited mechanical system, namely, a gantry
   crane. The control law cancels the parametric resonance by reshaping the
   stable regions. The parameters of the control law are tuned by using a
   graphic scheme based on the symbolic computation of the monodromy
   matrix. The control law is implemented experimentally in the laboratory
   model of a gantry crane, confirming the theoretical results.}},
