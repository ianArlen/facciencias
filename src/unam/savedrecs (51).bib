@article{ ISI:000331613300005,
Author = {Ayala, Guadalupe and Itzel Escobedo-Hinojosa, Wendy and Felipe de la
   Cruz-Herrera, Carlos and Romero, Irma},
Title = {{Exploring alternative treatments for Helicobacter pylori infection}},
Journal = {{WORLD JOURNAL OF GASTROENTEROLOGY}},
