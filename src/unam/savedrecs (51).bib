Title = {{Multiresolution analysis based on Mallat pyramidal algorithm applied to
   OPR data}},
Booktitle = {{PROCEEDINGS OF THE 2014 15TH INTERNATIONAL CONFERENCE ON GROUND
   PENETRATING RADAR (GPR 2014)}},
Year = {{2014}},
