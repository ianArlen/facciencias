@article{ ISI:000350109200005,
Author = {Lerma, Felix and Hernandez, Nicte and Pena, Daniela},
Title = {{AN APPROACH TO THE ASTHETICS OF ROCK ART FROM MEZQUITAL VALLEY, MEXICO}},
