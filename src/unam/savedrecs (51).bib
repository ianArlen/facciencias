@article{ ISI:000330571200064,
Author = {Tenopala-Carmona, F. and Garcia-Segundo, C. and Cuando-Espitia, Natanael
   and Hernandez-Cordero, Juan},
Title = {{Angular distribution of random laser emission}},
Journal = {{OPTICS LETTERS}},
Year = {{2014}},
Volume = {{39}},
Number = {{3}},
Pages = {{655-658}},
Month = {{FEB 1}},
Abstract = {{We present an experimental analysis of the angular distribution of the
   emission of a random laser (RL) operating in the diffusive regime
   (similar to 1\% volume fraction of scatterers). The RL ensemble was made
   of silica particles suspended in a 1: 1 methanol: water matrix with
   Rhodamine 6G dye as the active medium. We found that, for specific
   pumpin-genergy- dependent scattering strength, the RL spectrum reached
   stable features that were angularly preserved. From the analysis, we
   propose that this defines a novel parametric condition that may well be
   the equivalent of the RL critical volume, as proposed by Letokhov. (C)
   2014 Optical Society of America}},
