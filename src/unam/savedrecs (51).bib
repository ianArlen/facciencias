@inproceedings{ ISI:000348253202071,
Author = {Perez-Espinosa, R. and Ordaz-Flores, A. and Garcia-Valladares, O.},
Editor = {{Lentz, A and Renne, D}},
Title = {{Thermal evaluation of a two-phase solar domestic water heating systems
   following the Mexican Standard NMX-ES-004-NORMEX-2010}},
