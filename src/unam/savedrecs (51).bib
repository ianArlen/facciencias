@article{ ISI:000330955900055,
Author = {Villarreal D'Angelo, C. and Schneiter, M. and Costa, A. and Velazquez,
   P. and Raga, A. and Esquivel, A.},
Title = {{On the sensitivity of extrasolar mass-loss rate ranges: HD 209458b a
   case study}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2014}},
Volume = {{438}},
Number = {{2}},
Pages = {{1654-1662}},
Month = {{FEB}},
Abstract = {{We present a 3D hydrodynamic study of the effects that different stellar
   wind conditions and planetary wind structures have on the calculated Ly
   alpha absorptions produced by a cometary tail during transit. We
   concentrate, as a case study, on the known HD 209458b case. Initially,
   we assume a broad range of possible planetary mass-loss rate values:.
   (M) over dot(p) = {[}1-7] x 10(10) g s(-1). Then, by comparing the
   observational Ly alpha absorption with the numerically derived ones, we
   could constrain the (M) over dot(p) values within the given range. We
   find that the planetary mass-loss rate does not change dramatically for
   large changes in stellar wind speeds similar to{[}250-800] km s(-1) and
   temperature similar to{[}3-7] x 10(6) K while keeping fixed the stellar
   mass-loss rate ((M) over dot(star) = 9.0 x 10(-14)M(circle dot) yr(-1)).
   The (M) over dot(p) range found is similar to{[}3-5] x 10(10) g s(-1),
   depending upon the efficiency of the stellar wind to transport heat
   (polytropic index Gamma similar to {[}1.01-1.13]), leading to different
   stellar wind speeds. Several models with anisotropic evaporation
   profiles for the planetary escaping atmosphere were carried out, showing
   that both, the escape through polar regions, resembling the emission
   associated with reconnection processes, and through the night side,
   produced by a strong stellar wind that compresses the planetary
   atmosphere and inhibits its escape from the day hemisphere yields larger
   absorptions than an isotropic planetary wind.}},
