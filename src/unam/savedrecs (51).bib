@article{ ISI:000334278000010,
Author = {Navarrete-Ramirez, Pamela and Luna, Maricela and Valverde-R, Carlos and
   Orozco, Aurea},
Title = {{3,5-di-iodothyronine stimulates tilapia growth through an alternate
   isoform of thyroid hormone receptor beta 1}},
Journal = {{JOURNAL OF MOLECULAR ENDOCRINOLOGY}},
