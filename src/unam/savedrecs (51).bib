@article{ ISI:000347518500001,
Author = {Froese, Tom and Leavens, David A.},
Title = {{The direct perception hypothesis: perceiving the intention of another's
   action hinders its precise imitation}},
Journal = {{FRONTIERS IN PSYCHOLOGY}},
