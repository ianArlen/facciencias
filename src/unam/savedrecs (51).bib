@inproceedings{ ISI:000363767200031,
Author = {Ramirez, B. F. and Medina, F. A. and Guerrero, N. P. and Gonzalez, J. M.
   and Vinas, D. E. and Moctezuma, P. A.},
Editor = {{Braidot, A and Hadad, A}},
Title = {{Development Proposal of a Masticatory Reflexmetry System Used in
   Monitoring the Treatment of People with Craniomandibular Dysfunctions}},
Booktitle = {{VI LATIN AMERICAN CONGRESS ON BIOMEDICAL ENGINEERING (CLAIB 2014)}},
Series = {{IFMBE Proceedings}},
Year = {{2014}},
