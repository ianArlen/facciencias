Language = {{English}},
Affiliation = {{Martinez-Mayorga, K (Reprint Author), Univ Nacl Autonoma Mexico, Inst Quim, Av Univ 3000, Mexico City 04510, DF, Mexico.
   Marmolejo-Valencia, A. F.; Martinez-Mayorga, K., Univ Nacl Autonoma Mexico, Inst Quim, Av Univ 3000, Mexico City 04510, DF, Mexico.}},
DOI = {{10.1007/s10822-017-0016-7}},
ISSN = {{0920-654X}},
EISSN = {{1573-4951}},
Keywords = {{Herkinorin; Functional selectivity; Biased agonist; Mu opioid receptor;
