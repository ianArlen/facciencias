@article{ ISI:000411573300013,
Author = {Bokser Misses-Liwerant, Judit},
Title = {{Holocaust, Modernity, Memory... New Critical Reflections on Bauman}},
Journal = {{REVISTA MEXICANA DE CIENCIAS POLITICAS Y SOCIALES}},
Year = {{2017}},
Volume = {{62}},
Number = {{230}},
Pages = {{339-358}},
Month = {{MAY-AUG}},
Abstract = {{Zygmunt Bauman's work has emerged as a significant contribution to
   critical thinking aimed at redefining and expanding the margins of
   disciplinary reflection, to incorporate trends and events that have
   exposed the limits of the complex process of construction of Modernity.
   Among the topics brought to the field of sociological inquiry, an
   important place has been occupied by the Holocaust. The article argues
   that the analysis of the event is subsumed in the conceptualization of
   Modernity, as articulated by the Enlightenment, disregards the
   theoretical and practical struggles to define it and its impact on the
   Jewish condition, and extends its analysis to the conceptualization of
   the victims, their specificity, the approach to memory and the
   implications of the links between morality and political criticism.}},
