@article{ ISI:000395600800013,
Author = {Galeana-Sanchez, Hortensia and Jose Montellano-Ballesteros, Juan},
Title = {{A Richardson's theorem version for Pi -kernels}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2017}},
Volume = {{340}},
Number = {{5}},
Pages = {{984-987}},
Month = {{MAY}},
Abstract = {{Let D = (V(D), A(D)) be a digraph, DP(D) be the set of directed paths of
   D and let Pi be a subset of DP(D). A subset S subset of V(D) will be
   called Pi-independent if for any pair \{x, y\} subset of S, there is no
   xy-path noryx-path in Pi; and will be called Pi-absorbing if for every x
   epsilon V(D)\textbackslash{}S there is y epsilon S such that there is an
   xy-path in Pi. A set S subset of V(D) will be called a Pi-kernel if S is
   H-independent and Pi-absorbing. Given x, y epsilon V(D) and Q subset of
   Pi we will use (x ->(Q)y) (respectively (x negated right arrow(Q)y)) to
   denote the fact that there is an (respectively, no) xy-path in Q. A
   subset Q subset of Pi will be called transitive if whenever (x ->(Q)y)
   and (y ->(Q)z), then (x ->(Q)z). Given a partition P = \{Pi(i)\}(iEI) of
   Pi, the color digraph of D and I is the digraph C-D(P) with I as
   vertex-set and given any pair \{j, k\} subset of I, the arc jk belongs
   to C-D(P) if and only if there are x, y, w epsilon V(D) such that (x
   ->(Pi j)y) and (y ->Pi(w)(k)). In this paper we prove the following
   result: Let D be a digraph and Pi subset of DP(D). If there is a
   partition P = \{Pi i\}(i epsilon I) of Pi such that: For each i epsilon
   I, Pi(i) is transitive; and C-D(P) has no odd cycles of order greater
   than 1, then D has Pi-kernel. Some interesting previous results are
   obtained as a direct consequence of this result. 2017 (C) Elsevier B.V.
   All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
