@article{ ISI:000403122300032,
Author = {Hernandez-Guzman, F. and Nicho-Diaz, M. E. and Medrano-Solis, A. and
   Altuzar-Coello, P.},
Title = {{In-situ synthesis by Grignard Metathesis of poly(3-hexylthiophene) in
   presence of CdS and their properties}},
Journal = {{EUROPEAN POLYMER JOURNAL}},
Year = {{2017}},
Volume = {{90}},
Pages = {{407-417}},
Month = {{MAY}},
Abstract = {{In-situ synthesis by Grignard Metathesis and properties of a
   nanocomposite of poly (3-hexylthiophene) (P3HT) with Cadmium Sulfide
   (CdS) nanoparticles (P3HT/CdS) are reported. This synthesis method was
   used to obtain higher HT configuration in the P3HT/CdS nanocomposites
   synthesized in-situ, in order to improve their properties. It is
   noteworthy that this method has not been used for in-situ synthesis of
   such nanocomposites. P3HT synthesis was carried out in presence of
   different CdS content (3HT/CdS weight ratio of 1/0.01, 1/0.05 and
   1/0.11). Regioregular P3HT (RR-P3HT) and P3HT/CdS composites were
   characterized by FTIR, H-1 NMR, UV-Vis, photoluminescence,
   photoresponse, TGA, XRD and SEM-EDX. Effects of CdS content on
   properties in P3HT/CdS nanocomposites were studied. The formation of
   P3HT/CdS nanocomposites was confirmed by the FTIR spectra, CdS low
   concentration in P3HT led to better interaction between CdS and P3HT. A
   greater regioregularity to 94\% in P3HT within the composite was
   obtained. The composites exhibited interesting optical and electrical
   properties for optoelectronic devices.}},
