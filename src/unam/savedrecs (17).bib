@article{ ISI:000404474800010,
Author = {Kudriavtsev, Yu. and Asomoza-Palacio, R. and Manzanilla-Naim, L.},
Title = {{Interaction of Water Vapor with Silicate Glass Surfaces:
   Mass-Spectrometric Investigations}},
Journal = {{TECHNICAL PHYSICS LETTERS}},
Year = {{2017}},
Volume = {{43}},
Number = {{5}},
Pages = {{447-449}},
Month = {{MAY}},
Abstract = {{The secondary ion mass-spectroscopy technique was used to study the
   results of hydration of borosilicate, aluminosilicate, and soda-lime
   silicate glasses in H-1(2) O-18 water vapor containing 97\% of the
   isotope O-18. It is shown that hydration of the surface of the soda-lime
   silicate glass occurs as a result of the ion-exchange reaction with
   alkali metals. In the case of borosilicate and aluminosilicate glasses,
   water molecules decompose on the glass surface, with the observed
   formation of hydrogenated layer in the glass being the result of a
   solid-state chemical reaction-presumably, with the formation of
   hydroxides from aluminum and boron oxides.}},
