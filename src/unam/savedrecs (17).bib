@article{ ISI:000403465600033,
Author = {Huesca-Perez, Eduardo and Ortega, Roberto and Valenzuela, Raul W.},
Title = {{Continental crust anisotropy measurements from tectonic tremor in
   Cascadia}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-SOLID EARTH}},
