@article{ ISI:000400634900036,
Author = {Lara-Romero, J. y and Ocampo-Macias, T. and Martinez-Suarez, R. and
   Rangel-Segura, R. and Lopez-Tinoco, J. and Paraguay-Delgado, F. and
   Alonso-Nunez, G. and Jimenez-Sandoval, S. and Chinas-Castillo, F.},
Title = {{Parametric Study of the Synthesis of Carbon Nanotubes by Spray Pyrolysis
   of a Biorenewable Feedstock: alpha-Pinene}},
Journal = {{ACS SUSTAINABLE CHEMISTRY \& ENGINEERING}},
Year = {{2017}},
