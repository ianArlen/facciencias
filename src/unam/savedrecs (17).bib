@article{ ISI:000398623300002,
Author = {Kowalska-Leszczynska, Izabela and Bizouard, Marie-Anne and Bulik, Tomasz
   and Christensen, Nelson and Coughlin, Michael and Golkowski, Mark and
   Kubisz, Jerzy and Kulak, Andrzej and Mlynarczyk, Janusz and Robinet,
