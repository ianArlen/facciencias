@article{ ISI:000398277900002,
Author = {Barbero G, J. Fernando and Juarez-Aubry, Benito A. and
   Margalef-Bentabol, Juan and Villasenor, Eduardo J. S.},
Title = {{Boundary Hilbert spaces and trace operators}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{2017}},
Volume = {{34}},
Number = {{9}},
Month = {{MAY 4}},
Abstract = {{We discuss the introduction of boundary Hilbert spaces for a class of
   physical systems for which it is not possible to factor their state
   spaces as tensor products of Hilbert spaces naturally associated to
   their boundaries and bulks, respectively. In order to do this we make
   use of the so called trace operators that play a relevant role in the
   analysis of PDEs in bounded regions. By taking advantage of these
   operators and some functorial aspects of the construction of Fock
   spaces, we will show how to obtain quantum dynamics at the boundaries
   defined in appropriate Hilbert spaces associated with them.}},
