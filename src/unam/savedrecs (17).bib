@article{ ISI:000408407900012,
Author = {Gonzalez-Ramirez, Mireya and Zaragoza-Caballero, Santiago and Xinum
   Perez-Hernandez, Cisteil},
Title = {{Diversity analysis of Coleoptera in the tropical dry forest of
   Acahuizotla, Guerrero, Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2017}},
Volume = {{88}},
Number = {{2}},
Pages = {{381-388}},
Month = {{JUN}},
Abstract = {{A faunistic study of Coleoptera (Insecta) in the tropical dry forest of
   Acahuizotla, Guerrero, Mexico between February 2008 and October 2009 was
   done. Using entomological nets and light traps, 4,738 adult specimens
   were collected belonging to 50 families, 251 genera and 523
   morphospecies. 80.5\% of the specimens were collected with light traps.
   Diversity of order 1 (exponential of the Shannon index) reached a value
   of 153.32 effective species while diversity of order 2 (inverse of the
   Simpson index) was 68.30 effective species. According to the Chao1 and
   ACE estimates, the expected richness is 903 and 844 morphospecies,
   respectively (our sampling representing between 57.9\% and 61.9\%). The
   similarity between the sites was higher in those where light traps were
   used, because these insects were attracted by the artificial light. (C)
   2017 Universidad Nacional Autonoma de Mexico, Instituto de Biologia.}},
