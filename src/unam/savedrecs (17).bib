@article{ ISI:000404091600013,
Author = {Gomez-Inclan, Sofia and Duran-Arenas, Luis},
Title = {{Teenagers' access to contraception in Mexico City}},
Journal = {{SALUD PUBLICA DE MEXICO}},
Year = {{2017}},
Volume = {{59}},
Number = {{3}},
Pages = {{236-247}},
Month = {{MAY-JUN}},
Abstract = {{Objective. To study and understand the phenomenon of access to
   contraceptive methods in Mexican teenages, through the use of the
   Levesque model, which allows for the observation of both the system and
   the system and the user's participation in the access process. Materials
   and methods. A qualitative study was conducted with focus groups
   technique in a middle and high school of Mexico City. Results. The
   perception of ability to access to health care is limited, teenagers do
   not know the mechanisms of care or supply of contraceptive methods.
   Prejudices of service providers provoke a negative reaction.The family
   is a source of information for adolescents to make decisions.
   Conclusions.The model allowed the assessment of access to contraceptive
   methods in teenagers. It were identified different aspects that act as
   barriers to access and may inform health care providers about this
   population in their sexual and reproductive health.}},
