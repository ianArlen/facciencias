Author = {Espinosa-Paredes, Gilberto and Cazares-Ramirez, Ricardo-I. and Francois,
   Juan-Luis and Martin-del-Campo, Cecilia},
Title = {{On the stability of fractional neutron point kinetics (FNPK)}},
Journal = {{APPLIED MATHEMATICAL MODELLING}},
