@article{ ISI:000400533800009,
Author = {Nimrod Ramirez-Jarquin, Uri and Tapia, Ricardo},
Title = {{Chronic GABAergic blockade in the spinal cord in vivo induces motor
   alterations and neurodegeneration}},
