@article{ ISI:000405000200009,
Author = {Lopez, Norma and Candelaria, Carlos and Ramirez-Garcia, Pedro and
   Rodriguez, Deni},
Title = {{Structure and temporal dynamic of tropical turf-forming macroalgal
   assemblages of the western coast of Mexico}},
