Author = {Lopez-Conde, Oliver A. and Sterli, Juliana and Alvarado-Ortega, Jesus
   and Chavarria-Arellano, Maria L.},
Title = {{A NEW PLATYCHELYID TURTLE (PAN- PLEURODIRA) FROM THE LATE JURASSIC
   (KIMMERIDGIAN) OF OAXACA, MEXICO}},
Journal = {{PAPERS IN PALAEONTOLOGY}},
