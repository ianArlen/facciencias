@article{ ISI:000402690800003,
Author = {Ovalle-Encinia, Oscar and Arturo Mendoza-Nieto, J. and Ortiz-Landeros,
   Jose and Pfeiffer, Heriberto},
Title = {{Ce0.8Sm0.15Sr0.05O2 as Possible Oxidation Catalyst and Assessment of the
   CaO Addition in the Coupled CO Oxidation-CO2 Capture Process}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
Year = {{2017}},
