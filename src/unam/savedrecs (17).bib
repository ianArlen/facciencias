@article{ ISI:000397861700036,
Author = {Arzano, Michele and Nettel, Francisco},
Title = {{UV dimensional reduction to two from group valued momenta}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2017}},
Volume = {{767}},
Pages = {{236-241}},
Month = {{APR 10}},
Abstract = {{We describe a new model of deformed relativistic kinematics based on the
   group manifold U(1) xSU(2) as a four-momentum space. We discuss the
   action of the Lorentz group on such space andillustrate the deformed
   composition law for the group-valued momenta. Due to the geometric
   structure of the group, the deformed kinematics is governed by twoenergy
   scales lambda and kappa. A relevant feature of the model is that it
   exhibits a running spectral dimension dswith the characteristic short
   distance reduction to d(s)=2 found in most quantum gravity scenarios.
