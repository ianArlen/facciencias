@article{ ISI:000408407900008,
Author = {Coates, Rosamond and Ramirez-Lucho, Ismari and Gonzalez-Christen, Alvar},
Title = {{An updated species list of the bats of the Los Tuxtlas region, Veracruz}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2017}},
Volume = {{88}},
Number = {{2}},
Pages = {{349-357}},
Month = {{JUN}},
Abstract = {{Bats are a key component in the dynamics of terrestrial ecosystems and
   reach high levels of abundance and diversity in the Neotropics.
   Environmental transformations modulate alpha, beta and gamma
   biodiversity of a region, where some species are favored while others
   are not, reducing or expanding their regional presence. Also, the
   constant development of new systematic information requires necessary
   updates of the nomenclature in faunal lists. Updated information is
   basic for programs of environmental management and conservation. Here,
   we present an updated list of bats of the Los Tuxtlas Biosphere Reserve.
   The list is based on captures in the field, the revision of specimens
   deposited in collections, as well as the revision of online databases of
   national and international scientific collections. In this manner, we
   confirm the presence of 65 species of bats and discard 8 species still
   not confirmed in the region, but have been mentioned in various sources.
   (C) 2017 Universidad Nacional Autonoma de Mexico, Instituto de Biologia.}},
