@article{ ISI:000401390900005,
Author = {Velasco-Velazquez, M. A. and Salinas-Jazmin, N. and Hisaki-Itaya, E. and
   Cobos-Puc, L. and Xolalpa, W. and Gonzalez, G. and Tenorio-Calvo, A. and
   Pina-Lara, N. and Juarez-Bayardo, L. C. and Flores-Ortiz, L. F. and
   Medina-Rivero, E. and Perez, N. O. and Perez-Tapia, S. M.},
Title = {{Extensive preclinical evaluation of an infliximab biosimilar candidate}},
Journal = {{EUROPEAN JOURNAL OF PHARMACEUTICAL SCIENCES}},
Year = {{2017}},
Volume = {{102}},
Pages = {{35-45}},
Month = {{MAY 1}},
Abstract = {{Infliximab is therapeutic monoclonal antibody (mAb) against TNF-alpha
   employed in the treatment of immunoinflammatory diseases. The
   development of biosimilar mAbs is a global strategy to increase drug
   accessibility and reduce therapy-associated costs. Herein we compared
   key physicochemical characteristics and biological activities produced
   by infliximab and infliximab-Probiomed in order to identify functionally
   relevant differences between the mAbs. Binding of infliximab-Probiomed
   to TNF-alpha was specific and had kinetics comparable to that of the
   reference product. Both mAbs had highly similar neutralizing efficacy in
   HUVEC cell cultures stimulated with TNF-alpha. In vitro induction of CDC
   and ADCC were also similar between the evaluated products. In vivo
   comparability was assessed using a transgenic mouse model of arthritis
   that expresses human TNF-alpha in a 13 week multiple-administration
   study. Infliximab and infliximab-Probiomed showed comparable efficacy,
   safety, and pharmacokinetic profiles. Our results indicate that
   infliximab-Probiomed has highly similar activities to infliximab in
   preclinical models, warranting a clinical evaluation of its
   biosimilarity. (C) 2017 Elsevier B.V. All rights reserved.}},
