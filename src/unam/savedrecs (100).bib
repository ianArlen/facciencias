@inproceedings{ ISI:000256528600072,
Author = {Clark, David M. and Eikenberry, Stephen S. and Raines, Steven N.},
Editor = {{Bandyopadhyay, RM and Wachter, S and Gelino, D and Gelino, CR}},
Title = {{The environment of X-ray binaries in the dwarf starburst galaxy NGC 1569}},
Booktitle = {{POPULATION EXPLOSION: THE NATURE AND EVOLUTION OF X-RAY BINARIES IN
   DIVERSE ENVIRONMENTS}},
Series = {{AIP Conference Proceedings}},
Year = {{2008}},
Volume = {{1010}},
Pages = {{339+}},
Note = {{Conference on Nature and Evolution of X-Ray Binaries in Diverse
   Environments, St Pete Beach, FL, OCT 28-NOV 02, 2007}},
Organization = {{Natl Sci Fdn; Spitzer Sci Ctr; Univ Florida, Off Res; Univ Florida, Dept
   Astronomy}},
Abstract = {{We use deep, J and K-s observations of NGC 1569 acquired with FLAMINGOS
   on the KPNO 4-m to search for star cluster counterparts to X-ray
   binaries identified in archived Chandra images of this dwarf starburst
   galaxy. Performing near-IR photometry on the star cluster counterparts,
   we determine their colors, luminosities and masses. Comparing these
   results to the properties for all clusters in this galaxy, we search for
   trends in clusters associated with X-ray sources. Combining this study
   with FISICA, near-IR spectral observations, we further characterize the
   surroundings to X-ray binaries in NGC 1569. Contrasting this work with
   findings from a similar study performed on the Antennae galaxies, a
   large, merging system, we investigate the differences in X-ray binary
   environments.}},
