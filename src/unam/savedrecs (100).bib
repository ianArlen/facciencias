@article{ ISI:000259987500005,
Author = {Ramirez-Rojas, A. and Flores-Marquez, E. L. and Guzman-Vargas, L. and
   Galvez-Coyt, G. and Telesca, L. and Angulo-Brown, F.},
Title = {{Statistical features of seismoelectric signals prior to M7.4
   Guerrero-Oaxaca earthquake (Mexico)}},
Journal = {{NATURAL HAZARDS AND EARTH SYSTEM SCIENCES}},
Year = {{2008}},
Volume = {{8}},
Number = {{5}},
Pages = {{1001-1007}},
Abstract = {{In this paper a statistical analysis of seismoelectric ULF signals prior
   to the M7.4 Guerrero-Oaxaca earthquake (EQ) occurred at the Mexican
   Pacific coast on 14 September 1995, has been performed. The signals were
   simultaneously recorded at three monitoring stations from the last
   months of 1994 until July of 1996. The nonlinear time series methods of
   Higuchi fractal dimension (HFD) and detrended fluctuation analysis (DFA)
   have been used. In the three data sets we found some complex dynamical
   behaviors that seemingly reflect a relaxation-EQ preparation-main
   shock-relaxation process. In particular, clear spike-like anomalies in
   both DFA- and HFD-exponents some months before the main shock are
   revealed.}},
