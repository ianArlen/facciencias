@inproceedings{ ISI:000254895200001,
Author = {Alvarez-Buylla, Elena R. and Benitez, Mariana and Chaos, Alvaro and
   Corts, Yuriria and Escalera-Santos, Gerardo and Espinosa, Carlos and
   Padilla, Pablo},
Editor = {{Alama, S and Bronsard, L and Sternberg, PJ}},
Title = {{Variational problems arising in biology}},
Booktitle = {{SINGULARITIES IN PDE AND THE CALCULUS OF VARIATIONS}},
Series = {{CRM PROCEEDINGS \& LECTURE NOTES}},
Year = {{2008}},
Volume = {{44}},
Pages = {{1-10}},
Note = {{Workshop on Singularities in PDE and the Calculus of Variations, Univ
   Montreal, Montreal, CANADA, JUL 17-21, 2006}},
Abstract = {{The problem of understanding the emergence of biological structures and
   their changes is very complex and involves several temporal and spatial
   scales. At the biochemical level, it is grounded, to a great extent, on
   the interaction of the genes responsible for the differentiation of
   individual cells. These genetic regulatory networks are in turn coupled,
   at a larger scale, with external factors which include intercellular
   signaling, the diffusion of hormones, mechanical effects, etc. In this
   paper we propose a systematic way of deriving equations starting from
   experimental information on the architecture of the underlying
   regulatory network. We first construct an associated discrete dynamical
   system whose equilibrium points can be identified with cell types. We
   also analyze the behavior of the system when it is subject to random
   perturbations and relate it to the observed temporal sequence of cell
   differentiation. Based on this system, we can obtain a set of partial
   differential equations of Turing type. In this context, the study of
   pattern formation becomes a mathematical question: the formation of
   coherent structures and singularities and whether some variational or
   optimization principle can explain the way in which they interact and
   their overall organization. As a specific example we consider the
   genetic network involved underlying cell fate determination during early
   stages of flower development in Arabidopsis thaliana.}},
Publisher = {{AMER MATHEMATICAL SOC}},
Address = {{P.O. BOX 6248, PROVIDENCE, RI 02940 USA}},
