@article{ ISI:000253016000009,
Author = {Shustin, Eugenii and Fridman, Leonid and Fridman, Emilia and Castanos,
   Fernando},
Title = {{Robust semiglobal stabilization of the second order system by relay
   feedback with an uncertain variable time delay}},
Journal = {{SIAM JOURNAL ON CONTROL AND OPTIMIZATION}},
Year = {{2008}},
Volume = {{47}},
Number = {{1}},
Pages = {{196-217}},
Abstract = {{We present sufficient conditions for robust relay-delayed semiglobal
   stabilization of second order systems, which relate the upper bound to
   an uncertain time delay and the parameters of the plant. We also suggest
   an algorithm of delayed relay control gain adaptation for semiglobal
   stabilization, which is based on delayed information about the sign of
   the controlled variable only. The proposed algorithm suppresses bounded
   uncertainties in the time delay; that is, being designed for the upper
   bound of uncertainty in the time delay, the control law ensures
   semiglobal stabilization independently of any variable time delay
   obeying the given upper bound.}},
