@article{ ISI:000251030500009,
Author = {Uriostegui de la Sancha, Citlall and Martinez-Cadena, Guadalupe and
   Lopez-Godinez, Juana and Castellano, Laura E. and Nishisaki, Takuya and
   Darszon, Alberto and Garcia-Soto, Jesus},
Title = {{Rho-kinase (ROCK) in sea urchin sperm: Its role in regulating the
   intracellular pH during the acrosome reaction}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{2007}},
Volume = {{364}},
Number = {{3}},
Pages = {{470-475}},
Month = {{DEC 21}},
Abstract = {{Sperm must undergo the acrosome reaction (AR) in order to fertilize the
   egg. In sea urchins, this reaction is triggered by the egg jelly (EJ)
   which, upon binding to its sperm receptor, induces increases in the ion
   permeability of the plasma membrane and changes in protein
   phosphorylation. Here, we demonstrated that the sperm expresses ROCK
   (similar to 135 kDa), which is a serine/threonine protein kinase. ROCK
   localized, as RhoGTPase (Rho), in the acrosomal region, midpiece and
   flagellum. H-1152, a ROCK antagonist, inhibited the two cellular
   processes defining the AR: the acrosomal exocytosis and the actin
   polymerization. The ionophores nigericin and A23187 reversed the AR
   inhibition induced by H-1152, suggesting that ROCK. functions at the
   level of the EJ-induced ion fluxes. Accordingly, H-1152 blocked 70\% the
   intracellular alkalinization induced by EJ. These results indicate that
   EJ activates a Na+ H+ exchanger (NHE) in the sperm through a
   Rho/ROCK-dependent signaling pathway that culminates in the AR. (C) 2007
   Elsevier Inc. All rights reserved.}},
