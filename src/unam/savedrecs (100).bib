@article{ ISI:000251510300028,
Author = {Sandoval, Alberto and Gomez-Cortes, Antonio and Zanella, Rodolfo and
   Diaz, Gabriela and Saniger, Jose M.},
Title = {{Gold nanoparticles: Support effects for the WGS reaction}},
