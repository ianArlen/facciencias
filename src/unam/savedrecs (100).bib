Title = {{Viscoelasticity, conformational transition and ultrastructure of
   kappa-carrageenan in the presence of potassium ion around the critical
   total ion concentration}},
Booktitle = {{XVTH INTERNATIONAL CONGRESS ON RHEOLOGY - THE SOCIETY OF RHEOLOGY 80TH
   ANNUAL MEETING, PTS 1 AND 2}},
