@inproceedings{ ISI:000257879500019,
Author = {Mondragon, A. and Mondragon, M. and Peinado, E.},
Editor = {{CastillaValdez, H and Miranda, O and Santos, E}},
Title = {{Nearly tri-bimaximal mixing in the S-3 flavor symmetry}},
Booktitle = {{PARTICLES AND FIELDS: XI MEXICAN WORKSHOP ON PARTICLES AND FIELDS}},
Series = {{AIP CONFERENCE PROCEEDINGS}},
Year = {{2008}},
Volume = {{1026}},
Pages = {{164-169}},
