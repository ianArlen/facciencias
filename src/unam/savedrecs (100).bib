@inproceedings{ ISI:000267796500004,
Author = {Esteva, Luis},
Editor = {{Biondini, F and Frangopol, DM}},
Title = {{Life cycle optimization in earthquake engineering}},
Booktitle = {{LIFE-CYCLE CIVIL ENGINEERING}},
Year = {{2008}},
Pages = {{35+}},
Note = {{1st International Symposium on Life-Cycle Civil Engineering, Varenna,
   ITALY, JUN 10-14, 2008}},
Organization = {{Int Assoc Life Cycle Civil Engn; POLIMI; Amer Concrete Inst; Italian
   Assoc Reinforced \& Prestressed Concrete; Italian Assoc Offshore \&
   Marine Engn; ATLSS; Italian Soc Bldg Engineers; Int Assoc Bridge
   Maintenance \& Safety; Struct Engn Inst, Amer Soc Civil Engineers; Spea
   Ingegneria Europea; TY Lin Int; UPC}},
Abstract = {{An overview is presented of life-cycle optimization in the establishment
   of reliability- and performance-based seismic design requirements for
   multistory systems. Alternative approaches are presented for the
   development of seismic vulnerability functions that do not require the
   determination of lateral deformation capacities. The influence of damage
   accumulation on the evolution of the seismic reliability functions is
   discussed, and some results are presented about the sensitivity of the
   seismic reliability and performance functions to the contribution of
   energy-dissipating devices to the lateral strength and stiffness of
   multistory frames. The process of structural damage accumulation
   resulting from the action of sequences of seismic excitations is taken
   into account in the assessment of life-cycle system reliability and
   performance, and in the formulation of reliability and optimization
   criteria and methods for the establishment of structural design
   requirements and for the adoption of repair and maintenance strategies.
   Problems related to the transformation of research results into
   practically applicable seismic design criteria are briefly discussed.
   Several illustrative examples are presented.}},
