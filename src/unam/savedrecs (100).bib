@article{ ISI:000256799000008,
Author = {Martinez-Sosa, D. and Rafrafi, Y. and Torrijos, M. and Bernet, N. and
   Buitron, G. and Bosque, F. and Delgenes, J. P.},
Title = {{Anaerobic digestion of gelatinous water at laboratory and pilot scale
   and nitrogen inhibition}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{2008}},
Volume = {{57}},
Number = {{11}},
Pages = {{1735-1741}},
Abstract = {{The anaerobic digestion of the liquid residue (gelatinous water) coming
   from the production of fat from animal residue, was studied at
   laboratory and pilot scale. Biodegradability (>98\%) and biogas
   potential (675mL of biogas/g of CODapplied) of this wastewater are very
   high. However, due to the high content on nitrogen, an inhibition of the
   anaerobic activity was observed for quite low concentrations of N-NH3.
   Dilution of the wastewater and pH regulation in the reactor around 7.3
   are the 2 solutions which were investigated to overcome the nitrogen
   inhibition at industrial scale. These two solutions were validated at
   laboratory scale in an anaerobic SBR and then onsite at pilot scale in a
   continuous reactor. A stable anaerobic digestion was observed in both
   reactors showing that no nitrogen inhibition was obtained when N-NH3
   concentration in the reactor was kept low.}},
