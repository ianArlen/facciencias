@article{ ISI:000259036500020,
Author = {Alvarez-Bajo, O. and Arias, J. M. and Gomez-Camacho, J. and Lemus, R.},
Title = {{An approach to the study of the continuum effects in systems of
   interacting Morse oscillators}},
Journal = {{MOLECULAR PHYSICS}},
Year = {{2008}},
Volume = {{106}},
Number = {{9-10, SI}},
Pages = {{1275-1289}},
Note = {{20th Colloquium on High Resolution Molecular Spectroscopy, Dijon,
   FRANCE, SEP 03-07, 2007}},
Abstract = {{An algebraic approach to the study of the continuum effects described by
   interacting Morse oscillators is presented. In particular, two
   interacting Morse oscillators are studied using the direct product of a
   complete basis for each Morse oscillator. We start by considering the
   Hamiltonian for two Morse oscillators, taking interactions up to
   quadratic order in both the momenta and the coordinates. The
   diagonalization is first carried out in the basis of the direct product
   of bound Morse functions for each oscillator, and thereafter using a
   discrete complete basis. This allows the continuum effect to be taken
   into account through a discretization of the continuum space. Energies,
   wave function characteristics, probability densities and transition
   intensities are studied as the dimension of the basis is increased.}},
