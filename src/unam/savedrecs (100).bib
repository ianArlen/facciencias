@incollection{ ISI:000274636600002,
Author = {Lazcano, Antonio},
Editor = {{Botta, O and Bada, JL and GomezElvira, J and Javaux, E and Selsis, F and Summons, R}},
Title = {{Towards a Definition of Life: The Impossible Quest? Introduction}},
Booktitle = {{STRATEGIES OF LIFE DETECTION}},
Series = {{Space Science Series of ISSI}},
Year = {{2008}},
Volume = {{25}},
Pages = {{5-10}},
Abstract = {{``Life{''} is an empirical concept whose various definitions and
   phenomenological characterizations depend on historical frameworks.
   Although analysis of existing literature suggests that attempts to
   define life will remain, at best, a work in progress, the history of
   biology shows that some efforts have been more fruitful than others.
   There is a major distinction between natural selection-which is clearly
   a defining trait of biology-and the changes that result from purely
   physical chemical evolution, which can be observed in non-biological
   complex systems. Accordingly, it can be concluded that life cannot be
   understood without considering the presence of genetic material and
   Darwinian evolution. This shows the usefulness of the suggestion that
   life can be considered as a self-sustaining chemical system (i.e., one
   that turns environmental resources into its own building blocks) that is
   capable of undergoing natural selection.}},
