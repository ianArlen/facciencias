@inproceedings{ ISI:000257091000004,
Author = {Mendoza, S. and Huerta, E. A.},
Editor = {{Benitez, E and CruzGonzalez, I and Krongold, Y}},
Title = {{Accretion model of a rotating gas sphere onto a Schwarzschild black hole}},
Booktitle = {{NUCLEAR REGION, HOST GALAXY AND ENVIRONMENT OF ACTIVE GALAXIES}},
