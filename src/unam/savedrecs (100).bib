@article{ ISI:000269823200013,
Author = {Alfonso, I and Maldonado, C. and Gonzalez, J. G. and Medina, A.},
Title = {{Effect of the experimental variables on the Al-6Si-3Cu-xMg T6
   microhardness alloy}},
Journal = {{MATERIA-RIO DE JANEIRO}},
