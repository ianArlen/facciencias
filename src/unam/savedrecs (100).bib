@article{ ISI:000259918500005,
Author = {Loeser, Hannes},
Title = {{A new solitary coral genus of the suborder Heterocoeniina (Scleractinia)
   from the Aptian (Cretaceous) of Spain}},
Journal = {{PALAEONTOLOGISCHE ZEITSCHRIFT}},
Year = {{2008}},
Volume = {{82}},
Number = {{3}},
Pages = {{279-284}},
Abstract = {{The new scleractinian coral genus Hexasmiliopsis is described on the
   basis of material from the Early Aptian (Early Cretaceous) of Murcia
   (Spain). The new genus of the Heterocoeniidae family is characterised by
   its solitary growth form, a very strong main septum and the presence of
   apophysal septa. It is closely related to the genera Hexasmilia
   (phaceloid growth form), Rodinosmilia and Tiarasmilia (both without main
   septum). The genus is monospecific and represents only the type species,
   Hexasmiliopsis saldanai.}},
