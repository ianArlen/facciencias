@inproceedings{ ISI:000300845500020,
Author = {Maria, Moreno-Armenta G. and Armando, Reyes-Serrato and Gerardo, Soto H.},
Editor = {{Kniep, R}},
Title = {{The Effect of Composition on the Properties of Semiconducting Transition
   Metal Nitrides}},
