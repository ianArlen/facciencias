@inproceedings{ ISI:000261333300022,
Author = {Rochette, Martin and Sun, Kai and Hernandez-Cordero, Juan and Chen,
   Lawrence R.},
Editor = {{Vallee, R and Piche, M}},
Title = {{Multiwavelength self-pulsating fibre laser based on cascaded SPM
   spectral broadening and filtering}},
Booktitle = {{PHOTONICS NORTH 2008}},
Series = {{Proceedings of SPIE}},
Year = {{2008}},
Volume = {{7099}},
Note = {{International Conference on Application of Photonics Technology
   (Photonics North 2008), Montreal, CANADA, JUN 02-04, 2008}},
Organization = {{Quebec Photon Network; Inst Natl Opt; Canadian Photon Consort; Ontario
   Photon Ind Network; LaserFocusWorld; Photon Spectra; Dev Econ Innovat
   Exportat Quebec; Dev Econ Canada; Univ Laval, Ctr Opt Photon Laser;
   Ecole Polytech Montreal}},
Abstract = {{We experimentally demonstrate the operation of a laser based on
   self-phase modulation followed by offset spectral filtering. This laser
   has three operation modes: a continuous-wave mode, a self-pulsating mode
   where the laser self ignites and produces pulses, and a pulse-buffering
   mode where no new pulse is formed from spontaneous emission noise but
   only pulses already propagating or pulses injected in the laser cavity
   can be sustained. In the self-pulsating and pulse-buffering modes, the
   laser is multi-wavelength and continuously tunable over the entire gain
   band of the amplifiers. The output pulse width is quasi
   transform-limited with respect to the spectral-width of the filters used
   in the cavity. Overall, this device provides a simple alternative to
   pulsed laser source and also represents a promising approach for signal
   buffering.}},
