@article{ ISI:000261932900007,
Author = {Rojas-Hernandez, Alberto and Cesar Botello, Julio and de Lourdes
   Pacheco-Hernandez, Ma and Gutierrez, Atilano and Manuel Dominguez, Jose
   and Espinosa, Gabriela and Teresa Ramirez-Silva, Maria},
Title = {{Equilibrium Constants Determination of the Species Formation in the
   Al(III)-H2O System by Integration of Al-2-NMR Signals and Fitting with
   Species Fractions}},
Journal = {{JOURNAL OF THE MEXICAN CHEMICAL SOCIETY}},
Year = {{2008}},
Volume = {{52}},
Number = {{1}},
Pages = {{47-53}},
Month = {{JAN-MAR}},
Abstract = {{A quantitative methodology to determine formation constants that takes
   into consideration the Al-27-NMR signals of Al(III)H2O systems has been
   analyzed, where area fractions beneath the peaks recorded were
   associated with the corresponding species fractions. The values of the
   formation constants were compared with the model obtained by
   potentiometric studies. In agreement with previous reports, the
   -log{*}beta values obtained by NMR and potentiometry, at 0.020 M <=
   {[}Al(III)](total) <= 0.500 M are 12.96 +/- 0.02 for Al(OH)(3), 13.50
   +/- 0.02 for Al-3(OH)(4)(5+), and 109.70 +/- 0.09 for Al-13(OH)(32)(7+),
   by assuming that Al(OH)(2+) has a value of 5.29. Additionally the
   equilibrium constant values obtained allowed fitting of the
   conductimetry titrations in the 6.00(10-(3)) M <= {[}Al(III)](total) <=
   0.120 M. The algorithm presented in this work gives a support to
   establish that the area fractions of NMR signals are proportional to the
   species concentration fractions.}},
