@article{ ISI:A1991GR95500008,
Author = {CASTILLO, RRD},
Title = {{EMBEDDED EIGENVALUES OF STURM LIOUVILLE OPERATORS}},
Journal = {{COMMUNICATIONS IN MATHEMATICAL PHYSICS}},
Year = {{1991}},
Volume = {{142}},
Number = {{2}},
Pages = {{421-431}},
Abstract = {{In this work we study the behavior of embedded eigenvalues of
   Sturm-Liouville problems in the half axis under local perturbations. 
   When the derivative of the spectral function is strictly positive, we
   prove that the embedded eigenvalues either disappear or remain fixed. 
   In this case we show that local perturbations cannot add eigenvalues in
   the continuous spectrum.  If the condition on the spectral function is
   removed then a local perturbation can add infinitely many eigenvalues.}},
