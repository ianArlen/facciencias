@article{ ISI:A1990EP90100006,
Author = {LARAOCHOA, F and BUSTOS, VP},
Title = {{DENSITY DEPENDENT DIFFUSIONAL MODEL OF AN INFECTIVE POPULATION}},
Journal = {{BIOSYSTEMS}},
Year = {{1990}},
Volume = {{24}},
Number = {{3}},
Pages = {{223-238}},
Abstract = {{In this work, we present a density-dependent diffusional model which,
   coupled to three different types of growth, permitted us to study the
   infective potential of a bacteria species.  The results show that those
   species with strong internal competency have the higher colonizing
   capacity in terms of invasion speed.  Here, we also advanced a model for
   the static spatial inhomogeneous distribution that some species
   establish after migration.  It is proposed that the origin of these
   patterns is the result of a balance between the dispersal tendency and
   the attractive behavior.  The results obtained were compared with the
   observed behavior of Rhizobium spp. during infection of leguminous
   roots.  A possible explanation of the observed morphologies of nodule
   development in different legumes is suggested.}},
