@article{ ISI:A1991FA99700001,
Author = {TORRESDODGEN, AV and TAPIA, M and CARROLL, M},
Title = {{UVBY AND JHKL' PHOTOMETRY OF OB STARS IN THE ASSOCIATION CYGNUS OB2}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{1991}},
Volume = {{249}},
Number = {{1}},
Pages = {{1-12}},
Month = {{MAR 1}},
Abstract = {{We have obtained uvby and JHK photometry of about 80 stars in the
   association Cygnus OB2 (VI Cygni), including some foreground stars and
   three infrared sources.  We present colour-colour and colour-magnitude
   diagrams from which we derive the interstellar extinction and
   approximate spectral types.  We confirm that the interstellar extinction
   law in the direction of Cyg OB2 is similar to the galactic mean.  The
   ratio of total-to-selective extinction is found to be R(v) = 3.04 +/-
   0.09.  We derive a true distance modulus of V(o) - M(v) = 11.2 +/- 0.2,
   corresponding to d = 1.7 +/- 0.2 kpc.  The majority of the stars are
   still on the main sequence; we estimate a minimum age of 3 Myr for the
   association.  Only two stars in our sample show significant infrared
   excesses at lambda greater-than-or-equal-to 2.2-mu-m.}},
