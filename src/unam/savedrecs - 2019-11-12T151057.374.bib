@article{ ISI:A1991GC35000019,
Author = {MENDEZMORENO, RM and MORENO, M and ORTIZ, MA},
Title = {{STABLE CORRUGATED STATE OF THE 2-DIMENSIONAL ELECTRON-GAS}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{1991}},
Volume = {{44}},
Number = {{4}},
Pages = {{2370-2373}},
Month = {{AUG 15}},
Abstract = {{A corrugated and stable ground state is found for the two-dimensional
   electron gas in both the normal paramagnetic and the fully polarized
   phases.  The self-consistent Hartree-Fock method is used with a
   modulated set of trial wave functions within the deformable jellium
   model.  The results for high metallic densities reproduce the usual
   noncorrugated ferromagnetic electron-gas behavior.  A transition to a
   paramagnetic corrugated state for values of r(s) almost-equal-to 6.5 is
   predicted.  At lower densities r(s) almost-equal-to 30, a second
   transition to a corrugated ferromagnetic phase is suggested.}},
