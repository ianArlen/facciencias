@article{ ISI:A1991FL92500001,
Author = {CARDOSO, J and MANERO, O},
Title = {{THERMAL AND DILUTE-SOLUTION PROPERTIES OF ZWITTERIONIC COPOLYMERS}},
Journal = {{JOURNAL OF POLYMER SCIENCE PART B-POLYMER PHYSICS}},
Year = {{1991}},
Volume = {{29}},
Number = {{6}},
Pages = {{639-647}},
Month = {{MAY}},
Abstract = {{We report the synthesis, characterization, thermal and dilute-solution
   properties of zwitterionic copolymers of the sulfobetaine type, where
   the proportion of ionic groups is varied over a wide range.  Two
   different structures are considered:  poly(2-vinyl pyridine sulfopropyl
   betaine) and poly(4-vinyl pyridine sulfopropyl betaine).  Synthesis is
   carried out to obtain random copolymers which are further characterized
   and their ionic content determined.  At low proportions of ionic groups,
   the behavior is similar to that observed in ionomers, with substantial
   differences between the two structures.  Increased interaction among the
   zwitterionic groups at higher charge densities is evidenced, which leads
   to the formation of clusters and agglomerates.  An increase in the
   intrinsic viscosity with salt content in aqueous solutions is also
   observed.  This behavior is analogous to that of homopolymers with 100\%
   ionic content, but with particular features of interest.}},
