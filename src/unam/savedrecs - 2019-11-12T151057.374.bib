@article{ ISI:A1991GC96800004,
Author = {WOLF, KB},
Title = {{REFRACTING SURFACES BETWEEN FIBERS}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA A-OPTICS IMAGE SCIENCE AND
   VISION}},
Year = {{1991}},
Volume = {{8}},
Number = {{9}},
Pages = {{1389-1398}},
Month = {{SEP}},
Abstract = {{I characterize the transformation of optical phase space that is due to
   aligned elliptic index-profile fibers and axially symmetric refracting
   interfaces between two such media.  These include the homogeneous medium
   as a particular case.  I treat the phenomenon in phase space by working
   with the transformation that is the root of refraction.  This is exact
   in 4-pi optics and solved by an implicit equation.  By means of its
   polynomial approximation I obtain the seventh-order Lie-Seidel
   aberration coefficients.  The group composition of such transformations
   permits the symbolic computation of graded-index lens systems.  We apply
   this in a preliminary analysis of Fourier transformers built out of a
   fiber with a polynomial interface to a homogeneous medium.}},
