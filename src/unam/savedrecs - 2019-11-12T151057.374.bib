@article{ ISI:A1991GF55000002,
Author = {COTAARAIZA, L and GALVAN, DH and FARIAS, MH},
Title = {{ELECTRON-BEAM EFFECTS ON BI-SR-CA-CU-O SUPERCONDUCTORS}},
Journal = {{JOURNAL OF MATERIALS RESEARCH}},
Year = {{1991}},
Volume = {{6}},
Number = {{9}},
Pages = {{1820-1822}},
Month = {{SEP}},
Abstract = {{We present measurements on the superconductor compound Bi-Sr-Ca-Cu-O,
   obtained using a scanning Auger microscope (SAM), which indicate that
   this material is extremely unstable under an electron beam with energy
   of several KeV.  Using relatively high current densities for the SAM, it
   is possible to melt small regions of the material and to form spheres
   several micrometers in diameter in a few seconds.  We characterize the
   changes in the elemental concentration of the surface of these spheres
   as a function of time after the beam exposure.}},
