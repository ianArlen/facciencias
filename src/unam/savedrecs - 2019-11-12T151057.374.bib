@article{ ISI:A1991FC24600020,
Author = {DELARA, E and CHAVARRIA, C and LOPEZMOLINA, G},
Title = {{DISTANCE TO THE SERPENS CLOUD .2.}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{1991}},
Volume = {{243}},
Number = {{1}},
Pages = {{139-142}},
Month = {{MAR}},
Abstract = {{We report near infrared photometry of stars with reflection nebulosity
   (R-stars) and associated with the Serpens dark cloud. A reinspection of
   earlier slit spectrograms of four bright R-stars confirms the spectral
   types and luminosity classes given by Chavarria-K et al. (1988,
   hereafter Paper I). The earlier published uvby-beta and IR photometry of
   the R-stars, together with our new IR data sets the distance to the
   Serpens cloud at D = 311 +/- 38 pc, with mean visual extinction ratio to
   the region of R = 3.3 +/- 0.3.
   We have also obtained near IR data of four stars with the H-alpha line
   probably in emission. Finally, in an attempt to detect the star
   responsible for the red nebulosity in the vicinity of star 17 of Paper
   I, we report ten IR sources and discuss their probable nature. Except
   for IRS 5, and probably IRS 1, all other sources have no intrinsic IR
   excesses and can be explained as field stars reddened by a normal
   interstellar extinction law.}},
