@article{ ISI:000208523800008,
Author = {List, Roland and Garcia-Garcia, F. and Kuhn, R. and Greenan, B.},
Title = {{The Supercooling of Surface Water Skins of Spherical and Spheroidal
   Hailstones}},
Journal = {{ATMOSPHERIC RESEARCH}},
Year = {{1989}},
Volume = {{24}},
Number = {{1-4, SI}},
Pages = {{83-87}},
Month = {{DEC}},
Abstract = {{List, R., Garcia-Garcia, F., Kuhn, R. and Greenan, B., 1989. The
   supercooling of surface water skins of spherical and spheroidal
   hailstones. Atmos. Res., 24: 83-87.
   Remote infrared measurements of equator and pole temperatures of
   rotating spherical and gyrating spheroidal hailstones during growth in
   an icing tunnel demonstrate that the surface temperatures are not
   isotropic and homogeneous as has always been assumed. The temperature
   differences are measured to be as large as 6 degrees C for spheres and 4
   degrees C for spheroids. Surface temperatures, which strongly depend
   upon air pressure, remain below 0 degrees C even in the presence of
   water skins. These skins generally form in the equatorial region. The
   normally dry poles are colder. Thus the heat and mass transfer varies
   over the surface and is substantially reduced due to the smaller than
   hitherto assumed temperature difference between hailstone surface and
   surrounding air. A more sophisticated theory is required to describe
   these new findings.}},
