@article{ ISI:A1991FQ94300042,
Author = {ROMAN, CE and GARZON, IL},
Title = {{EVAPORATION OF LENNARD-JONES CLUSTERS}},
Journal = {{ZEITSCHRIFT FUR PHYSIK D-ATOMS MOLECULES AND CLUSTERS}},
