@article{ ISI:A1991FQ89000031,
Author = {APATIGA, LM and ANDRADE, E and RICKARDS, J and BELMONT, E and ALBA, F},
Title = {{A NUCLEAR-REACTION STUDY OF NITROGEN DIFFUSION INTO TITANIUM AND ITS
   RELATION TO MICROHARDNESS}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{1991}},
Volume = {{56-7}},
Number = {{2}},
Pages = {{799-801}},
Month = {{MAY}},
Note = {{11TH INTERNATIONAL CONF ON THE APPLICATION OF ACCELERATORS IN RESEARCH
   AND INDUSTRY, DENTON, TX, NOV 05-08, 1990}},
Abstract = {{The reaction N-14(d, alpha)-C-12 was used to measure the diffusion of
   nitrogen into titanium, with the beam from a 5.5 MV Van de Graaff
   accelerator.  By bombarding with 2.6 MeV deuterons and observing the
   spectra at a laboratory angle of 170-degrees a fairly flat region in the
   excitation curve of the ground state alpha particles is found.  The
   shape of the energy spectrum is therefore indicative of the nitrogen
   profile, allowing a study of about 10-mu-m from the metal surface. 
   Titanium samples were heated in a nitrogen atmosphere for periods of 1,
   2, 3 and 4 h at a temperature of 800-degrees-C.  The amount of nitrogen
   diffused was then correlated to microhardness measurements.  These
   measurements were compared with determinations carried out on a
   commercial titanium nitride coated drill.}},
