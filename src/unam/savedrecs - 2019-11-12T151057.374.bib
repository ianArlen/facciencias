@article{ ISI:A1991FY05500015,
Author = {REGALADO, LE and MACHORRO, R and SIQUEIROS, JM},
Title = {{ATTENUATED-TOTAL-REFLECTION TECHNIQUE FOR THE DETERMINATION OF
   OPTICAL-CONSTANTS}},
Journal = {{APPLIED OPTICS}},
Year = {{1991}},
Volume = {{30}},
Number = {{22}},
Pages = {{3176-3180}},
Month = {{AUG 1}},
Abstract = {{In a glass-metal-dielectric system, it is normally impossible to
   determine simultaneously the complex dielectric constant, the thickness
   of the metal, and the corresponding parameters of a dielectric
   overlayer.  We propose the use of the pseudo-Brewster or Abeles angle as
   an additional parameter to characterize simultaneously a dielectric
   thin-film overcoating and the metal surface parameters.  We use a
   Kretschmann attenuated-total-reflection configuration.  An admittance
   diagram is used to illustrate graphically the role of an absentee layer
   at this angle.  A study of the limitations of the method is also
   presented.}},
