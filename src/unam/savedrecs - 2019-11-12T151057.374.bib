@article{ ISI:A1991GC96800005,
Author = {WOLF, KB},
Title = {{FOURIER-TRANSFORM IN METAXIAL GEOMETRIC OPTICS}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA A-OPTICS IMAGE SCIENCE AND
   VISION}},
Year = {{1991}},
Volume = {{8}},
Number = {{9}},
Pages = {{1399-1403}},
Month = {{SEP}},
Abstract = {{The phase space of rays in geometric optics has a position coordinate on
   the screen that is unbounded, while its canonically conjugate momentum
   coordinate is bound by the local refractive index of the medium.  The
   classical Fourier transform is a rotation of phase space by 1/2-pi; this
   is possibly only in the Heisenberg-Weyl plane R2N of classical mechanis
   or, locally, near the optical axis and center.  A point map of optical
   momentum, however, permits a definition of an optical Fourier transform
   that is global and canonical and matches the classical Fourier transform
   in the paraxial regime.}},
