@article{ ISI:A1991GB79700006,
Author = {RODRIGUEZ, RT and ROMO, AU and OLAYO, RG},
Title = {{OBSERVED STRUCTURAL-CHANGE IN POLY-N-BUTYL ISOCYANATE}},
Journal = {{JOURNAL OF MACROMOLECULAR SCIENCE-PHYSICS}},
Year = {{1991}},
Volume = {{B30}},
Number = {{1-2}},
Pages = {{75-85}},
Abstract = {{We report a conformational change observed in poly-n-butyl isocyanate
   (PBIC) dissolved in carbon tetrachloride (CCl4) with temperature. This
   change was detected by measuring the reduced viscosity of the polymer
   solution with a Zimm-Crothers type viscometer of very low shear rate; we
   found a reversible jump in reduced viscosity whose amplitude depends on
   concentration, but its position is only molecular weight dependent. This
   jump goes from lower to higher values of reduced viscosity when we
   increase the temperature, corresponding to an increase in the
   hydrodynamic volume of the chain. The largest size of the jump we found
   was about 3.5\% of the viscosity value, and the jump decreased when the
   concentration approached the overlap concentration c{*}.}},
