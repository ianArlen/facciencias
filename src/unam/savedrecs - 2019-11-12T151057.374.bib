@article{ ISI:A1991EU13100016,
Author = {NAIR, PK and NAIR, MTS},
Title = {{CHEMICALLY DEPOSITED SNS-CUXS THIN-FILMS WITH HIGH SOLAR ABSORPTANCE -
   NEW APPROACH TO ALL-GLASS TUBULAR SOLAR COLLECTORS}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{1991}},
Volume = {{24}},
Number = {{1}},
Pages = {{83-87}},
Month = {{JAN 14}},
Abstract = {{Chemical deposition of SnS-Cu(x)S films on glass substrates showing a
   solar absorptance > 0.9 for the substrate side incidence is reported.
   This combination is obtained by successive depositions of approximately
   0.8-mu-m thick SnS thin films and Cu(x)S thin films of approximately
   0.3-mu-m thickness. After 30 min heating in air at 200-degrees-C of the
   film, a typical SnS-Cu(x)S solar absorber coating shows sheet resistance
   congruent-to 10-OMEGA/open-square-box, solar transmittance congruent-to
   2\% and solar reflectance congruent-to 7\% for glass side incidence.
   Considering a double pass of sunlight through the film in a tubular
   geometry, this indicates a solar absorptance of congruent-to 0.93. The
   possible applications of the films in all-glass tubular collectors are
   discussed.}},
