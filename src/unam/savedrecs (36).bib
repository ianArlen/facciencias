@article{ ISI:000361272200001,
Author = {Carmona-Tellez, S. and Alarcon-Flores, G. and Zaleta-Alejandre, E. and
   Rivera-Alvarez, Z. and Meza-Rocha, A. N. and Martinez-Martinez, R. and
   Murrieta S, H. and Aguilar-Frutis, M. and Falcony, C.},
Title = {{Luminescent polystyrene films, a novel way to reduce styrofoam residues}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2015}},
Volume = {{61}},
Number = {{5}},
Pages = {{323-329}},
Month = {{SEP-OCT}},
Abstract = {{Polystyrene (PS) films doped with blue, yellow and red organic pigments
   have been deposited on glass and on commercial LED chips by spray
   pyrolysis using recycled PS as precursor, at deposition rates between
   100 and 200 angstrom/s. Doped films have roughness between 100 and 2500
   A depending on the kind of pigment used; the surface morphology shows
   that the organic pigments incorporate into the polystyrene host forming
   globular particles of about 2 pm in diameter. The organic pigments used
   have luminescent emissions peaked at 440, 509 and 590 nm. The
   introduction of the proper combination of pigments on the PS film
   deposited on the LED chips renders a white light emission with (0.29,
   0.37) and (0.30, 0.33) CIE (Commission international de l' eclairage)
   color coordinates. It was determined that the exposure to violet light
   degrades the luminescent characteristics, however, blue light is not
   energetic enough to cause a degradation of the luminescent layer over
   similar period of time.}},
