@article{ ISI:000366095700016,
Author = {Uribe Mendoza, Blanca Irais},
Title = {{The invention of animals: a history of Mexican veterinary medicine in
   the nineteenth century}},
Journal = {{HISTORIA CIENCIAS SAUDE-MANGUINHOS}},
Year = {{2015}},
Volume = {{22}},
Number = {{4}},
Month = {{OCT-DEC}},
Abstract = {{This article analyzes a phenomenon I call ``the invention of
   animals,{''} that is, the way in which veterinary medicine and the
   practices of physiology, microbiology and zootechnics produced new and
   different ways of thinking about, studying, understanding, regulating,
   legislating, commercializing, exploiting and experiencing ``the
   animal.{''} In particular, the text focuses on the evolution of this
   phenomenon in Mexico during the nineteenth century, since during this
   period the impact of these disciplines on animal bodies led to
   significant changes in the fields of human medicine, public health, and
   livestock production.}},
