@article{ ISI:000365402800008,
Author = {Martinez, Hugo Tapia and Silva, Marta Angelica Iossi and Cabrera, Iniga
   Perez and Mendoza, Araceli Jimenez},
Title = {{Obstetric profile of pregnant adolescents in a public hospital: risk at
   beginning of labor, at delivery, postpartum, and in puerperium}},
Journal = {{REVISTA LATINO-AMERICANA DE ENFERMAGEM}},
Year = {{2015}},
Volume = {{23}},
Number = {{5}},
Pages = {{829-836}},
Month = {{SEP-OCT}},
Abstract = {{Objective: describe the obstetric profile of adolescents at beginning of
   labor, at delivery, postpartum, and in puerperium. Method:
   Cross-sectional descriptive study with 85 pregnant adolescents, selected
   by convenience, referred by health centers to a public hospital in
   Mexico City. Risks were evaluated before, during and after delivery and
   in puerperium, and measured respectively with the ``Previgenes{''} that
   compose the Reproductive and Perinatal Risk Assessment System. Results:
   socioeconomic status, occupation and education level had influence on
   the emotionality of adolescents in relation to labor, whose obstetric
   risk was low for 55\%, medium for 35\%, and high for 10\%. Risk in labor
   was low for 55\%, medium for 18\%, and high for 27\%. Risk postpartum
   was low for 50\%, medium for 25\%, and high for 25\%. In puerperium,
   most adolescents (90\%) had low risk. Conclusion: most adolescents had
   low risk in the stages evaluated. The study contributed to identify
   strategies to approach risk considering the vulnerability inherent in
   this type of population and favored the conduct of appropriate
   interventions for the respective needs.}},
