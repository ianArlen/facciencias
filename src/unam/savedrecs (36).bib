@article{ ISI:000368210200002,
Author = {Gomez-Torrente, Mario},
Title = {{On the Essence and Identity of Numbers}},
Journal = {{THEORIA-REVISTA DE TEORIA HISTORIA Y FUNDAMENTOS DE LA CIENCIA}},
Year = {{2015}},
Volume = {{30}},
Number = {{3}},
Pages = {{317-329}},
Month = {{SEP}},
Abstract = {{Taking as premises some reasonable principles about the essences of
   natural numbers, pluralities and sets, the paper offers two types of
   argument for the conclusions that the natural numbers could not be the
   Zermelo numbers, the von Neumann numbers, the ``Kripke numbers{''}, or
   the positions in the omega-structure, among other things. These
   conclusions are thus Benacerrafian in form, but it is emphasized that
   the two kinds of argument offered in the paper are anti-Benacerrafian in
   substance, as they are perfectly compatible and in fact congenial with
   some views on which the numbers could be things of certain other kinds.}},
