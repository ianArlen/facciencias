@article{ ISI:000359769900009,
Author = {Rios, Citlalli and Ramos, Estrella and Perez-Manriquez, Liliana and
   Salcedo, Roberto},
Title = {{Cycloaddition of oestrogen-like molecules on fullerene: theoretical
   approach}},
Journal = {{MOLECULAR SIMULATION}},
Year = {{2015}},
Volume = {{41}},
Number = {{15}},
Pages = {{1292-1297}},
Month = {{OCT 13}},
Abstract = {{The {[}2+2] cycloaddition reaction between a double bond from a
   fullerene and an oestrogen or similar organic derivative has been
   studied in the scheme of the ab initio atom-centred density matrix
   propagation molecular dynamics model. The reaction was simulated
   considering vacuum, a little polar medium (butyric acid) and a strong
   polar medium (water). The results are completely different with the
   change of environments; some leading to an unidentified broken species
   without identification, whereas others produced a defined organic
   complex. The reaction stages have been analysed here and possible
   applications for medicine are achieved.}},
