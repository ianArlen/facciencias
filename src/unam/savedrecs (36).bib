@article{ ISI:000360981100014,
Author = {Quevedo, Hernando and Quevedo, Maria N. and Sanchez, Alberto},
Title = {{Thermodynamics and geometrothermodynamics of Born-Infeld black holes
   with cosmological constant}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS D}},
Year = {{2015}},
Volume = {{24}},
Number = {{11}},
Month = {{OCT}},
Abstract = {{In this paper, we investigate a class of spherically symmetric
   Born-Infeld black holes which contains the mass, electric charge,
   Born-Infeld parameter and the cosmological constant as physical
   parameters. We show that for the mass to be an extensive thermodynamic
   variable, it is necessary to consider the cosmological constant and the
   Born-Infeld parameter as thermodynamic variables as well. We analyze the
   properties of such a thermodynamic system, explore the range of values
   where the system is thermodynamically well-defined, and the phase
   transition structure. In addition, we show that the equilibrium manifold
   in the context of geometrothermodynamics reproduces correctly the
   thermodynamic properties of this black hole class.}},
