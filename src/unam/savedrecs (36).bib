@article{ ISI:000361578800001,
Author = {Kumar, Vikas and Srivastava, P. C. and Ermamatov, M. J. and Morales,
   Irving O.},
Title = {{Analysis of proton and neutron pair breakings: High-spin structures of
   Te124-127 isotopes}},
