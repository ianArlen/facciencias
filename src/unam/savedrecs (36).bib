@article{ ISI:000361841200010,
Author = {Juarez Jaimes, Veronica},
Title = {{Marsdenia stevensiana (Apocynaceae, Asclepiadoideae), a New Species from
   Chiapas, Mexico}},
Journal = {{NOVON}},
Year = {{2015}},
Volume = {{24}},
Number = {{2}},
Pages = {{170-172}},
Month = {{SEP}},
Abstract = {{A new species, Marsdenia stevensiana Juarez-Jaimes (Apocynaceae,
   Asclepiadoideae), from the state of Chiapas in southeastern Mexico, is
   described and illustrated. It is distinguished by a fleshy,
   semicircular, papillose, and glabrous callus in each sinus of the
   corolla lobes, and this callus extends along the base of each lobe
   forming a continuous corona on the throat of the corolla. The
