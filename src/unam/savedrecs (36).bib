@article{ ISI:000361764900025,
Author = {Cuxim-Koyoc, Alan and Reyes-Novelo, Enrique and Morales-Malacara, Juan
   B. and Bolivar-Cime, Beatriz and Laborde, Javier},
Title = {{Streblidae (Diptera: Hippoboscoidea) from Yucatan and Updated Species
   List for Mexico}},
Journal = {{JOURNAL OF MEDICAL ENTOMOLOGY}},
