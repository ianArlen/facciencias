@article{ ISI:000363027900005,
Author = {Beirlaen, Mathieu},
Title = {{A Regress Objection to Thagard's Theory of Deductive Coherence}},
Journal = {{ERKENNTNIS}},
Year = {{2015}},
Volume = {{80}},
Number = {{5}},
Pages = {{975-986}},
Month = {{OCT}},
Abstract = {{Paul Thagard's theory of deductive coherence, as set out in his
   Coherence in Thought and Action, faces a regress objection. Thagard's
   method of solving deductive coherence problems presupposes some notion
   of logical consequence. The problem of specifying which logic to use to
   this end is itself a deductive coherence problem, so we would expect
   Thagard's theory to be able to solve it. However, on pain of regress,
   the theory of deductive coherence cannot reach such a solution.}},
