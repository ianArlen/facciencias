@article{ ISI:000355637300012,
Author = {Estala-Arias, Samuel},
Title = {{Distribution of cusp sections in the Hilbert modular orbifold}},
Journal = {{JOURNAL OF NUMBER THEORY}},
Year = {{2015}},
Volume = {{155}},
Pages = {{202-225}},
Month = {{OCT}},
Abstract = {{Text. Let K be a number field, let M be the Hilbert modular orbifold of
   K, and let m(q) be the probability measure uniformly supported on the
   cusp cross sections of M at height q. We show that m(q) distributes
   uniformly with respect to the normalized Haar measure m on M as q tends
   to zero, and relate the rate by which m(q) approaches m to the Riemann
   hypothesis for the Dedekind zeta function of K.
   Video. For a video summary of this paper, please visit
   http://youtu.be/\_39k9paBQjM. (C) 2015 Elsevier Inc. All rights
   reserved.}},
