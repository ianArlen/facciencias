@article{ ISI:000362083700015,
Author = {Robles, Victor H. and Lora, V. and Matos, T. and Sanchez-Salcedo, F. J.},
Title = {{EVOLUTION OF A DWARF SATELLITE GALAXY EMBEDDED IN A SCALAR FIELD DARK
   MATTER HALO}},
