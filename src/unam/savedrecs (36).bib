@article{ ISI:000360946200009,
Author = {Lopez-Romero, Rodolfo E. and Escudero, R.},
Title = {{Study of the New Superconducting Compound Zr0.97V0.03B2 with
   Point-Contact Spectroscopy}},
Journal = {{JOURNAL OF SUPERCONDUCTIVITY AND NOVEL MAGNETISM}},
Year = {{2015}},
Volume = {{28}},
Number = {{10}},
Pages = {{2953-2957}},
Month = {{OCT}},
Abstract = {{We present a study of the superconducting energy gap performed with
   point contact spectroscopy in a single crystal of the superconductor
   Zr0.97V0.03B2. We determined the size and evolution with temperature of
   the energy gap, the transition temperature in the compound occurs at the
   onset of T a parts per thousand 9.3 K. The point contact junctions were
   measured from 1.7 to 20 K. The differential conductance as a function of
   bias voltage was analyzed with the Blonder-Tinkham-Klapwijk model, the
   evolution of the gap with temperature was compared with the BCS theory,
   the ratio 2 Delta(0)/K (B) T (C) was of the order of 8.95 indicative of
   a very strong coupling limit.}},
