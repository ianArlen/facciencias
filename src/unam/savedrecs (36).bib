@article{ ISI:000362365000012,
Author = {Modak, Sujoy K. and Ortiz, Leonardo and Pena, Igor and Sudarsky, Daniel},
Title = {{Black hole evaporation: information loss but no paradox}},
Journal = {{GENERAL RELATIVITY AND GRAVITATION}},
Year = {{2015}},
Volume = {{47}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{The process of black hole evaporation resulting from the Hawking effect
   has generated an intense controversy regarding its potential conflict
   with quantum mechanics' unitary evolution. A recent set of works by a
   collaboration involving one of us, have revised the controversy with the
   aims of, on one hand, clarifying some conceptual issues surrounding it,
   and, at the same time, arguing that collapse theories have the potential
   to offer a satisfactory resolution of the so-called paradox. Here we
   show an explicit calculation supporting this claim using a simplified
   model of black hole creation and evaporation, known as the CGFIS model,
   together with a dynamical reduction theory, known as CSL, and some
   speculative, but seemingly natural ideas about the role of quantum
   gravity in connection with the would-be singularity. This work
   represents a specific realization of general ideas first discussed in
   Okon and Sudarsky (Found Phys 44:114-143, 2014) and a complete and
   detailed analysis of a model first considered in Modal et al. (phys Rev
   D 91(12):124009, 2015).}},
