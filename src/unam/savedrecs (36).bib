@article{ ISI:000366327600013,
Author = {Mena, Ramses H. and Walker, Stephen G.},
Title = {{On the Bayesian Mixture Model and Identifiability}},
Journal = {{JOURNAL OF COMPUTATIONAL AND GRAPHICAL STATISTICS}},
Year = {{2015}},
Volume = {{24}},
Number = {{4}},
Pages = {{1155-1169}},
Month = {{OCT 2}},
Abstract = {{This article is concerned with Bayesian mixture models and
   identifiability issues. There are two sources of unidentifiability: the
   well-known likelihood invariance under label switching and the perhaps
   less well-known parameter identifiability problem. When using latent
   allocation variables determined by the mixture model, these sources of
   unidentifiability create arbitrary labeling that renders estimation of
   the model very difficult. We endeavor to tackle these problems by
   proposing a prior distribution on the allocations, which provides an
   explicit interpretation for the labeling by removing gaps with high
   probability. We propose a Markov chain Monte Carlo (MCMC) estimation
   method and present supporting illustrations.}},
