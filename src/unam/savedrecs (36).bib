@article{ ISI:000363291400004,
Author = {Axelrod Praes, Ruth and Weiss Steider, Benny and Azar Andere, Juan},
Title = {{The Work of the Psychoanalyst: Reflections on Countertransference with
   Institutionalized Youth Patients}},
Journal = {{PSYCHOANALYTIC INQUIRY}},
Year = {{2015}},
Volume = {{35}},
Number = {{7, SI}},
Pages = {{696-713}},
Month = {{OCT 3}},
Abstract = {{In this work, we briefly describe present-day, postmodern evolution of
   the family and the attention centers that both the government and the
   private sector have established when this fails. The birth of child
   psychoanalysis and its application in one of those centers, forging
   roots for further psychoanalytic development, is briefly described. The
   importance of countertransference and analyst supervision for those
   treating traumatized patients left without a family is discussed, as is
   an institution that altruistically rescues these children; the function
   of a psychoanalyst and his/her avatars is shown by way of three clinical
   cases when a strong countertransference obviously occurred in him/her.
   Finally, reflections are made about the importance of handling
   countertransference with the analyst's supervisor.}},
