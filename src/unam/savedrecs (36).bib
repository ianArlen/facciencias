@article{ ISI:000364237000041,
Author = {Bietenholz, Wolfgang and Chryssomalakos, Chryssomalis and Salgado,
   Marcelo},
Title = {{Comment on ``The two dimensional motion of a particle in an inverse
   square potential: Classical and quantum aspects{''} {[}J. Math. Phys.
   54, 053509 (2013)]}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2015}},
Volume = {{56}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{We comment on a fatal flaw in the analysis contained in the work of
   Martinez-y-Romero et al., {[}J. Math. Phys. 54, 053509 (2013)], which
   concerns the motion of a point particle in an inverse square potential,
   and show that most conclusions reached there are wrong. In particular,
   the manifestly senseless claim that, in the attractive potential case,
   no bounded orbits exist for negative energies, is traced to a sign
   error. Several more mistakes, both in the classical and the quantum
   cases, are pointed out. (C) 2015 AIP Publishing LLC.}},
