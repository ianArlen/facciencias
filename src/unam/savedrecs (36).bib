@article{ ISI:000364822000005,
Author = {Mendez, Nuria and Yanez-Rivera, Beatriz},
Title = {{Distribution and morphometry of the deep-sea sternaspids, Sternaspis
   maior, Sternaspis uschakovi, and Caulleryaspis fauchaldi (Polychaeta),
   in Mexican Pacific waters}},
Journal = {{BULLETIN OF MARINE SCIENCE}},
Year = {{2015}},
Volume = {{91}},
Number = {{4}},
Pages = {{457-467}},
Month = {{OCT}},
Abstract = {{The distribution of three deep-water sternaspids in the Gulf of
   California, off the west coast of the Baja California Peninsula, and off
   the southwest coast of Mexico, were studied from 2000 to 2014 during 13
   oceanographic cruises at 215 stations (n = 1 per station). Polychaetes
   were collected from 180 to 2394 m depth with a benthic sled, a modified
   Karling dredge, and a Reineck box core. Sternaspids were found in 17
   stations located at depths from 180 to 1345 m. Morphometric techniques
   based on different measures of the ventro-caudal shield were performed
   to estimate body length by extrapolation. Sternaspis maior Chamberlin,
   1919 was collected and new records of its spatial distribution (West
   coast of Baja California Peninsula and Guerrero) and depth (1030, 1039,
   and 1158 m) are provided. Sternaspis uschakovi Salazar-Vallejo and
   Buzhinskaja, 2013 was collected only in the Gulf of California.
   Caulleryaspis fauchaldi Salazar-Vallejo and Buzhinskaja, 2013, collected
   from off the west coast of the Baja California Peninsula, constituted
   new records of the species' depth (903 and 1062 m).}},
