@article{ ISI:000363725300005,
Author = {Corona-Romero, P. and Gonzalez-Esparza, J. A. and Aguilar-Rodriguez, E.
   and De-la-Luz, V. and Mejia-Ambriz, J. C.},
Title = {{Kinematics of ICMEs/Shocks: Blast Wave Reconstruction Using Type-II
   Emissions}},
Journal = {{SOLAR PHYSICS}},
Year = {{2015}},
Volume = {{290}},
Number = {{9}},
Pages = {{2439-2454}},
Month = {{SEP}},
Abstract = {{We present a physical methodology for reconstructing the trajectory of
   interplanetary shocks using Type-II radio emission data. This technique
   calculates the shock trajectory assuming that the disturbance propagates
   as a blast wave in the interplanetary medium. We applied this blast-wave
   reconstruction (BWR) technique to analyze eight fast Earth-directed
   ICMEs/shocks associated with Type-II emissions. The technique deduces a
   shock trajectory that reproduces the Type-II frequency drifts and
   calculates shock onset speed, shock travel time, and shock speed at 1
   AU. The BWR results agreed well with the Type-II spectra, with data from
   coronagraph images, in-situ measurements, and interplanetary
   scintillation observations. Perturbations in the Type-II data affect the
