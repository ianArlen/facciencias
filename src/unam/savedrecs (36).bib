@article{ ISI:000364008100016,
Author = {Roman-Zuniga, Carlos G. and Ybarra, Jason E. and Megias, Guillermo D.
   and Tapia, Mauricio and Lada, Elizabeth A. and Alves, Joao F.},
Title = {{STAR FORMATION ACROSS THE W3 COMPLEX}},
