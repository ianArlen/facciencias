@article{ ISI:000361578300016,
Author = {Urbina-Frias, Alejandra and Lopez-Luke, Tzarara and Desirena, Haggeo and
   Salas, Pedro and Torres-Castro, Alejandro and De la Rosa, Elder},
Title = {{Switching green to red emission in tridoped ZrO2:Yb3+-Er3+-Bi3+
   nanocrystals}},
Journal = {{OPTICAL MATERIALS}},
Year = {{2015}},
Volume = {{48}},
Pages = {{92-96}},
Month = {{OCT}},
Abstract = {{Tridoped ZrO2:Yb3+-Er3+-Bi3+ nanophosphor was synthesized by
   hydrothermal process and demonstrated the possibility of switching from
   a strong green-yellow broadband signal to red emission due to Bi3+ and
   Er3+ ion by changing excitation from 350 nm to 970 nm, respectively. The
   green-yellow band is associated to the transition P-3(1) -> S-1(0) and
   was overlapped with the green band associated to the transition
   H-2(11/2) + S-4(3/2) -> I-4(15/2) of Er3+ as a result of the energy
   transfer from Bi3+ to Er3+. The strong red band is associated to
   F-4(9/2) -> I-4(15/2) transition of Er3+ and enhanced by a factor of 3
   as a result of the forward and backward energy transfer process (Er3+ ->
   Bi3+ -> Er3+). This sink effect where electrons can be stored temporally
   and released back can be useful to enhance the emitted signal of other
   lanthanides. (C) 2015 Elsevier B.V. All rights reserved.}},
