@article{ ISI:000455524600005,
Author = {Nava-Lopez, Mariana and Selfa, Theresa L. and Cordoba, Diana and
   Pischke, Erin C. and Torrez, David and Avila-Foucat, Sophie and
   Halvorsen, Kathleen E. and Maganda, Carmen},
Title = {{Decentralizing Payments for Hydrological Services Programs in Veracruz,
   Mexico: Challenges and Implications for Long-term Sustainability}},
Journal = {{SOCIETY \& NATURAL RESOURCES}},
Year = {{2018}},
Volume = {{31}},
Number = {{12}},
Pages = {{1389-1399}},
Month = {{DEC 2}},
Abstract = {{In this policy review, we analyze two payments for hydrological services
   matching programs operating in the Antigua water basin in the state of
   Veracruz, Mexico. Mexico's Matching Program was created to transition
   from the national payments for hydrological services program to programs
   that would be financed by local governments, local water users and the
   private sector. Based on the analysis of organizational documents and
   key informant interviews with institutional actors, we describe the
   distinctive origins and organizational structures of these two programs,
   and how these differences have led to unique challenges for each
   program. We conclude that payments for ecosystem services programs that
   incorporate community-level organization and extensive technical
   assistance, along with expanded direct payments from ecosystem service
   users, have the greatest potential for achieving long-term
   sustainability.}},
