@article{ ISI:000452326800003,
Author = {Bietenholz, Wolfgang and de Forcrand, Philippe and Gerber, Urs and
   Mejia-Diaz, Hector and Sandoval, Ilya O.},
Title = {{Topological susceptibility of the 2D O(3) model under gradient flow}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2018}},
Volume = {{98}},
Number = {{11}},
Month = {{DEC 5}},
Abstract = {{The 2D O(3) model is widely used as a toy model for ferromagnetism and
   for quantum chromodynamics. With the latter it shares-among other basic
   aspects-the property that the continuum functional integral splits into
   topological sectors. Topology can also be defined in its lattice
   regularized version, but semiclassical arguments suggest that the
   topological susceptibility chi(t )does not scale towards a finite
   continuum limit. Previous numerical studies confirmed that the quantity
   chi(2)(t xi)diverges at large correlation length xi. Here we investigate
   the question whether or not this divergence persists when the
   configurations are smoothened by the gradient flow (GF). The GF destroys
   part of the topological windings; on fine lattices this strongly reduces
   chi(t). However, even when the flow time is so long that the GF impact
   range-or smoothing radius-attains xi/2, we still do not observe evidence
   of continuum scaling.}},
