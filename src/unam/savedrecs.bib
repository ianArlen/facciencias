@article{ ISI:000454462500006,
Author = {Gutierrez-Marquez, J. G. and Avalos-Pina, L. Y. and Lopez-Valencia, A.
   and Palacios-Perez, L. L. and Vega-Carrillo, H. R. and Rivera-Montalvo,
   T.},
Title = {{OSL dosimeters for dental panoramic radiography}},
Journal = {{APPLIED RADIATION AND ISOTOPES}},
Year = {{2018}},
Volume = {{142}},
Pages = {{32-37}},
Month = {{DEC}},
Abstract = {{The aim of the present work is to determine dosimetric characteristics
   of commercial optically stimulated luminescence dosimeter (OSLD) to
   estimate equivalent dose in the patient undergoing panoramic radiography
   procedure. Digital panoramic unit ``Instrumentarium OP200D{''} was used.
   OSL dosimeters were optically bleached before any exposure procedure.
   InLight(TM) OSL nanodosimeters were placed on the thyroid surface
   between the head and neck. The exposure parameters for all measurements
   was standard value consisted in 66 kV, 5 mA, and 14.1 s. Standard size
   field of view (FOV) scanning mode was used. Dosimeters were calibrated
   for the air kerma. Reported male adult equivalent doses from 21 to 45 mu
   Sv for each scanning for standard size field of view (FOV). Meanwhile
   reported female adult equivalent doses from 28 to 75 mu Sv for standard
   size field of view (FOV) considering all heights. The lowest equivalent
   dose (21 mu Sv) was observed in the male thyroid gland surface (S)
   position for medium height. The highest equivalent dose (75 mu Sv) was
   for female small height in the right parotid surface (R) position. In
   conclusion, the results demonstrate that OSL dosimeters are appropriate
   in vivo dosimetry system for dental panoramic dose measurements.}},
