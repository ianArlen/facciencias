Author = {Fraija, N. and Aguilar-Ruiz, E. and Galvan-Gamez, A. and Marinelli, A.
   and de Diego, J. A.},
Title = {{Study of the PeV neutrino, gamma-rays, and UHECRs around the lobes of
   Centaurus A}},
