@article{ ISI:000455160800003,
Author = {Mercedes Castillo, Maria and Barba-Alvarez, Rafael and Mayorga, Alfredo},
Title = {{Richness and diversity of aquatic insects in the Usumacinta River basin
   in Mexico}},
