@article{ ISI:000449766300003,
Author = {Mizher, Ana Julia and Hernandez-Ortiz, Saul and Raya, Alfredo and
   Villavicencio, Cristian},
Title = {{Aspects of the pseudo chiral magnetic effect in 2D Weyl-Dirac matter}},
Journal = {{EUROPEAN PHYSICAL JOURNAL C}},
Year = {{2018}},
Volume = {{78}},
Number = {{11}},
Month = {{NOV 10}},
Abstract = {{A connection is established between the continuum limit of the
   low-energy tight-binding description of graphene immersed in an in-plane
   magnetic field and the Chiral Magnetic Effect in Quantum Chromodynamics.
   A combination of mass gaps that explicitly breaks the equivalence of the
   Dirac cones, favoring an imbalance of pseudo-chiralities, is the
   essential ingredient to generate a non-dissipative electric current
   along the external field. Currents, number densities and condensates
   generated from this setup are investigated for different hierarchies of
   the energy scales involved.}},
