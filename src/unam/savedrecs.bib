@article{ ISI:000453483900016,
Author = {Lopez-Hernandez, L. H. and Calderon-Oliver, M. and Soriano-Santos, J.
   and Severiano-Perez, P. and Escalona-Buendia, H. B. and Ponce-Alquicira,
   E.},
Title = {{DEVELOPMENT AND ANTIOXIDANT STABILITY OF EDIBLE FILMS SUPPLEMENTED WITH
   A TAMARIND SEED EXTRACT}},
Journal = {{REVISTA MEXICANA DE INGENIERIA QUIMICA}},
Year = {{2018}},
Volume = {{17}},
Number = {{3}},
Pages = {{975-987}},
Month = {{DEC}},
Abstract = {{In this study, the mechanical, antioxidants, and color properties of
   edible films made with native potato starch (NPS) and whey protein
   isolate (WPI) were evaluated by incorporating tamarind seed extract
   (TSE) as an antioxidant. Firstly, the conditions to produce edible films
   were optimized by a response surface model, varying the amount of
   plasticizer (2.3-6.3\% of glycerol), the pH (5-9), and protein denature
   temperature (75-95 degrees C) were varied to achieve the best elongation
   (\% epsilon), tensile strength (sigma), and Young's modulus (E)
   characteristics. The TSE was a source of natural antioxidants by a
   polyphenol content (187.0 +/- 0.11 mg GAE/g) and antioxidant activity
   (62.5 +/- 0.04\% for DPPH center dot and 45.6 +/- 0.14\% for
   ARTS(+center dot) radical scavenging methods). The TSE supplementation
   enhanced the mechanical properties, increasing \%epsilon by 8\%, and
   decreasing sigma and E by 50\% and 14\%, respectively; with a reddish
   coloration and a 2.7 fold increase in antioxidant activity. These
   parameters remained stable during storage so that edible films
   supplemented with TSE could be used as part of a system to prevent the
   oxidation reactions in foods.}},
