@article{ ISI:000450145600012,
Author = {Stinnesbeck, Sarah R. and Stinnesbeck, Wolfgang and Terrazas Mata,
   Alejandro and Aviles Olguin, Jeronimo and Benavente Sanvicente, Martha
   and Zell, Patrick and Frey, Eberhard and Lindauer, Susanne and Rojas
   Sandoval, Carmen and Velazquez Morlet, Adriana and Acevez Nunez, Eugenio
   and Gonzalez Gonzalez, Arturo},
Title = {{The Muknal cave near Tulum, Mexico: An early-Holocene funeral site on
   the Yucatan peninsula}},
Journal = {{HOLOCENE}},
Year = {{2018}},
Volume = {{28}},
Number = {{12}},
Pages = {{1992-2005}},
Month = {{DEC}},
Abstract = {{Here, we report on an incomplete human skeleton, soot patches related to
   anthropogenic fireplaces, and cut marks on the mandible of an extinct
   peccary, from the submerged Muknal cave southwest of Tulum on the
   Mexican Yucatan peninsula. The human individual, here named `Muknal
   Grandfather', was identified as a male based on cranial parameters. The
   age at the time of death was estimated to be between 40 and 45 years. We
   propose that the human bones have been brought to the cave during the
   latest Pleistocene or early Holocene, but not later than 8600 C-14 yr BP
   (ca. 9600 cal BP), as a secondary burial of a partial skeleton. The
   peccary mandible was placed close to the burial site, possibly as part
   of the same ritual. The Muknal cave therefore served as a place for
   funeral rituals.}},
