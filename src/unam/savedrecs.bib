@article{ ISI:000455073200103,
Author = {Denova-Gutierrez, Edgar and Mendez-Sanchez, Lucia and Munoz-Aguirre,
   Paloma and Tucker, Katherine L. and Clark, Patricia},
Title = {{Dietary Patterns, Bone Mineral Density, and Risk of Fractures: A
   Systematic Review and Meta-Analysis}},
