Author = {Granot, Jonathan and De Colle, Fabio and Ramirez-Ruiz, Enrico},
Title = {{Off-axis afterglow light curves and images from 2D hydrodynamic
   simulations of double-sided GRB jets in a stratified external medium}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2018}},
