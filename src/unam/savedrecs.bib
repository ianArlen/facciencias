@article{ ISI:000456533100003,
Author = {Centeno, I, Ramon},
Title = {{Reactivated Zapata: a Zizekian vision of the Centenary of the
   Constitution}},
Journal = {{MEXICAN STUDIES-ESTUDIOS MEXICANOS}},
Year = {{2018}},
Volume = {{34}},
Number = {{1}},
Pages = {{36-62}},
Month = {{WIN}},
Abstract = {{The Constitution of 1917 is now one hundred years old. Its primary
   innovation, a product of the ongoing civil war, was the development of a
   social constitutionalism, as expressed in articles 3, 27 and 123. One
   hundred years later, the emancipatory potential implied in these
   articles has been eliminated from political practice in Mexico. This
   essay substantiates this assertion, but also suggests a way to
   regenerate social justice. Inspired by Zizek's critique of ideology, I
   propose that we must begin from the beginning, we must repeat Zapata; in
   other words, we must retrieve, for our present moment, the type of
   intervention capable of infusing new directions to enable the political
   reinvention of the country.}},
