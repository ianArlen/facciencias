@article{ ISI:000454523000317,
Author = {Ivan Castillo-Arellano, Jorge and Carlos Gomez-Verjan, Juan and
   Rojano-Vilchis, Nadia A. and Mendoza-Cruz, Myrna and Jimenez-Estrada,
   Manuel and Lopez-Valdes, Hector E. and Martinez-Coria, Hilda and
   Gutierrez-Juarez, Roger and Gonzalez-Espinosa, Claudia and Reyes-Chilpa,
   Ricardo and Arrieta-Cruz, Isabel},
Title = {{Chemoinformatic Analysis of Selected Cacalolides from Psacalium
   decompositum (A. Gray) H. Rob. \& Brettell and Psacalium peltatum
   (Kunth) Cass. and Their Effects on Fc epsilon RI-Dependent Degranulation
   in Mast Cells}},
Journal = {{MOLECULES}},
Year = {{2018}},
Volume = {{23}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{Cacalolides are a kind of sesquiterpenoids natural compounds synthesized
   by Psacalium decompositum (A. Gray) H. Rob. \& Brettell or Psacalium
   peltatum (Kunth) Cass. Antioxidant and hypoglycemic effects have been
   found for cacalolides such as cacalol, cacalone or maturine, however,
   their effects on inflammatory processes are still largely unclear. The
   main aim of this study was to investigate the biological activities of
   secondary metabolites from P. decompositum and P. peltatum through two
   approaches: (1) chemoinformatic and toxicoinformatic analysis based on
   ethnopharmacologic background; and (2) the evaluation of their potential
   anti-inflammatory/anti-allergic effects in bone marrow-derived mast
   cells by IgE/antigen complexes. The bioinformatics properties of the
   compounds: cacalol; cacalone; cacalol acetate and maturin acetate were
   evaluated through Osiris DataWarrior software and Molinspiration and
   PROTOX server. In vitro studies were performed to test the ability of
   these four compounds to inhibit antigen-dependent degranulation and
   intracellular calcium mobilization, as well as the production of
   reactive oxygen species in bone marrow-derived mast cells. Our findings
   showed that cacalol displayed better bioinformatics properties, also
   exhibited a potent inhibitory activity on IgE/antigen-dependent
   degranulation and significantly reduced the intracellular calcium
   mobilization on mast cells. These data suggested that cacalol could
   reduce the negative effects of the mast cell-dependent inflammatory
   process.}},
