@article{ ISI:000451746300061,
Author = {Janet Mendoza-Figueroa, Monica and Escudero-Castellanos, Alondra and
   Julian Ramirez-Nava, Gerardo and Eli Ocampo-Garcia, Blanca and Leticia
   Santos-Cuevas, Clara and Ferro-Flores, Guillermina and Pedraza-Lopez,
   Martha and Angel Avila-Rodriguez, Miguel},
Title = {{Preparation and preclinical evaluation of Ga-68-iPSMA-BN as a potential
   heterodimeric radiotracer for PET-imaging of prostate cancer}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2018}},
Volume = {{318}},
Number = {{3}},
Pages = {{2097-2105}},
Month = {{DEC}},
Abstract = {{In this research, we report the labeling and preclinical evaluation of
   the heterodimer Ga-68-iPSMA-BN ligand regarding its ability to target
   the prostate-specific membrane antigen (PSMA) and the gastrin-releasing
   peptide receptor (GRPr) in prostate cancer tumors. A heterodimer
   conjugate of iPSMA (PSMA inhibitor) (Nal-Lys-CO-Glu-OH) and
   Lys(3)-Bombesin (BN) with DOTA
   (1,4,7,10-tetraazacyclododecane-1,4,7,10-tetraacetic acid) as chelator
   was synthesized, labeled with Ga-68 and the in vitro PSMA/GRPr affinity
   assessed. Using pulmonary micrometastasis LNCaP (PSMA+) and PC3 (GRPr+)
   models, the influence of the ligand heterobivalency and affinity on the
   tumor uptake was analyzed quantitatively from the micro-PET imaging
   data, obtaining promising results.}},
