@article{ ISI:000455323500027,
Author = {Sanchez-Alegria, Karina and Flores-Leon, Manuel and Avila-Munoz,
   Evangelina and Rodriguez-Corona, Nelly and Arias, Clorinda},
Title = {{PI3K Signaling in Neurons: A Central Node for the Control of Multiple
   Functions}},
