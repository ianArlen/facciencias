@article{ ISI:000234591100017,
Author = {Zarate, S and Perez-Nasser, N and Casas, A},
Title = {{Genetics of wild and managed populations of Leucaena esculenta subsp
   esculenta (Fabaceae; Mimosoideae) in La Montana of Guerrero, Mexico}},
Journal = {{GENETIC RESOURCES AND CROP EVOLUTION}},
