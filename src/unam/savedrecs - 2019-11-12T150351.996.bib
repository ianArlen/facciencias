@article{ ISI:000233293600128,
Author = {Gomez-Reynoso, R and Ramirez, J and Nares, R and Luna, R and Murrieta, F},
Title = {{Characterization and catalytic activity of Ni/SBA-15, synthesized by
   deposition-precipitation}},
