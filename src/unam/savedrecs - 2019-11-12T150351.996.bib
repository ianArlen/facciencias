@article{ ISI:000231843200006,
Author = {Garcia-Serrano, J and Casarrubias-Segura, G and Galindo, AG and Mathew,
   X and Pal, U},
Title = {{Infrared study of free carriers in X/ZnO (X= semiconductor, metal)
   nanocomposites}},
