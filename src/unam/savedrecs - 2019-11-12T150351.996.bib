@article{ ISI:000233487500048,
Author = {Vazquez-Lopez, C and Hernandez, A and Fragoso, R and Golzarri, JI and
   Espinosa, G},
Title = {{AFM tip gauge by nuclear tracks methodology}},
Journal = {{RADIATION MEASUREMENTS}},
Year = {{2005}},
