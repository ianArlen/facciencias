@article{ ISI:000232680700002,
Author = {Hagedorn, GA and Toloza, JH},
Title = {{Exponentially accurate quasimodes for the time-independent
   Born-Oppenheimer approximation on a one-dimensional molecular system}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2005}},
Volume = {{105}},
Number = {{5}},
Pages = {{463-477}},
Month = {{DEC 5}},
Abstract = {{We consider the eigenvalue problem for a one-dimensional molecular-type
   quantum Hamiltonian that has the form
   H(epsilon) = -epsilon(4)/2 partial derivative(2)/partial derivative y(2)
   + h(y),
   where h(y) is an analytic family of self-adjoint operators that has a
   discrete, nondegenerate electronic level E(y) for y in some open subset
   of R. Near a local minimum of the electronic level E(y) that is not at a
   level crossing, we construct quasimodes that are exponentially accurate
   in the square of the Born-Oppenheimer parameter E by optimal truncation
   of the Rayleigh-Schrodinger series. That is, we construct an energy
   E-epsilon and a wave function Xi(epsilon), such that the L-2-norm of
   Xi(epsilon) is O(1) and the L-2-norm of (H(epsilon) -
   E-epsilon)Xi(epsilon) is bounded by Lambda exp(-Gamma/epsilon(2)) with
   Gamma > 0. (c) 2005 Wiley Periodicals, Inc.}},
