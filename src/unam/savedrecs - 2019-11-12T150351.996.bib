@article{ ISI:000232845000005,
Author = {Raptakis, DG and Manakou, MV and Chavez-Garcia, FJ and Makra, KA and
   Pitilakis, KD},
Title = {{3D configuration of Mygdonian basin and preliminary estimate of its site
   response}},
Journal = {{SOIL DYNAMICS AND EARTHQUAKE ENGINEERING}},
