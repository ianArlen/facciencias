@article{ ISI:000234645700009,
Author = {Mendez, N and Green-Ruiz, C},
Title = {{Preliminary observations of cadmium and copper effects on juveniles of
   the polychaete Capitella sp Y (Annelida : Polychaeta) from Estero del
   Yugo, Mazatlan, Mexico}},
Journal = {{REVISTA CHILENA DE HISTORIA NATURAL}},
Year = {{2005}},
Volume = {{78}},
Number = {{4}},
Pages = {{701-710}},
Month = {{DEC}},
Abstract = {{The effects of cadmium (Cd) and copper (Cu) On Cultured juveniles of
   Capitella sp. Y were investigated. Seven-day old juveniles were exposed
   to 13 (control), 60, 160, 240 and 340 mu g Cd L-1 and 17 (control), 50,
   120 and 150 mu g Cu L-1 for 30 days. Mortality significantly increased
   with increasing Cu concentrations, but in Cd exposures. it was only
   evident at 340 mu g L-1. Body size and faecal pellet production were
   reduced earlier in juveniles exposed to Cu than those exposed to Cd,
   especially in the higher Cu concentrations. These facts indicate that
   juveniles are more sensitive to Cu in terms of mortality, feeding and
   growth. Haemoglobin production was inhibited due to the scarcity of food
   in the experimental sediment rather than to metal exposure. Tube
   building was not affected by the presence of metals or by the scarcity
   of food. This study Suggests that Cu concentrations from 50 to 150 mu g
   L-1 and 340 mu g L-1 of Cd Could affect the Population dynamics of this
   species in natural ecosystems.}},
