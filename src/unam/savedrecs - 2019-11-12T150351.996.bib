@article{ ISI:000233380300005,
Author = {Ludlow-Wiechers, B and Almeida-Lenero, L and Islebe, G},
Title = {{Paleoecological and climatic changes of the Upper Lerma Basin, Central
   Mexico during the Holocene}},
