@article{ ISI:000234008500008,
Author = {Magana-Zavala, CR and Rodriguez-Gomez, FJ and Acosta, DR and
   Avila-Godoy, R},
Title = {{Study of the behaviour, before corrosion, of nickel and nickel oxide
   thin films obtained by cathodic sputtering opposite a sour medium}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2005}},
Volume = {{51}},
Number = {{6}},
Pages = {{596-599}},
Month = {{DEC}},
Abstract = {{The aim of this work is to study the behavior of nickel and nickel oxide
   thin films obtained by cathodic sputtering on steel AISI 1018 (UNS G
   10180), with the purpose of diminishing the corrosion rate of low carbon
   steel when it is exposed to a sour medium (sulphidric acid and
   chlorides) and to extend the life of metallic petrochemical structures
   which would result in an important amount of monetary and human
   resources savings. The characterization of the surfaces was carried out
