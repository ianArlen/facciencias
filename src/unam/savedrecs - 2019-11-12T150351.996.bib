@article{ ISI:000232132600002,
Author = {Saab-Rincon, G and Mancera, E and Montero-Moran, G and Sanchez, F and
   Soberon, X},
Title = {{Generation of variability by in vivo recombination of halves of a
   (beta/alpha)(8) barrel protein}},
Journal = {{BIOMOLECULAR ENGINEERING}},
