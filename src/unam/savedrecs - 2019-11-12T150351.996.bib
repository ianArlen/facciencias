@article{ ISI:000234096600019,
Author = {Flores, M and Muhl, S and Huerta, L and Andrade, E},
Title = {{The influence of the period size on the corrosion and the wear abrasion
   resistance of TiN/Ti multilayers}},
Journal = {{SURFACE \& COATINGS TECHNOLOGY}},
Year = {{2005}},
Volume = {{200}},
