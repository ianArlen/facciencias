@article{ ISI:000236427200013,
Author = {Friedman, A and Kerkman, DD and Brown, NR and Stea, D and Cappello, HM},
Title = {{Cross-cultural similarities and differences in North Americans'
   geographic location judgments}},
Journal = {{PSYCHONOMIC BULLETIN \& REVIEW}},
Year = {{2005}},
Volume = {{12}},
Number = {{6}},
Pages = {{1054-1060}},
Month = {{DEC}},
Abstract = {{We examined some potential causes of bias in geographic location
   estimates by comparing location estimates of North American cities made
   by Canadian, U.S., and Mexican university students. All three groups
   placed most Mexican cities near the equator, which implies that all
   three groups were influenced by shared beliefs about the locations of
   geographical regions relative to global reference points. However, the
   groups divided North America into different regions and differed in the
   relative accuracy of the estimates within them, which implies that there
   was an influence of culture-specific knowledge. The data support a
   category-based system of plausible reasoning, in which biases in
   judgments are multiply determined, and underscore the utility of the
   estimation paradigm as a tool in cross-cutural cognitive research.}},
