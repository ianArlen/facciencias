@article{ ISI:000232926300005,
Author = {Luca, F},
Title = {{On the greatest common divisor of u-1 and v-1 with u and v near S-units}},
Journal = {{MONATSHEFTE FUR MATHEMATIK}},
Year = {{2005}},
Volume = {{146}},
Number = {{3}},
Pages = {{239-256}},
Month = {{NOV}},
Abstract = {{In {[}2], it was shown that if a and b are multiplicatively independent
   integers and epsilon > 0, then the inequality gcd(a(n) - 1, b(n) - 1) <
   exp(epsilon n) holds for all but finitely many positive integers n.
   Here, we generalize the above result. In particular, we show that if
   f(x), f(1)(x), g(x), g(1)(x) are non-zero polynomials with integer
   coefficients, then for every epsilon > 0, the inequality
   gcd (f(n)a(n) + g(n), f(1)(n)b(n) + g(1)(n)) < exp(n epsilon)
   holds for all but finitely many positive integers n.}},
