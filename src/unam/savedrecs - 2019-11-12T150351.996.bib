@article{ ISI:000231724400008,
Author = {Quintanar-Guerrero, D and Tamayo-Esquivel, D and Ganem-Quintanar, A and
   Allemann, E and Doelker, E},
Title = {{Adaptation and optimization of the emulsification-diffusion technique to
   prepare lipidic nanospheres}},
Journal = {{EUROPEAN JOURNAL OF PHARMACEUTICAL SCIENCES}},
Year = {{2005}},
