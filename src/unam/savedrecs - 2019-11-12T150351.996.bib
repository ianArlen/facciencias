@article{ ISI:000233603400078,
Author = {Jimenez-Mier, J and Ederer, DL and Schuler, T},
Title = {{X-ray Raman scattering at the manganese L edge of MnF2: Valence emission
   of Mn2+}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2005}},
Volume = {{72}},
Number = {{5}},
Month = {{NOV}},
Abstract = {{X-ray-absorption and x-ray Raman scattering spectra recorded in the
   vicinity of the manganese L-2,L-3 absorption edge in MnF2 are presented.
   The results are compared with free-ion Hartree-Fock calculations for
   Mn2+. We confirm that x-ray-absorption and valence emission spectra of
   MnF2 can be explained almost entirely in terms of the Mn2+ free ion,
   with the resonant valence emission providing detailed information about
   the d(5) electronic structure. We found that at the L-3 absorption edge
   elastic emission is dominant, while at the L-2 valence emission is
   dominated by a feature corresponding to a transition into a low-spin
   state in the ground configuration, with other emission satellites
   corresponding to energy losses as large as 10 eV. The measured energies
   of these low spin states are in good agreement, with the Mn2+ free ion
   calculation.}},
