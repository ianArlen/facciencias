@article{ ISI:000232977900018,
Author = {Trinidad, MA and Curiel, S and Migenes, V and Patel, N and Torrelles, JM
   and Gomez, JF and Rodriguez, LF and Ho, PTP and Canto, J},
Title = {{Very large array simultaneous 1.3 cm continuum and H2O maser
   observations toward IRAS 20126+4104}},
Journal = {{ASTRONOMICAL JOURNAL}},
