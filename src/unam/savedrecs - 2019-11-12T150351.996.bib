@article{ ISI:000232557900053,
Author = {Betancourt, I and Davies, HA},
Title = {{Enhanced coercivity in B-rich nanocomposite alpha-Fe/(NdPr)(2)Fe14B/Fe3B
   hard magnetic alloys}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2005}},
Volume = {{87}},
Number = {{16}},
Month = {{OCT 17}},
Abstract = {{The structures and magnetic properties of melt-spun B-rich (10 at. \%)
   and Nb-containing nanocomposite alloys, with compositions based on the
   formula (Nd0.75Pr0.25)(y)Fe90-y-xNbxB10 (y=8, 10; x=0, 2, 4) have been
   studied. Considerable enhancement of the intrinsic coercivity, (of 657
   kA/m) together with excellent energy density (of 113 kJ/m(3)), were
   observed for y=8, x=4; while for y=10 and x=2, values of 912 kA/m and
   140 kJ/m(3) were attained. Results are interpreted in terms of a grain
   size refining effect of the Nb addition and to the more complete
   exchange coupling of the soft grains to the hard phase grains afforded
   by reduced grain sizes. (C) 2005 American Institute of Physics.}},
