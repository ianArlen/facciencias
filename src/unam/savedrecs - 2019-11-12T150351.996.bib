@article{ ISI:000231935800001,
Author = {Priego-Jimenez, R and Pena, C and Ramirez, OT and Galindo, E},
Title = {{Specific growth rate determines the molecular mass of the alginate
   produced by Azotobacter vinelandii}},
Journal = {{BIOCHEMICAL ENGINEERING JOURNAL}},
Year = {{2005}},
Volume = {{25}},
Number = {{3}},
Pages = {{187-193}},
Month = {{OCT}},
Abstract = {{A mutant strain (SML2) of Azotobacter vinelandii, unable to produce
   alginases, was grown in exponentially fed-batch cultures under constant
   dissolved oxygen tension and using washed-cells inocula. Using this
   culture technique, a pseudo-stationary steady state was achieved at
   specific growth rates (mu) of 0.03 +/- 0.009, 0.06 +/- 0.013 and 0.09
   +/- 0.012 h(-1). A batch culture, which developed a A of 0.21 h(-1), was
   also performed. The mean molecular mass (MMM) of the alginate increased
   as it decreased, and this dependence was particularly pronounced at mu
   below 0.09 h(-1). The MMM obtained at 0.03 h(-1) was 15 times higher
   (1300 +/- 150 kDa) than in batch culture (95 +/- 8 kDa). At the lowest g
   tested (0.03 h(-1) +/- 0.009 h(-1)), the alginate yield on biomass
   (Yp/x) was about twice that observed at it above 0.06 +/- 0.013 h(-1).
   This study shows that the MMM of bacterial alginate can be maximized by
   a simple strategy of growth rate control. Furthermore, production of
   high MMM alginates might be a bacterial response to cope with substrate
   shortages. (c) 2005 Elsevier B.V. All rights reserved.}},
