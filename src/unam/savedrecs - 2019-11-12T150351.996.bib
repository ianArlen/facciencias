@article{ ISI:000233293600023,
Author = {Rodas-Grapain, A and Arenas-Alatorre, J and Gomez-Cortes, A and Diaz, G},
Title = {{Catalytic properties of a CuO-CeO2 sorbent-catalyst for de-SOx reaction}},
