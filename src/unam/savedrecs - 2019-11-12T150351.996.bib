@article{ ISI:000232663700005,
Author = {Bautista, R and Salmeron, L},
Title = {{On tense modules for extended algebras over rational fields}},
Journal = {{ARCHIV DER MATHEMATIK}},
Year = {{2005}},
Volume = {{85}},
Number = {{4}},
Pages = {{318-326}},
Month = {{OCT}},
Abstract = {{Let k(x) be the field of fractions of the polynomial algebra k{[}x] over
   the field k. We prove that, for an arbitrary finite dimensional
   k-algebra Lambda, any finitely generated Lambda circle
   times(k)k(x)-module M such that its minimal projective presentation
   admits no non-trivial selfextension is of the form M congruent to
   N-k(x), for some finitely generated Lambda-module N. Some consequences
   are derived for tilting modules over the rational algebra Lambda circle
   times(k)k( x) and for some generic modules for Lambda.}},
