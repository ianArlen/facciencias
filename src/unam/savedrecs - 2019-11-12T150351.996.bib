@article{ ISI:000232984100003,
Author = {Ramirez, M and Ostrosky-Solis, F and Fernandez, A and Ardila-Ardila, A},
Title = {{Semantic verbal fluency in Spanish-speaking people: A comparative
   analysis}},
Journal = {{REVISTA DE NEUROLOGIA}},
