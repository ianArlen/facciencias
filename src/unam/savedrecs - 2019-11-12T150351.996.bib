@article{ ISI:000236112200007,
Author = {Salazar, MH and Hendrickx, ME},
Title = {{Two new species of Synalpheus Bate, 1888 (Decapoda, Caridea, Alpheidae)
   from the SE Gulf of California, Mexico}},
Journal = {{CRUSTACEANA}},
Year = {{2005}},
Volume = {{78}},
Number = {{9}},
Pages = {{1099-1116}},
Month = {{OCT}},
Abstract = {{Two new species of Synalpheus Bate, 1888 are described for the Gulf of
   California, Mexico, in the eastern Pacific. Synalpheus wickstenae new
   species, and S. stylopleuron new species, belong to the paulsoni group
   as defined by Coutiere (1909) and are morphologically closest to S.
   sanjosei Coutiere, 1909, and S. superus Abele \& Kim, 1989. These new
   species are distinguished by a series of characters mostly related with
   the shape and position of rostrum and occular teeth, the relative length
   and length/width ratio of the rostrum, the proportion between cephalic
   appendages, the presence/absence of a dorsal spine on the palm of the
   minor cheliped, the proportion of dactylus and propodus of the third
   pereiopods, and the shape of the first abdominal pleura in males.}},
