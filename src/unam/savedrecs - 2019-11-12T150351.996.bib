@article{ ISI:000232977900016,
Author = {Lang, CC and Johnson, KE and Goss, WM and Rodriguez, LF},
Title = {{Stellar winds and embedded star formation in the Galactic center
   Quintuplet and Arches clusters: Multifrequency radio observations}},
