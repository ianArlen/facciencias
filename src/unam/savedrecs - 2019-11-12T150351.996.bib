@article{ ISI:000234339300002,
Author = {Luca, F and Shparlinski, IE},
Title = {{Prime divisors of shifted factorials}},
Journal = {{BULLETIN OF THE LONDON MATHEMATICAL SOCIETY}},
Year = {{2005}},
Volume = {{37}},
Number = {{6}},
Pages = {{809-817}},
Month = {{DEC}},
Abstract = {{For any positive integer n we let P(n) be the largest prime factor of n.
   We improve and generalize several results of P. Erdos and C. Stewart on
   P(n! + 1). In particular, we show that lim sup(n ->infinity) P(n! + 1)/n
   >= 2.5, which improves their lower bound of lim sup(n ->infinity) P(n! +
   1)/n > 2.}},
