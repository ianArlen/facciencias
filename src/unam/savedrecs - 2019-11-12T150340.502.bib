@inproceedings{ ISI:000238905000021,
Author = {Ortega-Guerrero, M. A.},
Editor = {{Martin-Duque, JF and Brebbia, CA and Emmanouloudis, DE and Mander, U}},
Title = {{Land subsidence evolution and controlling mechanisms near Mexico City:
   environmental planning and management}},
Booktitle = {{Geo-Environment and Landscape Evolution II: EVOLUTION, MONITORING,
   SIMULATION, MANAGEMENT AND REMEDIATION OF THE GEOLOGICAL ENVIRONMENT AND
   LANDSCAPE}},
