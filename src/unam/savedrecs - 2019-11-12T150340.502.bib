@article{ ISI:000243842900011,
Author = {Martinez-de-la-Vega, Vernica},
Title = {{Dimension of N-fold hyperspaces of graphs}},
Journal = {{HOUSTON JOURNAL OF MATHEMATICS}},
Year = {{2006}},
Volume = {{32}},
Number = {{3}},
Pages = {{783-799}},
Abstract = {{Given a finite graph X, the n-fold hyperspace of X is defined by C-n(X)
   \{A subset of X: A is nonempty, closed and has at most n Components).
   Consider R(X) as the set of ramification points of X. In this Paper we
   prove that
   dim{[}C-n (X)] = 2n + dim{[}C(X)].
   Also for every element A is an element of C-n(X),
   dim(A) {[}C-n (X)] = 2n + Sigma(p is an element of R(X)boolean AND A)
   (ord(x) (p) - 2).
   As a consequence, we prove that for any n is an element of N, the n-fold
   hyperspace of a finite graph (different from a simple closed curve) is a
   cone if and only if x is an arc or a simple k-od.}},
