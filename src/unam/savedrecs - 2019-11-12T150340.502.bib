@inproceedings{ ISI:000245033700028,
Author = {Russell, C. T. and Mayerberger, S. S. and Blanco-Cano, X.},
Editor = {{Grard, R and Masson, P and Khurana, K}},
Title = {{Proton cyclotron waves at Mars and Venus}},
