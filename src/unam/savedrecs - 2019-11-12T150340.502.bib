@inproceedings{ ISI:000241722600010,
Author = {Khotiaintsev, S. and Khotiaintsev, K. and Garcia-Moreno, A.},
Editor = {{Sukhoivanov, IA}},
Title = {{Modeling of the fiber-optical water salinity sensor}},
Booktitle = {{LFNM 2006: 8TH INTERNATIONAL CONFERENCE ON LASER AND FIBER-OPTICAL
   NETWORKS MODELING, PROCEEDINGS}},
Series = {{International Conference on Laser and Fiber Optical Networks Modeling}},
Year = {{2006}},
Pages = {{36+}},
Note = {{8th International Conference on Laser and Fiber-Optical Networks
   Modeling, Kharkiv, UKRAINE, JUN 29-JUL 01, 2006}},
Abstract = {{We describe the mathematical model, the calculation algorithm, the
   computer program, and the user interface developed for numerical
   simulation of the optical fiber refractometric sensor. The sensor
   consists of a pair of multimode optical fibers which are coupled via the
   hemispherical optical detection element. We employ the geometrical
   approach for the modeling of the sensor. The computer program is written
   in Visual Basic programming language. We obtained the optical
   transmission of the sensor as a function of the refractive index of the
   surrounding medium and showed that the present sensor can be used for
   water salinity measurement.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
