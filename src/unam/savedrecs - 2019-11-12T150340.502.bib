@inproceedings{ ISI:000243529300067,
Author = {Yanez, Y. Garcia and Ruiz, A. Candil and Rojo, F. Estrada},
Book-Group-Author = {{Medimond}},
Title = {{Relationship between teacher's evaluation and grades obtained by
   students in the microbiology and parasitology course at the Facultad de
   Medicina of the Universidad Nacional Autonoma de Mexico (UNAM)}},
Booktitle = {{ICOPA XI: PROCEEDINGS OF THE 11TH INTERNATIONAL CONGRESS OF PARASITOLOGY}},
Year = {{2006}},
Pages = {{391+}},
Note = {{11th International Congress of Parasitology (ICOPA XI), Glasgow,
   SCOTLAND, AUG 06-11, 2006}},
Organization = {{British Soc Parasitol; World Federat Parasitologists}},
Abstract = {{A recent study, intended to determine the relationship between the
   evaluation of teaching personnel and the grades students obtained, and
   whether or not the differences observed were significant. The evaluation
   instrument is anonymous and consists of 30 items. This instrument was
   given to students after they knew their grade in both the department's
   exam and their professor's exam. Several statistical analysis were used
   in order to find out if there was a relationship between the grade given
   to the student by the teacher, the one obtained by the student in the
   department's exam and the one the student gave to the professor. The
   results indicate that there are significant differences between the
   grade given by students to the teacher and that obtained in the exams,
   as well as among teachers who are MDs and those whose background is in
   health sciences but are not MDs. These results are relevance for our
   community because they provide data that allows the establishment of
   actions to improve the teaching - learning process.}},
