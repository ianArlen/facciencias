@article{ ISI:000238399700004,
Author = {Esqueda, Martin and Coronado, Martha and Sanchez, Alfonso and
   Perez-Silva, Evangelina and Herrera, Teofilo},
Title = {{Macromycetes of pinacate and great altar desert biosphere reserve,
   Sonora, Mexico}},
Journal = {{MYCOTAXON}},
Year = {{2006}},
Volume = {{95}},
Pages = {{81-90}},
Month = {{JAN-MAR}},
Abstract = {{Macromycetes associated with four different vegetation types in the
   Pinacate and Great Altar Desert biosphere reserve were studied over a
   one-year period. Twenty-seven taxa representing the Phallales and
   Agaricales were determined. Families represented include the Geastraceae
   (4), Agaricaceae (5), Phelloriniaceae (1), Lycoperdaceae (4),
   Schizophyllaceae (1), and Tulostomataceae (12). Of these, Montagnea
   arenaria and Podaxis pistillaris, which produced the highest number of
   collections, were the most widely distributed; species belonging to the
   Geastraceae and Lycoperdaceae showed a more restricted distribution. The
   most species-rich genus was Tulostoma with 12 taxa. Geastrum berkeleyi,
   G. schmidelii and Tulostoma mohavei are new records for Mexico.
   Chorology and phenology of all species are outlined, and SEM micrographs
   of basichospores representing six species are provided.}},
