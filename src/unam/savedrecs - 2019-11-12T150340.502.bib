@article{ ISI:000233822100006,
Author = {Herrera-Franco, PJ and Hernandez-Sanchez, F and Adem, E and Burillo, G},
Title = {{Dynamic mechanical properties of compatibilized PET with radiation
   oxidized HDPE}},
Journal = {{POLYMER BULLETIN}},
Year = {{2006}},
Volume = {{56}},
Number = {{1}},
Pages = {{47-52}},
Month = {{JAN}},
Abstract = {{Binary blends of HDPE/PET exhibit poor mechanical properties because of
   their non compatibility. In this work, HDPE was oxidized by gamma-ray
   preirradiation in air, subsequently heated to destroy peroxides formed
   by this irradiation and to form polar groups in the HDPE, and then
   extruded with PET as a compatibilizing method of the blend. The dynamic
   mechanical properties were studied, and an improvement was observed when
   the PET content was increased while the HDPE used was irradiated. The
   largest increase in the mechanical properties was observed for PET
   contents between 10 and 20\% (w/w). The improvement in the dynamic
   mechanical properties is believed to occur because of a percolation
   effect of the PET in the HDPE matrix and the radiation-improved
   compatibility by means of polar groups formed in the polyethylene.}},
