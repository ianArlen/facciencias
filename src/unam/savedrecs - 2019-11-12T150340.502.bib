@inproceedings{ ISI:000254378800125,
Author = {Tabares-Mendoza, Carolina and Guadarrama, Patricia},
Editor = {{Simos, T and Maroulis, G}},
Title = {{Predicting the catalytic efficiency by quantum-chemical descriptors.
   Theoretical study of pincer metallic complexes involved in the catalytic
   Heck reaction}},
