@article{ ISI:000235010000098,
Author = {Macias, A and Quevedo, H and Sanchez, A},
Title = {{Local Lorentz invariance in N=1 supergravity}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2006}},
Volume = {{73}},
Number = {{2}},
Month = {{JAN}},
Abstract = {{We discuss the local Lorentz invariance in the context of N=1
   supergravity and show that a previous attempt to find explicit solutions
   to the Lorentz constraint in terms of gamma-matrices is not correct. We
   improve that solution by using a different representation of the Lorentz
   operators in terms of the generators of the rotation group, and show its
   compatibility with the matrix representation of the fermionic field. We
   find the most general wave functional that satisfies the Lorentz
   constraint in this representation.}},
