@inproceedings{ ISI:000252251601049,
Author = {Aguilar, Luis T. and Boiko, Igor and Fridman, Leonid and Iriarte, Rafael},
Book-Group-Author = {{IEEE}},
Title = {{Output excitation via continuous sliding-modes to generate periodic
   motion in underactuated systems}},
Booktitle = {{PROCEEDINGS OF THE 45TH IEEE CONFERENCE ON DECISION AND CONTROL, VOLS
   1-14}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2006}},
Pages = {{1632+}},
Note = {{45th IEEE Conference on Decision and Control, San Diego, CA, DEC 13-15,
   2006}},
Organization = {{IEEE}},
Abstract = {{In this paper, we introduce a new approach in generating a periodic
   motion for underactuated mechanical systems using continuous
   sliding-mode (SM) algorithms as a feedback loop, particularly with
   super-twisting algorithm and Power-Fractional Sliding-Modes (PFSM),
   without computing a reference trajectory. Periodic solutions of the
   closed-loop system are found by mean of the describing function method
