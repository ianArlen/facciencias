@article{ ISI:000235009700099,
Author = {Sanchez-Castillo, A and Roman-Velazquez, CE and Noguez, C},
Title = {{Optical circular dichroism of single-wall carbon nanotubes}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2006}},
Volume = {{73}},
Number = {{4}},
Month = {{JAN}},
Abstract = {{The circular dichroism (CD) spectra of single-wall carbon nanotubes are
   calculated using a dipole approximation. The calculated CD spectra show
   features that allow us to distinguish between nanotubes with different
   angles of chirality, and diameters. These results provide theoretical
   support for the quantification of chirality and its measurement, using
   the CD line shapes of chiral nanotubes. It is expected that this
   information would be useful to motivate further experimental studies.}},
