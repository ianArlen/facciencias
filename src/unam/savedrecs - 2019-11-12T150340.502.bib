@article{ ISI:000239016300021,
Author = {Hernandez-Becerril, David U. and Diaz-Almeyda, Erika Marlene},
Title = {{The Nitzschia bicapitata group, new records of the genus Nitzschia, and
   further studies on species of Pseudo-nitzschia (Bacillariophyta) from
   Mexican Pacific coasts}},
Journal = {{NOVA HEDWIGIA}},
Year = {{2006}},
Number = {{130}},
Pages = {{293-306}},
Abstract = {{Diatoms of the genus Nitzschia sensu lato are widespread in freshwater
   and marine habitats all over the world. Only few species of Nitzschia
   are planktonic, whereas almost all Pseudo-nitzschia species are truly
   planktonic. Species of the Plitzschia bicapitata group and five new
   records of the genera Nitzschia and Pseudo-nitzschia, plus one new
   species, Pseudo-nitzschia roundii Hernandez-Becerril sp. nov., from
   coasts of the Mexican Pacific Ocean are dealt with in this paper. All
   samples were obtained during different seasons between 1985-1998, by
   bottle and phytoplankton net (35 mu m or 54 mu m mesh), following
   different study protocols. The species were studied by light and
   electron microscopy (SEM and TEM). Morphological details are provided
   for all the species, and discussion is made on the morphology,
   distribution and taxonomy of the group Nitzschia bicapitata and
   Pseudo-nitzschia. Pseudonitzschia roundii is very similar to P.
   subpacifica, except in the structure of the striae, the interstriae and
   the copulae of the cingulum: P. roundii has one row of large poroids
   forming the striae, each poroid divided into 2 (-4) sections, the
   interstriae are irregular and very lightly silicified, and the striae of
   the copulae are denser. Another superficially related species is
   Pseudo-nitzschia caciantha.}},
