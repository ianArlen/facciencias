@inproceedings{ ISI:000280139700046,
Author = {Hernandez-Morales, J. B. and Vergara-Hernandez, H. J. and
   Barrera-Godinez, J. A. and Beltran-Fragoso, B. and Alamo-Valde, C.},
Editor = {{Herring, D and Hill, R}},
Book-Group-Author = {{ASM International}},
Title = {{The Influence of Flowrate and Deflector Arrangement on Distortion in
   Agitated Quench Tanks}},
Booktitle = {{HEAT TREATING}},
Year = {{2006}},
Pages = {{314-319}},
Note = {{23rd ASM-Heat-Treating-Society Conference, Pittsburgh, PA, SEP 25-28,
   2005}},
Organization = {{ASM Heat Treating Soc}},
Abstract = {{Quench tanks are fitted with deflectors to modify the flow pattern of
   the quenchant, directing the flow towards the load of parts. In the
   past, the design of the deflector arrangement has been carried out based
   on trial and error. To generate fundamental knowledge, the residual
   distortion in AISI 1045 steel probes quenched in a physical model (scale
   1:10) of a draft-tube agitated quench tank was measured and related to
   the flow pattern produced by combinations of water flowrate and
   deflector arrangement and its effect on heat transfer.
   It was found that the flow patterns produced affect heat extraction and,
   therefore the evolution of the thermal and microstructural fields during
   quenching. The residual distortion could be explained based on these
   phenomena and linked to the fluid flow dynamics. The results will be
   used to validate a mathematical model to predict the flow patterns and
   distortion in agitated quench tanks. The completed model will be applied
   to design quench tanks.}},
