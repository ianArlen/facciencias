@article{ ISI:000235015000027,
Author = {Aragon-Gonzalez, G and Aragon, JL and Rodriguez-Andrade, MA},
Title = {{The decomposition of an orthogonal transformation as a product of
   reflections}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2006}},
Volume = {{47}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{In this work, an algorithm to decompose a given orthogonal
   transformation as a product of reflections through hyperplanes is
   presented. This in fact constitutes a constructive proof of a Cartan
   theorem, valid over any field K=Q, R or C. Clifford algebras are used to
   explicitly calculate the reflections that decompose a given orthogonal
   transformation. Our algorithm may have application in fields such as
   computer graphics or crystallography, and can also play an important
   role in orthogonal elimination and the solution of equations systems. An
   explicit example is provided and we apply our results to the
   crystallographic problem of coincidence lattices. (c) 2006 American
   Institute of Physics.}},
