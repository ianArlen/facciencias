@article{ ISI:000235016600002,
Author = {Ledezma, Jaime Arroyo and Gallegos-Sanchez, Jaime and Godoy, Alejandro
   Villa and Mendez, Javier Valencia},
Title = {{Feedback neural systems operating during the circannual reproductive
   cycle of the ewe}},
Journal = {{INTERCIENCIA}},
Year = {{2006}},
Volume = {{31}},
Number = {{1}},
Pages = {{8-15}},
Month = {{JAN}},
Abstract = {{The annual reproductive cycle in the ewe is controlled by the changes in
   the amplitud of photoperiod. The light stimulus is converted into a
   hormonal signal: the circadian rhytm of melatonin secretion. Short days
   are inductive whereas long days inhibits the ovulatory activity. During
   the reproductive season, progesterone regulates the estrous cycle of the
   ewe inhibiting the GnRH pulsatil secretion. Progesterone exerts its
   action on the preoptic area of the hypothalamus indirectly, possibly via
   the gamma aminobutiric acid and the endogeneous opiod peptides.
   Estradiol (E-2) increases the GnRH pulsatile secretion by a positive
   feedback effect. Increments in GnRH pulses induce the preovulatory peak
   of LH evoking estrous behaviour and ovulation. During the seasonal
   anestrus, the secretion pattern of melatonin increases the sensivity of
   the hypothalamus to the negative effect of E-2, then estradiol inhibits
   the pulsatil secretion of GnRH, acting in the A-15 dopaminergic nucleons
   of the retrochiasmatic lateral area of the hypothalamus; in this
   mechanism the dopaminergic system mediates the effect of estradiol on
   the GnRHergic neurons.}},
