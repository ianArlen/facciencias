@inproceedings{ ISI:000252251602138,
Author = {Fridman, L. and Poznyak, A. and Bejarano, F. J.},
Book-Group-Author = {{IEEE}},
Title = {{Hierarchical second-order sliding-mode observer for linear systems with
   unknown inputs}},
