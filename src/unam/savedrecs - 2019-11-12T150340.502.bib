@article{ ISI:000203496700007,
Author = {Flores Paredes, Joaquin},
Title = {{The reform of the common agricultural policy and the Doha Round}},
Journal = {{REVISTA DE ECONOMIA MUNDIAL}},
Year = {{2006}},
Number = {{15}},
Pages = {{155-177}},
Abstract = {{Developed countries have ignored the agreements reached at the GATT's
   Uruguay Round. This has generated the reaction of developing countries,
   now better organized since the beginning of the Doha Round. Developing
   Countries demand the effective reduction of agricultural subsidies,
   given that such practices have lowered international prices and
   therefore ruined many small producers in the South. In this context, the
   EU has been pushed to cut subsidies to agricultural production and
   exports. Nevertheless, the final impact of the new Common Agricultural
   Policy is yet unknown, since negotiations in the Doha Round respond, not
   only to the interests of member countries and the theoretical defense of
   world free trade, but also to the interests of transnational
   corporations. This paper explains how the latest reforms of ACP have
   responded to negotiations within GATT and the WTO, as well as their
   impact in the world agricultural market and in the agriculture of
   developing countries.}},
