@inproceedings{ ISI:000243663000128,
Author = {Fridman, Leonid and Levant, Arie and Davila, Jorge},
Book-Group-Author = {{IEEE}},
Title = {{High-order sliding-mode observer for linear systems with unknown inputs}},
Booktitle = {{PROCEEDINGS OF 2006 MEDITERRANEAN CONFERENCE ON CONTROL AND AUTOMATION,
   VOLS 1 AND 2}},
