@inproceedings{ ISI:000244439700111,
Author = {de Castro, R. Sangines and Ake, C. Sanchez and Sobral, H. and
   Villagran-Muniz, M.},
Editor = {{HerreraVelazquez, JJE}},
Title = {{Time resolved diagnostic of dual-pulsed laser ablation on graphite
   targets}},
