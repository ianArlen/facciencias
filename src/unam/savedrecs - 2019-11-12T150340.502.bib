@inproceedings{ ISI:000241987600004,
Author = {Valverde-Aguilar, Guadalupe and Garcia-Macedo, Jorge and Wang, Xianghuai
   and Zink, Jeffrey I. and Nelsen, Stephen F.},
Editor = {{Gaburro, Z and Cabrini, S}},
Title = {{Photoinduced electron-transfer in
   2-tert-Butyl-3-(Anthracen-9-yl)-2,3-Diazabicyclo{[}2.2.2] octane}},
Booktitle = {{NANOPHOTONIC MATERIALS III}},
