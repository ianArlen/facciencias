@inproceedings{ ISI:000238312600112,
Author = {Rendon, Pablo L.},
Editor = {{Atchley, AA and Sparrow, VW and Keolian, RM}},
Title = {{Nonlinear effects in propagation of broadband jet noise}},
Booktitle = {{Innovations in Nonlinear Acoustics}},
