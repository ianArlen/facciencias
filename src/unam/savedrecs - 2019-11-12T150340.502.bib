@article{ ISI:000237118000004,
Author = {Aguirre-Gomez, A and McBride, MB and Norvell, WA},
Title = {{A voltammetric method for determining free metal activities in aqueous
   solutions. Part 1: direct current voltammetry of Cd, Cu, and Pb}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENT AND POLLUTION}},
Year = {{2006}},
Volume = {{26}},
Number = {{1-3}},
Pages = {{41-67}},
Abstract = {{A selective and sensitive method for determining free metal activities
   (M2+) for Cd, Cu and Ph in aqueous solutions is presented based on the
   Lingane and DeFord and Flume models. Electrochemical and complexation
   studies were carried Out by direct current voltammetry (DCV) and by
   metal ion selective electrodes (ISE) as a reference method. A
   voltammetric equation was tested to evaluate (M2+) by varying ligand
   type and pH (4.5-7.5). Experimental DCV-Nernstian slopes and R values
   were 29.1, 0.887 for Cd2(+) 29.1, 0.957 for Cu2+ and 27.6, 0.925 for
   Ph2+, respectively. Comparison of the two independent methods gave for
   the DCV-ISE plots, slopes and R-2 values of 0.823, 0.825 for Cd, 0.996
   and 0,996, for Cu and 1.10 and 0.935 for Ph respectively. The DCV- and
   ISE-(M2+) values and those predicted by MINEQL+, using reported
   stability constants, were statistically similar. These studies are
   ecologically relevant as the method allows the assessment of chemical
   activities of essential and toxic free heavy metals in Soil Solution and
   other synthetic and natural solutions.}},
