@inproceedings{ ISI:000245035000034,
Author = {Perez-Enriquez, R. and Carrillo, A. and Rodriguez, C.},
Editor = {{Shay, M and Wiltberger, M and Lester, M and Liu, W}},
Title = {{Fractal character of G index of IPS data for the period 1991-1994,
   obtained from multiscale wavelet analysis}},
Booktitle = {{MAGNETOSPHERIC DYNAMICS AND THE INTERNATIONAL LIVING WITH A STAR PROGRAM}},
