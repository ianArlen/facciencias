@article{ ISI:000244651200016,
Author = {Lima, I. Sanchez and Hess, P. O.},
Title = {{Clebsch-Gordan coefficients for U(8) superset of O(8) superset of SU(3):
   the first steps}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2006}},
Volume = {{52}},
Number = {{1, S}},
Pages = {{82-87}},
Month = {{JAN}},
Note = {{28th Symposium on Nuclear Physics, Hacienda Cocoyoc, MEXICO, JAN 04-07,
   2005}},
Abstract = {{The group chain U(8) superset of O(8) superset of SU(3) plays an
   important role in systems of many gluons and in a schematic model for
   QCD at low energy. In order to calculate decay probabilities one has to
   calculate the Clebsch-Gordan coefficients of this group chain. In this
   contribution we present the basic idea in the example of SU(3) superset
   of SO(3). Afterwards, the polynomial states of the U(8) chain are
   constructed and the procedure to obtain the Clebsch-Gordan coefficients
   is outlined. Partial results are presented.}},
