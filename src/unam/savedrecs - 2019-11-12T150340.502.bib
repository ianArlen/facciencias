@article{ ISI:000238802700008,
Author = {Mendoza-Perez, R. and Aguilar-Hernandez, J. and Sastre-Hernandez, J. and
   Ximello-Quiebras, N. and Contreras-Puente, G. and Santana-Rodriguez, G.
   and Vigil-Galan, O. and Moreno-Garcia, E. and Morales-Acevedo, A.},
Title = {{Photoluminescence characteristics of CdS layers deposited in a chemical
   bath and their correlation to CdS/CdTe solar cell performance}},
Journal = {{SOLAR ENERGY}},
Year = {{2006}},
Volume = {{80}},
Number = {{6}},
Pages = {{682-686}},
Note = {{International Conference on Solar Renewable Energy News, Florence,
   ITALY, APR 02-08, 2005}},
Abstract = {{In this work, we study Us films processed by chemical bath deposition
   (CBD) using different thiourea concentrations in the bath solution with
   post-thermal treatments using CdCl2. We study the effects of the
   thiourea concentration on the photovoltaic performance of the CdS/CdTe
   solar cells, by the analysis of the I-V curve, for S/Cd ratios in the
   CBD solution from 3 to 8. In this range of S/Cd ratios the CdS/CdTe
   solar cells show variations of the open circuit voltage (V-oc), the
   short circuit current (J(sc)) and the fill factor (FF). Other
   experimental data such as the optical transmittance and
   photoluminescence were obtained in order to correlate to the I-V
   characteristics of the solar cells. The best performance of CdS-CdTe
   solar cells made with Us films obtained with a S/Cd ratio of 6 is
   explained in terms of the sulfur vacancies to sulfur interstitials ratio
   in the CBD-CdS layers. (c) 2006 Published by Elsevier Ltd.}},
