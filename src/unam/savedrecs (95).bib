@article{ ISI:000258366000012,
Author = {Supanitsky, A. D. and Etchegoyen, A. and Medina-Tanco, G. and Allekotte,
   I. and Berisso, M. Gomez and Medina, M. C.},
Title = {{Underground muon counters as a tool for composition analyses}},
Journal = {{ASTROPARTICLE PHYSICS}},
Year = {{2008}},
Volume = {{29}},
