@article{ ISI:000259683100086,
Author = {Cisneros, Luis A. and Minzoni, Antonmaria A. and Panayotaros, Panayotis
   and Smyth, Noel F.},
Title = {{Modulation analysis of large-scale discrete vortices}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2008}},
Volume = {{78}},
Number = {{3, 2}},
Month = {{SEP}},
Abstract = {{The behavior of large-scale vortices governed by the discrete nonlinear
   Schrodinger equation is studied. Using a discrete version of modulation
   theory, it is shown how vortices are trapped and stabilized by the
   self-consistent Peierls-Nabarro potential that they generate in the
   lattice. Large-scale circular and polygonal vortices are studied away
   from the anticontinuum limit, which is the limit considered in previous
   studies. In addition numerical studies are performed on large-scale,
   straight structures, and it is found that they are stabilized by a
   nonconstant mean level produced by standing waves generated at the ends
   of the structure. Finally, numerical evidence is produced for
   long-lived, localized, quasiperiodic structures.}},
