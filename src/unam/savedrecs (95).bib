@article{ ISI:000257260800055,
Author = {Nichita, Dan Vladimir and Garcia-Sanchez, Fernando and Gomez, Susana},
Title = {{Phase stability analysis using the PC-SAFT equation of state and the
   tunneling global optimization method}},
