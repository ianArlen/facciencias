@article{ ISI:000258974600025,
Author = {Wiersema, John H. and Alejandro, Novelo R. and Bonilla-Barbosa, Jaime R.},
Title = {{Taxonomy and typification of Nymphaea ampla (Salisb.) DC. sensu lato
   (Nymphaeaceae)}},
