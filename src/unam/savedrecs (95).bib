@article{ ISI:000258580500008,
Author = {Garcia-Serrano, J. and Pal, U. and Herrera, A. M. and Salas, P. and
   Angeles-Chavez, C.},
Title = {{One-Step ``Green{''} Synthesis and Stabilization of Au and Ag
   Nanoparticles Using Ionic Polymers}},
Journal = {{CHEMISTRY OF MATERIALS}},
Year = {{2008}},
Volume = {{20}},
Number = {{16}},
Pages = {{5146-5153}},
Month = {{AUG 26}},
Abstract = {{Generally, metal nanoparticles are prepared by a variety of chemical
   methods that are not environmentally friendly. We report on the use of
   aqueous solutions of ionic polymers containing arsonic acid groups for
   the green synthesis of Au and Ag nanoparticles of intriguing shapes at
   room temperature without using additional reducing reagent. Our results
   indicate that these ionic polymers are not only capable of reducing
   metal ions but also can protect the nanoparticles in the colloidal
   solutions. In general, the process of metal ions reduction using these
   ionic polymers is very slow, but the ability of the polymers to protect
   the Au and Ag nanoparticles permits us to obtain colloidal solutions
   that are stable for several months. This method would be very much
   useful for the preparation of other metal nanoparticles.}},
