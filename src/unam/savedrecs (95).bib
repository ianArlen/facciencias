@article{ ISI:000258226400003,
Author = {Nagarajan, Ramasamy and Sial, Alcides N. and Armstrong-Altrin, John S.
   and Madhavaraju, Jayagopal and Nagendra, Raghavendra},
Title = {{Carbon and oxygen isotope geochemistry of Neoproterozoic limestones of
   the Shahabad Formation, Bhima basin, Karnataka, southern India}},
Journal = {{REVISTA MEXICANA DE CIENCIAS GEOLOGICAS}},
Year = {{2008}},
Volume = {{25}},
Number = {{2}},
Pages = {{225-235}},
Month = {{AUG}},
Abstract = {{Petrography, major (including four trace elements), stable isotopes
   (carbon and oxygen), and Sr-87/Sr-86 geochemistry of limestones of the
   Shahabad Formation, Bhima basin, Karnataka, southern India are reported.
   These limestones show a narrow range of delta C-13 (similar to 1.34-1.96
   parts per thousand) and delta O-18 (similar to-6.04 to -7.61 parts per
   thousand) values. The petrographic study reveals the presence of
   microsparite and micro- and macrostylolites. The delta C-13 and
   Sr-87/Sr-86 values indicate that these limestones were deposited during
   the late Neoproterozoic age and the PO values also are very similar to
   the average Proterozoic carbonate values. Mn and Sr concentrations and
   low Mn/Sr ratio (<1) together with the stable and radiogenic isotope
   data suggest that the studied samples are well-preserved or scarcely
   altered limestones and probably have retained isotopic signatures.}},
