@article{ ISI:000258815500010,
Author = {Ramos-Brito, F. and Alejo-Armenta, C. and Garcia-Hipolito, M. and
   Camarillo, E. and Hernandez, J. and Murrieta, H. and Falcony, C.},
Title = {{Photoluminescent emission of Pr3+ ions in different zirconia crystalline
   forms}},
