@article{ ISI:000256689900022,
Author = {Briones-Jurado, Claudia and Agacino-Valdes, Esther},
Title = {{On the possible removal of nitrogen monoxide and carbon monoxide on
   copper ion-exchanged montmorillonite: A DFT study}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2008}},
Volume = {{108}},
Number = {{10}},
Pages = {{1802-1809}},
