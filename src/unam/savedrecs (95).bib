@article{ ISI:000257613600001,
Author = {Colin, J. and Serna, S. and Carnpillo, B. and Flores, O. and
   Juarez-Islas, J.},
Title = {{Microstructural and lattice parameter study of as-cast and rapidly
   solidified NiAl intermetallic alloys with Cu additions}},
Journal = {{INTERMETALLICS}},
