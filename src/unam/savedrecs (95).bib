@article{ ISI:000262280500005,
Author = {Guzman-Beltran, Silvia and Orozco-Ibarra, Marisol and
   Gonzalez-Cuahutencos, Octavio and Victoria-Mares, Sonia and
   Merchand-Reyes, Giovanna and Medina-Campos, Omar N. and
   Pedraza-Chaverri, Jose},
Title = {{NEUROPROTECTIVE EFFECT AND REACTIVE OXYGEN SPECIES SCAVENGING CAPACITY
   OF MANGOSTEEN PERICARP EXTRACT IN CULTURED NEURONS}},
Journal = {{CURRENT TOPICS IN NUTRACEUTICAL RESEARCH}},
Year = {{2008}},
Volume = {{6}},
Number = {{3}},
Pages = {{149-157}},
Month = {{AUG}},
Abstract = {{The pericarp of mangosteen (Garcinia mangostana L.) (GML) has been used
   in traditional medicine for the treatment of different illnesses. The
   potential protective effect of GML pericarp extract and of a commercial
   mangosteen juice against 3-nitropropionic avid (3-NP)-induced
   neurotoxicity and reactive oxygen species (ROS) production in cultured
   cerebellar granule neurons (CGNs) as well their in vitro ROS and
   reactive nitrogen species (RNS) scavenging capacity were studied in the
   present paper. The extract and the juice were able to ameliorate 3-NP
   induced cytotoxicity that was closely associated to the decrease in 3-NP
   induced ROS production in CGNs. The extract and the juice scavenged in a
   concentration-dependent way superoxide anion, hydroxyl radical,
   hypochlorous acid, and peroxynitrite. The extract also scavenged singlet
   oxygen. It is concluded that the antioxidant properties of both GML
   pericarp extract and mangosteen juice are involved in the
   neuroprotective effect against 3-NP in CGNs.}},
