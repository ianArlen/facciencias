@article{ ISI:000258898000038,
Author = {Scholz-Boettcher, Barbara M. and Ahlf, Stefanie and Vazquez-Gutierrez,
   Felipe and Rullkoetter, Juergen},
Title = {{Sources of hydrocarbon pollution in surface sediments of the Campeche
   Sound, Gulf of Mexico, revealed by biomarker analysis}},
