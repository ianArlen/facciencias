@article{ ISI:000265857900006,
Author = {Nakano, Hirotaka},
Title = {{The First Half of the Transcendental Deduction in Kant's Critique of
   Pure Reason (B)}},
Journal = {{IDEAS Y VALORES}},
Year = {{2008}},
Volume = {{57}},
Number = {{137}},
Pages = {{93-111}},
Month = {{AUG}},
Abstract = {{After the publication of Strawson's ``The Bounds of Sense{''}, the
   Transcendental Deduction in Kant's Critique of Pure Reason has been
   widely regarded as a proof of the objectivity of knowledge as a
   condition of unity of self-consciousness. Nevertheless, many
   interpreters accept that there are passages which cannot be easily
   integrated into such a strategy. In this article, through an analysis of
   the first half of the Transcendental Deduction (B), I try to point out
   the need to adopt a different interpretation according to which Kant
   does not consider objectivity as a condition of self-consciousness, but
   rather self-consciousness as a necessary condition of objectivity.}},
