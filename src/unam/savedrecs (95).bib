@article{ ISI:000260706300004,
Author = {Santana, Omar and Reina, Matias and Anaya, Ana Luisa and Hernandez,
   Fidel and Izquierdo, M. Elena and Gonzalez-Coloma, Azucena},
Title = {{3-O-Acetyl-narcissidine, a Bioactive Alkaloid from Hippeastrum puniceum
   Lam. (Amaryllidaceae)}},
Journal = {{ZEITSCHRIFT FUR NATURFORSCHUNG SECTION C-A JOURNAL OF BIOSCIENCES}},
Year = {{2008}},
Volume = {{63}},
Number = {{9-10}},
Pages = {{639-643}},
Month = {{SEP-OCT}},
Abstract = {{In the context of the study on plant defensive compounds we have
   isolated the main alkaloid front Hippeastrum puniceum (Amaryllidaceae).
   3-O-acetyl-narcissidine (1), and its biological activities tested
   against two divergent insect species and several plant species. 1 was
   isolated from the bioactive alkaloidal fraction of H. puniceum. Its
   chemical structure was established by spectroscopic analysis. The
   biological activity tests showed that 1 is an antifeedant against the
   polyphagous insect Spodoptera littoralis but not against file olyphage
   Leptinotarsa decemlineata. Furthermore, the root growth of Amaranthus
   hypochondriacus, Rouboellia cochinchunensis, Panicum maximum and Solanum
   lycopersicum was significantly affected by 1. These results suggest a
   plant protective role for H. puniceum alkaloids.}},
