@article{ ISI:000258618200044,
Author = {Robledo-Martinez, Arturo and Sobral, Hugo and Ruiz-Meza, Alfredo},
Title = {{Effect of pressure in fast-front lightning discharges in air}},
Journal = {{IEEE TRANSACTIONS ON PLASMA SCIENCE}},
Year = {{2008}},
Volume = {{36}},
Number = {{4, 1}},
Pages = {{948-949}},
Month = {{AUG}},
Abstract = {{The effect of gas pressure on fast impulse discharges (70 us) in a short
   point-plane gap was investigated. Fast-frame photography allowed the
   tracking of the development of the discharge with high temporal
   resolution. At low pressures, the gap is initially bridged by a bright
   glow that can carry significant amounts of current. At higher pressures,
   the gap is initially filled with numerous filaments that transport
   little current. In this case, the current takes off only when the arc
   bridges the gap. In the interval t = 1-10 mu s, the arc exhibited a
   braided structure that rotated rapidly.}},
