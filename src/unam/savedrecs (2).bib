@article{ ISI:000445982100009,
Author = {Lopez-Ortiz, A. and Rodriguez-Ramirez, J. and Mendez-Lagunas, L. L. and
   Martynenko, A. and Pilatowsky-Figueroa, I},
Title = {{Non-isothermal drying of garlic slices (Allium sativum, L.): Wave period
   and initial temperature of the heating/cooling effect}},
Journal = {{FOOD AND BIOPRODUCTS PROCESSING}},
Year = {{2018}},
Volume = {{111}},
Pages = {{83-92}},
Month = {{SEP}},
Abstract = {{Intermittent drying is a common technique for food conservation. The
   objective of this work is to evaluate the effect of non-isothermal
   drying and the wave periods using the effective diffusivity (D-eff), the
   glass transition temperature (T-g), their relationship to moisture
   content and the temperature of the product, as parameters for
   controlling quality deterioration. The heating-cooling profile was
   developed as a trapezoidal wave form using three different wave periods
   (tau:60, 120 and 160 min), with a midpoint temperature (T-m) of 50
   degrees C and Delta T =10 degrees C. The ascending (AC) heating-cooling
   treatment began at 40 degrees C, and the descending (DC) heating-cooling
   treatment began at 60 degrees C. The drying temperature (T-a), the
   surface temperature of the garlic (T-s), the internal temperature of the
   garlic (T-i), and the weight of the garlic slices were measured and
   recorded in real time. The D-eff was calculated using the slope method.
   A higher drying rate was observed in the AC drying treatment. The T-g
   variations were associated with the T-i and moisture content of the
   garlic. The drying time (t(s)) for the AC and the DC heating-cooling
   profiles was found to be between 180 to 238 min. (C) 2018 Institution of
   Chemical Engineers. Published by Elsevier B.V. All rights reserved.}},
