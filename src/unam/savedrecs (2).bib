@article{ ISI:000451476700020,
Author = {Revueltas, Eugenia},
Title = {{Evocations from the Eye of the Storm}},
Journal = {{REVISTA MEXICANA DE CIENCIAS POLITICAS Y SOCIALES}},
Year = {{2018}},
Volume = {{63}},
Number = {{234}},
Pages = {{377-394}},
Month = {{SEP-DEC}},
Abstract = {{This paper is a partial account, from July 24 to October the 2nd, of the
   1968 Mexican student movement. This period is considered the first stage
   of the struggle. This writing interweaves factual description with
   documents from the period and subjective memories from the narrator's
   point of view, derived from the consideration that a peculiar,
