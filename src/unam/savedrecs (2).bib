@article{ ISI:000444929900004,
Author = {de la Iglesia, Manuel D. and Roman, Pablo},
Title = {{Some bivariate stochastic models arising from group representation
   theory}},
Journal = {{STOCHASTIC PROCESSES AND THEIR APPLICATIONS}},
Year = {{2018}},
Volume = {{128}},
