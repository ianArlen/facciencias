@article{ ISI:000444777500005,
Author = {Chavez-Saab, J. E. and Toledo, Genaro},
Title = {{Closing the gap on R-D{*} by including longitudinal effects}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2018}},
Volume = {{98}},
Number = {{5}},
Month = {{SEP 17}},
Abstract = {{Measurements of the R-D{*}  Br(B -> tau upsilon D{*})/Br(B -> e upsilon
   D{*}) parameter remain in tension with the standard model prediction,
   despite recent results helping to close the gap. The standard model
   prediction it is compared with considers the D{*} as an external
   particle, even though what is detected in experiments is a Dir pair it
   decays into, from which it is reconstructed. We argue that the
   experimental result must be compared with the theoretical prediction
   considering the full four-body decay (B -> l upsilon D{*}-> l upsilon D
   pi). We show that the longitudinal degree of freedom of the off shell
   D{*} helps to further close the disagreement gap with experimental data.
   We find values for the ratio R-D pi(l)  Br(B -> tau upsilon D-tau
   pi)/Br(B -> l upsilon D-l pi) of R-D pi(e) = 0.274 +/- 0.003 and R-D
   pi(mu) = 0.275 +/- 0.003, where the uncertainty comes from the
   uncertainty of the form factors parameters. Comparing against R-D pi
   reduces the gap with the latest LHCb result from 1.1 sigma to 0.48
   sigma, while the gap with the latest Belle result is reduced from 0.42
   sigma to just 0.10 sigma and with the world average results from 3.7
   sigma to 2.1 sigma.}},
