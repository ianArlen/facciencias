@article{ ISI:000448876400005,
Author = {Martinez Lopez, M. R. and Mendoza, C.},
Title = {{A Graphic Processing Unit (GPU) based implementation of an
   incompressitile two-phase flow model in porous media}},
Journal = {{GEOFISICA INTERNACIONAL}},
