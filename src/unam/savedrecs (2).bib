@article{ ISI:000448542700007,
Author = {Gonzalez-Galvez, Oscar and Morales, Miguel and Seefoo, Carla and
   Morillon, David and Valdes, Hugo},
Title = {{Energy Diagnosis of University Buildings: Renewable Energy Institute
   from UNAM}},
