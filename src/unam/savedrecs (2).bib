@article{ ISI:000465571900007,
Author = {Almanza-Miranda, Enory and Pina-Aguilar, Raul E. and Ordonez-Gutierrez,
   Eduardo and Teresa Gorraez-de la More, Ma and Pena-Velez, Ruben},
Title = {{Cholestasis due to deficiency of 3 beta-Delta 5-C27-hydroxysteroid
   dehydrogenase in a patient with altered bile acid synthesis}},
