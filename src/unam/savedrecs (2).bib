@article{ ISI:000444400200045,
Author = {Gomez-Velazquez, Laura S. and Hernandez-Gordillo, Agileo and Robinson,
   Matthew J. and Leppert, Valerie J. and Rodil, Sandra E. and Bizarro,
   Monserrat},
Title = {{The bismuth oxyhalide family: thin film synthesis and periodic
   properties}},
