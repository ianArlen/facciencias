@inproceedings{ ISI:000221643000077,
Author = {Navarro-Gonzalez, R and Molina, P and Mckay, CP},
Editor = {{Harris, RA and Ouwehand, L}},
Title = {{Organic characterization of Atacama soils along a north-to-south
   precipitation gradient}},
Booktitle = {{PROCEEDINGS OF THE III EUROPEAN WORKSHOP ON EXO-ASTROBIOLOGY: MARS: THE
   SEARCH FOR LIFE}},
Series = {{ESA SPECIAL PUBLICATIONS}},
