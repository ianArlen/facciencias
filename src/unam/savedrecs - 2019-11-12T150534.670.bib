@article{ ISI:000187460500010,
Author = {Poindron, P and Terrazas, A and Hernandez, H},
Title = {{Exclusive mother-young bonding in sheep and goats: Physiological
   determinants and consequences}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{2}},
Pages = {{265-281}},
Month = {{DEC}},
Abstract = {{In sheep and goats, the onset of maternal behavior at parturition is
   controlled by hormonal changes in the female and fetus expulsion. This
   activation is also facilitated by maternal experience. Neurobiological
   changes induce temporary maternal responsiveness (sensitive period) and
   exclusive maternal bonding towards the neonate, through the memorization
   of its individual odor. Mothers also rapidly develop visual and/or
   auditory recognition of their baby. The neonate establishes an early
   preference for its mother on the basis of her acceptance behavior, and
   the first sucklings are important in this process. These studies
   underline the importance of the physiological and behavioral events of
   the peripartum period for the harmonious development of mutual
   recognition and mother-infant relationships in mammals in general.}},
Publisher = {{SOCIEDAD MEXICANA PSICOLOGIA}},
Address = {{APARTADO POSTAL 22-211, TLALPAN 14000, MEXICO}},
Type = {{Review}},
Language = {{English}},
Affiliation = {{Poindron, P (Reprint Author), Apartado Postal 1-1141, Queretaro 76001, Mexico.
   Univ Nacl Autonoma Mexico, Inst Neurobiol, Mexico City 04510, DF, Mexico.}},
ISSN = {{0185-6073}},
