@article{ ISI:000187241200028,
Author = {Gonzalez, C and Alvarez, O and Genesca, J and Juarez-Islas, JA},
Title = {{Solidification of chill-cast Al-Zn-Mg alloys to be used as sacrificial
   anodes}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS A-PHYSICAL METALLURGY AND
   MATERIALS SCIENCE}},
Year = {{2003}},
Volume = {{34A}},
Number = {{12}},
Pages = {{2991-2997}},
Month = {{DEC}},
Abstract = {{In the present research, Al-Zn-Mg alloys were vacuum induction melted
   and gravity cast into steel molds. Ingots were microstructurally and
   electrochemically characterized to evaluate their performance as
   Al-sacrificial anodes for cathodic protection of structures exposed to
   marine environments. The microstructure observed in as-cast ingots
   consisted mainly of alpha-Al dendrites with 0.68 to 2.25 vol pct of tau
   phase in alpha-Al matrix and eutectic in interdendritic regions. After
   heat treatment, the presence of the tau phase increased up to 5 vol pct.
   Electrochemical efficiencies obtained in Al alloys showed maximum values
   of 73 and 87 pct in as-cast ingots and heat-treated ingots,
   respectively. In order to contribute to the development of Al-Zn-Mg
   anodes, the Al-5.3 at. pct Zn-6.2 at. pct Mg (Al-12 wt pct Zn-5.4 wt pct
   Mg) alloy was monitored to identify the temperature changes as it cools
   through phase transformation intervals. Growth temperatures of the
   phases present in this alloy were employed to predict the structure
   growing at fixed growth velocity. Predictions of variation of solute
   concentration with growth velocity in alpha-Al dendrites were included,
   too. The results of these analyses help to select alloy composition and
   to control microstructure in order to develop a new generation of
   Al-sacrificial anodes free of In and Hg.}},
Publisher = {{SPRINGER}},
Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Gonzalez, C (Reprint Author), Univ Nacl Autonoma Mexico, Fac Chem, Circuito Escolar S-N,Cd Univ, Mexico City 04510, DF, Mexico.
   Univ Nacl Autonoma Mexico, Fac Chem, Mexico City 04510, DF, Mexico.
   Univ Nacl Autonoma Mexico, Mat Res Inst, Mexico City 04510, DF, Mexico.}},
DOI = {{10.1007/s11661-003-0198-6}},
