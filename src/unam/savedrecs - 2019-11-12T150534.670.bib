@article{ ISI:000186251000002,
Author = {Sanchez-Cruz, H and Bribiesca, E},
Title = {{A method of optimum transformation of 3D objects used as a measure of
   shape dissimilarity}},
Journal = {{IMAGE AND VISION COMPUTING}},
Year = {{2003}},
Volume = {{21}},
Number = {{12}},
Pages = {{1027-1036}},
Month = {{NOV 1}},
Abstract = {{In this work, we present a method which transforms an object into
   another. The computation of this transformation is used as a measure of
   shape-of-object dissimilarity. The considered objects are composed of
   voxels. Thus, the shape difference of two objects can be ascertained by
   counting how many voxels we have to move and how far to change one
   object into another. This work is based on the method presented in
   {[}Pattern Recognition 29 (1996) 1117], and our contributions to such a
   work are a method of optimum transformation of objects and a proposed
   method of principal axes, which is used to orientate objects. The
   proposed method is applied to global data. Finally, we present some
   results using objects of the real world. (C) 2003 Elsevier B.V. All
   rights reserved.}},
