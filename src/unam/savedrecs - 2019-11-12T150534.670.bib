@inproceedings{ ISI:000224688300741,
Author = {Verde, C and Visairo, N},
Book-Group-Author = {{acc}},
Title = {{Identificability of multi-leaks in a pipeline}},
Booktitle = {{PROCEEDINGS OF THE 2004 AMERICAN CONTROL CONFERENCE, VOLS 1-6}},
