@article{ ISI:000187460500008,
Author = {Pineda, ER and Garcia, OR and Cardenas, GM},
Title = {{A comparison between programs in beginning readers}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{2}},
Pages = {{247-253}},
Month = {{DEC}},
Abstract = {{Four programs for beginning readers were compared in order to assess the
   executions promoted in each. Twenty first-grade elementary school
   children participated, who were equivalently assigned to four different
   groups. At phase 1, the functional background of written language was
   assessed. At phase 11, three groups were trained at three different
   levels: context, selective, and reference. One group worked as control.
   At phase III, the abilities assessed were: reading fluency, card
   arrangement, and answering literal and inference questions. The tasks
   that significantly discriminated the groups were: answering to literal
   (p<0.05) and inference questions (p<0.01), while in reading fluency and
   card arrangement no significant differences were found. Results are
   discussed in terms of practical implications, as derived from the
   interbehavioral model.}},
