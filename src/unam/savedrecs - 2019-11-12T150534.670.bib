@article{ ISI:000186885100018,
Author = {Lucas, PW and Dominy, NJ and Riba-Hernandez, P and Stoner, KE and
   Yamashita, N and Loria-Calderon, E and Petersen-Pereira, W and
   Rojas-Duran, Y and Salas-Pena, R and Solis-Madrigal, S and Osorio, D and
   Darvell, BW},
Title = {{Evolution and function of routine trichromatic vision in primates}},
Journal = {{EVOLUTION}},
Year = {{2003}},
Volume = {{57}},
Number = {{11}},
Pages = {{2636-2643}},
Month = {{NOV}},
Abstract = {{Evolution of the red-green visual subsystem in trichromatic primates has
   been linked to foraging advantages, namely the detection of either ripe
   fruits or young leaves amid mature foliage. We tested competing
   hypotheses globally for eight primate taxa: five with routine
   trichromatic vision, three without. Routinely trichromatic species
   ingested leaves that were ``red shifted{''} compared to background
   foliage more frequently than species lacking this trait. Observed
   choices were not the reddest possible, suggesting a preference for
   optimal nutritive gain. There were no similar differences for fruits
   although red-greenness may sometimes be important in close-range fruit
   selection. These results suggest that routine trichromacy evolved in a
   context in which leaf consumption was critical.}},
