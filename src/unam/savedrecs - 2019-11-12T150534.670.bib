@article{ ISI:000187510100006,
Author = {Huerta-Cerdan, A and de la Rosa, JM and Gonzalez, CA and Genesca, J},
Title = {{Cyclic voltammetry and dielectric studies on PbS-potassium ethyl
   xanthate-dextrine system under flotation and depression conditions}},
