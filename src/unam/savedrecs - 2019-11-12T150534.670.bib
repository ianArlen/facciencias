@article{ ISI:000224355400004,
Author = {Osuna, J and Yanez, J and Soberon, X and Gaytan, P},
Title = {{Protein evolution by codon-based random deletions}},
Journal = {{NUCLEIC ACIDS RESEARCH}},
Year = {{2004}},
Volume = {{32}},
Number = {{17}},
Abstract = {{A method to delete in-phase codons throughout a defined target region of
   a gene has been developed. This approach, named the codon-based random
   deletion (COBARDE) method, is able to delete complete codons in a random
   and combinatorial mode. Robustness, automation and fine-tuning of the
   mutagenesis rate are essential characteristics of the method, which is
   based on the assembly of oligonucleotides and on the use of two
   transient orthogonal protecting groups during the chemical synthesis.
   The performance of the method for protein function evolution was
   demonstrated by changing the substrate specificity of TEM-1
   beta-lactamase. Functional ceftazidime-resistant beta-lactamase variants
   containing several deleted residues inside the catalytically important
   omega-loop region were found. The results show that the COBARDE method
   is a useful new molecular tool to access previously unexplorable
   sequence space.}},
