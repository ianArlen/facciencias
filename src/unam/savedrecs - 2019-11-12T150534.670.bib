@article{ ISI:000187318000007,
Author = {Cardenas, G and Taboada, E and Bravo, A and Miranda, SP},
Title = {{SEM-EDX studies of chitosan derivatives-metal adducts}},
Journal = {{JOURNAL OF THE CHILEAN CHEMICAL SOCIETY}},
Year = {{2003}},
Volume = {{48}},
Number = {{4}},
Pages = {{49-53}},
Month = {{DEC}},
Abstract = {{Chitosan was obtained from shrimps shells (pleuroncodes monodon) using
   chemical methods.
   A series of chitosan (QS)charged with solution of copper, cobalt, nickel
   and mercury ions were prepared at room temperature using the batch
   method.
   N-3,5-diethylamino benzoyl chitosan (QDAB); N,O-dimercapto succinate
   chitosan (QNOT) and 4-aminobenzoate chitosan (QAB) derivatives were
   prepared. The chitosan metal adducts with Cu, Co, Ni and Hg ions and
   derivatives maximum loading is discussed.
   Chitosan and derivatives containing copper were analyzed by SEM to find
   out the morphology of the polymers. Chitosan-Cu and derivatives charged,
   QDAB-Ni and QDAB-Hg were analyzed by EDX to verify the presence of the
   metal in the polymeric chain.}},
