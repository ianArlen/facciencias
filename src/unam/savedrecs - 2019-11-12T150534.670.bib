Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Bargas, J (Reprint Author), Univ Nacl Autonoma Mexico, Inst Fisiol Celular, Dept Biophys, POB 70-253, Mexico City 04510, DF, Mexico.
   Univ Nacl Autonoma Mexico, Inst Fisiol Celular, Dept Biophys, Mexico City 04510, DF, Mexico.
   Northwestern Univ, Feinberg Sch Med, Dept Physiol, Chicago, IL 60611 USA.}},
DOI = {{10.1113/jphysiol.2003.050799}},
ISSN = {{0022-3751}},
