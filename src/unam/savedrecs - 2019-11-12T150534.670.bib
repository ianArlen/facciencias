@article{ ISI:000187460500005,
Author = {Gomez-Maqueo, EL and Eguiarte, BEB and Patino, CD},
Title = {{Psychometric qualities of the Life-events Questionnaire}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{2}},
Pages = {{211-223}},
Month = {{DEC}},
Abstract = {{The objective of this study was to assess the validity and reliability
   of the Life Events Questionnaire for Adolescents. Participants were 1571
   adolescents from private and public junior and senior high-school
   students from Mexico City and the surrounding areas. Internal
   consistency coefficients from .61 to .87 were obtained from each of the
   questionnaire areas, with a total alpha coefficient of .89. Student's
   t-test showed significant differences in familiar, social, personal, and
   behavior problems between clinical and non-clinical subjects. Agreement
   percentages between judges of more than 80\% were also obtained.
   Logistic regression data showed that the questionnaire discriminated
   between normal and clinical sample. Results indicate that the
   questionnaire can be used in the assessment and detection of adolescents
   at risk with confidence. It is suggested to continue studying with other
   samples, in order to analyze other areas of the questionnaire.}},
