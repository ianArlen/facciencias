@article{ ISI:000187460500004,
Author = {Lopez, GC and Guzman, LH and Sierra, AV and Meza, V},
Title = {{Training in coping and pro-social competence abilities in homeless
   youngsters}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{2}},
Pages = {{201-209}},
Month = {{DEC}},
Abstract = {{The purpose of this study was to adapt, instrument and evaluate a
   training program of coping skills and prosocial communication, whose
   goals were to decrease achievement, personal, and home problems.
   Participants were seven teenagers between the ages of 16 and 2 1, two
   men and five women with a homeless history, low achievement and family
   disfunction. The intervention program considered four programmatic
   components in the form of exercises discovery, meeting, established
   rules, and plan (PAPs). The results obtained from a multiple baseline
   design through scenarios show a decrease in the behaviors reported by
   subjects in all three scenarios; however, it is necessary to have
   collateral measures to confirm those data obtained from teenagers'
   self-reports.}},
