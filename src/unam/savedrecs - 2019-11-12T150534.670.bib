@article{ ISI:000186357000009,
Author = {Gutierrez-Rios, RM and Rosenblueth, DA and Loza, JA and Huerta, AM and
   Glasner, JD and Blattner, FR and Collado-Vides, J},
Title = {{Regulatory network of Escherichia coli: Consistency between literature
   knowledge and microarray profiles}},
