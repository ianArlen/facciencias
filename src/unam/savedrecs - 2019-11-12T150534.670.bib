@article{ ISI:000187234100001,
Author = {Turbiner, AV},
Title = {{Exotic molecular states in a strong magnetic field: H-3((2+)) ion}},
Journal = {{PHYSICS OF ATOMIC NUCLEI}},
Year = {{2003}},
Volume = {{66}},
Number = {{11}},
Pages = {{1953-1963}},
Month = {{NOV}},
Note = {{International Seminar on Modern Trends in Classical Approaches, MOSCOW,
   RUSSIA, SEP 30-OCT 01, 2002}},
Abstract = {{The existence of the molecular ion H-3((2+)) in a magnetic field in a
   triangular and a linear configuration is discussed. A variational method
   (with an optimization of the form of the vector potential) is used. It
   is shown that, in the range of magnetic fields 10(8) < B < 10(11) G, the
   system (pppe), with the protons forming an equilateral triangle
   perpendicular to the magnetic line, has a well-pronounced minimum in the
   total energy. Also, for B greater than or similar to 10(10) G, if the
   Protons are situated along a magnetic line (linear configuration), a
   well-pronounced minimum in the total energy appears. Both configurations
   are unstable under the decays H(atom) + p + p and H-2((+)) + p. A
   possible connection between the H-3((2+)) molecular ion and a recently
   discovered absorption feature in a neutron-star atmosphere is discussed.
   (C) 2003 MAIK ``Nauka/Interperiodica{''}.}},
