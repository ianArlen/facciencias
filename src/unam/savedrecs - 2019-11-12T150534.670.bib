@article{ ISI:000187460500011,
Author = {Lopez, MRAG and Saldivar, NLG and Gomez-Romero, JG and Ramirez, PA and
   Cruz-Morales, SE},
Title = {{Modulation of striatal gabaergic activity on the scopalamine induced
   amnesia}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{2}},
Pages = {{283-289}},
Month = {{DEC}},
Abstract = {{Memory is facilitated by acetylcholine (ACh) and impaired by gamma-amino
   butyric acid (GABA). Intrastriatal cholinergic blockade after training
   induces retrograde amnesia. Striatal interneurons contain ACh and GABA,
   and an interaction between these systems in the modulation of memory has
   been proposed, but the role of different types of GABA receptors in this
   interaction is not clear. This experiment assessed the effect of
   blockade and stimulation of intrastriatal GABA(B) receptors on the
   amnesia induced by scopolamine in an inhibitory avoidance task.
   Independent groups of rats received intrastriatal injections of
   scopolamine, baclofen, phaclofen or the combination of scopolamine with
   both. Scopolamine and baclofen produced retrograde amnesia, while the
   gabaergic antagonist, phaclofen, reverted scopolamine-induced amnesia.
   These data support the hypothesis that GABA has an inhibitory effect on
   the cholinergic activity of striatum in memory modulation.}},
