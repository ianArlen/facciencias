@article{ ISI:000186808200003,
Author = {Flores-Acosta, M and Sotelo-Lerma, M and Arizpe-Chavez, H and
   Castillon-Barraza, FF and Ramirez-Bon, R},
Title = {{Excitonic absorption of spherical PbS nanoparticles in zeolite A}},
Journal = {{SOLID STATE COMMUNICATIONS}},
Year = {{2003}},
Volume = {{128}},
Number = {{11}},
Pages = {{407-411}},
Month = {{DEC}},
Abstract = {{We report the optical and structural properties of PbS nanoparticles in
   zeolite A. The samples were obtained by sulfidation Of the Pb2+
   ion-exchanged zeolite in a thiourea solution at 50 degreesC. The optical
   properties of the samples were studied by diffuse reflectance
   spectroscopy. Their crystalline structure and morphology were Studied by
   electron diffraction and by transmission and scanning electron
   microscopy. The results show that the nanoparticles are not inside the
   zeolite cages but outside, embedded in the zeolite matrix. Exciton
   absorption peaks at much higher energy than the fundamental absorption
   edge of bulk PbS indicate quantum confinement effects in the spherical
   shape nanoparticles as a consequence of their small size. (C) 2003
   Elsevier Ltd. All rights reserved.}},
