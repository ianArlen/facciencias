@article{ ISI:000188747700022,
Author = {Fransen, C and Pietralla, N and Linnemann, A and Werner, V and Bijker, R},
Title = {{Low-spin gamma-ray spectroscopy of the (critical-point?) nucleus Ba-122}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{2004}},
Volume = {{69}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{The nucleus Ba-122 possesses an excitation energy ratio
   R-4/2=E(4(1)(+))/E(2(1)(+))=2.90 matching exactly the value predicted by
   the recently discussed X(5) solution of the Bohr-Hamiltonian. Besides
   the ground state band, other low-spin positive-parity bands of Ba-122
   are unknown hampering detailed comparison to the theory. Excited
   low-spin states of Ba-122 have been studied by in-beam
   gammagamma-coincidence spectroscopy with the Sn-112(C-12,2n)Ba-122
   reaction. A level structure was observed which we interpret as a part of
   the gamma band of Ba-122.}},
