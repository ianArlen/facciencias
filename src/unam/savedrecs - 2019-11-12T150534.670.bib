@article{ ISI:000227239900001,
Author = {Schulze, CH and Keppie, JD and Ortega-Rivera, A and Ortega-Gutierrez, F
   and Lee, JKW},
Title = {{Mid-Tertiary cooling ages in the Precambrian Oaxacan Complex of southern
   Mexico: indication of exhumation and inland arc migration}},
Journal = {{REVISTA MEXICANA DE CIENCIAS GEOLOGICAS}},
Year = {{2004}},
Volume = {{21}},
Number = {{2}},
Pages = {{203-211}},
Abstract = {{In southern Mexico, the similar to1 Ga Oaxacan Complex is in fault
   contact with the Hesozoic-Cenozoic Chatino terrane. Ar-40/Ar-39 dating
   of minerals from hornblende gneiss and quartz monzonitic gneiss in the
   southern Oaxacan Complex collected, 10 km and 1 km north of the Oaxaca
   Xolapa boundary, yielded the following data, respectively: (1) a plateau
   age of 584 +/- 10 Ma in hornblende and a pseudoplateau age of 23 +/- 3
   Ma in biotite: and (2) a plateau age of 42 +/- 3 Ma in biotite, and a
   maximum age of 36 +/- 1 Ma in K-feldspar. These are inferred to date
   cooling through similar to500-550 degreesC for hornblende, similar to280
   degreesC for biotite, similar to220 degreesC for plagioclase and similar
   to310-270 degreesC for K-feldspar The similar to582 Alfa age is much
   Younger than cooling ages from the northern Oaxacan Complex, suggesting
   that it records resetting during a Neoproterozoic Brasiliano
   tectonothermal event. On the other hand, the Tertiary cooling ages
   suggest reheating adjacent to either similar to40 Ma or 35-25 Ma
   Tertiary plutons: similar to42 Ma and similar to23 Ma biolite ages
   respectively. The former was followed by rapid cooling through similar
   to310-270 degreesC by similar to36 Ma and exhumation before deposition
   of Miocene volcanic rocks. We relate these cooling ages to northward
   migration of the magmatic arc during the Oligocene-Miocene as a
   consequence of flattening of the subduction zone due to subduction
   erosion.}},
