@article{ ISI:000187460500006,
Author = {Mondragon, L and Monroy, Z and Medina-Mora, ME and Borges, G},
Title = {{Life events, depression, alcohol consumption and suicide ideation in
   emergency rooms: A gender approach}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{2}},
Pages = {{225-235}},
Month = {{DEC}},
Abstract = {{This study sought to examine the differences between males and females
   regarding suicide ideation as related to life events, depression and
   alcohol consumption. Patients aged 18-65 years old were interviewed at
   admission in one of three emergency rooms (ER). Results revealed a 5.7\%
   prevalence of suicide ideation, 66.3\% of them were women, and 45.3\% of
   ideators fluctuated between 18 and 29 years of age. Suicide ideation was
   higher among separated/divorced patients. Differences among males and
   females in alcohol consumption and life events were found. It was
   concluded that suicide prevention programs should consider differences
   in prevalence, as well as similarities in risk factors for suicide
   ideation in males and females. Gender roles in suicide ideation are
   discussed.}},
