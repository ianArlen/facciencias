@article{ ISI:000187460500001,
Author = {Velasco, CS and Corral, EL},
Title = {{Public goods dilemma in dwelling housing: An integrative perspective}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{2}},
Pages = {{165-175}},
Month = {{DEC}},
Abstract = {{This paper deals with the study of public goods dilemma, particularly in
   a communitarian dwelling housing situation, from an integration
   information theory perspective. An experimental situation based on a
   factorial design of 6 (quantity of money contribution by neighbors){*} 5
   (percent of neighbors ready to contribute) was prepared. Students of
   three cities (Barcelona, Chihuahua and Mexico City) participated in this
   study. A multiplicative integration pattern was identified. Neighbors'
   monetary contribution directly affects the subjects' monetary
   contribution in all three samples. Percent of neighbors exerts influence
   only in a context of a high percent of neighbors ready to contribute.
   Data are discussed on the basis of the implications of integration
   information theory in the context of social dilemmas.}},
