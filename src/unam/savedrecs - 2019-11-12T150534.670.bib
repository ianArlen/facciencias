@inproceedings{ ISI:000226379100003,
Author = {Flandes, A and Maravilla, D},
Editor = {{Lebreton, JP and Agee, CB and Stabekis, PD and Bernhardt, P and Ganguli, GI}},
Title = {{Dust escape mechanism from Io}},
Booktitle = {{SCIENTIFIC EXPLORATION, PLANETARY PROTECTION, ACTIVE EXPERIMENTS AND
   DUSTY PLASMAS}},
