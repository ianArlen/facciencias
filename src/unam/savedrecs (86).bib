@article{ ISI:000270893600008,
Author = {Zuriaga-Monroy, Carolina and Martinez-Magadan, Jose-Manuel and Ramos,
   Estrella and Gomez-Balderas, Rodolfo},
Title = {{A DFT study of the electronic structure of cobalt and nickel
   mono-substituted MoS2 triangular nanosized clusters}},
Journal = {{JOURNAL OF MOLECULAR CATALYSIS A-CHEMICAL}},
Year = {{2009}},
Volume = {{313}},
Number = {{1-2}},
Pages = {{49-54}},
Month = {{NOV 2}},
Abstract = {{Molybdenum disulfide nanoparticles are of interest for their extensive
   use in heterogeneous catalysis. Here, we report a systematic density
   functional theory study carried out to investigate the electronic
   effects of Co(Ni) mono-substitutions on triangular molybdenum sulfide
   models of nanometric scale. On the basis of the electronic structure,
   the charge distribution and the Delta(E-LUMO - E-HOMO) gap analysis, the
   triangle molecular model with nickel substitution is identified as more
   favorable for inducing the best catalytic performance. Nickel
   consistently induces stronger electronic rearrangements than cobalt, on
   the molybdenum first-neighbor atoms, which are connected with its higher
   promoting effect. Charge distribution analysis points out a chemical
   reduction on the molybdenum sites when the cluster is doped. Moreover,
   nickel substitution produces smaller Delta(E-LUMO - E-HOMO) gaps than
   cobalt substitutions, revealing that Ni-doped clusters are more
   reactive. (C) 2009 Elsevier B.V. All rights reserved.}},
