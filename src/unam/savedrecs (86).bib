@article{ ISI:000284811300005,
Author = {Ramirez, Elizabeth and Robles, Esperanza and Guadalupe Sainz, Ma and
   Ayala, Reynaldo and Campoy, Emelia},
Title = {{Microbiological quality of the Zacatepec aquifer, Morelos, Mexico}},
Journal = {{REVISTA INTERNACIONAL DE CONTAMINACION AMBIENTAL}},
