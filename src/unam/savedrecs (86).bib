@article{ ISI:000268364100020,
Author = {Portillo, Wendy and Paredes, Raul G.},
Title = {{Conditioned place preference induced by morphine in non-copulating male
   rats}},
Journal = {{BEHAVIOURAL BRAIN RESEARCH}},
Year = {{2009}},
Volume = {{203}},
Number = {{2}},
Pages = {{308-311}},
Month = {{NOV 5}},
Abstract = {{Non-copulators (NC) are normal males that fail to display sexual
   behavior, we evaluate if NC male rats have a functional general reward
   system. NC male rats were injected with morphine to determine if a
   reward state, as evaluated by the conditioned place preference (CPP)
   paradigm, could be induced. Our results demonstrate that NC males
   develop CPP to a morphine injection, indicating that their general
   reward system is functional. (C) 2009 Elsevier B.V. All rights reserved.}},
