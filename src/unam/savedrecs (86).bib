@article{ ISI:000273064400004,
Author = {Ayala, Alejandro and Bashir, Adnan and Raya, Alfredo and Sanchez, Angel},
Title = {{Chiral phase transition in relativistic heavy-ion collisions revisited:
   toward a description of weak magnetic fields effects}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2009}},
Volume = {{55}},
Number = {{2}},
Pages = {{13-17}},
Month = {{DEC}},
Note = {{32nd Symposium on Nuclear Physics, Cocoyoc, MEXICO, JAN 05-08, 2009}},
Abstract = {{It has recently been pointed out that in peripheral collisions of heavy
   nuclei at high energies, a sizable magnetic field is produced in the
   interaction region. This fact opens up the possibility to study the
   chiral phase transition in a quark gluon plasma in the presence of a
   magnetic field. Here we work in the linear sigma model to compute the
   finite-temperature effective potential. We give an account Only LIP to
   one-loop order where we show that in the weak field case, the effects of
   the magnetic field are subdominant. We argue that the magnetic field
   effects will show up when including the next order correction, the so
   called ring diagrams.}},
