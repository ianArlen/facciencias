@article{ ISI:000275008500005,
Author = {Flores-Villela, Oscar and Smith, Eric N.},
Title = {{A NEW SPECIES OF CONIOPHANES (SQUAMATA: COLUBRIDAE), FROM THE COAST OF
   MICHOCAN, MEXICA}},
Journal = {{HERPETOLOGICA}},
Year = {{2009}},
Volume = {{65}},
Number = {{4}},
Pages = {{404-412}},
Month = {{DEC}},
Abstract = {{We describe a new species of colubrid snake of the genus Coniophanes
   from the Pacific coast of Michoacan, Mexico. This species is known from
   a single adult female collected near sea level in dry deciduous forests.
   The new species is most similar to C, piceivittis, C. schmidti and C.
   taylori. It shares with these three species 25 scale rows at midbody, a
   small sub-procular scale, and a pattern of three dark brown stripes over
   a pale brown body. The new species differs most noticeably from the
   other three in possessing a narrower dark vertebral stripe. Coniophanes
   p. taylori is elevated to a species status, therefore the C. Picevittis
   group consists of four species.}},
