@article{ ISI:000273142700011,
Author = {Angeleri Hugel, Lidia and Antonio De La Pena, Jose},
Title = {{LOCALLY FINITELY GENERATED MODULES OVER RINGS WITH ENOUGH IDEMPOTENTS}},
Journal = {{JOURNAL OF ALGEBRA AND ITS APPLICATIONS}},
Year = {{2009}},
Volume = {{8}},
Number = {{6}},
Pages = {{885-901}},
Month = {{DEC}},
Abstract = {{Let R = circle plus Re-lambda = circle plus e(lambda)R be an associative
   ring with enough idempotents indexed over a possibly infinite set
   Lambda. Assume that \{e(lambda) : lambda is an element of Lambda\} is a
   set of pairwise orthogonal primitive idempotents, and that R is locally
   bounded, that is, the projective modules e(lambda)R and Re-lambda are of
   finite length for each lambda is an element of Lambda. We prove the
   existence of almost split sequences ending at the indecomposable
   finitely generated non-projective unital R-modules. Moreover, we
   consider the unital R-modules X that are locally finitely generated,
   that is, Xe-lambda is a finitely generated e(lambda)Re(lambda)-module
   for all lambda is an element of Lambda. We show that such X accept
   perfect decompositions X = circle plus X-i as direct sums of
   indecomposable modules.}},
