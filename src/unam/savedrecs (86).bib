@article{ ISI:000272704800009,
Author = {Hinojosa-Torres, J.},
Title = {{High resolution images of microstructure of TIG welded Zn-22Al-2Cu alloy
   plates}},
Journal = {{MATERIALS TECHNOLOGY}},
Year = {{2009}},
Volume = {{24}},
Number = {{4}},
Pages = {{244-246}},
Month = {{DEC}},
Abstract = {{By applying the tungsten inert gas welding process, plates of the
   Zn-22Al-2Cu (wt-\%) alloy were welded. With the aid of a scanning
   electron microscope, high resolution images of microstructures in the
   welded zone were obtained. The images showed the presence of subgrains
   inside the dendrites (in the fusion zone) and inside the equiaxed grains
   (in the heat affected zone). Inside the subgrains, a pearlitic
   microstructure (lamellar or granular) was observed. X-ray diffraction
   analysis revealed that the alpha, alpha(S)', alpha(T)', eta, eta(S)',
   eta(T)' and e crystalline phases are present in the microstructure. The
   elemental analysis by energy dispersive X-ray spectroscopy showed that
   the lamellar microstructure is composed by lamellas of the alpha(T)' and
   eta(T)' phases. In the interdendritic region, elemental analysis showed
   the presence of the eta(S)', eta, alpha and e phases. In the
   intergranular region the elemental analysis showed the presence of the
   eta(T)', alpha, alpha(T)' and e phases.}},
