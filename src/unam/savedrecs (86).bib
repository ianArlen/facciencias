@article{ ISI:000269853000006,
Author = {Reolid, Matias and Miguel Molina, Jose and Loeser, Hannes and Navarro,
   Vicente and Ruiz-Ortiz, Pedro A.},
Title = {{Coral biostromes of the Middle Jurassic from the Subbetic (Betic
   Cordillera, southern Spain): facies, coral taxonomy, taphonomy, and
   palaeoecology}},
