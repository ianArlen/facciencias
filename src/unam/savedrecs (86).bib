@article{ ISI:000271436600011,
Author = {Javier Bejarano, Francisco and Fridman, Leonid M. and Poznyak, Alexander
   S.},
Title = {{Output Integral Sliding Mode for Min-Max Optimization of Multi-Plant
   Linear Uncertain Systems}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
