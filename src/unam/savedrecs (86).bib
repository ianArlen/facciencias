@article{ ISI:000272641200008,
Author = {Pandarinath, Kailasa},
Title = {{Evaluation of geochemical sedimentary reference materials of the
   Geological Survey of Japan (GSJ) by an objective outlier rejection
   statistical method}},
Journal = {{REVISTA MEXICANA DE CIENCIAS GEOLOGICAS}},
