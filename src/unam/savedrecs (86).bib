@article{ ISI:000283527800013,
Author = {Uribe Bravo, Geronimo},
Title = {{The falling apart of the tagged fragment and the asymptotic
   disintegration of the Brownian height fragmentation}},
Journal = {{ANNALES DE L INSTITUT HENRI POINCARE-PROBABILITES ET STATISTIQUES}},
Year = {{2009}},
Volume = {{45}},
