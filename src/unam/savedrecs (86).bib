@article{ ISI:000271824600055,
Author = {Arroyo-Cabanas, F. G. and Aguillon-Martinez, J. E. and Ambriz-Garcia, J.
   J. and Canizal, G.},
Title = {{Electric energy saving potential by substitution of domestic
   refrigerators in Mexico}},
Journal = {{ENERGY POLICY}},
Year = {{2009}},
Volume = {{37}},
Number = {{11}},
Pages = {{4737-4742}},
Month = {{NOV}},
Abstract = {{We evaluate the potential for electric power saving in Mexico that would
   follow the substitution of old, low efficiency domestic refrigerators
   with modern, high efficiency ones. Our results indicate that a total
   substitution of domestic refrigerators would save 4.7 TWh/year, which
   represents 33\% of the annual total consumption of 14.1 TWh. Assuming an
   average daily use of 14 h for domestic refrigeration, 900 MW of
   electricity would be saved. An annual substitution of 20\% over 5 years
   would save approximately 1 TWh a year and almost 180 MW would be
   released. It is recommended that this program of replacement should be
   supported by the Federal and State governments, beginning with the
   States (which have more direct influence), and with the oldest, least
   efficient refrigerators along the lines followed in programs that
   introduced fluorescent compact lamps. (C) 2009 Elsevier Ltd. All rights
   reserved.}},
