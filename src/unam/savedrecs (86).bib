@article{ ISI:000271257100006,
Author = {Turcio-Ortega, D. and Rodil, S. E. and Muhl, S.},
Title = {{Corrosion behavior of amorphous carbon deposit in 0.89\% NaCl by
   electrochemical impedance spectroscopy}},
Journal = {{DIAMOND AND RELATED MATERIALS}},
Year = {{2009}},
Volume = {{18}},
Number = {{11}},
Pages = {{1360-1368}},
Month = {{NOV}},
Abstract = {{Amorphous carbon films were deposited on SS316L substrates using a DC
   magnetron sputtering system. aiming at the application of the coated
   SS316L for biomedical implants. The biocompatibility and chemical
   stability of the carbon layers have been previously demonstrated. The
   films were deposited on top of sputtered titanium coatings introduced as
   a buffer layer to enhance film-substrate adhesion. The corrosion
   resistance of the a-C/Ti/SS316L systems was investigated by
   electrochemical techniques. The electrolyte used in this work was 0.89
   wt.\% NaCl at pH 7.4. which simulates body fluid ionic concentrations.
   The coated samples displayed corrosion resistance values in the saline
   solution much higher than the stainless steel substrates and the role of
   the Ti coating thickness was analysed in order to determine the optimal
   system for biological applications. (C) 2009 Elsevier B.V. All rights
   reserved.}},
