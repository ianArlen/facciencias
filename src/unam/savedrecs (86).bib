@article{ ISI:000273017700014,
Author = {Perez-Morales, Deyanira and Ostoa-Saloma, Pedro and Espinoza, Bertha},
Title = {{Trypanosoma cruzi SHSP16: Characterization of an alpha-crystallin small
   heat shock protein}},
Journal = {{EXPERIMENTAL PARASITOLOGY}},
