@article{ ISI:000272311400072,
Author = {Kunold, A. and Torres, M.},
Title = {{Nonlinear transport theory for negative-differential resistance states
   of two-dimensional electron systems in strong magnetic fields}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2009}},
Volume = {{80}},
Number = {{20}},
Month = {{NOV}},
Abstract = {{We present a model to describe the nonlinear response to a direct dc
   current applied to a two-dimensional electron system in a strong
   magnetic field. The model is based on the solution of the von Neumann
   equation incorporating the exact dynamics of two-dimensional damped
   electrons in the presence of arbitrarily strong magnetic and dc electric
   fields while the effects of randomly distributed impurities are
   perturbatively added. From the analysis of the differential resistivity
   and the longitudinal voltage, we observe the formation of
   negative-differential resistivity states that are the precursors of the
   zero-differential resistivity states. Both the effects of elastic
   impurity scattering as well as those related to inelastic processes play
   an important role. The theoretical predictions correctly reproduce the
   main experimental features provided that the inelastic-scattering rate
   obeys a T-2 temperature dependence, consistent with electron-electron
   interaction effects.}},
