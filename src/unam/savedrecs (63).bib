Author = {Adrian Perez-Crespo, Victor and Arroyo-Cabrales, Joaquin and Benammi,
   Mouloud and Johnson, Eileen and Polaco, Oscar J. and Santos-Moreno,
   Antonio and Morales-Puente, Pedro and Cienfuegos-Alvarado, Edith},
Title = {{Geographic variation of diet and habitat of the Mexican populations of
   Columbian Mammoth (Mammuthus columbi)}},
Journal = {{QUATERNARY INTERNATIONAL}},
Year = {{2012}},
