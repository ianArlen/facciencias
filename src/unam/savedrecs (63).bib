@article{ ISI:000312211000006,
Author = {Luongo, Orlando and Bonanno, Luca and Iannone, Gerardo},
Title = {{SECOND-ORDER INVARIANTS AND HOLOGRAPHY}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS D}},
Year = {{2012}},
Volume = {{21}},
Number = {{12}},
Month = {{NOV}},
Abstract = {{Motivated by recent works on the role of the holographic principle in
   cosmology, we relate a class of second-order Ricci invariants to the IR
   cutoff characterizing the holographic dark energy density. The choice of
   second-order invariants provides an invariant way to account the problem
   of causality for the correct cosmological cutoff, since the presence of
   event horizons is not an a priori assumption. We find that these models
   work fairly well, by fitting the observational data, through a combined
   cosmological test with the use of SNeIa, BAO and CMB. This class of
   models is also able to overcome the fine-tuning and coincidence
   problems. Finally, to make a comparison with other recent models, we
   adopt the statistical tests AIC and BIC.}},
