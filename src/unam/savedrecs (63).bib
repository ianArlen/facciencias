Author = {Monte-Perez, Ines and Sosa, Ana M. and Bernes, Sylvain and
   Aliaga-Alcalde, Nuria and Ugalde-Saldivar, Victor M. and Gasque, Laura},
Title = {{pH-Dependent Imidazolato Bridge Formation in Dicopper Complexes:
   Magnetic, Electrochemical, and Catalytic Repercussions}},
Journal = {{EUROPEAN JOURNAL OF INORGANIC CHEMISTRY}},
