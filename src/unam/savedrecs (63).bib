@article{ ISI:000325754300003,
Author = {Narro-Robles, Jose and Hiram Hernandez-Bringas, Hector and
   Flores-Arenales, Rene},
Title = {{2010 population census: four million Mexicans more than those
   forecasted, the end of a State policy?}},
Journal = {{PAPELES DE POBLACION}},
Year = {{2012}},
Volume = {{18}},
Number = {{74}},
Pages = {{17-55}},
Month = {{OCT-DEC}},
Abstract = {{The XIII census of population and housing was carried out in Mexico in
   2010. When compared to existing projections of population the census
   data indicated there was an addition of four million people in Mexico.
   This document analyzes the differences between the census data and the
   projections, including the characteristics of population, age groups,
   gender, breakdowns according to states in the federation and the size of
   the localities where this population - unaccounted for in the
   projections- resides. In addition, we analyses the possible causes for
   this difference between projected and real data and reflect on its
   effects on national policies which use population projections for
   planning.}},
