@article{ ISI:000307824400021,
Author = {Valencia, Diego and Klimova, Tatiana and Garcia-Cruz, Isidoro},
Title = {{Aromaticity of five- and six-membered heterocycles present in crude oils
   - An electronic description for hydrotreatment process}},
Journal = {{FUEL}},
Year = {{2012}},
Volume = {{100}},
Pages = {{177-185}},
Month = {{OCT}},
