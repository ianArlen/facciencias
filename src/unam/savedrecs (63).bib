@article{ ISI:000309157200048,
Author = {Valenzuela, R. and Ammar, S. and Nowak, S. and Vazquez, G.},
Title = {{Low Field Microwave Absorption in Nanostructured Ferrite Ceramics
   Consolidated by Spark Plasma Sintering}},
Journal = {{JOURNAL OF SUPERCONDUCTIVITY AND NOVEL MAGNETISM}},
