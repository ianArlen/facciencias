@article{ ISI:000310598500011,
Author = {Diaz-Garcia, C. M. and Fuentes-Silva, D. and Sanchez-Soto, C. and
   Dominguez-Perez, D. and Garcia-Delgado, N. and Varela, C. and
   Mendoza-Hernandez, G. and Rodriguez-Romero, A. and Castaneda, O. and
   Hiriart, M.},
Title = {{Toxins from Physalia physalis (Cnidaria) Raise the Intracellular Ca2+ of
   Beta-Cells and Promote Insulin Secretion}},
Journal = {{CURRENT MEDICINAL CHEMISTRY}},
Year = {{2012}},
Volume = {{19}},
Number = {{31}},
Pages = {{5414-5423}},
Month = {{NOV}},
Abstract = {{Physalia physalis is a marine cnidarian from which high molecular weight
   toxins with hemolytic and neurotoxic effects have been isolated. In the
   present work, two novel toxins, PpV9.4 and PpV19.3 were purified from P.
   physalis by bioactive guideline isolation. It involved two steps of
   column chromatography, gel filtration and RP-HPLC. The molecular weights
   were 550.7 and 4720.9 Da for PpV9.4 and PpV19.3, respectively. In the
   light of the Edman sequencing results, the structure of these toxins
   included the presence of modified amino acids. Both toxins increased the
   percentage of insulin secreting beta-cells and induced cytosolic Ca2+
   elevation. To date, this is the first report of low molecular weight
   toxins increasing insulin secretion purified from cnidarians, by
   constituting a new approach to the study of beta-cells physiology.}},
