@article{ ISI:000309111800018,
Author = {Ullrich, B. and Brown, G. J. and Xi, H.},
Title = {{Semiconductor band gap localization via Gaussian function}},
Journal = {{SEMICONDUCTOR SCIENCE AND TECHNOLOGY}},
Year = {{2012}},
Volume = {{27}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{To determine the band gap of bulk semiconductors with transmission
   spectroscopy alone is considered as an extremely difficult task because
   in the higher energy range, approaching and exceeding the band gap
   energy, the material is opaque yielding no useful data to be recorded.
   In this paper, by investigating the transmission of industrial GaSb
   wafers with a thickness of 500 mu m, we demonstrate how these obstacles
   of transmission spectroscopy can be overcome. The key is the
   transmission spectrums' derivative, which coincides with the Gaussian
   function. This understanding can be used to transfer Beers' law in an
   integral form opening the pathway of band gap determinations based on
   mathematical parameters only. The work also emphasizes the correlation
   between the thermal band gap variation and Debye temperature.}},
