@article{ ISI:000313161000007,
Author = {Carlos Gonzalez-Hernandez, Juan and Farias Rosales, Lorena and Zamudio
   Jaramillo, Miguel Angel and Alvarez-Navarrete, Mariana and Vera Villa,
   Juan Carlos and Martinez Corona, Ricardo and del Carmen Chavez-Parga,
   Ma. and Pena, Antonio},
Title = {{Chemical Hydrolysis of the Polysaccharides of the Tamarind Seed}},
Journal = {{JOURNAL OF THE MEXICAN CHEMICAL SOCIETY}},
Year = {{2012}},
Volume = {{56}},
Number = {{4}},
Pages = {{395-401}},
Month = {{OCT-DEC}},
Abstract = {{Biotechnological processes with lignocellulose materials are promising
   technologies for the production of chemicals and energy involving the
   same main production stages: hydrolysis of the hemicellulose and the
   cellulose to monomeric sugars, fermentation, recovery of the product,
   and concentration. We studied the effect of different treatments on the
   release of reducing sugars (RS) from seeds. Three factors were tested:
   temperature from 86 to 130.2 degrees C; acid concentration, 0.32 to
   3.68\% (v/v); and exposure time, from 13.2 to 40 min. Two acids were
   tested. Temperature and time were the factors that had more effect on
   hydrolysis, whereas no interactions between factors had a significant
   effect on the release of RS. The best conditions for RS release were for
   both acids (2\% v/v), 130.2 degrees C, and 30 min, with a concentration
   of about 110 g/L. Results suggest tamarind seed is a feasible option for
   xylitol production.}},
