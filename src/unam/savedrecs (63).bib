@article{ ISI:000311030500004,
Author = {Amatayakul-Chantler, S. and Jackson, J. A. and Stegner, J. and King, V.
   and Rubio, L. M. S. and Howard, R. and Lopez, E. and Walker, J.},
Title = {{Immunocastration of Bos indicus x Brown Swiss bulls in feedlot with
   gonadotropin-releasing hormone vaccine Bopriva provides improved
   performance and meat quality}},
Journal = {{JOURNAL OF ANIMAL SCIENCE}},
Year = {{2012}},
Volume = {{90}},
Number = {{11}},
Pages = {{3718-3728}},
Month = {{NOV}},
Abstract = {{The objective of this study was to determine the effects of a GnRH
   vaccine on feedlot performance and meat quality in Bos indicus Zebu x
   Brown Swiss bulls. The study was a 2 x 2 factorial arrangement of
   treatments with 1,600 bulls allocated by BW into 4 groups of similar to
   400 animals. The GnRH vaccine (Bopriva) was injected on d 0 and 42, and
   anabolic implants given on d 0 (Component E-S) and d 84 (Synovex
   Choice). Group designations were: Con = placebo control; Imp = implants
   alone; Vac = GnRH vaccine alone; and Vac+Imp = GnRH vaccine together
   with implants. The second GnRH vaccination at d 42 resulted in elevated
   titers of IgG antibody and suppressed concentrations of testosterone in
   vaccinated groups (Vac and Vac+Imp) at d 56 (P < 0.001), with titers and
   suppressed testosterone persisting to d 147 (P < 0.001). Groups Vac and
   Vac+Imp had reduced testes weights at slaughter on d 147 (P < 0.001).
   Bulls in group Vac were not different in final BW, HCW, or ADG (d 42 to
   147) relative to bulls in group Con. Bulls in group Vac+Imp had greater
   final BW than bulls in group Imp (P = 0.008) and greater BW than bulls
   in group Vac and group Con (P < 0.001). The HCW of Vac+Imp bulls was
   greater than the Vac or Con bulls (P < 0.001) but was not different to
   the Imp bulls (P = 0.294). Improved ADG was obtained by vaccination with
   the GnRH vaccine, in the presence of implants (group Vac+Imp compared
   with group Imp, P < 0.001) or absence of implants (group Vac compared
   with group Con, P = 0.028). Meat quality of bulls receiving the GnRH
   vaccine was improved irrespective of implant status, with a 1.6- to
   2.6-fold increase in the proportion of bulls in groups Vac and Vac+Imp,
   respectively, grading as USDA Choice (P < 0.002) and with greater fat
   depth at the 12th rib (P < 0.001). Meat tenderness was improved in the
   vaccine groups (Vac and Vac+Imp) compared with groups Con and Imp (P <
   0.004). Use of the GnRH vaccine Bopriva in Bos indicus x Brown Swiss
   bulls finishing in a feedlot under Mexican husbandry conditions can
   provide improved performance in combination with implants (increased BW
   and ADG) and improved meat quality, with or without implants, and in
   particular, better USDA carcass grading and loin fat cover.}},
