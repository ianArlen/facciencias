@article{ ISI:000310436900073,
Author = {Basiuk, Elena V. and Basiuk, Vladimir A. and Meza-Laguna, Victor and
   Contreras-Torres, Flavio F. and Martinez, Melchor and Rojas-Aguilar,
   Aaron and Salerno, Marco and Zavala, Guadalupe and Falqui, Andrea and
   Brescia, Rosaria},
Title = {{Solvent-free covalent functionalization of multi-walled carbon nanotubes
   and nanodiamond with diamines: Looking for cross-linking effects}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{2012}},
Volume = {{259}},
Pages = {{465-476}},
Month = {{OCT 15}},
Abstract = {{The covalent functionalization of carbon nanomaterials with diamines is
   a way to enhance the mechanical strength of nanocomposites due to
   cross-linking effects, to form complex networks for nanotube-based
   electronic circuits, as well as is important for a number of biomedical
   applications. The main goal of the present work was to covalently
   functionalize pristine multi-walled carbon nanotubes and nanodiamond
   with three aliphatic diamines (1,8-diaminooctane, 1,10-diaminodecane and
   1,12-diaminododecane) and one aromatic diamine (1,5-diaminonaphthalene),
   by employing a simple one-step solvent-free methodology, which is based
   on thermal instead of chemical activation. We looked for experimental
   evidences of cross-linking effects in the carbon nanomaterials
   synthesized by using solubility/dispersibility tests, atomic force
   microscopy, scanning and transmission electron microscopy, as well as
   Fourier-transform infrared spectroscopy and thermogravimetric analysis
   for additional characterization. (C) 2012 Elsevier B. V. All rights
