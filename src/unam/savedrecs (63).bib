@article{ ISI:000310808900006,
Author = {Ustarroz-Cano, M. and Garcia-Pelaez, I. and Pinon-Zarate, G. and
   Herrera-Enriquez, M. and Soldevila, G. and Fortoul, T. I.},
Title = {{CD11c decrease in mouse thymic dendritic cells after vanadium inhalation}},
