@article{ ISI:000310458900010,
Author = {McClure, M. K. and Manoj, P. and Calvet, N. and Adame, L. and Espaillat,
   C. and Watson, D. M. and Sargent, B. and Forrest, W. J. and D'Alessio,
   P.},
Title = {{PROBING DYNAMICAL PROCESSES IN THE PLANET-FORMING REGION WITH DUST
   MINERALOGY}},
