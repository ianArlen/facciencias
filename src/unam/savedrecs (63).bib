Author = {Ramirez-Ponce, Andres and Angel Moron, Miguel},
Title = {{Revision of Bucaphallanus, a New Subgenus of Paranomala (Coleoptera:
   Melolonthidae, Rutelinae, Anomalini) With Description of Six New Species
   From Mexico}},
Journal = {{ANNALS OF THE ENTOMOLOGICAL SOCIETY OF AMERICA}},
