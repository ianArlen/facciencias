@article{ ISI:000307981500011,
Author = {Camarillo-Cadena, Menandro and Garza-Ramos, Gerogina and Peimbert,
   Mariana and Polaina, Julio and Perez-Hernandez, Gerardo and Zubillaga,
   Rafael A.},
Title = {{Additive Effect of Single Amino Acid Replacements on the Kinetic
   Stability of beta-Glucosidase B}},
Journal = {{PROTEIN JOURNAL}},
Year = {{2012}},
Volume = {{31}},
Number = {{7}},
Pages = {{615-622}},
Month = {{OCT}},
Abstract = {{Previously, we applied in vitro evolution to generate the
   thermoresistant triple mutant H62R/N223Y/M319I of beta-glucosidase B
   (BglB) from Paenibacillus polymyxa. In order to dissect the energetic
   contributions to protein stabilization achieved by these mutations, we
   measured the kinetic constants of the heat denaturation of wild type
   BglB, the triple mutant and the three single mutants (H62R, N223Y,
   M319I) by circular dichroism at various temperatures. Our results show
   that all four mutants delayed the denaturation process. Based on the
   Transition State theory, the increase of the activation barrier for the
   thermal denaturation of the triple mutant (Delta Delta G (N -> TS) ) is
   equivalent to that produced by the sum of the contributions from the
   three single mutants, whose C (beta) s are located at least 18 apart.
   This analysis provides a formal demonstration of the generally accepted
   idea that protein thermal stability can be increased through sequential
   addition of individual mutations. Each of the mutations described here
   contribute in part to the overall effect, which in this case affects the
   unfolding barrier.}},
