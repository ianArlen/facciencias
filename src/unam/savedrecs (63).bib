@article{ ISI:000311927700009,
Author = {Colorado-Peralta, Raul and Sanchez-Vazquez, Mario and
   Hernandez-Ahuactzi, Iran F. and Sanchez-Ruiz, Sonia A. and Contreras,
   Rosalinda and Flores-Parra, Angelina and Castillo-Blum, Silvia E.},
Title = {{Structural study of molybdenum(VI) complexes containing bidentate
   ligands: Synthesis, characterization and DFT calculations}},
Journal = {{POLYHEDRON}},
Year = {{2012}},
Volume = {{48}},
Number = {{1}},
Pages = {{72-79}},
Month = {{NOV 14}},
Abstract = {{A series of molybdenum(VI) complexes containing bidentate ligands:
   toluene-3,4-dithiol (1), 2-mercaptophenol (2), 2-aminothiophenol (3),
   ethane-1,2-dithiol (4), dithiooxamide (5) and 2-thiopheno-carboxamide
   (6) are reported. The hexacoordinated molybdenum tris-chelates
   compounds: tris-(toluene-3, 4-dithiolate)molybdenum(VI) (7),
   tris-(2-mercaptophenolate)molybdenum(VI) (8),
