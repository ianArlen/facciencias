@article{ ISI:000309307000003,
Author = {Pola, Antonio and Crosta, Giovanni and Fusi, Nicoletta and Barberini,
   Valentina and Norini, Gianluca},
Title = {{Influence of alteration on physical properties of volcanic rocks}},
