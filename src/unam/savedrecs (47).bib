@article{ ISI:000343759800031,
Author = {Rodriguez, I. and Hess, P. O. and Schramm, S. and Greiner, W.},
Title = {{Baryonic properties of neutron stars within pseudo-complex General
   Relativity}},
Journal = {{ASTRONOMISCHE NACHRICHTEN}},
Year = {{2014}},
Volume = {{335}},
Number = {{6-7, SI}},
Pages = {{745-750}},
Month = {{AUG}},
Abstract = {{The baryonic properties of neutron stars within the theory of
   pseudo-complex General Relativity (pc-GR) are studied. The
   pc-Tolman-Oppenheimer-Volkoff equations are numerically integrated in
   order to understand the structure of these objects. The pc-component
   energy density epsilon(A) has been linearly coupled to the respective
   baryonic quantity epsilon(m). Solutions have been presented for
   different values of the coupling parameter. It is shown that
   accumulation of the Lambda-component allows the theoretical existence of
   larger and more massive neutron stars. (C) 2014 WILEY-VCH Verlag GmbH\&
   Co. KGaA, Weinheim}},
