@article{ ISI:000346424200037,
Author = {Rivas Manzano, Patricia and Chavira Ramirez, Carlos Tonatiuh and Ortiz
   Hernandez, Rosario},
Title = {{Adaptation of the Llombart's silver impregnation technique for
   demonstration of nerve fibers in any tissue in paraffin sections}},
Journal = {{INTERNATIONAL JOURNAL OF MORPHOLOGY}},
Year = {{2014}},
Volume = {{32}},
Number = {{3}},
Pages = {{973-980}},
Month = {{SEP}},
Abstract = {{In Biological research is still necessary for the demonstration of the
   peripheral innervation in numerous tissues and organs. The aim of this
   study was to rescue and modernize one of the most consistent methods
   that we have tried to demonstrate peripheral innervation. Llombart's
   technique for nerve fibers was adapted by paraffin cuts of 7 mu m in
   different animal tissue. The silver impregnation was done by dripping in
   a moist chamber. It was demonstrated in a constant, precise and serial
   form, nerve terminations, and sensorial corpuscles, neurons, and
   peripheral nerve fibers. Despite being highly specific to nerve fibers,
   the technique does not sacrifice tissue panorama so it gives beautiful
   images set. Without being a technique to argentaffin structures, it
   clearly shows two types of argentaffin cells in the adrenal glands. The
   addition of the metal reactive in droplets and in a humid chamber
   provides a very economical variant.}},
