@article{ ISI:000343759800020,
Author = {Hess, P. O. and Rodriguez, I. and Greiner, W.},
Title = {{Pseudo-complex General Relativity and neutron stars}},
Journal = {{ASTRONOMISCHE NACHRICHTEN}},
Year = {{2014}},
Volume = {{335}},
Number = {{6-7, SI}},
Pages = {{679-684}},
Month = {{AUG}},
Abstract = {{The pseudo-complex General Relativity, which includes a minimal length,
   is resumed and similarities to other theories are pointed out. For a
   zero minimal length the theory exhibits a remnant contribution, which is
   associated to a dark energy. It suggests the principle that a mass not
   only curves the space around it but also changes the vacuum properties,
   which in turn affect the metric. Some experimental predictions are
   shortly mentioned. The consequences of the presence of a dark energy are
   investigated for the case of neutron stars. It is shown that large
   masses of neutron stars are possible. (C) 2014 WILEY-VCH Verlag GmbH \&
   Co. KGaA, Weinheim}},
