@article{ ISI:000346754300007,
Author = {Arana-Gabriel, Yolanda and Burrola-Aguilar, Cristina and Garibay-Orijel,
   Roberto and Franco-Maass, Sergio},
Title = {{OBTAINING STRAINS AND SPAWN PRODUCTION FROM FIVE SPECIES OF ALPINE WILD
   EDIBLE MUSHROOMS IN THE CENTER OF MEXICO}},
Journal = {{REVISTA CHAPINGO SERIE CIENCIAS FORESTALES Y DEL AMBIENTE}},
Year = {{2014}},
Volume = {{20}},
Number = {{3}},
Pages = {{213-226}},
Month = {{SEP-DEC}},
Abstract = {{Strains of Psathyrella spadicea, Floccularia aff. luteovirens, Clitocybe
   squamulosa, Flammulina mexicana and Lyophyllum aff. shimeji were
   obtained, their sporomes use to be collected and consumed by inhabitants
   of the Nevado de Toluca, Mexico. The strains were studied in vitro on
   six culture media (DFA: Dog food agar, MA: Maize agar, PDA-PL: potato
   dextrose-peptone and yeast agar, EMA-PL: malt extract agar peptone and
   yeast, PDA and EMA) at 18 degrees C and 25 degrees C. Species
   satisfactorily developed at 18 degrees C, F. mexicana and L. aff.
   shimeji had the highest growth rate and biomass production. Flammulina
   mexicana only developed at 18 degrees C showing higher growth and
   biomass production on DFA. The strain of L. aff. shimeji grew at 18 and
   25 degrees C, the highest growth rate was recorded on PDA at 18 degrees
   C and highest biomass production on EMA-PL at 18 degrees C. Regarding
   the best conditions for the in vitro development of mycelium, primary
   spawn was obtained from F. mexicana and L. aff. shimeji in. wheat and
   sorghum. The highest density and percentage of covered mycelium were
   obtained using wheat substrate.}},
