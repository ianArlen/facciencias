@article{ ISI:000342399700001,
Author = {Casado Izquierdo, Jose Maria},
Title = {{SCHEDULES THE DAILY MOBILITY PATTERNS IN THE METROPOLITAN AREA OF THE
   VALLEY OF MEXICO, 1994-2007}},
Journal = {{SCRIPTA NOVA-REVISTA ELECTRONICA DE GEOGRAFIA Y CIENCIAS SOCIALES}},
Year = {{2014}},
Volume = {{18}},
Number = {{487}},
Month = {{SEP 1}},
Abstract = {{In Mexico City Metropolitan Area (MCMA) daily mobility shows a three
   peak-time pattern of trips. Such pattern is uncommon in urban areas in
   other countries, which tend to present a two peak-pattern. This
   difference is mainly attributed to young population trips back home from
   school. In the last 13 years, this pattern has only been slightly
   modified despite a large increase in the number of trips.
   However, some of MCMA trips do not follow this general pattern, showing
   considerable time segmentation; this is the case of trips by purpose, or
   centrifugal and centripetal trips. From a space-time point of view,
   people living in the periphery begin their first trip earlier, and even
   though if high-income travelers enjoy a later first trip, rich and poor
   show a similar and synchronic time pattern.}},
