@article{ ISI:000342139500001,
Author = {Allmond, J. M. and Brown, B. A. and Stuchbery, A. E. and
   Galindo-Uribarri, A. and Padilla-Rodal, E. and Radford, D. C. and
   Batchelder, J. C. and Howard, M. E. and Liang, J. F. and Manning, B. and
   Varner, R. L. and Yu, C. -H.},
Title = {{High-precision B(E2) measurements of semi-magic Ni-58,Ni-60,Ni-62,Ni-64
   by Coulomb excitation}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{2014}},
Volume = {{90}},
Number = {{3}},
Month = {{SEP 15}},
Abstract = {{High-precision reduced electric-quadrupole transition probabilities
   B(E2; 0(1)(+) -> 2(1)(+)) have been measured from single-step Coulomb
   excitation of semi-magic Ni-58,Ni-60,Ni-62,Ni-64 (Z = 28) beams at 1.8
   MeV per nucleon on a natural carbon target. The energy loss of the
   nickel beams through the carbon target were directly measured with a
   zero-degree Bragg detector and the absolute B(E2) values were normalized
   by Rutherford scattering. The B(E2) values disagree with recent lifetime
   studies that employed the Doppler-shift attenuation method. The present
   high-precision B(E2) values reveal an asymmetry about Ni-62, midshell
   between N = 28 and 40, with larger values towards Ni-56 (Z = N = 28).
   The experimental B(E2) values are compared with shell-model calculations
   in the full pf model space and the results indicate a soft Ni-56 core.}},
