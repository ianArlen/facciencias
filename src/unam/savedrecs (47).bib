@article{ ISI:000340257000019,
Author = {Ferrer-Ugalde, Albert and Gonzalez-Campo, Arantzazu and Vinas, Clara and
   Rodriguez-Romero, Jesus and Santillan, Rosa and Farfan, Norberto and
   Sillanpaa, Reijo and Sousa-Pedrares, Antonio and Nunez, Rosario and
   Teixidor, Francesc},
Title = {{Fluorescence of New o-Carborane Compounds with Different Fluorophores:
   Can it be Tuned?}},
Journal = {{CHEMISTRY-A EUROPEAN JOURNAL}},
Year = {{2014}},
Volume = {{20}},
