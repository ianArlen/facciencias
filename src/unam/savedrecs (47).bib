@article{ ISI:000343787500005,
Author = {Guerrero Mc Manus, Fabrizzio},
Title = {{Philosophy of Biology and Gender Studies. A Delayed Symbiosis}},
Journal = {{CRITICA-REVISTA HISPANOAMERICANA DE FILOSOFIA}},
