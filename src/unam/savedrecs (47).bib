@article{ ISI:000343235700002,
Author = {Martinez-Lopez, L. and Rodriguez-Cuevas, J. and Martinez-Lopez, J. I.
   and Martynyuk, A. E.},
Title = {{Cascaded circular-polarisation-selective surface based on bisected split
   rings}},
Journal = {{ELECTRONICS LETTERS}},
Year = {{2014}},
Volume = {{50}},
Number = {{19}},
Pages = {{1335+}},
Month = {{SEP 11}},
Abstract = {{A cascaded circular-polarisation-selective surface (CPSS) based on
   bisected split rings is presented. The proposed CPSS allows the
   transmission of one sense of circular polarisation while reflecting the
   other sense. To validate the proposed design, a nine-layer prototype
   that operates in the Ka band has been designed, fabricated and tested.
   Experimental results show that transmission coefficients higher than -1
   dB and axial ratios lower than 3 dB are obtained for one sense of
   circular polarisation, whereas transmission coefficients lower than -20
   dB are obtained for the opposite sense over the frequency band from
   26.39 to 35.43 GHz.}},
