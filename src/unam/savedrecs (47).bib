@article{ ISI:000340572200019,
Author = {Gutierrez-Perez, O. and Durand-Montano, C. and Rojas-Castaneda, J. C.
   and Chavez-Saldana, M. and Vigueras-Villasenor, R. M.},
Title = {{Effect of pyrimethamine treatment on male rat testicular cell population
   development}},
