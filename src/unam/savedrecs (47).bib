@article{ ISI:000343759800005,
Author = {Sussman, R. A.},
Title = {{Gravitational entropy of cosmic expansion}},
Journal = {{ASTRONOMISCHE NACHRICHTEN}},
Year = {{2014}},
Volume = {{335}},
Number = {{6-7, SI}},
Pages = {{587-592}},
Month = {{AUG}},
Abstract = {{We apply a recent proposal to define ``gravitational entropy{''} to the
   expansion of cosmic voids within the framework of non-perturbative
   General Relativity. By considering CDM void configurations compatible
   with basic observational constraints, we show that this entropy grows
   from post-inflationary conditions towards a final asymptotic value in a
   late time fully nonlinear regime described by the Lemaitre-Tolman-Bondi
   (LTB) dust models. A qualitatively analogous behavior occurs if we
   assume a positive cosmological constant consistent with Lambda-CDM
   background model. However, the. term introduces a significant
   suppression of entropy growth with the terminal equilibrium value
   reached at a much faster rate. (C) 2014 WILEY-VCH Verlag GmbH \& Co.
   KGaA, Weinheim}},
