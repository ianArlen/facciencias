@inproceedings{ ISI:000346075700005,
Author = {Castanos, L. O. and Weder, R.},
Book-Group-Author = {{IOP}},
Title = {{Classical dynamics of a moving mirror due to radiation pressure}},
Booktitle = {{8TH INTERNATIONAL SYMPOSIUM ON QUANTUM THEORY AND SYMMETRIES (QTS8)}},
