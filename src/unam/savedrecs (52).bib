@article{ ISI:000342325200010,
Author = {Brindis, F. and Gonzalez-Andrade, M. and Gonzalez-Trujano, M. E. and
   Estrada-Soto, S. and Villalobos-Molina, R.},
Title = {{Postprandial glycaemia and inhibition of alpha-glucosidase activity by
   aqueous extract from Coriandrum sativum}},
Journal = {{NATURAL PRODUCT RESEARCH}},
Year = {{2014}},
Volume = {{28}},
Number = {{22}},
Pages = {{2021-2025}},
Abstract = {{The antihyperglycaemic properties of the aqueous extract from the leaves
   and stems of Coriandrum sativum L. were evaluated in normoglycaemic
   rats, and on alpha-glucosidase activity from Saccharomyces cerevisiae,
   in order to validate its use in folk medicine. In in vivo experiments
   rats were administered with the aqueous extract of the plant at 100, 300
   and 500 mg/kg, to observe the effect on oral sucrose tolerance test. The
   aqueous extract exhibited significant antihyperglycaemic activity at the
   three tested doses. In vitro experiments with a-glucosidase exhibited a
   competitive-type inhibition. These results confirm the antidiabetic
   properties of the extract of C. sativum L., probably by the inhibition
   of alpha-glucosidase in the gastrointestinal tract.}},
