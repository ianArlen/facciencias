@article{ ISI:000332189800003,
Author = {Forero-Vivas, Maria E. and Hernandez-Cruz, Arturo},
Title = {{Increased firing frequency of spontaneous action potentials in
   cerebellar Purkinje neurons of db/db mice results from altered
   auto-rhythmicity and diminished GABAergic tonic inhibition}},
Journal = {{GENERAL PHYSIOLOGY AND BIOPHYSICS}},
Year = {{2014}},
Volume = {{33}},
Number = {{1}},
Pages = {{29-41}},
Abstract = {{The hormone leptin, by binding to hypothalamic receptors, suppresses
   food intake and decreases body adiposity. Leptin receptors are also
   widely expressed in extra-hypothalamic areas such as hippocampus,
   amygdala and cerebellum, where leptin modulates synaptic transmission.
   Here we show that a defective leptin receptor affects the
   electrophysiological properties of cerebellar Purkinje neurons (PNs).
   PNs from (db/db) mice recorded in cerebellar slices display a higher
   firing rate of spontaneous action potentials than PNs from wild type
   (WT) mice. Blockade of GABAergic tonic inhibition with bicuculline in WT
   mice changes the firing pattern from continuous, uninterrupted spiking
   into bursting firing, but bicuculline does not produce these alterations
   in db/db neurons, suggesting that they receive a weaker GABAergic
   inhibitory input. Our results also show that the intrinsic firing
   properties (auto-rhythmicity) of WT and db/db PNs are different. Tonic
   firing of PNs, the only efferent drive from the cerebellar cortex, is a
   persistent signal to downstream cerebellar targets. The significance of
   leptin modulation of PNs spontaneous firing is not known. Also, it is
   not clear if the increased excitability of cerebellar PNs in db/db mice
   results from hyperglycemia or from the lack of leptin signaling, since
   both conditions coexist in the db/db strain.}},
