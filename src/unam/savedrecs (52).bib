Editor = {{BarronPalos, L and MartinezQuiroz, E and MoralesAgiss, I}},
Title = {{Fidelity, susceptibility and critical exponents in the Dicke model}},
Booktitle = {{XXXVI SYMPOSIUM ON NUCLEAR PHYSICS (COCOYOC 2013)}},
