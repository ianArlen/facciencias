@article{ ISI:000333973400001,
Author = {Miroshnichenko, A. S. and Manset, N. and Zharikov, S. V. and Zsargo, J.
   and Juarez Jimenez, J. A. and Groh, J. H. and Levato, H. and Grosso, M.
   and Rudy, R. J. and Laag, E. A. and Crawford, K. B. and Puetter, R. C.
   and Reichart, D. E. and Ivarsen, K. M. and Haislip, J. B. and
   Nysewander, M. C. and LaCluyze, A. P.},
Title = {{Confirmation of the Luminous Blue Variable Status of MWC 930}},
Journal = {{ADVANCES IN ASTRONOMY}},
Year = {{2014}},
Abstract = {{We present spectroscopic and photometric observations of the
   emission-line star MWC 930 (V446 Sct) during its long-term optical
   brightening in 2006-2013. Based on our earlier data we suggested that
   the object has features found in Luminous Blue Variables (LBV), such as
   a high luminosity (similar to 310(5) L-circle dot), a low wind terminal
   velocity (similar to 140 km s(-1)), and a tendency to show strong
   brightness variations (similar to 1 mag over 20 years). For the last
   similar to 7 years it has been exhibiting a continuous optical and
   near-IR brightening along with a change of the emission-line spectrum
   appearance and cooling of the star's photosphere. We present the
   object's V-band light curve, analyze the spectral variations, and
   compare the observed properties with those of other recognized Galactic
   LBVs, such as AG Car and HR Car. Overall we conclude the MWC 930 is a
   bona fide Galactic LBV that is currently in the middle of an S Dor
   cycle.}},
