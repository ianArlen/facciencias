@inproceedings{ ISI:000345929100039,
Author = {Salas Bautista, N. and Martinez-Davalos, A. and Rodriguez-Villafuerte,
   M. and Murrieta-Rodriguez, T. and Manjarrez-Marmolejo, J. and
   Franco-Perez, J. and Calvillo-Velasco, M. E.},
