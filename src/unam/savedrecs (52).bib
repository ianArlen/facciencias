@inproceedings{ ISI:000338062100004,
Author = {Ayala, Alejandro and Dominguez, Isabel and ElenaTejeda-Yeomans, Maria},
Editor = {{ChaconAcosta, G and GarciaPerciante, AL and SandovalVillalbazo, A}},
Title = {{The double-hump of azimuthal correlations in HIC: Head shock vs. Mach
   cone}},
Booktitle = {{PLASMA PHYSICS AND RELATIVISTIC FLUIDS}},
Series = {{AIP Conference Proceedings}},
Year = {{2014}},
Volume = {{1578}},
Pages = {{61-73}},
Note = {{5th Leopoldo Garcia-Colin Mexican Meeting on Mathematical and
   Experimental Physics, Colegio Nacl, Mexico City, MEXICO, SEP 09-13, 2013}},
Organization = {{Natl Council Sci \& Technol; Inst Sci \& Technol Mexico City; Univ
   Iberoamericana; Univ Autonoma Metropolitana; German Serv Acad Exchange}},
Abstract = {{We study the energy-momentum deposited by fast moving partons within a
   medium in the context of relativistic heavy-ion collisions. We use
   linearized viscous hydrodynamics to compute the spatial distribution of
   this energy-momentum and the Cooper-Frye formalism to compute the
   corresponding particle distribution. We show that for the case of a
   constant energy loss per unit length and when the parton produced in a
   hard scattering has a velocity larger than the speed of sound in the
   palsma, energy-momentum is preferentially deposited along the head shock
   of the fast moving partons. In this scenario, the double hump in the
   away-side of azimuthal correlations can be produced by two away-side
   partons depositing their energy-momentum along their directions of
   motion. These partons are originated in the in-medium hard scattering in
   2 -> 3 processes. We compare the results of the analysis to azimuthal
   angular correlations from PHENIX and show that the calculation
   reproduces the data systematics of a decreasing away side correlation
   when the momentum of the associated hadron becomes closer to the
   momentum of the leading hadron. This scenario seems to avoid the
   shortcomings of the Mach cone as the origin of the double-hump structure
   in the away side.}},
