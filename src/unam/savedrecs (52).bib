@article{ ISI:000337361500003,
Author = {Francisco Rivera-Benitez, Jose and Martinez-Bautista, Rebeca and
   Rios-Cambre, Francisco and Ramirez-Mendoza, Humberto},
Title = {{Molecular detection and isolation of avian metapneumovirus in Mexico}},
Journal = {{AVIAN PATHOLOGY}},
Year = {{2014}},
Volume = {{43}},
Number = {{3}},
Pages = {{217-223}},
Abstract = {{We conducted a longitudinal study to detect and isolate avian
   metapneumovirus (aMPV) in two highly productive poultry areas in Mexico.
   A total of 968 breeder hens and pullets from 2 to 73 weeks of age were
   analysed. Serology was performed to detect aMPV antibodies and 105
   samples of tracheal tissue were collected, pooled by age, and used for
   attempted virus isolation and aMPV nested reverse
   transcriptase-polymerase chain reaction (nRT-PCR). The serological
   analysis indicated that 100\% of the sampled chickens showed aMPV
   antibodies by 12 weeks of age. Five pools of pullet samples collected at
   3 to 8 weeks of age were positive by nRT-PCR and the sequences obtained
   indicated 98 to 99\% similarity with the reported sequences for aMPV
   subtype A. Virus isolation of nRT-PCR-positive samples was successfully
   attempted using chicken embryo lung and trachea mixed cultures with
   subsequent adaptation to Vero cells. This is the first report of
   detection and isolation of aMPV in Mexico.}},
