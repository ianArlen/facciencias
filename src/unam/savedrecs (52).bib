Title = {{A comparative study of CNC part programming addressing energy
   consumption and productivity}},
Booktitle = {{6TH CIRP INTERNATIONAL CONFERENCE ON HIGH PERFORMANCE CUTTING (HPC2014)}},
Series = {{Procedia CIRP}},
Year = {{2014}},
