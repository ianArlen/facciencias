@inproceedings{ ISI:000345902100051,
Author = {Rosales, Antonio and Fridman, Leonid and Shtessel, Yuri},
Book-Group-Author = {{IEEE}},
Title = {{Practical Relative Degree in SMC systems: Frequency Domain Approach}},
Booktitle = {{2014 13TH INTERNATIONAL WORKSHOP ON VARIABLE STRUCTURE SYSTEMS (VSS)}},
Series = {{IEEE International Workshop on Variable Structure Systems}},
Year = {{2014}},
Note = {{13th IEEE International Workshop on Variable Structure Systems (VSS),
   Nantes, FRANCE, JUN 29-JUL 02, 2014}},
Organization = {{IEEE; CSS; IR CCYN; Gdr Macs; ECN; Nantes Metropole}},
Abstract = {{A variety of output regulation problems can be addressed via sliding
   mode control when system's relative degree is known. The difficulties in
   the implementation of sliding mode controllers emerge when the relative
   degree is unknown. On the other hand, practical systems controlled by
   sliding mode algorithms always work under tolerance limits, which are
   frequently known. This behavior is called real sliding motion. In this
   work, the concept of practical relative degree in
   single-input-single-output systems controlled by sliding mode
   controllers is revisited from the view point of frequency analysis.
   Practical relative degree is understood as the smallest order of the
   sliding mode controller that satisfies the tolerance limits. Also, the
   notion of practical relative degree is analyzed in terms of the
   definitions of performance margins. The usefulness of the proposed
   concept is the way to design sliding mode controllers for systems that
   can be considered a black-box. The practical relative degree is studied
   and illustrated via examples and simulations.}},
