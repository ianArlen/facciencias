@inproceedings{ ISI:000345929100038,
Author = {Rosas-Gonzalez, S. and Martinez-Davalos, A. and Rodriguez-Villafuerte,
   M. and Murrieta-Rodriguez, T.},
Editor = {{BernalAlvarado, JD and GuzmanCabrera, R and Brandan, ME}},
Title = {{Monte Carlo Simulation of an X-ray Luminescence Optical Tomography
   Scanner Prototype}},
Booktitle = {{XIII MEXICAN SYMPOSIUM ON MEDICAL PHYSICS}},
