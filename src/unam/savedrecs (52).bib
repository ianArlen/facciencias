@inproceedings{ ISI:000337048500019,
Author = {Escobar, Carlos and Urrutia, Luis F.},
Editor = {{Macias, A and Maceda, M}},
Title = {{Towards an interpretation of spontaneous Lorentz symmetry breaking in
   modified BF theories}},
Booktitle = {{RECENT DEVELOPMENTS ON PHYSICS IN STRONG GRAVITATIONAL FIELDS}},
Series = {{AIP Conference Proceedings}},
Year = {{2014}},
Volume = {{1577}},
