@article{ ISI:000333541700003,
Author = {Mayer, Alicia},
Title = {{Thoughts of Bartolome de Las Casas on Indigenist Discourse. A Compared
   Perspective of European Colonies in the Americas}},
Journal = {{HISTORIA MEXICANA}},
Year = {{2014}},
Volume = {{63}},
Number = {{3}},
Pages = {{1121+}},
Month = {{JAN-MAR}},
Abstract = {{This work analyzes the reflections carried out in the European colonies
   in America on the nature and rights of indigenous peoples. The debate
   held between the friar Bartolome de Las Casas and the treatise writer
   Gines de Sepulveda during the first decades of the sixteenth century, on
   Indian rational capacity and the integration of Indians into the
   Christian congregation, became a referent both in Spanish colonies and
   in French and English possessions. Local and regional circumstances
   determined the way in which each nation undertook its civilizational and
   evangelical activities.}},
