@article{ ISI:000343638100003,
Author = {Estradas-Romero, Alejandro and Maria Prol-Ledesma, Rosa},
Title = {{Effect of shallow hydrothermal venting on the richness of benthic diatom
   species}},
Journal = {{CAHIERS DE BIOLOGIE MARINE}},
Year = {{2014}},
Volume = {{55}},
Number = {{4}},
Pages = {{399-408}},
Abstract = {{A qualitative analysis of the diatom community was conducted on sediment
   samples from two shallow submarine venting localities off the Mexican
   coast: Bahia Concepcion (BC) on the Gulf coast of the Baja California
   Peninsula and Punta de Mita (PM) on the Pacific coast of Nayarit. In
   both areas, control sites were also examined. Ninety-nine diatom species
   and two silicoflagellates were found at the BC vent location, and 77
   diatom species were found at the PM vent location. Non-metric
   multi-dimensional scaling and cluster analysis, based on Bray-Curtis
   similarity index (presence/absence), showed that the control sites
   grouped together, whereas the vent sites were distinct from the control
   sites and each other. The Simprof test, associated with the cluster
   analysis of the vent areas, indicated a clear difference between the two
   vent sites (pi = 2.05, p < 7\%). The floristic composition revealed that
   the difference between the two study areas (vents) is due to the
   difference in species richness. Hydrothermal vent sites in both areas
   had higher species richness than the respective control locations. This
   higher richness in species may be due to venting hydrothermal fluids,
   which are enriched with nutrients and trace elements that favour the
   proliferation of primary producers.}},
