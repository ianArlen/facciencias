Title = {{A Grand Unified model with Q(6) as the flavour symmetry}},
Booktitle = {{PASCOS 2012 - 18TH INTERNATIONAL SYMPOSIUM ON PARTICLES STRINGS AND
   COSMOLOGY}},
Series = {{Journal of Physics Conference Series}},
Year = {{2014}},
Volume = {{485}},
