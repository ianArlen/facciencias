Title = {{A Six-Channel Pediatric Coil Array for Detection of Children Spinal
   Pathologies by MRI at 1.5 Tesla}},
Booktitle = {{XIII MEXICAN SYMPOSIUM ON MEDICAL PHYSICS}},
Series = {{AIP Conference Proceedings}},
Year = {{2014}},
Volume = {{1626}},
Pages = {{82-85}},
