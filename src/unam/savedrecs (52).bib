Author = {Raquel Enriquez, Lucrecia and Aguirre, Rodolfo},
Title = {{Presentation of the Dossier ``The Patronage of the American Church: From
   Monarchy to National States{''}}},
Journal = {{HISTORIA CRITICA}},
Year = {{2014}},
Number = {{52}},
Pages = {{13-20}},
Month = {{JAN-APR}},
Abstract = {{This article studies the process of congregations among Muiscas in the
   province of Santafe during the 16th and 17th centuries. By examining
   archive documentation, the author limits and explores three specific
   moments: the configuration of the peoples, the creation of reservations,
   and the dynamics of aggregation. The article attempts to observe the
   adaptations made by the colonial administration in order to forge a new
   residential pattern guided by the evangelization of indigenous peoples
   and adjusted to the new order imposed by the encomienda. We also observe
   the economic, territorial, and sociopolitical transformations led by
   said process in this society.}},
