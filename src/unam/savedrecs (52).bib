@inproceedings{ ISI:000345989600082,
Author = {Vazquez, F. and Auvinet, G. and Vermeer, P.},
Editor = {{Hicks, MA and Brinkgreve, RBJ and Rohe, A}},
Title = {{Stochastic inverse modeling applied to earth dams for detecting
   preferential seepage paths}},
Booktitle = {{NUMERICAL METHODS IN GEOTECHNICAL ENGINEERING, VOL 1}},
Year = {{2014}},
Pages = {{497-502}},
Note = {{8th European Conference on Numerical Methods in Geotechnical Engineering
   (NUMGE), Delft Univ Technol, Delft, NETHERLANDS, JUN 18-20, 2014}},
Organization = {{European Reg Tech Comm 7; Int Soc Soil Mech \& Geotechn Engn; Delft Univ
   Technol; Deltares; CESAR; ITASCA Consultants s a s; Royal HaskoningDHV;
   CRUX; FUGRO; PLAXIS; Volker InfraDesign; Heymans; TNO DIANA; Witteveen
   Bos}},
Abstract = {{A numerical solution to the stochastic inverse problem of hydrology is
   presented. It is obtained by means of an extension of the Ensemble
   Kalman Filter. The benefits of the solution are illustrated with the
   results of a hypothetical case study in which records of piezometric
   heads are generated by solving a transient seepage problem over a
   simulated field that represents the distribution of conductivities in
   the core of an idealized earth dam. Realizations of conductivities
   conditional to a set of such histories of heads are used within a Monte
   Carlo scheme to determine the mean seepage velocity field. This field is
   used to detect, by visual inspection, preferential seepage paths in the
   core and to compare the locations of such paths against those in the
   simulated field.}},
