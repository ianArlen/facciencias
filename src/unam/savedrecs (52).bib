@article{ ISI:000337599500027,
Author = {Lanzilotta M, Marcelo and Mendoza, Octavio and Saenz, Corina},
Title = {{SPLIT-BY-NILPOTENT EXTENSIONS ALGEBRAS AND STRATIFYING SYSTEMS}},
Journal = {{COMMUNICATIONS IN ALGEBRA}},
Year = {{2014}},
Volume = {{42}},
Number = {{11}},
Pages = {{4997-5017}},
Abstract = {{Let Gamma and Lambda be artin algebras such that Gamma is a
   split-by-nilpotent extension of Lambda by a two sided ideal I of Gamma.
   Consider the change of rings functors G := (Gamma)Gamma(Lambda) circle
   times(Lambda) - and F := (Lambda)Lambda(Gamma) circle times(Gamma) -. In
   this article, by assuming that I-Lambda is projective, we find the
   necessary and sufficient conditions under which a stratifying system
   (Theta, <=) in mod Lambda can be lifted to a stratifying system (G
   Theta, <=) in mod(Gamma). Furthermore, by using the functors F and G, we
   study the relationship between their filtered categories of modules; and
   some connections with their corresponding standardly stratified algebras
   are stated (see Theorem 5.12, Theorem 5.15 and Theorem 5.18). Finally, a
   sufficient condition is given for stratifying systems in mod(Gamma) in
   such a way that they can be restricted, through the functor F, to
   stratifying systems in mod(Lambda).}},
