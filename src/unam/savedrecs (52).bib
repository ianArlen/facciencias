@article{ ISI:000344664000001,
Author = {Nava-Castro, Karen E. and Morales-Montor, Jorge and Ortega-Hernando,
   Alejandra and Camacho-Arroyo, Ignacio},
Title = {{Diethylstilbestrol Exposure in Neonatal Mice Induces Changes in the
   Adulthood in the Immune Response to Taenia crassiceps without
   Modifications of Parasite Loads}},
Journal = {{BIOMED RESEARCH INTERNATIONAL}},
Year = {{2014}},
Abstract = {{Industrial growth has increased the exposition to endocrine disruptor
   compounds (EDC's), which are exogenous agents with agonist or antagonist
   action of endogenous steroid hormones that may affect the course of
   parasite infections. We wanted to determine if the exposure to
   diethylstilbestrol (DES), an estrogen agonist, to both male and female
   mice affected the immune response and their susceptibility to T.
   crassiceps cysticercosis. In all infected groups, females showed higher
   parasite loads than males, and neonatal DES administration did not
   modify this pattern. In the spleen, noninfected mice showed sex-related
   differences in the percentage of the CD8+ subpopulation, but DES
   decreased the percentage of CD3+, CD19+, and CD8+ subpopulations in
   infected mice. In the mesenteric lymphatic node (MNL), DES showed a
   dimorphic effect in the percentage of CD19+ cells. Regarding estrogen
   receptor alpha (ER-alpha) expression, DES treatment induced a reduction
   in the expression of this receptor in both noninfected female and male
   mice in the spleen, which was decreased only in males in CD3+ and CD8+
   lymphocytes in MNL cell subpopulations. Our study is the first one to
   demonstrate that DES neonatal treatment in male and female mice affects
   the immune cell percentage, without effect on the susceptibility to T.
   crassiceps cysticercosis.}},
