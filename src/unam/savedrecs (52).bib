@article{ ISI:000336458700008,
Author = {Gerali, Francesco},
Title = {{SCIENCE AND LIFE OF A GEOLOGIST THROUGH HIS PAPERS. THE PERSONAL ARCHIVE
   OF GIOVANNI CAPELLINI IN LA SPEZIA}},
