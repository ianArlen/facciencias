@inproceedings{ ISI:000340733700135,
Author = {Diaz-Felix, L. A. and Escobar-Toledo, M. and Waissman, J. and
   Pitalua-Diaz, N. and Arancibia-Bulnes, C. A.},
Editor = {{Pitchumani, R}},
Title = {{Evaluation of heliostat field global tracking error distributions by
   Monte Carlo simulations}},
Booktitle = {{PROCEEDINGS OF THE SOLARPACES 2013 INTERNATIONAL CONFERENCE}},
