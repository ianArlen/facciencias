@inproceedings{ ISI:000337223400021,
Author = {Castillo-Mejia, F. and Gamboa-de Buen, I. and Herrera-Velazquez, J. J.
   E. and Rangel-Gutierrez, Jose},
Book-Group-Author = {{IOP}},
Title = {{Neutron emission characterisation at the FN-II Dense Plasma Focus}},
Booktitle = {{15TH INTERNATIONAL CONGRESS ON PLASMA PHYSICS (ICPP2010) \& 13TH LATIN
   AMERICAN WORKSHOP ON PLASMA PHYSICS (LAWPP2010)}},
Series = {{Journal of Physics Conference Series}},
Year = {{2014}},
