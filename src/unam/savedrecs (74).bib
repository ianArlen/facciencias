@article{ ISI:000292668800009,
Author = {Montero-Pardo, A. and Hernandez-Ceron, J. and Rojas-Maya, S. and
   Valencia, J. and Rodriguez-Cortez, A. and Gutierrez, C. G.},
Title = {{Increased cleavage and blastocyst rate in ewes treated with bovine
   somatotropin 5 days before the end of progestin-based estrous
   synchronization}},
Journal = {{ANIMAL REPRODUCTION SCIENCE}},
Year = {{2011}},
Volume = {{125}},
Number = {{1-4}},
