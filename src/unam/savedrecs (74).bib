@article{ ISI:000289817200022,
Author = {Garcia-Gomez, Elizabeth and Espinosa, Norma and de la Mora, Javier and
   Dreyfus, Georges and Gonzalez-Pedrajo, Bertha},
Title = {{The muramidase EtgA from enteropathogenic Escherichia coli is required
   for efficient type III secretion}},
