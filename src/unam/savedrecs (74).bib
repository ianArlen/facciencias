@article{ ISI:000290229500002,
Author = {Castanos, O. and Nahmad-Achar, E. and Lopez-Pena, R. and Hirsch, J. G.},
Title = {{No singularities in observables at the phase transition in the Dicke
   model}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2011}},
Volume = {{83}},
Number = {{5}},
Month = {{MAY 4}},
Abstract = {{The Dicke Hamiltonian describes the simplest quantum system with atoms
   interacting with photons: N two-level atoms inside a perfectly
   reflecting cavity, which allows only one electromagnetic mode. It has
   also been successfully employed to describe superconducting circuits
   that behave as artificial atoms coupled to a resonator. The system
   exhibits a transition to a superradiant phase at zero temperature. When
   the interaction strength reaches its critical value, both the number of
   photons and atoms in excited states in the cavity, together with their
   fluctuations, exhibit a sudden increase from zero. By employing
   symmetry-adapted coherent states, it is shown that these properties
   scale with the number of atoms, their reported divergences at the
   critical point represent the limit when this number goes to infinity,
   and, in this limit, they remain divergent in the superradiant phase.
   Analytical expressions are presented for all observables of interest,
   for any number of atoms. Comparisons with exact numerical solutions
   strongly support the results.}},
Publisher = {{AMER PHYSICAL SOC}},
