@article{ ISI:000290718900036,
Author = {Iannone, G. and Luongo, O.},
Title = {{Dark energy from a scale-free distribution}},
Journal = {{EPL}},
Year = {{2011}},
Volume = {{94}},
Number = {{4}},
Month = {{MAY}},
Abstract = {{We investigate some intriguing consequences of characterizing dark
   energy by the so-called scale-free statistics in a flat
   Friedman-Robertson-Walker cosmology. We adopt a toy model, derived from
   a power law distribution, being P(k) similar to k(-gamma), with 2 <
   gamma < 3, showing as a result that the dynamics mimics the Lambda CDM
   paradigm at low redshift, whereas it gives a varying equation of state
   around the value w similar to -1 for higher redshift. In addition, we
   test the equation of state with the modern cosmological data and we make
   a comparison with the observational constraints. Copyright (C) EPLA,
   2011}},
