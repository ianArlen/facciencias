@article{ ISI:000288376700011,
Author = {Chavez, Jaime and Vargas, Mario H. and Cruz-Valderrama, Jose E. and
   Montano, Luis M.},
Title = {{Non-quantal release of acetylcholine in guinea-pig airways: role of
   choline transporter}},
Journal = {{EXPERIMENTAL PHYSIOLOGY}},
