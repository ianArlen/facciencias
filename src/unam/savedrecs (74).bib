@article{ ISI:000291715000027,
Author = {Avalos-Rendon, Tatiana and Pfeiffer, Heriberto},
Title = {{Evidence of CO2 Chemisorption at High Temperature in Lithium Gal late
   (Li5GaO4)}},
Journal = {{CHEMISTRY LETTERS}},
Year = {{2011}},
Volume = {{40}},
Number = {{5}},
Pages = {{504-505}},
Month = {{MAY 5}},
Abstract = {{Li5GaO4 was tested as a possible CO2 captor. Li5GaO4 was synthesized by
   solid-state reaction, structurally characterized, and then thermally
   treated under a CO2 flow, from 30 to 900 degrees C, having the highest
   CO2 chemisorption at around 709 degrees C. The results clearly showed
   that Li5GaO4 is able to trap CO2 chemically in two different steps. The
   CO, quantity trapped was equal to 8.9 mmol g(-1), which is considerably
   high in comparison to other ceramics.}},
