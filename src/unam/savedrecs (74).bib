@article{ ISI:000299378100001,
Author = {Juan Escobar-Chavez, Jose and Luisa Dominguez-Delgado, Clara and Marlen
   Rodriguez-Cruz, Isabel},
Title = {{Targeting nicotine addiction: the possibility of a therapeutic vaccine}},
