@article{ ISI:000291024100011,
Author = {Garcia-Valenzuela, Augusto and Sant'Anna Cuppo, Fabio Luiz and Alberto
   Olivares, Jose and Galvan-Miyoshi, Julian M.},
Title = {{Comprehensive analysis of the diffuse-to-near-normal-viewing reflectance
   factor of paint films}},
Journal = {{OPTICAL ENGINEERING}},
Year = {{2011}},
Volume = {{50}},
Number = {{5}},
Month = {{MAY}},
Abstract = {{We present a comprehensive derivation of the so-called diffuse to
   near-normal-viewing reflectance factor of an architectural paint coating
   and its measurement with an integrating sphere. We focus our attention
   to the surface correction terms and consider the case when the surface
   is rough. We point out limits of the standard formula with the so-called
   Saunderson corrections commonly used for paint coatings. We provide and
   discuss a corrected definition of the roughness-dependent ``gloss
   factor{''} needed to describe specular-component excluded measurements.
   We show that as the roughness of the surface increases, the
   specular-excluded reflectance-factor increases, approaching its value
   with the specular-component included. (C) 2011 Society of Photo-Optical
   Instrumentation Engineers (SPIE). {[}DOI: 10.1117/1.3574069]}},
