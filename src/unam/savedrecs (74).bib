@article{ ISI:000291035700003,
Author = {Taran, Yuri A.},
Title = {{N-2, Ar, and He as a tool for discriminating sources of volcanic fluids
   with application to Vulcano, Italy}},
Journal = {{BULLETIN OF VOLCANOLOGY}},
Year = {{2011}},
Volume = {{73}},
Number = {{4, SI}},
Pages = {{395-408}},
Month = {{MAY}},
Abstract = {{A detailed analysis of published data on the N-2, Ar, and He content and
   Ar and He isotopic composition of fumarolic fluids from Vulcano crater
   (south Italy) supports a model with two endmembers comprising magmatic
   and hydrothermal fluids with correspondingly low and high H2O content.
   The magmatic component with the highest He-3/He-4 and highest absolute
   concentrations of N-2, Ar, and He also has the lowest N-2/Ar and N-2/He
   ratios (similar to 300 and similar to 500, respectively). In contrast,
   the hydrothermal endmember, with the lower He-3/He-4 and lower absolute
   N-2, Ar, and He abundances, has high N-2/Ar (similar to 1,000) and high
   N-2/He (> 3,000) ratios. The hydrothermal component is also
   characterized by the highest Ar-40/Ar-36 ratios (> 1,000) and is
   proposed to be the main carrier of metamorphic gases from the arc crust.}},
