@article{ ISI:000289567500002,
Author = {Huelsz, Guadalupe and Gomez, Fabrisio and Pineirua, Miguel and Rojas,
   Jorge and de Alba, Mauricio and Guerra, Victor},
Title = {{Evaluation of refrigerator/freezer gaskets thermal loads}},
Journal = {{HVAC\&R RESEARCH}},
Year = {{2011}},
Volume = {{17}},
Number = {{2}},
Pages = {{133-143}},
Month = {{APR}},
Abstract = {{The gasket total thermal load is calculated as the heat transfer load
   plus the infiltration load. A method is developed to evaluate the gasket
   heat transfer load due to convective, radiative, and conductive
   mechanisms. It uses a quasi-one-dimensional theoretical model combined
   with experimental and numerical results. The gasket infiltration load is
   evaluated using the tracer gas method and humidity measurements. The
   gasket total thermal load obtained by these methods agrees with the load
   estimated by the reverse heat leak method, with the advantages of having
   a smaller uncertainty and providing information about the heat transfer
   along the perimeter of the gasket.}},
