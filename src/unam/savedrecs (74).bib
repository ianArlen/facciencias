@article{ ISI:000288637000004,
Author = {Calderon, Kadiya and Rodelas, Belen and Cabirol, Nathalie and
   Gonzalez-Lopez, Jesus and Noyola, Adalberto},
Title = {{Analysis of microbial communities developed on the fouling layers of a
   membrane-coupled anaerobic bioreactor applied to wastewater treatment}},
Journal = {{BIORESOURCE TECHNOLOGY}},
