@article{ ISI:000289002100005,
Author = {Chertoprud, Elena S. and Gheerardyn, Hendrik and Gomez, Samuel},
Title = {{Harpacticoida (Crustacea: Copepoda) of the South China Sea: faunistic
   and biogeographical analysis}},
Journal = {{HYDROBIOLOGIA}},
