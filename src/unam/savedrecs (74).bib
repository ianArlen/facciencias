@article{ ISI:000293207300003,
Author = {Hendrickx, Michel E. and Wicksten, Mary K.},
Title = {{New distribution ranges and records of caridean shrimps (Crustacea:
   Decapoda: Caridea) from the west coast of Mexico}},
Journal = {{HIDROBIOLOGICA}},
Year = {{2011}},
Volume = {{21}},
Number = {{1}},
Pages = {{26-33}},
Month = {{APR}},
Abstract = {{Geographic records are presented for 24 species of Caridea (Crustacea:
   Decapoda) along Pacific coast of Mexico, in the East Pacific. New
   records are presented for Psathyrocaris fragilis Wood-Mason, 1893 (from
   Peru to Mexico), Periclimenes infraspinis (Rathbun, 1902), Pontonia
   margarita Smith, 1869, Alpheus cristulifrons Rathbun, 1900, Alpheus umbo
   Kim \& Abele, 1988, Automate rugosa Coutiere, 1900, and Lysmata
   californica (Stimpson, 1866) (within the Gulf of California), and Typton
   hephaestus Holthuis, 1951 (from the Gulf of California to the Gulf of
   Tehuantepec, Mexico). Additional records are given that establish the
   presence of species at intermediate localities within the Gulf of
   California and along the southwestern coast of Mexico.}},
