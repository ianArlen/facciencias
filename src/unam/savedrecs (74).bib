@article{ ISI:000298079800004,
Author = {Medina Valtierra, Guadalupe and Juarez Cipres, Rocio and Pena Alvarez,
   Araceli},
Title = {{Identification and Quantification of Aldehydes in Mezcal by Solid Phase
   Microextraction with On-fiber Derivatization - Gas Cromatography}},
Journal = {{JOURNAL OF THE MEXICAN CHEMICAL SOCIETY}},
Year = {{2011}},
Volume = {{55}},
Number = {{2}},
Pages = {{84-88}},
Month = {{APR-JUN}},
Abstract = {{A headspace solid phase microextraction with on fiber derivatization
   procedure followed by gas chromatography and flame ionization detection
   was applied for the determination of aldehydes in mezcal. A
   derivatization agent o-(2,3,4,5,6-pentafluorobenzyl) hydroxylamine
   (PFBHA) was adsorbed onto a Polydimethylsiloxane/divinyl benzene
   (PDMS/DVB, 65 mu m) fiber and exposed to the headspace of a vial with a
   mezcal sample. The aldehydes selectively reacted with PFBHA, and the
   oximes were desorbed into a gas chromatograph injection port.
   Identification of the compounds was performed with standards and
   confirmed by mass spectrometry. The procedure was applied to the
   analysis of 13 mezcal samples from different agave varieties.
   Calibration curves for acetaldehyde and furfural showed good linearity
   (r(2) > 0.99) in the studied concentration range, 1-9 mu g/mL for
   acetaldehyde and 0.5-1 mu g/mL for furfural in the diluted mezcal
   samples. Good precision was obtained for acetaldehyde, CV < 6.3\% and
   furfural < 10\%. Based on the standard for tequila NOM-006-SCFI-2005,
   all mezcal samples showed concentrations of acetaldehyde within the
   norm, while 2 mezcals exceeded the allowed limit for furfural.}},
