@article{ ISI:000290602900003,
Author = {Gracia-Sanchez, Jesus and Franco, Victor and Fuentes-Mariles, Oscar A.},
Title = {{Oblique hydraulic jump in stilling basins with a trapezoidal cross
   section}},
Journal = {{TECNOLOGIA Y CIENCIAS DEL AGUA}},
Year = {{2011}},
Volume = {{2}},
Number = {{2}},
Pages = {{39-49}},
Month = {{APR-JUN}},
Abstract = {{A hydraulic device was built to analyze the oblique hydraulic jump in
   stilling basins with a trapezoidal cross-section. From the experiments
   performed, it is concluded that if asymmetric flow occurs, then it
   induces a reversal flow which causes a maximum water depth in one of the
   sides, remarkably higher than those that usually occur in the stilling
   basin. This paper proposes a theoretical approach to explain the
   observed phenomenon and a calculation method to determine the maximum
   depth. The measured and computed maximum depths agree well.}},
