@article{ ISI:000290455700014,
Author = {Verduzco, J. A. and Lemus-Ruiz, J. and Tellez-Arias, M. G. and Bedolla,
   A. and Lara, G.},
Title = {{Mechanical properties of AISI 316L stainless steels joined with a
   commercial Ni based amorphous foil}},
Journal = {{MATERIALWISSENSCHAFT UND WERKSTOFFTECHNIK}},
Year = {{2011}},
Volume = {{42}},
Number = {{5, SI}},
Pages = {{429-434}},
Month = {{MAY}},
Abstract = {{Ni based braze filler metals containing B and Si elements, prepared
   either by rapid solidification or powder metallurgy have relatively low
   melting temperatures, which make them liable to join. This work presents
   the mechanical resistance of stainless steels 316L joints, bonded with
   an amorphous Metglas (R) MBF-15 (R) foil at 1105 degrees C and 1170
   degrees C joined for 5, 10, 15, 20 and 40 minutes, under a nitrogen
   atmosphere into an electric resistance furnace. Intermixed precipitate
   particles at the joining zone were formed and an improvement on the
   bonding samples for conditions of 1105 degrees C for 10 minutes were
   observed, since the number of unjoined islands were reduced and no
   interfase crack was detected. The average shear-tensile mechanical
   resistance of the joints ranged from about 54 MPa to 70 MPa, and from 47
   MPa to 79 MPa, for samples joined at 1105 degrees C and 1170 degrees C,
   respectively.}},
