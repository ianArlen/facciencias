@article{ ISI:000290365100020,
Author = {Bhattarai, Rachana and Budke, Christine M. and Carabin, Helene and
   Proano, Jefferson V. and Flores-Rivera, Jose and Corona, Teresa and
   Cowan, Linda D. and Ivanek, Renata and Snowden, Karen F. and Flisser,
   Ana},
Title = {{Quality of Life in Patients with Neurocysticercosis in Mexico}},
Journal = {{AMERICAN JOURNAL OF TROPICAL MEDICINE AND HYGIENE}},
Year = {{2011}},
Volume = {{84}},
Number = {{5}},
Pages = {{782-786}},
Month = {{MAY}},
Abstract = {{The objective of this study was to compare quality of life measures in
   patients with neurocysticercosis (NCC) to those of a matched control
   group. The NCC outpatients and their controls were recruited from two
   neurology referral hospitals in Mexico City, Mexico during 2007-2008.
   The quality of life of 224 NCC patients was compared with 224
   age-sex-hospital-day matched controls using the short form 12 v2 (SF-12
   v2) quality of life survey. Medical chart reviews were also conducted
   for the NCC outpatients to evaluate presenting clinical manifestations.
   Compared with the controls, NCC patients had a significantly lower score
   for each of the eight domains of health evaluated and significantly
   lower Physical and Mental Component Summary scores. Chart reviews
   indicated that hydrocephalus (48\%), severe headaches (47\%), and
   epilepsy (31\%) were the most common clinical manifestations in these
   NCC outpatients.}},
