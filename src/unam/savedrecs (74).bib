@article{ ISI:000289134400011,
Author = {Palacios-Rodriguez, Yadira and Gazarian, Tatiana and Huerta, Leonor and
   Gazarian, Karlen},
Title = {{Constrained peptide models from phage display libraries highlighting the
   cognate epitope-specific potential of the anti-HIV-1 mAb 2F5}},
Journal = {{IMMUNOLOGY LETTERS}},
