@article{ ISI:000292668200026,
Author = {Fernando Lopez-Arevalo, Hugo and Gallina, Sonia and Landgrave, Rosario
   and Martinez-Meyer, Enrique and Munoz-Villers, Lyssette E.},
Title = {{Local knowledge and species distribution models' contribution towards
   mammalian conservation}},
