Author = {Perez-Lopez, R. and Legrand, D. and Garduno-Monroy, V. H. and
   Rodriguez-Pascua, M. A. and Giner-Robles, J. L.},
Title = {{Scaling laws of the size-distribution of monogenetic volcanoes within
   the Michoacan-Guanajuato Volcanic Field (Mexico)}},
