@article{ ISI:000289423500005,
Author = {Rodriguez-Sosa, Leonardo and Calderon-Rosete, Gabina and Calvillo,
   Minerva E. and Guevara, Jorge and Flores, Gonzalo},
Title = {{Dopaminergic Modulation of the Caudal Photoreceptor in Crayfish}},
