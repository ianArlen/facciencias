@article{ ISI:000289525900008,
Author = {Sofia Gonzalez-Insuasti, Martha and Casas, Alejandro and Mendez-Ramirez,
   Ignacio and Martorell, Carlos and Caballero, Javier},
Title = {{Intra-cultural Differences in the Importance of Plant Resources and
   Their Impact on Management Intensification in the Tehuacan Valley,
   Mexico}},
Journal = {{HUMAN ECOLOGY}},
Year = {{2011}},
Volume = {{39}},
Number = {{2}},
Pages = {{191-202}},
Month = {{APR}},
Abstract = {{Management of plant resources is a complex process that involves the
   interaction of numerous cultural, environmental, economic, and
   ecological factors Therefore, understanding factors influencing
   management decisions requires multidisciplinary approaches, including
   both quantitative and qualitative perspectives. This study explores
   quantitative approaches as methodological tools with which to identify
   patterns underlying more intensive management of plant resources. The
   research was conducted in the village of Santa Maria Tecomavaca, Oaxaca,
   in south-central M,xico, within the semiarid Tehuacan-Cuicatlan
   Biosphere Reserve. Three models explaining the management intensity
   tendencies were identified, consistent with the pattern of multiple
   variables influencing plant management.}},
