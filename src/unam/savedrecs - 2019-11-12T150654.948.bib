@article{ ISI:000179714300016,
Author = {Ayala, A and Cuautle, E and Herrera, G and Montano, LM},
Title = {{Lambda(0) polarization in ultrarelativistic nucleus-nucleus collisions}},
Journal = {{ACTA PHYSICA HUNGARICA NEW SERIES-HEAVY ION PHYSICS}},
Year = {{2002}},
Volume = {{16}},
Number = {{1-4}},
Pages = {{141-152}},
Note = {{4th Latin American Symposium on Nuclear Physics, COLEGIO NACL, MEXICO
   CITY, MEXICO, SEP 24-28, 2001}},
Abstract = {{We study the polarization of Lambda(0)'s produced in ultrarelativistic
   heavy-ion collisions. We argue that when the density of participants in
   the reaction is below the critical value for the production of a
   Quark-Gluon Plasma (QGP), recombination processes dominate the Lambda(0)
   production cross section and hence, Lambda(0)'s are polarized. However,
   when the density of participants is above the critical value for QGP
   formation, coalescence processes dominate Lambda(0) production, giving
   rise to a reduction in the Lambda(0) polarization. For densities below
   the critical density for QGP production, we describe Lambda(0)
   polarization in terms of the DeGrand-Miettinen model and correct for the
   effects introduced by multiple scattering of the produced Lambda(0)
   within the nuclear environment.}},
