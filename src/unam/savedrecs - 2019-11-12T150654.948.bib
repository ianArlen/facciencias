@inproceedings{ ISI:000187006800013,
Author = {Vera-Graziano, R and Torres, FR and Ordonez-Medrano, A},
Editor = {{Mittal, KL}},
Title = {{A study of contact angles on optoelectronic materials}},
Booktitle = {{CONTACT ANGLE, WETTABILITY AND ADHESION, VOL 2}},
Year = {{2002}},
Pages = {{239-251}},
Note = {{2nd International Symposium on Contact Angle, Wettability and Adhesion,
   NEWARK, NJ, JUN 21-23, 2000}},
Organization = {{MST Conf}},
Abstract = {{Contact angle measurements were made to allow us to study the effect of
   wavelength of illumination on cadmium sulfide and diamond thin films,
   and also on non-etched and etched surfaces of crystalline silicon and
   amorphous hydrogenated silicon thin films. The advancing and receding
   contact angles of distilled water were measured in the dark and at three
   wavelengths of illumination: red (640.4 nm), green (536.2 nm), and
   violet (402.8 nm). The contact angles measured by the sessile drop were
   confirmed by the Wilhelmy slide method. Our results indicate that the
   equilibrium contact angles of water not only depend on substrate
   chemical structure, but also on the wavelength of illumination, and the
   treatment of the sample surface. These surface phenomena related to the
   surface photovoltaic and electrical double layer effects are completely
   reversible provided steady state conditions exist. A method based on the
   equations of Lippmann and Young allows us to determine the electrical
   character, p-type or n-type, of each material surface under
   illumination, and to interpret the effect of the wavelength of
   illumination on the contact angles.}},
