@inproceedings{ ISI:000177066800032,
Author = {Sanchez, A and Ayala, A},
Editor = {{DiazCruz, JL and Engelfried, J and Kirchbach, M and Mondragon, M}},
Title = {{Boundary and expansion effects on two-pion correlation function in
   relativistic heavy-ion collisions}},
