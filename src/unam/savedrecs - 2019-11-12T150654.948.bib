@inproceedings{ ISI:000178271800035,
Author = {Masciadri, E},
Editor = {{Vernin, J and Benkhaldoun, Z and MunozTunon, C}},
Title = {{What can meteorological numerical models do for the astronomers?}},
Booktitle = {{ASTRONOMICAL SITE EVALUATION IN THE VISIBLE AND RADIO RANGE}},
