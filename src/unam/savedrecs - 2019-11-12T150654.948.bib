@article{ ISI:000177217000056,
Author = {Ruiz, AJP and Castaneda-Guzman, R and Muniz, MV and Reyes, M},
Title = {{Depolarization in real time by photoacoustic.}},
Journal = {{FERROELECTRICS}},
Year = {{2002}},
Volume = {{273}},
Pages = {{2711-2716}},
Note = {{10th International Meeting on Ferroelectricity (IMF-10), MADRID, SPAIN,
   SEP 03-07, 2001}},
Abstract = {{We present the results of depolarization measurements in ferroelectric
   ceramics based on lead and barium titanate, with the photoacoustic
   method. This method is based on real time monitoring of the acoustic
   signal with PZT sensors after an excitation of the sample with laser
   pulses.
   We present the polarization condition, and the photoacoustic
   measurements before and after polarization. The photoacoustic signal
   analysis revealed clearly the beginning and final temperatures of the
   depolarization, and it is also possible to observe the Curie temperature
   of the ferro-paraelectric structural change. The presented analysis
   shows, furthermore, the thermal stability of these materials.}},
