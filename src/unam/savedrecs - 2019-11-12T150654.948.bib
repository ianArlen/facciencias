@article{ ISI:000173933600005,
Author = {Charrin, L and Becquart-Gallissian, A and Combe, A and Gonzalez, G and
   Charai, A},
Title = {{Key experimental parameters for internal-band formation: Relationship
   between stress and oxidation kinetics in silver-magnesium alloys}},
Journal = {{OXIDATION OF METALS}},
Year = {{2002}},
Volume = {{57}},
Number = {{1-2}},
Pages = {{81-98}},
Month = {{FEB}},
Abstract = {{Microstructural features of dilute (1.3 to 5 at.\%) Ag-Mg internally
   oxidized alloys were studied at temperatures from 400 to 750degreesC The
   experimental conditions needed for oxide-band formation were determined.
   A model is proposed based on kinetics and stress-relaxation
   considerations that explains band formation from the first stages. The
   kinetics are driven by oxygen diffusion and the formation of
   hyperstoichiometric MgO clusters leading to a dilatation of the crystal.
   If the Mg content is high enough (>3 at.\%), the stress state induced by
   the clusters form a diffusion barrier that is responsible for the
   formation of internal oxide bands.}},
