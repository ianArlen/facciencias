@article{ ISI:000173003100002,
Author = {Ramirez, E and Cabeza, M and Heuze, I and Gutierrez, E and Bratoeff, E
   and Membrillo, M and Lira, A},
Title = {{Synthesis and pharmacological evaluation of new 16-methyl pregnane
   derivatives}},
