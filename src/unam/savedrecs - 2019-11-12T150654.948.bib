@inproceedings{ ISI:000179251300013,
Author = {Echevarria, J and Costero, R and Tovmassian, G and Zharikov, S and
   Michel, R and Richer, M and Arellano-Ferro, A},
Editor = {{Hernanz, M and Jose, J}},
Title = {{The 2001 superoutburst of WZ Sge: Is there any connection with Nova
   Outbursts?}},
Booktitle = {{CLASSICAL NOVA EXPLOSIONS}},
Series = {{AIP CONFERENCE PROCEEDINGS}},
Year = {{2002}},
Volume = {{637}},
Pages = {{77-81}},
Note = {{International Conference on Classical Nova Explosions, SITGES, SPAIN,
   MAY 20-24, 2002}},
Organization = {{Inst Space Res Catalonia; Natl Res Council Spain; Polytech Univ
   Catalonia; Spanish Minist Sci \& Technol; Dept Univ \& Res, Generalitat
   Catalunya}},
Abstract = {{We have observed WZ Sge with high resolution echelle spectroscopy during
   the 2001 superoutburst. The hot-spot arising near the secondary star is
   again visible 15 days after its first detection. We have been able to
   measure its position as a function of orbital phase directly from
   individual spectra. We derive a a semiamplitude of K-hs = 457 +/-
   16kms(-1) and a systemic velocity of gamma = 0 +/- 14km s(-1). We also
   show that the accretion disc has an ellipsoidal shape with a value
   around e = 0.75. Based on these facts we derive a phase shift of
   DeltaPhi = -0. 12 between the eclipse and the inferior conjuntion
   ephemeris. If this is the case, then the hot-spot appears located at the
   trailing side of the red dwarf and must be the result of in-falling
   material back to the donor star.}},
