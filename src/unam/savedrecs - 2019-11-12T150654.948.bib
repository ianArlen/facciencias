Editor = {{Jabbour, GE and Carter, SA and Kido, J and Lee, ST and Sariciftci, NS}},
Title = {{Behavior comparison of PMMA-PTEGDMA copolymerized by two different
   methods}},
Booktitle = {{ORGANIC AND POLYMERIC MATERIALS AND DEVICES-OPTICAL, ELECTRICAL AND
   OPTOELECTRONIC PROPERTIES}},
Series = {{Materials Research Society Symposium Proceedings}},
