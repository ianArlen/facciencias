@inproceedings{ ISI:000180752500043,
Author = {Kurtz, S and Martos, M and Allen, C and Franco, J},
Editor = {{Grebel, EK and Brandner, W}},
Title = {{Spiral-density-wave-induced star formation at high galactic latitudes}},
Booktitle = {{MODES OF STAR FORMATION AND THE ORIGIN OF FIELD POPULATIONS, PROCEEDINGS}},
Series = {{ASTRONOMICAL SOCIETY OF THE PACIFIC CONFERENCE SERIES}},
