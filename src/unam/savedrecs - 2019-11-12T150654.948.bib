@article{ ISI:000173616500010,
Author = {Xiao, MF},
Title = {{A simple method of forming small gold particles on a thin gold film}},
Journal = {{MATERIALS LETTERS}},
Year = {{2002}},
Volume = {{52}},
Number = {{4-5}},
Pages = {{301-303}},
Month = {{FEB}},
Abstract = {{We have produced gold particles whose sizes range from several tenth
   nanometers up to about 1 mum. The small gold particles are distributed
   on a gold thin film of thickness 50-80 Angstrom. The method for the
   formation is simple. We first evaporated the gold thin film, and then
   applied DC field along the film. When the field increases from 0 to 30
   V, and the measured current increases from 0 to 650 mA, the particles
   were formed. The sizes and the distribution distances could be
   controlled by the applied DC field. (C) 2002 Elsevier Science B.V. All
