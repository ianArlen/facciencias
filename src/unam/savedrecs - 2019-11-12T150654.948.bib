@inproceedings{ ISI:000174597300029,
Author = {Flisser, A},
Editor = {{Craig, P and Pawlowski, Z}},
Title = {{Risk factors and control measures for taeniosis/cysticercosis}},
Booktitle = {{CESTODE ZOONOSES: ECHINOCOCCOSIS AND CYSTICERCOSIS: AN EMERGENT AND
   GLOBAL PROBLEM}},
