@inproceedings{ ISI:000181977500005,
Author = {Anaya, AL and Waller, GR and Owuor, PO and Friedman, J and Chou, CH and
   Suzuki, T and Arroyo-Estrada, JF and Cruz-Ortega, R},
Editor = {{Reigosa, MJ and Pedrol, N}},
Title = {{The role of caffeine in the production decline due to autotoxicity in
   coffee and tea plantations}},
Booktitle = {{ALLELOPATHY: FROM MOLECULES TO ECOSYSTEMS}},
Year = {{2002}},
Pages = {{71-91}},
Note = {{1st European Allelopathy Symposium, VIGO, SPAIN, 2001}},
Organization = {{European Allelopathy Soc; Int Allelopathy Soc}},
Abstract = {{Autotoxicity occurs when a plant releases toxic chemical substances into
   the environment that inhibit germination and growth of same plant
   species. Autotoxicity has been known to occur in a number of weeds and
   crops in natural ecosystems and agroecosystems where it causes a number
   of ecological and economic implications such as a decline in crop yield
   due to soil sickness, regeneration failure of natural forests, and
   replant problem in orchards, among others. Tea and coffee are mentioned
   as examples of allelopathic and autotoxic plants. Caffeine is present to
   the greatest extent in fruits of coffee and in the young leaves of tea.
   Young roots of coffee and tea are highly susceptible to caffeine.
   Aqueous solutions of caffeine of various concentrations significantly
   inhibited rootlet growth of coffee seedlings. It has been shown that one
   of the modes of action of caffeine is arresting cell division by
   preventing cell plate formation. Caffeine may act by incorporation into
   DNA and RNA chains. Caffeine also causes irregularities in root cells of
   maize. These cells are larger and show irregular shapes, two nuclei,
   diffuse nuclei, and also two or more nucleoli. Caffeine and other
   naturally toxic compounds accumulated in soils may be the cause of the
   decrease of tea and coffee production. Tea and coffee plantations that
   are clean of plant residues have no accumulation of secondary
   metabolites and are thus relatively free of autotoxicity due to
   caffeine.}},
