Author = {Petranovskii, V and Gurin, V and Bogdanchikova, N and Phatanasri, S and
   Praserthdam, P},
Editor = {{Lakhtakia, A and Dewar, G and McCall, MW}},
Title = {{An effect of zeolite type upon properties of copper nanoparticles and
   clusters produced within them}},
Booktitle = {{COMPLEX MEDIUMS III: BEYOND LINEAR ISOTROPIC DIELECTRICS}},
Series = {{Proceedings of SPIE}},
Year = {{2002}},
