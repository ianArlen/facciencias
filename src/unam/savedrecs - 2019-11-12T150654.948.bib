@article{ ISI:000174564900016,
Author = {Gutierrez-Wing, C and Ascencio, JA and De la Cruz, W and Cota, L and
   Jose-Yacaman, M},
Title = {{Observation of sulfur on the surface of giant aurothiol nanoclusters}},
Journal = {{JOURNAL OF NANOSCIENCE AND NANOTECHNOLOGY}},
Year = {{2002}},
Volume = {{2}},
Number = {{1}},
Pages = {{101-105}},
Month = {{FEB}},
Abstract = {{We report the study of aurothiol nanoclusters using high-resolution
   electron microscopy, energy loss spectroscopy, X-ray photoelectron
   spectroscopy, Auger spectroscopy, and microscopy. It is concluded that
   the sulfur atoms are located on the surface of the gold nanoparticles in
   both (100) and (111) microfacets. The X-ray photoelectron spectroscopy
   data show that there is a Au-Au bond as well as a Au-S bond. Auger depth
   profile measurements made by sputtering of the nanoparticles
   corroborates that the sulfur is located on the surface of the
   nanoparticle. Quantitative Auger analysis indicates a ratio Au/S between
   approximate to1.79 and 1.98.}},
