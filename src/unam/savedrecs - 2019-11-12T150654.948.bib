@article{ ISI:000179714300003,
Author = {Yepez, H and Hess, PO and Misicu, S},
Title = {{Heavy nuclear molecules with 2 and 3 clusters}},
Journal = {{ACTA PHYSICA HUNGARICA NEW SERIES-HEAVY ION PHYSICS}},
Year = {{2002}},
Volume = {{16}},
Number = {{1-4}},
Pages = {{19-26}},
Note = {{4th Latin American Symposium on Nuclear Physics, COLEGIO NACL, MEXICO
   CITY, MEXICO, SEP 24-28, 2001}},
Abstract = {{We review geometric models for long-lived nuclear molecules with three
   clusters. Problems arise when the center of masses are in a triangular
   configuration and inner structure of the clusters has to be introduced.
   Possible solutions come from algebraic models. Returning to a simpler
   model, we discuss the description of a two-cluster molecule with an
   algebraic Hamiltonian. The geometrical content of the model is discussed
   via the use of coherent states. The relation of the physical distance
   variable between the clusters and the parameter of the coherent state is
   given. The final potential is orientation dependent and can be adjusted
   to the potential as obtained in double folding calculations.}},
