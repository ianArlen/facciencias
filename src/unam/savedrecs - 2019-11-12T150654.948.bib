@article{ ISI:000174615900002,
Author = {Chryssomalakos, C and Quevedo, H and Rosenbaum, M and Vergara, JD},
Title = {{Normal coordinates and primitive elements in the Hopf algebra of
   renormalization}},
Journal = {{COMMUNICATIONS IN MATHEMATICAL PHYSICS}},
Year = {{2002}},
Volume = {{225}},
Number = {{3}},
Pages = {{465-485}},
Month = {{FEB}},
Abstract = {{We introduce normal coordinates on the infinite dimensional group G
   introduced by Connes and Kreimer in their analysis of the Hopf algebra
   of rooted trees. We study the primitive elements of the algebra and show
   that they are generated by a simple application of the inverse Poincare
   lemma, given a closed left invariant 1-form on G. For the special case
   of the ladder primitives, we find a second description that relates them
   to the Hopf algebra of functionals on power series with the usual
   product. Either approach shows that the ladder primitives are given by
   the Schur polynomials. The relevance of the lower central series of the
   dual Lie algebra in the process of renormalization is also discussed,
   leading to a natural concept of k-primitiveness, which is shown to be
   equivalent to the one already in the literature.}},
