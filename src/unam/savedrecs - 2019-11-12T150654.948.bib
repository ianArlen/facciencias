@inproceedings{ ISI:000179110000029,
Author = {Ayala, A and Cuautle, E and Herrera, G and Montano, LM},
Editor = {{Elze, HT and Ferreira, E and Kodama, T and Letessier, J and Rafelski, J and Thews, RL}},
Title = {{Lambda(0) polarization and QGP production in high-energy heavy-ion
   collisions}},
Booktitle = {{NEW STATES OF MATTER IN HADRONIC INTERACTIONS}},
Series = {{AIP CONFERENCE PROCEEDINGS}},
Year = {{2002}},
Volume = {{631}},
Pages = {{620-630}},
Note = {{PASI New States of Matter in Hadronic Interactions Meeting, CAMPOS DO
   JORDAO, BRAZIL, JAN 07-18, 2002}},
Organization = {{Pan Amer Study Inst; US DOE; US Natl Sci Fdn; CLAF; CNPq; FAPESP; FAPERJ}},
Abstract = {{We study the polarization of Lambda(0)'s produced in ultra-relativistic
   heavy-ion collisions. We argue that when the density of participants in
   the reaction is below the critical value for the production of a
   Quark-Gluon Plasma (QGP), recombination processes dominate the Lambda(0)
   production cross section and hence, Lambda(0)'s are polarized. However,
   when the density of participants is above the critical value for QGP
   formation, coalescence processes dominate, giving rise to a reduction in
   the Lambda(0) polarization. For densities below the critical density for
   QGP production, we describe Lambda(0) polarization in terms of the
   DeGrand-Miettinen model correcting for the effects introduced by
   multiple scattering of the produced Lambda(0)'s within the nuclear
   environment.}},
