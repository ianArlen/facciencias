@article{ ISI:000172994500028,
Author = {Hernandez-Cordero, J and Kozlov, VA and Morse, TF},
Title = {{Highly accurate method for single-mode fiber laser wavelength
   measurement}},
Journal = {{IEEE PHOTONICS TECHNOLOGY LETTERS}},
Year = {{2002}},
Volume = {{14}},
Number = {{1}},
Pages = {{83-85}},
Month = {{JAN}},
Abstract = {{We propose a simple technique by which the wavelength of a single-mode
   fiber laser can be easily measured to accuracy of the order of 1 pm. The
   technique is based on the wavelength dependence of polarization mode
   beating frequencies. Direct application of this method for wavelength
   stabilization of fiber lasers and the possibility of obtaining
   information about the polarization mode dispersion of the fiber used
   within the laser cavity are discussed.}},
