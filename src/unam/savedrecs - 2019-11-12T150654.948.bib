@inproceedings{ ISI:000178976600026,
Author = {Koenigsberger, G and Moreno, E},
Editor = {{Chavez, M and Bressan, A and Buzzoni, A and Mayya, D}},
Title = {{Comments on binary star evolution and the small magellanic cloud system
   HD 5980}},
