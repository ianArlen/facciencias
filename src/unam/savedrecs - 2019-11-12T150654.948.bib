@article{ ISI:000173210400005,
Author = {Illanes, A},
Title = {{Hyperspaces of arcs and two-point sets in dendroids}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2002}},
Volume = {{117}},
Number = {{3}},
Pages = {{307-317}},
Month = {{FEB 14}},
Abstract = {{Let X be a metric continuum. Let A(X) = \{A subset of X: A is an arc or
   a one-point set\} and F-2(X) = \{A subset of X: A is nonempty and A has
   at most two points\}. In this paper we show that if X is a fan (i.e., X
   is a dendroid with exactly one ramification point) and A(X) is
   homeomorphic to F,7 (X), then X is a dendrite. The question whether this
   implication is true for all dendroids is still open. (C) 2002 Elsevier
   Science B.V. All rights reserved.}},
