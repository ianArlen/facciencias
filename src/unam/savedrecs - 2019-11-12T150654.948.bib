@inproceedings{ ISI:000182119400019,
Author = {Avila-Reese, V and Firmani, C and Zavala, J},
Editor = {{Rosado, M and Binnette, L and Arias, L}},
Title = {{The shape of rotation curves: Models vs observations}},
Booktitle = {{GALAXIES: THE THIRD DIMENSION}},
