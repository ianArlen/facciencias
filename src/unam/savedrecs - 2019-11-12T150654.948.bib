@inproceedings{ ISI:000177665400037,
Author = {Botello, AV and Mondragon, JL and Olmedo, CFV and Villanueva, SF},
Editor = {{Pellei, M and Porta, A and Hinchee, RE}},
Title = {{Petroleum pollution in sediments from coastal areas of Tabasco, Mexico}},
Booktitle = {{CHARACTERIZATION OF CONTAMINATED SEDIMENTS}},
Year = {{2002}},
Pages = {{303-310}},
Note = {{1st International Conference on Remediation of Contaminated Sediments,
   VENICE, ITALY, OCT 10-12, 2001}},
Organization = {{Battelle Geneva Res Ctr; Azienda Multiservizi Ambientali Veneziana}},
Abstract = {{An integrated and multidisciplinary assessment of the effects of the oil
   industry on fragile coastal systems in Tabasco State was accomplished
   with the concourse of several international institutions and Mexican
   universities between 1996 and 2000. The wetlands studied are important
   areas for the feeding, nesting and growth of several species, showing a
   very high level of biodiversity. Ninety-nine sediment samples were taken
   using a stainless steel I-It Van Veen grab in 10 coastal lagoons and
   three rivers, all receiving, directly or indirectly the refuse of the
   oil activities carried out in the region. Aliphatics and polycyclic
   aromatic hydrocarbon (PAHs) were quantified and identified by means of
   HRCGC and their presence confirmed by GC-MS. Additionally the organic
   content, type and grain size of sediments, as well as the carbon isotope
   ratios, were measured in each sediment sample. Results showed that the
   sediment samples had a complex mixture of hydrocarbons originated by
   biogenic, petrogenic, and pyrolitic sources; and only two lagoons were
   considered ``hot spots{''} showing carbon isotope ratios similar to
   crude oil. However, most of the PAHs were characterized as pyrolitic
   compounds, being dominated by those formed of four and five benzene
   rings; there was a high concentration of benzo(a)pyrene in the samples
   taken in the Yucateco y Limon lagoons.}},
