@article{ ISI:000361934000008,
Author = {Orduna, Vladimir},
Title = {{Impulsivity and sensitivity to amount and delay of reinforcement in an
   animal model of ADHD}},
Journal = {{BEHAVIOURAL BRAIN RESEARCH}},
