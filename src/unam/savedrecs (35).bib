@article{ ISI:000362508300001,
Author = {Rios, H. and Davila, J. and Fridman, L. and Edwards, C.},
Title = {{Fault detection and isolation for nonlinear systems via
   high-order-sliding-mode multiple-observer}},
Journal = {{INTERNATIONAL JOURNAL OF ROBUST AND NONLINEAR CONTROL}},
Year = {{2015}},
Volume = {{25}},
Number = {{16}},
Pages = {{2871-2893}},
Month = {{NOV 10}},
Abstract = {{In this paper, fault detection and isolation problems are studied for a
   certain class of nonlinear systems. Under some structural conditions,
   multiple high-order sliding-mode observers are proposed. The value of
   the equivalent output injection is used for detecting faults and the
   multiple-model approach for isolating particular faults in the system.
   The proposed method provides fast detection and isolation of actuator
   and plant faults. Simulation results support the proposed approach.
