@article{ ISI:000366994700045,
Author = {Hacyan, Shahen},
Title = {{Galileo and the equivalence principle: a faulty argument with the
   correct conclusion}},
Journal = {{EUROPEAN JOURNAL OF PHYSICS}},
Year = {{2015}},
Volume = {{36}},
Number = {{6}},
Month = {{NOV}},
Abstract = {{Galileo asserted that the fall of bodies is independent of their weight.
   Whether or not he relied on actual experiments, he explicitly argued in
   his writings that this fact could be proved by purely logical reasoning.
   However, there is an important flaw in the argument he provided.
   Clarification of this point may help one to grasp the full significance
   of the equivalence principle, which is usually overlooked in many
   physics courses.}},
