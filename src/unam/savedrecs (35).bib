Author = {Martinez, Federico and Olvera-Sanchez, Sofia and Esparza-Perusquia,
   Mercedes and Gomez-Chang, Erika and Flores-Herrera, Oscar},
Title = {{Multiple functions of syncytiotrophoblast mitochondria}},
Journal = {{STEROIDS}},
Year = {{2015}},
Volume = {{103}},
