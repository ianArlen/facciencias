@article{ ISI:000366054300068,
Author = {Ramirez, G. and Nunez, J. and Hernandez, G. N. and Hernandez-Cruz, G.
   and Ramos, E.},
Title = {{Note: Device for obtaining volumetric, three-component velocity fields
   inside cylindrical cavities}},
Journal = {{REVIEW OF SCIENTIFIC INSTRUMENTS}},
Year = {{2015}},
Volume = {{86}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{We describe a device designed and built to obtain the three-component,
   steady state velocity field in the whole volume occupied by a fluid in
   motion contained in a cavity with cylindrical walls. The prototype
   comprises a two-camera stereoscopic particle image velocimetry system
   mounted on a platform that rotates around the volume under analysis and
   a slip ring arrangement that transmits data from the rotating sensors to
   the data storage elements. Sample observations are presented for natural
   convection in a cylindrical container but other flows can be analyzed.
