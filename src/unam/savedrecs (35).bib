@article{ ISI:000366339600053,
Author = {Han, Chang Wan and Majumdar, Paulami and Marinero, Ernesto E. and
   Aguilar-Tapia, Antonio and Zanella, Rodolfo and Greeley, Jeffrey and
   Ortalan, Volkan},
Title = {{Highly Stable Bimetallic Aulr/TiO2 Catalyst: Physical Origins of the
   Intrinsic High Stability against Sintering}},
Journal = {{NANO LETTERS}},
Year = {{2015}},
Volume = {{15}},
Number = {{12}},
Pages = {{8141-8147}},
Month = {{DEC}},
Abstract = {{It has been a long-lived research topic in the field of heterogeneous
   catalysts to find a way of stabilizing supported gold catalyst against
   sintering. Herein, we report highly stable AuIr bimetallic nanopartides
   on TiO2 synthesized by sequential deposition-precipitation. To reveal
   the physical origin of the high stability of AuIr/TiO, we used
   aberration-corrected scanning transmission electron microscopy (STEM),
   STEM-tomography, and density functional theory (DFT) calculations.
   Three-dimensional structures of Aulr/TiO2 obtained by STEM-tomography
   indicate that AuIr nanopartides on TiO2 have intrinsically lower free
   energy and less driving force for sintering than Au nanopartides. DFT
   calculations on segregation behavior of AuIr slabs on TiO2 showed that
   the presence of Jr near the TiO2 surface increases the adhesion energy
   of the bimetallic slabs to the TiO2 and the attractive interactions
   between Ir and TiO2 lead to higher stability of AuIr nanoparticles as
   compared to Au nanopartides.}},
