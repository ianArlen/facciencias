@article{ ISI:000366723600014,
Author = {Marquez, Raymundo and Tapia, Alan and Bernal, Miguel and Fridman, Leonid},
Title = {{LMI-based second-order sliding set design using reduced order of
   derivatives}},
Journal = {{INTERNATIONAL JOURNAL OF ROBUST AND NONLINEAR CONTROL}},
Year = {{2015}},
Volume = {{25}},
Number = {{18}},
Pages = {{3763-3779}},
Month = {{DEC}},
Abstract = {{Sliding mode control design for systems with relative degree r requires
   a number r - 1 of time-derivatives of the system output, which usually
   leads to deterioration of the whole scheme; if the highest-order
   derivative is spared, a better precision is ensured. This paper proposes
   a control algorithm that guarantees reaching a second-order sliding
   manifold using only r - 2 derivatives of the system output. This
   objective is achieved at the price of yielding finite-time convergence
   while preserving the essential feature of insensitivity to matched
   disturbances. The results take full advantage of convex representations
   and linear matrix inequalities, whose formulation easily allows dealing
   with unmatched disturbances by convex optimization techniques already
   implemented in commercially available software. Simulation examples are
   included to show the effectiveness of the proposed approach. Copyright
   (C) 2014 John Wiley \& Sons, Ltd.}},
