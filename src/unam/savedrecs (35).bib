@article{ ISI:000365392100026,
Author = {Cacho, N. Ivalu and Kliebenstein, Daniel J. and Strauss, Sharon Y.},
Title = {{Macroevolutionary patterns of glucosinolate defense and tests of
   defense-escalation and resource availability hypotheses}},
Journal = {{NEW PHYTOLOGIST}},
