@article{ ISI:000365132200004,
Author = {Sahagun-Ruiz, Alfredo and Velazquez, Leticia V. and Bhaskaran, Shoba and
   Jay, Chris M. and Morales-Salinas, E. and Rathore, Keerti and Wagner,
   Gale G. and Waghela, Suryakant D.},
Title = {{Reduction of enterotoxin induced fluid accumulation in ileal loops of
   neonatal calves with anti-F5 fimbriae recombinant antibody}},
Journal = {{VETERINARY RESEARCH COMMUNICATIONS}},
Year = {{2015}},
Volume = {{39}},
Number = {{4}},
Pages = {{229-236}},
Month = {{DEC}},
Abstract = {{Neonatal calf colibacillosis caused by enterotoxigenic Escherichia coli
   (ETEC) is an economically significant problem in most parts of the
   world. The most common ETEC found in calves express the F5 (K99)
   fimbriae, which are necessary for the attachment of the bacteria to the
   ganglioside receptors on enterocytes. It is known that prevention of
   ETEC F5(+) adhesion to its ganglioside receptors with specific
   antibodies protects calves from colibacillosis. Previously we have
   described the development and characterization of a mouse recombinant
   antibody fragment (moRAb) that prevents F5 fimbrial protein induced
   agglutination of horse red blood cells (HRBC), which exhibit the same
   gangloside receptor for F5 fimbriae. Here we demonstrate that this
   recombinant antibody fragment inhibits in vitro the attachment of ETEC
   F5(+) bacteria to HRBC as well as isolated calf enterocytes, and in vivo
   it decreases fluid accumulation in intestinal loops of calves. Thus,
   correct oral administration of this anti-F5 moRAb may serve as an
   immunoprophylactic for cost effective control of colibacillosis in
   calves.}},
