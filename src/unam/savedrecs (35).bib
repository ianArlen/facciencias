@article{ ISI:000209909100018,
Author = {Vellinga, Else C. and Kuyper, Thomas W. and Ammirati, Joe and Desjardin,
   Dennis E. and Halling, Roy E. and Justo, Alfredo and Laessoe, Thomas and
   Lebel, Teresa and Lodge, D. Jean and Matheny, P. Brandon and Methven,
   Andrew S. and Moreau, Pierre-Arthur and Mueller, Gregory M. and
   Noordeloos, Machiel E. and Nuytinck, Jorinde and Ovrebo, Clark L. and
   Verbeken, Annemieke},
Title = {{Six simple guidelines for introducing new genera of fungi}},
Journal = {{IMA FUNGUS}},
Year = {{2015}},
Volume = {{6}},
Number = {{2}},
Pages = {{65-68}},
Month = {{DEC}},
Abstract = {{We formulate five guidelines for introducing new genera, plus one
   recommendation how to publish the results of scientific research. We
   recommend that reviewers and editors adhere to these guidelines. We
   propose that the underlying research is solid, and that the results and
   the final solutions are properly discussed. The six criteria are: (1)
   all genera that are recognized should be monophyletic; (2) the coverage
   of the phylogenetic tree should be wide in number of species, geographic
   coverage, and type species of the genera under study; (3) the branching
   of the phylogenetic trees has to have sufficient statistical support;
   (4) different options for the translation of the phylogenetic tree into
   a formal classification should be discussed and the final decision
   justified; (5) the phylogenetic evidence should be based on more than
   one gene; and (6) all supporting evidence and background information
   should be included in the publication in which the new taxa are
   proposed, and this publication should be peer-reviewed.}},
