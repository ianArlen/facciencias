@article{ ISI:000365414400003,
Author = {Gutierrez-Pineres, Antonio C. and Lopez-Monsalvo, Cesar S. and Quevedo,
   Hernando},
Title = {{Variational thermodynamics of relativistic thin disks}},
Journal = {{GENERAL RELATIVITY AND GRAVITATION}},
Year = {{2015}},
Volume = {{47}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{We present a relativistic model describing a thin disk system composed
   of two fluids. The system is surrounded by a halo in the presence of a
   non-trivial electromagnetic field. We show that the model is compatible
   with the variational multifluid thermodynamics formalism, allowing us to
   determine all the thermodynamic variables associated with the matter
   content of the disk. The asymptotic behavior of these quantities
   indicates that the single fluid interpretation should be abandoned in
   favor of a two-fluid model.}},
