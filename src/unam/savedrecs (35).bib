@article{ ISI:000365431300004,
Author = {Vargas, R. O. and Lira-Galeana, C. and Silva, D. and Manero, O.},
Title = {{Inflow performance of wells intersecting long fractures with transient
   pressure and variable flow-rate behavior}},
Journal = {{JOURNAL OF PETROLEUM SCIENCE AND ENGINEERING}},
Year = {{2015}},
Volume = {{136}},
Pages = {{23-31}},
Month = {{DEC}},
Abstract = {{A mathematical model which predicts the performance of vertical and
   horizontal wells intersecting fractures is presented. Based on the
   formulation originally proposed by duo and Schechter (1997), this model
   predicts the transient pressure behavior with variable flow-rate both
   during the increasing pressure stage and in the declining pressure and
   flow-rate situation. The flow in the matrix and that in the fracture are
   coupled to allow the calculation of the pressure distribution in the
   matrix and in the fracture, as functions of space and time. The model is
   consistent with transient pressure analyses in finite-conductivity
   vertical fractures (Cinco-Ley and Samaniego, 1981), and presents a
   useful tool for productivity analyses of hydraulically fractured wells
   during the rapid decline stage. Comparisons with real well data are
   analyzed and discussed. (C) 2015 Elsevier B.V. All rights reserved.}},
