@article{ ISI:000365686500006,
Author = {Vargas-Almeida, Alexander and Angel Olivares-Robles, Miguel and Mendez
   Lavielle, Federico},
Title = {{Performance of a Composite Thermoelectric Generator with Different
   Arrangements of SiGe, BiTe and PbTe under Different Configurations}},
