@article{ ISI:000364244400036,
Author = {Garcia-Rocha, R. and Moreno-Lafont, M. and Mora-Garcia, M. L. and
   Weiss-Steider, B. and Montesinos, J. J. and Pina-Sanchez, P. and
   Monroy-Garcia, A.},
Title = {{Mesenchymal stromal cells derived from cervical cancer tumors induce
   TGF-beta 1 expression and IL-10 expression and secretion in the cervical
   cancer cells, resulting in protection from cytotoxic T cell activity}},
Journal = {{CYTOKINE}},
Year = {{2015}},
Volume = {{76}},
Number = {{2}},
Pages = {{382-390}},
Month = {{DEC}},
Abstract = {{Cervical cancer (CeCa) tumors are characterized by increased expression
   of TGF-beta 1 and IL-10, which are correlated with downregulated
   expression of major histocompatibility complex class I antigens (HLA-I)
   on cancer cells and a reduced immune response mediated by cytotoxic T
   lymphocytes (Cas). Mesenchymal stromal cells (MSCs) are important
   components in the tumor microenvironment that have been suggested to
   contribute to cancer progression through the induction of TGF-beta 1 and
   IL-10. In this study, we provided evidence that MSCs derived from
   cervical tumors (CeCa-MSCs) cocultured with CeCa cells induced
   significant expression of TGF-beta 1 and secretion of IL-10 by CeCa
   cells compared to MSCs derived from the normal cervix (NCx-MSCs) and
   normal bone marrow (BM-MSCs; gold standard). This increase in expression
   was associated with a significant downregulation of HLA-I molecules and
   protection of the cells against specific CTL lysis. Interestingly, the
   addition of the neutralizing antibody anti-TGF-beta to the
   CeCa/CeCa-MSCs coculture strongly inhibited the expression and
   production of IL-10 by CeCa cells. Anti-TGF-beta as well as anti-IL-10
   also abolished HLA-I downregulation, and reversed the inhibition of CTL
   cytotoxicity. These results provide evidence that TGF-beta 1 and IL-10
   could play an important role in the downregulation of HLA-I molecules on
   CeCa cells induced by tumor MSCs. Our findings suggest a novel mechanism
   through which MSCs may protect tumor cells from immune recognition by
   specific CTLs. (C) 2015 Elsevier Ltd. All rights reserved.}},
