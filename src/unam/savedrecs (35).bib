@article{ ISI:000366712000012,
Author = {Calderon-Cruz, Beatriz and Rodriguez-Galvan, Karen and Antonio
   Manzo-Francisco, Luis and Vargas-Alarcon, Gilberto and Manuel Fragoso,
   Jose and Antonio Pena-Duque, Marco and Alberto Reyes-Gomez, Carlos and
   Antonio Martinez-Rios, Marco and De la Pena-Diaz, Aurora},
Title = {{C3435T polymorphism of the ABCB1 gene is associated with poor
   clopidogrel responsiveness in a Mexican population undergoing
   percutaneous coronary intervention}},
Journal = {{THROMBOSIS RESEARCH}},
Year = {{2015}},
Volume = {{136}},
Number = {{5}},
Pages = {{894-898}},
Month = {{NOV}},
Abstract = {{Background: Clopidogrel is a pro-drug and its intestinal absorption is
   limited by the P-glycoprotein encoded by the ABCB1 gene. It is
   metabolized hepatically by cytochrome P450 enzymes encoded by CYP genes
   to produce an active metabolite that antagonizes the P2Y12 platelet
   receptor. Some patients exhibit poor clopidogrel responsiveness due to
   polymorphisms, resulting in thrombotic events. The aim of this study was
   to determine the relationship between poor clopidogrel responsiveness
   and the ABCB1, CYP and P2RY12 gene polymorphisms among patients
   undergoing percutaneous coronary intervention (PCI).
   Methods and results: Two hundred seventy-six patients who underwent PCI
   were included in this study. Clopidogrel responsiveness was determined
   via optical aggregometry in platelet-rich plasma using 10 mu M ADP.
   Patients exhibiting a platelet aggregation response higher than 70\%
   were classified as poor responders. The genetic polymorphisms were
   analyzed via real-time PCR.
   Poor responsiveness to clopidogrel was noted in 22.1\% of the patients.
   The TT genotype of the C3435T polymorphism of the ABCB1 gene and
   omeprazole usage were each associated with poor clopidogrel
   responsiveness (Exp (beta) 2.73, p = 0.009 and Exp (beta) 3.86, p =
   0.04, respectively).
   Conclusion: Poor clopidogrel responsiveness is associated with the TT
   genotype of the C3435T polymorphism of the ABCB1 gene. (C) 2015 Elsevier
   Ltd. All rights reserved.}},
