@article{ ISI:000386566500006,
Author = {Galeana-Sanchez, Hortensia and Toledo, Micael},
Title = {{New classes of panchromatic digraphs}},
Journal = {{AKCE INTERNATIONAL JOURNAL OF GRAPHS AND COMBINATORICS}},
Year = {{2015}},
Volume = {{12}},
Number = {{2-3}},
Pages = {{124-132}},
Month = {{NOV-DEC}},
Abstract = {{A digraph D = (V, A) with a k-colouring of its arcs zeta : A -> {[}k] is
   said to have a zeta-kernel if there exists a subset K of V such that
   there are no monochromatic uv-paths for any two vertices u, v is an
   element of K, but for every w is an element of V - K, there exists a
   vertex v is an element of K such that there is a monochromatic wv-path
   in D. The panchromatic number, pi(D), of D is the greatest integer k for
   which D has a zeta-kernel for every possible k-colouring of its arcs. D
   is said to be a panchromatic digraph if, for every k <= vertical bar A
   vertical bar and every k-colouring zeta : A -> {[}k], D has a
   zeta-kernel. In this paper we study the panchromaticity of cycles. In
   particular, we show that even cycles are panchromatic and that pi(C) = 2
   when C is an odd cycle. We also set sufficient conditions, in terms of
   its induced subdigraphs, for a digraph D to be panchromatic, and we show
   through counterexamples that these results cannot be improved. (C) 2015
   Kalasalingam University. Production and Hosting by Elsevier B.V.}},
