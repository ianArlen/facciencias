@article{ ISI:000365555000016,
Author = {Angeles-Pascual, A. and Esparza, R. and Tellez-Vazquez, O. and Velumani,
   S. and Perez, R.},
Title = {{Synthesis and Characterization of Bimetallic Ni50Pt50 Catalyst Supported
   on SiO2 for N2O Decomposition}},
