@article{ ISI:000366351200001,
Author = {Estrada, Francisco and Tol, Richard S. J.},
Title = {{TOWARD IMPACT FUNCTIONS FOR STOCHASTIC CLIMATE CHANGE}},
Journal = {{CLIMATE CHANGE ECONOMICS}},
Year = {{2015}},
Volume = {{6}},
Number = {{4}},
Month = {{NOV}},
Abstract = {{Most functions of economic impact assume that climate change is smooth.
   We here propose impact functions that have stochastic climate change as
   an input. These functions are identical in shape and have similar
   parameters as do deterministic impact functions. The mean stochastic
   impacts are thus similar to deterministic impacts. Welfare effects are
   larger, and the stochasticity premium is larger than the risk premium.
   Results suggest that stochasticity is more important for past impacts
   than for future impacts. This outcome is partly caused by an
   underestimation of natural variability in the 21st century climate
   projections.}},
