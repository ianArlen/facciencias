@article{ ISI:000364267300002,
Author = {Ortiz Velasquez, Antonio},
Title = {{Mean p(T) scaling with m/n(q) at the LHC: Absence of (hydro) flow in
   small systems?}},
Journal = {{NUCLEAR PHYSICS A}},
Year = {{2015}},
Volume = {{943}},
Pages = {{9-17}},
Month = {{NOV}},
Abstract = {{In this work, a study of the average transverse momentum (p(T)) as a
   function of the mid-rapidity charged hadron multiplicity (N-ch) and
   hadron mass (m) in p-Pb and Pb-Pb collisions at LHC energies is
   presented. For the events producing low N-ch, the average p(T) is found
   to scale with the reduced hadron mass, i.e., mass divided by the number
   of quark constituents (m/n(q)), this scaling also holds for inelastic pp
   collisions at RHIC and LHC energies. The scaling is broken in high
   multiplicity p-Pb and Pb-Pb collisions, where, for < dN(ch)/d eta > less
   than or similar to 60 the average p(T) is higher for baryons than that
   for mesons, though they increase linearly with m/n(q). This behavior is
   qualitatively well reproduced by Pythia 8, but not by hydro
   calculations, where an universal scaling with the hadron mass (and not
   with m/n(q)) is predicted for all the multiplicity event classes. Only
   the central (0-60\%) Pb-Pb collisions behave as expected from hydro. (C)
   2015 Elsevier B.V. All rights reserved.}},
