@article{ ISI:000364891000007,
Author = {Lopez, Jhon J. and Perez, Edwin G. and Garcia-Colunga, Jesus},
Title = {{Dual effects of a 2-benzylquinuclidinium derivative on alpha
   7-containing nicotinic acetylcholine receptors in rat hippocampal
   interneurons}},
Journal = {{NEUROSCIENCE LETTERS}},
Year = {{2015}},
Volume = {{607}},
Pages = {{35-39}},
Month = {{OCT 21}},
Abstract = {{Nicotinic acetylcholine receptors (nAChRs) are widely distributed in the
   brain. Particularly alpha 7-containing nAChRs, associated with several
   physiological roles and pathologies, are one of the most abundant. Here,
   we studied 2-(4-hexyloxybenzyl)-1-methylquinuclidin-1-ium iodide
   (designated as 8d), on ion currents elicited by choline, I-Ch(Ch, a
   selective agonist for alpha 7-containing nAChRs), recorded in
   interneurons from the stratum radiatum of the rat hippocampal CA1 region
   by using the whole-cell voltage-clamp technique. The
   8d-concentration/Ch-response relationship exhibited high and low
   inhibitory affinities for alpha 7-containing nAChRs, with IC50 values of
   0.59 and 6.80 mu M, respectively. Interestingly, 8d in a range of 3-10
   mu M exerted opposite effects: a short early potentiation and a long
   late inhibition of the I-Ch; and 8d alone elicited a non-decaying inward
   current. Furthermore, potentiation and inhibition of the I-Ch by 8d
   depended on the membrane potential, both being stronger at -20 than at
   -70 mV; indicating that 8d interacts with at least two sites into the
   ion channel/receptor complex: one for potentiating and another for
   inhibiting the alpha 7-containing nAChRs. These results suggest that 8d
   may act as agonist, antagonist and positive modulator of alpha
   7-containing nAChRs in hippocampal interneurons. (C) 2015 Elsevier
   Ireland Ltd. All rights reserved.}},
