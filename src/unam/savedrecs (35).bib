@article{ ISI:000386566500002,
Author = {Alejandria Dosal-Trujillo, Loiret and Galeana-Sanchez, Hortensia},
Title = {{The Fibonacci numbers of certain subgraphs of circulant graphs}},
Journal = {{AKCE INTERNATIONAL JOURNAL OF GRAPHS AND COMBINATORICS}},
