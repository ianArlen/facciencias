@article{ ISI:000358459300016,
Author = {Paulina Figueroa, Ana and Olsen, Mika and Zuazua, Rita},
Title = {{On the vertices of a 3-partite tournament not in triangles}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2015}},
Volume = {{338}},
Number = {{11}},
Pages = {{1982-1988}},
Month = {{NOV 6}},
Abstract = {{Let T be a 3-partite tournament and F-3 (T) be the set of vertices of T
   not in triangles. We prove that, if the global irregularity of T, i(g)
   (T), is one and vertical bar F-3 (T)vertical bar > 3, then F-3 (T) must
   be contained in one of the partite sets of T and vertical bar
   F-3(T)vertical bar <= left perpendiculark+1/4right perpendicular + 1,
   which implies vertical bar F-3(T)vertical bar <= left
   perpendicularn+5/12right perpendicular + 1, where k is the size of the
   largest partite set and n the number of vertices of T. Moreover, we give
   some upper bounds on the number, as well as results on the structure of
   said vertices within the digraph, depending on its global irregularity.
   (C) 2015 Elsevier B.V. All rights reserved.}},
