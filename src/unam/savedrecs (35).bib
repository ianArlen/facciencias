@article{ ISI:000363345900079,
Author = {Solis, C. and Chavez, E. and Ortiz, M. E. and Andrade, E. and Ortiz, E.
   and Szidat, S. and Wacker, L.},
Title = {{AMS-C14 analysis of graphite obtained with an Automated Graphitization
   Equipment (AGE III) from aerosol collected on quartz filters}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
