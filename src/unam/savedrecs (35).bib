@article{ ISI:000367560200008,
Author = {Villuendas-Rey, Yenny and Raul Alvarez-Idaboy, Juan and Galano, Annia},
Title = {{Assessing the Protective Activity of a Recently Discovered Phenolic
   Compound against Oxidative Stress Using Computational Chemistry}},
Journal = {{JOURNAL OF CHEMICAL INFORMATION AND MODELING}},
