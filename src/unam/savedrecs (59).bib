@article{ ISI:000316331900002,
Author = {Gonzalez-Andrade, Martin and DelValle, Paulina and Macias-Rubalcavac,
   Martha L. and Sosa-Peinado, Alejandro and Del Carmen Gonzalez, Maria and
   Mata, Rachel},
Title = {{Calmodulin Inhibitors from Aspergillus stromatoides}},
Journal = {{CHEMISTRY \& BIODIVERSITY}},
