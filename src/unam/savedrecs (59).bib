@article{ ISI:000315543800001,
Author = {Beukes, J. and Reyneke, J. P. and Becker, P. J.},
Title = {{Variations in the anatomical dimensions of the mandibular ramus and the
   presence of third molars: its effect on the sagittal split ramus
   osteotomy}},
Journal = {{INTERNATIONAL JOURNAL OF ORAL AND MAXILLOFACIAL SURGERY}},
Year = {{2013}},
Volume = {{42}},
Number = {{3}},
Pages = {{303-307}},
Month = {{MAR}},
Abstract = {{The authors undertook a prospective study to evaluate the influence that
   the anatomical dimensions of the ramus of the mandible and the presence
   of lower third molar teeth may have on the sagittal split ramus
   osteotomy. The anatomical dimensions measured included the width of the
   anterior mandibular ramus, the height of the corpus posterior to the
   second mandibular molar and the anteroposterior anatomical position of
   the lingual. The influence that these dimensions of the mandible may
   have on the successful splitting of the mandibular ramus was
   investigated. The effect that the presence of wisdom teeth had on the
   difficulty of the procedure was also investigated. This study found
   that, unlike the presence of third molars, there was no single
   anatomical measurement that contributed to the level of difficulty of
   the sagittal split osteotomy. For descriptive purposes the authors
   proposes a classification of the four typical patterns of unfavourable
   splits.}},
