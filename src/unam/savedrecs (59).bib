@article{ ISI:000342875100008,
Author = {Cuevas Hernandez, Ana Lilia and Sanchez-Gonzalez, Arturo and Daniel
   Tejero-Diez, Jose},
Title = {{Pteridophytes of a Semiarid Natural Protected Area in Central Mexico}},
Journal = {{NATURAL AREAS JOURNAL}},
