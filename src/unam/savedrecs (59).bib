@article{ ISI:000209312700016,
Author = {Arturo Hernandez-Mijangos, L. and Medellin, Rodrigo A.},
Title = {{USE OF TENTS BY THE BIG FRUIT-EATING BAT ARTIBEUS LITURATUS (CHIROPTERA:
   PHYLLOSTOMIDAE) IN CHIAPAS, MEXICO}},
Journal = {{SOUTHWESTERN NATURALIST}},
Year = {{2013}},
Volume = {{58}},
Number = {{1}},
Pages = {{107-109}},
Month = {{MAR}},
Abstract = {{We report the first records of use of tents by the big fruit-eating bat
   Artibeus lituratus in palm fronds of Sabal mexicana. We found 1-10 A.
   lituratus in three modified palm fronds. Architecture of tents
   corresponds to an umbrella tent. Individuals of A. lituratus could use
   tents as alternative roosts and they may display opportunistic behavior
   in use of tents.}},
