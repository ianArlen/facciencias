@article{ ISI:000314758300006,
Author = {Cordero, S. and Lopez-Pena, R. and Castanos, O. and Nahmad-Achar, E.},
Title = {{Quantum phase transitions of three-level atoms interacting with a
   one-mode electromagnetic field}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2013}},
Volume = {{87}},
Number = {{2}},
Month = {{FEB 7}},
Abstract = {{We apply the energy surface method to study a system of N-a three-level
   atoms interacting with a one-mode radiation field in the Xi, Lambda, and
   V configurations. We obtain an estimation of the ground-state energy,
   the expectation value of the total number of excitations, and the phase
   diagram of the model in the interaction parameter space and compare the
   results with the exact solutions. The phase diagram separates the
   behavior of, in general, a dominant single state without excitations (M
   = 0) from a collective state with excitations (M not equal 0). We have
   first- and second-order phase transitions, except for the V
   configuration, which only presents second-order phase transitions. DOI:
   10.1103/PhysRevA.87.023805}},
