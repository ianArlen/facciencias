EISSN = {{1557-7759}},
Keywords = {{Trypanosome; Chagas disease; Genetics; Nifurtimox; Benznidazole}},
Keywords-Plus = {{VITRO SUSCEPTIBILITY; LINEAGE-I; MOLECULAR CHARACTERIZATION;
