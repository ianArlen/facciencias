@article{ ISI:000317882100005,
Author = {Moura Veiga, Thiago Andre and King-Diaz, Beatriz and Ferrari Marques,
   Anna Sylvia and Sampaio, Olivia Moreira and Vieira, Paulo Cezar and das
   Gracas Fernandes da Silva, Maria Fatima and Lotina-Hennsen, Bias},
Title = {{Furoquinoline alkaloids isolated from Balfourodendron riedelianum as
   photosynthetic inhibitors in spinach chloroplasts}},
Journal = {{JOURNAL OF PHOTOCHEMISTRY AND PHOTOBIOLOGY B-BIOLOGY}},
Year = {{2013}},
Volume = {{120}},
Pages = {{36-43}},
Month = {{MAR 5}},
Abstract = {{In the search for natural inhibitors of plant growth, we investigate the
   mechanism of action of the natural furoquinoline alkaloids isolated from
   Balfourodendron riedelianum (Rutaceae): evolitrine (1), kokusaginine
   (2), gamma-fagarine (3), skimmianine (4) and maculosidine (5) on the
   photosynthesis light reactions. Their effect on the electron transport
   chain on thylakoids was analyzed. Alkaloids 1, 2, 4 and 5 inhibited ATP
   synthesis, basal, phosphorylating and uncoupled electron transport
   acting as Hill reaction inhibitors on spinach chloroplasts. Alkaloid 3
   was not active. The inhibition and interaction site of alkaloids 1, 2, 4
   and 5 on the non-cyclic electron transport chain was studied by
   polarography and fluorescence of the chlorophyll a (Chl a). The results
   indicate that the target for 1 was localized on the donor and acceptor
   side of PS II. In addition alkaloids 2 and 5 affect the PS I electron
   acceptors on leaf discs. (C) 2013 Elsevier B.V. All rights reserved.}},
