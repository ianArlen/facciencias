@article{ ISI:000316979500006,
Author = {Peralta, Oscar and Castro, Telma and Duron, Matilde and Salcido,
   Alejandro and Celada-Murillo, Ana-Teresa and Navarro-Gonzalez, Rafael
   and Marquez, Claudia and Garcia, Jose and de la Rosa, Jose and Torres,
   Ricardo and Villegas-Martinez, Raymundo and Carreon-Sierra, Susana and
   Imaz, Mireya and Martinez-Arroyo, Ampara and Saavedra, Isabel and de la
   Luz Espinosa, Maria and Torres-Jaramillo, Alejandro},
Title = {{H2S emissions from Cerro Prieto geothermal power plant, Mexico, and air
   pollutants measurements in the area}},
Journal = {{GEOTHERMICS}},
Year = {{2013}},
Volume = {{46}},
Pages = {{55-65}},
Month = {{APR}},
Abstract = {{We measured meteorological parameters and environmental concentrations
   of hydrogen sulfide (H2S) and criteria pollutants at Cerro Prieto,
   Mexico, from May to July 2010. In addition, non-condensable gases from
   vent stacks at Cerro Prieto geothermal power plant were sampled and
   analyzed by gas chromatography-mass spectrometry. A dispersion model,
   which was validated with environmental measurements, was applied to H2S.
   Criteria pollutants never exceeded the Mexican air quality standards.
   Results from dispersion modeling were compared against measured
   concentrations showing acceptable agreements for Chi square goodness of
   fit (chi(2)) in the 95\% significance level (alpha = 0.05), at
   afternoons and evening events when boundary layer is decreasing and wind
   speed is less than 5.5 ms(-1). Determination coefficient of curve
   fitting for computed concentrations at less than 4 km from the source
   was 0.513 and farther 0.568. This study was the first to measure
   criteria pollutants and H2S in Cerro Prieto area. (C) 2012 Elsevier Ltd.
   All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
Type = {{Article}},
