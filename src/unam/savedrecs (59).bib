@article{ ISI:000314824700011,
Author = {Srivastava, P. C. and Ermamatov, M. J. and Morales, Irving O.},
Title = {{High-spin structures of Xe-136(54), Cs-137(55), Ba-138(56), La-139(57)
   and Ce-140(58): a shell model description}},
Journal = {{JOURNAL OF PHYSICS G-NUCLEAR AND PARTICLE PHYSICS}},
Year = {{2013}},
Volume = {{40}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{In the present work recently available experimental data (Astier et al
   2012 Phys. Rev. C 85 064316) for the high-spin states of five N = 82
   isotones, Xe-136(54), Cs-137(55), Ba-138(56), La-139(57) and Ce-140(58)
   have been interpreted with shell model calculations. The calculations
   have been performed in the 50-82 valence shell composed of 1g(7/2),
   2d(5/2), 1h(11/2), 3s(1/2) and 2d(3/2) orbitals. We have compared our
   results with the available experimental data for excitation energies
   including high-spin states, occupancy numbers and transition
   probabilities. As expected the structure of these isotones are due to
   proton excitations across the Z = 50 shell. The structure of the
   positive-parity states are mainly from (pi g(7/2 pi)d(5/2))(n) and (pi
   g(7/2)pi d(5/2))(n-2)(pi h(11/2))(2) configurations, while the
   negative-parity states have a (pi g(7/2)pi d(5/2))(n)(pi h(11/2))(1)
   configuration. Additionally, for the Xe-136(54), Cs-137(55) and
   Ba-138(56) isotones the excitation of the neutrons across the N = 82 gap
   is important.}},
