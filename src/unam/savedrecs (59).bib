@article{ ISI:000315069600004,
Author = {Camargo, Javier and Pellicer-Covarrubias, Patricia and Rincon, Michael},
Title = {{On g-contractibility of continua}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2013}},
Volume = {{160}},
Number = {{3}},
Pages = {{461-474}},
Month = {{FEB 15}},
Abstract = {{A continuum X is g-contractible provided that there exists an onto map
   f:X -> X such that f is homotopic to a constant map. Thus,
   g-contractibility is a natural generalization of contractibility. In
   this paper we present properties related to the g-contractibility of
   products and symmetric products of continua.
   Furthermore, we show an uncountable family of pairwise non-homeomorphic,
   uniformly pathwise connected, non-g-contractible, planar dendroids such
   that their hyperspaces of subcontinua are not g-contractible either. (C)
   2012 Elsevier B.V. All rights reserved.}},
