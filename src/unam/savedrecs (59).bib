@article{ ISI:000314973400006,
Author = {Reyes-Haro, Daniel and Alejandra Gonzalez-Gonzalez, Maria and Petriz,
   Adriana and Rosas-Arellano, Abraham and Kettenmann, Helmut and Miledi,
   Ricardo and Martinez-Torres, Ataulfo},
Title = {{gamma-Aminobutyric acid-rho expression in ependymal glial cells of the
   mouse cerebellum}},
Journal = {{JOURNAL OF NEUROSCIENCE RESEARCH}},
Year = {{2013}},
Volume = {{91}},
Number = {{4}},
Pages = {{527-534}},
Month = {{APR}},
Abstract = {{The ependymal glial cells (EGCs) from the periventricular zone of the
   cerebellum were studied to determine their distribution and the
   functional properties of their -aminobutyric acid type A (GABAA)
   receptors. EGCs were identified by the presence of ciliated structures
   on their ventricular surface and their expression of glial fibrillary
   acidic protein (GFAP). Interestingly, diverse cell types, including
   neurons, astrocytes, and other types of glia, were identified in the
   subventricular zone by their current profiles. Electron microscopy
   showed ciliated cells and myelinated axons in this zone, but we found no
   collateral connections to suggest the presence of functional synapses.
   GABA-mediated currents were recorded from EGCs in cerebellar slices from
   postnatal days 13 to 35 (PN13PN35). These currents were blocked by TPMPA
   (a highly specific GABAA subunit antagonist) and bicuculline (a
   selective antagonist for classic GABAA receptors). Pentobarbital failed
   to modulate GABAA-mediated currents despite the expression of GABA1 and
   GABA2 subunits. In situ hybridization, RT-PCR, and immunofluorescence
   studies confirmed GABA1 expression in EGCs of the cerebellum. We
   conclude that cerebellar EGCs express GABA1, which is functionally
   involved in GABAA receptor-mediated responses that are unique among
   glial cells of the brain. (c) 2013 Wiley Periodicals, Inc.}},
