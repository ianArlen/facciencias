@article{ ISI:000317840700032,
Author = {Torres-Freyermuth, Alec and Puleo, Jack A. and Pokrajac, Dubravka},
Title = {{Modeling swash-zone hydrodynamics and shear stresses on planar slopes
   using Reynolds-Averaged Navier-Stokes equations}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-OCEANS}},
