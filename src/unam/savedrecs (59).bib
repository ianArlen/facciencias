@article{ ISI:000323914400009,
Author = {Encino-Ruiz, Limberg and Lindig-Cisneros, Roberto and Gomez-Romero,
   Mariela and Blanco-Garcia, Arnulfo},
Title = {{PERFORMANCE OF THREE TROPICAL FOREST TREE SPECIES IN A TEST DECIDUOUS
   ECOLOGICAL RESTORATION}},
Journal = {{BOTANICAL SCIENCES}},
Year = {{2013}},
Volume = {{91}},
Number = {{1}},
Pages = {{107-114}},
Month = {{MAR}},
Abstract = {{Survival and growth of three tree species from tropical dry forest
   (Albizia occidentalis, Cedrela dugesii, and Ceiba aesculifolia) were
   measured in an ecological trial using a shrub species (Eysenhardtia
   polystachya) in order to assess its potential role as a nurse plant.
   Seven hundred and twenty seedlings of the three species were planted
   beneath, in the edge and outside from the canopy of E. polystachya.
   Survival of A. occidentalis after one year averaged 26\%, while C.
   dugesii and C. aesculifolia averaged 37 and 56\% and the highest
   mortality occurred during the driest months of the year (from March to
   May). The three species showed higher survival beneath the canopy of E.
   polystachya, but only C. dugesii showed significant differences (P >
   0.002). Only A. occidentalis showed a significant higher growth beneath
   E. polystachya. We found harsh microclimatic conditions in the study
   site during the dry season (with air maximum temperatures surpassing 40
   C) but these conditions were more severe out of E. polystachya canopies
   (41 degrees C) compared to sites beneath the canopy of this shrub (36.7
   degrees C). This study is relevant because A. occidentalis y C. dugesii
   are protected by Mexican government laws.}},
