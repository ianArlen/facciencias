@article{ ISI:000316908700002,
Author = {Rios Molina, Andres},
Title = {{``DICTATING THE SUITABLE WAY OF LIFE{''}: MENTAL HYGIENE FOR CHILDREN
   AND WORKERS IN SOCIALIST MEXICO, 1934-1940}},
Journal = {{JOURNAL OF THE HISTORY OF THE BEHAVIORAL SCIENCES}},
Year = {{2013}},
Volume = {{49}},
Number = {{2}},
Pages = {{142-166}},
Month = {{SPR}},
Abstract = {{After the Mexican Revolution (1910-1920), an ambitious project of
   national reconstruction began in which education and health were two
   priorities in the consolidation of a new nation. In this context of
   social, cultural, and political transformation, mental hygiene was a
   field that made it possible to articulate the professional practice of
   psychiatrists with the project of the nation promoted by
   postrevolutionary governments. In Mexico, the mental hygiene movement
   was headed by the same doctors who professionalized the practice of
   psychiatry and made it a specialized field of knowledge. The first
   generation of psychiatrists managed to integrate mental hygiene into
   health and education policies during the socialist administration of
   president Lazaro Cardenas; a phenomenon that made evident the
   articulation between mental hygiene, social medicine, and nationalist
   discourse. Discussion will focus on proposals made from the perspective
   of mental hygiene as a function of two social sectors regarded as
   priorities by the Cardenas government: children and workers. (C) 2013
   Wiley Periodicals, Inc.}},
