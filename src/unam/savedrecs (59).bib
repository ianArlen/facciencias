@article{ ISI:000315596700017,
Author = {Ullrich, B. and Wang, J. S.},
Title = {{All-optical tuning of the Stokes shift in PbS quantum dots}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2013}},
Volume = {{102}},
Number = {{7}},
Month = {{FEB 18}},
Abstract = {{The Stokes shift of colloidal 4.7 nm PbS quantum dots was measured
   between 5 and 300 K at incrementally increasing continuous laser
   intensities. The results demonstrate Stokes shift tuning by optical
   means only at stable given temperatures due to optically enforced
   electronic state alteration in the quantum dots. The tuning phenomenon
   is perfectly fit by a semi-empirical model, which provides a design tool
   for the chromaticity of quantum dots at different optical pump
   intensities. (C) 2013 American Institute of Physics.
