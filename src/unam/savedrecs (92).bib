@article{ ISI:000271238900005,
Author = {Valenzuela, E. and Gamboa, S. A. and Sebastian, P. J. and Moreira, J.
   and Pantoja, J. and Ibanez, G. and Reyes, A. and Campillo, B. and Serna,
   S.},
Title = {{Proton Charge Transport in Nafion Nanochannels}},
Journal = {{JOURNAL OF NANO RESEARCH}},
Year = {{2009}},
Volume = {{5}},
Pages = {{31-36}},
Abstract = {{The Nafion perfluorinated membranes are one of the best electrolytes
   used in the Proton Exchange Membrane Fuel Cell (PEMFC). Some methods
   have been used to study the electrical properties of Nafion;
   nevertheless, there are some aspects of the conduction process that are
   not well understood, such as the contribution of the bulk and the
   interfacial phenomena to the total proton conduction process. In this
   work the Electrochemical Impedance Spectroscopy (EIS) was employed in a
   four electrode system to study the protonic charge transport under
   conditions that simulate the operation of the PEMFC. Two Nafion
   membranes were evaluated to determine the relation of the activation
   procedure with the resistance to the protonic charge transference. The
   results are not only consistent with other measurements but also allow
   to separate the protonic charge transference process in two stages. Each
   stage was studied and their electrical parameters were calculated using
   Electrical Equivalent Circuits.}},
