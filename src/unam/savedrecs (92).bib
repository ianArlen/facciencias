@article{ ISI:000271005700005,
Author = {Lopez-Serrano, F. and Mendizabal, E. and Puig, J. E. and Alvarez, J.},
Title = {{Microemulsion Copolymerization Modeling Study Via a Combined
   Integral-Differential Experimental Data Processing Approach}},
Journal = {{MACROMOLECULAR SYMPOSIA}},
