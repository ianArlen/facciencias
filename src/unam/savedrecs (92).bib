@inproceedings{ ISI:000271080100015,
Author = {Russell, Jane M. and Ainsworth, Shirley and Diaz-Aguilar, Janet},
Editor = {{Larsen, B and Leta, J}},
Title = {{Visibility in Internet of the Scientific Production and Activities of
   the National Autonomous University of Mexico (UNAM) Research Institutes}},
Booktitle = {{PROCEEDINGS OF ISSI 2009 - 12TH INTERNATIONAL CONFERENCE OF THE
   INTERNATIONAL SOCIETY FOR SCIENTOMETRICS AND INFORMETRICS, VOL 2}},
Series = {{Proceedings of the International Conference on Scientometrics and
   Informetrics}},
