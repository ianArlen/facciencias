@article{ ISI:000262133900004,
Author = {Vargas, Alejandro and Moreno, J. Carlos and Mendoza, Ismael},
Title = {{Development of an Automated Anaerobic Biodegradability Testing Device}},
Journal = {{INSTRUMENTATION SCIENCE \& TECHNOLOGY}},
Year = {{2009}},
Volume = {{37}},
Number = {{1}},
Pages = {{40-49}},
Abstract = {{The development of a low-cost device used for anaerobic biodegradability
   testing is described, together with its experimental validation. The
   equipment is based on manometric measurements of the biogas produced
   during the fermentation of test substances in up to seven parallel
   closed bioreactors. It uses separate pressure sensors and relief valves,
   which enable continuous measurements and avoid pressure buildup that
   could inhibit the biochemical reactions. A low-cost data acquisition
   module is used and software has been developed that is specifically
   suited for the end-user. The equipment has been tested and validated
   experimentally with a system that accounts for biohydrogen production,
   testing the effect of original temperature on three different
   pre-treated inocula, using glucose as sole source of carbon and energy.
   The results validate the operation of the testing equipment.}},
