@inproceedings{ ISI:000276056800045,
Author = {Pelczer, I and Gamboa, F.},
Editor = {{Tzekaki, M and Kaldrimidou, M and Sakonidis, H}},
Title = {{PROBLEM POSING: COMPARISON BETWEEN EXPERTS AND NOVICES}},
Booktitle = {{PME 33: PROCEEDINGS OF THE 33RD CONFERENCE OF THE INTERNATIONAL GROUP
   FOR THE PSYCHOLOGY OF MATHEMATICS EDUCATION, VOL 4}},
