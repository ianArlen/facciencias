@article{ ISI:000261906200084,
Author = {Keseler, Ingrid M. and Bonavides-Martinez, Cesar and Collado-Vides,
   Julio and Gama-Castro, Socorro and Gunsalus, Robert P. and Johnson, D.
   Aaron and Krummenacker, Markus and Nolan, Laura M. and Paley, Suzanne
   and Paulsen, Ian T. and Peralta-Gil, Martin and Santos-Zavaleta, Alberto
   and Glennon Shearer, Alexander and Karp, Peter D.},
