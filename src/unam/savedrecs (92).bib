@inproceedings{ ISI:000294493400050,
Author = {Martinez, Omar S. and Palomera, Roger C. and Cruz, Jose S. and Mathew,
   Xavier},
Editor = {{Kasap, S and DeCorby, R and Ruda, H and Hegmann, F and Kashyap, R}},
Title = {{Co-evaporated Cd1-xMgx Te thin films for applications in tandem solar
   cells}},
Booktitle = {{PHYSICA STATUS SOLIDI C: CURRENT TOPICS IN SOLID STATE PHYSICS, VOL 6,
   SUPPL 1}},
