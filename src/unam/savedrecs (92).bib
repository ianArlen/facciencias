@article{ ISI:000261403900010,
Author = {Rodriguez-Antolin, Jorge and Xelhuantzi, Nicte and Garcia-Lorenzana,
   Mario and Cuevas, Estela and Hudson, Robyn and Martinez-Gomez, Margarita},
Title = {{General tissue characteristics of the lower urethral and vaginal walls
   in the domestic rabbit}},
Journal = {{INTERNATIONAL UROGYNECOLOGY JOURNAL}},
Year = {{2009}},
Volume = {{20}},
Number = {{1}},
Pages = {{53-60}},
Month = {{JAN}},
Abstract = {{In European rabbits, the distal urethra (DU) opens into the anterior
   pelvic vagina forming a single canal by which females copulate, give
   birth, and urinate. We investigated whether the histological
   characteristics of the DU and the pelvic and perineal vagina are
   different. The DU and vagina of rabbits (Oryctolagus cuniculus) were cut
   and stained with hematoxylin-eosin and Masson's trichrome (n=3). Data
   were compared by using Friedman's ANOVA for repeated measures. The walls
   of the DU and vagina are composed of mucosa, submucosa, smooth muscle,
   and an external layer. Differences in tissue characteristics of the
   mucosa, orientation of the smooth muscle fibers, components of the
   external layer (connective tissue, blood vessels, and striated
   musculature), and thickness of the tissue layers were found among
   regions. The lack of histological homogeneity along the urethra and
   vagina possibly reflects differences in the functions of each segment.}},
