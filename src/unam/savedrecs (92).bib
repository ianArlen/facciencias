@article{ ISI:000261734400002,
Author = {Juarez, Gelacio and Ayala, Gustavo},
Title = {{Variational Formulation of the Material Failure Process in Solids by
   Embedded Discontinuities Model}},
Journal = {{NUMERICAL METHODS FOR PARTIAL DIFFERENTIAL EQUATIONS}},
Year = {{2009}},
Volume = {{25}},
Number = {{1}},
Pages = {{26-62}},
Month = {{JAN}},
Abstract = {{This work presents a variational formulation of the material failure
   process, idealized as strain or displacement discontinuities, by weak,
   strong, or discrete embedded discontinuities into a continuum. It is
   shown that the solution of the proposed variational formulation may be
   approximated by different types of finite elements with embedded
   discontinuities. The developed displacement approximation of a finite
   element split by the discontinuity leads to a symmetric stiffness
   matrix, which considers not only the continuity of fractions but also
   the rigid body relative motions of the portions in which the element is
   split. The variational formulation of a continuum with more than one
   discontinuity in its interior is developed. It is shown that this
   formulation may lead to finite elements with embedded discontinuities
   that can be classified as displacement, force, mixed, and hybrid models.
   To show the effectiveness of the proposed formulation, the classical
   example of a bar under tension is solved using one and 2D finite element
   approximations. (C) 2008 Wiley Periodicals. Inc. Numer Methods Partial
   Differential Eq 25: 26-62, 2009}},
Publisher = {{WILEY}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Ayala, G (Reprint Author), Univ Nacl Autonoma Mexico, Inst Engn, Mexico City 04510, DF, Mexico.
