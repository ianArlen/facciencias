@article{ ISI:000262896900001,
Author = {Melendez-Ortiz, H. I. and Bucio, E. and Burillo, G.},
Title = {{Radiation-grafting of 4-vinylpyridine and N-isopropylacrylamide onto
   polypropylene to give novel pH and thermo-sensitive films}},
Journal = {{RADIATION PHYSICS AND CHEMISTRY}},
Year = {{2009}},
Volume = {{78}},
Number = {{1}},
Pages = {{1-7}},
Month = {{JAN}},
Abstract = {{Here 4-vinylpyridine (4VP) was grafted onto polypropylene films (PP) by
   mutual irradiation method to give PP-g-4VP: N-isopropylacrylamide
   (NIPAAm) was then grafted onto the PP-g-4VP films to give
   (PP-g-4VP)-g-NIPAAm by pre-irradiation method, using a Co-60
   gamma-source. The dependence of grafting percentage on radiation dose,
   temperature, reaction time, and monomer concentration was studied.
   (PP-g-4VP)-g-NIPAAm films were characterized by infrared spectroscopy
   (FTIR-ATR), differential scanning calorimetry (DSC), and
   thermogravimetric analysis (TGA). The critical PH point and lower
   critical solution temperature (LCST) were determined by swelling and
   water contact angle measurements. The LCST also was determined by DSC.
   The binary graft copolymer films are shown to be thermo-pH sensitive.
   (C) 2008 Elsevier Ltd. All rights reserved.}},
