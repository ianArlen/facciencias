@inproceedings{ ISI:000265216200014,
Author = {Puerari, I. and Gonzalez-Lopezlira, R. A. and Valdez-Gutierrez, M.},
Editor = {{Dagostino, MC and Bertone, E and Gonzalez, DR}},
Title = {{A Morphological Study of M81: Comparison Between GALEX and Spitzer
   Imaging}},
Booktitle = {{NEW QUESTS IN STELLAR ASTROPHYSICS II: ULTRAVIOLET PROPERTIES OF EVOLVED
   STELLAR POPULATIONS}},
Series = {{Astrophysics and Space Science Proceedings}},
Year = {{2009}},
