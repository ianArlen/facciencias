@article{ ISI:000271005700033,
Author = {Ortiz-Estrada, C. H. and Luna-Barcenas, G. and Alvarado, J. F. J. and
   Gonzalez-Alatorre, G. and Sanchez, I. C. and Ramirez, N. Flores and
   Vasquez Garcia, Salomon R. and Castillo-Tejas, J. and Manero-Brito, O.},
Title = {{Polymer Chain Collapse in Supercritical Fluids. 2. Experimental Evidence}},
