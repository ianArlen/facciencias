@article{ ISI:000262547400007,
Author = {Soler Claudin, Carmen},
Title = {{Universal access, six years results in the Mexico City HIV/AIDS Program}},
Journal = {{SALUD PUBLICA DE MEXICO}},
Year = {{2009}},
Volume = {{51}},
Number = {{1}},
Pages = {{26-33}},
Month = {{JAN-FEB}},
Abstract = {{Objective. To analyze the results of the Free Antiretroviral Medication
   Program of the Mexico City HIV/AIDS Program among the affected
   population in Mexico City. Material and Methods. A retrospective
   analysis was conducted of medical attention, antiretroviral treatment
   and specialized laboratory results data from 2001 to 2006 from patients
   who sought services from the Mexico City HIV/AIDS Program. Results. Data
   from 5 146 patients who were undergoing ARV are presented. At the end of
   the period studied, 74\% were current, 12.2\% were not found for
   follow-up and 13.9\% had died. Conclusions. During the period evaluated,
   wider coverage was achieved in Mexico City as well as increased
   efficiency in ARV treatment and increased patient survival.}},
