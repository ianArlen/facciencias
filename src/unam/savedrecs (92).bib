@article{ ISI:000262195600003,
Author = {Hernandez-Segura, Gerardo O. and Campos, Myriam and Costas, Miguel and
   Torres, Luis A.},
Title = {{Temperature dependence of the heat capacities in the solid state of 18
   mono-, di-, and poly-saccharides}},
Journal = {{JOURNAL OF CHEMICAL THERMODYNAMICS}},
