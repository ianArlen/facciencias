@article{ ISI:000270410200004,
Author = {Damek, Ewa and Dziubanski, Jacek and Jaming, Philippe and Perez-Esteva,
   Salvador},
Title = {{DISTRIBUTIONS THAT ARE CONVOLVABLE WITH GENERALIZED POISSON KERNEL OF
   SOLVABLE EXTENSIONS OF HOMOGENEOUS LIE GROUPS}},
Journal = {{MATHEMATICA SCANDINAVICA}},
Year = {{2009}},
Volume = {{105}},
Number = {{1}},
Pages = {{31-65}},
Abstract = {{In this paper, we characterize the class of distributions on a
   homogeneous Lie group R that can be extended via Poisson integration to
   a solvable one-dimensional extension G of R. To do so, we introduce the
   L'-convolution on R and show that the set of distributions that are
   L'-convolvable with Poisson kernels is precisely the set of suitably
   weighted derivatives of L(1) functions. Moreover, we show that the
   L'-convolution of such a distribution with the Poisson kernel isharmonic
   andhas theexpected boundary behavior. Finally, we show that such
   distributions satisfy some global weak-L(1) estimates.}},
