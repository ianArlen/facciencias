@article{ ISI:000271005700043,
Author = {Calderas, F. and Sanchez-Solis, A. and Maciel, A. and Manero, O.},
Title = {{The Transient Flow of the PET-PEN-Montmorillonite Clay Nanocomposite}},
Journal = {{MACROMOLECULAR SYMPOSIA}},
