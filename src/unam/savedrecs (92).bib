@article{ ISI:000268553400007,
Author = {Ruiz-Fernandez, A. C. and Hillaire-Marcel, C.},
Title = {{Pb-210-derived ages for the reconstruction of terrestrial contaminant
   history into the Mexican Pacific coast: Potential and limitations}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2009}},
Volume = {{59}},
Number = {{4-7, SI}},
Pages = {{134-145}},
Note = {{Environmental Records of Anthropogenic Impacts On Coastal Ecosystems
   held at the Ocean Sciences 2008 Meeting, Orlando, FL, MAR 02-07, 2008}},
Abstract = {{Pb-210 is widely used for dating recent sediments in the aquatic
   environment; however, our experiences working in shallow coastal
   environments in the Pacific coast of Mexico have demonstrated that the
   potential of Pb-210 for reliable historical reconstructions might be
   limited by the low Pb-210 atmospheric fallout, sediment mixing,
   abundance of coarse sediments and the lack of Cs-137 signal for Pb-210
   corroboration. This work discusses the difficulties in obtaining
   adequate sedimentary records for geochronological reconstruction in such
   active and complex settings, including examples of Pb-210
   geochronologies based on sediment profiles collected in two contrasting
   areas coastal areas (mudflats associated to coastal lagoons of Sinaloa
   State and the continental shelf of the Gulf of Tehuantepec), in which
   geochemical data was used to support the temporal frame established and
