@article{ ISI:000264091200003,
Author = {Torres-Gurrola, Guadalupe and Montes-Hernandez, Salvador and
   Espinosa-Garcia, Francisco J.},
Title = {{PATTERNS OF VARIATION AND GEOGRAPHIC DISTRIBUTION IN FOLIAR CHEMICAL
   PHENOTYPES OF Persea americana var. drymifolia}},
Journal = {{REVISTA FITOTECNIA MEXICANA}},
