@article{ ISI:000263422800019,
Author = {Jalbout, Abraham F. and Contreras-Torres, Flavio F. and Roy, Amlan K.
   and de Leon, Aned},
Title = {{Monte Carlo Simulation on the RKKY Interactions of Co-Doped ZnS and ZnSe
   Nano-Films}},
Journal = {{JOURNAL OF COMPUTATIONAL AND THEORETICAL NANOSCIENCE}},
Year = {{2009}},
Volume = {{6}},
Number = {{1}},
Pages = {{148-152}},
Month = {{JAN}},
Abstract = {{Simulations with previously reported Monte Carlo methods, using a
   three-dimensional lattice model studied the RKKY indirect exchange
   interaction of doped magnetic Co(2+) ions in ZnS, ZnSe films were
   performed. As for the previous ZnO case, the results of the calculations
   show that the RKKY interaction in Co-Doped ZnS, ZnSe is long-ranged and
   its magnitude is proportional to R(-1). For ZnS the results become
   constant beyond a CO(2+) concentration of 15\%, and for ZnSe results
   beyond 10\% show no RKKY interaction at all.}},
