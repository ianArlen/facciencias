@inproceedings{ ISI:000279542700009,
Author = {Guzman-Speziale, Marco},
Editor = {{James, KH and Lorente, MA and Pindell, JL}},
Title = {{A seismotectonic model for the Chortis Block}},
Booktitle = {{ORIGIN AND EVOLUTION OF THE CARIBBEAN PLATE}},
