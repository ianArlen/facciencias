@inproceedings{ ISI:000260486400004,
Author = {Hernandez-Cordero, Juan and Chen, Lawrence R. and Rochette, Martin},
Editor = {{Corderiro, CMB and DeMatos, CJS}},
Title = {{Highly Nonlinear Fibers in All-Optical Modulators}},
Booktitle = {{1ST WORKSHOP ON SPECIALTY OPTICAL FIBERS AND THEIR APPLICATIONS}},
Series = {{AIP Conference Proceedings}},
Year = {{2008}},
Volume = {{1055}},
Pages = {{11+}},
