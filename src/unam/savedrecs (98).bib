@article{ ISI:000254020500014,
Author = {Rodriguez-Gonzalez, Vicente and Zanellac, Rodolfo and del Angela, Gloria
   and Gomeza, Ricardo},
Title = {{MTBE visible-light photocatalytic decomposition over Au/TiO2 and
   Au/TiO2-Al2O3 sol-gel prepared catalysts}},
Journal = {{JOURNAL OF MOLECULAR CATALYSIS A-CHEMICAL}},
Year = {{2008}},
Volume = {{281}},
Number = {{1-2, SI}},
Pages = {{93-98}},
