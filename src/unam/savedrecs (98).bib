@article{ ISI:000253548300007,
Author = {Salazar, Hector and Llorente, Itzel and Jara-Oseguera, Andres and
   Garcia-Villegas, Refugio and Munari, Mika and Gordon, Sharona E. and
   Islas, Leon D. and Rosenbaum, Tamara},
Title = {{A single N-terminal cysteine in TRPV1 determines activation by pungent
   compounds from onion and garlic}},
Journal = {{NATURE NEUROSCIENCE}},
Year = {{2008}},
Volume = {{11}},
Number = {{3}},
Pages = {{255-261}},
Month = {{MAR}},
Abstract = {{Some members of the transient receptor potential (TRP) family of cation
   channels mediate sensory responses to irritant substances. Although it
   is well known that TRPA1 channels are activated by pungent compounds
   found in garlic, onion, mustard and cinnamon extracts, activation of
   TRPV1 by these extracts remains controversial. Here we establish that
   TRPV1 is activated by pungent extracts from onion and garlic, as well as
   by allicin, the active compound in these preparations, and participates
   together with TRPA1 in the pain-related behavior induced by this
   compound. We found that in TRPV1 these agents act by covalent
   modification of cysteine residues. In contrast to TRPA1 channels,
   modification of a single cysteine located in the N-terminal region of
   TRPV1 was necessary and sufficient for all the effects we observed. Our
   findings point to a conserved mechanism of activation in TRP channels,
   which provides new insights into the molecular basis of noxious stimuli
   detection.}},
Publisher = {{NATURE PUBLISHING GROUP}},
Address = {{75 VARICK ST, 9TH FLR, NEW YORK, NY 10013-1917 USA}},
Type = {{Article}},
