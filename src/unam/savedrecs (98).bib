@article{ ISI:000257808000052,
Author = {Bernal, R. and Garcia-Haro, A. R. and Machi, L. and Brown, F. and
   Perez-Salas, R. and Castano, V. M. and Cruz-Vazquez, C.},
Title = {{Advances in the synthesis of new Europium doped CaSO4 phosphors and
   their thermoluminescence characterization}},
Journal = {{RADIATION MEASUREMENTS}},
Year = {{2008}},
Volume = {{43}},
Number = {{2-6}},
Pages = {{371-374}},
Month = {{FEB-JUN}},
Note = {{15th International Conference on Solid State Dosimetry, Delft Univ
   Technol, Delft, NETHERLANDS, JUL 08-13, 2007}},
Abstract = {{Novel self-agglomerating Eu doped CaSO4 phosphors were synthesized by a
   cheap, easy, and environmental friendly chemical route. Pellet-shaped
   sintered samples were obtained without any binding material, and exposed
   to beta particle irradiation. The thermoluminescence (TL) response in
   the 0.08-2.6 Gy dose range increased linearly as the radiation dose
   increased. Glow curves exhibit the most intense maximum close to 200
   degrees C, and two much less intense maxima at 250 and 330 degrees C
   when a 5 degrees C/s heating rate is used. No TL peaks shift is observed
   when dose changes, which is characteristic of first order kinetics TL
   processes. The whole glow curve displays a remarkable stability under
   Storage at room temperature. The main peak intensity is 2.5 times
   greater than that of the TLD-100. The method here reported can be used
   to synthesize CaSO4 with other dopants. (C) 2007 Elsevier Ltd. All
   rights reserved.}},
