@article{ ISI:000254430600017,
Author = {Cervantes, L. and Avila, O. and Ruvalcaba, J. L. and Miranda, J. and
   Munoz, R.},
Title = {{The use of biomonitors and PIXE analysis in the study of air pollution
   in Mexico City}},
