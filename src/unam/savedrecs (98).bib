@article{ ISI:000253016500019,
Author = {Castillo, F. and Gamboa-deBuen, I. and Herrera, J. J. E. and Rangel, J.
   and Villalobos, S.},
Title = {{High contrast radiography using a small dense plasma focus}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2008}},
Volume = {{92}},
Number = {{5}},
Month = {{FEB 4}},
Abstract = {{Radiographs are obtained with a small (4.6 kJ) dense plasma focus
   machine, using the x-rays which cross a 300 mu m aluminum window on the
   axis. Contrast is improved by inserting an iron needle on the tip of the
   electrode. Measurements with TLD-100 dosimeters have shown the average
   dose to increase from 0.077 +/- 0.006 mGy/shot, when a hollow cathode is
   used, to 0.11 +/- 0.01 mGy/shot, with the needle. The spectrum is
   estimated using aluminum filters and the effective equivalent energy is
   found to be in the range of 20-25 keV. (C) 2008 American Institute of
   Physics.}},
