@article{ ISI:000255318700011,
Author = {Massillon-JL, G. and Gamboa-DeBuen, I. and Buenfil, A. E. and
   Monroy-Rodriguez, M. A. and Brandan, M. E.},
Title = {{CaF2 : Tm (TLD-300) thermoluminescent response and glow curve induced by
   gamma-rays and ions}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2008}},
Volume = {{266}},
Number = {{5}},
Pages = {{772-780}},
Month = {{MAR}},
Abstract = {{The thermoluminescent response of CaF2:Tm after exposure to (CO)-C-60
   gamma-rays at doses from 0.44 Gy to 8.75 kGy and to low fluences
   (10(5)similar to 10(8) cm(-2)) of 25 and 40 MeV H-1, 75 and 120 MeV
   He-3. 180, 300 and 480 MeV C-12 400 MeV O-16 and 800 MeV Ne-20 ion
   beams, spanning a LET interval up to about 500 keV/mu m, has been
   investigated. A careful deconvolution analysis of the glow curve has
   been performed in order to obtain information for individual peaks. The
   region of linear response to gammas extends up to approximate to 1 Gy,
   while that for ions includes the complete dose interval covered in the
   study (up to approximate to 1.3 Gy). The ratio between the high- and
   low-temperature structures in the glow curve is correlated with
   radiation quality and dependence on ion identity, besides LET, is
   strongly suggested by the data. The thermoluminescent efficiency to ion
   exposure, with respect to irradiation with Co-60 gamma-rays, shows a
   different dependence on LET for each of the peaks. In general terms, the
   efficiency reaches a maximum between 1.2 and 1.4 near 6 keV/mu m and
   decreases for higher LET. Peak 3 displays a unique trend, its relative
   efficiency is always less than 1.0 and shows a strong monotonic inverse
   dependence with LET. (c) 2008 Elsevier B.V. All rights reserved.}},
