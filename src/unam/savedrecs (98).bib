@article{ ISI:000253878000003,
Author = {Luca, Florian},
Title = {{Prime divisors of binary holonomic sequences}},
Journal = {{ADVANCES IN APPLIED MATHEMATICS}},
Year = {{2008}},
Volume = {{40}},
Number = {{2}},
Pages = {{168-179}},
Month = {{FEB}},
Abstract = {{We prove that if (u(n))(n >= 0) is a sequence of rational numbers
   satisfying a recurrence of the type
   f(0)(n)u(n+2) + f(1)(n)u(n+1) + f(2)(n)u(n) = 0,
   where f(i)(X) is an element of Q{[}X] are not all zero for i = 0, 1, 2,
   which is not binary recurrent for all sufficiently large n, then there
   exists a positive constant c depending on the sequence (u(n))(n >= 0)
   such that the product of the numerators and denominators of the nonzero
   rational numbers u(n) for all n <= N has at least clog N prime factors
   as N -> infinity. (c) 2007 Elsevier Inc. All rights reserved.}},
