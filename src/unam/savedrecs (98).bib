@article{ ISI:000254036200012,
Author = {Barr, Elaine Espino and Cabello, Manuel Gallardo and Cabral Solis,
   Esther Guadalupe and Boa, Arturo Garcia and Gomez, Marcos Puente},
Title = {{Growth of the Pacific jack Caranx caninus (Pisces : Carangidae) from the
   coast of Colima, Mexico}},
Journal = {{REVISTA DE BIOLOGIA TROPICAL}},
Year = {{2008}},
Volume = {{56}},
Number = {{1}},
Pages = {{171-179}},
Month = {{MAR}},
Abstract = {{The Pacific jack Caranx caninus is a common species fished by artisanal
   fishermen off the coast of Colima, Mexico. During 2002, monthly samples
   of morphometric data and otoliths were taken to determine age and
   growth. Seven age groups were identified. The highest growth, 14.4 cm,
   takes place during the first year of life. During the second year, C.
   caninus grows 11.76 cm; the third year 9.61 cm; the fourth 7.85 cm; the
   fifth 6.41 cm and sixth year 5.24 cm. The constants of von
   Bertalariffy's growth equation were: L-infinity = 83.26 cm, W = 18.138
   g, K = 0.202, t(o) = -0.283 and A(0.95) = 15 years. Growth curves of
   other species of the same genus were calculated in order to compare them
   with the one obtained in the present work. The gonadosomatic index
   presented higher values during November and May. The periods of more
   intensive feeding are from August to February.}},
