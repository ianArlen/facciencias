@article{ ISI:000253677700012,
Author = {Sandoval-Diaz, A. and Pfeiffer, H.},
Title = {{Effects of potassium doping on the composition, structure and carbon
   dioxide chemisorption of Na2ZrO3}},
Journal = {{REVISTA MEXICANA DE FISICA}},
