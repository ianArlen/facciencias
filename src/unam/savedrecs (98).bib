@article{ ISI:000253848400022,
Author = {Elena Fernandez, Luisa and Gomez, Isabel and Pacheco, Sabino and Arenas,
   Ivan and Gilla, Sarjeet S. and Bravo, Alejandra and Soberon, Mario},
Title = {{Employing phage display to study the mode of action of Bacillus
   thuringiensis Cry toxins}},
