@inproceedings{ ISI:000263406501085,
Author = {Vitela, J. E. and Castanos, O.},
Book-Group-Author = {{IEEE}},
Title = {{A Real-Coded Niching Memetic Algorithm for Continuous Multimodal
   Function Optimization}},
