@article{ ISI:000252855700012,
Author = {Elena Sanchez-Mendoza, Maria and Castillo-Henkel, Carlos and Navarrete,
   Andres},
Title = {{Relaxant action mechanism of berberine identified as the active
   principle of Argernone ochroleuca Sweet in guinea-pig tracheal smooth
   muscle}},
Journal = {{JOURNAL OF PHARMACY AND PHARMACOLOGY}},
Year = {{2008}},
Volume = {{60}},
Number = {{2}},
Pages = {{229-236}},
Month = {{FEB}},
Abstract = {{In this study we investigated the relaxant effect of the aerial parts of
   Argemone ochroleuca (Papaveraceae), which is used in Mexican traditional
   medicine for the treatment of various respiratory diseases such as
   cough, bronchitis and asthma. The alkaloid berberine was identified as
   one of the active relaxant principles (EC50 = 118.50 +/- 3.91 mu M) in
   the dichloromethane extract of A. ochroleuca (EC50= 78.03 +/- 2.15 mu g
   mL(-1) with 95.12 +/- 3.56\% of relaxation). Berberine
   concentration-dependently relaxed the carbachol-induced precontractions
   but not histamine- or KCl-induced precontraction. The relaxant effect of
   berberine was unaffected by the presence of propranolol (3 mu M),
   glibenclamide (10 mu M) or ODQ (11 mu M). However, 2',
   5'-dideoxyadenosine (10 mu M) blocked the log concentration-response
   curves of berberine. On the other hand, berberine produced a leftward
   shift of the log concentration-response curves of isoproterenol,
   forskolin and nitroprusside. Additionally, berberine produced a parallel
   rightward shift of the concentration-response curve of carbachol in a
   competitive manner with a pA(2) of 3.87 +/- 0.045. The above results
   suggest that the relaxant effect of berberine on tracheal muscle is due
   to its antagonistic effect on muscarinic acetylcholine receptors.}},
