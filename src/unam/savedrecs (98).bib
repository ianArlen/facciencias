@article{ ISI:000255351700015,
Author = {Ake, Miguel Zavala and Cocoletzi, Gregorio H. and Takeuchi, Noboru},
Title = {{First-principles calculations of the atomic and electronic properties of
   group IIIA disilicides in AlB2 type structures}},
Journal = {{SOLID STATE SCIENCES}},
Year = {{2008}},
Volume = {{10}},
Number = {{3}},
Pages = {{355-361}},
Month = {{MAR}},
Abstract = {{Using first-principles total energy calculations we have studied the
   structural and electronic properties of group IIIA disilicides (ScSi2,
   YSi2, and LaSi2) in the AlB2 and Th3Pd5 structures. The latter can be
   obtained from the former structure after relaxation around Si vacancies
   within the (0001) plane. For ScSi2 and YSi2 we use the AlB2 Cell in our
   calculations, which yield lattice constants and cla values that are in
   poor agreement with experimental values. Instead, with Th3Pd5, the
   calculated structural parameters are in excellent agreement with
   experimental values. The introduction of vacancies results in more
   favorable metal-metal and Si-metal distances. For LaSi2, the
   introduction of vacancies barely changes the lattice constant, cla
   value, and atomic positions. Therefore, the La-La distance remains very
   large compared with the nearest neighbor distance in hexagonal bulk.
   This may be one of the reasons why LaSi2 has not been found in the AlB2
   type structure. Local density of states show that changes in the
   structural parameters play an important role in the stability of the
   Th3Pd5 phase. (c) 2007 Elsevier Masson SAS. All rights reserved.}},
