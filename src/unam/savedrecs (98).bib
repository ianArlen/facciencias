@article{ ISI:000252931000011,
Author = {Nagel, Erick and Pichardo, Barbara},
Title = {{A simple analytical formulation for periodic orbits in binary stars}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2008}},
Volume = {{384}},
Number = {{2}},
Pages = {{548-562}},
Month = {{FEB 21}},
Abstract = {{An analytical approximation to periodic orbits in the circular
   restricted three-body problem is provided. The formulation given in this
   work is based on calculations known from classical mechanics, but with
   the addition of certain terms necessary to give a reasonably good
   approximation. The results are compared with simulations. The derived
   simple set of analytical expressions gives periodic orbits on the discs
   of binary systems without the need to solve the equations of motion by
   numerical integration.}},
