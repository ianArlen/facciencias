@article{ ISI:000254441300011,
Author = {Lara, Alejandro and Borgazzi, Andrea and Mendes, Jr., Odim and Rosa,
   Reinaldo R. and Domingues, Margarete Oliveira},
Title = {{Short-period fluctuations in coronal mass ejection activity during solar
   cycle 23}},
Journal = {{SOLAR PHYSICS}},
Year = {{2008}},
Volume = {{248}},
Number = {{1}},
Pages = {{155-166}},
Month = {{MAR}},
Abstract = {{We have constructed a time series of the number of coronal mass
   ejections (CMEs) observed by SOHO/LASCO during solar cycle 23. Using
   spectral analysis techniques (the maximum entropy method and wavelet
   analysis) we found short-period (< one year) semiperiodic activity.
   Among others, we found interesting periodicities at 193, 36, 28, and 25
   days. We discuss the implications of such short-period activity in terms
   of the emergence and escape of magnetic flux from the convection zone,
   through the low solar atmosphere (where these periodicities have been
   found for numerous activity parameters), toward interplanetary space.
   This analysis shows that CMEs remove the magnetic flux in a
   quasiperiodic process in a way similar to that of magnetic flux
   emergence and other solar eruptive activity.}},
