@article{ ISI:000252224100009,
Author = {Ramirez-Fuentes, Yessica S. and Bucio, Emilio and Burillo, Guillermina},
Title = {{Thermo and pH sensitive copolymer based on acrylic acid and
   N-isopropylacrylamide grafted onto polypropylene}},
