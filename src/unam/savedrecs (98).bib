@article{ ISI:000254421000002,
Author = {Gendron, T. M.},
Title = {{The external fundamental group of an algebraic number field}},
Journal = {{COMPTES RENDUS MATHEMATIQUE}},
Year = {{2008}},
Volume = {{346}},
Number = {{5-6}},
Pages = {{253-256}},
Month = {{MAR}},
Abstract = {{We associate to every algebraic number field K/Q a hyperbolic surface
   lamination and an external fundamental group degrees Gamma(K): a
   generalization of the fundamental germ construction of Gendron that
   necessarily contains external (not first order definable) elements. The
   external fundamental group degrees Gamma(Q) is an extension of the
   absolute Galois group (Gamma) over cap (Q), that conjecturally contains
   a subgroup whose abelianization is isomorphic to the idele class group.}},
