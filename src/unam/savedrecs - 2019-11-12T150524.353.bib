@article{ ISI:000187659400031,
Author = {Gadea, A and Lopez, E and Lopez-Colome, AM},
Title = {{Glutamate-induced inhibition of D-aspartate uptake in Muller glia from
   the retina}},
Journal = {{NEUROCHEMICAL RESEARCH}},
