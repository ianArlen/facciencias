@article{ ISI:000224461300046,
Author = {Osorio, M and Anglada, G and Lizano, S and D'Alessio, P},
Title = {{A model to test the internal structure of the G31.41+0.31 hot molecular
   core}},
Journal = {{ASTROPHYSICS AND SPACE SCIENCE}},
Year = {{2004}},
Volume = {{292}},
Number = {{1-4}},
Pages = {{369-374}},
Note = {{International Workshop on Magnetic Fields and Star Foramtion, Univ
   Complutense de Madrid, Madrid, SPAIN, APR 21-25, 2003}},
Abstract = {{We investigate the physical conditions of the material around massive
   protostars by modelling the so-called hot molecular cores. We present a
   model for the spectral energy distribution ( SED) as well as the ammonia
   line emission of the G31.41+0.31 hot core. We find that the best fit to
   the SED is achieved by assuming a density distribution corresponding to
   the collapse of a magnetized logatropic sphere. On the other hand, the
   line emission is reproduced by adopting a gas phase ammonia abundance
   increasing towards the center of the core, as a result of the release of
   the ammonia molecules trapped in ice grain mantles.}},
