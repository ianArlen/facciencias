@article{ ISI:000220303700008,
Author = {Diaz-Sandoval, R and Mendoza, B},
Title = {{Study of the interaction between biological cells of different shapes
   and sizes and electromagnetic fields produced by natural phenomena}},
Journal = {{NATURAL HAZARDS}},
Year = {{2004}},
Volume = {{31}},
Number = {{1}},
Pages = {{143-156}},
Month = {{JAN}},
Abstract = {{We carry out a study of the interaction between electromagnetic fields
   produced in nature and cells with no structure but with different shapes
   and sizes, considered as approximations of those observed in the human
   body. Magnetic fields produced by various atmospheric and geomagnetic
   phenomena are compared with the fields generated by the cells
   themselves. Working with frequencies up to 10(5) Hz, we found that in
   most cases lightning is the natural phenomena capable of producing field
   intensities comparable to those generated by the cells. As a first
   approximation we assume that a cell might be affected only if a field of
   intensity comparable to the cell's field is present. Oscillating fields
   of less intensity and their harmonics can involve resonant interactions
   with the cells, however this situation is not considered here. Under the
   approximation of our study we found that the size and the shape are
   parameters of paramount importance: lightning represents a natural
   hazard only for the largest cells, while the quadrangular cells are not
   affected by any of the natural phenomena considered here.}},
