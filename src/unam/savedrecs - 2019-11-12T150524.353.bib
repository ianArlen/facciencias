@article{ ISI:000224936500010,
Author = {Hamza, MH and Added, A and Ben Mammou, A and Abdeljaoued, S and
   Rodriguez, R},
Title = {{Assessment of the vulnerability to pollution potential by pesticides, of
   the coastal aquifer of the plain of Metline-Ras Jebel-Raf Raf (Northeast
   of Tunisia), through GIS modelling using DRASTIC method}},
Journal = {{HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU}},
Year = {{2004}},
Number = {{5}},
Pages = {{86-94}},
Abstract = {{The exploitation of the coastal aquifer of Metline-Ras Jebel-Raf Raf is
   an economic imperative judged very important, because in the region
   considered, a great part of the population speeds up in the agricultural
   field. The use of pesticides in the agricultural field is becoming more
   pronounced, that's why a study of the vulnerability to pollution by
   pesticides of the aquifer, appeared necessary. Study of the
   vulnerability of Metline-Ras Jebel-Raf Raf coastal aquifer, was
   performed using the DRASTIC pesticides model, applied using ARC/Info and
   Idrisi GIS software. Results are presented as vulnerability map showing
   areas with low, moderate and high vulnerability. Zones with high
   vulnerability occupy 33\% of the total plain surface, zones with medium
   vulnerability occupy 53\%, and those with low vulnerability occupy 14\%.
   These results show that the studied aquifer is enough vulnerable to
   pollution by pesticides. More over, the high vulnerable zones are
   located in agricultural areas, where pesticides are extensively used,
   and that can increase the risk of contamination.}},
