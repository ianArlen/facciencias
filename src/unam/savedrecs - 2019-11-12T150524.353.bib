@article{ ISI:000188250200019,
Author = {Reyes-Lopez, CA and Hernandez-Santoyo, A and Pedraza-Escalona, M and
   Mendoza, G and Hernandez-Arana, A and Rodriguez-Romero, A},
Title = {{Insights into a conformational epitope of Hev b 6.02 (hevein)}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{2004}},
Volume = {{314}},
Number = {{1}},
Pages = {{123-130}},
Month = {{JAN 30}},
Abstract = {{Hevein (Hev b 6.02) is a major IgE-binding allergen in natural rubber
   latex and manufactured products. Both tryptophans (Trp(21) and Trp(23))
   of the hevein molecule were chemically modified with BNPS-skatole
   (2-nitrophenylsulfenyl-3-methyl-3'-bromoindolenine); derivatized
   allergen failed to significantly inhibit binding of serum IgE in ELISA
   assays. Similarly, skin prick tests showed that hevein-positive patients
   gave no response with the modified allergen. Dot blot experiments
   carried out with anti-hevein mono- and polyclonal antibodies confirmed
   the importance of Trp(21) and Trp(23) for antibody-recognition, and
   demonstrated the specific cross-reactivity of other molecules containing
   hevein-like domains. We also report the structure of Hev b 6.02 at an
   extended resolution (1.5 Angstrom) and compare its surface properties
   around Trp residues with those of similar regions in other allergens.
   Overall our results indicate that the central part of the protein, which
   comprises three aromatic and other acidic and polar residues,
   constitutes a conformational epitope. (C) 2003 Elsevier Inc. All rights
   reserved.}},
