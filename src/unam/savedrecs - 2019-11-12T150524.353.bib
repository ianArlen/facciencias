Author = {Ruiz-Fernandez, AC and Paez-Osuna, F and Urrutia-Fucugauchi, J and
   Preda, M and Rehault, I},
Title = {{Historical trace metal fluxes in the Mexico City Metropolitan Zone as
   evidenced by a sedimentary record from the Espejo de los Lirios lake}},
Journal = {{JOURNAL OF ENVIRONMENTAL MONITORING}},
Year = {{2004}},
Volume = {{6}},
