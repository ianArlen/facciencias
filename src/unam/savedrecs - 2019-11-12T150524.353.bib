@article{ ISI:000188928100007,
Author = {Aguilera-Navarro, VC and Fortes, M and de Llano, M},
Title = {{Stable and resonant Cooper pairs}},
Journal = {{SOLID STATE COMMUNICATIONS}},
Year = {{2004}},
Volume = {{129}},
Number = {{9}},
Pages = {{577-581}},
Month = {{FEB}},
Abstract = {{A Bethe-Salpeter treatment of Cooper pairs (CPs) based on an ideal Fermi
   gas (IFG) `sea' yields the familiar stable negative-energy,
   infinite-lifetime two-particle bound-state if two-hole CPs are ignored.
   Otherwise, it gives purely imaginary energies and thus unphysical
   solutions. However, a Bogoliubov transformation to the BCS ground state
   yields the trivial sound solution plus a nontrivial one. The latter are
   two-fermion `moving CPs' as positive-energy stable objects for zero
   center-of-mass momentum (CMM), and finite-lifetime resonant ones for
   nonzero CMM with a linear dispersion leading term. Hence, Bose-Einstein
