@article{ ISI:000224857800008,
Author = {Romero-Davila, E and Miranda, J},
Title = {{Trace element determination in tomato puree using particle induced X-ray
   emission and Rutherford backscattering}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2004}},
Volume = {{262}},
Number = {{2}},
Pages = {{355-362}},
Abstract = {{Particle induced X-ray emission (PIXE) and Rutherford backscattering
   spectrometry (RBS) were used to determine the concentrations of trace
   elements in samples of 12 tomato puree brands sold in the Mexican
   market. While RBS offered information about the main elements present in
   the matrix, PIXE gave results on trace elements. As a whole, data for 17
   elements (C, N, O, Na, Mg, S, Cl, K, Ca, Ti, V, Cr, Mn, Fe, Ni, Cu, and
   Zn) were obtained. To evaluate the results, a comparison with brands
   from USA, Japan, Colombia, and Chile was carried out, using tomato
   purees produced following the domestic technology recipe. Additionally,
   the results were considered in the light of the Codex Alimentarius and
   the Mexican standard. It was found that all of the brands fall within
   the limits established by these standards, being of the same order of
   magnitude as the foreign brands.}},
