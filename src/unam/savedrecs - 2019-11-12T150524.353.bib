@inproceedings{ ISI:000222031800012,
Author = {Cabral-Rosetti, LG and Hernandez, X and Sussman, RA},
Editor = {{Breton, N and Cervantes, JL and Salgado, M}},
Title = {{Infering annihilation channels of neutralinos in galactic halos}},
Booktitle = {{EARLY UNIVERSE AND OBSERVATIONAL COSMOLOGY}},
Series = {{LECTURE NOTES IN PHYSICS}},
Year = {{2004}},
Volume = {{646}},
Pages = {{309-320}},
Note = {{5th Mexican School on Early Universe and Observational Cosmology, Mexico
   City, MEXICO, NOV, 2002}},
Abstract = {{Applying the microcanonical definition of entropy to a weakly
   interacting and self-gravitating neutralino gas, we evaluate the change
   in the local entropy per particle of this gas between the freeze out era
   and present day virialized halo structures. An ``entropy consistency{''}
   criterion emerges by comparing the obtained theoretical entropy per
   particle of the virialized halos with an empirical entropy per particle
   given in terms of dynamical halo variables of actual galactic
   structures. We apply this criterion to the cases when neutralinos are
   mostly B-inos and mostly Higgsinos, in conjunction with the usual
   ``abundance{''} criterion requiring that present neutralino relic
   density complies with 0.2 < Omega(chi)-1(0) < 0.4 for h similar or equal
   to 0.65. The joint application of both criteria reveals that; a much
   better fitting occurs for the B-ino than for the Higgsino channels, so
   that the former seems to be a favored channel along the mass range of
   150 GeV < m(chi)-1(0) < 250 GeV. These results axe consistent with
   neutralino annihilation patterns that emerge from recent theoretical
   analysis on cosmic ray positron excess data reported by the HEAT
   collaboration. The suggested methodology can be applied to test, other
   annihilation channels of the neutralino; as well as other particle
   candidates of thermal WIMP gas relies.}},
