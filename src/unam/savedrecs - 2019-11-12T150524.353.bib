@article{ ISI:000220236400015,
Author = {Marzantowicz, W and Prieto, C},
Title = {{The unstable equivariant fixed point index and the equivariant degree}},
Journal = {{JOURNAL OF THE LONDON MATHEMATICAL SOCIETY-SECOND SERIES}},
Year = {{2004}},
Volume = {{69}},
Number = {{1}},
Pages = {{214-230}},
Month = {{FEB}},
Abstract = {{A correspondence between the equivariant degree introduced by Ize,
   Massabo, and Vignoli and an unstable version of the equivariant fixed
   point index defined by Prieto and Ulrich is shown. With the help of
   conormal maps and properties of the unstable index, a sum decomposition
   formula is proved for the index and consequently also for the degree. As
   an application, equivariant homotopy groups are decomposed as direct
   sums of smaller groups of fixed orbit types, and a geometric
   interpretation of each summand is given in terms of conormal maps.}},
