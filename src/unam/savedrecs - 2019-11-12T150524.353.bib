@article{ ISI:000222028900008,
Author = {Martinez Perez, ME and Macek, M and Castro Galvan, MT},
Title = {{Do protozoa control the elimination of Vibrio cholerae in brackish
   water?}},
Journal = {{INTERNATIONAL REVIEW OF HYDROBIOLOGY}},
Year = {{2004}},
Volume = {{89}},
Number = {{2}},
Pages = {{215-227}},
Abstract = {{Elimination of inoculated Vibrio cholerae (greater than or equal to10(7)
   cells ml(-1)) within a brackish water bacteria assemblage (Mecoacan
   Lagoon, State of Tabasco, Mexico) was studied in laboratory microcosms
   with filtration-fractionated water. Feeding of a ciliate, Cyclidium
   glaucoma was evaluated using fluorescently labelled V. cholerae O1. Even
   though V. cholerae was not exploited as the major food source, ciliates
   were able to eliminate it efficiently. An addition of chitin directly
   supported the growth of bacteria, although not so much of V. cholerae,
   and indirectly the growth of the protistan assemblage. Generally, the
   changes in a bacterial assemblage structure were the most important in
   V. cholerae elimination.}},
