@article{ ISI:000222266300011,
Author = {Krupski, P},
Title = {{Families of continua with the property of Kelley, arc continua and
   curves of pseudo-arcs}},
Journal = {{HOUSTON JOURNAL OF MATHEMATICS}},
Year = {{2004}},
Volume = {{30}},
Number = {{2}},
Pages = {{459-482}},
Abstract = {{It is shown that (1) the family of all continua in I-n, n greater than
   or equal to 2, which have the property of Kelley (with the Hausdorff
   metric) is an absolute true F-sigmadelta-set; (2) the family of all arc
   continua in I-n, n greater than or equal to 3, is coanalytic complete;
   (3) the families of all arcs, circles, solenoids of pseudo-arcs and all
   Menger or Sierpinski curves of pseudo-arcs in cubes are Borel sets which
   are not Gr(deltasigma).}},
