@article{ ISI:000220646100008,
Author = {Gromovoy, TY and Palyanytsya, BB and Pokrovskiy, VA and Basiuk, EV and
   Basiuk, VA},
Title = {{Interaction of thermally pretreated carbon nanomaterials with water
   vapor}},
Journal = {{JOURNAL OF NANOSCIENCE AND NANOTECHNOLOGY}},
Year = {{2004}},
Volume = {{4}},
Number = {{1-2}},
Pages = {{77-81}},
Month = {{JAN-FEB}},
Abstract = {{We used temperature-programmed desorption-mass spectrometry to verify
   whether low-molecular-weight products can form by reacting thermally
   pretreated single-walled and multi-walled carbon nanotubes (SWNTs and
   MWNTs, respectively) with water vapor. The reactivity of MWNTs toward
   water is similar to the reactivity of graphite, whereas acid-oxidized
   SWNTs behave like polymerized C-60 fullerene. We think the main factors
   influencing the reactivity are molecular surface curvature and the
   presence of pyrolyzable defect groups, which create highly strained
   bonds upon their elimination.}},
