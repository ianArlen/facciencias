@inproceedings{ ISI:000229933400021,
Author = {Lerma, S and Jescarz, S and Hess, PO and Civitarese, O and Reboiro, M},
Editor = {{Escher, J and Castanos, O and Hirsch, JG and Pittel, S and Stoitcheva, G}},
Title = {{A toy model for QCD at low and high temperatures}},
Booktitle = {{COMPUTATIONAL AND GROUP-THEORETICAL METHODS IN NUCLEAR PHYSICS}},
