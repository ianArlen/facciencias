@article{ ISI:A1995TP57600001,
Author = {Capovilla, R and Guven, J},
Title = {{Algebra of surface deformations}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{1995}},
Volume = {{12}},
Number = {{12}},
Pages = {{L107-L111}},
Month = {{DEC}},
Abstract = {{We present the universal algebra of infinitesimal deformations for
   surfaces of arbitrary co-dimension in an arbitrary ambient manifold.
   This algebra generalizes the algebra of hypersurface deformations,
   familiar in the canonical formulation of general relativity. It differs
   in the important respect that, in general, there will be a dependence of
   the `structure functions' on the extrinsic geometry, via the extrinsic
   twist potential, in addition to the known dependence on the intrinsic
   geometry of the surface, via the contravariant induced metric.}},
