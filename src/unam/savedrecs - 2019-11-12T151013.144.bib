@article{ ISI:A1995RA72800002,
Author = {RODRIGUEZ, MD and ZAMUDIO, F and TORRES, JA and GONZALEZCERON, L and
   POSSANI, LD and RODRIGUEZ, MH},
Title = {{EFFECT OF A CECROPIN-LIKE SYNTHETIC PEPTIDE (SHIVA-3) ON THE SPOROGONIC
   DEVELOPMENT OF PLASMODIUM-BERGHEI}},
Journal = {{EXPERIMENTAL PARASITOLOGY}},
Year = {{1995}},
Volume = {{80}},
Number = {{4}},
Pages = {{596-604}},
Month = {{JUN}},
Abstract = {{The effect of a synthetic cecropin-like peptide, Shiva-3, on in vitro
   ookinete development and on the early sporogonic stages of Plasmodium
   berghei in the midgut of Anopheles albimanus was investigated. Peptide
   concentrations of 75 and 100 mu M were effective (P < 0.05) in reducing
   ookinete production and the number of infected mosquitoes in almost all
   experiments. These peptide concentrations in the midgut were not toxic
   for the survival of the mosquitoes. Complete inhibition was obtained if
   100 mu M Shiva-3 was applied in the first 8 hr of parasite development.
   The deleterious effect of the peptide on the parasite was effective
   after exposure for as short as 50 sec and the permanence of free peptide
   in the mosquito midgut was estimated to be of a minimum of 5 min, These
   observations indicate the possibility of using Shiva-like peptide genes
   to engineering malaria-resistant vectors as an alternative in malaria
   control strategies. (C) 1995 Academic Press, Inc.}},
