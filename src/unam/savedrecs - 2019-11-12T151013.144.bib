@article{ ISI:A1995TT03800006,
Author = {BritoDeLaFuente, E and Tovar, LR},
Title = {{Popping and cleaning of amaranth seeds in a fluidized bed}},
Journal = {{FOOD AND BIOPRODUCTS PROCESSING}},
Year = {{1995}},
Volume = {{73}},
Number = {{C4}},
Pages = {{183-188}},
Month = {{DEC}},
Abstract = {{Amaranth was a major crop for pre-Colombian civilizations. Traditional
   methods for processing amaranth grains include popping in a hot clay
   plate. If processed this way, popping temperatures, reached mainly by a
   conductive heat transfer mechanism, may damage the high nutritional
   value of the grains. This paper describes the development of a popping
   process based on fluidization engineering principles. Differences around
   70\% between the apparent densities of unpopped and popped grains
   allowed a continuous popping operation. The popping temperatures used in
   the fluidized bed are lower than those found in the traditional popping
   methods. The bed fluid dynamics were studied as a function of the column
   diameter, the height of the bed and the superficial air velocity.
   Minimum and terminal fluidization velocities were determined for both
   popped and unpopped grains. The fluidization columns were also used for
   cleaning purposes to separate the small amaranth grain from solid
   residues due to harvesting operations. A mathematical model was used to
   describe elutriation kinetics as a function of the operational
   conditions.}},
