@inproceedings{ ISI:000089787000121,
Author = {Cruz-Mena, J and Caswell, B and Mena, B},
Editor = {{AitKadi, A and Dealy, JM and James, DF and Williams, MC}},
Title = {{Extensional flow of Non-Newtonian fluids with shearing histories:
   Experimental results.}},
Booktitle = {{XIITH INTERNATIONAL CONGRESS ON RHEOLOGY, PROCEEDINGS}},
Year = {{1996}},
Pages = {{210}},
Note = {{XIIth International Congress on Rheology, QIUEBEC CITY, CANADA, AUG
   18-23, 1996}},
Abstract = {{A shear-elongational rheometer has been developed to relate shear
   properties of complex fluids in a viscometric flow situation to
   simultaneous measurements on elongational flow. The viscometric flow
   imposes a pre-shearing history on the fluid, allowing for the systematic
   study of its influence on the extensional properties of various
   materials. Measurements are presented for Newtonian oils, viscoelastic
   polymeric solutions and a constant shear viscosity, elastic fluid of the
   Boger type.}},
Publisher = {{UNIV LAVAL, DEPT MECH ENGN}},
Address = {{CITE UNIVERSITAIRE, QUEBEC CITY, QUEBEC G1K 7P4, CANADA}},
