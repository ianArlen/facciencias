@article{ ISI:A1995RE61400009,
Author = {MARTINEZFLORES, I and BUSTAMANTE, VH and PUENTE, JL and CALVA, E},
Title = {{CLONING AND CHARACTERIZATION OF THE SALMONELLA-TYPHI OMPR AND ENVZ GENES}},
Journal = {{ASIA-PACIFIC JOURNAL OF MOLECULAR BIOLOGY AND BIOTECHNOLOGY}},
Year = {{1995}},
Volume = {{3}},
Number = {{2}},
Pages = {{135-144}},
Month = {{JUN}},
Abstract = {{In S. typhi, OmpC is a major outer membrane protein whose synthesis is
   not repressed at low osmolarity as in Escherichia coli. In order to
   better understand the regulation of OmpC in S. typhi, we isolated and
   characterized its regulatory locus, ompB. The comparison of ompB DNA
   sequences among S. typhi, S. typhimurium, and E. coli, reveals that they
   are highly conserved, and that the S. typhi ompB locus is also
   constituted by the ompR and envZ genes. The ompR and envZ coding regions
   are overlapped by four base pairs and code for proteins OmpR and EnvZ,
   of 239 and 450 amino acids (aa), respectively. The aa identity between
   S. typhi and E. coli OmpR is 100\%. The aa identity between S. typhi and
   E. coli EnvZ is 95.3\%. The alignments reveal the conservation of
   important aa, such as Asp55 of OmpR and His243 of EnvZ. Only one aa
   residue in S. typhi EnvZ, at position 354 where a Cys replaces a Gly, is
   neither conserved in S. typhimurium nor in E. coli EnvZ.}},
