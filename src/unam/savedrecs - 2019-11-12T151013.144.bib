@article{ ISI:A1995TT18600006,
Author = {PerezMitre, GG},
Title = {{Real weight, imaginary weight and body image distortion}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1995}},
Volume = {{12}},
Number = {{2}},
Pages = {{185-197}},
Month = {{DEC}},
Abstract = {{The present paper proposes a two category system for weight problems:
   real weight and imaginary weight. The aim is to facilitate the
   understanding and management of body image distortion. Data on prevalent
   trends and obesity distribution patterns as well as image distortion
   were collected from a series of samples of Mexican populations. The main
   findings from both theoretical and practical points of view revealed a
   conditional character of distortion effects and a likely inversion in
   the following sequence: a) body image distortion first, preceding b) the
   development of high-risk practices associatied to eating disorders.}},
