@article{ ISI:A1995QZ44400003,
Author = {BELMONTMORENO, E and MENCHACAROCHA, A},
Title = {{GEOMETRICAL EFFECTS IN CHARGED-PARTICLE DETECTION USING
   SCINTILLATION-COUNTERS}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{1995}},
Volume = {{28}},
Number = {{5}},
Pages = {{835-839}},
Month = {{MAY 14}},
Abstract = {{An algorithm is described which uses the Monte Carlo method to evaluate
   the fraction of the light produced by charged particles in the bulk
   which reaches the scintillator's output face, by taking into account
   surface and optical absorption. Local light production is estimated
   using the model of Michaelian and Menchaca-Rocha (1994 Phys. Rev. B 49
   15550-62). These absorption mechanisms are shown to introduce
   range-dependent effects which are most important when the penetration of
   the particles is comparable with the depth of the scintillator.}},
