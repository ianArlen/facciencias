@article{ ISI:A1995TG26900009,
Author = {NAVARRO, Y and MIRANDA, J and SOBERON, M},
Title = {{CYTOCHROME EXPRESSION IN 2 STRAINS OF BRADYRHIZOBIUM SPECIES}},
Journal = {{ASIA-PACIFIC JOURNAL OF MOLECULAR BIOLOGY AND BIOTECHNOLOGY}},
Year = {{1995}},
Volume = {{3}},
Number = {{4}},
Pages = {{327-331}},
Month = {{DEC}},
Abstract = {{Two strains belonging to two different groups of the genus
   Bradyrhizobium, were compared with regard to their cytochrome expression
   pattern and symbiotic nitrogen fixation capacities. Strain USDA8085 of
   the Bradyrhizobium japonicum-and strain USDA6ln of the Bradyrhizobium
   elkanii group were equally effective for nitrogen fixation during
   symbiosis as judged by the total nitrogen content of nodulated soybean
   plants 30 days after inoculation Both strains showed a similar
   cytochrome pattern with cytochrome aa(3) and cytochrome (o) under bar as
   the main terminal oxidases. Nevertheless, strain USDA61n repressed the
   expression of cytochrome aa(3) in the stationary phase of growth much
   more rapidly than strain USDA8085. Also USDA61n showed a reproducible
   cytochrome d expression in the late stationary phase in contrast with
   USDA8085.}},
