@article{ ISI:A1995QY31000007,
Author = {MOLLOY, KC and MAHON, MF and HAIDUC, I and SILVESTRU, C},
Title = {{SOLID-STATE SUPRAMOLECULAR STRUCTURE OF
   (TETRAPHENYLDITHIOIMIDODIPHOSPHINATO)TRIMETHYLTIN(IV)CENTER-DOT-BENZENE,
   {[}ME(3)SN(SPPH2)(2)N-CENTER-DOT-C6H6](INFINITY), A UNIQUE POLYMER
   CONTAINING A BRIDGING -SPR(2)NPR(2)S- LIGAND}},
Journal = {{POLYHEDRON}},
Year = {{1995}},
Volume = {{14}},
Number = {{9}},
Pages = {{1169-1174}},
Month = {{MAY}},
Abstract = {{In the solid state (tetraphenyldithioimidodiphosphinato)trimethyltin(IV)
   consists of Me(3)Sn{[}SPPh(2)NPPh(2)S] units linked into a 1-D polymer
   by secondary Sn...S interactions. The coordination geometry about tin is
   trigonal bipyramidal trans-S2SnC3, with two sulphur atoms (belonging to
   different molecular units) in axial positions {[}Sn(1)-S(1) = 2.517(4)
   Angstrom and Sn(1)...S(2') = 3.627 Angstrom] and three methyl groups in
   the equatorial sites. This coordination is uncommon for organotin
   thio-derivatives and is now crystallographically confirmed for the first
   time. The compound also incorporates the first example of a bridging,
   rather than chelating, dithioimidodiphosphinato ligand, thus generating
   a supramolecular structure which acts as a host for benzene molecules
   aligned along the b axis of the crystal.}},
