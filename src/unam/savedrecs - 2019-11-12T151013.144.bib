@article{ ISI:A1995RV94000013,
Author = {STUART, RG},
Title = {{DETERMINATION OF THE SOLAR DIAMETER AND IDENTIFICATION OF LUNAR LIMB
   FEATURES USING BESSELIAN ELEMENTS}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{1995}},
Volume = {{276}},
Number = {{2}},
Pages = {{476-482}},
Month = {{SEP 15}},
Abstract = {{It is shown how the rectangular coordinates, hour angles, declinations,
   horizontal parallaxes, semidiameters, position angles and separation of
   the centres of the Sun and the Moon, which are necessary for the
   reduction of certain types of solar eclipse data, which can be
   calculated directly from the Besselian elements. As an application, a
   method is derived for determining the solar angular semidiameter from
   the time that known lunar features are observed to cross the solar limb.
   The related problem of finding the selenographic coordinates of features
   on or near the lunar limb is also treated.}},
