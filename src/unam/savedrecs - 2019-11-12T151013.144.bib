@article{ ISI:A1995TD90600001,
Author = {ACEVES, R and CALDINO, U and RUBIO, J and CAMARILLO, E},
Title = {{NONRADIATIVE ENERGY-TRANSFER SN2+-]MN2+ IN MONOCRYSTALLINE KBR}},
Journal = {{JOURNAL OF LUMINESCENCE}},
Year = {{1995}},
Volume = {{65}},
Number = {{3}},
Pages = {{113-119}},
Month = {{AUG}},
Abstract = {{Energy transfer from Sn2+ to Mn2+ ions in single crystals of KBr codoped
   with tin and manganese ions has been investigated. The spectroscopic
   data obtained clearly indicate that sensitized luminescence with tin
   ions as sensitizers and manganese ions as activators takes place in this
   material even at very low concentrations of the dopants and after the
   crystals are subjected to severe quenching treatments. This result
   suggests that the impurity ions are not randomly distributed but rather
   are forming small Sn-Mn clusters. It is established that an exchange
   (superexchange) interaction mechanism might be the responsible one for
   the Sn2+ --> Mn2+ energy transfer process taking place in KBr.}},
