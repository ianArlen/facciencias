@article{ ISI:A1995TJ16300014,
Author = {FLORESHERRERA, O and PARDO, JP and ESPINOSAGARCIA, MT and MARTINEZ, F},
Title = {{CALCIUM-TRANSPORT IN HUMAN TERM PLACENTAL MITOCHONDRIA}},
Journal = {{BIOCHEMISTRY AND MOLECULAR BIOLOGY INTERNATIONAL}},
Year = {{1995}},
Volume = {{35}},
Number = {{4}},
Pages = {{793-801}},
Month = {{APR}},
Abstract = {{The kinetic characteristics of the transport of Ca2+ in the human term
   placental mitochondria previously depleted of their intramitochondrial
   calcium, was studied. Maximal Ca2+ uptake was observed in the presence
   of inorganic phosphate and ADP. Ca2+ transport showed a Michaelis-Menten
   behavior, with a Vmax of 61 nmol mg(-1) min(-1) and a Km of 118 mu M.
   Ruthenium red inhibited mitochondrial Ca2+ uptake uptake. Under optimal
   conditions of Ca2+ uptake, the cation inhibited progesterone synthesis
   in placental mitochondria. The kinetic parameters of Ca2+ transport
   agree with the physiological role of mitochondria in the human placenta.}},
