@article{ ISI:A1995RZ31400013,
Author = {ANTILLON, A and ESCALONA, J and GERMAN, G and TORRES, M},
Title = {{SELF-DUAL NON-ABELIAN VORTICES IN A PHI(2) CHERN-SIMONS THEORY}},
Journal = {{PHYSICS LETTERS B}},
Year = {{1995}},
Volume = {{359}},
Number = {{3-4}},
Pages = {{327-333}},
Month = {{OCT 12}},
Abstract = {{We study a non-Abelian Chem-Simons gauge theory in 2 + 1 dimensions with
   the inclusion of an anomalous magnetic interaction. For a particular
   relation between the Chem-Simons (CS) mass and the anomalous magnetic
   coupling the equations for the gauge fields reduce from second- to
   first-order differential equations of the pure CS type. We derive the
   Bogomol'nyi-type or self-dual equations for a Phi(2) scalar potential,
   when the scalar and topological masses are equal. The corresponding
   vortex solutions carry magnetic flux that is not quantized due to the
   non-topological nature of the solitons. However, as a consequence of the
   quantization of the CS term, both the electric charge and angular
   momentum are quantized.}},
