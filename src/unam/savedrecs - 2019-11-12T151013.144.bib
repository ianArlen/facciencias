@article{ ISI:A1995RC80400013,
Author = {VALENZUELABENAVIDES, J and DELAGARZA, LM},
Title = {{ELECTRONIC SUPERSTRUCTURES ON THE GRAPHITE SURFACE OBSERVED BY
   SCANNING-TUNNELING-MICROSCOPY - AN INTERFERENCE MODEL}},
Journal = {{SURFACE SCIENCE}},
Year = {{1995}},
Volume = {{330}},
Number = {{2}},
Pages = {{227-233}},
Month = {{JUN 10}},
Abstract = {{Electron-density superstructures (SS) on highly oriented pyrolytic
   graphite (HOPG) have long been observed near surface defects and
   surrounding metal particles by scanning tunneling microscopoy (STM). 
   Xhie et al. analyzed and catalogued ten such SS near Pt particles on
   HOPG {[}Phys. Rev. B 43 (1991) 8917].  Mizes and Foster suggested that
   the SS are the result of interference between the normal electron waves
   and waves scattered by the metal particle {[}Science 244 (1989) 559]. 
   This suggestion was used by Shedd and Russell to introduce an
   interference model using simple electron wave functions and simulated
   two of the most commonly observed SS {[}Surf. Sci. 266 (1992) 259]. 
   With this work we find that, by introducing a more general form for the
   electron wave functions and by considering multiple component
   interference, we can simulate many of the complex SS observed by STM,
   supporting the assertion that these SS are of an electronic nature and
   not a real surface reconstruction.}},
