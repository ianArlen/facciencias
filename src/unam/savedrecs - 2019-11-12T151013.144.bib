@article{ ISI:A1995RR68500008,
Author = {SMIRNOV, Y and TURBINER, A},
Title = {{LIE ALGEBRAIC DISCRETIZATION OF DIFFERENTIAL-EQUATIONS}},
Journal = {{MODERN PHYSICS LETTERS A}},
Year = {{1995}},
Volume = {{10}},
Number = {{24}},
Pages = {{1795-1802}},
Month = {{AUG 10}},
Abstract = {{A certain representation for the Heisenberg algebra in finite difference
   operators is established. The Lie algebraic procedure of discretization
   of differential equations with isospectral property is proposed. Using
   sl(2)-algebra based approach, (quasi)-exactly-solvable finite difference
   equations are described. It is shown that the operators having the Hahn,
   Charlier and Meissner polynomials as the eigenfunctions are reproduced
   in the present approach as some particular cases. A discrete version of
   the classical orthogonal polynomials (like Hermite, Laguerre, Legendre
   and Jacobi ones) is introduced.}},
