@article{ ISI:A1995TD68500031,
Author = {ALBARRAN, G and NEGRONMENDOZA, A},
Title = {{SYNTHESIS OF BETA-HALOACIDS BY RADIATION}},
Journal = {{RADIATION PHYSICS AND CHEMISTRY}},
Year = {{1995}},
Volume = {{46}},
Number = {{4-6, 1}},
Pages = {{541-544}},
Month = {{OCT-DEC}},
Abstract = {{Hydrogen halides add to alkenes yielding the corresponding haloalkane.
   The addition of HX ordinally follows the course of Markownikoff's rule.
   In this paper we analyze the HBr addition reaction to unsaturated
   carboxylic acids. The course of the addition is anti-Markownikoff. This
   implies that the reaction goes through free radical mechanism and the
   haloacid formed is in the beta-position. The acids under study are
   fumaric, itaconic, citraconic acids and the methyl ester of fumaric
   acid. The beta-haloacids formed are easily purified from the bulk
   solutions.}},
