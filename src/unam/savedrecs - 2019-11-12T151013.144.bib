@article{ ISI:A1995RW02500002,
Author = {ARCE, C and RAMOS, J and GUEVARA, MA and CORSICABRERA, M},
Title = {{EFFECT OF SPATIAL ABILITY AND SEX ON EEG POWER IN HIGH-SCHOOL-STUDENTS}},
