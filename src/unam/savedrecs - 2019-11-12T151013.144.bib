@article{ ISI:A1995TN95800005,
Author = {SolerClaudin, C and BasualdoSigales, MD},
Title = {{A perinatal diagnostic problem: HIV infected patients who are
   serologically negative.}},
Journal = {{SALUD PUBLICA DE MEXICO}},
Year = {{1995}},
Volume = {{37}},
Number = {{6}},
Pages = {{515-519}},
Month = {{NOV-DEC}},
Abstract = {{Comprehensive HN diagnosis of 142 patients was done by ELISA, IgG
   immunoblot, IgA immunoblot (in patients under 24 months), plasma antigen
   determination, viral isolation by culture and genome detection by
   polymerasa chain reaction (PCR) amplification. Results showed that 14
   patients (10\%) with negative or indefinite results for anti-HIV-1 IgG
   antibody were in fact infected by the virus. Eleven of these patients
   were between 2 and 24 months of age, two between 4 and 6.5 years and one
   was 30 years old. Diagnosis was obtained by antigen positivity in four
   of them; by PCR amplification of peripheral mononuclear cells of
   proviral DNA in five of them; by PCR and immunofluorescence (IF) of
   cultured cells in four cases, and the last diagnosis was made by IgG
   immunoblot and IF of the viral culture. These cases pose a problem
   because of false negative HN serology, particularly ly in patients under
   24 months of age.}},
