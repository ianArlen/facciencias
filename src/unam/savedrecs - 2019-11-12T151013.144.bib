@article{ ISI:A1995TE17300065,
Author = {DENG, F and RENWICK, S and MARTINEZ, H and MORGAN, TJ},
Title = {{ABSOLUTE CROSS-SECTION MEASUREMENTS FOR IONIZATION OF HE RYDBERG ATOMS
   IN COLLISIONS WITH K}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{1995}},
Volume = {{52}},
Number = {{5}},
Pages = {{3833-3835}},
Month = {{NOV}},
Abstract = {{Absolute cross sections for ionization of 1.5-10.0 keV/amu Rydberg
   helium atoms in principal quantum states 12 less than or equal to n less
   than or equal to 15 due to collisions with potassium have been measured.
   The data are compared with the free-electron cross section at equal
   velocity. Our results for the collisional ionization cross sections
   (sigma(i)) agree both in shape and absolute magnitude with the data
   available for the total electron-scattering cross sections(sigma(e)) and
   support recent theoretical models for ionization of Rydberg atoms with
   neutral perturbers.}},
