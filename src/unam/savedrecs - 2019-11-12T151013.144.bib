@article{ ISI:A1995RM21400001,
Author = {MAGANA, LF and VAZQUEZ, GJ},
Title = {{AB-INITIO CALCULATION OF THE ELASTIC-CONSTANTS OF MAGNESIUM}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{1995}},
Volume = {{7}},
Number = {{30}},
Pages = {{L393-L396}},
Month = {{JUL 24}},
Abstract = {{We have used a local, first principles pseudopotential to calculate the
   elastic constants of magnesium. Our results are in good agreement with
