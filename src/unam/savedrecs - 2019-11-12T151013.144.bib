@article{ ISI:A1995TT18600002,
Author = {Galaz, MMF and Loving, RD},
Title = {{Development and validation of a multidimensional assertiveness scale for
   students}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1995}},
Volume = {{12}},
Number = {{2}},
Pages = {{133-144}},
Month = {{DEC}},
Abstract = {{The main goal of the present research project was to develop, and
   psychometrically validate a multidimensional assertiveness scale for
   Mexican students. 439 college and high school students from Mexico City,
   were selected non-probabilistically. The sample was selected by cuotas
   for gender, age and educational level. Based on previous studies (Flores
   1989; 1994), assertiveness and its dimensions were defined and a
   multidimensional Likert instrument was developed. The results obtained
   from the principal component ortogonal rotation factor analysis show
   assertiveness has five principal dimensions: indirect assertiveness
   (alpha = 0.86), non-assertiveness in affective situations (alpha =
   0.70), assertiveness in daily situations (alpha = 0.78), indirect
   assertiveness with authority figures (alpha = 0.58) and
   non-assertiveness in school areas (alpha = 0.72). The results confirm
   the importance of the sociocultural context in the structure of the
   construct, and point toward the importance of an ethnopsychological
   perspective in the conceptualization and development of personality
   measures.}},
