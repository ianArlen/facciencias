Editor = {{Niemela, V and Morell, N}},
Title = {{The erupting Wolf-Rayet binary HD 5980 in the small magellanic cloud:
   Spectral transition from B1.5Ia(+) to WN6 and the accompanying light
   curve.}},
Booktitle = {{WORKSHOP ON COLLIDING WINDS IN BINARY STARS TO HONOR JORGE SAHADE}},
Series = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA, SERIE DE CONFERENCIAS}},
