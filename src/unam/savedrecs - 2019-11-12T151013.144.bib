@article{ ISI:A1995TL03500007,
Author = {Mondragon, A and Hernandez, E},
Title = {{Multiple poles of the S(E) matrix and the geometric phase of resonant
   states}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1995}},
Volume = {{41}},
Number = {{1}},
Pages = {{53-61}},
Month = {{DEC}},
Note = {{XVIII Symposium on Nuclear Physics, OAXTEPEC, MEXICO, JAN 04-07, 1995}},
Organization = {{Inst Mexicano Seguridad Social}},
Abstract = {{We show that, in striking contrast with the case of bound states,
   accidental degeneracies of resonant states give rise to multiple poles
   in the scattering matrix as function of complex energy. We also show
   that, close to a crossing of resonances, the topology of the energy
   surfaces is different from that at a crossing of bound states. We
   discuss some consequences of these differences for such physical
   properties as the Berry phase of resonant states, the distribution of
   resonance spacings in the spectrum of a quantum chaotic system and the
   phenomenon of level width attraction.}},
