@article{ ISI:A1995RH44600020,
Author = {ESPINOSA, G and SILVA, RJ},
Title = {{CR-39 NUCLEAR TRACK DETECTORS FOR IDENTIFICATION OF PU-239 AND AM-241
   ALPHA-CONTAMINATION}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY-ARTICLES}},
Year = {{1995}},
Volume = {{194}},
Number = {{1}},
Pages = {{185-189}},
Month = {{JUL}},
