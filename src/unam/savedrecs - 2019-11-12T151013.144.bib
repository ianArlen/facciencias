@article{ ISI:A1995QN24000017,
Author = {MARTINEZ, H and MORGAN, TJ},
Title = {{ABSOLUTE CROSS-SECTION MEASUREMENTS FOR COLLISIONAL DESTRUCTION OF HE
   RYDBERG ATOMS AND SINGLE-ELECTRON CAPTURE OF HE+ IONS IN COLLISION WITH
   SF6}},
Journal = {{CHEMICAL PHYSICS}},
Year = {{1995}},
Volume = {{193}},
Number = {{1-2}},
Pages = {{193-198}},
Month = {{APR 1}},
Abstract = {{Collisional destruction of 6-40 keV Rydberg helium atoms, having
   principal quantum numbers in the band 12 less than or equal to n less
   than or equal to 15, and single electron capture of 6-34 keV He+ ions in
   collisions with SF6 gas have been measured. Both cross-sections art in
   good accord with previous lower energy measurements and, for single
   electron capture, with estimates based on a semiempirical model. Our
   measured Rydberg collisional destruction cross-sections have been
   compared with the sum of the free electron cross-section sigma(e) (at
   equal velocity) and the single electron capture cross-sections sigma(10)
   of He+. The data show that the cross-sections for Rydberg collisional
   destruction are consistent with the independent particle model.}},
