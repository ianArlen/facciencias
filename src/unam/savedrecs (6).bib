@article{ ISI:000430940900059,
Author = {Parikh, A. S. and Wijnands, R. and Degenaar, N. and Ootes, L. and Page,
   D.},
Title = {{Further constraints on neutron star crustal properties in the low-mass
   X-ray binary 1RXS J180408.9-342058}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2018}},
Volume = {{476}},
Number = {{2}},
Pages = {{2230-2237}},
Month = {{MAY}},
Abstract = {{We report on two new quiescent XMM-Newton observations (in addition to
   the earlier Swift/XRT and XMM-Newton coverage) of the cooling neutron
   star crust in the low-mass X-ray binary 1RXS J180408.9-342058. Its crust
   was heated during the similar to 4.5 month accretion outburst of the
   source. From our quiescent observations, fitting the spectra with a
   neutron star atmosphere model, we found that the crust had cooled from
   similar to 100 to similar to 73 eV from similar to 8 to similar to 479 d
   after the end of its outburst. However, during the most recent
   observation, taken similar to 860 d after the end of the outburst, we
   found that the crust appeared not to have cooled further. This suggested
   that the crust had returned to thermal equilibrium with the neutron star
   core. We model the quiescent thermal evolution with the theoretical
   crustal cooling code NSCool and find that the source requires a shallow
   heat source, in addition to the standard deep crustal heating processes,
   contributing similar to 0.9 MeV per accreted nucleon during outburst to
   explain its observed temperature decay. Our high quality XMM-Newton data
   required an additional hard component to adequately fit the spectra.
   This slightly complicates our interpretation of the quiescent data of
   1RXS J180408.9-342058. The origin of this component is not fully
   understood.}},
