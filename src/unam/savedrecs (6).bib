@article{ ISI:000434093100006,
Author = {Wellenreuther, Maren and Munoz, Jesus and Chavez-Rios, Jesus R. and
   Hansson, Bengt and Cordero-Rivera, Adolfo and Sanchez-Guillen, Rosa A.},
Title = {{Molecular and ecological signatures of an expanding hybrid zone}},
