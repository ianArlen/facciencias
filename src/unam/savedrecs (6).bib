@article{ ISI:000441503700011,
Author = {Garciamarin Hernandez, Hugo Antonio},
Title = {{Populism in the 20th Century: A Comparative Analysis between Asia and
   Latin America (Thailand, South Korea, Venezuela, and Bolivia)}},
Journal = {{REVISTA MEXICANA DE CIENCIAS POLITICAS Y SOCIALES}},
Year = {{2018}},
Volume = {{63}},
Number = {{233}},
Pages = {{255-283}},
Month = {{MAY-AUG}},
Abstract = {{Although populism has been widely studied by Political Science, there
   are few comparative studies, besides those related to Europe and Latin
   America. Thus, little progress has been made in the analysis of less
   explored contexts such as those in Asia, and in their contrast with more
   studied populisms, such as those in Latin America. Hence, this paper
   compares populisms from Asia and Latin America based on four analytic
   dimensions: symbolic, political, ideological, and impact on the quality
   of democracy. Our findings reveal that, although both populisms are
   symbolically inclusive, their political repercussions, their ideological
   expression, and their democratic results are diverse.}},
