@article{ ISI:000428216300016,
Author = {Antonio de la Torre, J. and Gonzalez-Maya, Jose F. and Zarza, Heliot and
   Ceballos, Gerardo and Medellin, Rodrigo A.},
Title = {{The jaguar's spots are darker than they appear: assessing the global
   conservation status of the jaguar \&ITPanthera onca\&IT}},
