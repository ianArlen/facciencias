@article{ ISI:000441667100007,
Author = {Geiss, Christof and Leclerc, Bernard and Schroeer, Jan},
Title = {{Quivers with Relations for Symmetrizable Cartan Matrices II: Change of
   Symmetrizers}},
Journal = {{INTERNATIONAL MATHEMATICS RESEARCH NOTICES}},
Year = {{2018}},
Volume = {{2018}},
Number = {{9}},
Pages = {{2866-2898}},
Month = {{MAY}},
Abstract = {{For k >= 1, we consider the K-algebra H(k) := H(C, kD, Omega) associated
   to a symmetrizable Cartan matrix C, a symmetrizer D and an orientation
   Omega of C, which was defined in Geiss et al. {[}8]. We construct and
   analyze a reduction functor from rep(H(k)) to rep(H(k - 1)). As a
   consequence, we show that the canonical decomposition of rank vectors
   for H(k) does not depend on k, and that the rigid locally free
   H(k)-modules are up to isomorphism in bijection with the rigid locally
   free H(k - 1)-modules. Finally, we show that for a rigid locally free
   H(k)-module of a given rank vector the Euler characteristic of the
   variety of flags of locally free submodules with fixed ranks of the
   subfactors does not depend on the choice of k.}},
