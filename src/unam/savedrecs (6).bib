@article{ ISI:000457029900004,
Author = {Lee-Cortes, V, Jimena and Delgadillo-Macias, Javier},
Title = {{TERRITORIAL POTENTIAL AS FACTOR OF DEVELOPMENT: RURAL MANAGEMENT MODEL}},
