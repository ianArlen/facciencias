@article{ ISI:000430452300063,
Author = {Mejia-Lopez, M. and Verea, L. and Verde, A. and Lara, B. and Campos, I.
   and Najera, M. C. and Sebastian, P. J.},
Title = {{Improvement of the Carbon Electrode Treatment to Obtain Bioanodes for
   Microbial Electrolysis Cell (MEC)}},
Journal = {{INTERNATIONAL JOURNAL OF ELECTROCHEMICAL SCIENCE}},
Year = {{2018}},
Volume = {{13}},
