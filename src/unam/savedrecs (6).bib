@article{ ISI:000430798100007,
Author = {Eberhart-Phillips, Luke J. and Kuepper, Clemens and Carmona-Isunza,
   Maria Cristina and Vincze, Orsolya and Zefania, Sama and Cruz-Lopez,
   Medardo and Kosztolanyi, Andras and Miller, Tom E. X. and Barta, Zoltan
   and Cuthill, Innes C. and Burke, Terry and Szekely, Tamas and Hoffman,
   Joseph I. and Krueger, Oliver},
Title = {{Demographic causes of adult sex ratio variation and their consequences
   for parental cooperation}},
Journal = {{NATURE COMMUNICATIONS}},
Year = {{2018}},
Volume = {{9}},
Month = {{APR 25}},
Abstract = {{The adult sex ratio (ASR) is a fundamental concept in population
   biology, sexual selection, and social evolution. However, it remains
   unclear which demographic processes generate ASR variation and how
   biases in ASR in turn affect social behaviour. Here, we evaluate the
   demographic mechanisms shaping ASR and their potential consequences for
   parental cooperation using detailed survival, fecundity, and behavioural
   data on 6119 individuals from six wild shorebird populations exhibiting
   flexible parental strategies. We show that these closely related
   populations express strikingly different ASRs, despite having similar
   ecologies and life histories, and that ASR variation is largely driven
   by sex differences in the apparent survival of juveniles. Furthermore,
   families in populations with biased ASRs were predominantly tended by a
   single parent, suggesting that parental cooperation breaks down with
   unbalanced sex ratios. Taken together, our results indicate that sex
   biases emerging during early life have profound consequences for social
   behaviour.}},
