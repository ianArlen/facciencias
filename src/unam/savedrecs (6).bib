@article{ ISI:000427141900012,
Author = {Tapia, M. and Persi, P. and Roman-Zuniga, C. and Elia, D. and
   Giovannelli, F. and Sabau-Graziati, L.},
Title = {{The star-forming cores in the centre of the Trifid nebula (M 20): from
   Herschel to the near-infrared}},
