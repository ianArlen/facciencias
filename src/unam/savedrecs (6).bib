@article{ ISI:000430996700050,
Author = {Lopez-Mayorga, B. and Sandoval-Chavez, C. I. and Carreon-Castro, P. and
   Ugalde-Saldivar, V. M. and Cortes-Guzman, F. and Lopez-Cortes, J. G. and
   Ortega-Alfaro, M. C.},
Title = {{Ferrocene amphiphilic D-pi-A dyes: synthesis, redox behavior and
   determination of band gaps}},
Journal = {{NEW JOURNAL OF CHEMISTRY}},
Year = {{2018}},
Volume = {{42}},
Number = {{8}},
Pages = {{6101-6113}},
Month = {{APR 21}},
Abstract = {{We report the synthesis of a series of ferrocene amphiphilic
   donor-pi-acceptor dyes, with the general formula
   (Fc-CHQCH-HetNC(16)H(33))(+) X- {[}where: Fc behaves as the donor group,
   a double bond as the pi bridge, and 2-,4-pyridinium and 4-quinolinium as
   the potent acceptor groups (2a-b and 4, X = Br- or BF4-)], in good
   overall yields. Together with their neutral counterparts (6a-b and 7),
   the photophysical and electrochemical properties of these compounds were
   investigated by means of UV-Vis spectroscopy and cyclic voltammetry. The
   optical and electrochemical band gaps of these dyes were calculated,
   which indicated that 4 has the lowest bandgap value. Time-dependent DFT
