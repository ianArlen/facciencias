@article{ ISI:000431206800004,
Author = {Manriquez-Bucio, Yurixhi and Garibay Orozco, Claudio and Urquijo Torres,
   Pedro S.},
Title = {{Resistance to mining-energy projects: from local opposition to
   regionalization in the Sierra Norte de Puebla, Mexico}},
Journal = {{JOURNAL OF LATIN AMERICAN GEOGRAPHY}},
Year = {{2018}},
Volume = {{17}},
Number = {{1}},
Pages = {{59-85}},
Month = {{APR}},
Abstract = {{The mining sector in Mexico has undergone a significant resurgence,
   triggering increased numbers of associated conflicts. This trend has
   characterized conflict in the Sierra Norte region of the state of Puebla
   since 2011. Between 2011 and 2015, three mining exploration projects and
   mining energy project by private corporations have been initiated in the
   region, as well as a government population - relocation project. The
   situation in which mining becomes an activity with significant potential
   to reconfigure territories has been described as the accumulation of
   mining capital by dispossession. In the case of the Sierra Norte de
   Puebla, we observe that the conflicts are emerging as a response to the
   systemic, subjective, and symbolic violence employed in the process of
   territorial dispossession. This case also allows us to observe: (1)
   local cases of resistance characterized by heterogeneous actors,
   practices and arguments; and (2) their rescaling toward a regional
   discursive coalition, transcending heterogeneity and creating a form to
   face up to what has been known as ``death projects.{''}}},
