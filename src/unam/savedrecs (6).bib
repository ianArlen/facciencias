@article{ ISI:000432489800006,
Author = {Queuedo, Marcela C. and Galicia, Gonzalo and Mayen-Mondragon, Rodrigo
   and Genesca Llongueras, Juan},
Title = {{Role of turbulent flow seawater in the corrosion enhancement of an
   Al-Zn-Mg alloy: an electrochemical impedance spectroscopy (EIS) analysis
   of oxygen reduction reaction (ORR)}},
Journal = {{JOURNAL OF MATERIALS RESEARCH AND TECHNOLOGY-JMR\&T}},
Year = {{2018}},
Volume = {{7}},
Number = {{2}},
Pages = {{149-157}},
Month = {{APR-JUN}},
Abstract = {{The effect of flow on the corrosion of Al-14 wt\% Zn-8 wt\% Mg alloy in
   aerated synthetic seawater at ambient temperature was studied using a
   rotating cylinder electrode (RCE) under turbulent regime conditions by
   means of electrochemical impedance spectroscopy (EIS). The overall
   electrochemical corrosion process was found to be strongly influenced by
   the oxygen mass transfer process under turbulent flow conditions on the
   cathodic kinetics, driving to a significant increase in corrosion rate.
   At corrosion potential, E. value, contributions from the anodic and
   cathodic processes involved were observed in the impedance diagrams.
   Instead, at a cathodic potential of -1.2 V (sce), impedance measurements
   proved the predominance of the mass-transfer process for oxygen. A
   primary analysis of the impedance plots allowed to confirm such
   situation. (C) 2017 Brazilian Metallurgical, Materials and Mining
   Association. Published by Elsevier Editora Ltda. This is an open access
   article under the CC BY-NC-ND license.}},
