@article{ ISI:000428419500021,
Author = {Ayala, Yaneri and Malmierca, Manuel S.},
Title = {{The effect of inhibition on stimulus-specific adaptation in the inferior
   colliculus}},
Journal = {{BRAIN STRUCTURE \& FUNCTION}},
