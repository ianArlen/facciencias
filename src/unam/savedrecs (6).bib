@article{ ISI:000441173100268,
Author = {Felix, Angelica and Mendoza, Edgar and Chavez, Valeria and Silva,
   Rodolfo and Rivillas-Ospina, German},
Title = {{Wave and wind energy potential including extreme events: A case study of
   Mexico}},
Journal = {{JOURNAL OF COASTAL RESEARCH}},
Year = {{2018}},
Number = {{85}},
Pages = {{1336-1340}},
Month = {{MAY}},
Note = {{15th International Coastal Symposium (ICS), Busan, SOUTH KOREA, MAY
   13-18, 2018}},
Organization = {{Korea Inst Ocean Sci \& Technol; Korea Soc Coastal Disaster Prevent;
   JCR; Coastal Educ \& Res Fdn}},
Abstract = {{The available wind and wave energy potential of the coasts of Mexico was
   found to be low in several analyses. However, these analyses considered
   only average conditions. To adequately assess the energy potential of
   Mexican coasts for energy extraction, it is necessary to characterize
   maritime weather and to define both normal and storm thresholds. For the
   present study, a 60-year historical analysis of wave and wind conditions
   along the Mexican coasts was performed. A statistical analysis of
   extreme conditions was carried out using the peaks-over-threshold
   approach. Also, seasonal (winter, spring, summer and autumn) and hourly
   available power were included in the analysis. The differences between
   the energy potentials when including and excluding extreme events are
   considerable. The energy potential of events occurring along the Mexican
   coasts confirms that energy can be adequately and profitably obtained
   from waves and wind. The greatest energy potential in the Atlantic is
   found to the northeast of the Yucatan Peninsula and in the Pacific along
   the Baja California Peninsula. Winter is the best season for energy
   extraction, while summer is the most unfavorable season.}},
