@article{ ISI:000430472900013,
Author = {de Jesus Ruiz-Baltazar, Alvaro},
Title = {{Green Composite Based on Silver Nanoparticles Supported on Diatomaceous
   Earth: Kinetic Adsorption Models and Antibacterial Effect}},
Journal = {{JOURNAL OF CLUSTER SCIENCE}},
