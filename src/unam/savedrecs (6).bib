@article{ ISI:000428771900005,
Author = {Flores-Mendoza, Giovanna and Sanson, Stephanie P. and Rodriguez-Castro,
   Santiago and Crispin, Jose C. and Rosetti, Florencia},
Title = {{Mechanisms of Tissue Injury in Lupus Nephritis}},
Journal = {{TRENDS IN MOLECULAR MEDICINE}},
Year = {{2018}},
Volume = {{24}},
Number = {{4}},
Pages = {{364-378}},
Month = {{APR}},
Abstract = {{Disease heterogeneity remains a major challenge for the understanding of
   systemic lupus erythematosus (SLE). Recent work has revealed the
   important role of nonimmune factors in the development of end-organ
   damage involvement, shifting the current paradigm that views SLE as a
   disease inflicted by a disturbed immune system on passive target organs.
   Here, we discuss the pathogenesis of lupus nephritis in a comprehensive
   manner, by incorporating the role that target organs play by
   withstanding and modulating the local inflammatory response. Moreover,
   we consider the effects that genetic variants exert on immune and
   nonimmune cells in order to shape the phenotype of the disease in each
   affected individual.}},
