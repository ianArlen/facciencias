@article{ ISI:000431900700001,
Author = {Golasinski, Marek and Goncalves, Daciberg Lima and Jimenez, Rolando},
Title = {{FREE AND PROPERLY DISCONTINUOUS ACTIONS OF GROUPS ON HOMOTOPY 2N-SPHERES}},
Journal = {{PROCEEDINGS OF THE EDINBURGH MATHEMATICAL SOCIETY}},
Year = {{2018}},
Volume = {{61}},
Number = {{2}},
Pages = {{305-327}},
Month = {{MAY}},
Abstract = {{Let G be a group acting freely, properly discontinuously and cellularly
   on some finite dimensional CW-complex Sigma(2n) which has the homotopy
   type of the 2n-sphere S-2n. Then, that action induces a homomorphism G
   -> Aut(H-2n(Sigma(2n))). We classify all pairs (G, (sic)), where G is a
   virtually cyclic group and (sic) : G -> Aut(Z) is a homomorphism, which
   are realizable in the way above and the homotopy types of all possible
   orbit spaces as well. Next, we consider the family of all groups which
   have virtual cohomological dimension one and which act on some
   Sigma(2n). Those groups consist of free groups and semi-direct products
   F x Z(2) with F a free group. For a group G from the family above and a
   homomorphism phi : G -> Aut( Z), we present an algebraic criterion
   equivalent to the realizability of the pair (G, (sic)). It turns out
   that any realizable pair can be realized on some Sigma(2n) with dim
   Sigma(2n) <= 2n + 1.}},
Publisher = {{CAMBRIDGE UNIV PRESS}},
Address = {{32 AVENUE OF THE AMERICAS, NEW YORK, NY 10013-2473 USA}},
Type = {{Article}},
Language = {{English}},
