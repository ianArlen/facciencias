@article{ ISI:000429732200001,
Author = {Ramirez-Reyes, Tonatiuh and Flores-Villela, Oscar},
Title = {{Taxonomic changes and description of two new species for the
   Phyllodactylus lanei complex (Gekkota: Phyllodactylidae) in Mexico}},
