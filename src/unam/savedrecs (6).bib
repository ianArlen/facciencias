@article{ ISI:000437444000011,
Author = {Demidova, Yu. S. and Simakov, A. V. and Simakova, I. L. and Murzin, D.
   Yu.},
Title = {{Hydrogenation of (-)-Carvone in Presence of Gold Catalysts: Role of the
   Support}},
