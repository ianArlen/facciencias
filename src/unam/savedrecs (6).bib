@article{ ISI:000427160600049,
Author = {Delgado-Enciso, Ivan and Lopez-Lemus, Uriel A. and Valcarcel-Gamino,
   Jose A. and Rodriguez-Sanchez, Iram P. and Valle-Reyes, Salvador and
   Martinez-Fierro, Margarita L. and Melnikov, Valery and Guzman-Esquivel,
