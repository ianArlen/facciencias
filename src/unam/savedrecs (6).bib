@article{ ISI:000430185900015,
Author = {Abril Martinez-Velasco, Maria and Perper, Marina and Maddy, Austin J.
   and Cervantes, Jessica and Eber, Ariel E. and Verne, Sebastian H. and
   Elizabeth Vazquez-Herrera, Norma and Nouri, Keyvan and Tosti, Antonella},
Title = {{In vitro determination of Mexican Mestizo hair shaft diameter using
   optical coherence tomography}},
Journal = {{SKIN RESEARCH AND TECHNOLOGY}},
Year = {{2018}},
Volume = {{24}},
Number = {{2}},
Pages = {{274-277}},
Month = {{MAY}},
Abstract = {{Background: Mexican mestizo population has a pluriethnic mixture of
   Amerindian, European and African ancestry, predominant in most Latin
   American countries. Until now, there are no reports about hair
   characteristics in this population, necessary to define normal values,
   for hair diseases evaluation and comparison among other ethnic groups.
   Methods: The VivoSight((R)) swept-source multibeam optical coherence
   tomography system was used to evaluate hair diameter and shape in 30
   females. Three hair samples from each volunteer were measured
   transversely along three distances, generating nine cross-sectional
   images, two measurements per image and 18 measurements per patient for a
   total of 540 diameter measurements.
   Results: Minimum hair diameter (n=540) was 0.06mm while maximum was
   0.14mm. Mean diameter was 0.100.01mm as compared with Asian hair, which
   ranges from 0.08mm to 0.12mm. On morphological analysis, Mexican Mestizo
   hair tends to have a round shape with homogenous diameters, resembling
   Caucasian and Asian hair.
   Conclusion: Mexican hair is similar to Asian hair in diameter and shape
   and can be classified as ``thick' hair, which make it more resistant and
   with more volume. Cosmetic products intended to improve hair care in
   this population must to consider this characteristic.}},
