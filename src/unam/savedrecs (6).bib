@article{ ISI:000430612800022,
Author = {Avendano, Carlos G. and Palomares, Laura O.},
Title = {{Omnidirectional narrow optical filters for circularly polarized light in
   a nanocomposite structurally chiral medium}},
Journal = {{APPLIED OPTICS}},
Year = {{2018}},
Volume = {{57}},
Number = {{12}},
Pages = {{3119-3125}},
Month = {{APR 20}},
Abstract = {{We consider the propagation of electromagnetic waves throughout a
   nanocomposite structurally chiral medium consisting of metallic
   nanoballs randomly dispersed in a structurally chiral material whose
   dielectric properties can be represented by a resonant effective
   uniaxial tensor. It is found that an omnidirectional narrow pass band
   and two omnidirectional narrow band gaps are created in the blue optical
   spectrum for right and left circularly polarized light, as well as
   narrow reflection bands for right circularly polarized light that can be
   controlled by varying the light incidence angle and the filling fraction
   of metallic inclusions. (C) 2018 Optical Society of America.}},
