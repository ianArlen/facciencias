@article{ ISI:000430465600003,
Author = {Gomez, Samuel},
Title = {{New species of the genus \&ITMesocletodes\&IT Sars, 1909 from the deep
   Gulf of California (Copepoda, Harpacticoida)}},
Journal = {{ZOOKEYS}},
Year = {{2018}},
