@article{ ISI:000444653900004,
Author = {Vasco-Leal, J. F. and Mosquera-Artamonov, J. D. and Hernandez-Rios, T.
   and Mendez-Gallegos, S. J. and Perea-Flores, M. J. and Pena-Aguilar, J.
   M. and Rodriguez-Garcia, M. E.},
Title = {{Physicochemical characteristics of seeds from wild and cultivated castor
   bean plants (Ricinus communis L.)}},
Journal = {{INGENIERIA E INVESTIGACION}},
Year = {{2018}},
Volume = {{38}},
Number = {{1}},
Pages = {{24-30}},
Month = {{APR}},
Abstract = {{The castor (Ricinus communis L.) is an oilseed plant whose main features
   are its drought resistance, and its adaptation to eroded, polluted, and
   low fertility soils. Its oil has a great demand in the industrial sector
   and it has recently attracted considerable interest for its use in the
   production of biodiesel and jet fuel. In this study, morphological,
   physical and chemical characterizations were performed to ascertain the
   quality of wild (VQ-1) and under cultivation (VQ-7) oil castor seeds.
   The results showed that there are differences in the morphological and
   physicochemical characteristics regarding oil content (44,95 vs
   33,84\%), ash (3,20 vs 2,42\%), and 100-seed-weight (45,87 vs 54,23g);
   similar behavior was recorded when characterizing the oil: kinematic
   viscosity (269,67 vs 266,44mm(2)/s), density (0,9389 vs 0,9465g/cm(3)),
   and acidity index (0,9918 vs 0,5440 mg KOH/g) for VQ-1 and VQ-7,
   respectively. Growing conditions to which castor plants were subjected
   may influence both the final quality of seeds and chemical properties of
   the oil.}},
