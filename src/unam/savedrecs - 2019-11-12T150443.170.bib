@article{ ISI:000226814200005,
Author = {Medina, L and Moreno, E},
Title = {{Ultrasonic imaging system using a 2D envelope beamforming technique:
   In-homogeneities location}},
Journal = {{JOURNAL OF COMPUTATIONAL ACOUSTICS}},
