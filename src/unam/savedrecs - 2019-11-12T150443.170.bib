@article{ ISI:000226097500010,
Author = {Aguayo, JMB and Palos, PA},
Title = {{A predictive model of residential water conservation}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2004}},
Volume = {{21}},
Number = {{2}},
Pages = {{215-226}},
Month = {{DEC}},
Abstract = {{A sample of 349 women living in Mexico City were asked to participate in
   a survey including the assessment of water conservation motives,
   perception of risks for health, beliefs in the obligation to conserve
   water, internal locus of control, skills, knowledge, and water
   conservation behaviors. A model was specified and tested using path
   analysis, which showed goodness of fit in three of four measures(X-2 =
   40.49, df = 18, p = 0.002; NFI = .99; NNFI = 0.99; RMSEA =.06). Skills
   predicted water conservation ((a) over cap = .31); also, individuals
   with an internal locus of control tended to hold higher levels of belief
   in the obligation to conserve water ((a) over cap = .38). Model
   structure and the relative importance of each variable were discussed in
   terms of previous findings about water conservation.}},
