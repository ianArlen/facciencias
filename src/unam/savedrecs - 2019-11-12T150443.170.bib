@article{ ISI:000226097500004,
Author = {Bruner, CA and Acuna, L},
Title = {{The influence of the enemies of science on the undergraduate students of
   UNAM}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2004}},
Volume = {{21}},
Number = {{2}},
Pages = {{143-156}},
Month = {{DEC}},
Abstract = {{Undergraduate students of 17 different disciplines taught at the main
   campus of the National Autonomous University of Mexico (Ciudad
   Universitaria, UNAM) expressed their adherence to 30 statements either
   in favor or against natural science. The global mean score indicated a
   moderately positive opinion of science. Differences by sex, social class
   and discipline of study were found, the later being the most important
   source of variation in the data. The students of physics, chemistry and
   biology agreed with a more positive view of science than the students of
   the other 14 disciplines, who did not differ from each other. Although
   the mean score from the psychology sample was not reliably different
   from the other ``soft{''} disciplines, the results showed that at least
   marginally, the students of psychology expressed a less favorable view
   of science than the students of any other discipline taught in the
   University City campus of UNAM. These results were interpreted as the
   result of the influence of the New Academic Left within UNAM and the
   consequences of this pseudo-intellectual movement were discussed.}},
