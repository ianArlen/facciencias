@article{ ISI:000227459400089,
Author = {Ledesma-Aguilar, R and Quevedo-Reyes, M and Poire, EC and
   Hernandez-Machado, A},
Title = {{Lateral instability in normal viscous fingers}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2005}},
Volume = {{71}},
Number = {{1, 2}},
Month = {{JAN}},
Abstract = {{We study a low-amplitude, long-wavelength lateral instability of the
   Saffman-Taylor finger by means of a phase-field model. We observe such
