@article{ ISI:000227047900003,
Author = {Arana-Magallon, EC and Ortega-Salas, AA},
Title = {{Growth of postlarval Macrobrachium rosenbergii at two temperatures}},
Journal = {{NORTH AMERICAN JOURNAL OF AQUACULTURE}},
Year = {{2005}},
Volume = {{67}},
Number = {{1}},
Pages = {{10-12}},
Month = {{JAN}},
Abstract = {{Postlarvae of the giant Malaysian prawn Macrobrachium rosenbergii were
   reared for a 120-d period under laboratory conditions at 20degreesC and
   33degreesC. We stocked 480 50-d-old postlarvae in eight 70-L aquaria. A
   balanced commercial feed containing 40\% protein was supplied daily at
   20\% to 9\% of body weight during the first 2 months and at 5\% to 3\%
   of body weight during the final 2 months. Live brine shrimp Artemia
   francis-cana were also provided as a supplemental food. The growth of
   the giant Malaysian prawn was significantly greater at 33degreesC than
   at 20degreesC. Basic knowledge of giant Malaysian prawn rearing
   requirements under artificial conditions is important for the production
   of juveniles as stocks for large-scale prawn culture.}},
