@article{ ISI:000225692300008,
Author = {Albiter, A and Espinosa-Medina, MA and Casales, M and
   Gonzalez-Rodriguez, JG},
Title = {{Corrosion performance of NiAl intermetallic with Mo, Ga and Fe in
   neutral and alkaline media}},
