@article{ ISI:000226541900012,
Author = {Martinez-Sanchez, E and Garcia-Hipolito, M and Guzman, J and
   Ramos-Brito, F and Santoyo-Salazar, J and Martinez-Martinez, R and
   Alvarez-Fregoso, O and Ramos-Cortes, MI and Mendez-Delgado, JJ and
   Falcony, C},
Title = {{Cathodoluminescent characteristics of sm-doped ZnAl2O4 nanostructured
   powders}},
