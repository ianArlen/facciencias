@inproceedings{ ISI:000235191500012,
Author = {Basiuk, VA and Montiel, HM},
Editor = {{Bernstein, M and NavarroGonzalez, R and Raulin, R}},
Title = {{Glycine amide hydrolysis with water and OH radical: a comparative DFT
   study}},
Booktitle = {{SPACE LIFE SCIENCES: ASTROBIOLOGY: STEPS TOWARD ORIGIN OF LIFE AND TITAN
   BEFORE CASSINI}},
