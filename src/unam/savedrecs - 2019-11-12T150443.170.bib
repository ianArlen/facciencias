@inproceedings{ ISI:000234674000073,
Author = {Pena, M and Stasinska, G and Gorny, SK},
Editor = {{Szczerba, R and Stasinska, G and Gorny, SK}},
Title = {{Using planetary nebulae to analyze the abundance gradient in the Milky
   Way}},
Booktitle = {{PLANETARY NEBULAE AS ASTRONOMICAL TOOLS}},
Series = {{AIP Conference Proceedings}},
Year = {{2005}},
Volume = {{804}},
Pages = {{243-245}},
Note = {{International Conference on Planetary Nebulae as Astronomical Tools,
   Gdansk, POLAND, JUN 28-JUL 02, 2005}},
Organization = {{LEA Astronome Pologne-France; Polish Acad Sci, Nicolaus Copernicus
   Astron Ctr; Lab Univers Theories, Observat Paris; Polish State Comm Sci
   Res; City Gdansk; Gdansk Univ Technol; Acad Comp Ctr TASK; TeleAdreson}},
Abstract = {{This is a progress report on a project aiming at reexamining the
   abundance gradients in the Milky Way as derived from planetary nebulae
   (PNe). Our first efforts were to constitute an adequate sample. We have
   observed over 100 PNe using low- and high resolution spectroscopy. We
   have combined this data with large, homogeneous and good quality data
   samples from the literature. We have derived the abundances of all the
   PNe of the resulting sample in a consistent way. The determination of
   abundance gradients strongly depends on the assumed PN distances, while
   PN distances in the Milky Way are in general not well-known. We have
   devised a method to obtain PN distances, which is based on interpolation
   in a grid of photoionization models. Our distances do not depend on
   assumptions on a nebular mass or any other nebular or stellar property
   that is supposed to hold for all PNe. Error bars can be estimated in a
   realistic way. After several checks of our method to derive distances,
   we will proceed to the study of abundance gradients and their possible
   variation in time.}},
