Title = {{Friction layer along the sides of the plasma channels in the Venus
   nightside ionosphere}},
Booktitle = {{PLANETARY ATMOSPHERES, IONOSPHERES, AND MAGNETOSPHERES}},
Series = {{ADVANCES IN SPACE RESEARCH-SERIES}},
