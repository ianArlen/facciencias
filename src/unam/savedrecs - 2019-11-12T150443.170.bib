@article{ ISI:000225737900011,
Author = {Estrada, S and Lopez-Guerrero, JJ and Villalobos-Molina, R and Mata, R},
Title = {{Spasmolytic stilbenoids from Maxillaria densa}},
Journal = {{FITOTERAPIA}},
Year = {{2004}},
Volume = {{75}},
Number = {{7-8}},
Pages = {{690-695}},
Month = {{DEC}},
Abstract = {{2,5-Dihydroxy-3,4-diTnethoxyphenanthrene (1), fimbriol-A (2), nudol (3),
   gymnopusin (4) and erianthridin (5) isolated from Maxillaria densa
   provoked a concentration-dependent inhibition of the spontaneous
   contractions of the rat ileum with potencies comparable to papaverine.
   In order to establish the mode of action of stilbenoids 1-5, their
   effect on the contractions induced by different spasmogens (histamine,
   barium chloride and L-NAME) was investigated. In general, the results
   suggested that the relaxant activity of the products does not involve a
   direct nitrergic or antihistaminergic mode of action or an interference
   with calcium influx into the smooth muscle cells. (C) 2004 Elsevier B.V.
   All rights reserved.}},
