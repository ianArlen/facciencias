Author = {Cedeno, L and Zanella, R and Ramirez, J and Mendoza, H and Hernandez, G
   and Schacht, P},
Title = {{Synthesis and characterization of Ce-stabilized titania for NiMoHDS
   catalysts}},
Journal = {{CATALYSIS TODAY}},
