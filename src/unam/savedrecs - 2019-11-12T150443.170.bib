@article{ ISI:000226299200059,
Author = {Leppanen, T and Karttunen, M and Barrio, RA and Kaski, K},
Title = {{Morphological transitions and bistability in Turing systems}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2004}},
Volume = {{70}},
Number = {{6, 2}},
Month = {{DEC}},
Abstract = {{It is well known that in two dimensions Turing systems produce spots,
   stripes and labyrinthine patterns, and in three dimensions lamellar and
   spherical structures, or their combinations, are observed. In this paper
   we study transitions between these states in both two and three
   dimensions. First, we derive the regions of stability for different
   patterns using nonlinear bifurcation analysis. Then, we apply large
   scale computer simulations to analyze the pattern selection in a
   bistable system by studying the effect of parameter selection on
   morphological clustering and the appearance of topological defects. The
   method elaborated in this paper presents a probabilistic approach for
   studying pattern selection in a bistable reaction-diffusion system.}},
