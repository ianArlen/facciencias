@article{ ISI:000226951300019,
Author = {Gordon, JE and Gonzalez, MA and Hernandez, JV and Lavariega, RO and
   Reyes-Garcia, A},
Title = {{Guaiacum coulteri: an over-logged dry forest tree of Oaxaca, Mexico}},
Journal = {{ORYX}},
Year = {{2005}},
Volume = {{39}},
Number = {{1}},
Pages = {{82-85}},
Month = {{JAN}},
Abstract = {{Logging history and population structure of the dry forest tree species
   Guaiacum coulteri are described for Oaxaca, Mexico. We propose that the
   species has been adversely affected by selective logging, with large
   individuals now being uncommon. We argue that the species is likely to
   have undergone similar changes elsewhere in its range and, following
   IUCN guidelines, propose that it is categorized as Endangered.
   Maintenance of G. cotilteri as an understorey shrub may be possible
   given current land use practices in the area but recovery of populations
   of large individuals may be more difficult. Suggestions for future
   research are made.}},
