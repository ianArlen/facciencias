@inproceedings{ ISI:000235501900023,
Author = {Mochan, WL},
Editor = {{Stutzmann, M}},
Title = {{SHG from bulk and surface of nanoparticle composites}},
Booktitle = {{Physica Status Solidi C - Conferences and Critical Reviews, Vol 2 , No
   12}},
