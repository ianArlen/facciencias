@article{ ISI:000226097500003,
Author = {Figueiras, SC},
Title = {{Exploratory study of the psychology major student's formation in Mexico}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2004}},
Volume = {{21}},
Number = {{2}},
Pages = {{131-141}},
Month = {{DEC}},
Abstract = {{Even though in Mexico the Psychology Major student's formation is
   sustained in highly appreciated and commonly shared foundations,
   teachers and professionals identify gaps between the expected and the
   actuality of the generated offer. In this exploratory study
   relationships that explain effects of the basic over the specific
   formation in three professional application areas were identified, as
   well as the effects of these over the achievement in a graduating
   general exam. Data from 1245 undergraduates of 15 states showed that
   basic formation didn't explain higher and tower achievement in the
   areas, but that formation within them explains two achievement levels in
   the global scoring of the exam. The best predictor in low achievement
   was theoretical knowledge while that of the high achievement was the
   integration of the theoretical associated with the contextual and
   technical. Mechanisms for the improvement are discussed.}},
