@article{ ISI:000225754200028,
Author = {Bader, RFW and Matta, CF},
Title = {{Where to draw the line in defining a molecular structure}},
Journal = {{ORGANOMETALLICS}},
Year = {{2004}},
Volume = {{23}},
Number = {{26}},
Pages = {{6253-6263}},
Month = {{DEC 20}},
Abstract = {{It is the purpose of this paper to demonstrate how the theory of atoms
   in molecules (AIM) can be used to complement molecular orbital models to
   obtain an increased understanding of molecular structure and reactivity.
   The oxidative addition reaction of a hydrosilane with a Mn
   cyclopentadienyl dicarbonyl fragment, a common constituent of a large
   class of organometallic complexes, provides an ideal vehicle for
