@inproceedings{ ISI:000227350800120,
Author = {Tapia, M and Persi, P and Roth, M and Bohigas, J and Gomez, M},
Editor = {{Wilson, A}},
Title = {{The photodissociation region car I: Optical and near-infrared imaging}},
Booktitle = {{PROCEEDINGS OF THE DUSTY AND MOLECULAR UNIVERSE: A PRELUDE TO HERSCHEL
   AND ALMA}},
Series = {{ESA Special Publications}},
Year = {{2005}},
