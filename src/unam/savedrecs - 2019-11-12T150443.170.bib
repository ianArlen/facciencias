@article{ ISI:000225781900010,
Author = {Avendano, C and Reyes, JA and del Castillo-Mussot, M and Vazquez, GJ and
   Spector, H},
Title = {{1D stark effect with an electric field confined in a finite region}},
Journal = {{CHINESE JOURNAL OF PHYSICS}},
Year = {{2004}},
Volume = {{42}},
Number = {{6}},
Pages = {{751-763}},
Month = {{DEC}},
Abstract = {{We study the electronic properties of a strictly ID quantum system,
   formed by two particles interacting through a Coulomb attractive
   potential, under a uniform electric field E applied over a spatially
   confined region of thickness 2a which satisfies -a < x < a. From the
   corresponding Schrodinger equation we derive asymptotic expressions to
   represent the eigenfunctions in the regions where E does not exist
   (\textbackslash{}x\textbackslash{} greater than or equal to a), and then
   we find numerically the solution in the region -a < x < a by using the
   shooting method. We use the WKB method to find the critical or
   ionization field above which there are no bound states.}},
