@inproceedings{ ISI:000236762900027,
Author = {Pena, M},
Book-Group-Author = {{UNAM}},
Title = {{HuDo 1 and HuBi 1: Two planetary nebulae ionized by cool {[}WC] central
   stars}},
Booktitle = {{NINTH TEXAS-MEXICO CONFERENCE ON ASTROPHYSICS}},
Series = {{Revista Mexicana de Astronomia y Astrofisica-Serie de Conferencias}},
Year = {{2005}},
Volume = {{23}},
Pages = {{120-121}},
Note = {{9th Texas-Mexico Conference on Astrophysics, San Antonio, TX, APR 13-16,
   2005}},
Organization = {{Trinity Univ; UNAM, Inst Astronomia; Univ Texas Austin; Rice Univ}},
Abstract = {{As part of our spectroscopic survey of planetary nebulae with {[}WC]
   nuclei (Pefia et al. 2001), low- and high-resolution spectra of the
   planetary nebulae HuDo1 (PN G060.4+01.5, PM1-310) and HuBi1 (PN
   G012.2+04.9, PM1-188) were secured and analyzed. Both objects are
   ionized by very late {[}WC] central stars. We found that the objects
   belong to the galactic disk, with heliocentric radial velocities of -12
   km s(-1) (HuDo1) and 57 km s(-1) (HuBil). Both objects are heavily
   extinguished showing a logarithmic reddening, c(H beta), of 2.04 for
   HuDo1 and 1.22 for HuBi1. Our data cover a wide wavelength range;
   therefore we obtained several plasma line ratios to estimate physical
   conditions and abundances. The derived electron temperature and density
   for HuBi1 are 9,400 +/- 1,500 K and 800 cm(-3). This density is very low
   for a nebula around a {[}WC]-late star. HuDo1 has N(e) = 3300 cm(-3). We
   find log(O/H)+12 = 8.43 and 8.57, and N/O = 0.2 and 0.1 for HuDo1 and
   HuBi1 respectively, typical of disk PNe. Intense nebular He I
   recombination lines are detected for HuBi1, this being the only PN
   excited by a very late {[}WC] star showing such an emission. The He(+)
   abundance derived for HuBi1 is 0.11, which is indicating a large He
   enhancement in HuBi1.}},
