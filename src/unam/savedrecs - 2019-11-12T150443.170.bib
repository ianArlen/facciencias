@inproceedings{ ISI:000236762900006,
Author = {Gazol, A and Vazquez-Semadeni, E and Kim, J},
Book-Group-Author = {{UNAM}},
Title = {{The thermal pressure distribution in the atomic ISM}},
Booktitle = {{NINTH TEXAS-MEXICO CONFERENCE ON ASTROPHYSICS}},
Series = {{Revista Mexicana de Astronomia y Astrofisica-Serie de Conferencias}},
Year = {{2005}},
Volume = {{23}},
Pages = {{23-26}},
Note = {{9th Texas-Mexico Conference on Astrophysics, San Antonio, TX, APR 13-16,
   2005}},
Organization = {{Trinity Univ; UNAM, Inst Astronomia; Univ Texas Austin; Rice Univ}},
Abstract = {{We present results from a systematic numerical study of the effect of
   turbulent velocity fluctuations on the thermal pressure distribution in
   thermally bistable flows. The turbulent fluctuations are characterized
   by their rms Mach number M (with respect to the warm medium) and the
   energy injection wavenumber, k(for). Our results are consistent with the
   picture that as either of these parameters is increased, the local ratio
   of turbulent crossing time to cooling time decreases, causing transient
   structures in which the effective behavior is intermediate between the
   thermal-equilibrium and adiabatic regimes. As a result, the effective
   polytropic exponent gamma(e) of the simulations ranges between similar
   to 0.2 to similar to 1.1, and the mean pressure of the diffuse gas is
   generally reduced below the thermal equilibrium pressure P-eq, while
   that of the dense gas is increased with respect to P-eq. The fraction of
   high-density zones (n > 7.1 cm(-3)) with P > 10(4) K cm(-3) increases
   from roughly 0.1\% at k(for) = 2 and M = 0.5 to roughly 70\% for k(for)
   = 16 and M = 1.25. A preliminary comparison with the recent pressure
   measurements of Jenkins (2004) in CI favors our case with M = 0.5 and
   k(for), = 2.}},
