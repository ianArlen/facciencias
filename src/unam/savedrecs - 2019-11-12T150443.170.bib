@article{ ISI:000223910000009,
Author = {Ortiz, WR and Ostos, EJ and Ruiz, SG and Betancourt, AE and Nava, HM and
   Sanchez, JS and Vargas, MDC},
Title = {{Ecological and agricultural productivity indices and their dynamics in a
   sub-humid/semi-arid region from central Mexico}},
Journal = {{JOURNAL OF ARID ENVIRONMENTS}},
Year = {{2004}},
Volume = {{59}},
Number = {{4}},
Pages = {{753-769}},
Month = {{DEC}},
Abstract = {{Using precipitation data from a semi-arid region in the highlands of
   central Mexico, historical values of productivity are assessed.
   Production models based on climatic indices are discussed. An empirical
   rain simulation equation shows that it is possible to reproduce the
   expected values of productivity from observed ecosystems and
   agricultural yields. A graphical method is proposed to estimate
   intrinsic growth rate and maximum sustainable yield. Spatial rainfall
   variability induced by orographic effects, introduces a chaotic
   semicyclic behavior as expected in a logistic simulation equation.
   Resilience and predictable drought-productivity tendencies can also be
   derived from this model. (C) 2004 Elsevier Ltd. All rights reserved.}},
