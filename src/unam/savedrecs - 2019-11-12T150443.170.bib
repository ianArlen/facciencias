@article{ ISI:000226466100013,
Author = {Geiss, C and Schroer, J},
Title = {{Extension-orthogonal components of preprojective varieties}},
Journal = {{TRANSACTIONS OF THE AMERICAN MATHEMATICAL SOCIETY}},
Year = {{2005}},
Volume = {{357}},
Number = {{5}},
Pages = {{1953-1962}},
Abstract = {{Let Q be a Dynkin quiver, and let Lambda be the corresponding
   preprojective algebra. Let C = \{C-i | i is an element of I\} be a set
   of pairwise different indecomposable irreducible components of varieties
   of Lambda-modules such that generically there are no extensions between
   C-i and C-j for all i, j. We show that the number of elements in C is at
   most the number of positive roots of Q. Furthermore, we give a
   module-theoretic interpretation of Leclerc's counterexample to a
   conjecture of Berenstein and Zelevinsky.}},
