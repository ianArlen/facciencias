@article{ ISI:000227140300003,
Author = {Verdin, E and Escudero, R},
Title = {{Crystalline structure and physical properties of UCo2Al3}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2004}},
Volume = {{18}},
Number = {{30}},
Pages = {{3905-3914}},
Month = {{DEC 10}},
Abstract = {{Some intermetallic compounds which contain uranium or cerium present
   heavy fermion characteristics. Take, for example, in the UM2Al3 (M = Pd,
   Ni) family, superconductivity and magnetism coexist and present heavy
   fermion behavior. This work presents the crystallographic
   characteristics and physical properties of a new compound of this
   family; the intermetallic compound UCo2Al3. Our initial crystallographic
   studies performed in a small single crystal show that the structure is
   hexagonal and similar to the UNi2Al3 and UPd2Al3 parent compounds. The
   space group is P6/mmm with a = 5.125 Angstrom and c = 4.167 Angstrom
   crystalline parameters. Measurements of resistivity and magnetization
   performed on the single crystal reveal that the compound is not
   superconducting when measured at about 1.8 K. The compound is highly
   anisotropic and features related to Kondo-like behavior are observed. A
   weak ferromagnetic transition is observed at a temperature of about 20
   K.}},
