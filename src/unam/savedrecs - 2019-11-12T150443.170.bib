@inproceedings{ ISI:000228485400015,
Author = {Vazquez-Contreras, E and Hernandez, GP and Sanchez-Rebollar, BG and
   Chanez-Cardenas, ME},
Editor = {{Uribe, FJ and Scherer, LGC and DiazHerrera, E}},
Title = {{The unfolding and refolding reactions of triosephosphate isomerase from
   trypanosoma Cruzi follow similar pathways. Guanidinium hydrochloride
   studies.}},
Booktitle = {{STATISTICAL PHYSICS AND BEYOND}},
Series = {{AIP Conference Proceedings}},
