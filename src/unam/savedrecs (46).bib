@article{ ISI:000344084800008,
Author = {Becco, Lorena and Carlos Garcia-Ramos, Juan and Ruiz Azuara, Lena and
   Gambino, Dinorah and Garat, Beatriz},
Title = {{Analysis of the DNA Interaction of Copper Compounds Belonging to the
   CasiopeinasA (R) Antitumoral Series}},
Journal = {{BIOLOGICAL TRACE ELEMENT RESEARCH}},
