@article{ ISI:000342848100020,
Author = {Jacobo-Cabral, Carlos O. and Garcia-Roca, Pilar and Reyes, Herlinda and
   Lozada-Rojas, Lucero and Cruz-Antonio, Leticia and Medeiros, Mara and
   Castaneda-Hernandez, Gilberto},
Title = {{Limustin (R), a non-innovator tacrolimus formulation, yields reduced
   drug exposure in pediatric renal transplant recipients}},
Journal = {{PEDIATRIC TRANSPLANTATION}},
Year = {{2014}},
Volume = {{18}},
Number = {{7}},
Pages = {{706-713}},
Month = {{NOV}},
Abstract = {{The aim of this study was to evaluate the bioavailability of two oral
   tacrolimus formulations, the innovator Prograf((R)) and a formulation
   commercialized in Mexico with the brand name Limustin((R)), in children.
   Stable Mexican pediatric renal transplant recipients received the
   product authorized by their social security provider, being either
   Prograf((R)) or Limustin((R)). At steady state, blood samples were drawn
   and tacrolimus blood concentration against time curves was constructed.
   CYP3A5 genotype was also determined. There was no significant difference
   in dose or in trough concentrations between formulations. However, AUC
   and C-max were significantly higher with Prograf((R)). The lower
   tacrolimus bioavailability with Limustin((R)) was observed in both
   expressers and non-expressers of the functional CYP3A5 protein.
   Dose-normalized AUC values in expressers were 12.7 +/- 11.9 and 48.7 +/-
   20.4 ng center dot h/mL/mg for Limustin((R)) and Prograf((R)), whereas
   in non-expressers, dose-normalized AUC was 54.4 +/- 49.1 and 110.4 +/-
   42.9 ng center dot h/mL/mg for Limustin((R)) and Prograf((R)),
   respectively (p<0.05). Pharmaceutical quality analysis showed that
   Limustin((R)) dissolution at 120min was 31.1 +/- 6.2\% while
   Prograf((R)) dissolution was 100 +/- 4.8\%. Furthermore, the mean
   percentage of labeled amount of Limustin((R)) and Prograf((R)) was 91.0
   +/- 3.1\% and 100.0 +/- 0.7\%, respectively. Hence, Limustin((R))
   exhibits pharmaceutical characteristics dissimilar to the innovator that
   likely explain the reduced tacrolimus exposure in children. We consider
   Limustin((R)) is not adequate for pediatric use.}},
