@article{ ISI:000342999900003,
Author = {Colin-Garcia, M. and Heredia, A. and Negron-Mendoza, A. and Ortega, F.
   and Pi, T. and Ramos-Bernal, S.},
Title = {{Adsorption of HCN onto sodium montmorillonite dependent on the pH as a
   component to chemical evolution}},
Journal = {{INTERNATIONAL JOURNAL OF ASTROBIOLOGY}},
Year = {{2014}},
Volume = {{13}},
Number = {{4}},
Pages = {{310-318}},
Month = {{OCT}},
Abstract = {{The aim of this work is to study the behaviour of hydrogen cyanide (HCN)
   adsorbed onto mineral surfaces (sodium montmorillonite, a clay mineral)
   in different pH environments as a possible prebiotic process for
   complexation of organics. Our experimental results show that specific
   sites on the surface of the clay increased the concentration of HCN
   molecules dependent on the pH values. Moreover, this adsorption can
   occur through physical and chemical interactions enhanced by the channel
   structure of the sodium montmorillonite. The three-dimensional
   channelling structure of the clay accumulates the organics, hindering
   the releasing (desorption) of the organic molecules. A molecular model
   developed here also confirms the role of the pH as a regulating factor
   in the adsorption of HCN onto the inorganic surfaces and the possibility
   for further reactions forming more complex molecules, as an abiotic
   mechanism important in prebiotic chemical evolution processes.}},
