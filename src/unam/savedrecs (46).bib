@article{ ISI:000342160700007,
Author = {Landa-Marban, David and Bietenholz, Wolfgang and Hip, Ivan},
Title = {{Features of a 2d gauge theory with vanishing chiral condensate}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS C}},
Year = {{2014}},
Volume = {{25}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{The Schwinger model with N-f >= 2 flavors is a simple example for a
   fermionic model with zero chiral condensate Sigma (in the chiral limit).
   We consider numerical data for two light flavors, based on simulations
   with dynamical chiral lattice fermions. We test properties and
   predictions that were put forward in the recent literature for models
   with Sigma = 0, which include IR conformal theories. In particular, we
   probe the decorrelation of low lying Dirac eigenvalues, and we discuss
   the mass anomalous dimension and its IR extrapolation. Here, we
   encounter subtleties, which may urge caution with analogous efforts in
   other models, such as multi-flavor QCD.}},
