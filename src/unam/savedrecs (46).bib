@article{ ISI:000342494100001,
Author = {Raygoza-Viera, J. R. and Ruiz-Fernandez, A. C. and Ruelas-Inzunza, J.
   and Alonso-Hernandez, C. and Perez-Bernal, L. H. and Paez-Osuna, F.},
Title = {{Accumulation and distribution of Hg and Pb-210 in superficial sediments
   from a coastal lagoon in the SE Gulf of California associated with
   urban-industrial and port activities}},
Journal = {{ENVIRONMENTAL EARTH SCIENCES}},
Year = {{2014}},
Volume = {{72}},
Number = {{8}},
Pages = {{2729-2739}},
Month = {{OCT}},
Abstract = {{The Estero de Urias Lagoon (EUL) is an inner shelf barrier coastal
   lagoon, located in the Mexican Pacific Coast (SE Gulf of California). It
   is surrounded by Mazatlan City, one of the most important international
   tourist areas of Mexico. To provide a comprehensive reassessment of the
   concentration levels and spatial variability of Hg and Pb-210 in the
   EUL, 40 surface sediment samples were analyzed for several geochemical
   variables (e.g. grain size distribution, organic matter and reference
   element concentrations) that could explain the observed variability of
   Hg and Pb-210. The Hg concentrations ranged from 23 to 214 ng g(-1),
   whereas Pb-210 activities varied from 20 to 56 Bq kg(-1). No defined
   distribution pattern was observed for Hg and Pb-210 concentrations in
   the lagoon and no evidence of a common atmospheric delivery route was
   observed. The sediments from EUL were found contaminated by Hg, and
   according to international guidelines 48 \% of the sampling sites have
   concentrations that could be harmful to biota.}},
