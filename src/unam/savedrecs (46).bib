@article{ ISI:000343207600001,
Author = {Sasai, Yoshinori and Nagai, Yuya and Itow, Yoshitaka and Matsubara,
   Yutaka and Sako, Takashi and Lopez, Diego and Itow, Tsukasa and
   Munakata, Kazuoki and Kato, Chihiro and Kozai, Masayoshi and Miyazaki,
   Takahiro and Shibata, Shoichi and Oshima, Akitoshi and Kojima, Hiroshi
   and Tsuchiya, Harufumi and Watanabe, Kyoko and Koi, Tatsumi and
   Francisco Valdes-Galicia, Jose and Xavier Gonzalez, Luis and Ortiz,
   Ernesto and Musalem, Octavio and Hurtado, Alejandro and Garcia, Rocio
   and Anzorena, Marcos},
Title = {{Performance of the SciBar cosmic ray telescope (SciCRT) toward the
   detection of high-energy solar neutrons in solar cycle 24}},
Journal = {{EARTH PLANETS AND SPACE}},
Year = {{2014}},
Volume = {{66}},
Month = {{SEP 30}},
Abstract = {{We plan to observe solar neutrons at Mt. Sierra Negra (4,600 m above sea
   level) in Mexico using the SciBar detector. This project is named the
   SciBar Cosmic Ray Telescope (SciCRT). The main aims of the SciCRT
   project are to observe solar neutrons to study the mechanism of ion
   acceleration on the surface of the sun and to monitor the anisotropy of
   galactic cosmic-ray muons. The SciBar detector, a fully active tracker,
   is composed of 14,848 scintillator bars, whose dimension is 300 cm x 2.5
   cm x 1.3 cm. The structure of the detector enables us to obtain the
   particle trajectory and its total deposited energy. This information is
   useful for the energy reconstruction of primary neutrons and particle
   identification. The total volume of the detector is 3.0 m x 3.0 m x 1.7
   m. Since this volume is much larger than the solar neutron telescope
   (SNT) in Mexico, the detection efficiency of the SciCRT for neutrons is
   highly enhanced. We performed the calibration of the SciCRT at Instituto
   Nacional de Astrofisica, Optica y Electronica (INAOE) located at 2,150 m
   above sea level in Mexico in 2012. We installed the SciCRT at Mt. Sierra
   Negra in April 2013 and calibrated this detector in May and August 2013.
