Author = {Sanchez-Salcedo, F. J. and Hidalgo-Gamez, A. M. and Martinez-Garcia,
   Eric E.},
Title = {{THE INCLINATION OF THE DWARF IRREGULAR GALAXY HOLMBERG II}},
