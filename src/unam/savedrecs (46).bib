@article{ ISI:000343761300006,
Author = {Saldana-Vazquez, Romeo A.},
Title = {{Intrinsic and extrinsic factors affecting dietary specialization in
   Neotropical frugivorous bats}},
Journal = {{MAMMAL REVIEW}},
