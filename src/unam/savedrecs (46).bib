@article{ ISI:000342524200003,
Author = {Ortega-Gutierrez, Fernando and Elias-Herrera, Mariano and Moran-Zenteno,
   Dante J. and Solari, Luigi and Luna-Gonzalez, Laura and Schaaf, Peter},
Title = {{A review of batholiths and other plutonic intrusions of Mexico}},
Journal = {{GONDWANA RESEARCH}},
