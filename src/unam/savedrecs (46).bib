@article{ ISI:000340329300031,
Author = {Estrada, Ernesto and de la Pena, Jose A.},
Title = {{Maximum walk entropy implies walk regularity}},
Journal = {{LINEAR ALGEBRA AND ITS APPLICATIONS}},
Year = {{2014}},
Volume = {{458}},
Pages = {{542-547}},
Month = {{OCT 1}},
Abstract = {{The notion of walk entropy S-V (G, beta) for a graph G at the inverse
   temperature beta was put forward recently by Estrada et al. (2014)
   {[}7]. It was further proved by Benzi {[}1] that a graph is walk-regular
   if and only if its walk entropy is maximum for all temperatures beta is
   an element of I, where I is a set of real numbers containing at least an
   accumulation point. Benzi {[}1] conjectured that walk regularity can be
   characterized by the walk entropy if and only if there is beta > 0 such
   that S-V (G, beta) is maximum. Here we prove that a graph is walk
   regular if and only if the S-V (G, beta = 1) = ln n. We also prove that
   if the graph is regular but not walk-regular S-V (G, beta) < ln n for
   every beta > 0 and lim(beta -> 0) S-V (G, beta) = ln n = lim(beta
   ->infinity) S-V (G, beta). If the graph is not regular then S-V (G,
   beta) <= ln n - epsilon for every beta > 0, for some epsilon > 0. (C)
   2014 Elsevier Inc. All rights reserved.}},
