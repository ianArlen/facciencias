@article{ ISI:000345189800022,
Author = {Easton, Jonathan F. and Stephens, Christopher R. and Angelova, Maia},
Title = {{Risk factors and prediction of very short term versus short/intermediate
   term post-stroke mortality: A data mining approach}},
Journal = {{COMPUTERS IN BIOLOGY AND MEDICINE}},
