@article{ ISI:000071489100006,
Author = {Torres, E and Tinoco, R and Vazquez-Duhalt, R},
Title = {{Biocatalytic oxidation of polycyclic aromatic hydrocarbons in media
   containing organic solvents}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
