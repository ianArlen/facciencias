@article{ ISI:A1997WX52700001,
Author = {Romero, D and SanchezFlores, A},
Title = {{Omnipotence in stretchable two-dimensional scaling}},
Journal = {{MATHEMATICAL SOCIAL SCIENCES}},
Year = {{1997}},
Volume = {{33}},
Number = {{1}},
Pages = {{1-21}},
Month = {{FEB}},
Abstract = {{In voting theory, unimodality is a transitivity condition for the
   aggregation of preference orders through the majority method.
   Unimodality can also be seen as an extension of the Coombs
   multidimensional unfolding model when only one dimension is considered:
   points on the real line still correspond to both the stimuli and the
   individuals but, unlike the Coombs model, in which the position of the
   points is fixed, each individual's order is assessed once parts of the
   real line are arbitrarily `stretched', i.e. the position of the points
   is changed while preserving their natural order on the line. In this
   paper we propose a stretchable model for multidimensional scaling and
   prove that, in two dimensions, some instances (called here omnipotent)
   might lead to the induction of all possible preference orders. Our main
   result is the full characterization of omnipotence in this sense.}},
