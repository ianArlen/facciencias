@article{ ISI:A1996VY20100046,
Author = {Naumis, GG and Aragon, JL},
Title = {{Substitutional disorder in a Fibonacci chain: Resonant eigenstates and
   instability of the spectrum}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{1996}},
Volume = {{54}},
Number = {{21}},
Pages = {{15079-15085}},
Month = {{DEC 1}},
Abstract = {{The effect of substitutional disorder in a Fibonacci chain is studied.
   In particular it is shown that the presence of a single impurity affects
   all the states of the unperturbed system, reducing the fractal dimension
   of the spectrum. Resonant eigenstates are also observed. The
   consequences of the instability of the spectrum are discussed in the
   context of the experimental electronic measurements and also the effect
   of boundary conditions in theoretical calculations.}},
