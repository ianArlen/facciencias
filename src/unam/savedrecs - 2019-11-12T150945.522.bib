@article{ ISI:000171776800004,
Author = {Ramos, J and Guevara, MA and Martinez, A and Arce, C and del Rio, Y and
   Amezcua, C and Corsi-Cabrera, M},
Title = {{Evaluation of the affective states evoked by music}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1996}},
Volume = {{13}},
Number = {{2}},
Pages = {{131-145}},
Month = {{DEC}},
Abstract = {{Music is a modifier of human behavior and affective states. This
   characteristic has been widely used for a variety of purposes. In order
   to understand the way this occurs, it is necessary to develop new
   instruments that allow for a more objective evaluation of the subjective
   states experienced by listening to different kinds of music. With this
   purpose a questionnaire was constructed and experimentally tested. In a
   first experiment the questionnaire was applied and a musical preference
   predictive model was generated. Results confirm the idea that music
   drives people into different specifiable emotional states, both pleasant
   and unpleasant, depending on both music and listener characteristics and
   that these emotional states can be evaluated through the questionnaire
   developed and tested for this study.}},
