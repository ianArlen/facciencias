@inproceedings{ ISI:A1997BJ54C00019,
Author = {Bravo, S and BlancoCano, X and ChiCerrito, K},
Editor = {{Antonucci, E and Page, DE}},
Title = {{Interplanetary signatures of solar mass ejections}},
Booktitle = {{SUN AND ITS ROLE IN THE HELIOSPHERE}},
