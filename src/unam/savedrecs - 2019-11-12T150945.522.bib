@article{ ISI:A1996VY19200016,
Author = {Chavez, JA and Gibb, TC and West, AR},
Title = {{Characterisation and properties of the non-stoichiometric perovskite,
   Ca2Fe2-xNbxOy (0.45<x<0.65)}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY}},
Year = {{1996}},
Volume = {{6}},
Number = {{12}},
Pages = {{1957-1961}},
Month = {{DEC}},
Abstract = {{The cubic perovskite phase Ca(2)Fe(2-x)Nb(x)O(g)amma (0.45 < x < 0.65)
   has been prepared by solid-state reaction at 1400 degrees C followed by
   rapid cooling to room temperature. On slower cooling, or heating below
   1360 degrees C, it decomposes to a mixture of Ca2Fe2O5 and Ca2FeNbO6,
   but the quenched single-phase material is kinetically stable up to at
   least 950 degrees C. The oxygen content of composition x = 0.60,
   determined by thermogravimetry, can be varied over the range 5.55 <
   gamma < 5.89 by heat treatments under a range of conditions, including
   H-2-Ar at 700 degrees C to obtain low gamma, and high-pressure oxygen,
   420 bar at 950 degrees C, for high gamma. The main oxidation state of
   Fe, as determined by Mossbauer spectroscopy was Fe3+, but about 13\%
   Fe4+ was present in the as-prepared sample, increasing to 42\% under
   high-pressure O-2. For most gamma values, the crystal structure was that
   of a cubic perovskite with Fe/Nb disorder on the B sites and some oxygen
   vacancies; at high gamma contents, an orthorhombic distortion gave a
   structure resembling that of Ca2FeNbO6. Electrical properties range from
   semiconducting at high gamma consistent with Fe3+/Fe4+ exchange, to
   insulating at low gamma; although there was no direct evidence of oxide
   ion conduction over the range 25-300 degrees C, the rapid adjustment of
   oxygen content gamma on changing the atmosphere at 700-900 degrees C
   indicated significant mobility of oxide ions. The phase is therefore
   likely to be a mixed oxide ion-electron conductor.}},
