@article{ ISI:A1996VX92200003,
Author = {Apatiga, LM and Alba, F and Castano, VM and VeraGraziano, R and Saldana,
   E},
Title = {{Illumination-induced surface charges on thin diamond films at different
   wavelengths}},
Journal = {{JOURNAL OF MATERIALS SCIENCE-MATERIALS IN ELECTRONICS}},
Year = {{1996}},
Volume = {{7}},
Number = {{6}},
Pages = {{381-383}},
Month = {{DEC}},
Abstract = {{Contact angle studies of pure water droplets were carried out on thin
   diamond films. The films were deposited on a fresh Si {[}111] wafer by
   plasma assisted chemical vapour deposition (PACVD) and confirmed to be
   diamond by Raman spectroscopy and scanning electron microscopy.
   Advancing and receding contact angles were measured as a function of
   droplet volume by using different incident wavelengths: red, green and
   violet. Large differences in the contact angles were observed, depending
   on the wavelength of the incident light. These differences are usually
   related to the existence of illumination-induced surface charges or
   electromagnetically active inhomogeneities in the diamond films. The
   results show that this technique is sufficiently sensitive to correlate
   changes in the contact angle with changes in the wavelength used.}},
