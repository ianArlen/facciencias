@article{ ISI:A1997WK62200002,
Author = {Cruz, ML and OrtegaCorona, BG},
Title = {{Hypothalamic monoamine levels during early and late postnatal periods in
   male and female rats}},
Journal = {{MEDICAL SCIENCE RESEARCH}},
Year = {{1997}},
Volume = {{25}},
Number = {{2}},
Pages = {{75-78}},
Month = {{FEB}},
Abstract = {{Physiological and biochemical variations in neurotransmitter (NT)
   systems might be correlated with brain functional and structural sex
   differences. To determine the physiological significance of this
   correlation, we measured the hypothalamic levels of serotonin (5HT) and
   its metabolite 5-hydroxyindolacetic acid (5HIAA), dopamine (DA) and
   norepinephrine (NE) by radioenzymatic methods in male and female rats
   during early (1-5 days) and late (7-25 days) postnatal periods. Results
   showed: (a) Differences in NT levels during both postnatal periods, (b)
   Changes in an opposite direction between NE and DA levels and (c) A
   clear sex difference in 5HT levels, particularly on day 10. These data
   suggest that monoaminergic metabolism may be an important factor
   contributing to hormonal and environmental-induced changes
   (morphological and behavioural), that occur during brain sexual
   differentiation.}},
