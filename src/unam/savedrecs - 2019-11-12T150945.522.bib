@inproceedings{ ISI:000072639100152,
Author = {Aramburo, C and Carranza, M and Martinez, H and Luna, M},
Editor = {{Kawashima, S and Kikuyama, S}},
Title = {{A comparative overview of growth hormone: proteins and genes}},
Booktitle = {{ADVANCES IN COMPARATIVE ENDOCRINOLOGY, TOMES 1 AND 2}},
Year = {{1997}},
Pages = {{893-898}},
Note = {{XIIIth International Congress of Comparative Endocrinology, YOKOHAMA,
   JAPAN, NOV 16-21, 1997}},
Organization = {{Japan Racing Assoc}},
Abstract = {{Somatotropin (GH) is a pituitary hormone of 191 aminoacid residues in
   most vertebrates. Except for human and fish there is little evidence for
   more than one GH gene in vertebrates. The structure of the GH gene
   consists of 5 exons and 4 introns (except in some fish where there are 6
   exons). Eased on structural homology, it appears that there is a
   GH/prolactin (PRL) family of proteins which evolved by duplication from
   an original proto-GH/PRL gene. Growth hormone shows structural
   heterogeneity in all vertebrate classes due to transcriptional,
   posttranscriptional and posttranslational events. Posttranslational
   modifications may be important for expressing quantitative and
   qualitative differences in bioactivity. Synthesis and/or secretion of GH
   might be differentially regulated.}},
