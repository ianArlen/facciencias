@article{ ISI:000071859700003,
Author = {Jauregui, E},
Title = {{Climate changes in Mexico during the historical and instrumented periods}},
Journal = {{QUATERNARY INTERNATIONAL}},
Year = {{1997}},
Volume = {{43-4}},
Pages = {{7-17}},
Abstract = {{This paper reviews the literature on climate fluctuations in Mexico. In
   addition, illustrations of climate changes that occurred during the
   instrumental period in this country e.g. late 19th and 20th centuries
   are also presented. Periods of prominent meridionality in mid latitudes
   that occurred during the late 1890s and during the 1960s seem to have
   brought about significant changes in the climate of Mexico. The
   possibilities for attempting to study climate changes in Mexico as
   related to human history, are also examined. (C) 1997 INQUA/Elsevier
   Science Ltd.}},
