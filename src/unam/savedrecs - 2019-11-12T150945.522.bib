@article{ ISI:000171776800003,
Author = {Tirado, F and Fuentes, R and Gomez, P},
Title = {{Learning by images or word labels. Potential applications for teaching
   History}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1996}},
Volume = {{13}},
Number = {{2}},
Pages = {{119-130}},
Month = {{DEC}},
Abstract = {{Images have a number of advantages for the presentation of information
   since they can express relationships and structures. Several authors
   have experimentally shown the mnemonic superiority of images over its
   word labels. The present study attempts to extend those findings using
   experimental materials such as words and pictures related to teaching
   about the history of the universe. Adult and children (3rd and 5th
   graders) had to recognize and order images and word labels. Results
   suggest a mnemonic superiority of images which is more obvious in
   children than in adults. However, the most relevant developmental
   findings point to learning order as relevant for chindren's retrieval,
   but irrelevant for adults'. Some educational consequences are addressed.
   Historical concepts could be better understood by using images instead
   of words.}},
