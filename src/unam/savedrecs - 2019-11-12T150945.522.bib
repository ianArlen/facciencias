@article{ ISI:A1997WA60800040,
Author = {SalasBrito, AL and NunezYepez, HN and MartinezYRomero, RP},
Title = {{Superintegrability in classical mechanics: A contemporary approach to
   Bertrand's theorem}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS A}},
Year = {{1997}},
Volume = {{12}},
Number = {{1}},
Pages = {{271-276}},
Month = {{JAN 10}},
Note = {{2nd International Workshop on Classical and Quantum Integrable Systems -
   Algebraic Methods and Lie Algebra Contractions (IWCQIS 96), DUBNA,
   RUSSIA, JUL 08-12, 1996}},
Organization = {{Joint Inst Nucl Res, Bogoliubov Lab Theoret Phys}},
Abstract = {{Superintegrable Hamiltonians in three degrees of freedom posses more
   than three functionally independent globally defined and single-valued
   constants of motion. In this contribution and under the assumption of
   the existence of only periodic and plane bounded orbits in a classical
   system we are able to establish the superintegrability of the
   Hamiltonian. Then, using basic algebraic ideas, we obtain a contemporary
   proof of Bertrand's theorem. That is, we are able to show that the
   harmonic oscillator and the Newtonian gravitational potentials are the
   only 3D potentials whose bounded orbits are all plane and periodic.}},
