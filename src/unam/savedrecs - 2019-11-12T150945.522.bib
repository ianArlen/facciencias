@inproceedings{ ISI:A1997BJ97W00043,
Author = {Morozov, DK and Herrera, JJE},
Editor = {{Sugai, H and Hayashi, T}},
Title = {{Slow thermal waves in magnetized radiative plasmas}},
Booktitle = {{ICPP 96 CONTRIBUTED PAPERS - PROCEEDINGS OF THE 1996 INTERNATIONAL
   CONFERENCE ON PLASMA PHYSICS, VOLS 1 AND 2}},
Year = {{1997}},
Pages = {{170-173}},
Note = {{1996 International Conference on Plasma Physics (ICPP 96), NAGOYA,
   JAPAN, SEP 09-13, 1996}},
Organization = {{Japan Soc Plasma Sci \& Nucl Fus Res; Natl Inst Fus Sci; Aichi
   Prefectural Govt; City Nagoya; Fus Sci Assoc; Future Energy Assoc; Int
   Union Pure \& Appl Phys; Inertial Confinement Fus Forum; Japan Atom
   Energy Res Inst; Japan Atom Ind Forum Inc; Minist Educ Sci Sports \&
   Culture, Japan; Union Raidio Sci Int}},
Abstract = {{Thermal force acting on impurity ions can produce a new type of slow
   sound-like waves, existing near the threshold of radiative-condensation
   instability in unmagnetized radiative plasmas. We examine the
   propagation of slow waves normal to a magnetic field in uniform
   magnetized plasmas, taking into account a perpendicular thermal force.
   It is shown that such type of waves can propagate with small damping in
   weakly as well as in strongly magnetized plasmas. We investigate the
   possibility of the existence of parallel wave propagation in non uniform
   magnetized plasmas. We solve the eigenfunction problem for a reduced
   differential equation. It is shown that the wave can propagate in
   radiative layers.}},
