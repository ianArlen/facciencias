@article{ ISI:000171776800008,
Author = {Martinez, LR and Otero, GS},
Title = {{Coping stress and mental health criteria in single mothers through
   reflexion groups}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1996}},
Volume = {{13}},
Number = {{2}},
Pages = {{187-197}},
Month = {{DEC}},
Abstract = {{The present study explored whether Participative Research, when applied
   to a group of single mothers, could improve their coping capabilities.
   An experimental and control two-group design study, with fifteen women
   in each one was conducted. The sample was non-probabilistic and
   intentional. Subjects were selected among single 18: to 25 years old
   mothers, with a minimum of high school education, born and living in
   Mexico City. Ten variables conceptually related to coping efficiency
   toward their situation were measured: assertiveness, self-esteem,
   future-time perspective, conformism, locus of control, organizational
   capability and four conciousness levels. Results revealed statistically
   significant differences between measurements before-after in the
   experimental group, and between the control and experimental post tests
   in all variables. Results are discussed in the context of coping theory.}},
