@article{ ISI:A1997WQ33400150,
Author = {Garcia, J and Castano, VM and Mondragon, MA and Ramirez, E and Gonzalez,
   F and Campero, A and Renteria, V},
Title = {{UV radiation effects on SiO2 gels doped with organic dyes}},
Journal = {{JOURNAL OF SOL-GEL SCIENCE AND TECHNOLOGY}},
Year = {{1997}},
Volume = {{8}},
Number = {{1-3}},
Pages = {{911-916}},
Note = {{8th International Workshop on Glasses and Ceramics from Gels, ALGARVE,
   PORTUGAL, SEP 18-22, 1995}},
Abstract = {{Silica gels prepared by the sol-gel method with TEOS as a precursor and
   doped with the organic dyes rhodamine B, Na fluoresceine and eosine G
   were exposed to UV radiation pulses from a nitrogen laser. The
   absorption, luminescence and IR spectra were taken as a function of the
   exposure time. The absorption and the luminescence curves show
   successive maxima and minima. The same response is observed when the
   exposure procedure is repeated. These results are explained in terms of
   the cyclic filling and depletion of the long-lived triplet excited
   electronic state. At short exposure times, the data can be fitted with a
   Lorentzian curve and the corresponding parameters are given.}},
