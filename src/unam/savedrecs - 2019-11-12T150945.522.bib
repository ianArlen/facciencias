@article{ ISI:A1997WK77000005,
Author = {GaleanaSanchez, H and Rajsbaum, S},
Title = {{Cycle-pancyclism in tournaments .3.}},
Journal = {{GRAPHS AND COMBINATORICS}},
Year = {{1997}},
Volume = {{13}},
Number = {{1}},
Pages = {{57-63}},
Abstract = {{Let T be a hamiltonian tournament with n vertices and gamma a
   hamiltonian cycle of T. For a cycle C-k of length k in T we denote
   J(gamma)(C-k) = \textbackslash{}A(gamma)boolean AND
   A(C-k)\textbackslash{}, the number of arcs that gamma and C, have in
   common. Let f(n,k, T,gamma) = max\{J(gamma)(C-k)\textbackslash{}C-k
   subset of T\} and f(n,k) = min(f(n, k, T, y)\textbackslash{} T is a
   hamiltonian tournament with n vertices, and y a hamiltonian cycle of T).
   In a previous paper {[}3] we studied the case of n greater than or equal
   to 2k - 4 and proved that
   f(n, 3) = 1, f(n, 4) = 1 and f(n, 5) = 2 if n not equal 2k - 2;
   f(n, k) = k - 1 if and only if n = 2k - 2;
   for k > 5, f(n, k) = k - 2 if and only if n greater than or equal to 2k
   - 4, n not equal 2k - 2 and n = k (mod k - 2);
   for k > 5, f(n, k) = k - 3 if and only if n greater than or equal to 2k
   - 4 and n not equivalent to k (mod k - 2).
   In this paper we consider the case of n less than or equal to 2k - 5 and
   complete the description of f(n, k) by proving that f(n, k) = k - 4 if
   and only if n less than or equal to 2k - 5.}},
