@article{ ISI:A1997XE93100018,
Author = {Paniagua, MJA and Montes, JR and Wisbauer, R},
Title = {{Spectral torsion theories in module categories}},
Journal = {{COMMUNICATIONS IN ALGEBRA}},
Year = {{1997}},
Volume = {{25}},
Number = {{7}},
Pages = {{2249-2270}},
Abstract = {{Let M be an R-module and sigma{[}M] the category of air R-modules that
   are subgenerated by M. For ally hereditary torsion theory T in
   sigma{[}M], the class of quotient modules defines a Grothendieck
   category denoted by epsilon(T){[}M]. T is called spectral if
   epsilon(T){[}M] is a spectral category (every short exact sequence in
   epsilon(T){[}M] splits). The paper is devoted to the investigation of
   such torsion theories. Various characterizations are obtained and
   properties of the endomorphism rings of the corresponding quotient
   modules are studied. The final section is concerned with the role of
   simple and semisimple modules with respect to spectral torsion theories.
   Our presentation generalizes and extends related results for spectral
   torsion theories in the category of all R-modules R-Mod.}},
