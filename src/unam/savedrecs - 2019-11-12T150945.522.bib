@article{ ISI:A1997WN17400032,
Author = {Jauregui, R and Bunge, CF and LeyKoo, E},
Title = {{Upper bounds to the eigenvalues of the no-pair Hamiltonian}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{1997}},
Volume = {{55}},
Number = {{3}},
Pages = {{1781-1784}},
Month = {{MAR}},
Abstract = {{A Dirac-like Hamiltonian H with two-body terms, and its no-pair
   Hamiltonian H+ = Lambda(++)H Lambda(++) when Lambda(++) is related to a
   one-particle Hamiltonian ho, are studied in finite-basis representations
   H and Hi. Using finite-basis eigenfunctions of h(0), it holds E(i)(+)
   less than or equal to E(N-+i), i>0, where E(N-+i) and E(i)(+) an the
   ordered eigenvalues of H and H+, and N- is the difference between the
   dimensions of H and H+. The states of order i less than or equal to N-
   exhibit continuum dissolution. In contrast, those of order N-+i, i>0,
   are bounded from below and after application of a variational principle
   they represent bound states.}},
