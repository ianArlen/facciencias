@inproceedings{ ISI:A1997BH46M00010,
Author = {Lugo, JE and deRio, JA and TaguenaMartinez, J},
Editor = {{Collins, RW and Fauchet, PM and Shimizu, I and Vial, JC and Shimada, T and Alivisatos, AP}},
Title = {{Theoretical thermal conductivity of porous silicon: Nonlinear behavior}},
Booktitle = {{ADVANCES IN MICROCRYSTALLINE AND NANOCRYSTALLINE SEMICONDUCTORS - 1996}},
Series = {{MATERIALS RESEARCH SOCIETY SYMPOSIUM PROCEEDINGS}},
Year = {{1997}},
