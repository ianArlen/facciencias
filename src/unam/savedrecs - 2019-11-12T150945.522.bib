@article{ ISI:A1997WL38000003,
Author = {Arechiga, H and RodriguezSosa, L},
Title = {{Coupling of environmental and endogenous factors in the control of
   rhythmic behaviour in decapod crustaceans}},
Journal = {{JOURNAL OF THE MARINE BIOLOGICAL ASSOCIATION OF THE UNITED KINGDOM}},
Year = {{1997}},
Volume = {{77}},
Number = {{1}},
Pages = {{17-29}},
