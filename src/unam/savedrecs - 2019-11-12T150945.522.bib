@article{ ISI:A1996VZ26200016,
Author = {Kubo, J and Mondragon, M and Zoupanos, G},
Title = {{Perturbative unification of soft supersymmetry-breaking terms}},
Journal = {{PHYSICS LETTERS B}},
Year = {{1996}},
Volume = {{389}},
Number = {{3}},
Pages = {{523-532}},
Month = {{DEC 19}},
Abstract = {{Perturbative unification of soft supersymmetry-breaking (SSB) parameters
   is proposed in gauge-Yukawa unified models, The method, which can be
   applied in any finite order in perturbation theory, consists in
   searching for renormalization group invariant relations among the SSB
   parameters, which are consistent with perturbative renormalizability.
   For the minimal gauge-Yukawa unified model based on SU(5) we find that
   the low energy SSB sector contains a single arbitrary parameter, the
   unified gaugino mass. Within a certain approximation we find that the
   model predicts a superpartner spectrum which is consistent with the
   experimental data.}},
