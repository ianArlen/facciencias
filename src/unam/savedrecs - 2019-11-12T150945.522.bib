@inproceedings{ ISI:000073445600024,
Author = {Poveda, A and Allen, C and Herrera, MA},
Editor = {{Leung, KC}},
Title = {{Red stragglers: The problem of chromospheric activity in old stars}},
Booktitle = {{THIRD PACIFIC RIM CONFERENCE ON RECENT DEVELOPMENT ON BINARY STAR
   RESEARCH}},
Series = {{Astronomical Society of the Pacific Conference Series}},
Year = {{1997}},
Volume = {{130}},
Pages = {{123-128}},
Note = {{3rd Pacific Rim Conference on Recent Development on Binary Star
   Research, CHIANG MAI, THAILAND, OCT 26-NOV 01, 1995}},
Organization = {{Chiang Mai Univ; Thai Astronom Soc; Univ Nebraska Lincoln; Natl Res
   Council Thailand; Think Earth Thailand; Trantraphun Dept Store,
   Thailand; Tra Co Ltd, Thailand}},
Abstract = {{It is well known that stellar chromospheric activity is a signature of
   stellar age, the rotational angular momentum being the critical
   parameter whose decay is associated to diminishing stellar activity and
   hence to the aging of a star. Tidal coupling in close binaries modifies
   this simple aging scheme. In fact, transfer of orbital angular momentum
   into rotation prolongs the duration of the active chromospheric phase in
   close binaries. The loss of orbital angular momentum through the
   chromospheric activity of tidally locked components leads to an
   irreversible process: a close binary evolves into a contact binary,
   which in turn coalesces into a single rejuvenated star. In the present
   paper we discuss various examples that illustrate this evolutionary
   scheme. We show a number of single stars whose kinematic and photometric
   characteristics would advertise their old age, while their chromospheric
   activity (flares, calcium H and K emission, and H-alpha emission) would
   lead us to assign a young age to them. We propose to identify such stars
   as examples of the terminal evolution of close binaries, a process by
   which a pair of old, tidally locked stars are fused into a rejuvenated
   single star.}},
