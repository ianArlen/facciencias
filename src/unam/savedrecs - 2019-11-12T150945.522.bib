@article{ ISI:A1997VX25400017,
Author = {Tagle, PG and Yatsimirsky, AK},
Title = {{Esterolytic activity of metal complexes of 2,6-diacetylpyridine dioxime}},
Journal = {{POLYHEDRON}},
Year = {{1997}},
Volume = {{16}},
Number = {{5}},
Pages = {{877-879}},
Abstract = {{The rate constants of the cleavage of 4-nitrophenyl acetate
   (4-O2NC6H4O2CCH3) by the complexes of 2,6-diacetylpyridine dioxime with
   Ni-II, Hg-II, Zn-II, Pb-II, Mn-II and Cd-II range from 2 to ca 6000
   times higher than that of the free ligand under the same conditions.
   Complexation of the oxime ligand with Co-II and Cu-II does not affect
   the reaction rate. The complex with Cd-II operates in a catalytic regime
   with respect to the oxime ligand. The activities of the complexes with
   different metals, excluding Co-II, correlate with those of
   metallocarboxypeptidases towards an ester substrate. Copyright (C) 1996
   Elsevier Science Ltd}},
