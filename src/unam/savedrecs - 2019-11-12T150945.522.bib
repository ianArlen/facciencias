@article{ ISI:A1997XC78200052,
Author = {Garzon, IL and Kaplan, IG and Santamaria, R and Vaisberg, BS and Novaro,
   O},
Title = {{Ab initio model potential and molecular dynamics simulation of Ag-6
   clusters}},
Journal = {{ZEITSCHRIFT FUR PHYSIK D-ATOMS MOLECULES AND CLUSTERS}},
Year = {{1997}},
Volume = {{40}},
Number = {{1-4}},
Pages = {{202-205}},
Note = {{8th International Symposium on Small Particles and Inorganic Clusters
   (ISSPIC 8), COPENHAGEN, DENMARK, JUL 01-06, 1996}},
Organization = {{Augustinus Fonden; Carlsbergfondet; Danish Ctr Nanostructures; Danfys
   A/S; Danish Nat \& Tech Sci Res Councils; Nord Inst Theoret Phys
   (NORDITA); Novo Nordisk Fonden; Otto Monsteds Fond; Niels Bohr Inst
   Astron Phys \& Geophys; Thomas B Thrige Fond}},
Abstract = {{We present an approach to generate a model potential with parameters
   fitted to ab initio energetic surfaces. The potential includes two-,
   three- and four-body terms. Each of them consists of an exponential
   exchange and dispersion terms. The analytical form of the latter was
   taken from perturbation theory up to fourth order. We illustrate the
   present approach by constructing an ab initio model potential for the
   Ag-6 cluster. A molecular dynamics simulation using this potential
   reveals interesting features in the isomerization of the C-2v structure.
   A two step isomerization transition is observed: First, at temperatures
   around 350 K, the cluster structures fluctuate between two-dimensional
   isomers. At higher temperatures (450 K), fast transitions occur between
   two- and three-dimensional cluster configurations.}},
