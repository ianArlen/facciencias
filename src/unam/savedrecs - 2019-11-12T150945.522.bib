@article{ ISI:A1997WV62700001,
Author = {delaPena, L and Cetto, AM},
Title = {{Tunneling times for opaque barriers}},
Journal = {{FOUNDATIONS OF PHYSICS LETTERS}},
Year = {{1997}},
Volume = {{10}},
Number = {{1}},
Pages = {{1-15}},
Month = {{FEB}},
Abstract = {{We propose new criteria to evaluate the average time spent by particles
   in a tunneling barrier. First we construct a sojourn time, on the basis
   of statistical information provided by quantum mechanics, which seems to
   be an appropriate measure of the time spent by all particles within the
   barrier. A simple, stochastic treatment is then used to deal with the
   particles that actually traverse the barrier, in order to study their
   interaction time. The results obtained show that opaque barriers have
   important effects on the particles before they enter the potential
   region, confirming previously published numerical findings. No
   arbitrarily high effective velocities appear anywhere in the present
   treatment.}},
