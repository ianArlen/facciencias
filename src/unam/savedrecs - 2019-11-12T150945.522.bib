@inproceedings{ ISI:A1997BJ28R00047,
Author = {Henney, WJ and Arthur, SJ},
Editor = {{Reipurth, B and Bertout, C}},
Title = {{Two-wind interaction models of the proplyds in the Orion nebula}},
Booktitle = {{HERBIG-HARO FLOWS AND THE BIRTH OF LOW MASS STARS}},
Series = {{IAU SYMPOSIA}},
Year = {{1997}},
Number = {{182}},
Pages = {{561-570}},
Note = {{182nd Symposium of the International-Astronomical-Union on Herbig-Haro
   Flows and the Birth of Low Mass Stars, CHAMONIX, FRANCE, JAN 20-26, 1997}},
Organization = {{Int Astron Union}},
Abstract = {{Many low-mass stars in the Orion nebula are associated with very compact
   (similar or equal to 1 arcsec) emission knots, known variously as
   proplyds, PIGs or LV knots. Some of these knots are teardrop-shaped,
   with `'tails'' pointing away from the massive star theta(1) Ori C, which
   is the principal exciting star of the nebula. We discuss models of such
   knots, which invoke the interaction of the fast stellar wind from
   theta(1) Ori C with a transonic photoevaporated flow from the surface of
   an accretion disk around a young low-mass star. We review previous
   analytic work and compare the results of the model with the observed
   brightnesses, morphologies and emission line profiles of the knots, as
   well as presenting new results from numerical hydrodynamical
   simulations.}},
