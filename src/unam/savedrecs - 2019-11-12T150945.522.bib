@inproceedings{ ISI:A1997BJ08F00018,
Author = {NavarroGonzalez, R and Ramirez, SI},
Editor = {{Raulin, F and Greenberg, JM}},
Title = {{Corona discharge of Titan's troposphere}},
Booktitle = {{LIFE SCIENCES: COMPLEX ORGANICS IN SPACE}},
