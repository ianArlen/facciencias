@article{ ISI:A1997WY87300003,
Author = {Vilas, CM},
Title = {{On ambulances, firefighters and police officers: Social policy of
   neoliberalism}},
Journal = {{DESARROLLO ECONOMICO-REVISTA DE CIENCIAS SOCIALES}},
