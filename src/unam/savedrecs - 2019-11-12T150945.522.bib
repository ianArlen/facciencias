@inproceedings{ ISI:A1997BJ47P00019,
Author = {Darszon, A and Santi, CM and Serrano, CJ and Trevino, CL and
   HernandezCruz, A and Lievano, A},
Editor = {{Waites, GMH and Frick, J and Baker, GWH}},
Title = {{T-type Ca2+ channels expressed during mouse spermatogenesis may mediate
   sperm acrosome reaction}},
Booktitle = {{CURRENT ADVANCES IN ANDROLOGY}},
Year = {{1997}},
Pages = {{165-170}},
Note = {{VIth International Congress of Andrology - Current Advances in
   Andrology, SALZBURG, AUSTRIA, MAY 25-29, 1997}},
Abstract = {{Ion channel are key elements in the mammalian sperm acrosome reaction
   (AR). Sperm are differentiated terminal cells unable to synthesize
   protein, and difficult to study electrophysiologically. Using molecular
   biology to learn about their ion channels requires spermatogenic cells
   where channel proteins are being synthesized. These cells are larger
   than sperm and easier to patch clamp. We have looked for the expression
   of the alpha(1) genes, which code for the channel subunit of the various
   types of voltage activated Ca2+ channels, in purified spermatogenic
   cells with RT-PCR. We found that mainly alpha(IE) mRNA is expressed, and
   increases during spermiogenesis. Interestingly, we only detected T-type
   Ca2+ currents in pachytene spermatocytes. Since these currents are
   blocked by Ni2+ and dehydropyridines, as is the ZP3 induced AR, it is
   likely that T-type Ca2+ channels play a key role in the Ca2+ uptake
   required for mammalian sperm AR.}},
