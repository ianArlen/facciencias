@inproceedings{ ISI:A1997BJ97W00146,
Author = {Soboleva, TK and Krasheninnikov, SI and Pigarov, AY},
Editor = {{Sugai, H and Hayashi, T}},
Title = {{Plasma flow in recycling region of tokamak divertor and plasma
   recombination}},
Booktitle = {{ICPP 96 CONTRIBUTED PAPERS - PROCEEDINGS OF THE 1996 INTERNATIONAL
   CONFERENCE ON PLASMA PHYSICS, VOLS 1 AND 2}},
Year = {{1997}},
Pages = {{582-585}},
