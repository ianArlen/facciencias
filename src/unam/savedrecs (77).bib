@article{ ISI:000290117600009,
Author = {Salazar Schettino, Paz Maria and Bucio Torres, Martha and Cabrera Bravo,
   Margarita and Ruiz Hernandez, Adela Luisa},
Title = {{Chagas disease in Mexico. Report of two acute cases}},
Journal = {{GACETA MEDICA DE MEXICO}},
