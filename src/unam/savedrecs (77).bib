@article{ ISI:000291113200035,
Author = {Galano, Annia and Francisco-Marquez, Misaela and Raul Alvarez-Idaboy, J.},
Title = {{Mechanism and kinetics studies on the antioxidant activity of sinapinic
   acid}},
Journal = {{PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
Year = {{2011}},
Volume = {{13}},
Number = {{23}},
Pages = {{11199-11205}},
Abstract = {{The OOH radical scavenging activity of sinapinic acid (HSA) has been
   studied in aqueous and lipid solutions, using the Density Functional
   Theory. HSA is predicted to react about 32.6 times faster in aqueous
   solution than in lipid media. The overall rate coefficients are
   predicted to be 5.39 x 10(5) and 1.66 x 10(4) M-1 s(-1), respectively.
   Branching ratios for the different channels of reaction are also
   reported for the first time, as well as the UV-Vis spectra of the main
   products of reaction. It was found that the reactivity of sinapinic acid
   towards OOH radicals takes place almost exclusively by H atom transfer
   from its phenolic moiety. However it was found to react via SET, at
   diffusion-limit controlled rate constants, with (OH)-O-center dot,
   (OCCl3)-O-center dot and (OOCCl3)-O-center dot radicals. It was found
   that the polarity of the environment and the deprotonation of HSA in
   aqueous solution, both increase the reactivity of this compound towards
   peroxyl radicals.}},
