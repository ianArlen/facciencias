@article{ ISI:000286730000008,
Author = {Arias-Olguin, Imilla I. and Carrillo, Elisa and Meza-Torres, Bernardo
   and Barriga-Montoya, Carolina and Balleza, Daniel and Gomez-Lagunas,
   Froylan},
Title = {{Induction of a fast inactivation gating on delayed rectifier Shab K+
   channels by the anti-inflammatory drug celecoxib}},
Journal = {{CHANNELS}},
Year = {{2011}},
Volume = {{5}},
Number = {{1}},
Pages = {{56-64}},
Month = {{JAN-FEB}},
Abstract = {{Celecoxib is a drug designed to selectively inhibit COX-2, an
   inflammation-inducible cyclooxygenase isoform, over the constitutively
   expressed COX-1 isoform. In addition to this selective inhibition it is
   now known that celecoxib exerts a variety of effects on several types of
   ion channels, thus producing secondary physiological effects. In this
   work we demonstrate that at therapeutically relevant concentrations
   celecoxib interacts with Shab K+ channels specifically promoting a fast
   inactivation gating (without blocking the pore or significantly
   affecting other gating processes). At least two celecoxib molecules bind
   to each channel promoting a fast inactivation that develops from both
   open and closed states. Channel inactivation in turn causes a reduction
   of the size of I-kappa. Taken together, our observations show that in
   addition to its intended therapeutic target celecoxib is a useful tool
   to further study the mechanism of Shab channel inactivation.}},
