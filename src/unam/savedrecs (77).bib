@inproceedings{ ISI:000313557900013,
Author = {Miguel, Meza J. and Fernando, Pena},
Editor = {{Onate, E and Owen, DRJ and Peric, D and Suarez, B}},
Title = {{A NEW FORMULATION TO ASSESS THE SEISMIC DEMAND OF MASONRY STRUCTURES BY
   MEANS OF INPUT ENERGY}},
Booktitle = {{COMPUTATIONAL PLASTICITY XI: FUNDAMENTALS AND APPLICATIONS}},
Year = {{2011}},
Pages = {{159-200}},
Note = {{11th International Conference on Computational Plasticity (COMPLAS) -
   Fundamentals and Applications, Barcelona, SPAIN, SEP 07-09, 2011}},
Abstract = {{The main objective of this paper is to evaluate the elastic input energy
   of unreinforced masonry structures by means of the input energy
   spectrum. The energy is a novel approach which allows evaluating in a
   global and easily way the performance of the masonry structures.
   Structures modeled with non frame elements require of a great number of
   2D or 3D elements, thereby making the calculation of the input energy a
   complicated issue. In this context, a new formulation that calculates
   the input energy using an input energy spectrum and the balance of
   energy is proposed. Two examples of application of unreinforced masonry
   structures were considered to evaluate the input energy and compare it
   with the proposed formula. The formula proposed shows interesting
   results that allowed identify the key features of the accelerograms that
   influence the input energy into structures.}},
