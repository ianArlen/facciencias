@article{ ISI:000286992700004,
Author = {Ramirez-Barahona, Santiago and Luna-Vega, Isolda and Tejero-Diez, Daniel},
Title = {{Species richness, endemism, and conservation of American tree ferns
   (Cyatheales)}},
Journal = {{BIODIVERSITY AND CONSERVATION}},
