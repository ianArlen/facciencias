@article{ ISI:000286301900002,
Author = {Tejeda-Martinez, A. and Luyando, E. and Jauregui, E.},
Title = {{Average conditions of thermal stress in Mexican cities with more than
   one million inhabitants in the face of climatic change}},
Journal = {{ATMOSFERA}},
Year = {{2011}},
Volume = {{24}},
Number = {{1, SI}},
Pages = {{15-30}},
Month = {{JAN 1}},
Abstract = {{Human bioclimatic scenarios are presented for Mexican cities with more
   than a million inhabitants. For this, both urban and global warming were
   considered, the former having been inferred from demographic data, and
   the latter from the output of general circulation models. Thus, it was
   possible to estimate increases in temperature and modifications in
   hygrometric conditions, which were then used to estimate the human
   bioclimate for the period of 1981-2000 and the decades of 2030 and 2050,
   as well as the domestic electrical consumption for air-conditioning in
   housing resulting there from.}},
