@article{ ISI:000284215500009,
Author = {Diaz-Torrejon, C. C. and Espinosa-Magana, F. and Kaplan, Ilya G.},
Title = {{Comparative Theoretical Study of the Electron Affinities of the
   Alkaline-Earth Clusters: Be-n, Mg-n, and Ca-n (n=2, 3)}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2011}},
Volume = {{111}},
Number = {{1}},
Pages = {{103-110}},
Month = {{JAN}},
Abstract = {{If in atoms only one type of the electron affinity (EA) can be defined,
   in molecules there are three types of EAs: the vertical electron
   affinity (VEA), adiabatic electron affinity (AEA), and vertical electron
   detachment energy (VEDE). These three types of EAs for beryllium,
   magnesium, and calcium dimers and trimers are calculated at the
   all-electron MP4(SDTQ) level employing the Dunning-type basis sets. All
   obtained EAs satisfy the following inequality VEDE > AEA > VEA and are
   quite large to be observed in experiment, especially in the trimer case:
   VEDE (Be-3(-)) = 1.63 eV, VEDE (Mg-3(-)) = 0.72 eV, and VEDE (Ca-3(-)) =
   0.95 eV. The decomposition of VEDE into physical components (Koopmans,
   relaxation, and correlation) and the atomic orbital population analysis
   (at the NBO level) are used to elucidate the nature of the outer
   electron binding in studied anions. (C) 2009 Wiley Periodicals, Inc. Int
   J Quantum Chem 111: 103-110, 2011}},
