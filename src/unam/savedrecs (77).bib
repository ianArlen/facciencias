Author = {Rius-Alonso, C. and Olivares-Lachica, L. and Gallard-Paz, D. and
   Hernandez-Delgado, R. and Diaz-Merito, G. and Rueda-Alatorre, M. F. and
   Gonzalez-Quezada, Y.},
