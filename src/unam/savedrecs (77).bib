@inproceedings{ ISI:000393696300098,
Author = {Diaz-Rodriguez, J. Abraham},
Editor = {{Chung, CK and Kim, HK and Lee, JS and Jung, YH and Kim, DS}},
Title = {{Diatomaceous soils: monotonic behavior}},
