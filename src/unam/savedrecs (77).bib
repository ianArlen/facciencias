@inproceedings{ ISI:000393725100028,
Author = {Muro, Victor and Amezcua, Felipe},
Editor = {{Michaletz, PH and Travnichek, VH}},
Title = {{Observations on the Reproductive Biology of the Chihuil Sea Catfish in
   the Southeast Gulf of California: Implications for Management}},
Booktitle = {{CONSERVATION, ECOLOGY, AND MANAGEMENT OF CATFISH: THE SECOND
   INTERNATIONAL SYMPOSIUM, 2010}},
