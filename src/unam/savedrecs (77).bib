@article{ ISI:000298490200007,
Author = {Pacheco-Alvarez, Diana and Gamba, Gerardo},
Title = {{WNK3 is a Putative Chloride-sensing Kinase}},
Journal = {{CELLULAR PHYSIOLOGY AND BIOCHEMISTRY}},
