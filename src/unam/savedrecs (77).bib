@article{ ISI:000285766000012,
Author = {Clark, D. M. and Eikenberry, S. S. and Brandl, B. R. and Wilson, J. C.
   and Carson, J. C. and Henderson, C. P. and Hayward, T. L. and Barry, D.
   J. and Ptak, A. F. and Colbert, E. J. M.},
Title = {{Multiwavelength study of Chandra X-ray sources in the Antennae}},
