@article{ ISI:000299520900006,
Author = {Ramirez, Clara},
Title = {{The historical hermeneutics and the importance of explain the world
   history from Mexico}},
Journal = {{CO-HERENCIA}},
Year = {{2011}},
Volume = {{8}},
Number = {{14}},
Pages = {{133-159}},
Month = {{JAN-JUN}},
Abstract = {{In this paper I argue that Latin America accepts a worldview developed
   in Europe. This worldview was conceived during the nineteenth and
   twentieth centuries, and it serves the interests of one small part of
   the world. It is not useful to adequately explain our history. In the
   article, I point out some inconsistencies present in the History we
   received from Europe. I show the need to deconstruct this vision and I
   appeal to the need to write a universal history from Latin America, to
   explain our societies.}},
