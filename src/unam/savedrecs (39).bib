@article{ ISI:000355345600008,
Author = {Schneiter, E. M. and Velazquez, P. F. and Reynoso, E. M. and Esquivel,
   A. and De Colle, F.},
Title = {{3D MHD simulation of polarized emission in SN 1006}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2015}},
Volume = {{449}},
Number = {{1}},
Pages = {{88-93}},
Month = {{MAY 1}},
Abstract = {{We use three-dimensional magnetohydrodynamic simulations to model the
   supernova remnant SN 1006. From our numerical results, we have carried
   out a polarization study, obtaining synthetic maps of the polarized
   intensity, the Stokes parameter Q, and the polar-referenced angle, which
   can be compared with observational results. Synthetic maps were computed
   considering two possible particle acceleration mechanisms:
   quasi-parallel and quasi-perpendicular. The comparison of synthetic maps
   of the Stokes parameter Q maps with observations proves to be a valuable
   tool to discern unambiguously which mechanism is taking place in the
   remnant of SN 1006, giving strong support to the quasi-parallel model.}},
