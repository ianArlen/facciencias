@article{ ISI:000354612200002,
Author = {Cunningham, Gabe and Del Rio-Francos, Maria and Hubard, Isabel and
   Toledo, Micael},
Title = {{Symmetry Type Graphs of Polytopes and Maniplexes}},
Journal = {{ANNALS OF COMBINATORICS}},
Year = {{2015}},
Volume = {{19}},
Number = {{2}},
Pages = {{243-268}},
Month = {{JUN}},
Abstract = {{We extend the notion of symmetry type graphs of maps to include
   maniplexes and (abstract) polytopes, using them to study k-orbit
   maniplexes (where the automorphism group has k orbits on flags). In
   particular, we show that there are no fully-transitive k-orbit
   3-maniplexes with k > 1 an odd number. We classify 3-orbit maniplexes
   and determine all face transitivities for 3- and 4-orbit maniplexes.
   Moreover, we give generators of the automorphism group of a maniplex,
   given its symmetry type graph. Finally, we extend these notions to
   oriented maniplexes, and we provide a classification of oriented 2-orbit
   maniplexes and a generating set for their orientation-preserving
   automorphism group.}},
