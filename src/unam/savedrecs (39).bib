Author = {Rodriguez-Rodriguez, Marco A. and Gasca-Pineda, Jaime and Medellin,
   Rodrigo A. and Eguiarte, Luis E.},
Title = {{Analysis of genetic diversity of bighorn sheep (Ovis canadensis) from
   Mexican populations}},
