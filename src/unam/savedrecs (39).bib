@article{ ISI:000420062500008,
Author = {Landeros Aguirre, Leticia Gabriela},
Title = {{Technological resources design for ethical \& civic education: the
   Telesecondary experience in Mexico}},
Journal = {{TEORIA DE LA EDUCACION}},
Year = {{2015}},
Volume = {{27}},
Number = {{1}},
Pages = {{137-153}},
Month = {{JUN}},
Abstract = {{This article shares the experience related to the design of training
   materials for the subject Civic \& Ethical Education (C\&EE) in Mexico.
   It refers back to the case of Telesecondary, a learning approach based
   on the use of technologies covering more than one million students from
   12 to 15 years old in semi-urban and rural zones. It presents a view on
   the process followed in 2006 and some aspect that resulted to be the
   most relevant when articulating a curricular proposal to be used
   nationwide, as well as a didactical proposal based in the use of
   technologies. Three elements are highlighted: the development of a
   theoretical and pedagogical positioning about the civics \& ethics
   education, the definition of core learnings to be promoted throughout
   the designed materials, and the challenges that involves the generation
   and implementation of technological resources in contexts of social
   exclusion and a wide digital gaps.}},
