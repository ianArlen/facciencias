@article{ ISI:000355495600017,
Author = {Suarez-Alcantara, K. and Sorby, M. H. and Pistidda, C. and Karimi, F.
   and Saldan, I. and Hauback, B. C. and Klassen, T. and Dornheim, M.},
Title = {{Synchrotron Diffraction Studies of Hydrogen Absorption/Desorption on
   CaH2 + MgB2 Reactive Hydride Composite Mixed With Fluorinated Compounds}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2015}},
Volume = {{119}},
