@article{ ISI:000353084500023,
Author = {van Aardt, Susan A. and Burger, Alewyn P. and Dunbar, Jean E. and Frick,
   Marietjie and Llano, Bernardo and Thomassen, Carsten and Zuazua, Rita},
Title = {{Destroying longest cycles in graphs and digraphs}},
Journal = {{DISCRETE APPLIED MATHEMATICS}},
Year = {{2015}},
Volume = {{186}},
Pages = {{251-259}},
Month = {{MAY 11}},
Abstract = {{In 1978, C. Thomassen proved that in any graph one can destroy all the
   longest cycles by deleting at most one third of the vertices. We show
   that for graphs with circumference k <= 8 it suffices to remove at most
   1/k of the vertices. The Petersen graph demonstrates that this result
   cannot be extended to include k = 9 but we show that in every graph with
   circumference nine we can destroy all 9-cycles by removing 1/5 of the
   vertices. We consider the analogous problem for digraphs and show that
   for digraphs with circumference k = 2, 3, it suffices to remove 1/k of
   the vertices. However this does not hold for k >= 4. (C) 2015 Elsevier
   B.V. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
