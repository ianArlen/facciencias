@article{ ISI:000352219600045,
Author = {Escalera-Zamudio, Marina and Zepeda Mendoza, M. Lisandra and Heeger,
   Felix and Loza-Rubio, Elizabeth and Rojas-Anaya, Edith and Mendez-Ojeda,
   Maria L. and Taboada, Blanca and Mazzoni, Camila J. and Arias, Carlos F.
   and Greenwood, Alex D.},
Title = {{A Novel Endogenous Betaretrovirus in the Common Vampire Bat (Desmodus
   rotundus) Suggests Multiple Independent Infection and Cross-Species
   Transmission Events}},
Journal = {{JOURNAL OF VIROLOGY}},
Year = {{2015}},
Volume = {{89}},
Number = {{9}},
Pages = {{5180-5184}},
Month = {{MAY}},
Abstract = {{The Desmodus rotundus endogenous betaretrovirus (DrERV) is fixed in the
   vampire bat D. rotundus population and in other phyllostomid bats but is
   not present in all species from this family. DrERV is not
   phylogenetically related to Old World bat betaretroviruses but to
   betaretroviruses from rodents and New World primates, suggesting recent
   cross-species transmission. A recent integration age estimation of the
   provirus in some taxa indicates that an exogenous counterpart might have
   been in recent circulation.}},
Publisher = {{AMER SOC MICROBIOLOGY}},
Address = {{1752 N ST NW, WASHINGTON, DC 20036-2904 USA}},
Type = {{Article}},
