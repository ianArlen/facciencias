@article{ ISI:000355714600009,
Author = {Medina-Franco, Jose L. and Mendez-Lucio, Oscar and Duenas-Gonzalez,
   Alfonso and Yoo, Jakyung},
Title = {{Discovery and development of DNA methyltransferase inhibitors using in
   silico approaches}},
Journal = {{DRUG DISCOVERY TODAY}},
Year = {{2015}},
Volume = {{20}},
Number = {{5}},
Pages = {{569-577}},
Month = {{MAY}},
Abstract = {{Multiple strategies have evolved during the past few years to advance
   epigenetic compounds targeting DNA methyltransferases (DNMTs).
   Significant progress has been made in HTS, lead optimization and
   determination of 3D structures of DNMTs. In light of the emerging
   concept of epi-informatics, computational approaches are employed to
   accelerate the development of DNMT inhibitors helping to screen chemical
   databases, mine the DNMT-relevant chemical space, uncover SAR and design
   focused libraries. Computational methods also synergize with
   natural-product-based drug discovery and drug repurposing. Herein, we
   survey the latest developments of in silico approaches to advance
   epigenetic drug and probe discovery targeting DNMTs.}},
