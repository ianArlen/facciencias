@article{ ISI:000350932400009,
Author = {Ivan Melendez-Ortiz, H. and Alvarez-Lorenzo, Carmen and Burillo,
   Guillermina and Magarinos, Beatriz and Concheiro, Angel and Bucio,
   Emilio},
Title = {{Radiation-grafting of N-vinylimidazole onto silicone rubber for
   antimicrobial properties}},
Journal = {{RADIATION PHYSICS AND CHEMISTRY}},
Year = {{2015}},
Volume = {{110}},
Pages = {{59-66}},
Month = {{MAY}},
Abstract = {{Poly(N-vinylimidazole) (PVIm) was grafted \$numbers onto silicone rubber
   (SR) with the aim of providing antimicrobial properties. The grafting
   was carried out by means of gamma rays using the direct method. The
   influence on the grafting yield of absorbed dose, monomer concentration,
   addition of FeSO4 salt, composition and type of solvent (H2O, MeOH, THF,
   and acetone) was investigated. Grafts onto SR between 10\% and 90\% were
   obtained at doses from 20 to 100 kGy and a dose rate 10.9 kGy h(-1);
   grafting yield increased with monomer concentration and dose. The new
   graft copolymers were confirmed by Fourier transform infrared
   spectroscopy (FT-IR). Differential scanning calorimeter (DSC) showed
   glass transition at 149 and 159 degrees C for 38\% and 88\% grafting
   respectively. Thermogravimetry analysis (TGA) presented two
   decomposition temperatures for SR-g-Vlm at 380 (PVIm) and 440 degrees C
   (SR). SR-g-VIm showed antibacterial activity against Pseudomonas
   aeruginosa. (C) 2015 Elsevier Ltd. All rights reserved.}},
