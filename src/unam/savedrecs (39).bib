@article{ ISI:000353667000002,
Author = {Barrera-Rodriguez, Carlos},
Title = {{A collection of nonfilling multicurve complexes}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2015}},
Volume = {{187}},
Pages = {{26-61}},
Month = {{JUN 1}},
Abstract = {{We introduce a new collection of simplicial complexes associated to a
   connected orientable compact surface S, called k-curve complexes,
   denoted by k-C(S), each containing vertices given by (k-1)-simplices of
   the original curve complex of S,C(S) and edges given by a restricted
   nonfillingness property between vertices. We prove that each complex of
   this collection of complexes is connected and we study their coarse
   geometry, in particular, we prove that the first complex, 1-C(S), is
   hyperbolic and distances in k-C(S) are comparable to the distances in
   the marking complex and therefore comparable to the length of word path
   in MCG(S). In addition, we relate the combinatorial information of
   1-C(S) with the topology of 3-manifolds and we provide an improved bound
   for distance of Heegaard splittings by using the distance in the 1-curve
   complex, 1-C(S). (C) 2015 Elsevier B.V. All rights reserved.}},
