@article{ ISI:000355933000005,
Author = {Hernandez-Vazquez, Eduardo and Chagoya, Victoria},
Title = {{Potential utility of adenosine 5'-ester prodrugs to enhance its plasma
   half-life: synthesis and molecular docking studies}},
Journal = {{MEDICINAL CHEMISTRY RESEARCH}},
