@article{ ISI:000355964300035,
Author = {Garduno, Jorge A. and Garcia, Juventino J.},
Title = {{Synthesis of Annidines and Benzoxazoles from Activated Nitriles with
   Ni(0) Catalysts}},
Journal = {{ACS CATALYSIS}},
