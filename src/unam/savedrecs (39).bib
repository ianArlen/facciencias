@article{ ISI:000355208100012,
Author = {Rodriguez-Andrade, M. A. and Aragon-Gonzalez, G. and Aragon, J. L. and
   Gomez-Rodriguez, A. and Romeu, D.},
Title = {{The Coincidence Site Lattices in 2D Hexagonal Lattices Using Clifford
   Algebra}},
Journal = {{ADVANCES IN APPLIED CLIFFORD ALGEBRAS}},
Year = {{2015}},
Volume = {{25}},
Number = {{2}},
Pages = {{425-440}},
Month = {{JUN}},
Abstract = {{The problem of characterizing the coincidence site lattices obtained by
   superimposing a planar hexagonal lattice with its rotated version is
   solved using simple reflections handled with Clifford algebras. For any
   possible coincidence rotation, analytical expressions for the
