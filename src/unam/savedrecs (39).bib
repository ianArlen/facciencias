@article{ ISI:000356718500011,
Author = {Avila-Arcos, Maria C. and Sandoval-Velasco, Marcela and Schroeder,
   Hannes and Carpenter, Meredith L. and Malaspinas, Anna-Sapfo and Wales,
   Nathan and Penaloza, Fernando and Bustamante, Carlos D. and Gilbert, M.
   Thomas P.},
Title = {{Comparative performance of two whole-genome capture methodologies on
   ancient DNA Illumina libraries}},
Journal = {{METHODS IN ECOLOGY AND EVOLUTION}},
Year = {{2015}},
Volume = {{6}},
Number = {{6}},
Pages = {{725-734}},
Month = {{JUN}},
Abstract = {{Application of whole-genome capture (WGC) methods to ancient DNA (aDNA)
   promises to increase efficiency of ancient genome sequencing. We
   compared the performance of two recent WGC methods in enriching human
   aDNA within Illumina libraries built using both double-stranded and
   single-stranded build protocols. Although both methods effectively
   enriched aDNA, we observed consistent differences between the methods,
   providing the opportunity to further explore parameters influencing WGC
   experiments. Our results suggest bait length has a potential influence
   on library enrichment. Moreover, we show WGC biases against shorter
   molecules that are enriched in single-stranded libraries preparation
   protocols. Lastly, we document the effect of WGC on features including
   clonality, GC composition and repetitive DNA content. Our findings
   provide insights relevant to those planning WGC on aDNA and suggest
   future tests and optimization to improve WGC efficiency.}},
