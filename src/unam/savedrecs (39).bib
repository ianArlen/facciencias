@article{ ISI:000354339300006,
Author = {Lopez-Neira, Juan Pablo and Galicia-Hernandez, Jose Mario and
   Reyes-Coronado, Alejandro and Perez, Elias and Castillo-Rivera,
   Francisco},
Title = {{Surface Enhanced Raman Scattering of Amino Acids Assisted by Gold
   Nanoparticles and Gd3+ Ions}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY A}},
Year = {{2015}},
Volume = {{119}},
Number = {{18}},
Pages = {{4127-4135}},
Month = {{MAY 7}},
Abstract = {{The surface enhanced raman scattering (SERS) signal from the L-tyrosine
   (tyr) molecule adsorbed on gold nanoparticles (Au-tyr) is compared with
   the SERS signal assisted by the presence of gadolinium ions (Gd3+)
   coordinated with the Au-tyr system. An enhancement factor of the SERS
   signal in the presence of Gd3+ ions was similar to 5 times higher than
   that produced by L-tyrosine adsorbed on gold nanoparticles. The
   enhancement of the SERS signal. can be attributed to a corresponding
   increase in the local electric field due to the presence of Gd3+ ions in
   the vicinity of a gold dimer configuration. This scenario was confirmed
   by solving numerically Maxwell equations, showing an increase of 1 order
   of magnitude in the local electric scattered field when the Gd3+ ion is
   located in between a gold dimer compared With naked gold nanopartides.}},
