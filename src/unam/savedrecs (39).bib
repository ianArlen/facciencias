@article{ ISI:000355158400005,
Author = {Becerril, David and Noguez, Cecilia},
Title = {{Adsorption of a Methylthio Radical on Silver Nanoparticles: Size
   Dependence}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2015}},
Volume = {{119}},
Number = {{20}},
