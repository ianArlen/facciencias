@article{ ISI:000376204900014,
Author = {Hennecke, Anna M. and Mueller-Lindenlauf, Maria and Garcia, Carlos A.
   and Fuentes, Alfredo and Riegelhaupt, Enrique and Hellweg, Stefanie},
Title = {{Optimizing the water, carbon, and land-use footprint of bioenergy
   production in Mexico - Six case studies and the nationwide implications}},
