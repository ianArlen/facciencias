@article{ ISI:000372321100003,
Author = {Perez-Larios, Alejandro and Hernandez-Gordillo, Agileo and
   Morales-Mendoza, Getsemani and Lartundo-Rojas, Luis and Mantilla,
   Angeles and Gomez, Ricardo},
Title = {{Enhancing the H-2 evolution from water-methanol solution using
   Mn2+-Mn+3-Mn4+ redox species of Mn-doped TiO2 sol-gel photocatalysts}},
Journal = {{CATALYSIS TODAY}},
Year = {{2016}},
Volume = {{266}},
Pages = {{9-16}},
