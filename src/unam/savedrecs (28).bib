@article{ ISI:000373281200005,
Author = {Clapp, Monica and Ghimenti, Marco and Micheletti, Anna Maria},
Title = {{Semiclassical states for a static supercritical
   Klein-Gordon-Maxwell-Proca system on a closed Riemannian manifold}},
Journal = {{COMMUNICATIONS IN CONTEMPORARY MATHEMATICS}},
Year = {{2016}},
Volume = {{18}},
Number = {{3}},
Month = {{JUN}},
Abstract = {{We establish the existence of semiclassical states for a nonlinear
   Klein-Gordon-Maxwell-Proca system in static form, with Proca mass 1, on
   a closed Riemannian manifold. Our results include manifolds of arbitrary
   dimension and allow supercritical nonlinearities. In particular, we
   exhibit a large class of three-dimensional manifolds on which the system
   has semiclassical solutions for every exponent p is an element of
   (2,infinity). The solutions we obtain concentrate at closed submanifolds
   of positive dimension as the singular perturbation parameter goes to
   zero.}},
Publisher = {{WORLD SCIENTIFIC PUBL CO PTE LTD}},
Address = {{5 TOH TUCK LINK, SINGAPORE 596224, SINGAPORE}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Clapp, M (Reprint Author), Univ Nacl Autonoma Mexico, Inst Matemat, Circuito Exterior CU, Mexico City 04510, DF, Mexico.
