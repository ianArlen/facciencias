@article{ ISI:000378784000002,
Author = {Jones, Christopher K. R. T. and Marangell, Robert and Miller, Peter D.
   and Plaza, Ramon G.},
Title = {{On the spectral and modulational stability of periodic wavetrains for
   nonlinear Klein-Gordon equations}},
