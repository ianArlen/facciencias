@article{ ISI:000379751100056,
Author = {Avendano Pradel, Rafael and Jesus Martinez Anda, J. and Torres
   Villanueva, Mariano and Castillo Rangel, Carlos and Escobar Briones,
   Carolina and Pineda Martinez, Diego and Arteaga Martinez, Sebastian
   Manuel},
Title = {{Microsurgical anatomy and arterial irrigation of the uncal region}},
Journal = {{INTERNATIONAL JOURNAL OF MORPHOLOGY}},
Year = {{2016}},
Volume = {{34}},
Number = {{2}},
Pages = {{784-787}},
Month = {{JUN}},
Abstract = {{The objective of this work was realizing a microsurgical review of the
   main arterial ramus that distribute irrigation to the uncal region,
   identifying the most common variations and more relevant relationships
   with surrounding structures. Twenty (20) fixed human brain hemispheres
   were studied, with the arterial latex and red colorant perfusion
   technique for dissection under microscope magnification (3X-40X).
   Morphometric characterization and data were obtained of the structures
   studied. Arterial irrigation of the uncal region of the temporal lobe is
   established by three groups of uncal ramus: the anterior uncal rami,
   deriving from the temporopolar artery, which is one of the first
   branches of the middle cerebral artery in segment M1. The medial uncal
   rami, branches of the cisternal portion of the anterior choroidal
   artery. The posterior uncal rami, branches of the P2A and P2P segments
   of the posterior cerebral artery. The relevance of arterial vascular
   description of the uncus, results in the application of knowledge of the
   variations and relationships during the diagnostic and surgical
   procedures of the temporal lobe.}},
