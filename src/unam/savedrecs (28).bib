Author = {Medina, J. C. and Bizarro, M. and Gomez, C. L. and Depablos-Rivera, O.
   and Mirabal-Rojas, R. and Monroy, B. M. and Fonseca-Garcia, A. and
   Perez-Alvarez, J. and Rodil, S. E.},
Title = {{Sputtered bismuth oxide thin films as a potential photocatalytic
   material}},
Journal = {{CATALYSIS TODAY}},
Year = {{2016}},
Volume = {{266}},
