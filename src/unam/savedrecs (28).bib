@article{ ISI:000375953200006,
Author = {Hernandez-Arciga, Ulalume and Herrera M, L. Gerardo and
   Morales-Malacara, Juan B.},
Title = {{Tracking host use by bat ectoparasites with stable isotope analysis}},
Journal = {{CANADIAN JOURNAL OF ZOOLOGY}},
