@article{ ISI:000372321100005,
Author = {Hernandez-Gordillo, Agileo and Rodriguez-Gonzalez, Vicente and
   Oros-Ruiz, Socorro and Gomez, Ricardo},
Title = {{Photodegradation of Indigo Carmine dye by CdS nanostructures under
   blue-light irradiation emitted by LEDs}},
