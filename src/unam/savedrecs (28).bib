@article{ ISI:000377734600006,
Author = {Hernandez, E. M. and Hernandez, L. and Serkovic-Loli, L. N. and
   Hinojosa, G.},
Title = {{Collisional induced double electron loss of NO- and CH4- anions below 10
   keV energies}},
Journal = {{INTERNATIONAL JOURNAL OF MASS SPECTROMETRY}},
Year = {{2016}},
Volume = {{403}},
Pages = {{39-42}},
Month = {{JUN 1}},
Abstract = {{Double electron detachment cross sections for methane and nitric oxide
   negative ions passing through oxygen and nitrogen gas targets have been
   studied in the energy ranges of 2.0-10.0 keV for CH41 and 4.0-10.0 keV
   for NO-. The signal growth rate method was employed. Fragmentation was
   only observed for NO- anions colliding with both targets. In the case of
   CH4- anions no fragmentation was observed. The double electron
   detachment cross sections for methane ions is almost one order of
   magnitude larger than the one for nitric oxide ions. The fragmentation
   cross -sections for NO- is larger than the double electron detachment
   cross sections. (C) 2016 Elsevier B.V. All rights reserved.}},
