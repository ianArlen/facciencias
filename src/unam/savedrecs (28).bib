@article{ ISI:000374354400017,
Author = {Cordero-Michel, Narda and Galeana-Sanchez, Hortensia and Goldfeder, Ilan
   A.},
Title = {{Some results on the existence of Hamiltonian cycles in the generalized
   sum of digraphs}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2016}},
Volume = {{339}},
Number = {{6}},
Pages = {{1763-1770}},
Month = {{JUN 6}},
Abstract = {{Let D-1, ..., D-k be a family of pairwise vertex-disjoint digraphs. The
   generalized sum of D-1, D-k, denoted by D-1 circle plus ... circle plus
   D-k, is the set of all digraphs D which satisfies: (i) V (D) = U-i=1(k)
   V (D-i), (ii) D{[}V (D-i)] congruent to D-i for every i is an element of
   {[}k] and (iii) between each pair of vertices in different summands of D
   there is exactly one arc. When each D-i has no arcs, we have that D-1
   circle plus ... circle plus D-k is a set of k-partite digraphs.
   Moreover, for each tournament T on k vertices, we always have that
   T{[}D-1, ..., D-k], the composition of digraphs, is in D-1 circle plus
   ...circle plus D-n. In this work, we give some results on the existence
   or the non-existence of Hamiltonian cycles and cycle-factors in such
   digraphs. Particularly, we prove a generalization of the
   characterization of Hamiltonian semicomplete bipartite digraphs due to
   Gutin, Haggkvist and Manoussakis. (C) 2016 Elsevier B.V. All rights
   reserved.}},
