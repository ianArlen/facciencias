@article{ ISI:000376392700003,
Author = {Drotos, G. and Jung, C.},
Title = {{The chaotic saddle of a three degrees of freedom scattering system
   reconstructed from cross-section data}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2016}},
Volume = {{49}},
Number = {{23}},
Month = {{JUN 10}},
Abstract = {{The topic of this paper is hyperbolic chaotic scattering in a three
   degrees of freedom system. We generalize how shadows in the domain of
   the doubly differential cross-section are found: they are traced out by
   the appropriately filtered unstable manifolds of the periodic
   trajectories in the chaotic saddle. These shadows are related to the
   rainbow singularities in the doubly differential cross-section. As a
   result of this relation, we discover a method of how to recognize in the
   cross section a smoothly deformed image of the chaotic saddle, allowing
   the reconstruction of the symbolic dynamics of the chaotic saddle, its
   topology and its scaling factors.}},
