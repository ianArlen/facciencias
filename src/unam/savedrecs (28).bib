@article{ ISI:000378879500005,
Author = {Corkidi, G. and Montoya, F. and Hernandez-Cruz, G. and Vargas, M. and
   Luviano-Ortiz, J. L. and Ramos, E.},
Title = {{Evaporation dynamics and sedimentation pattern of a sessile particle
   laden water droplet}},
Journal = {{EXPERIMENTS IN FLUIDS}},
Year = {{2016}},
Volume = {{57}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{The dynamics of the flow inside an evaporating sessile droplet of water
   with polystyrene micro-spheres of 1.0 mu m in diameter in suspension is
   described. The initial volume of the droplets is in the range from 0.6
   to 1.0 mu l, and observations were made in the last stages before total
   evaporation. The flow was recorded in a sequence of images that were
   analyzed with a micro-PIV system to extract quantitative information.
   Also, using image analysis techniques we determined the dynamics of the
   retreating liquid film once unpinned from the original contact line.
   Additionally, we have explored its correlation to the formation of the
   sediment pattern which is organized in elongated mounds roughly
   deposited in azimuthal and radial orientations. It is found that the
   aggregation dynamics of micro-spheres in the segments of the two
   orientations is different. This might have a substantial influence on
   the final arrangement of micro-spheres in the sediments.}},
