@article{ ISI:000383906400002,
Author = {Diaz-Guerrero, D. S. and Rodriguez-Vargas, I. and Naumis, G. G. and
   Gaggero-Sager, L. M.},
Title = {{SELF-SIMILAR CHARGE TRANSMISSION IN GAPPED GRAPHENE}},
