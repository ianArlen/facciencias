@article{ ISI:000372321100002,
Author = {Hinojosa-Reyes, Mariana and Hernandez-Gordillo, Agileo and Zanella,
   Rodolfo and Rodriguez-Gonzalez, Vicente},
Title = {{Renewable hydrogen harvest process by hydrazine as scavenging electron
   donor using gold TiO2 photocatalysts}},
Journal = {{CATALYSIS TODAY}},
Year = {{2016}},
Volume = {{266}},
Pages = {{2-8}},
