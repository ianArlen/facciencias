@article{ ISI:000372321100011,
Author = {Mendoza-Damian, G. and Tzompantzi, F. and Perez-Hernandez, R. and Gomez,
   R. and Hernandez-Gordillo, A.},
Title = {{Photocatalytic properties of boehmite-SnO2 composites for the
   degradation of phenol}},
Journal = {{CATALYSIS TODAY}},
Year = {{2016}},
Volume = {{266}},
Pages = {{82-89}},
Month = {{MAY 15}},
