@article{ ISI:000376988400011,
Author = {Chavez-Garcia, Cesar and Jimenez-Corona, Aida and Graue-Hernandez,
   Enrique O. and Zaga-Clavellina, Veronica and Garcia-Mejia, Mariana and
   Jimenez-Martinez, Maria Carmen and Garfias, Yonathan},
Title = {{Ophthalmic indications of amniotic membrane transplantation in Mexico:
   an eight years Amniotic Membrane Bank experience}},
