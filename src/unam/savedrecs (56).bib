@article{ ISI:000323804800026,
Author = {Sheinbaum-Pardo, Claudia and Calderon-Irazoque, Andrea and
   Ramirez-Suarez, Mariana},
Title = {{Potential of biodiesel from waste cooking oil in Mexico}},
