@article{ ISI:000318833100007,
Author = {Galeana-Sanchez, Hortensia and Olsen, Mika},
Title = {{Characterization of asymmetric CKI- and KP-digraphs with covering number
   at most 3}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2013}},
Volume = {{313}},
Number = {{13}},
Pages = {{1464-1474}},
Month = {{JUL 6}},
Abstract = {{A set N subset of V (D) is said to be a kernel if N is an independent
   set and for every vertex x is an element of (V (D) \textbackslash{} N)
   there is a vertex y is an element of N such that xy is an element of
   A(D). Let D be a digraph such that every proper induced subdigraph of D
   has a kernel. D is said to be kernel perfect digraph (KP-digraph) if the
   digraph D has a kernel and critical kernel imperfect digraph
   (CKI-digraph) if the digraph D does not have a kernel. In this paper we
   characterize the asymmetric CKI-digraphs with covering number at most 3.
   Moreover, we prove that the only asymmetric CKI-digraphs with covering
   number at most 3 are: (C) over right arrow (3), (C) over right arrow (5)
   and (C) over right arrow (7) (1, 2). Several interesting consequences
   are obtained. (C) 2013 Elsevier B.V. All rights reserved.}},
