@article{ ISI:000323374100005,
Author = {Cano, Javier and Diaz-Banez, Jose-Miguel and Huemer, Clemens and
   Urrutia, Jorge},
Title = {{The Edge Rotation Graph}},
Journal = {{GRAPHS AND COMBINATORICS}},
Year = {{2013}},
Volume = {{29}},
Number = {{5}},
Pages = {{1207-1219}},
Month = {{SEP}},
Abstract = {{Let P be a set of n points on the plane in general position, n a parts
   per thousand yen 3. The edge rotation graph of P is the graph whose
   vertices are the plane geometric graphs on P with exactly k edges, two
   of which are adjacent if one can be obtained from the other by an edge
   rotation. In this paper we study some structural properties of , such as
   its connectivity and diameter. We show that if the vertices of are not
   triangulations of P, then it is connected and has diameter O(n (2)). We
   also show that the chromatic number of is O(n), and show how to compute
   an implicit coloring of its vertices. We also study edge rotations in
   edge-labelled geometric graphs.}},
