@article{ ISI:000323364500007,
Author = {Pelaez-Ballestas, Ingris and Perez-Taylor, Rafael and Francisco
   Aceves-Avila, Jose and Burgos-Vargas, Ruben},
Title = {{`Not-Belonging': Illness Narratives of Mexican Patients with Ankylosing
   Spondylitis}},
Journal = {{MEDICAL ANTHROPOLOGY}},
Year = {{2013}},
Volume = {{32}},
Number = {{5}},
Pages = {{487-500}},
Month = {{SEP 1}},
Abstract = {{In the study that we describe in this article, we aimed to preserve the
   voices of people suffering from chronic pain by encouraging them to tell
   the story of their suffering. Participants' stories reflect how chronic
   pain has altered their lives. This is partly due to their discomfort,
   but also to the frustration they experience from delays in diagnosis and
   treatment. Patients often pursue a lengthy quest for medical care that
   culminates in the diagnosis of an incurable' disease and a prognosis of
   disability. This in turn affects patient interactions with doctors and
   their social network. We illustrate how chronic pain leads to a sense of
   alienation or not-belonging,' whereby patients perceive pain as an
   entity parallel to yet dominating their own lives.}},
Publisher = {{ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD}},
