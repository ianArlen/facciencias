@article{ ISI:000324785400008,
Author = {Pettit, L. R. and Hart, M. B. and Medina-Sanchez, A. N. and Smart, C. W.
   and Rodolfo-Metalpa, R. and Hall-Spencer, J. M. and Prol-Ledesma, R. M.},
Title = {{Benthic foraminifera show some resilience to ocean acidification in the
   northern Gulf of California, Mexico}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2013}},
Volume = {{73}},
Number = {{2, SI}},
Pages = {{452-462}},
Month = {{AUG 30}},
Abstract = {{Extensive CO2 vents have been discovered in the Wagner Basin, northern
   Gulf of California, where they create large areas with lowered seawater
   pH. Such areas are suitable for investigations of long-term biological
   effects of ocean acidification and effects of CO2 leakage from subsea
   carbon capture storage. Here, we show responses of benthic foraminifera
   to seawater pH gradients at 74-207 m water depth. Living (rose Bengal
   stained) benthic foraminifera included Nonionella basispinata,
   Epistominella bradyana and Bulimina marginata. Studies on foraminifera
   at CO2 vents in the Mediterranean and off Papua New Guinea have shown
   dramatic long-term effects of acidified seawater. We found living
   calcareous benthic foraminifera in low pH conditions in the northern
   Gulf of California, although there was an impoverished species
   assemblage and evidence of post-mortem test dissolution. (C) 2013
   Elsevier Ltd. All rights reserved.}},
