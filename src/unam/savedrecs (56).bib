@article{ ISI:000329220400004,
Author = {Yoval-Sanchez, Belem and Pablo Pardo, Juan and Rodriguez-Zavala, Jose S.},
Title = {{New insights into the half-of-the-sites reactivity of human aldehyde
   dehydrogenase 1A1}},
