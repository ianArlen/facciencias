Author = {Lopez-Lopez, Eduardo and Sabina, Federico J. and Guinovart-Diaz, Raul
   and Bravo-Castillero, Julian and Rodriguez-Ramos, Reinaldo},
Title = {{Overall longitudinal shear elastic modulus of a 1-3 composite with
   anisotropic constituents}},
