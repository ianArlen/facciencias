@article{ ISI:000332809300002,
Author = {Depetris, Carolina},
Title = {{Crossing readings on mysticism: Alejandra Pizarnik, Antonin Artaud,
   Miguel de Molinos, Simone Weil and Georges Bataille}},
Journal = {{RLC-REVUE DE LITTERATURE COMPAREE}},
Year = {{2013}},
Number = {{347}},
Pages = {{283+}},
Month = {{JUL-SEP}},
Abstract = {{In Argentine poet Alejandra Pizarnik's poetical and critical texts,
   there exists a strong conviction that the exercise of writing is closely
   related to the search and recomposition of a lost Absolute. This
   conviction appears, starting in her book Extraccion de la piedra de la
   locura (1968) linked to the notion of excess understood as a necessary
   trespassing beyond any limits imposed to poetry and to poetic essence.
   Discovering the mystical proposals-whether catholics or not-of Antonin
   Artaud, Miguel de Molinos, Simone Weil and Geroges Bataille, led
   Pizarnik to bind her expressive excess to the concepts of nudity and
   annihilation as ways of poetic plenitude.}},
