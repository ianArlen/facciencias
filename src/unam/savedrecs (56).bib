@article{ ISI:000323623800014,
Author = {Hernandez-Alcantara, Pablo and Alberto Salas-de Leon, David and
   Solis-Weiss, Vivianne and Adela Monreal-Gomez, Maria},
Title = {{Geographical patterns in species richness of the benthic polychaetes in
   the continental shelf of the Gulf of California, Mexican Pacific}},
Journal = {{HELGOLAND MARINE RESEARCH}},
Year = {{2013}},
Volume = {{67}},
Number = {{3}},
Pages = {{579-589}},
Month = {{SEP}},
Abstract = {{The present study is the first attempt to describe meso-scale patterns
   in the species richness of polychaetes along the Gulf of California,
   which stretches from about 23A degrees N to 31A degrees N. We examine
   herein the spatial changes in species distribution and explore the
   overlapping of species' ranges towards the centre of the Gulf, to test
   whether the mid-domain effect (MDE) could explain an expected mid-domain
   peak in species richness. The faunal composition and the latitudinal
   range of 244 species of polychaetes recorded along the continental shelf
   of the Gulf of California were analysed in latitude bands of 1A degrees.
   The species composition changes around the Gulf's archipelago ( 29A
   degrees N), and the highest values of species richness are found at the
   25A degrees (197 species) and 26A degrees (193 species) of latitude.
   Although the species richness pattern could be described by a parabolic
   shape, the regional trend was not strongly consistent with the peak of
   diversity at 27A degrees N (176-191 species) predicted by the mid-domain
   effect: the random sorting of species' ranges within spatial domain does
   not explain satisfactorily the geographical patterns of diversity.
   Nevertheless, a partial contribution of MDE to these natural patterns of
   diversity could be detected, and the increase in species richness
   towards middle latitudes was basically determined by species with
   distribution ranges larger than 6A degrees. The low level of
   significance between the empirical species richness pattern and the
   mid-domain model prediction for polychaetes in the Gulf does not
   restrict their use as a model for exploring the randomness of the
   diversity patterns.}},
