@article{ ISI:000325965100004,
Author = {Tavera, Rosaluz and Calderon, Eva},
Title = {{Use of CTAB as a cost-effective solution to an old problem: the
   interference of the mucilage of desmids for scanning electron microscopy}},
Journal = {{PHYCOLOGIA}},
Year = {{2013}},
Volume = {{52}},
Number = {{5}},
Pages = {{422-425}},
Month = {{SEP}},
Abstract = {{Observation of desmids in a scanning electron microscope is a problem
   due to abundant extracellular mucilage. The mucilage is deposited around
   the cells and appears as a fibrous material after drying. We compared
   two reagents for pretreating cells before drying. Formalin acetoalcohol,
   which has been used as a pretreatment, was compared with
   cetyltrimethylammonium bromide (CTAB), which has been used as a mucilage
   treatment in plants and algae. From our results, we recommend CTAB,
   which is readily available, inexpensive, does not require special safety
   handling, and is able to remove many adhering bacteria.}},
