@article{ ISI:000324005100007,
Author = {Azuela, Antonio and Cosacov, Natalia},
Title = {{Urban changes and environmental claims. On the social productivity of
   conflict related to the construction of buildings in Buenos Aires City}},
Journal = {{EURE-REVISTA LATINOAMERICANA DE ESTUDIOS URBANO REGIONALES}},
Year = {{2013}},
Volume = {{39}},
Number = {{118}},
Pages = {{149-172}},
Month = {{SEP}},
Abstract = {{In several Latin American cities, metropolitan expansions are parallel
   to the densification of the central city expressed in the proliferation
   of real estate developments that impact the built city. Residents of
   those areas display protest actions because they perceive deterioration
   in their environment. Therefore it is important to analyze some
   ``proximity conflicts{''} that arise, recovering one of the main
   orientations of the sociology of conflict, which is to inquire about
   their social productivity in three dimensions: the territorialization,
   the eruption of conflict in public space and local updating of the law.
   This article is an attempt to approach recent urban conflicts from this
   perspective through the analysis of the conflict that arose from the
   construction of new buildings in the central zone of the metropolitan
   area,of Buenos Aires, dialoguing with a case fiom Mexico City, where
   similar conflicts have emerged.}},
