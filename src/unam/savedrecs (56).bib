@article{ ISI:000320652000007,
Author = {Bogdanchikova, N. and Pestryakov, A. and Tuzovskaya, I. and Zepeda, T.
   A. and Farias, M. H. and Tiznado, H. and Martynyuk, O.},
Title = {{Effect of redox treatments on activation and deactivation of gold
   nanospecies supported on mesoporous silica in CO oxidation}},
Journal = {{FUEL}},
Year = {{2013}},
Volume = {{110}},
Pages = {{40-47}},
Month = {{AUG}},
Note = {{International-Mexican Congress on Chemical Reaction Engineering
   (IMCCRE), City of Ixtapa-Zihuatanejo, MEXICO, JUN, 2012}},
Abstract = {{The influence of pretreatments with He, O-2 and H-2 on the
   physicochemical and catalytic properties of gold supported on hexagonal
   mesoporous silica (HMS) has been studied by a variety of methods
   including: XPS, TPR, N-2 sorption, UV-visible spectroscopy, FTIR and
   catalytic testing. The investigations show supported gold to form
   different states under redox treatments: Au3+ and Au+ ions, neutral and
   partly charged gold clusters and metal nanoparticles of different sizes.
   Reducing pretreatments increase the catalytic activity of the samples;
   while oxidative treatments deactivate the catalyst. Catalytic tests show
   several regions of the reduced samples with various catalytic
   behaviours: 20-200 degrees C, 200-400 degrees C and >400 degrees C; this
   situation is explained by the co-existence of gold active sites of
   different types. A comparative analysis among spectroscopic and
   catalytic data allows suggesting Au-n(delta+) clusters to be responsible
   for the activity in the low-temperature region (<200 degrees C); neutral
   Au-n clusters are active in the range 200-400 degrees C; and gold
   nanoparticles may catalyze high-temperature CO oxidation. Au3+ ions are
   inactive in this process. (C) 2012 Elsevier Ltd. All rights reserved.}},
