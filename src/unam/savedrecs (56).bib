@article{ ISI:000322591800019,
Author = {Anaya-Vera, S. and Garcia-Martinez, L. and Rosete-Aguilar, M. and Bruce,
   N. C. and Garduno-Mejia, J.},
Title = {{Temporal spreading generated by diffraction in the focusing of
   ultrashort light pulses with perfectly conducting spherical mirrors}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA A-OPTICS IMAGE SCIENCE AND
   VISION}},
Year = {{2013}},
Volume = {{30}},
Number = {{8}},
Pages = {{1620-1626}},
Month = {{AUG}},
Abstract = {{We study femtosecond pulses at the focal plane of a perfectly conducting
   spherical mirror which is a dispersionless system, that is, it
   introduces no group velocity dispersion and no propagation time
   difference to the pulses after reflection. By using the scalar
   diffraction theory we will show that the neglected terms in the
   diffraction integral, when using the approximation of the bandwidth
   being smaller than the frequency of the carrier, have a significant
   influence on imaging if a laser pulse of a few femtoseconds is used in
   time-resolved imaging. The neglected terms introduce temporal spreading
   to extremely short pulses of a few optical cycles incident on the
   mirror, which avoids a fully compensated pulse, i.e., a one optical
   cycle pulse, at the focus of the mirror. The study in this paper also
   applies to refracting optical systems such as microscope objectives or
   lenses. (C) 2013 Optical Society of America}},
