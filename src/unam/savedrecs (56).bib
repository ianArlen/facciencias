@article{ ISI:000328501300051,
Author = {Rivera-Espinosa, Liliana and Floriano-Sanchez, Esau and
   Pedraza-Chaverri, Jose and Coballase-Urrutia, Elvia and Sampieri,
   Aristides Iii and Ortega-Cuellar, Daniel and Cardenas-Rodriguez, Noemi
   and Carmona-Aparicio, Liliana},
Title = {{Contributions of Microdialysis to New Alternative Therapeutics for
   Hepatic Encephalopathy}},
Journal = {{INTERNATIONAL JOURNAL OF MOLECULAR SCIENCES}},
Year = {{2013}},
Volume = {{14}},
Number = {{8}},
Pages = {{16184-16206}},
Month = {{AUG}},
Abstract = {{Hepatic encephalopathy (HE) is a common complication of cirrhosis, of
   largely reversible impairment of brain function occurring in patients
   with acute or chronic liver failure or when the liver is bypassed by
   portosystemic shunts. The mechanisms causing this brain dysfunction are
   still largely unclear. The need to avoid complications caused by late
   diagnosis has attracted interest to understand the mechanisms underlying
   neuronal damage in order to find markers that will allow timely
   diagnosis and to propose new therapeutic alternatives to improve the
   care of patients. One of the experimental approaches to study HE is
