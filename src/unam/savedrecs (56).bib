@article{ ISI:000322883900008,
Author = {Prathap, M. U. Anu and Huerta Aguilar, Carlos Alberto and Pandiyan,
   Thangarasu and Srivastava, Rajendra},
Title = {{Synthesis of imidazole-based NHC-Au(I) complexes and their application
   in non-enzymatic glucose sensing}},
Journal = {{JOURNAL OF APPLIED ELECTROCHEMISTRY}},
Year = {{2013}},
Volume = {{43}},
Number = {{9}},
Pages = {{939-951}},
Month = {{SEP}},
Abstract = {{In this study, imidazole-based NHC-Au(I) complexes were prepared.
   Geometrically optimized structures and molecular orbital energy diagrams
   were computed using density functional theory. Cyclic voltammetry and
   amperometric methods were used to evaluate the catalytic activity of the
   NHC-Au(I)-modified electrodes toward glucose oxidation. The mechanism of
   electrocatalytic oxidation at NHC-Au(I)-modified electrodes is explained
   on the basis of the oxidation and reduction potentials in the presence
   of glucose. Applicability of NHC-Au(I)-modified electrode was extended
   to determine the glucose level in the blood serum and the precision of
