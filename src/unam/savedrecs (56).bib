@article{ ISI:000325587800003,
Author = {del Rosario Baez-Carlos, Adriana},
Title = {{Accountability of federal Mexican deputies in a context of electoral
   alternation}},
Journal = {{CONVERGENCIA-REVISTA DE CIENCIAS SOCIALES}},
Year = {{2013}},
Volume = {{20}},
Number = {{63}},
Pages = {{67-98}},
Month = {{SEP-DEC}},
Abstract = {{The change we have witness in the Mexican political system in the last
   decades of the past century can be summarized as a process of
   decentralization, which as a result of the electoral competition
   increased the number of relevant political actors for legislators. This
   paper attempts to document, through the analysis of roll call votes in
   the Chamber of Deputies that from 2000 the President of the Republic,
   national leaders of parliamentary groups and sub-national party leaders
   are disputing the in the intention of the voting of legislators. The
   analysis finds that the parties with more pronounced territorial
   organizations reported higher ratings when the parliamentary groups are
   divided, indicating the influence of subnational party leaders, and that
   the unity of the parties at subnational level is greater than that of
   the party caucuses as a whole.}},
