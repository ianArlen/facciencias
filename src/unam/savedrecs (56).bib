@article{ ISI:000325815500023,
Author = {Rico Molina, Ricardo and Hernandez Aguilar, Claudia and Dominguez
   Pacheco, Arturo and Cruz-Orea, Alfredo and Angel Canseco, Miguel},
Title = {{Alternative Method to Characterize Corn Grain by Means of Photoacoustic
   Spectroscopy}},
