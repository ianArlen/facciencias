@article{ ISI:000323177100064,
Author = {Portelles, J. and Fuentes, J. and Durruthy-Rodriguez, M. D. and Duarte,
   A. and Raymond, O. and Heiras, J. and Cruz, M. P. and Siqueiros, J. M.},
Title = {{A ferroelastic phase transition in Pb(Zr0.53Ti0.47)O-3:Gd}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2013}},
Volume = {{114}},
Number = {{6}},
Month = {{AUG 14}},
Abstract = {{A study of a new ferroelastic phase transition in Pb(Zr0.53Ti0.47)O-3:Gd
   detected at 144 degrees C, induced by Gd doping is presented. The
   existence of such phase transition is barely detectable in the
   permittivity vs. temperature curve; however, it is made evident through
   the analyses of the temperature coefficient of the resonant frequency
   T-kf dependence with temperature in the radial mode resonance spectrum.
   This coefficient has a sharp discontinuity at around 144 degrees C.
   Micro Raman spectra show that the Raman modes associated to Pb/Gd and
   (Zr/Ti)O-6 positioned at 93.6 cm(-1) E(1TO) and 144 cm(-1) E(1LO) have a
   minimum in intensity around 140 degrees C. Besides the modes of the
   tetragonal structure, there are the modes at 67.3 and 77.8 cm(-1)
   corresponding to a rhombohedral structure, appearing at all
   temperatures. The Raman intensity vs. temperature plots of the different
   modes shows a minimum around 140 degrees C that confirms the
   ferroelastic transition detected in the resonance spectrum. (C) 2013 AIP
   Publishing LLC.}},
