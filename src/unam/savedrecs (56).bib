@article{ ISI:000323374100031,
Author = {Jose Montellano-Ballesteros, Juan and Rivera-Campo, Eduardo},
Title = {{On the Heterochromatic Number of Hypergraphs Associated to Geometric
   Graphs and to Matroids}},
Journal = {{GRAPHS AND COMBINATORICS}},
Year = {{2013}},
Volume = {{29}},
Number = {{5}},
Pages = {{1517-1522}},
Month = {{SEP}},
Abstract = {{The heterochromatic number h (c) (H) of a non-empty hypergraph H is the
   smallest integer k such that for every colouring of the vertices of H
   with exactly k colours, there is a hyperedge of H all of whose vertices
   have different colours. We denote by nu(H) the number of vertices of H
   and by tau(H) the size of the smallest set containing at least two
   vertices of each hyperedge of H. For a complete geometric graph G with n
   a parts per thousand yen 3 vertices let H = H(G) be the hypergraph whose
   vertices are the edges of G and whose hyperedges are the edge sets of
   plane spanning trees of G. We prove that if G has at most one interior
   vertex, then h (c) (H) = nu(H) - tau(H) + 2. We also show that h (c) (H)
   = nu(H) - tau(H) + 2 whenever H is a hypergraph with vertex set and
   hyperedge set given by the ground set and the bases of a matroid,
   respectively.}},
Publisher = {{SPRINGER JAPAN KK}},
Address = {{CHIYODA FIRST BLDG EAST, 3-8-1 NISHI-KANDA, CHIYODA-KU, TOKYO, 101-0065,
   JAPAN}},
