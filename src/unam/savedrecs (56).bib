@article{ ISI:000322532600003,
Author = {Landau, Susana and Leon, Gabriel and Sudarsky, Daniel},
Title = {{Quantum origin of the primordial fluctuation spectrum and its statistics}},
Journal = {{PHYSICAL REVIEW D}},
