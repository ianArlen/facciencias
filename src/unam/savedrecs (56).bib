@article{ ISI:000322116100004,
Author = {Velasquez, A. Ortiz and Christiansen, P. and Cuautle Flores, E. and
   Maldonado Cervantes, I. A. and Paic, G.},
Title = {{Color Reconnection and Flowlike Patterns in pp Collisions}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2013}},
Volume = {{111}},
Number = {{4}},
Month = {{JUL 22}},
Abstract = {{Increasingly, with the data collected at the LHC we are confronted with
   the possible existence of flow in pp collisions. In this work, we show
   that PYTHIA 8 produces flowlike effects in events with multiple hard
   subcollisions due to color string formations between final partons from
   independent hard scatterings, the so-called color reconnection. We
   present studies of different identified hadron observables in pp
   collisions at 7 TeV. Studies have been done both for minimum bias and
   multiplicity intervals in events with and without color reconnection to
   isolate the flowlike effect.}},
