@article{ ISI:000321913800012,
Author = {de la Cruz-May, L. and Mejia, E. B. and Benavides, O. and Vasquez
   Jimenez, J. and Castro-Chacon, J. and May-Alarcon, M.},
Title = {{Novel Technique for Obtaining the Raman Gain Efficiency of Silica Fibers}},
Journal = {{IEEE PHOTONICS JOURNAL}},
Year = {{2013}},
Volume = {{5}},
Number = {{4}},
Month = {{AUG}},
Abstract = {{We present a simple novel technique that determines with precision the
   Raman gain efficiency for telecom fibers. The method is supported by a
   mathematical relation, which is the solution for the coupled
   differential equations that govern the stimulated Raman scattering. This
   technique is valuable due to its simplicity and consists of measuring
   the residual pump and the Raman scattering signals at the stimulated
   Raman scattering threshold. Our technique was proven for different
   fibers; as a result, we found that the rate of pump power to stimulated
   Raman scattering at threshold is lower than similar to 16, which is the
   historically used value.}},
Publisher = {{IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC}},
Address = {{445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA}},
Type = {{Article}},
