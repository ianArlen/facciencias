@article{ ISI:000242276800011,
Author = {van Marle, A. J. and Langer, N. and Achterberg, A. and Garcia-Segura, G.},
Title = {{Forming a constant density medium close to long gamma-ray bursts}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
