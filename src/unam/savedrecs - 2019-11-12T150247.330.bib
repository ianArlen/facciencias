@article{ ISI:000244732200005,
Author = {Ortega-Salas, A. A. and Reyes-Bustamante, H.},
Title = {{Fecundity, survival, and growth of the seahorse Hippocampus ingens
   (Pisces : Syngnathidae) under semi-controlled conditions}},
Journal = {{REVISTA DE BIOLOGIA TROPICAL}},
Year = {{2006}},
Volume = {{54}},
Number = {{4}},
Pages = {{1099-1102}},
Month = {{DEC}},
Abstract = {{We studied fecundity, survival, and growth of the seahorse Hippocampus
   ingens under semi-controlled conditions. Three wild brood stock mature
   males of 14.8, 24.5, and 32.0 g released 1 598, 1 703, and 1 658
   juveniles. Juvenile stocking densities of 12 were settled in 60-1
   aquariums in groups of 1, 12, and 20 days old organisms. The rate of
   survival was 21.5, 61.9, and 59.0 \%, respectively, in 35 days.
   Juveniles were fed a mix diet of rotifers B. plicatilis and Artemia
   nauphi, then they were transferred to a cement tank of 100 000 1 at a
   density of 50/1 000 1 and fed with live adult Artemia for 60 days more.
   They grew from an average of 0.7, 1.5, and 2.7 to 4.5, 5.4, and 6.7 cm,
   respectively, in 95 days. The seawater temperature varied from 17 to 23
   degrees C.}},
