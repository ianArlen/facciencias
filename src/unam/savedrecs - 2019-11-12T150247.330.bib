@article{ ISI:000241754400008,
Author = {Ramirez-Galicia, Guillermo and Rubio, Manuel F. and Jimenez-Cruz,
   Federico},
Title = {{Intramolecular competition between alkenes in ketene-alkene {[}2+2]
   cycloaddition. A theoretical study}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE-THEOCHEM}},
Year = {{2006}},
Volume = {{773}},
Number = {{1-3}},
Pages = {{53-58}},
Month = {{OCT 30}},
Abstract = {{In this paper, we describe the results of the intramolecular
   ketene-alkene {[}2+2] cycloaddition in
   2-pent-4-enyl-octa-1,7-dien-l-one, at HF/6-31+G{*} and
   DFT-B3LYP/6-31+G{*} computational levels of theory. For this reaction,
   we have explored the four possible pathways at a supra-antara approach
   in which four possible products: 5-4-, and 6-4-membered fused {[}n.2.0]
   bicycle rings; 6-4-, and 7-4-membered fused {[}n.1.1] bicycle rings. In
   all cases, an asynchronous process was found with an appreciable
   interaction of the ketene central carbon with both carbons from the
   alkenes. We demonstrated that the reaction is driven towards the
   formation of 5-4 member rings instead of 6-4 member rings. In addition,
   the internal molecular orbital HOMO-2 was identified as the molecular
   orbital with higher nucleophilic reactivity. (c) 2006 Elsevier B.V. All
   rights reserved.}},
