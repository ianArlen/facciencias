@article{ ISI:000249759500006,
Author = {Cosio, F. Arambula and Castaneda, M. A. Padilla and Martinez, P. R.
   Sevilla},
Title = {{Computer assisted prostate surgery training}},
Journal = {{INTERNATIONAL JOURNAL OF HUMANOID ROBOTICS}},
Year = {{2006}},
Volume = {{3}},
Number = {{4}},
Pages = {{485-498}},
Month = {{DEC}},
Abstract = {{We report the current development of a new computer assisted system for
   local and remote training of transurethral resection of the prostate
   (TURP). The system is based on a 3D computer model of the prostate
   gland, which simulates deformations and resections of prostatic tissue.
   Simulated resections of the prostate can be repeatedly performed by the
   residents of urology to enhance their training. Automatic prostate
   recognition software was developed for the construction of a large
   library of prostate models. We report the software development of the
   system and show promising preliminary results of a new prostate
   recognition algorithm.}},
Publisher = {{WORLD SCIENTIFIC PUBL CO PTE LTD}},
Address = {{5 TOH TUCK LINK, SINGAPORE 596224, SINGAPORE}},
