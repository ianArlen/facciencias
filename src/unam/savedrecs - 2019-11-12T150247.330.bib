@inproceedings{ ISI:000251337400057,
Author = {Barea, J. and Frank, A. and Hirsch, J. G. and Lopez, J. C. and Morales,
   I. and Mendoza, J. and Velazquez, V.},
Editor = {{Alarcon, R and Cole, PL and Djalali, C and Umeres, F}},
Title = {{Calculation of nuclear masses using image reconstruction techniques}},
