@article{ ISI:000242014200012,
Author = {Stehlik, Matej},
Title = {{Minimal connected tau-critical hypergraphs}},
Journal = {{GRAPHS AND COMBINATORICS}},
Year = {{2006}},
Volume = {{22}},
Number = {{3}},
Pages = {{421-426}},
Month = {{NOV}},
Abstract = {{A hypergraph H is tau-critical if tau (H - \{E\}) < tau(H) for every
   edge is an element of H, where tau(H) denotes the transversal number of
   H. We show that if H is a connected tau-critical hypergraph, then H -
   \{E\} can be partitioned into tau(H) - 1 stars of size at least two, for
   every edge E is an element of H. An immediate corollary is that a
   connected tau-critical hypergraph has at least 2 tau(H) - 1 edges. This
   extends, in a very natural way, a classical theorem of Gallai on
   colour-critical graphs, and is equivalent to a theorem of Furedi on
   t-stable hypergraphs. We deduce a lower bound on the size of
   tau-critical hypergraphs of minimum degree at least two.}},
