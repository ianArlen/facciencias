@article{ ISI:000243077400004,
Author = {Gonzalez-Montiel, Simplicio and Andrade-Lopez, Noemi and
   Garcia-Montalvo, Veronica and Cogordan, Juan Antonio and
   Alvarado-Rodriguez, Jose G.},
Title = {{Pentacoordination at germanium by transannular bonding of sulfur or
   oxygen in an eight-membered ring: An experimental and theoretical study}},
Journal = {{EUROPEAN JOURNAL OF INORGANIC CHEMISTRY}},
Year = {{2006}},
Number = {{23}},
Pages = {{4752-4760}},
Month = {{DEC 4}},
Abstract = {{Treatment of LnGeCl4-n, with D(C6H4SH)(2) in the presence of nBuLi leads
   to the formation of the stable compounds D(C6H4S)(2)GeL1L2 (for D = S,
   L-1 = Cl, L-2 = Ph, 1 and L-1 = L-2 = Ph, 3; for D = O, L-1 = Cl, L-2 =
   Et, 4 and L-1 = L-2 = Ph, 5). The S(C6H4S)(2)Ge(Ph)Br compound (2) has
   been synthesised by halogen exchange from 1 and potassium bromide. X-ray
   structure determinations of complexes 1-5 reveal that the germanium atom
   acts as an acceptor atom displaying an intramolecular transannular
   interaction with the chalcogen D atom. The geometry of the
   pentacoordinate Ge atom in the title compounds is described as distorted
   trigonal bipyramidal with a 64-31 \% distortion displacement. The D ->
   Ge (D = S, O) hypercoordinate interaction was studied by DFT methods
   using correlation consistent basis sets and relativistic ECP for Ge. NBO
   analysis showed that this interaction can be explained in terms of the
   interplay of covalent and Coulombic interactions. (c) Wiley-VCH Verlag
   GmbH \& Co.}},
