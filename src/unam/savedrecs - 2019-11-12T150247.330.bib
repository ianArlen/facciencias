@article{ ISI:000241307400061,
Author = {Gomez-Ros, J. M. and Furetta, C. and Cruz-Zaragoza, E. and Lis, M. and
   Torres, A. and Monsivais, G.},
Title = {{Dose dependence and thermal stability of the thermoluminescence emission
   in inorganic dust from mint and camomile}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
Year = {{2006}},
Volume = {{566}},
Number = {{2}},
Pages = {{727-732}},
Month = {{OCT 15}},
Abstract = {{The inorganic phase extracted from mint and camomile samples obtained
   from commercial products in Mexico was selected according to different
   grain sizes and exposed to Co-60 gamma radiation at different doses in
   the range 0.5-12 kGy. Thermoluminescence (TL) glow curves show a single
   broad peak, centred around 175 degrees C for prompt readouts. X-ray
   diffraction analysis indicates the inorganic dust is mainly composed by
   quartz, Na,K-feldspars and amphiboles, which use to be characterized by
   TL emissions associated to continuous distribution of trapping centres.
   The high fading of the TL signal during the first days of storage at
   room temperature can be related to the shallowest part of the
   distribution while the deepest traps originate the asymptotic behaviour
   for longer storage times. The TL intensity also increases
   significatively with the grain size, being linear with the absorbed dose
   at least up to 10 kGy. (c) 2006 Elsevier B.V. All rights reserved.}},
