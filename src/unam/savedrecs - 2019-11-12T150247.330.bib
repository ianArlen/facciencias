@article{ ISI:000245042300010,
Author = {Hernandez-Alcantara, Pablo and Perez-Mendoza, Alma Yazmin and
   Solis-Weiss, Vivianne},
Title = {{Description of three new species of Ninoe and Cenogenus (Polychaeta :
   Lumbrineridae) from the Mexican Pacific}},
Journal = {{SCIENTIA MARINA}},
Year = {{2006}},
Volume = {{70}},
Number = {{3}},
