@article{ ISI:000242908500012,
Author = {Buch, A. and Glavin, D. P. and Sternberg, R. and Szopa, C. and Rodier,
   C. and Navarro-Gonzalez, R. and Raulin, F. and Cabane, M. and Mahaffy,
   P. R.},
Title = {{A new extraction technique for in situ analyses of amino and carboxylic
   acids on Mars by gas chromatography mass spectrometry}},
Journal = {{PLANETARY AND SPACE SCIENCE}},
Year = {{2006}},
Volume = {{54}},
Number = {{15}},
Pages = {{1592-1599}},
