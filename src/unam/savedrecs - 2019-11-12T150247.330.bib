@article{ ISI:000244652000011,
Author = {Palomares, L. O. and Reyes, J. A.},
Title = {{Guiding of optical fields in a smectic B liquid crystal cylindrical
   fiber}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2006}},
Volume = {{52}},
Number = {{5, S}},
Pages = {{71-79}},
Month = {{NOV}},
Note = {{7th Ibero-American Workshop on Complex Fluids and their Applications,
   Playa del Carmen, MEXICO, OCT 03-06, 2005}},
Abstract = {{The propagation of optical fields in a cylindrical fiber of smectic B
   liquid crystal with banana-shaped molecules is studied. We consider a
   cylinder of banana-smectic liquid crystal whose tilt angle is assumed to
   be uniform, surrounded by an infinite homogeneous isotropic cladding. We
   perform an analytic description of the optical field propagation within
   the guide for Transverse Magnetic modes (TM) for a low intensity optical
   beam. We take into account the coupling between the optical field and
   the orientational configuration of the smectic. Eikonal equations are
   derived and ray trajectories are analyzed in their corresponding
   Hamiltonian representation, in the optical limit. Finally, by solving
   Maxwell's equations analytically, we obtain the electromagnetic modes
   and the spatial electromagnetic energy distribution within the guide.}},
