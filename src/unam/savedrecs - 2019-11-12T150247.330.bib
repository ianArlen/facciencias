@article{ ISI:000242262000006,
Author = {Mello, P. A. and Yepez, M. and Froufe-Perez, L. S. and Saenz, J. J.},
Title = {{Statistical scattering of waves in disordered waveguides: Universal
   properties}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2006}},
Volume = {{372}},
Number = {{2}},
Pages = {{203-209}},
