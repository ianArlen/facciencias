@article{ ISI:000250546900070,
Author = {van Marle, A. J. and Langer, N. and Garcia-Segura, G.},
Title = {{Models of the circumstellar medium around gamma-ray burst progenitors}},
Journal = {{NUOVO CIMENTO DELLA SOCIETA ITALIANA DI FISICA B-GENERAL PHYSICS
   RELATIVITY ASTRONOMY AND MATHEMATICAL PHYSICS AND METHODS}},
Year = {{2006}},
Volume = {{121}},
Number = {{12}},
Pages = {{1607-1608}},
Month = {{DEC}},
Note = {{Workshop on Swift and GRBs - Unveiling the Relativistic University,
   Venice, ITALY, JUN 05-09, 2006}},
Abstract = {{We present two ways to obtain information on the circumstellar medium of
   long gamma-Ray Burst (GRB) progenitors. One: if the afterglow spectrum
   of the GRB shows blue-shifted absorption lines,. these can be related to
   hydrodynamical interactions between the stellar wind of the progenitor
   and its environment. Two: the afterglow light curve is directly related
   to the density profile of the circumburst medium. This can either
   decrease with the square of the distance as should be-expected for an
   expanding stellar wind medium or be constant, in which case we think
   that the GRB jet is expanding in a shocked wind region. In turn, an
   understanding of the circumstellar medium of the GRB progenitor can help
   understand the evolution of the progenitor star.}},
