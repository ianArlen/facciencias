@article{ ISI:000250546900047,
Author = {Nava, L. and Ghisellini, G. and Ghirlanda, G. and Tavecchio, F. and
   Firmani, C.},
Title = {{Wind circumburst density profile: A linear E-p-E-y correlation({*})}},
Journal = {{NUOVO CIMENTO DELLA SOCIETA ITALIANA DI FISICA B-BASIC TOPICS IN PHYSICS}},
Year = {{2006}},
Volume = {{121}},
Number = {{12}},
Pages = {{1541-1542}},
Month = {{DEC}},
Note = {{Workshop on Swift and GRBs - Unveiling the Relativistic University,
   Venice, ITALY, JUN 05-09, 2006}},
Abstract = {{Chirlanda et al. (2004) derived the collimation-corrected energy E, for
   a sample of 1.5 bursts under the assumption of a homogeneous circumburst
   density profile. They found a correlation (the so-called Ghirlanda
   correlation) between E, and the rest frame peak energy of the F-v(v)
   prompt spectrum (E-p). Nava et al. (2006) showed that, assuming a
   circumburst density distributed with a r(-2) wind profile, the Ghirlanda
   correlation remains tight and becomes linear. This implies that: i) it
   remains linear also in the comoving frame, no matter the distribution of
   bulk Lorentz factors, ii) it entails that different bursts have the same
   number of relevant photons. We have updated these findings including
   recently detected bursts (21 in total), stressing the two important
   implications.}},
