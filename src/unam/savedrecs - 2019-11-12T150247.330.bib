@article{ ISI:000242262000012,
Author = {Alatriste, Fernando R. and Mateos, Jose L.},
Title = {{Phase synchronization in tilted deterministic ratchets}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2006}},
Volume = {{372}},
Number = {{2}},
Pages = {{263-271}},
Month = {{DEC 15}},
