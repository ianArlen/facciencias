@article{ ISI:000245965800006,
Author = {Estevez, Miriam and Vargas, Susana and Lobland, Haley E. Hagg and de la
   Isla, Agustin and Brostow, Witold and Rodriguez, J. Rogelio},
Title = {{Characterization of novel dental obturation materials}},
Journal = {{MATERIALS RESEARCH INNOVATIONS}},
Year = {{2006}},
Volume = {{10}},
Number = {{4}},
Pages = {{117+}},
Month = {{DEC}},
Abstract = {{Abrasion behavior of new nanohybrid polymer + ceramic dental materials
   designed for filling dental cavities is reported. The polymer matrix is
   a polyurethane while the dispersed phases include particles of silica
   and alumina with the diameters below 100 nm. Several commercial
   obturation materials are investigated by the same technique. Our new
   nanohybrids show clearly superior resistance to abrasion wear-in one
   case some 20 times better than a commercial sample. Moreover, our new
   materials exhibit low toxicity, short curing times (below 10 minutes),
   good dimensional stability (low contraction), high chemical resistance,
   good pigmentation capacity, and are easy to prepare and show good
   adhesion to the substrate (dentine or enamel).}},
