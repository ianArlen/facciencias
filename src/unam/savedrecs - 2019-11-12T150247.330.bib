@article{ ISI:000243149700059,
Author = {Apatiga, L. M. and Rubio, E. and Rivera, E. and Castano, V. M.},
Title = {{Surface morphology of nanostructured anatase thin films prepared by
   pulsed liquid injection MOCVD}},
