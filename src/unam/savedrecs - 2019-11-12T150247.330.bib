@article{ ISI:000241893800005,
Author = {Drago-Serrano, Maria Elisa and Parra, Sandra Gavilanes and
   Manjarrez-Hernandez, H. Angel},
Title = {{EspC, an autotransporter protein secreted by enteropathogenic
   Escherichia coli (EPEC), displays protease activity on human hemoglobin}},
Journal = {{FEMS MICROBIOLOGY LETTERS}},
Year = {{2006}},
Volume = {{265}},
Number = {{1}},
Pages = {{35-40}},
Month = {{DEC}},
Abstract = {{Some enteropathogenic Escherichia coli (EPEC) strains, which are an
   important cause of diarrhea among infants, secrete a serine protease
   autotransporter protein called EspC. The pathogenic role of EspC upon
   EPEC infection is unknown. In this study, we demonstrated that purified
   EspC protein, obtained from supernatants of EPEC cultures, interacted
   with hemoglobin and degraded it. Moreover, we have shown that EspC is a
   hemin-binding protein. We hypothesized that hemoglobin proteolysis by
   EspC may contribute to the utilization of heme and hemoglobin iron for
   bacterial growth.}},
