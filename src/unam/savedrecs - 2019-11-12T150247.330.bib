@article{ ISI:000243166700069,
Author = {Lu, M. and Alna'washi, G. and Habibi, M. and Gharaibeh, M. F. and
   Phaneuf, R. A. and Kilcoyne, A. L. D. and Levenson, E. and Schlachter,
   A. S. and Cisneros, C. and Hinojosa, G.},
Title = {{Photoionization and electron-impact ionization of Kr3+}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2006}},
Volume = {{74}},
Number = {{6}},
Month = {{DEC}},
Abstract = {{Absolute photoionization cross sections for Kr3+ were measured in the
   energy range 39.05-143.2 eV for single ionization and 120.6-137.7 eV for
   double ionization. For comparison, an electron-impact single-ionization
   measurement was made in the energy range 43.1-179.1 eV and normalized to
   previously published absolute measurements. The Flexible Atomic Code and
   Cowan atomic structure codes were used to calculate energy levels,
   excitation energies and oscillator strengths for 3d -> np, 3d -> nf, and
   4s -> np autoionizing transitions from the ground and metastable states.
   From the single-photoionization measurements, ionization thresholds of
   the P-2(3/2)o, D-2(5/2)o metastable states and S-4(3/2)o ground state
   were measured to be 46.62 +/- 0.02, 48.59 +/- 0.01, and 50.70 +/- 0.02
   eV, nearly 2 eV lower than National Institute of Standards and
   Technology tabulated values. Within the experimental uncertainty,
   oscillator strengths determined from the photoionization measurements
   are in agreement with both calculations. Excitation-autoionization and
   resonant-excitation double-autoionization features are evident in the
   electron-impact ionization cross section.}},
