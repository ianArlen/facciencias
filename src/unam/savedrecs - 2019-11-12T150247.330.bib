@article{ ISI:000241300900021,
Author = {Page, Dany and Geppert, Ulrich and Weber, Fridolin},
Title = {{The cooling of compact stars}},
Journal = {{NUCLEAR PHYSICS A}},
Year = {{2006}},
Volume = {{777}},
Pages = {{497-530}},
Month = {{OCT 17}},
Abstract = {{The cooling of a compact star depends very sensitively on the state of
   matter at supranuclear densities, which essentially controls the
   neutrino emission, as well as on the structure of the stellar outer
   layers which control the photon emission. Open issues concern the
   hyperon population, the presence of meson condensates, superfluidity and
   superconductivity, and the transition of confined hadronic matter to
   quark matter. This paper describes these issues and presents cooling
   calculations based on a broad collection of equations of state for
   neutron star matter and strange matter. These results are tested against
   the body of observed cooling data. (c) 2005 Elsevier B.V. All rights
   reserved.}},
