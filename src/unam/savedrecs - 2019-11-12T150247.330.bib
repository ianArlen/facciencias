@article{ ISI:000244652000008,
Author = {Gallardo, C. and Salinas-Rodriguez, E. and Zamora, J. M. and Rodriguez,
   R. F.},
Title = {{Stochastic modeling of alginate production by Azotobacter Vinelandii}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2006}},
Volume = {{52}},
