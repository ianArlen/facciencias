@article{ ISI:000243165300010,
Author = {Huerta, Adrian and Henderson, Douglas and Trokhymchuk, Andrij},
Title = {{Freezing of two-dimensional hard disks}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2006}},
Volume = {{74}},
Number = {{6, 1}},
Month = {{DEC}},
Abstract = {{We argue that structural rearrangements experienced by an assembly of
   hard disks under increasing disk density are accompanied by the mutual
   caging of each disk by its three alternating Voronoi nearest neighbors.
   This caging becomes effective at a packing fraction eta=pi root 3/8
   approximate to 0.680 when the average gap width between neighboring
   disks in the system shrinks to about 15\% of the disk diameter. The
   freezing occurs when the fraction of caged disks is about 40\%.}},
