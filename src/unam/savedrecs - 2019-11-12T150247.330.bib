@article{ ISI:000244955800008,
Author = {Sevostianov, Igor and Sabina, Federico J. and Bravo-Castillero, Julian
   and Guinovart-Diaz, Raul and Rodriguez-Ramos, Reinaldo},
Title = {{Cross-property connections for fiber-reinforced composites with
   transversely isotropic constituents}},
Journal = {{INTERNATIONAL JOURNAL OF FRACTURE}},
Year = {{2006}},
Volume = {{142}},
Number = {{3-4}},
Pages = {{299-306}},
Month = {{DEC}},
Abstract = {{The paper addresses the problem of the connection between effective
   elastic and conductive properties of a fiber reinforced material with
   both phases (the matrix and the fibers) being transversely-isotropic.
   The exact solution for a square array of fibers and approximate
   solutions for randomly located parallel fibers are constructed and
   compared. The results allow one to predict the entire set of macroscopic
   elastic stiffnesses through one or two measurement of thermal, or
   electrical conductivity. As a side result, it is shown that the mutual
   positions of inhomogeneities produce only a minor effect and that
   applicability of the non-interaction approximation is much wider than
   expected.}},
