@article{ ISI:000241888000049,
Author = {Campos, I. and Torres, R. and Ramirez, G. and Ganem, R. and Martinez, J.},
Title = {{Growth kinetics of iron boride layers: Dimensional analysis}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{2006}},
Volume = {{252}},
Number = {{24}},
Pages = {{8662-8667}},
Month = {{OCT 15}},
Abstract = {{Dimensional analysis is presented as a powerful tool in the study of the
   paste boriding process. In particular, a dimensional method is used to
   study the growth kinetics of the boride layers FeB and Fe2B. Experiments
   were performed in AISI 1045 steel and AISI M2 steel, to test the
   suggested model. Samples of 1045 steel were prepared and treated using
   boron paste thickness of 3-5 mm, at temperatures of 1193, 1223 and 1273
   K, with 2, 4 and 6 It of treatment time. The M2 specimens had boron
   paste thickness of 3 and 4 turn and temperatures of 1223, 1253 and 1273
   K for 2 and 6 h. Results indicate that the growth of boron layers obeys
   power laws of the form y = alpha x(beta) where alpha and beta constants
   are a function of the material and the interface of interest. Validation
   of the model was carried out using experimental data with an average
   error percentage of 7.6\% for Fe2B in 1045 steel, 15.8\% for FeB and
   3.4\% for Fe2B in M2 steel. (c) 2005 Elsevier B.V. All rights reserved.}},
