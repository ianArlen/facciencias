@article{ ISI:000242262000007,
Author = {Barrio, R. A. and Varea, C.},
Title = {{Non-linear systems}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2006}},
