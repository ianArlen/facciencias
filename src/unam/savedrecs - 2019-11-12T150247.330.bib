Author = {Lopez, Erna and Bocco, Gerardo and Mendoza, Manuel and Velazquez,
   Alejandro and Aguirre-Rivera, J. Rogelio},
Title = {{Peasant emigration and land-use change at the watershed level: A
   GIS-based approach in Central Mexico}},
