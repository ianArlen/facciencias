@article{ ISI:000338912100009,
Author = {Gomez, Samuel and Neptali Morales-Serna, F.},
Title = {{Updated checklist of published and unpublished records of harpacticoid
   copepods (Crustacea: Copepoda: Harpacticoida) from Mexico}},
Journal = {{PROCEEDINGS OF THE BIOLOGICAL SOCIETY OF WASHINGTON}},
