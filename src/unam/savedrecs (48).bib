@article{ ISI:000347944200004,
Author = {Sotelo Valencia, Adrian},
Title = {{Latin America: Dependency and Super-Exploitation}},
Journal = {{CRITICAL SOCIOLOGY}},
Year = {{2014}},
Volume = {{40}},
Number = {{4}},
Pages = {{539-549}},
Month = {{JUL}},
Abstract = {{Dependency theory in the tradition of Ruy Mauro Marini emphasized the
   super-exploitation of labor and helped direct attention to capital's
   quest to further expropriate part of the consolidated consumption fund
   historically won by labor. Marini's work represented a significant
   departure from the ECLAC conception of vulnerabilities exhibited by
   developing countries, opting instead to take Lenin's theory of
   imperialism as the key point of departure for analyzing dependency.
   Marini was critiqued on intellectual grounds by more conservative
   dependency theorists such as Cardoso, Serra and Cueva, consequently
   blunting the critical leading edge of dependency theory. This
   exploration of Marini's critical Marxist formulation helps contextualize
   the continuing relevance of dependency theory for comprehending the
   ongoing class struggle in 21st century Latin America.}},
