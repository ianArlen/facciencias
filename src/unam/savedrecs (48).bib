@article{ ISI:000340346400004,
Author = {Mendoza-Barcenas, M. A. and Vicente-Vivas, E. and Rodriguez-Cortes, H.},
Title = {{Mechatronic Design, Dynamic Modeling and Results of a Satellite Flight
   Simulator for Experimental Validation of Satellite Attitude
   Determination and Control Schemes in 3-Axis}},
Journal = {{JOURNAL OF APPLIED RESEARCH AND TECHNOLOGY}},
