@article{ ISI:000338283000055,
Author = {Larrieta-Carrasco, Elena and Acuna-Alonzo, Victor and Velazquez-Cruz,
   Rafael and Barquera-Lozano, Rodrigo and Leon-Mimila, Paola and
   Villamil-Ramirez, Hugo and Menjivar, Marta and Romero-Hidalgo, Sandra
   and Mendez-Sanchez, Nahum and Cardenas, Vanessa and Banuelos-Moreno,
