@article{ ISI:000339115800075,
Author = {Allen, Christine and Monroy-Rodriguez, Miguel A.},
Title = {{AN IMPROVED CATALOG OF HALO WIDE BINARY CANDIDATES}},
Journal = {{ASTROPHYSICAL JOURNAL}},
