@article{ ISI:000339547200025,
Author = {Cervantes-Garcia, David and Rubalcaba-Sil, Jose Luis and
   Gonzalez-Mendoza, Daniel and Aviles-Marin, Monica},
Title = {{Assessment of copper bioaccumulation in Euglena gracilis by X-Ray
   fluorescence technique}},
Journal = {{LATIN AMERICAN JOURNAL OF AQUATIC RESEARCH}},
Year = {{2014}},
Volume = {{42}},
Number = {{3}},
Pages = {{662-665}},
Month = {{JUL}},
Abstract = {{The effect of the exposure to copper in metal bioaccumulation capacity
   in cells from Euglena gracilis was evaluated in this study. Metal
   accumulation measured with the X-ray fluorescence (XRF) technique showed
   that the accumulation of Cu+2 in E. gracilis was dose dependent and was
   significantly increased in cells treated with 0.4 and 0.8 mM of Cu+2
   with respect to control. The presence of an accumulation strategy in E.
   gracilis that could involve the participation of a number of multiple
   processes, such as production of vacuoles, is not discarded. Future
   studies should be oriented to evaluate the potential for bioaccumulation
   of E. gracilis to be applied in bioremediation of aquatic systems
   programs.}},
