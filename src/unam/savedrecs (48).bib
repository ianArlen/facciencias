@article{ ISI:000339171100008,
Author = {Moreno, Ivan and Avendano-Alejo, Maximino and Saucedo-A, Tonatiuh and
   Bugarin, Alejandra},
Title = {{Modeling LED street lighting}},
Journal = {{APPLIED OPTICS}},
Year = {{2014}},
Volume = {{53}},
Number = {{20}},
Pages = {{4420-4430}},
Month = {{JUL 10}},
Abstract = {{LED luminaires may deliver precise illumination patterns to control
   light pollution, comfort, visibility, and light utilization efficiency.
   Here, we provide simple equations to determine how the light distributes
   in the streets. In particular, we model the illuminance spatial
   distribution as a function of Cartesian coordinates on a floor, road, or
   street. The equations show explicit dependence on the luminary position
   (pole height and arm length), luminary angle (fixture tilt), and the
   angular intensity profile (radiation pattern) of the LED luminary. To
   achieve this, we propose two mathematical representations to model the
   sophisticated intensity profiles of LED luminaries. Furthermore, we
   model the light utilization efficiency, illumination uniformity, and
   veiling luminance of glare due to one or several LED streetlamps. (C)
   2014 Optical Society of America}},
