@article{ ISI:000338183900008,
Author = {Varejao, Jodieh O. S. and Barbosa, Luiz C. A. and Varejao, Eduardo V. V.
   and Maltha, Celia R. A. and King-Diaz, Beatriz and Lotina-Hennsen, Blas},
Title = {{Cyclopent-4-ene-1,3-diones: A New Class of Herbicides Acting as Potent
   Photosynthesis Inhibitors}},
