@article{ ISI:000333728300002,
Author = {Aichholzer, Oswin and Fabila-Monroy, Ruy and Gonzalez-Aguilar, Hernan
   and Hackl, Thomas and Heredia, Marco A. and Huemer, Clemens and Urrutia,
   Jorge and Vogtenhuber, Birgit},
Title = {{4-Holes in point sets}},
Journal = {{COMPUTATIONAL GEOMETRY-THEORY AND APPLICATIONS}},
Year = {{2014}},
Volume = {{47}},
Number = {{6}},
Pages = {{644-650}},
Month = {{AUG}},
Note = {{27th European Workshop on Computational Geometry (EuroCG), Morschach,
   SWITZERLAND, MAR 28-30, 2011}},
Abstract = {{We consider a variant of a question of Erdos on the number of empty
   k-gons (k-holes) in a set of n points in the plane, where we allow the
   k-gons to be non-convex. We show bounds and structural results on
   maximizing and minimizing the number of general 4-holes, and maximizing
   the number of non-convex 4-holes. In particular, we show that for n >=
   9, the maximum number of general 4-holes is ((pi)(4)); the minimum
   number of general 4-holes is at least 5/2 n(2) - circle minus(n); and
   the maximum number of non-convex 4-holes is at least 1/2 n(3) - circle
   minus(n(2) logn) and at most 1/2 n(3) - circle minus(n(2)). 2014 (c)
   Elsevier B.V. All rights reserved.}},
