@article{ ISI:000339997800003,
Author = {Ariadna Perez, Nora and Lima, Enrique and Bosch, Pedro and Mendez-Vivar,
   Juan},
Title = {{Consolidating materials for the volcanic tuff in western Mexico}},
Journal = {{JOURNAL OF CULTURAL HERITAGE}},
Year = {{2014}},
Volume = {{15}},
Number = {{4}},
Pages = {{352-358}},
Month = {{JUL-AUG}},
Abstract = {{The diversity of volcanic tuff buildings standing in Western Mexico is
   large, therefore, it was considered relevant to evaluate and synthetize
   different stone consolidants that could be applied in the regional
   weather conditions and renew the stability of those buildings; since the
   commercial products are developed and tested in foreign countries, and
   in some cases, when applied under the weather conditions of Mexico, the
   results differ from those reported. For the altered tuffs from Santa
   Monica's Church in Guadalajara, Mexico, the deterioration mechanism of
   the local tuff was determined, thus a silicate and aluminosilicate
   consolidants were synthesized by the sol-gel method and applied to
   altered tuffs in order to evaluate their performance from structural to
   macroscopic level. (C) 2013 Elsevier Masson SAS. All rights reserved.}},
