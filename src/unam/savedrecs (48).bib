@article{ ISI:000338766100006,
Author = {Bejarano, Marylin and Crosby, Maria M. and Parra, Victor and Etchevers,
   Jorge D. and Campo, Julio},
Title = {{Precipitation Regime and Nitrogen Addition Effects on Leaf Litter
   Decomposition in Tropical Dry Forests}},
Journal = {{BIOTROPICA}},
Year = {{2014}},
Volume = {{46}},
Number = {{4}},
Pages = {{415-424}},
Month = {{JUL}},
Abstract = {{We conducted a year-long field experiment to investigate how nitrogen
   addition affected decomposition of Piscidia piscipula and Gymnopodium
   floribundum along a precipitation gradient in the Yucatan Peninsula,
   Mexico. Nitrogen addition did not affect decomposition rates at the
   drier sites. However, fertilization at the wettest site increased the
   decomposition of Gymnopodium litter and decreased the decomposition of
   Piscidia litter. Water-soluble carbon and lignin, and water-soluble
   carbon and nitrogen concentrations were the best predictors of
   decomposition for Gymnopodium and Piscidia litters, respectively. We
   conclude that the effects of nitrogen addition on decomposition will
   vary from site to site as a function of mean annual precipitation,
   inherent soil fertility, and species identity.}},
