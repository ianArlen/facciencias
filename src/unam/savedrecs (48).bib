@article{ ISI:000339324900004,
Author = {Jeronimo-Castro, Jesos and Ruiz-Hernandez, Gabriel and Tabachnikov,
   Sergei},
Title = {{The equal tangents property}},
Journal = {{ADVANCES IN GEOMETRY}},
Year = {{2014}},
Volume = {{14}},
Number = {{3}},
Pages = {{447-453}},
Month = {{JUL}},
Abstract = {{Let M be a C-2-smooth strictly convex closed surface in R-3 and denote
   by H the set of points x in the exterior of M such that all the tangent
   segments from x to M have equal lengths. In this note we prove that if H
   is either a closed surface containing M or a plane, then M is a
   Euclidean sphere. Moreover, we shall see that the situation in the
   Euclidean plane is very different.}},
