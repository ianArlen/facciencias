@article{ ISI:000338401200007,
Author = {Manrique-Arias, Juan C. and Avila-Rodriguez, Miguel A.},
Title = {{A simple and efficient method of nickel electrodeposition for the
   cyclotron production of Cu-64}},
Journal = {{APPLIED RADIATION AND ISOTOPES}},
Year = {{2014}},
Volume = {{89}},
Pages = {{37-41}},
Month = {{JUL}},
Abstract = {{Nickel targets for the cyclotron production of Cu-64 were prepared by
   electrodeposition on a gold backing from nickel chloride solutions using
   boric acid as buffer. Parameters studied were nickel chloride and boric
   acid concentration, temperature and current density. All plating
   conditions studied were successful obtaining efficiencies of
   approximately 90\% in 2-3 h, reaching almost quantitative plating ( >
   97\%) in 10-20 h depending on the current density. All plated targets
   withstood proton irradiations up to 40 AA for 2 h. Recovered nickel was
   successfully recycled and reused with an overall efficiency > 95\%. (C)
   2014 Elsevier Ltd. All rights reserved.}},
