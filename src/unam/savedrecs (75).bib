@article{ ISI:000286093900013,
Author = {Hamilton, Marcus J. and Davidson, Ana D. and Sibly, Richard M. and
   Brown, James H.},
Title = {{Universal scaling of production rates across mammalian lineages}},
Journal = {{PROCEEDINGS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES}},
Year = {{2011}},
Volume = {{278}},
Number = {{1705}},
Pages = {{560-566}},
Month = {{FEB 22}},
Abstract = {{Over many millions of years of independent evolution, placental,
   marsupial and monotreme mammals have diverged conspicuously in
   physiology, life history and reproductive ecology. The differences in
   life histories are particularly striking. Compared with placentals,
   marsupials exhibit shorter pregnancy, smaller size of offspring at birth
   and longer period of lactation in the pouch. Monotremes also exhibit
   short pregnancy, but incubate embryos in eggs, followed by a long period
   of post-hatching lactation. Using a large sample of mammalian species,
   we show that, remarkably, despite their very different life histories,
   the scaling of production rates is statistically indistinguishable
   across mammalian lineages. Apparently all mammals are subject to the
   same fundamental metabolic constraints on productivity, because they
   share similar body designs, vascular systems and costs of producing new
   tissue.}},
