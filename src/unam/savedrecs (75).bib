@article{ ISI:000288024000010,
Author = {Castillo-Rojas, S. and Ignes-Mullol, J. and Sagues, F.},
Title = {{Influence of initial composition changes caused by UV and gamma
   irradiation on wave propagation in the Belousov-Zhabotinsky reaction}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{2011}},
Volume = {{504}},
Number = {{4-6}},
Pages = {{162-164}},
Month = {{MAR 10}},
Abstract = {{The aim of this work is to look for the effects of controlled UV and
   gamma irradiation of reactants on chemical wave propagation in the BZ
   reaction. We use a quasi-2D gel layered system to prevent convection
   effects. We monitored the onset of wave propagation and measured the
   velocities and number of generated wave-fronts. Results are compared
   with the non-irradiated BZ system. UV totally suppresses the induction
   period, while gamma radiation may affect both the induction period and
   propagation parameters. Results are interpreted in terms of some of the
   most critical intermediate species of the BZ mechanism. (C) 2011
   Elsevier B.V. All rights reserved.}},
