@article{ ISI:000287451400002,
Author = {Arias-Almeida, Juan Carlos and Rico-Martinez, Roberto},
Title = {{Inhibition of two enzyme systems in Euchlanis dilatata (Rotifera:
   Monogononta) as biomarker of effect of metals and pesticides}},
Journal = {{BIOMARKERS}},
Year = {{2011}},
Volume = {{16}},
Number = {{1}},
Pages = {{12-19}},
Month = {{FEB}},
Abstract = {{The inhibitory effects on esterases and phospholipase A2 (PLA2) in the
   freshwater rotifer Euchlanis dilatata, native to Mexico, were assessed
   by fluorimetry after in vivo exposure (30 min) in laboratory conditions
   to sublethal concentrations of metals and pesticides. EC(50) values for
   esterases ranged from 7.9 x 10(-7) for DDT to 61.9 mu g l(-1) for methyl
   parathion, while corresponding values for PLA2 ranged from 0.96 x 10(-6)
   for mercury to 69.2 mu g l(-1) for lead. These enzyme systems in E.
   dilatata are very sensitive to the tested agents and suggest they would
   be suitable biomarkers. However, sensitivity to other environmental
   contaminants should be investigated in laboratory conditions and field
   studies to assess their potential as environmental biomarkers.}},
