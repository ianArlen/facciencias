@article{ ISI:000286730900006,
Author = {Sussman, Roberto A. and Izquierdo, German},
Title = {{A dynamical system study of the inhomogeneous Lambda-CDM model}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
