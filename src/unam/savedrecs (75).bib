@article{ ISI:000287968000024,
Author = {Lopez-Juarez, Rigoberto and Gonzalez-Garcia, Federico and Zarate-Medina,
   Juan and Escalona-Gonzalez, Ricardo and Diaz de la Torre, Sebastian and
   Villafuerte-Castrejon, Maria-Elena},
Title = {{Piezoelectric properties of Li-Ta co-doped potassium-sodium niobate
   ceramics prepared by spark plasma and conventional sintering}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2011}},
Volume = {{509}},
Number = {{9}},
Pages = {{3837-3842}},
Month = {{MAR 3}},
Abstract = {{Lead-free piezoelectric ceramics with
   (K0.48Na0.52)(0.96)Li0.04Nb0.85Ta0.15O3 composition were sintered by
   conventional pressureless (PLS) and spark plasma sintering (SPS)
   techniques. A comparative analysis of the sintering effect on the
   piezoelectric and dielectric properties was conducted. The SPS treated
   specimen revealed an enhancement in the piezoelectric properties as
   compared to the PLS counterpart. The piezoelectric and dielectric
   parameters of PLS sintered sample were k'(p) = 0.41, s'(E)(11) = 11.80 x
   10(-12) m(2)/N, d'(31) = -55.7 (pC/N), epsilon'(T)(33) = 464.5 and tan
   delta = 0.010 at 10 kHz, whereas these parameters for the SPS sintered
   sample were k'(p) = 0.50, s'(E)(11) = 12.20 x 10(-12) m(2)/N, d'(31) =
   -66.17 (pC/N), epsilon'(T)(33) = 557.4 and tan delta = 0.024 at 10 kHz.
   The piezoelectric properties enhancement of the SPS treated sample is
   attributed to, both the lower sintering temperature and the higher
   density achieved over the PLS sintered sample. (C) 2010 Elsevier B.V.
   All rights reserved.}},
