Author = {Vazquez-Vuelvas, Oscar F. and Morales-Morales, David and German-Acacio,
   Juan M. and Tlenkopatchev, Mikhail and Pineda-Contreras, Armando},
Title = {{Crystal Structure of
   (E)-2,3-Dihydro-2-(R-Phenylacylidene)-1,3,3-Trimethyl-1H-Indole (R=4-CN,
   4-Cl)}},
