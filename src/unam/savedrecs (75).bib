@article{ ISI:000287635400022,
Author = {Fabian-Mijangos, Lorenzo and Cedeno-Caero, Luis},
Title = {{V Loading Effect on V2O5/ZrO2 Catalysts for Oxidative Desulfurization}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
