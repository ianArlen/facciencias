@article{ ISI:000289140900005,
Author = {Rudall, Paula J. and Hilton, Jason and Vergara-Silva, Francisco and
   Bateman, Richard M.},
Title = {{Recurrent abnormalities in conifer cones and the evolutionary origins of
   flower-like structures}},
Journal = {{TRENDS IN PLANT SCIENCE}},
Year = {{2011}},
Volume = {{16}},
Number = {{3}},
Pages = {{151-159}},
Month = {{MAR}},
Abstract = {{Conifer cones are reproductive structures that are typically of
   restricted growth and either exclusively pollen-bearing (male) or
   exclusively ovule-bearing (female). Here, we review two common
   spontaneous developmental abnormalities of conifer cones: proliferated
   cones, in which the apex grows vegetatively, and bisexual cones, which
   possess both male and female structures. Emerging developmental genetic
   data, combined with evidence from comparative morphology, ontogeny and
   palaeobotany, provide new insights into the evolution of both cones and
   flowers, and prompt novel strategies for understanding seed-plant
   evolution.}},
