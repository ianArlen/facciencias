@article{ ISI:000288885800009,
Author = {Garcia-Garcia, Fabio and De la Herran-Arita, Alberto K. and
   Juarez-Aguilar, Enrique and Regalado-Santiago, Citlalli and
   Millan-Aldaco, Diana and Blanco-Centurion, Carlos and Drucker-Colin,
   Rene},
Title = {{Growth hormone improves hippocampal adult cell survival and counteracts
   the inhibitory effect of prolonged sleep deprivation on cell
   proliferation}},
Journal = {{BRAIN RESEARCH BULLETIN}},
Year = {{2011}},
Volume = {{84}},
Number = {{3}},
Pages = {{252-257}},
Month = {{FEB 28}},
Abstract = {{Sleep deprivation (SD) produces numerous deleterious changes in brain
   cells, including apoptosis. It has been demonstrated that growth hormone
   (GH) stimulates cell growth and counteracts apoptosis, although this
   anti-apoptotic effect has not been tested against SD. To determine the
   protective effect of GH administration on cell proliferation and
   survival in the dentate gyrus (DG) of the hippocampus after sleep
   deprivation: we injected Wistar adult rats with a low dose of
   recombinant human GH (rhGH 5 ng/kg) per seven days and then we gently
   sleep deprived the animals for 48 consecutive hours. 5-Bromodeoxiuridine
   (BrdU) was administered to assess cell proliferation after the GH
   treatment and NeuN was used as marker of cell fate. Our results indicate
   that GH produced a three fold increase in the number of BrdU positive
