@article{ ISI:000284659500018,
Author = {del Rio, Rafael},
Title = {{Random Sturm-Liouville operators}},
Journal = {{APPLIED MATHEMATICS LETTERS}},
Year = {{2011}},
Volume = {{24}},
Number = {{2}},
Pages = {{179-183}},
Month = {{FEB}},
Abstract = {{Self-adjoint Sturm-Liouville operators H(omega) on L(2)(a, b) with
   random potentials are considered and it is proven, using positivity
   conditions, that for almost every omega the operator H(omega) does not
   share eigenvalues with a broad family of random operators and in
   particular with operators generated in the same way as H(omega) but in
   L(2) ((a) over tilde, (b) over tilde) where ((a) over tilde, (b) over
   tilde) subset of (a, b). (C) 2010 Elsevier Ltd. All rights reserved.}},
