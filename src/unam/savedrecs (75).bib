@article{ ISI:000288041000002,
Author = {Martinez Pineiro, Esmeralda Lizet and Ruiz Herrera, Brenda Lizette and
   Escudero, Roberto and Bucio, Lauro},
Title = {{Possible coexistence of superconductivity and magnetism in intermetallic
   NiBi3}},
Journal = {{SOLID STATE COMMUNICATIONS}},
Year = {{2011}},
Volume = {{151}},
Number = {{6}},
Pages = {{425-429}},
Month = {{MAR}},
Abstract = {{NiBi3 polycrystals were synthesized via a solid state method. X-ray
   diffraction analysis shows that the main phase present in the sample
   corresponds to NiBi3 in a weight fraction of 96.82 \% according to the
   refinement of the crystalline structure. SEM - EDS and XPS analysis
   reveal a homogeneous composition of NiBi3, without Ni traces. The powder
   superconducting samples were studied by performing magnetic
   measurements. The superconducting transition temperature and critical
   magnetic fields were determined as T-C = 4.05 K, H-C1 = 110 Oe and H-C2
   = 3, 620 Oe. The superconducting parameters were xi(GL) = 301.5
   angstrom. lambda(GL) = 1549 angstrom, and kappa = 5.136. Isothermal
   measurements below the transition temperature show an anomalous
   behavior. Above the superconducting transition the compound presents
   ferromagnetic characteristics up to 750 K, well above the Ni Curie
   temperature. (C) 2011 Elsevier Ltd. All rights reserved.}},
