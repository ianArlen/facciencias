@article{ ISI:000287758800007,
Author = {Hubard, Alfredo and Montejano, Luis and Mora, Emiliano and Suk, Andrew},
Title = {{Order Types of Convex Bodies}},
Journal = {{ORDER-A JOURNAL ON THE THEORY OF ORDERED SETS AND ITS APPLICATIONS}},
Year = {{2011}},
Volume = {{28}},
Number = {{1}},
Pages = {{121-130}},
Month = {{MAR}},
Abstract = {{We prove a Hadwiger transversal-type result, characterizing convex
   position on a family of non-crossing convex bodies in the plane. This
   theorem suggests a definition for the order type of a family of convex
   bodies, generalizing the usual definition of order type for point sets.
   This order type turns out to be an oriented matroid. We also give new
   upper bounds on the ErdAs-Szekeres theorem in the context of convex
   bodies.}},
