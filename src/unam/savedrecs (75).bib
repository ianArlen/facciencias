@article{ ISI:000288149200006,
Author = {Colares, A. Gervasio and Palmas, Oscar},
Title = {{On Riemannian manifolds foliated by (n-1)-umbilical hypersurfaces}},
Journal = {{BULLETIN OF THE BRAZILIAN MATHEMATICAL SOCIETY}},
Year = {{2011}},
Volume = {{42}},
Number = {{1}},
Pages = {{105-130}},
Month = {{MAR}},
Abstract = {{in this paper we define closed partially conformal vector fields and use
   them to give a characterization of Riemannian manifolds which admit this
   kind of fields as some special warped products foliated by (n -
   1)-umbilical hypersurfaces. Examples are described in space forms. In
   particular, closed partially conformal vector fields in Euclidean spaces
   are associated to the most simple foliations given by hyperspheres,
   hyperplanes or coaxial cylinders. Finally, for manifolds admitting such
   vector fields, we impose conditions for a hypersurface to be (n -
   1)-umbilical, or, in particular, a leaf of the corresponding foliation.}},
