@article{ ISI:000286435900004,
Author = {Robles-Vasquez, O. and Moscoso-Sanchez, F. and Lopez-Serrano, F. and
   Arellano, M. and Gonzalez-Alvarez, A. and Mendizabal, E. and Puig, J. E.},
Title = {{Microemulsion polymerization of 1,3-butadiene}},
Journal = {{POLYMER BULLETIN}},
Year = {{2011}},
Volume = {{66}},
Number = {{3}},
Pages = {{341-349}},
Month = {{FEB}},
Abstract = {{One-phase microemulsion regions at 25 and 60 A degrees C and the
   polymerization at 60 A degrees C in o/w microemulsion formed by
   1,3-butadiene, water, and a mixture of the surfactants,
   dodecyltrimethylammonium bromide, and didodecyldimethylamonium bromide
   (3/1 w/w), are reported. The polybutadienes obtained here have similar
   characteristics to those of their homologous obtained by emulsion
   polymerization, with the only difference that the average particle size
   of the former (25-30 nm) was smaller by an order of magnitude. The
   obtained polymer had high average molecular weights, but the gel content
