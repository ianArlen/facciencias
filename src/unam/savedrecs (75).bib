@article{ ISI:000290833100005,
Author = {Luca, Florian and Pomerance, Carl and Wagner, Stephan},
Title = {{Fibonacci integers}},
Journal = {{JOURNAL OF NUMBER THEORY}},
Year = {{2011}},
Volume = {{131}},
Number = {{3}},
Pages = {{440-457}},
Month = {{MAR}},
Abstract = {{A Fibonacci integer is an integer in the multiplicative group generated
   by the Fibonacci numbers. For example, 77 = 21 . 55/(3 . 5) is a
   Fibonacci integer. Using some results about the structure of this
   multiplicative group, we determine a near-asymptotic formula for the
   counting function of the Fibonacci integers, showing that up to x the
   number of them is between exp(c(log x)(1/2) - (log x)(is an element of))
   and exp(c(log x)(1/2) + (log x)(1/6+is an element of)), for an
   explicitly determined constant c. The proof is based on both
   combinatorial and analytic arguments. (C) 2010 Elsevier Inc. All rights
   reserved.}},
