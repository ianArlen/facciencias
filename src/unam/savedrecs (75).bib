@article{ ISI:000292482000008,
Author = {Guevara Guzman, Rosalinda and Urrutia Aguilar, Maria Esther},
Title = {{Diagnosis on the unique curriculum of the medical career at the National
   University of Mexico}},
Journal = {{GACETA MEDICA DE MEXICO}},
Year = {{2011}},
Volume = {{147}},
Number = {{2}},
Pages = {{137-142}},
Month = {{MAR-APR}},
Abstract = {{The analysis of the Plan Unico de Estudios of the medical career was the
   result of five years of collegiate work in which commissions consisting
   of academic staff of different departments from the faculty of medicine
   at the Universidad Nacional Autonoma de Mexico (UNAM) participated.
   The most significant conclusions derived from this analysis are: to
   adjust the graduate's profile in order to face the emerging health
   problems of the twenty-first century in our country; to update the
   academic curricula of the different subjects, including new ones; to
   create a nuclear curricula; as well as to strengthen the basic-clinic
   and the clinic-basic.
   With regard to the teaching/learning process, we acknowledged the need
   to develop self-cognitive and self-motivational skills in students as
   well as to consider a different evaluation for students and teachers.}},
