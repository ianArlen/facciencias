@article{ ISI:000288097400001,
Author = {Davila, J. and Poznyak, A.},
Title = {{Sliding mode parameter adjustment for perturbed linear systems with
   actuators via invariant ellipsoid method}},
Journal = {{INTERNATIONAL JOURNAL OF ROBUST AND NONLINEAR CONTROL}},
Year = {{2011}},
Volume = {{21}},
Number = {{5}},
Pages = {{473-487}},
Month = {{MAR 25}},
Abstract = {{A methodology for the design of sliding mode controllers for perturbed
   quasi-linear systems in the presence of actuators is presented. This
   technique is based on the invariant ellipsoid method and given in terms
   of the solution of a set of linear matrix inequalities. The provided
   methodology allows the design of the controller parameters ensuring
   global convergence of the states to a suboptimal ellipsoidal region
   around the origin even in the presence of both matched and unmatched
   uncertainties/disturbances. A benchmark example illustrates a good
   workability of the suggested technique. Copyright (C) 2010 John Wiley \&
   Sons, Ltd.}},
