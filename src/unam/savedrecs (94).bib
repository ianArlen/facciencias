@article{ ISI:000265761300007,
Author = {Quintanar-Guerrero, David and Nadyr Zorraquin-Cornejo, Briza and
   Ganem-Rondero, Adriana and Pinon-Segundo, Elizabeth and Guadalupe
   Nava-Arzaluz, Maria and Manuel Cornejo-Bravo, Jose},
Title = {{Controlled Release of Model Substances from pH-Sensitive Hydrogels}},
Journal = {{JOURNAL OF THE MEXICAN CHEMICAL SOCIETY}},
Year = {{2008}},
Volume = {{52}},
Number = {{4}},
Pages = {{272-278}},
Month = {{OCT-DEC}},
Abstract = {{pH-Sensitive hydrogels of the poly(methacrylic acid-co-methyl
   methacrylate) (MAA/MMA) type, 22/78 molar \% with different
   cross-linking percentages (0.3 and 0.5\%) were synthesized. These gels
   were loaded with a model hydrophilic compound (dichlorobrmophenol blue
   dye, DCBFB) with the aim of evaluating its release at different pH
   values (1.2, 5.0, 6.8, 7.4). The swelling degree and the release from
   these hydrogels are highly dependent on the pH of the dissolution medium
   and on the cross-linking degree. Scanning electron microscopy and
   differential scanning calorimetry studies demonstrated that part of the
   dye is embedded in crystal form within the hydrogel. The release
   profiles of the hydrogels assessed at pH = 6.8 and 7.4 were adjusted to
   the Higuchi model, regarding them as matrix delivery systems.}},
