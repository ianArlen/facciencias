@article{ ISI:000262313800019,
Author = {Correa-Mora, F. and DeMets, C. and Cabral-Cano, E. and Marquez-Azua, B.
   and Diaz-Molina, O.},
Title = {{Interplate coupling and transient slip along the subduction interface
   beneath Oaxaca, Mexico}},
Journal = {{GEOPHYSICAL JOURNAL INTERNATIONAL}},
