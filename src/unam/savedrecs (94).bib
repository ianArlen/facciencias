@article{ ISI:000259341900022,
Author = {Martinez, Ana and Rodriguez-Girones, Miguel A. and Barbosa, Andres and
   Costas, Miguel},
Title = {{Donator acceptor map for carotenoids, melatonin and vitamins}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY A}},
Year = {{2008}},
