@article{ ISI:000259739000011,
Author = {Arellano Ferro, A. and Giridhar, Sunetra and Rojas Lopez, V. and
   Figuera, R. and Bramich, D. M. and Rosenzweig, P.},
Title = {{A new search for variable stars in the globular cluster NGC 6366}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2008}},
Volume = {{44}},
Number = {{2}},
Pages = {{365-380}},
Month = {{OCT}},
Abstract = {{New CCD photometry of NGC 6366 has lead to the discovery of some
   variable stars. Two possible Anomalous Cepheids (or Pop II Cepheids),
   three long period variables, one SX Phe and one eclipsing binary have
   been found. Also, a list of 10 candidate variables is reported. The
   light curve of the RRab star, V1, has been decomposed into its Fourier
   harmonics, and the Fourier parameters were used to estimate the star's
   metallicity and distance; {[}Fe/H] = -0.87 +/- 0.14 and d = 3.2 +/- 0.1
   kpc. It is argued that V1 may not be a member of the cluster but rather
   a more distant object. If this is so, an upper limit for the distance to
   the cluster of 2.8 +/- 0.1 kpc can be estimated. The P - L relationship
   for SX Phe stars and the identified modes in the newly discovered SX Phe
   variable, V6, allow yet another independent determination of the
   distance to the cluster, d = 2.7 +/- 0.1 kpc. The M-V-{[}Fe/H]
   relationship for RR Lyrae stars is addressed and the case of VI is
   discussed.}},
Publisher = {{UNIV NACIONAL AUTONOMA MEXICO, INST DE ASTRONOMIA}},
Address = {{APDO POSTAL 70-264, MEXICO CITY 04510, MEXICO}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Ferro, AA (Reprint Author), Univ Nacl Autonoma Mexico, Inst Astron, Apdo Postal 70-264, Mexico City 04510, DF, Mexico.
   Arellano Ferro, A.; Rojas Lopez, V., Univ Nacl Autonoma Mexico, Inst Astron, Mexico City 04510, DF, Mexico.
