@article{ ISI:000261069500010,
Author = {Juarez Olguin, Hugo and Flores Perez, Carmen and Ramirez Mendiola,
   Blanca and Coria Jimenez, Rafael and Sandoval Ramirez, Eunice and Flores
   Perez, Janett},
Title = {{Extemporaneous Suspension of Propafenone: Attending Lack of Pediatric
   Formulations in Mexico}},
Journal = {{PEDIATRIC CARDIOLOGY}},
Year = {{2008}},
Volume = {{29}},
Number = {{6}},
Pages = {{1077-1081}},
Month = {{NOV}},
Note = {{35th Annual Meeting of the American-College-of-Clinical-Pharmacology,
   Cambridge, MA, SEP 17-19, 2006}},
Organization = {{Amer Coll Clin Pharm}},
Abstract = {{Background Physicians have frequently encountered difficulties when
   prescribing drugs not available in doses suitable for pediatric age
   groups. Furthermore, children have difficulty swallowing tablets. This
   study aimed to determine the stability of an oral propafenone suspension
   made from commercial tablets with a syrup vehicle and to establish its
   reliable use with children. Methods An extemporaneous suspension of
   propafenone 1.5 mg/ml was prepared with commercial tablets. Its
   physicochemical and microbiologic stability was established by constant
   monitoring during 90 days at room temperature (15 +/- 5 degrees C) and
   at refrigeration (3-5 degrees C). Plasma levels of propafenona were
   measured in two children with supraventricular tachycardia at steady
   state. Results The suspension was stable, maintaining its original
   physicochemical and microbiologic properties. Moreover, no apparent
   changes in color or odor were observed. Plasma levels of propafenone in
   patients demonstrated therapeutic concentrations after they had taken
   the suspension, with no unwanted outcome. Conclusions The conservation
   of both physicochemical and microbiologic stability of the suspension
   represents an option for the administration of propafenone to children.}},
