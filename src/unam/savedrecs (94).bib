@article{ ISI:000258976100014,
Author = {Mijangos, Edgar and Costa, Jose Sanchez and Roubeau, Olivier and Teat,
   Simon J. and Gamez, Patrick and Reedijk, Jan and Gasque, Laura},
Title = {{Self-assembly of an infinite copper(II) chiral metallohelicate}},
Journal = {{CRYSTAL GROWTH \& DESIGN}},
