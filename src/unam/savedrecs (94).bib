@article{ ISI:000260728900016,
Author = {Espinosa-Cardena, J. M. and Campos-Enriquez, J. O.},
Title = {{Curie point depth from spectral analysis of aeromagnetic data from Cerro
   Prieto geothermal area, Baja California, Mexico}},
Journal = {{JOURNAL OF VOLCANOLOGY AND GEOTHERMAL RESEARCH}},
Year = {{2008}},
Volume = {{176}},
Number = {{4}},
Pages = {{601-609}},
Month = {{OCT 15}},
Abstract = {{Using aeromagnetic data acquired in the area from the Cerro Prieto
   geothermal Held, we estimated the depth to the Curie point isotherm,
   interpreted as the base of the magnetic sources, following statistical
   spectral-based techniques. According to our results the Curie point
   isotherm is located at a depths ranging from 14 to 17 km. Our result is
   somewhat deeper than that obtained previously based only in 2-D and 3-D
   forward modeling of previous low-quality data. However, our results are
   supported by independent information comprising geothermal gradients,
   seismicity distribution in the crust, and gravity determined crustal
   thickness. Our results imply a high thermal gradient (ranging between 33
   and 38 degrees C/km) and high heat flow (of about 100 mW/m(2)) for the
   study area. The thermal regime for the area is inferred to be similar to
   that from the Salton trough. (C) 2008 Elsevier B.V. All rights reserved.}},
