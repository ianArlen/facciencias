@article{ ISI:000260858400009,
Author = {Corsi-Cabrera, Maria and Guevara, Miguel Angel and del Rio-Portilla,
   Yolanda},
Title = {{Brain activity and temporal coupling related to eye movements during REM
   sleep: EEG and MEG results}},
