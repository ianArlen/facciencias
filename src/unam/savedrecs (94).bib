@article{ ISI:000259700100030,
Author = {Moreno-Oliva, V. I. and Campos-Garcia, M. and Diaz-Uribe, R.},
Title = {{Improving the quantitative testing of fast aspherics with
   two-dimensional point shifting by only rotating a cylindrical null
   screen}},
Journal = {{JOURNAL OF OPTICS A-PURE AND APPLIED OPTICS}},
Year = {{2008}},
Volume = {{10}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{A new design of cylindrical null screen is proposed, to allow
   two-dimensional point shifting ( angular and radial) of the spots on the
   image plane by only applying a small angular movement of the test
   surface. A screen for producing an image with spots ordered in a radial
   arrangement is used. This new arrangement allows the evaluation of the
   surface with better accuracy. Through some numerical simulations we
   analyze the accuracy of the method by introducing random displacements
   into the measured data. Experimental results for a test of a fast convex
   aspheric surface (F/\#=0.22) are shown.}},
