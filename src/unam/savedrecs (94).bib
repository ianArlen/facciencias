@article{ ISI:000259913000021,
Author = {Ramirez, Victoria and Mejia-Vilet, Juan M. and Hernandez, Damian and
   Gamba, Gerardo and Bobadilla, Norma A.},
Title = {{Radicicol, a heat shock protein 90 inhibitor, reduces glomerular
   filtration rate}},
Journal = {{AMERICAN JOURNAL OF PHYSIOLOGY-RENAL PHYSIOLOGY}},
Year = {{2008}},
Volume = {{295}},
Number = {{4}},
Pages = {{F1044-F1051}},
Month = {{OCT}},
Abstract = {{The heat shock protein subfamily of 90 kDa (HSP90) is composed of five
   isoforms. The more abundant proteins of this subfamily are cytosolic
   isoforms known as HSP90 alpha and HSP90 beta. More than 100 client
   proteins have been found to be regulated by HSP90. Several studies have
   shown that HSP90 regulates nitric oxide synthesis that is dependent on
   endothelial nitric oxide synthase (eNOS). Because eNOS regulates renal
   vascular tone and glomerular filtration rate (GFR), the present study
   HSP90-eNOS was designed to evaluate the effect of acute HSP90 inhibition
   with radicicol on GFR and the eNOS pathway. Twenty male Wistar rats were
   divided into two groups: control vehicle animals and radicicol-infused
   animals (25 mu g . ml(-1) . min(-1)). Basal levels were taken before
   experimental measurements. Mean arterial pressure and renal blood flow
   (RBF) were recorded, as well as GFR, urinary nitrite and nitrate
   excretion (UNO2/NO3V). Additionally, we evaluated eNOS expression,
   Ser1177 and Thr495 eNOS phosphorylation levels, the eNOS
   dimer-to-monomer ratio, as well as oxidative stress by assessing renal
   lipoperoxidation and urinary isoprostane F-2 alpha and hydrogen
   peroxide. HSP90 inhibition with radicicol produced a fall in RBF and GFR
   that was associated with a significant reduction of UNO2/NO3V. The
   effects of radicicol were in part mediated by a significant decrease in
   eNOS phosphorylation and in the eNOS dimer-to-monomer ratio. Our
   findings suggest that GFR is in part maintained by HSP90-eNOS
   interaction.}},
