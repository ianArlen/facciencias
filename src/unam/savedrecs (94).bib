@article{ ISI:000261814500006,
Author = {Cruz Gomez, Raul Candelario and Monreal Gomez, Maria Adela and
   Nicolaevich Bulgakov, Sergey},
Title = {{EFFECTS OF VORTICES IN AQUATIC SYSTEMS AND ITS RELATION TO CHEMISTRY,
   BIOLOGY AND GEOLOGY}},
Journal = {{INTERCIENCIA}},
Year = {{2008}},
Volume = {{33}},
Number = {{10}},
Pages = {{741-746}},
Month = {{OCT}},
Abstract = {{The importance of vortices as one of the principal actors in
   transporting the internal energy (heat), salts, nutrients and sediments
   in marine and continental ecosystems is discussed. The thermocline and
   pycnocline uplift effects caused It v the formation of cyclonic
   vortices, which bring about an increase in nutrients in the euphotic
   zone, are presented. Also, the effects of anticyclonic vortices in the
   transport of chlorophyll and sediments over large distances compared to
   their diameters, are described. The end result is the need of joining
   efforts by different disciplines to achieve a complete understanding of
   the phenomena, which could help to predict and reasonably exploit marine
   resources.}},
