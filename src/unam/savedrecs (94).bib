@article{ ISI:000259955300016,
Author = {Rajsbaum, Sergio and Raynal, Michel and Travers, Corentin},
Title = {{An impossibility about failure detectors in the iterated immediate
   snapshot model}},
Journal = {{INFORMATION PROCESSING LETTERS}},
Year = {{2008}},
Volume = {{108}},
Number = {{3}},
Pages = {{160-164}},
Month = {{OCT 16}},
Abstract = {{The Iterated Immediate Snapshot model (IIS) is ail asynchronous
   computation model where processes communicate through a sequence of
   one-shot Immediate Snapshot (IS) objects. It is known that this model is
   equivalent to the usual asynchronous read/write shared memory model, for
   wait-free task solvability. Its interest lies in the fact that its runs
   are more structured and easier to analyze than the runs in the shared
   memory model. As the IIS model and the shared memory model are
   equivalent for wait-free task solvability, a natural question is the
   following: Are they still equivalent for wait-free task solvability,
   when they are enriched with the same failure detector? The paper shows
   that the answer to this question is ``no{''}. (C) 2008 Elsevier B.V. All
   rights reserved.}},
