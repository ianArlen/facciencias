@article{ ISI:000261614900017,
Author = {Nunez-Gonzalez, Roberto and Reyes-Serrato, Armando and Posada-Amarillas,
   Alvaro and Galvan, Donald H.},
Title = {{First-principles calculation of the band gap of AlxGa1-xN and InxGa1-xN}},
