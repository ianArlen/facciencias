@article{ ISI:000263370400004,
Author = {Cabral Vargas, Brenda},
Title = {{Necessary elements for distant education in Library Science}},
Journal = {{INVESTIGACION BIBLIOTECOLOGICA}},
Year = {{2008}},
Volume = {{22}},
Number = {{46}},
Pages = {{59-89}},
Month = {{SEP-DEC}},
Abstract = {{Reference is made to the main elements which should be considered in
   planning a distant education mode in Library Science. These are the
   result of an analysis made on different national and international
   sources plus the information obtained from three Latin American
