@article{ ISI:000259894100088,
Author = {Kunold, A. and Torres, M.},
Title = {{Hall field-induced magnetoresistance oscillations of a two-dimensional
   electron system}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
Year = {{2008}},
Volume = {{403}},
Number = {{19-20}},
Pages = {{3803-3808}},
Month = {{OCT 1}},
Abstract = {{We develop a model of the nonlinear response to a dc electrical current
   of a two-dimensional electron system (2DES) placed on a magnetic field.
   Based on the exact solution to the Schrodinger equation in arbitrarily
   strong electric and magnetic fields, and separating the relative and
   guiding center coordinates, a Kubo-like formula for the current is
   worked out as a response to the impurity scattering. Self-consistent
   expressions determine the longitudinal and Hall components of the
   electric field in terms of the dc current. The differential resistivity
   displays strong Hall field-induced oscillations, in agreement with the
   main features of the phenomenon observed in recent experiments. (C) 2008
   Elsevier B.V. All rights reserved.}},
