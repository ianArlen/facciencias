@article{ ISI:000261446500004,
Author = {Abel Jimenez-Castaneda, Amado and Gracia-Sanchez, Jesus and
   Berezowsky-Verduzco, Moises and Martinez-Cuaxospa, Jesus},
Title = {{Hydraulic behavior of the Mezcalapa-Samaria-Carrizal bifurcation}},
Journal = {{INGENIERIA HIDRAULICA EN MEXICO}},
Year = {{2008}},
Volume = {{23}},
Number = {{4}},
Pages = {{53-67}},
Month = {{OCT-DEC}},
Abstract = {{The study of a river bifurcation with a mathematical model is presented.
   The modification of the discharge distribution in the bifurcation of the
   Mezcalapa River in the Samaria and Carrizal rivers is discussed. This
   discharge change can cause floods in the city of Villahermosa, Tabasco,
   Mexico. The conditions of the river for the year 1997 are first
   represented and used for calibration of the Manning coefficient in the
   model. Afterwards, assuming that a strong deposition of sand at the
   Samaria River had taken place, the conditions for the year 1999 are
   reproduced. The mathematical model results are compared to measured
   values and a good agreement is found. This shows that the mathematical
   model represents correctly the hydraulic behavior of the river
   bifurcation. The model is used also to study the effect of a control
   structure, currently under construction in the bifurcation.}},
