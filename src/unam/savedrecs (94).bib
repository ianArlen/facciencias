@article{ ISI:000259441400008,
Author = {Blanco-Garcia, Arnulfo and Saenz-Romero, Cuauhtemoc and Alvarado-Sosa,
   Pedro and Lindig-Cisneros, Roberto},
Title = {{Native pine species performance in response to age at planting and
   mulching in a site affected by volcanic ash deposition}},
Journal = {{NEW FORESTS}},
Year = {{2008}},
Volume = {{36}},
Number = {{3}},
Pages = {{299-305}},
Month = {{NOV}},
Abstract = {{Under heavily disturbed conditions, the selection of the appropriate
   native species and of planting and ameliorating techniques is necessary.
   Volcanic eruptions create harsh conditions that can preclude native
   plant establishment. We tested the performance of two native species
   Pinus pseudostrobus and P. montezumae for restoration of volcanic ash
   covered areas. Two age classes of P. pseudostrobus and one of P.
   montezumae were tested as well as the effect of mulching to ameliorate
   harsh substrate conditions. Results show that older plants of P.
   pseudostrobus (19-month old at planting) have higher survival and growth
   rates than young plants (8-months at planting). Plants at least
   19-months-old at planting of P. pseudostrobus and P. montezumae, are
   appropriate for restoration of volcanic ash covered areas. Mulching had
   no effect on plant survival or growth for this experiment.}},
