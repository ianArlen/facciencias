@article{ ISI:000417010100011,
Author = {Salazar-Iribe, Alexis and Zuniga-Sanchez, Esther and Zavaleta Mejia,
   Emma and Gamboa-deBuen, Alicia},
Title = {{Cell Wall Localization of Two DUF642 Proteins, BIIDXI and TEEBE, during
   Meloidogyne incognita Early Inoculation}},
Journal = {{PLANT PATHOLOGY JOURNAL}},
Year = {{2017}},
Volume = {{33}},
Number = {{6}},
Pages = {{614-618}},
Month = {{DEC}},
Abstract = {{The root-knot nematode Meloidogyne incognita infects a variety of
   plants, including Arabidopsis thaliana. During migration, root-knot
   nematodes secrete different proteins to modify cell walls, which include
   pectolytic enzymes. However, the contribution of host cell wall proteins
   has not been described during this process. The function of two DUF642
   cell wall proteins, BIIDXI (BDX, At4g32460) and TEEBE (TEB, At2g41800),
   in plant development could be related to the regulation of pectin methyl
   esterification status in the cell walls of different tissues.
   Accordingly, the expression of these two genes is up-regulated by auxin.
   BDX and TEB were highly induced during early M. incognita inoculation.
   Moreover, cell wall localization of the proteins was also induced. The
   cell wall localization of BDX and TEB DUF642 proteins during M.
   incognita early inoculation suggested that these two proteins could be
   involved in the regulation of the degree of pectin methylation during
   cell separation.}},
