@article{ ISI:000407661600001,
Author = {Coronel-Escamilla, A. and Gomez-Aguilar, J. F. and Torres, L. and
   Escobar-Jimenez, R. F. and Valtierra-Rodriguez, M.},
Title = {{Synchronization of chaotic systems involving fractional operators of
   Liouville-Caputo type with variable-order}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2017}},
Volume = {{487}},
Pages = {{1-21}},
Month = {{DEC 1}},
Abstract = {{In this paper, we propose a state-observer-based approach to synchronize
   variable-order fractional (VOF) chaotic systems. In particular, this
   work is focused on complete synchronization with a so-called
   unidirectional master-slave topology. The master is described by a
   dynamical system in state-space representation whereas the slave is
   described by a state observer. The slave is composed of a master copy
   and a correction term which in turn is constituted of an estimation
   error and an appropriate gain that assures the synchronization. The
   differential equations of the VOF chaotic system are described by the
   Liouville-Caputo and Atangana-Baleanu-Caputo derivatives. Numerical
   simulations involving the synchronization of Rossler oscillators, Chua's
   systems and multi-scrolls are studied. The simulations show that
   different chaotic behaviors can be obtained if different smooths
   functions defined in the interval (0, 1] are used as the variable order
   of the fractional derivatives. Furthermore, simulations show that the
   VOF chaotic systems can be synchronized. (C) 2017 Elsevier B.V. All
   rights reserved.}},
