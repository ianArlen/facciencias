@article{ ISI:000416446800001,
Author = {Fabila-Bustos, Diego A. and Arroyo-Camarena, Ursula D. and
   Lopez-Vancell, Maria D. and Duran-Padilla, Marco A. and Azuceno-Garcia,
   Itzel and Stolik-Isakina, Suren and Valor-Reed, Alma and
   Ibarra-Coronado, Elizabeth and Hernandez-Quintanar, Luis F. and
   Escobedo, Galileo and de la Rosa-Vazquez, Jose M.},
Title = {{Fluorescence Spectroscopy as a Tool for the Assessment of Liver Samples
   with Several Stages of Fibrosis}},
Journal = {{PHOTOMEDICINE AND LASER SURGERY}},
Year = {{2018}},
Volume = {{36}},
Number = {{3}},
Pages = {{151-161}},
Month = {{MAR}},
Abstract = {{During the last years, fluorescence spectroscopy has been used as a
   potential tool for the evaluation and characterization of tissues with
   different disease conditions due to its low cost, high sensitivity, and
   minimally or noninvasive character. In this study, fluorescence
   spectroscopy was used to study 19 paraffin blocks containing human liver
   tissue from biopsies. All samples were previously analyzed by two senior
   pathologists in a single-blind trial. After their evaluation, four liver
   samples were classified as nonfibrosis (F0), four as initial fibrosis
   (F1-F2), four as advanced fibrosis (F3), and six as cirrhosis (F4). The
   fluorescence was induced at different wavelengths as follows: 330, 365,
   and 405nm using a portable fiber-optic system. The fluorescence spectra
   were recorded in the range of 400-750nm. A distinctive correlation
   between the shape of each spectrum and the level of fibrosis in the
   liver sample was detected. A multi-variate statistical analysis based on
   principal component analysis followed by linear discrimination analysis
   was applied to develop algorithms able to distinguish different stages
   of fibrosis based on the characteristics of fluorescence spectra.
   Pairwise comparisons were performed: F0 versus F1-F2, F1-F2 versus F3,
   F3 versus F4, and F1-F2 versus F4. The algorithms applied to each set of
   data yielded values of sensitivity and specificity that were higher than
   90\% and 95\%, respectively, in all the analyzed cases. With this study,
   it is concluded that fluorescence spectroscopy can be used as a
   complementary tool for the assessment of liver fibrosis in liver tissue
   samples, which sets the stage for subsequent clinical trials.}},
Publisher = {{MARY ANN LIEBERT, INC}},
Address = {{140 HUGUENOT STREET, 3RD FL, NEW ROCHELLE, NY 10801 USA}},
