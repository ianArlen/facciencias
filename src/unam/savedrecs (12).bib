@article{ ISI:000419242400064,
Author = {Rebeca Rivera-Yanez, C. and Ignacio Terrazas, L. and Jimenez-Estrada,
   Manuel and Campos, Jorge E. and Flores-Ortiz, Cesar M. and Hernandez,
   Luis B. and Cruz-Sanchez, Tonatiuh and Garrido-Farina, German I. and
   Rodriguez-Monroy, Marco A. and Margarita Canales-Martinez, M.},
Title = {{Anti-Candida Activity of Bursera morelensis Ramirez Essential Oil and
   Two Compounds, -Pinene and -TerpineneAn In Vitro Study}},
Journal = {{MOLECULES}},
Year = {{2017}},
Volume = {{22}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{The candidiasis caused by C. albicans is a public health problem. The
   abuse of antifungals has contributed to the development of resistance.
   B. morelensis has demonstrated antibacterial and antifungal activities.
   In this work the activity of the essential oil of B. morelensis was
   evaluated and for its two pure compounds with analysis of the different
   mechanisms of pathogenesis important for C. albicans. The essential oil
   was obtained by the hydro-distillation method and analyzed using GC-MS.
   The anti-Candida activity was compared between to essential oil, -Pinene
   and -Terpinene. GC-MS of the essential oil demonstrated the presence of
   13 compounds. The essential oil showed antifungal activity against four
   C. albicans strains. The most sensitive strain was C. albicans 14065
   (MFC 2.0 mg/mL and MIC50 0.125 mg/mL) with -Pinene and -Terpinene having
   MFCs of 4.0 and 16.0 mg/mL respectively. The essential oil inhibited the
   growth of the germ tube in 87.94\% (8.0 mg/mL). Furthermore, it was
   observed that the essential oil diminishes the transcription of the gene
   INT1. This work provides evidence that confirms the anti-Candida
   activity of the B. morelensis essential oil and its effect on the growth
   of the germ tube and transcription of the gene INT1.}},
