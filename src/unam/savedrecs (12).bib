@article{ ISI:000418986200011,
Author = {Brancalion, Pedro H. S. and Lamb, David and Ceccon, Eliane and Boucher,
   Doug and Herbohn, John and Strassburg, Bernardo and Edwards, David P.},
Title = {{Using markets to leverage investment in forest and landscape restoration
   in the tropics}},
Journal = {{FOREST POLICY AND ECONOMICS}},
Year = {{2017}},
Volume = {{85}},
