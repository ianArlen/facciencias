@article{ ISI:000416431200009,
Author = {Bonder, Yuri and Garcia-Chung, Angel and Rastgoo, Saeed},
Title = {{Bounds on the polymer scale from gamma ray bursts}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2017}},
Volume = {{96}},
Number = {{10}},
Month = {{NOV 29}},
Abstract = {{The polymer representations, which are partially motivated by loop
   quantum gravity, have been suggested as alternative schemes to quantize
   the matter fields. Here we apply a version of the polymer
   representations to the free electromagnetic field, in a reduced phase
   space setting, and derive the corresponding effective (i.e.,
   semiclassical) Hamiltonian. We study the propagation of an
   electromagnetic pulse, and we confront our theoretical results with
   gamma ray burst observations. This comparison reveals that the
   dimensionless polymer scale must be smaller than 4 x 10(-35), casting
   doubts on the possibility that the matter fields are quantized with the
   polymer representation we employed.}},
