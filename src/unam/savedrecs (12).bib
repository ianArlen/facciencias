@article{ ISI:000416158500042,
Author = {Wang, Xingli and Sofia Varela, Ana and Bergmann, Arno and Kuehl,
   Stefanie and Strasser, Peter},
Title = {{Catalyst Particle Density Controls Hydrocarbon Product Selectivity in
   CO2 Electroreduction on CuOx}},
Journal = {{CHEMSUSCHEM}},
Year = {{2017}},
Volume = {{10}},
Number = {{22, SI}},
Pages = {{4642-4649}},
Month = {{NOV 23}},
Abstract = {{A key challenge of the carbon dioxide electroreduction (CO2RR) on
   Cu-based nanoparticles is its low faradic selectivity toward
   higher-value products such as ethylene. Here, we demonstrate a facile
   method for tuning the hydrocarbon selectivities on CuOx nanoparticle
   ensembles by varying the nanoparticle areal density. The sensitive
   dependence of the experimental ethylene selectivity on catalyst particle
   areal density is attributed to a diffusional interparticle coupling that
   controls the de- and re-adsorption of CO and thus the effective coverage
   of COad intermediates. Thus, higher areal density constitutes
   dynamically favored conditions for CO re-adsorption and {*}CO
   dimerization leading to ethylene formation independent of pH and applied
   overpotential.}},
