Author = {Bohorquez-Hernandez, A. and Gratton, Enrico and Pacheco, Jonathan and
   Asanov, Alexander and Vaca, Luis},
Title = {{Cholesterol modulates the cellular localization of Orail channels and
   its disposition among membrane domains}},
