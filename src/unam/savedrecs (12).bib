@article{ ISI:000418013400019,
Author = {Botello, Francisco and Romero-Calderon, Ana G. and Sanchez-Hernandez,
   Jhovani and Hernandez, Omar and Lopez-Villegas, Geronimo and
   Sanchez-Cordero, Victor},
Title = {{Population density of Central American tapir (Tapirella bairdii) in
   cloud forest in Totontepec Villa de Morelos, Oaxaca, Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2017}},
Volume = {{88}},
Number = {{4}},
Pages = {{918-923}},
Month = {{DEC}},
Abstract = {{The Central American tapir (Tapirella bairdii) is the largest mammal in
   the Neotropics, and it is considered as an indicator species of habitat
   quality in tropical forests. The presence of a population of T bairdii
   has been recently reported for the Sierra Mixe in the State of Oaxaca.
   Here we estimated the population density of the Central American tapir
   at this site using capture -recapture methods with camera traps and 5
   independent observers. Six individuals were detected in an area of 18.75
   km(2), and a population density of 0.32 ind/km(2) was estimated for this
   site. This is one of the highest population densities reported for the
   genus, highlighting Totontepec as an important area for the conservation
   of the species. (C) 2017 Universidad Nacional Autonoma de Mexico,
   Instituto de Biologia.}},
Publisher = {{INST BIOLOGIA, UNIV NACIONAL AUTONOMA MEXICO}},
