@article{ ISI:000407661600015,
Author = {Telesca, Luciano and Lovallo, Michele and Leticia Flores-Marquez, E.},
Title = {{Characterizing volcanic states at Popocatepetl, Mexico by informational
   analysis of continuous geomagnetic signal}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{2017}},
Volume = {{487}},
Pages = {{178-184}},
Month = {{DEC 1}},
Abstract = {{The investigation of the dynamics of the continuous geomagnetic signal
   recorded at Popocatepetl volcano from 1997 to 1999 was performed using
   the Fisher-Shannon approach, a statistical tool for identifying dynamic
   changes in complex systems. Our results reveal that dynamic changes in
   the Total Magnetic Field (TMF) are correlated with different stress
   states of the magmatic setting and the plumbing system in the volcano.
   The results contribute to reveal the probable signatures of the thermal
   processes involved in the movement of the magma that induce the
   variations of the TMF signal. (C) 2017 Elsevier B.V. All rights
   reserved.}},
