@article{ ISI:000419002700017,
Author = {Martin-Cao-Romero, Carolina and Alonso Solis-Marin, Francisco and
   Alejandra Caballero-Ochoa, Andrea and Quetzalli Hernandez-Diaz, Yoalli
   and Lopez Lujan, Leonardo and Zuniga-Arellano, Belem},
Title = {{New echinoderm remains in the buried offerings of the Templo Mayor of
   Tenochtitlan, Mexico CityCarolina}},
Journal = {{REVISTA DE BIOLOGIA TROPICAL}},
Year = {{2017}},
Volume = {{65}},
Number = {{1}},
Pages = {{S168-S179}},
Month = {{NOV}},
Note = {{3rd Latin American Congress of Echinoderms, Univ Costa Rica, San Jose,
   COSTA RICA, JUL 18-22, 2016}},
Abstract = {{Between 1978 and 1982 the ruins of the Templo Mayor of Tenochtitlan were
   exhumed a few meters northward from the central plaza (Zocalo) of Mexico
   City. The temple was the center of the Mexica's ritual life and one of
   the most famous ceremonial buildings of its time (15th and 16th
   centuries). More than 200 offerings have been recovered in the temple
   and surrounding buildings. We identified vestiges of 14 species of
   echinoderms (mostly as disarticulated plates). These include six species
   of sea stars (Luidia superba, Astropecten regalis, Astropecten
   duplicatus, Phataria unifascialis, Nidorellia armata, Pentaceraster
   cumingi), one ophiuroid species (Ophiothrix rudis), two species of sea
   urchins (Eucidaris thouarsii, Echinometra vanbrunti), four species of
   sand dollars (Mellita quinquiesperforata, Mellita notabilis, Encope
   laevis, Clypeaster speciosus) and one species of sea biscuit (Meoma
   ventricosa grandis). They date back to the reigns of kings Axayacatl (AD
   1469-1481), Tizoc (AD 1481-1486), Ahuitzotl (AD 1486-1502), and
   Motecuhzoma II (AD 1502-1520). Apparently the presence of echinoderms in
   the offerings is related to the realm of Tlaloc (god of rain and earth).
   It is believed this organisms, like other marine animals, were used by
   the priests, like other marine animals, to represent the aquatic
   underworld of Mesoamerican world-view.}},
