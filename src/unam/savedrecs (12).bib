@article{ ISI:000418285700006,
Author = {Poorter, Lourens and van der Sande, Masha T. and Arets, Eric J. M. M.
   and Ascarrunz, Nataly and Enquist, Brian and Finegan, Bryan and Licona,
   Juan Carlos and Martinez-Ramos, Miguel and Mazzei, Lucas and Meave,
