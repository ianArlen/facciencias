@article{ ISI:000413633500006,
Author = {Mendoza-Cruz, Eva and Moreno-Mendoza, Norma and Zambrano, Luis and
   Villagran-SantaCruz, Maricela},
Title = {{Development and gonadal sex differentiation in the neotenic urodele:
   Ambystoma mexicanum}},
Journal = {{ZOOMORPHOLOGY}},
