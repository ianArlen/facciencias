@article{ ISI:000419173600010,
Author = {Ahuja-Aguirre, Concepcion and Perez-Gutierrez, Omar and Lopez-deBuen,
   Lorena and Rojas-Maya, Susana and Montiel-Palacios, Felipe},
Title = {{EARLY ONSET OF THE REPRODUCTIVE SEASON IN CAPTIVE WHITE-TAILED DEER
   (ODOCOILEUS VIRGINIANUS VERAECRUCIS) IN EASTERN MEXICO}},
Journal = {{JOURNAL OF ZOO AND WILDLIFE MEDICINE}},
Year = {{2017}},
Volume = {{48}},
