@article{ ISI:000415983800012,
Author = {Reyes-Vaca, Arturo and de la Cruz, Lizbeth and Garduno, Julieta and
   Arenas, Isabel and Garcia, David E.},
Title = {{Fast Inactivation of Ca(V)2.2 Channels Is Prevented by the G beta(1)
   Subunit in Rat Sympathetic Neurons}},
Journal = {{JOURNAL OF MOLECULAR NEUROSCIENCE}},
Year = {{2017}},
Volume = {{63}},
Number = {{3-4}},
Pages = {{377-384}},
Month = {{DEC}},
Abstract = {{Voltage-dependent regulation of Ca(V)2.2 channels by G-proteins is
   performed by the beta (G beta) subunit. Most studies of regulation by
   G-proteins have focused on channel activation; however, little is known
   regarding channel inactivation. This study investigated inactivation of
   Ca(V)2.2 channels in superior cervical ganglion neurons that
   overexpressed G beta subunits. Ca(V)2.2 currents were recorded by
   whole-cell patch clamping configuration. We found that the G beta(1)
   subunit reduced inactivation, while G beta(5) subunit did not alter at
   all inactivation kinetics compared to control recordings. Ca(V)2.2
   current decay in control neurons consisted of both fast and slow
   inactivation; however, G beta(1)-overexpressing neurons displayed only
   the slow inactivation. Fast inactivation was restored by a strong
   depolarization of G beta(1)-overexpressing neurons, therefore, through a
   voltage-dependent mechanism. The G beta(1) subunit shifted the voltage
   dependence of inactivation to more positive voltages and reduced the
   fraction of Ca(V)2.2 channels resting in the inactivated state. These
   results support that the G beta(1) subunit inhibits the fast
   inactivation of Ca(V)2.2 channels in SCG neurons. They explain the
   long-observed sustained Ca2+ current under G-protein modulation.}},
