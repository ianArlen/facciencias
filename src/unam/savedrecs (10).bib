Title = {{Passivity-Based Control for Battery Charging/Discharging Applications by
   Using a Buck-Boost DC-DC Converter}},
Booktitle = {{2018 IEEE GREEN TECHNOLOGIES CONFERENCE (GREENTECH)}},
Series = {{IEEE Green Technologies Conference}},
Year = {{2018}},
