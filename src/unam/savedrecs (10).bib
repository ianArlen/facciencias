@article{ ISI:000443699000003,
Author = {Romero-Rodriguez, Jesus and Martinez-Mayen, Mario},
Title = {{REDISCOVERY OF THE BOPYRID ISOPOD PARABOPYRELLA THOMASI (NIERSTRASZ \&
   BRENDER A BRANDIS, 1929), PARASITE OF THE ARROW SHRIMP TOZEUMA
   CAROLINENSE KINGSLEY, 1878 (DECAPODA, CARIDEA) IN THE CARIBBEAN REGION}},
Journal = {{CRUSTACEANA}},
Year = {{2018}},
Volume = {{91}},
Number = {{10}},
Pages = {{1183-1194}},
Abstract = {{This is only the second report of the bopyrid isopod Parabopyrella
   thomasi (Nierstrasz \& Brender a Brandis, 1929) since it was described.
   The species infests the arrow shrimp Tozeuma carolinense Kingsley, 1878,
   and this report of new material extends its known geographic
   distribution to the Caribbean coasts of Mexico. We provide data on some
