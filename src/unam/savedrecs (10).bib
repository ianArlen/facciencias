@article{ ISI:000443887900002,
Author = {Grisel Mendez-Garcia, Carmen and Teresita Romero-Guzman, Elizabeth and
   Hernandez-Mendoza, Hector and Solis Rosales, Corina and Chavez Lomeli,
   Efrain Rafael},
Title = {{Anomalous ratios of radioisotopes in PM10 as tracer of global fallout
   impact in the centre of Mexico}},
Journal = {{ISOTOPES IN ENVIRONMENTAL AND HEALTH STUDIES}},
Year = {{2018}},
Volume = {{54}},
Number = {{5}},
Pages = {{451-462}},
Abstract = {{The monitoring and evaluation of radioactive content in samples of PM10
   aerosols have been investigated. The specific radioactivity
   concentrations (SRC) of U-234, U-235, U-238 and Th-232 were determined
   using inductively coupled plasma-sector field mass spectrometry in 13
   samples collected in Mexico City and 8 samples collected in Cuernavaca
   in the centre of Mexico. The SRC of the radioisotopes analysed in PM10
   were larger than those reported in PM2.5. The enrichment factor was
   greater than 5, indicating anthropogenic influences in both sites. The
   activity ratios of these isotopes in the samples were determined. The
   U-235/U-238 ratio showed variations with respect to the natural value,
   while the U-234/U-238 and Th-232/U-238 ratios did not show any secular
   equilibrium in all sites, corroborating that the increase of uranium is
   not influenced by natural sources. The annual dose results obtained have
   no impact on health.}},
