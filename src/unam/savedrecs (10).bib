@article{ ISI:000448820000008,
Author = {Agustin Garcia-Reynoso, Jose and Eugenia Mar-Morales, Bertha and Gerardo
   Ruiz-Suarez, Luis},
Title = {{SPATIAL, TEMPORARY AND SPECIAL DISTRIBUTION MODEL OF THE MEXICO
   EMISSIONS INVENTORY (BASE YEAR 2008) FOR USE IN AIR QUALITY MODELING
   (DIETE)}},
Journal = {{REVISTA INTERNACIONAL DE CONTAMINACION AMBIENTAL}},
Year = {{2018}},
Volume = {{34}},
Number = {{4}},
Pages = {{635-649}},
Abstract = {{This paper presents the spatial, temporal and speciation distribution
   model (DiETE) of the 2008 Mexico National Emissions Inventory (MNEI).
   The methodology and information used to estimate the spatial, temporal
   and chemical distribution of the emissions is described. The results and
   validation procedure are shown using the 2008 MNEI, which considers
   seven pollutants: volatile organic compounds (VOC), ammonia (NH3),
   nitrogen oxide (NOx), sulfur dioxide (SO2), carbon monoxide (CO), PM10
   and PM2.5. Three source types are also considered: fixed, area and
   mobile. They are further subdivided into 258, 64 and 17 categories,
   respectively. Each category was related to a source classification code
   (SCC). Temporal and chemical profiles were obtained from each SCC
   category. It was identified that the total mass of VOC in the
   aggregation procedure compared to the emission inventory produces a
   difference up to 17 \%. The DiETE model can generate files for RADM2,
   RACM, CBM5 and SAPRC99, used by WRF-chem. Although emissions models have
   been used in other studies, this is the first to include validation in
   the conversion procedure, which is useful for the whole year and can be
   used for environmental management.}},
