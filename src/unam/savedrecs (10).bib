@article{ ISI:000432800600002,
Author = {Rodriguez Resendiz, Perla Olivia},
Title = {{Sustainable Digital Preservation Factors for Sound Archives}},
Journal = {{BIBLIOTECAS-REVISTA DE LA ESCUELA DE BIBLIOTECOLOGIA DOCUMENTACION E
   INFORMACION}},
Year = {{2018}},
Volume = {{36}},
Number = {{2}},
Month = {{JAN-JUN}},
Abstract = {{The preservation of digital documents is one of the most relevant
   challenges facing sound archives. The conservation and access to large
   volumes of digital sound information that is protected in sound archives
   is a complex task that requires a change of perspective in the
   traditional way of preserving sound documents. To face this challenge,
   it is proposed the sustainable digital preservation as the procedure to
   conserve and give access to the long-term sound content. The digital
   sustainable preservation takes in consideration three factors: a) shared
   technology infrastructure, b) reduce the consumption energy and the
   impact environmental and c) continuity and self-generation of economic
   resources.}},
