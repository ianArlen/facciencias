@article{ ISI:000434336000003,
Author = {Angel Casas-Ramos, Miguel and Eduardo Sandoval-Romero, Gabriel},
Title = {{Strain detection and measurement using a matched fibre Bragg grating}},
Journal = {{JOURNAL OF ELECTROMAGNETIC WAVES AND APPLICATIONS}},
Year = {{2018}},
Volume = {{32}},
Number = {{12}},
Pages = {{1519-1526}},
Abstract = {{We present and describe a method to demodulate the incoming strain
   signal from sensors based on fibre Bragg gratings (FBGs). This
   demodulation is achieved by using a reference FBG with similar
   characteristics to those of the FBG sensor as a band-pass filter to
   recover the total power in the detected signal from the shift in the
   Bragg wavelength caused by the strain experienced by the FBG sensor.
   From the experimental data and the numerical simulation, the system
   static transfer characteristic curves are obtained and compared. Using
   this method, a linear measurement range of 227pm (183 epsilon), along
   with a precision of 0.8 epsilon and a resolution of 1.6 epsilon were
   achieved. This simple and inexpensive method is a practical alternative
   to conventional FBG interrogation units because few optical elements are
   used in the set-up, and the Bragg wavelength shifts are determined by
   the changes in the detected optical intensity. These intensity changes
   are caused by the mechanical axial strain experienced by the FBG sensor.}},
