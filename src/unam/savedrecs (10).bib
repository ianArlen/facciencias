Editor = {{Acosta, L and AmadorValenzuela, P and Velazquez, V}},
Title = {{Characterization of the LEMA isotope separator to measure concentrations
   of Be-10 from atmospheric filters}},
Booktitle = {{XLI SYMPOSIUM ON NUCLEAR PHYSICS 2018 (COCOYOC2018)}},
Series = {{Journal of Physics Conference Series}},
