@article{ ISI:000448820000011,
Author = {Garcia Franco, Rodolfo and Angel Hernandez, Miguel and Portillo Reyes,
   Roberto and Petranovskii, Vitalii and Rubio, Efrain and Quiroz Estrada,
   Karla Fabiola},
Title = {{ADSORPTION OF CO2, H-2 and CH4 IN NATURAL ZEOLITES OF NARROW PORE}},
Journal = {{REVISTA INTERNACIONAL DE CONTAMINACION AMBIENTAL}},
Year = {{2018}},
Volume = {{34}},
Number = {{4}},
Pages = {{685-696}},
Abstract = {{The characteristics and results of gas adsorption on a natural zeolite
   from Escalerillas, San Luis Potosi, Mexico are presented. This material
   was subjected to various treatment methods that improved its texture
   parameters, adsorption capacity, and also increased the Si/Al atomic
   ratio in its composition. As a result of these treatments, the
   adsorption capacity of the zeolite with respect to CH4 and H-2 has been
   improved, resulting in higher adsorption heats than those given for some
   hybrid materials. However, although the natural sample (N) adsorbed more
   CO2 at temperatures above 548 K, chemically modified samples showed
   better behavior at temperatures below 523 K.}},
