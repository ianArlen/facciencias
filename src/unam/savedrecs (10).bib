@article{ ISI:000446697900001,
Author = {Viera-Gonzalez, Perla M. and Sanchez-Guerrero, Guillermo E. and
   Martinez-Guerra, Edgar and Ceballos-Herrera, Daniel E.},
Title = {{Mathematical Analysis of Nonimaging Fresnel Lenses Using Refractive and
   Total Internal Reflection Prisms for Sunlight Concentration}},
Journal = {{MATHEMATICAL PROBLEMS IN ENGINEERING}},
Year = {{2018}},
Abstract = {{During the design of nonimaging Fresnel lenses, the reflectance losses
   at the refracting surfaces should be taken into account because,
   depending on the parameters of each prism, semiacceptance angle
   theta(in) and output angle theta(out), these losses can decrease the
   total efficiency of the lens. On this subject, an analysis and
   comparison between a Fresnel lens composed only of refractive prisms and
   others with total internal reflection (TIR) prisms are presented and,
   using this information, a design method of hybrid Fresnel lenses
   composed of refractive and TIR prisms is proposed to reduce reflectance
   losses from the surface. The work presents the radiance profile and
   transmittance for a Fresnel lens of each type, to compare its advantages
   and disadvantages providing an alternative for the design and
   construction of nonimaging Fresnel lenses according to optical devices
   requirements.}},
