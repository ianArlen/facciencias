@article{ ISI:000446987900002,
Author = {Diaz-Marin, Homero G. and Oeckl, Robert},
Title = {{Quantum Abelian Yang-Mills Theory on Riemannian Manifolds with Boundary}},
Journal = {{SYMMETRY INTEGRABILITY AND GEOMETRY-METHODS AND APPLICATIONS}},
Year = {{2018}},
Volume = {{14}},
Abstract = {{We quantize abelian Yang-Mills theory on Riemannian manifolds with
   boundaries in any dimension. The quantization proceeds in two steps.
   First, the classical theory is encoded into an axiomatic form describing
   solution spaces associated to manifolds. Second, the quantum theory is
   constructed from the classical axiomatic data in a functorial manner.
   The target is general boundary quantum field theory, a TQFT-type
   axiomatic formulation of quantum field theory.}},
