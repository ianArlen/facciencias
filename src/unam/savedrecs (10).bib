@article{ ISI:000432436100004,
Author = {Toledo-Fernandez, Aldebaran and Garcia-Gomez, Leonor and
   Villalobos-Gallegos, Luis and Salvador-Cruz, Judith},
Title = {{Validity of the Montreal Battery of Evaluation of Amusia: An Analysis
   Using Structural Equation Modeling}},
Journal = {{STUDIA PSYCHOLOGICA}},
Year = {{2018}},
Volume = {{60}},
Number = {{1}},
Pages = {{42-56}},
Abstract = {{The Montreal Battery of Evaluation of Amusia (MBEA) is the gold standard
   for diagnosing amusia. We aimed to evaluate its factorial and convergent
   validity. Data were collected for the MBEA and a self-report Amusic
   Dysfunction Inventory on a non-random sample (n = 249), and the
   following Structural Equation Modeling (SEM) procedures were conducted:
   confirmatory factor analysis of the theoretical model; exploratory SEM
   for alternative non-restricted factor solutions; and structural models
   with each of these solutions as predictors of the inventory's items. The
   theoretical model did not prove acceptable goodness of fit, and two-and
