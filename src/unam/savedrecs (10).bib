@article{ ISI:000439030900006,
Author = {Gonzalez Suarez, Lucero},
Title = {{The presence of God in the interior castle. On the complementarity of
   mystic anthropology of Santa Teresa de Jesus and Edith Stein's
   phenomenological anthropology}},
Journal = {{VALENCIANA}},
Year = {{2018}},
Volume = {{11}},
Number = {{21}},
Pages = {{127-151}},
Month = {{JAN-JUN}},
Abstract = {{The aim of this article is showing the most important coincidences
   between Santa Teresa de Jesus's mystic anthropology and Edith Stein's
   phenomenological anthropology, about soul's comprehension, in order to
   exhibit their agreement. This meditation is philosophical anthropology
   by its object; phenomenology, by its method, and hermeneutics, because
   the analysis is directed to three textual constructions: The interior
   castle (written by Santa Teresa de Jesus), Finite and eternal Being and
   Soul's Castle (written by E. Stein). The thesis I'll try to prove is: if
   the human essence is defined by the opening to the mystery of God, the
   integral comprehension of human being should consider the mystical
   testimonies.}},
