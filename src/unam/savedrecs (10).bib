@article{ ISI:000419674100003,
Author = {Adriano, Bruno and Fujii, Yushiro and Koshimura, Shunichi and Mas, Erick
   and Ruiz-Angulo, Angel and Estrada, Miguel},
Title = {{Tsunami Source Inversion Using Tide Gauge and DART Tsunami Waveforms of
   the 2017 Mw8.2 Mexico Earthquake}},
Journal = {{PURE AND APPLIED GEOPHYSICS}},
