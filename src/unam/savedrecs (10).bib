@article{ ISI:000435957100009,
Author = {Toledo Marin, Leonel and Herrera-Balboa, Samuel and Silva, Carmen},
Title = {{THE NATURALISTIC AGENDA OF PASSIONS IN EARLY MODERN PHILOSOPHY}},
Journal = {{ASCLEPIO-REVISTA DE HISTORIA DE LA MEDICINA Y DE LA CIENCIA}},
Year = {{2018}},
Volume = {{70}},
Number = {{1}},
Month = {{JAN-JUN}},
Abstract = {{In this paper we will try to characterize the main theoretical grounds
   that promoted the change of view from Scholasticism to Early Modern
   Philosophy on the emotive and cognitive faculties; to achieve this task,
   we will summarize the intellectual background for the study of passions;
   then, we will differentiate two mainly ``naturalistic{''} trends of
   thought: First, the reductionist thesis, adopted by Thomas Hobbes,
   Pierre Gassendi and Rene Descartes, among others; and second, the
   attempt to establish and describe the dynamics of mental life, which was
   developed by Thomas Hobbes, John Locke and David Hume. On giving this
   account, we expect also to gain a clearer comprehension about the
   changes of perspective that were proposed by some Early Modern thinkers,
   whose views developed towards a new naturalized philosophical
   anthropology.}},
Publisher = {{CONSEJO SUPERIOR INVESTIGACIONES CIENTIFICAS-CSIC, INST GRASA}},
