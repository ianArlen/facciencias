@inproceedings{ ISI:000449236600036,
Author = {Mager Hois, Elisabeth A.},
Editor = {{Janikova, V and Brychova, A and Velickova, J and Wagner, R}},
Title = {{The outsiders of a dehumanised society in Georg Heym's lyrics}},
