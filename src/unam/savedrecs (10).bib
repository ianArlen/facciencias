@article{ ISI:000439901300008,
Author = {Salas-Porras, Alejandra},
Title = {{American think tank networks and expert debates around the Global
   Financial Crisis: Keynesian insurgents against austerity defenders}},
Journal = {{POLICY AND SOCIETY}},
Year = {{2018}},
Volume = {{37}},
Number = {{2}},
Pages = {{243-259}},
Abstract = {{This article applies social network analysis and a Bourdieu-inspired
   understanding of think tanks to a particular historical period, the
   aftermath of the Global Financial Crisis of 2007-2009, when the
   neoliberal policy consensus was fractured and struggles to replace it by
   Keynesian policies ensued. The analysis finds that think tank and expert
   networks have played a key role in mobilising policy ideas, contesting
   or defending policy discourses. Experts advanced their proposals for
   policy changes in all those fields of power where they have gained a
   foothold. However, no consensus has been reached on monetary and fiscal
   policies, and the policy communities remain divided in these domains.}},
