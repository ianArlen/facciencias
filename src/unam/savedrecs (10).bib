@article{ ISI:000438698700006,
Author = {Valdes De Anda, Mauricio and Hendrickx, Michel E. and Alonso
   Solis-Marin, Francisco},
Title = {{New records of Florometra serratissima (AH Clark, 1907) (Echinodermata:
   Crinoidea) from the Gulf of California and the Pacific Coast of Mexico}},
Journal = {{CAHIERS DE BIOLOGIE MARINE}},
Year = {{2018}},
Volume = {{59}},
Number = {{2}},
Pages = {{167-173}},
Abstract = {{The crinoid Floromeira serratissima is reported in new localities off
   the western coast of Mexico. With a total of 109 specimens, this
   material represents the first record of crinoids from the Gulf of
   California and for the state of Colima. These new localities
   considerably increase the geographical distribution range of F.
   seiraiissima. Environmental data associated with these new findings
   indicate that F. serratissima lives mostly below the Oxygen Minimum Zone
   and is highly tolerant to oxygen deficiency.}},
