@article{ ISI:000443935500003,
Author = {Edgar Gonzalez-Varela, Jose},
Title = {{The Razor Argument of Metaphysics A.9}},
Journal = {{PHRONESIS-A JOURNAL FOR ANCIENT PHILOSOPHY}},
Year = {{2018}},
Volume = {{63}},
Number = {{4}},
Pages = {{408-448}},
Abstract = {{I discuss Aristotle's opening argument against Platonic Forms in
   Metaphysics A.9, `the Razor', which criticizes the introduction of Forms
   on the basis of an analogy with a hypothetical case of counting things.
   I argue for a new interpretation of this argument, and show that it
   involves two interesting objections against the introduction of Forms as
   formal causes: one concerns the completeness and the other the adequacy
   of such an explanatory project.}},
