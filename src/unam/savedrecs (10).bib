@article{ ISI:000440871200012,
Author = {Carlos Manriquez-Betanzos, Juan and Montero-Lopez Lena, Maria},
Title = {{Validation of the Scale of Emotions toward Water Conservation Behavior}},
Journal = {{REVISTA IBEROAMERICANA DE DIAGNOSTICO Y EVALUACION-E AVALIACAO
   PSICOLOGICA}},
Year = {{2018}},
Volume = {{1}},
Number = {{46}},
Pages = {{147-159}},
Month = {{JAN-MAR}},
Abstract = {{This study had the purpose of obtain the psychometric characteristics of
   a scale of composite emotions to the water conservation behavior, which
   was developed considering assumptions of cognitive theory of emotions.
   Responses from a purposive sample of 376 undergraduate students from
   Mexico City were analyzed. Through a second-order factor analysis,
   estimated by the robust maximum likelihood method, a final group of
   three dimensions was ratified, achieving accurate goodness of fit levels
   and internal consistency. The evidence generated allows for assume that
   there is, within the Mexican context, a valid and reliable instrument
   addressed to evaluate three specific emotions (gratitude, anger and
   remorse) towards actions of water consumption.}},
