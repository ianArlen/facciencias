@inproceedings{ ISI:000450922800016,
Author = {Serrano, Sergio A. and Benitez-Jimenez, Ricardo and Nunez-Rosas, Laura
   and del Coro Arizmendi, Ma and Greeney, Harold and Reyes-Meza, Veronica
   and Morales, Eduardo and Jair Escalante, Hugo},
Editor = {{MartinezTrinidad, JF and CarrascoOchoa, JA and OlveraLopez, JA and Sarkar, S}},
Title = {{Automated Detection of Hummingbirds in Images: A Deep Learning Approach}},
Booktitle = {{PATTERN RECOGNITION}},
Series = {{Lecture Notes in Computer Science}},
Year = {{2018}},
Volume = {{10880}},
Pages = {{155-166}},
