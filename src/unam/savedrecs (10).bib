@inproceedings{ ISI:000443413100021,
Author = {Yepez-Martinez, T. and Civitarese, O. and Amor Quiroz, D. A. and Hess,
   P. O.},
Editor = {{Acosta, L and AmadorValenzuela, P and Velazquez, V}},
Title = {{Structure of baryon states from non-perturbative methods}},
