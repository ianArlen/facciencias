@article{ ISI:000438785800008,
Author = {Bayard, Pierre and Mendez Varela, Felipe and Sanchez-Bringas, Federico},
Title = {{EXTRINSIC GEOMETRY AND HIGHER ORDER CONTACTS OF SURFACES IN R-5}},
Journal = {{JOURNAL OF SINGULARITIES}},
Year = {{2018}},
Volume = {{17}},
Pages = {{193-213}},
Month = {{JAN-DEC}},
Abstract = {{We study the extrinsic geometry of a surface in R-5 in relation to
   contact theory. We first completely determine the numerical invariants
   of the second fundamental form and describe the corresponding curvature
   ellipse. We then introduce and study a new quadratic map closely related
   to the degenerate directions of the surface, we characterize inflection
   and umbilic points of the surface in terms of the invariants, and we
   obtain an intrinsic equation of the asymptotic lines. Finally, we give a
   simple condition which guarantees the existence of an isometric
   reduction of codimension of the surface into R-4.}},
