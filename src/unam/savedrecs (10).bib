@article{ ISI:000445650100005,
Author = {Huesca-Perez, Maria Elena and Sheinbaum-Pardo, Claudia and Koeppel,
   Johann},
Title = {{From global to local: impact assessment and social implications related
   to wind energy projects in Oaxaca, Mexico}},
Journal = {{IMPACT ASSESSMENT AND PROJECT APPRAISAL}},
Year = {{2018}},
Volume = {{36}},
Number = {{6}},
Pages = {{479-493}},
Abstract = {{In order to forecast likely impacts, Environmental Impact Assessment
   (EIA) has been applied worldwide. There are numerous social and cultural
   impacts that can hardly be addressed in a single EIA, and raise the
   awareness of social issues and proper assessment therein. This paper
   analyzes the social conflicts related to wind development projects in
   the Isthmus of Tehuantepec, Mexico using a qualitative approach. By
   conducting a Constellation Analysis (CA) in our case studies, this
   article aims at: 1) identifying the challenges and improvement
   opportunities for the social and environmental assessment of wind energy
   projects in Oaxaca, and 2) presenting CA as a novel approach to
   visualize and identify social and environmental issues. This paper
   emphasizes the relevance of earlier assessment of social implications to
   projects. This example can contribute to a better understanding of
   further research at Mexico's regional and national levels, as well as in
   other regions or countries with similar development patterns.}},
