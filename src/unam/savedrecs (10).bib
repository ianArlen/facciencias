@inproceedings{ ISI:000445102200001,
Author = {Ortega, Isaac and Espinosa-Perez, Gerardo and Montoya Giraldo, Oscar
   Danilo and Garces Ruiz, Alejandro and Grisales Norena, Luis Fernando},
Book-Group-Author = {{IEEE}},
Title = {{Current Control Mode in PV Systems Integrated with DC-DC Converters for
   MPPT: an IDA-PBC Approach}},
Booktitle = {{2018 IEEE GREEN TECHNOLOGIES CONFERENCE (GREENTECH)}},
Series = {{IEEE Green Technologies Conference}},
Year = {{2018}},
Pages = {{1-6}},
Note = {{IEEE Green Technologies Conference (GreenTech), Austin, TX, APR 04-06,
   2018}},
Organization = {{IEEE; IEEE Comp Soc}},
Abstract = {{In this paper, an interconnection and damping assignment passivity-based
   control (IDA-PBC) theory is employed to obtain maximum power point
   tracking (MPPT) for a photovoltaic (PV) module. A current control mode
   is selected to obtain the general control law, which guarantees
   exponential stability of the system in the sense of Lyapunov. The
   current is selected as the objective of control in this paper, due to
   the variations of irradiance and temperature on the PV module to produce
   the most impact in the current provided by the panel in comparison with
   its voltage profile. A modification of the classical IDA-PBC theory is
   employed to control the dynamical system under trajectory tracking.
   Simulation results show the capacity of the proposed control to extract
   the maximum power from the PV module under high changes in the
   irradiance and temperature. All simulations are conducted in
   MATLAB/Simulink.}},
