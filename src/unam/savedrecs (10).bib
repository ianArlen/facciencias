@inproceedings{ ISI:000451582300015,
Author = {Arroyo Ginez, M. and Espinoza Vazquez, A. and Rodriguez Gomez, F. J.},
Editor = {{RodriguezGomez, FJ and Nava, JL and OrozcoCruz, R and PeraltaHernandez, JM and PerezSegura, T}},
Title = {{Evaluation of Corrosion in Standard Bronze with Uric Acid, Chlorides and
   a Mixture of Both Compounds}},
Booktitle = {{XXXII NATIONAL CONGRESS OF THE MEXICAN SOCIETY OF ELECTROCHEMISTRY/10TH
   MEETING OF THE ECS MEXICAN SECTION}},
Series = {{ECS Transactions}},
Year = {{2018}},
Volume = {{84}},
Number = {{1}},
Pages = {{125-130}},
Note = {{32nd National Congress of the Mexican-Electrochemical-Society (MES) /
   10th Meeting of the Mexican-Section of the Electrochemical-Society
   (ECS), Guanajuato, MEXICO, JUN 05-08, 2017}},
Organization = {{Mexican Electrochem Soc; Electrochem Soc, Mexico Sect}},
Abstract = {{Bronze is used for the craftwork of precious metals, as in the creation
   of statues and bells. Currently, this sort of artistic work shows
   evident structural damage due to the presence of chlorides in the air in
   seashore settings with high relative humidity, as well as in the feces
   of birds and reptiles which are in contact with the metallic surfaces of
   the artworks. The chemical damage that these factors produce in
   copper-based alloys has not been studied in depth. In this paper, a
   gravimetric assay was applied, as well as different electrochemical
   techniques in order to evaluate the corrosive behaviour of a standard
