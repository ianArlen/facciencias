@article{ ISI:000435420000001,
Author = {Camargo, Javier and Macias, Sergio and Uzcategui, Carlos},
Title = {{ON THE IMAGE OF JONES' SET FUNCTION T}},
Journal = {{COLLOQUIUM MATHEMATICUM}},
Year = {{2018}},
Volume = {{153}},
Number = {{1}},
Pages = {{1-19}},
Abstract = {{We study possible images of Jones' set function T. In particular, we are
   interested in when either T(F-1 (X)) or T(2(x)) is finite or countable.
   We introduce the notion of omega-indecomposable continuum as a
   generalization of the well known concept of n-indecomposable continuum.
   We also present results about connectedness and compactness of T(2(x)).
   Finally, we give a generalization, to continua with the property of
   Kelley, of a couple of results known for homogeneous continua.}},
