@article{ ISI:A1993MM59700009,
Author = {DELGADILLO, C},
Title = {{THE NEOTROPICAL AFRICAN MOSS DISJUNCTION}},
Journal = {{BRYOLOGIST}},
Year = {{1993}},
Volume = {{96}},
Number = {{4}},
Pages = {{604-615}},
Month = {{WIN}},
Abstract = {{The neotropical flora shares with Africa about 334 specific and
   infraspecific moss taxa. Their disjunct range may be explained by an
   ancient land connection or by long-distance dispersal Certain species
   may represent lineages whose taxonomic identity has been preserved for
   millions of years, but in species with rapid evolutionary rates the
   long-distance dispersal hypothesis is favored. An alternate explanation
   for the neotropics proposes secondary centers of dispersal from an
   ancestral continuous distribution in western Gondwanaland. Other
   hypotheses are inadequate because they concern themselves with few
   species or with portions of the general range.}},
