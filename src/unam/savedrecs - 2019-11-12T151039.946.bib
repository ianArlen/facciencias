@article{ ISI:A1993MP62300004,
Author = {CASTILLO, RQ},
Title = {{THE INFLUENCE OF A TUTORIAL SYSTEM ON THE ACADEMIC-ACHIEVEMENT OF
   OUTSTANDING BACCALAUREATE STUDENTS}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1993}},
Volume = {{10}},
Number = {{2}},
Pages = {{145-152}},
Month = {{DEC}},
Abstract = {{Many educational institutions usually limit their non-mainstream
   programs to support low achievers, and tend to neglect outstanding
   students, assuming they do not need additional assistance to maintain
   their standard of excellency. The present study investigated the
   influence of the support given by tutors on academic achievement of
   outstanding students. Nine hundred outstanding students were selected
   from nine baccalaureate schools in Mexico City. They were assigned to
   two groups: one receiving assitance from tutors, the other with no
   additional support. Results showed that students adviced by tutors
   improved, or at least kept their grades, in a better ratio (78\%) than
   the group of outstanding students whithout tutorial assistance (42\%).
   The difference between both groups was both statistically and
   educationally significant.}},
