@article{ ISI:A1993LC94300004,
Author = {STUART, RG},
Title = {{STRUCTURE OF THE Z(0) RESONANCE AND THE PHYSICAL-PROPERTIES OF THE Z(0)
   BOSON}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{1993}},
Volume = {{70}},
Number = {{21}},
Pages = {{3193-3196}},
Month = {{MAY 24}},
Abstract = {{The proximity of multiparticle production thresholds means that the Z0
   resonance is formed, not by a single particle, but by a `'multiplet'' of
   distinct Z0 bosons differing slightly in mass. The effects that these
   thresholds can have on the gauge-invariant pole expansion near the Z0
   resonance are studied. It is shown how this expansion naturally yields
   physical quantities from which the mass and partial widths of the
   unstable Z0 bosons can be defined. The recent discussion on the
   definition of the Z0 boson mass is reviewed and commented upon,
   stressing the distinction between the renormalized and physical masses.}},
