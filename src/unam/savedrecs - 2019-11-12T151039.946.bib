@article{ ISI:A1993LJ00800008,
Author = {CUEVAS, A and CHAPA, M and SILVA, M and MENCHACAROCHA, A},
Title = {{FISSION OF DROPS INDUCED BY ANGULAR-MOMENTUM}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1993}},
Volume = {{39}},
Number = {{3}},
Pages = {{428-438}},
Month = {{JUN}},
Abstract = {{The collisions of m = 0.3-2.0 g, v = 5-50 cm/s, mercury drops axe
   studied experimentally. A transition between a fusion and a fission
   outcome is observed, and found to be influenced by angular momentum. The
   mass dependence measured for the limiting angular momentum L(c)(ex) is
   compared with predictions L(c)(th) of a surface potential, the rotating
   liquid-drop model widely used in nuclear physics. A systematic L(c)(ex)
   less-than-or-equal-to L(c)(th) discrepancy is found in these and in
   other drop collision data. Dynamical considerations, and the use of more
   elaborate surface shapes than those assumed by the model are found to
   reduce the disagreement.}},
