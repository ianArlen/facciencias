@article{ ISI:A1993LD03100001,
Author = {CHICUREL, R},
Title = {{DUAL-FUEL SYSTEM TO PROVIDE A VARIABLE OCTANE MIXTURE TO AN ENGINE}},
Journal = {{ENERGY}},
Year = {{1993}},
Volume = {{18}},
Number = {{6}},
Pages = {{611-614}},
Month = {{JUN}},
Abstract = {{The fuel octane required for knock-free operation of a spark-ignition
   engine varies widely with load. Therefore, the octane rating of the fuel
   used is under-utilized, except near full loading operating conditions.
   The cost and energy expenditure of producing gasoline increase with
   octane rating. This observation led us to the idea of a dual-fuel system
   to provide an adjustable mixture of two fuels of different octane
   ratings that would match the instantaneous requirements of the engine.
   In a van equipped with a prototype of the system, approximately 50\% of
   the gasoline consumed in city driving was regular (81 octane) and 50\% a
   gasoline with an octane rating of only 60.}},
