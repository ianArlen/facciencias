@article{ ISI:A1993MH74800032,
Author = {NOVARO, O},
Title = {{IMPORTANCE OF THE EXCITED-STATES OF TRANSITION-METAL ATOMS FOR CATALYTIC
   AND PHOTOCATALYTIC PROCESSES}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE-THEOCHEM}},
Year = {{1993}},
Volume = {{106}},
Pages = {{217-227}},
Month = {{NOV 14}},
Abstract = {{A systematic study of the reaction of the H-2 molecule with the ground,
   excited and ionic states of several metal atoms (Ru, Pd, Pt, Ni, Zn and
   Ga) shows that it is always the maximum d-occupancy states of the
   transition metals that are responsible for H-2 capture and activation.
   In general, the maximum d-occupancy states are highly excited states of
   the transition metal, so this implies a series of symmetry-avoided
   crossings before the MH(2) molecule is stabilized, including the
   interesting case of Herzberg-Teller couplings in the case of zinc ions.}},
