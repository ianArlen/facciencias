@article{ ISI:A1993LF07100015,
Author = {NAVARRO, O and WANG, C},
Title = {{ELECTRONIC CORRELATION IN 2-DIMENSIONAL SYSTEMS}},
Journal = {{SURFACE SCIENCE}},
Year = {{1993}},
Volume = {{287}},
Number = {{B}},
Pages = {{622-626}},
Month = {{MAY 10}},
Abstract = {{We introduce a simple mapping method to study local electron
   correlations in solids using the extended Hubbard Hamiltonian with
   on-site and inter-site interactions. This method consists of mapping the
   Hubbard problem onto a tight-binding case in a higher dimensional space.
   For systems containing few electrons this mapping allows us to find an
   exact solution of the extended Hubbard Hamiltonian. As an example, we
   applied this mapping method to analyze the local pairing energy and the
   coherence length for a square lattice.}},
