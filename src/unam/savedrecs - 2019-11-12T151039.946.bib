@article{ ISI:A1993MP41200012,
Author = {DELAMORA, MP and HERNANDEZGOMEZ, AM and MENDEZFRANCO, J and FUXE, K},
Title = {{CHOLECYSTOKININ-8 INCREASES K+-EVOKED {[}H-3] GAMMA-AMINOBUTYRIC-ACID
   RELEASE IN SLICES FROM VARIOUS BRAIN-AREAS}},
Journal = {{EUROPEAN JOURNAL OF PHARMACOLOGY}},
Year = {{1993}},
Volume = {{250}},
Number = {{3}},
Pages = {{423-430}},
Month = {{DEC 21}},
Abstract = {{{[}H-3]gamma-Aminobutyric acid (GABA) release was studied in rat brain
   slices in the absence or presence of cholecystokinin-8 (CCK-8).
   {[}H-3]GABA release under the conditions used was Ca2+-dependent and
   insensitive to the presence of the glial uptake blocker beta-alanine.
   While the basal release of {[}H-3]GABA was not affected by CCK-8, the
   K+-stimulated release of {[}H-3]GABA was significantly enhanced by 300
   nM of CCK-8 in the caudate putamen, the substantia nigra, the
   hippocampal formation and the parietofrontal cortex. In the cerebral
   cortex the CCK-8 enhancement of {[}H-3]GABA release was
   concentration-dependent and abolished by the CCKB receptor antagonists
   PD135,158 (1.0 nM) and L-365,260 (100 nM). A significant counteraction
   of the CCK-8 action was also found with the CCKA receptor antagonist
   L-364,718 (100 nM) but only in concentrations at which both CCKA. and
   CCKB receptors are blocked. No CCK-8 effects on {[}H-3]GABA release were
   observed when tetrodotoxin was superfused 5 min before the K+-induced
   {[}H-3]GABA release. It is suggested that the enhancing actions of CCK-8
   on K+-stimulated {[}H-3]GABA release is mainly related to an activation
   of CCKB receptors.}},
