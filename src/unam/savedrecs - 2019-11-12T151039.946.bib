@article{ ISI:A1993LV78600027,
Author = {MICHAELIAN, K and MENCHACAROCHA, A and BELMONTMORENO, E},
Title = {{CALIBRATION OF SCINTILLATION DETECTORS USING A SPECIFIC LUMINESCENCE
   MODEL}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
Year = {{1993}},
Volume = {{334}},
Number = {{2-3}},
Pages = {{457-460}},
Month = {{OCT 1}},
Abstract = {{An algorithm is described which determines the light response function
   L(E) for scintillation detectors using a specific luminescence model.
   The light response is simulated from purely physical principles and
   normalized to the experimental L vs DELTAE data. Calibration of the
   detector is therefore facilitated by requiring fewer (L, DELTAE) points,
   and can be safely interpolated or extrapolated into regions where
   calibration points do not exist. This article describes the algorithm as
   applied to data from Si-CsI(Tl) DELTAE-L arrays, however, its potential
   application is more general.}},
