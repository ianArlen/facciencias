@article{ ISI:A1993LN69800007,
Author = {DEHARO, ML and GARZO, V},
Title = {{ON THE BURNETT EQUATIONS FOR A DENSE MONATOMIC HARD-SPHERE GAS}},
Journal = {{PHYSICA A-STATISTICAL MECHANICS AND ITS APPLICATIONS}},
Year = {{1993}},
Volume = {{197}},
Number = {{1-2}},
Pages = {{98-112}},
Month = {{JUL 15}},
Abstract = {{The Burnett transport equations for a dense hard-sphere gas as given by
   the standard (SET) and revised (RET) Enskog theories are discussed. It
   is shown that beyond the Navier-Stokes approximation, the expressions
   for the collisional transfer contributions to the molecular fluxes are
   formally different in both theories. In contrast, by using the
   Chapman-Enskog method, the Burnett approximation to the single particle
   distribution function is found to be identical. The differences between
   the SET and the RET in this hydrodynamic order appear only through two
   linearized Burnett transport coefficients in the momentum flux. The
   numerical evaluation of these differences is explicitly carried out.}},
