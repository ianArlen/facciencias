@article{ ISI:A1993LR83800010,
Author = {MEABURN, J and LOPEZ, JA},
Title = {{OPTICAL EVIDENCE FOR DENSE, NEUTRAL GLOBULES IN THE DUMBBELL
   PLANETARY-NEBULA (NGC-6853, M27)}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{1993}},
Volume = {{263}},
Number = {{4}},
Pages = {{890-894}},
Month = {{AUG 15}},
Abstract = {{CCD images of the bright core of the Dumbbell planetary nebula have been
   obtained with the 2.2-m telescope at the San Pedro Martir Observatory in
   the light of the {[}O III] 5007-angstrom and He II 4686-angstrom nebular
   emission lines. Dusty globules of molecular gas in the near side of the
   outer {[}N II] 6584-angstrom and {[}O I] 6300-angstrom emitting shell of
   the central bar of the nebula are silhouetted against the more central
   {[}O III] and He II emissions. The dimensions, masses and densities of
   the 11 most prominent globules are derived. A typical diameter is
   almost-equal-to 1.5 x 10(16) cm, a typical mass is almost-equal-to
   10(-4) M. and the mean gas density is almost-equal-to 5 x 10(4) cm-3.
   It is suggested that CO observations of the radial velocities of these
   globules would help to identify their origin.}},
