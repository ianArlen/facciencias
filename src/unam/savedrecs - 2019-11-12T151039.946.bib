@article{ ISI:A1994MW25900011,
Author = {OLMEDO, F and ITURBE, F and GOMEZHERNANDEZ, J and LOPEZMUNGUIA, A},
Title = {{CONTINUOUS PRODUCTION OF 5'-RIBONUCLEOTIDES FROM YEAST RNA BY HYDROLYSIS
   WITH IMMOBILIZED 5'-PHOSPHODIESTERASE AND 5'-ADENYLATE DEAMINASE}},
