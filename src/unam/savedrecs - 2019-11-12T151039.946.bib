@article{ ISI:A1993MN37000004,
Author = {CABRERA, A and VELASCO, L and CHRIST, ML and SALMON, M and ARIAS, JL},
Title = {{ORGANIC CARBONATE ACTIVATION - ALDEHYDE ACETALIZATION STUDY USING
   SN{[}CO(CO)4](4) AS CATALYST IN THE ACTIVATION OF PROPYLENE CARBONATE}},
Journal = {{JOURNAL OF MOLECULAR CATALYSIS}},
Year = {{1993}},
Volume = {{85}},
Number = {{3}},
Pages = {{279-285}},
Month = {{DEC 14}},
Abstract = {{The catalytic activity of the bimetallic system Sn{[}Co(CO)4]4 (I), is
   evaluated on the basis of the activation of organic carbonates to
   produce the acetalization of n-hexanal under pressure, by the following
   reaction: R-CHO + (R'O)2CO-->RCH(OR')2 + CO2 In the process it was found
   that the yield of the obtained product is dependent on the catalyst
   amount, the reaction temperature, and the structure of the organic
   carbonate. The precursor activity of (I) compared with Co2 (CO)8 (II)
   showed important differences, suggesting significant participation of
   the tin species in the catalytic cycle. The divalent tin complex Sn
   {[}Co(CO)4]2 can reasonably be expected to be one of the active
   catalytic species involved in the process. The occurrence of an aldolic
   condensation was observed in all the examples as a secondary reaction.}},
