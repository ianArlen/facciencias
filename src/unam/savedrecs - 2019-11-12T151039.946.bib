@article{ ISI:A1993LK38000006,
Author = {SOULLARD, J and CRUZ, SA and CABRERATRUJILLO, R},
Title = {{CHEMICAL-BOND EFFECTS ON THE LOW-ENERGY ELECTRONIC STOPPING POWER OF LI
   AND HE IONS ON SATURATED ALCOHOLS, ETHERS AND AMINES}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{1993}},
Volume = {{80-1}},
Number = {{1}},
Pages = {{20-23}},
Month = {{JUN}},
Abstract = {{A method developed recently to account for bond effects in the
   calculation of the electronic stopping cross section of heavy ions in
   compound materials is applied to helium and lithium projectiles
   interacting with large saturated molecules like alcohols, ethers and
   amines. The method is free of adjustable parameters and is based on the
   modified Firsov model using the floating spherical Gaussian
   representation of molecular orbitals together with the concept of
   molecular fragments.}},
