@article{ ISI:A1994PP35300002,
Author = {MORENOMARTINEZ, E and VAZQUEZBADILLO, ME and NAVARRETEMAYA, R and
   RAMIREZGONZALEZ, J},
Title = {{SEED VIABILITY OF DIFFERENT VARIETIES OF BEAN (PHASEOLUS-VULGARIS L)
   STORED UNDER LOW AND HIGH RELATIVE-HUMIDITY}},
Journal = {{SEED SCIENCE AND TECHNOLOGY}},
Year = {{1994}},
Volume = {{22}},
Number = {{2}},
Pages = {{195-202}},
Abstract = {{To obtain information about the variability between beam genotypes with
   respect to seed storability, 15 varieties of beans were grown,
   harvested, and handled together to allow for comparative storage tests
   under relative humidities (RH) of 75\% and 85\% at 25-degrees-C. Most of
   the bean varieties stored for 180 days at 75\% RH maintained high seed
   germination rates (74 to 98\%), except the varieties Bayo 400 and Rosa
   de Castilla with germination rates of 62 and 59\%, respectively. All the
   varieties stored 60 days at 85\% RH had a drastic decrease in seed
   viability with germinations between 10 and 68\% (with the exception of
   Negro Chiapas and Negro Veracruz). The varieties Negro Chiapas and Negro
   Veracruz had seed germination rates of 88 and 80\%, respectively, at
   85\% RH. At 90 days of storage, the varieties Negro Chiapas and Negro
   Veracruz had the highest germinations percentages, 58 and 42\%,
   respectively. The remaining varieties had germination percentages
   between 1 and 29\%. Among these were the varieties Bayo 400 (2\%) and
   Rosa de Castilla (1\%), which also had the lowest germination
   percentages at the 75\% RH storage environment. On the other hand, Negro
   Chiapas and Negro Veracruz were the varieties which showed good
   storability. Storage fungi did not appear to have an important role in
   loss of seed viability for the varieties Negro Chiapas and Negro
   Veracruz.}},
