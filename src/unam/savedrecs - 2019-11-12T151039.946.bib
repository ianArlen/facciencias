@article{ ISI:A1993LT84100045,
Author = {REYNAUD, E and DEDELATORRE, L and ZAPATA, O and LIEVANO, A and DARSZON,
   A},
Title = {{IONIC BASES OF THE MEMBRANE-POTENTIAL AND INTRACELLULAR PH CHANGES
   INDUCED BY SPERACT IN SWOLLEN SEA-URCHIN SPERM}},
Journal = {{FEBS LETTERS}},
Year = {{1993}},
Volume = {{329}},
Number = {{1-2}},
Pages = {{210-214}},
Month = {{AUG 23}},
Abstract = {{Signal transduction initiated by the egg peptide, speract, in sea urchin
   sperm is not fully understood. Hypotonically swollen sperm are a
   suitable model to study peptide signal transduction. lon substitution
   experiments now indicate (i) that the permeability to Na+, Ca2+, and
   Mg2+ contributes to the sperm resting membrane potential; (ii) the
   repolarization induced by nM concentrations of speract is Na+ dependent
   and mediated by an as yet unidentified channel; (iii) the depolarization
   triggered by nM concentrations of speract involves Ca2+ channels since
   it is Ca2+-dependent and blocked by Co2+ and Ni2+, two Ca2+ channel
   blockers; (iv) hyperpolarizing swollen sperm with valinomycin increases
   intracellular pH (pH(i)) in the same way as speract, thus the
   speract-induced hyperpolarization may be responsible for the pH(i)
   increase.}},
