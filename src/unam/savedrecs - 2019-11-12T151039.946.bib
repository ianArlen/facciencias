@article{ ISI:A1993LE32600047,
Author = {ANTILLON, A and GERMAN, G},
Title = {{NUMERICAL STUDY OF THE NAMBU-GOTO STRING MODEL AT FINITE-LENGTH AND
   TEMPERATURE}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{1993}},
Volume = {{47}},
Number = {{10}},
Pages = {{4567-4574}},
Month = {{MAY 15}},
Abstract = {{We study the Nambu-Goto model numerically for a string of finite length
   at an arbitrary temperature. The static quark-antiquark potential is
   investigated and it is found that the deconfinement radius r(dec) (the
   distance between quarks for which the potential vanishes) is independent
   of the temperature. For tau = tau(dec) (the temperature for which the
   string tension vanishes), the potential becomes a constant for large r,
   thus losing its `'confining'' property. This is clearly a consistent
   result with the interpretation of tau(dec) as a deconfinement
   temperature. For tau > tau(dec), solutions to the gap equations exist
   which allow us to have a well-defined quark potential although only for
   `'unconfining'' strings of a certain length. The string tension at
   finite temperature for a string of various lengths is also calculated as
   well as tau(dec)max (the maximum temperature for which a tension exists)
   which might signal a first-order transition to a deconfined phase.}},
