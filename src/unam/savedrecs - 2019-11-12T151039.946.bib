@article{ ISI:A1993KU02100017,
Author = {PALMA, V and TUDON, H and BUENTELLO, L and NAVA, S and OSTROSKY, P and
   SALAMANCA, F},
Title = {{METHODS FOR THE ANALYSIS OF CELLULAR KINETICS IN PHA-STIMULATED
   BLOOD-LYMPHOCYTES USING BRDU INCORPORATION - A COMPARATIVE-STUDY}},
Journal = {{MUTATION RESEARCH}},
Year = {{1993}},
Volume = {{286}},
Number = {{2}},
Pages = {{267-273}},
Month = {{APR}},
Abstract = {{The cellular cycle (cc) span was measured by using differential
   sister-chromatid staining (DSCS) and applying the following methods:
   cellular cycle time (cct) according to the graphic method of Dutrillaux
   and Fosse (G-cct); the analytical equation (A-cct) proposed in the
   present paper, and the average generation time (AGT) suggested by Ivett
   and Tice. The mean values obtained by the three methods were 12.5, 12.7,
   and 19.5 h, respectively. A-cct is the more precise method, since the
   equation of the analytical procedure allows the utilization of numerical
   data, and when the graphical method is used, the values plotted in a
   graph may vary according to the employed scale. Cct is the choice over
   AGT because the first evaluates actively dividing cells and only
   considers those at M2 or M3. It will be useful to study cell
   proliferation kinetics in genetic pathological conditions and to
   investigate with accuracy the effect of cytostatic and cytotoxic drugs.}},
