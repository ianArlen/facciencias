@article{ ISI:A1994MT09700029,
Author = {KOROLIJA, M and SHAPIRA, D and DELCAMPO, JG and CHAVEZ, E and CINDRO, N},
Title = {{PROTON-PROTON CORRELATIONS - DETERMINATION OF THE SOURCE SIZE AND
   LIFETIME FROM DEEP-INELASTIC COLLISIONS OF NI-58+NI-58 15 MEV/NUCLEON}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{1994}},
Volume = {{49}},
Number = {{1}},
Pages = {{272-278}},
Month = {{JAN}},
Abstract = {{Triple (p-p-fragment) coincidences from the Ni-58+Ni-58 reaction at 850
   MeV have been measured. By detecting the source of the emitted particles
   we were able to study the directional dependence of the p-p correlation
   function. From this dependence both the source size (r(0)) and the
   particle emission time (tau) have been extracted. The extracted p-p
   correlation functions could be fitted with source size and lifetime in
   the range of 3.3(-0.7)(+1.3)fm and 3.2(-2.2)(+1.3)X10(-22) s,
   respectively.}},
