@article{ ISI:A1993MH73400003,
Author = {MENDOZA, S and CIPRIAN, A and CAMACHO, J and MONTARAZ, JA},
Title = {{COMPARISON OF 3 BIOLOGICAL ASSAYS FOR THE DETECTION OF TOXIGENIC
   PROPERTIES OF BORDETELLA-BRONCHISEPTICA ASSOCIATED WITH ATROPHIC
   RHINITIS}},
Journal = {{VETERINARY MICROBIOLOGY}},
Year = {{1993}},
Volume = {{36}},
Number = {{3-4}},
Pages = {{215-219}},
Month = {{SEP}},
Abstract = {{The toxigenic properties of Bordetella bronchiseptica strains associated
   with clinical atrophic rhinitis were compared using three biological
   assays: the guinea pig skin test, the mouse spleen atrophy and the
   suckling mice mortality assay. Forty one sick pigs were cultured and, in
   20 of them B. bronchiseptica strains were isolated. Eighteen of the
   strains were toxigenic by at least one test. However, positivity did not
   always coincide amongst assays. This discrepancy raises the possibility
   of either differences in sensitivity between assays or the existence of
   more than one toxin.}},
