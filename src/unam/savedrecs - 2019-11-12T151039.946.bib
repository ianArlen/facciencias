@article{ ISI:A1993LF06700097,
Author = {VAREA, C and ROBLEDO, A},
Title = {{MAGNITUDE OF THE PREWETTING BOUNDARY TENSION NEAR WETTING FOR
   SHORT-RANGE FORCES}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{1993}},
Volume = {{47}},
Number = {{5}},
Pages = {{3772-3775}},
Month = {{MAY}},
Abstract = {{We determine in a mean field approximation the spin-1/2 Ising model line
   tension tau along the boundary between surface states at the prewetting
   transition in the neighborhood of the wetting transition at bulk phase
   coexistence. We find very close agreement with the predictions of the
   interface displacement model for short-range interactions, i.e., tau
   increases (with a square-root dependence on the bulk external field h)
   towards a finite limit with diverging slope at wetting. Our findings
   help both in settling the discussion on the limiting value of tau and in
   understanding the origin of its singular behavior.}},
