@article{ ISI:A1993LX14300041,
Author = {TALANQUER, V and OXTOBY, DW},
Title = {{NUCLEATION IN DIPOLAR FLUIDS - STOCKMAYER FLUIDS}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{1993}},
Volume = {{99}},
Number = {{6}},
Pages = {{4670-4679}},
Month = {{SEP 15}},
Abstract = {{We have used density functional theory to begin the study of nucleation
   in dipolar fluids. The simplest case of a system composed of Stockmayer
   molecules has been considered. Under a Weeks-Chandler-Andersen
   perturbation scheme we obtain the corresponding bulk phase diagram and
   interfacial properties of planar and spherical liquid-vapor interfaces.
   Molecules tend to align perpendicular to the interface on the liquid
   side of small droplets, but the overall effect of this on free energies
   is negligible. The resulting nonclassical effects on nucleation rates
   are, hence, close to those found in nonpolar Lennard-Jones fluids.}},
