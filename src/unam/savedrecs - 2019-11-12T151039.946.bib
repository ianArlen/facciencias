@article{ ISI:A1993LK51000006,
Author = {DODONOV, VV and KLIMOV, AB and NIKONOV, DE},
Title = {{QUANTUM PHENOMENA IN RESONATORS WITH MOVING WALLS}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{1993}},
Volume = {{34}},
Number = {{7}},
Pages = {{2742-2756}},
Month = {{JUL}},
Abstract = {{New solutions for the mode functions of the electromagnetic field in
   ideal cavity which boundary oscillates at a resonance frequency are
   obtained in the long-time limit. The rate of photons creation from
   initial vacuum state is shown to be time independent and proportional to
   the amplitude of oscillations and the resonance frequency. Temperature
   corrections are evaluated. The squeezing coefficients for the quantum
   states of the field generated are calculated, as well as the backward
   reaction of the field on the vibrating wall.}},
