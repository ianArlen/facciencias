@article{ ISI:A1993KQ74100012,
Author = {PRIETO, FE and RENERO, C},
Title = {{REDUCED VARIABLES IN HIGH-PRESSURE PHYSICS}},
Journal = {{JOURNAL OF PHYSICS AND CHEMISTRY OF SOLIDS}},
Year = {{1993}},
Volume = {{54}},
Number = {{2}},
Pages = {{217-220}},
Month = {{FEB}},
Abstract = {{Reduced variables have long been used in high pressure physics, but in
   most of the cases as an abridged algebraic notation, with no theoretical
   support. in this paper we will first briefly recall the reduced
   variables formalism obtained in an earlier work, based on a law of
   corresponding states as well as in the scaling factors which ensued. We
   will then show that the reduced variables most widely used to describe
   static or dynamic high pressure states accomplish their purpose only
   because the law of corresponding states at high pressures is hidden
   behind their definitions. A universal equation for the shock Hugoniot is
   presented as a consequence of the law of corresponding states. Based on
   goodness of fit considerations, criteria for comparison and equivalence
   between scaling fractors are introduced. This comparison shows that
   scaling factors resulting from dimensional analysis are either
   equivalent, or result as particular cases of those scaling factors
   obtained from similarity considerations.}},
