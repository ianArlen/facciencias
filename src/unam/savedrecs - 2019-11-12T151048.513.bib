@article{ ISI:A1993KP93600077,
Author = {BARRIO, RA and WANG, CM},
Title = {{SOME PHYSICAL INFERENCES FROM THE QUASI-CRYSTALLINE TOPOLOGY OF PENROSE
   LATTICES}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{1993}},
Volume = {{153}},
Pages = {{375-379}},
Month = {{FEB}},
Abstract = {{The vertex problem of a Penrose lattice is analyzed, using a nearest
   neighbor tight-binding Hamiltonian. It is discovered that there are
   connectivity rules, which suffice to predict some important features of
   the eigenvalue spectrum, such as the bandwidth, the existence and size
   of a central gap, and the wave functions of the highly degenerate state
   at the central peak (E = 0). The theoretical predictions are tested
   numerically, by using a novel renormalization method to diagonalize the
   Hamiltonian in Penrose lattice of large size. In particular, the
   existence of connected paths between sites of certain coordination
   determines the nature of the states around the center of spectrum.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
