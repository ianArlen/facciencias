@article{ ISI:A1992JX47400018,
Author = {JUAREZGORDIANO, C and HERNANDEZ, T and GONZALEZ, I and QUIROZGUTIERREZ,
   A},
Title = {{ELECTROCHEMICAL STUDIES ON SOME CU-MO INTERACTIONS .2. SYNTHESIS AND
   ELECTROCHEMICAL-BEHAVIOR OF THE HISTIDYL-HISTIDINE-CU COMPLEX}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1992}},
