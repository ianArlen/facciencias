@article{ ISI:A1992HZ03800021,
Author = {TEHUACANERO, S and HERRERA, R and AVALOS, M and YACAMAN, MJ},
Title = {{HIGH-RESOLUTION TEM STUDIES OF GOLD AND PALLADIUM NANO-PARTICLES}},
Journal = {{ACTA METALLURGICA ET MATERIALIA}},
Year = {{1992}},
Volume = {{40}},
Number = {{7}},
Pages = {{1663-1674}},
Month = {{JUL}},
Abstract = {{In the present work we analyze nano-particles of gold and palladium
   using a combination of High Resolution Electron Microscopy (HREM) and
   image processing techniques. The experimental results are compared with
   calculations of images using dynamical diffraction theory. We focus
   mainly on the decahedral (D(h)) and icosahedral (Ic) Particles. We found
   that the decahedral particles correspond well with the model for a
   perfect MPT. The icosahedral particle on the other hand appears to be
   much more distorted than the MPT model predicts. Images of
   polycrystalline and amorphous particles are also shown. No significant
   differences between Au and Pd were found.}},
