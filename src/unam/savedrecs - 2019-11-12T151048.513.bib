@article{ ISI:A1992KA60500016,
Author = {MOSHINSKY, M and MESA, AD},
Title = {{TRANSFORMATION FROM A U(3) TO A PSEUDO U(3) BASIS OF THE MATRIX
   REPRESENTATION OF A DEFORMED NILSSON HAMILTONIAN}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1992}},
Volume = {{38}},
Number = {{2}},
Pages = {{146-151}},
Month = {{NOV}},
Abstract = {{In recent publications Castanos, Moshinsky and Quesne obtained
   explicitly the operator U that transforms us from a U(3) to pseudo U(3)
   basis. They then applied it to a spherical Nilsson Hamiltonian to show
   that it transforms into a similar one in which the coefficient of the
   spin orbit term changes and, in fact, becomes very small for the
   parameters of the Nilsson Hamiltonian taken for experimental results in
   the spectra of heavy nuclei. In this paper we consider the deformed
   Nilsson Hamiltonian H. We show how we can obtain its matrix elements in
   an oscillator basis with spin and we then obtain the corresponding
   results for H = UHU dagger. Thus we have the ground work for eventually
   obtaining the operator form of H.}},
