@article{ ISI:A1991JQ51500002,
Author = {PAEZOSUNA, F and BOJORQUEZLEYVA, H and OSUNALOPEZ, JI},
Title = {{ACCUMULATION AND DISTRIBUTION OF PHOSPHORUS IN SEDIMENTS OF THE GULF OF
   CALIFORNIA}},
Journal = {{MARINE MINING}},
Year = {{1991}},
Volume = {{10}},
Number = {{4}},
Pages = {{285-301}},
Month = {{OCT-DEC}},
Abstract = {{The distribution of phosphorus and its association with organic carbon
   and carbonates were determined in 120 surface sediment samples collected
   from the Gulf of California. The results indicate that inorganic
   phosphorus is the most abundant form. Higher concentrations occur only
   as patches on the continental shelf and upper slope, and the highest
   value coincides roughly with the plankton bloom areas that also
   correspond with zones of upwelling. Based on phosphorus analysis and
   sedimentation rates of 26 cores, the accumulation rates of total and
   inorganic phosphorus were estimated. They were lowest in the slowly
   accumulating sediments of the basin depressions and comparable or up to
   about three orders of magnitude higher in the rapidly deposited
   sediments of the upper continental slope of the gulf. In Guaymas Basin
   (central portion), the rates suggest that phosphorus should be
   incorporated in the sedimentary column before being ejected onto the
   surface sediments when the hydrothermal fluid passes through the
   sedimentary column in the south trough. Phosphorus burial rates indicate
   that Guaymas is the basin that receives the major (17.6 x 10(9) g/yr)
   and Farallon the minor (10.2 x 10(9) g/yr) proportion of phosphorus.}},
