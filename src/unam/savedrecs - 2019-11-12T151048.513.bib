@article{ ISI:A1993KY13600086,
Author = {MENCHACAROCHA, A and CUEVAS, A and CHAPA, M and SILVA, M},
Title = {{ROTATING-LIQUID-DROP MODEL LIMIT TESTED ON MACROSCOPIC DROPS}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{1993}},
Volume = {{47}},
Number = {{2}},
Pages = {{1433-1436}},
Month = {{FEB}},
Abstract = {{The collisions of m = 0.3-2.0 g, v = 5-50 cm/s mercury drops are studied
   experimentally. A transition between a fusion and a nonfusion outcome is
   observed, and found to be influenced by angular momentum. The mass
   dependence measured for the limiting angular momentum L(c)(expt) is
   compared with predictions L(c)(theor) of a surface-potential model
   widely used in nuclear physics. A systematic L(c)(expt)
   less-than-or-equal-to L(c)(theor) discrepancy is found in these and in
   other drop collision data. Dynamical considerations and the use of more
   elaborate surface shapes than those assumed by the model are found to
   reduce the disagreement.}},
