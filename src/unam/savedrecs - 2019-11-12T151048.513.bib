@article{ ISI:A1992JQ94300018,
Author = {SAGER, WW and MORTERAGUTIERREZ, CA and URRUTIAFUCUGAUCHI, J},
Title = {{PALEOMAGNETIC EVIDENCE OF TERTIARY TECTONIC ROTATION IN WEST TEXAS}},
Journal = {{GEOLOGY}},
Year = {{1992}},
Volume = {{20}},
Number = {{10}},
Pages = {{935-938}},
Month = {{OCT}},
Abstract = {{Paleomagnetic samples obtained from Rawls Formation (26.9-28.3 Ma) lava
   flows of Bofecillos volcano in west Texas give a paleomagnetic pole at
   lat 76.5-degrees-N, long 348.0-degrees-E (alpha95 = 9.4-degrees; N =
   20). This pole is displaced significantly from North American reference
   poles of similar age, implying a clockwise rotation of 21.3-degrees +/-
   8.8-degrees. It is also consistent with other paleomagnetic data from
   west Texas. We argue that the discordance is a result of vertical-axis
   crustal rotation in west Texas caused by shear deformation that Was
   probably related to Basin and Range tectonics.}},
