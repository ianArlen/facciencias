@article{ ISI:A1993ML58600064,
Author = {KAPLAN, IG and SANTAMARIA, R and NOVARO, O},
Title = {{THEORETICAL-STUDY OF THE GEOMETRIC STRUCTURES AND ENERGETIC PROPERTIES
   OF ANIONIC CLUSTERS - AG-N(-) (N=2 TO 6)}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{1993}},
Number = {{27}},
Pages = {{743-753}},
Abstract = {{The systematic quantum-mechanical investigation of the stable geometries
   and some energetic characteristics of the anionic silver clusters up to
   the hexamer are performed by an all-electron spin-density approach with
   nonlocal corrections included. Calculated vertical detachment energies
   are in good agreement with the experimental ones without any scaling
   procedure. The fragmentation energy for the channel Ag-n(-) -->
   Ag-n-1(-) + Ag shows very pronounced oscillations for even-add n. The
   obtained stable geometries for Ag-n(-) (n = 2-4) are in agreement with
   previous calculations by Bauschlicher et al. {[}26,27]. To the contrary
   of the case of tetramers and pentamers we found that anionic and neutral
   hexamers have different stable geometries. (C) 1993 John Wiley and Sons,
   Inc.}},
