@article{ ISI:A1993MP62200004,
Author = {LARATAPIA, L and GOMEZALEGRIA, P and FUENTESMORALES, R},
Title = {{SOCIOCULTURAL CHANGES OF CONCEPTS OF ABNEGATION IN THE MEXICAN FAMILY -
   A STUDY RELATED TO SOCIAL-CHANGE}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1993}},
Volume = {{10}},
Number = {{1}},
Pages = {{29-35}},
Month = {{JAN-JUN}},
Abstract = {{The present paper analizes research results on changes developed in the
   concepts of abnegation in samples of school teenagers in Mexico City
   along a 30-years follow up through several studies. Results indicate
   significant changes in this typical Mexican socio-cultural premise.
   Changes seem more apparent in female than in male subjects including
   statistically significant differences. The study's general
   interpretation and discussion suggests that both genders have evolved
   toward a change in the conceptualization of abnegation, a concept that
   originally arrived with Catholic evangelization, as a desirable value.
   At the present time, however, adolescents progressively view it as a
   self-humiliating, old-fashioned and undesirable concept.}},
