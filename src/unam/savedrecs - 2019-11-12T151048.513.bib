@article{ ISI:A1992KB24400005,
Author = {RODRIGUEZ, LF and MIRABEL, IF and MARTI, J},
Title = {{THE RADIO COUNTERPART OF THE HARD X-RAY SOURCE GRS-1758-258}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1992}},
Volume = {{401}},
Number = {{1, 2}},
Pages = {{L15-L18}},
Month = {{DEC 10}},
Abstract = {{GRS 1758 - 258 and 1E 1740.7 - 2942 are two persistent hard X-ray
   sources in a region of about 100 deg2 around the Galactic center. The
   Galactic center annihilator, 1E 1740.7 - 2942, has recently been found
   to be associated with a compact radio source from which a double radio
   jet emanates. In this Letter we report the results of a deep 6 cm VLA
   mapping of the field around the GRS 1758 - 258 X-ray position. Our
   observations reveal, as in 1E 1740.7 - 2942, the presence of another
   triple radio source, whose central component is time variable and falls
   inside the error circle (10{''} of radius) of ROSAT. Within the
   positional uncertainty, this object appears to be the radio counterpart
   of GRS 1758-258.}},
