@article{ ISI:A1993KJ26800009,
Author = {MONTEMAYORALDRETE, J and MENDOZA, A and OROZCO, E},
Title = {{ABOUT THE INTERNAL-STRESS MEASUREMENTS DURING SIGMOIDAL TRANSIENT CREEP
   IN CU-16AT-PERCENT-AL}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{1993}},
Volume = {{160}},
Number = {{1}},
Pages = {{71-79}},
Month = {{JAN 15}},
Abstract = {{It is well established that during steady state power law creep, the
   main sources of long-range internal stress sigma(i) are the cell wall
   dislocations. However, the physical source of sigma(i) for sigmoidal
   transient creep is not so well established. From literature on the
   subject, the pioneer work due to Hasegawa, Karashima and Ikeuchi was
   taken for a deep analysis. According to our analysis the mobile
   dislocation must be taken into account in order to explain sigma(i)
   during sigmoidal transient creep. An experimental technique for
   determining sigma(i) during the primary stage of creep, based on
   previous ideas from Qian and Reed-Hill, is proposed.}},
