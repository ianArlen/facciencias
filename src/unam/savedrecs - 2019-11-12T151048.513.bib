@article{ ISI:A1992JR53300010,
Author = {JACOBCERVANTES, M and GALLARDOCABELLO, M and CHIAPPACARRARA, X and RUIZ,
   A},
Title = {{FEEDING REGIME OF OPISTHONEMA-LIBERTATE (PISCES, CLUPEIDAE) IN THE GULF
   OF CALIFORNIA}},
Journal = {{REVISTA DE BIOLOGIA TROPICAL}},
Year = {{1992}},
Volume = {{40}},
Number = {{2}},
Pages = {{233-238}},
Month = {{AUG}},
Abstract = {{The diet variation of thread herring Opisthonema libertate, in relation
   to age and latitudinal distribution in the Gulf of California, was
   analized during the 1987 fishing season. Diatoms were the most important
   components of dietary groups, followed by pelagic crustaceans, fish eggs
   and dinoflagellates. There were no significant variations with respect
   to age groups, but significant latitudinal differences for diet
   composition were found. These differences were attributed to the
   time-space distribution of the herring feeding resources. 0. libertate
   is an opportunistic forager, rather than a specialized filter feeder.}},
