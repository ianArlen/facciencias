@article{ ISI:A1992HB66600005,
Author = {CRUVINEL, PE and MASCARENHAS, S and MIRANDA, J and FLOCCHINI, RG},
Title = {{THE USE OF A PEROVSKITE CRYSTAL AS A DETECTOR FOR PROTON-BEAM CURRENT}},
Journal = {{IEEE TRANSACTIONS ON NUCLEAR SCIENCE}},
Year = {{1992}},
Volume = {{39}},
Number = {{1}},
Pages = {{25-28}},
Month = {{FEB}},
Abstract = {{Using a Perovskite crystal, a thermal detector has been designed for
   measurements of proton beam currents. For a given energy, the detector
   has a linear response with current intensity and an inverse response
   with chopping frequency. Measurements up to 50 nA (4.5 MeV H+) were
   carried out using a cyclotron, and a calibration curve is presented. The
