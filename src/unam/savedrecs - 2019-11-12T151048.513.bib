@article{ ISI:A1992JT55600010,
Author = {VILLARREAL, C and JAUREGUI, R and HACYAN, S and COCHO, G},
Title = {{QUANTUM NOISE IN RECTANGULAR CAVITIES}},
Journal = {{MODERN PHYSICS LETTERS A}},
Year = {{1992}},
Volume = {{7}},
Number = {{31}},
Pages = {{2957-2960}},
Month = {{OCT 10}},
Abstract = {{We calculate the energy spectrum of vacuum fluctuations for a massless
   scalar field which satisfies boundary conditions inside a wave guide or
   a rectangular box. The spectrum is piecewise continuous in the first
   case and discrete in the second, and exhibits resonances which
   correspond, as expected, to the energy levels of a particle in a box
   (with the difference that all these levels are ``occupied{''}). Since it
   is known that these fluctuations exhibit a very slow convergence to a
   Poisson distribution, we conjecture that a realistic detector must find
   severe quantum vacuum deviations from `white' noise, except in the
   non-realistic limit of extremely high frequencies.}},
