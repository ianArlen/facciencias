@article{ ISI:A1992KD36900026,
Author = {SEBASTIAN, PJ and GOMEZDASA, O and NAIR, PK},
Title = {{SCREEN PRINTED ZNCDS FILMS FOR OPTOELECTRONIC APPLICATIONS}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{1992}},
Volume = {{25}},
Number = {{12}},
Pages = {{1848-1850}},
Month = {{DEC 14}},
Abstract = {{Screen printing, a simple and versatile technique, is used for the
   preparation of ZnCdS thick films from a paste containing CdS and ZnCl2
   pigments. XRD studies of the screen printed films (sintered at
   260-degrees-C, the melting point of ZnCl2) have indicated the presence
   of CdCl2 and ZnS in the film, as well as CdS and ZnCl2. However, firing
   of the screen printed films at about 400-degrees-C transformed them into
   ZnCdS films. Suitably fired films with respect to time and temperature
   exhibited promising characteristics for solar cell application and also
   high photoconductivity, with a photo-to-dark-current ratio of five
   orders of magnitude, and very fast photoresponse. Measurement of the
   diffuse reflectance of the films in the UV-VIS region showed a shift in
   the band edge towards the lower wavelength side for those films fired at
   higher temperatures (400-degrees-C), which corresponds to ZnCdS films.}},
