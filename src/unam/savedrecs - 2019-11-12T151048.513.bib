@article{ ISI:A1993MJ37100003,
Author = {VAZQUEZYANES, C and OROZCO-SEGOVIA, A},
Title = {{PATTERNS OF SEED LONGEVITY AND GERMINATION IN THE TROPICAL RAIN-FOREST}},
Journal = {{ANNUAL REVIEW OF ECOLOGY AND SYSTEMATICS}},
Year = {{1993}},
Volume = {{24}},
Pages = {{69-87}},
Abstract = {{Tropical rainforest plants produce seeds showing a wide range of sizes,
   shapes, structures, chemical composition, water content, dormancy
   mechanisms, and patterns of longevity. Prompt germination seems to be
   the most common behavior, although there are many cases where
   germination is delayed by a hard coat or endogenous or enforced dormancy
   mechanisms. Some gap colonizers show sophisticated light or temperature
   regulated dormancy. Most rainforest seeds remain alive for a short time
   in the soil, even those that form part of the soil seed bank.}},
