@article{ ISI:A1992HQ15500005,
Author = {DELRIO, JA and DEHARO, ML and VAZQUEZ, F},
Title = {{THE NONEQUILIBRIUM THERMODYNAMICS OF THE SOIL-WATER SYSTEM - A
   VARIATIONAL APPROACH}},
Journal = {{JOURNAL OF NON-EQUILIBRIUM THERMODYNAMICS}},
Year = {{1992}},
Volume = {{17}},
Number = {{1}},
Pages = {{67-76}},
Abstract = {{A variational principle for extended irreversible thermodynamics
   (Vazquez, F., del Rio, J.A., Rev. Mex. Fis., 35 (1990), 71) is
   reformulated to incorporate the spatial inhomogeneities of the
   non-conserved variables in their time evolution equations.  This
   approach is applied to water transport through soils and an exact time
   evolution equation for the water flux is derived as an Euler-Lagrange
   equation.  In the structure of this equation the Forchheimer correction
   of Darcy's Law (applied to non-stationary problems) can be identified.}},
