@article{ ISI:A1993KP93600048,
Author = {ROMEU, D},
Title = {{DECAGONAL PHASE MODEL WITH MULTIPLE PERIODS AND QUASI-PERIODIC
   COINCIDENCE LATTICE}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{1993}},
Volume = {{153}},
Pages = {{232-236}},
Month = {{FEB}},
Abstract = {{The decahedral-recursive model is applied to the decagonal phase.
   General results derived from energy minimizing considerations common to
   all decagonal systems are described. The structures obtained match
   scanning tunnelling observations and provide an explanation for the
   origin of the different periods in multiples of 0.4 nm, the formation of
   the quasiperiodic state and periodic approximants, and the Fibonacci
   lines observed in some alloys after heat treatment.}},
