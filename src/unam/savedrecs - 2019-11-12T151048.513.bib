@article{ ISI:A1993MK93400079,
Author = {GOMEZ, R and LOPEZ, T and HERRERA, L and CASTRO, AA and SCELZA, O and
   BARONETTI, G and LAZZARI, E and CUAN, A and CAMPOS, M and POULAIN, E and
   RAMIREZSOLIS, A and NOVARO, O},
Title = {{OXIDATIVE COUPLING OF METHANE OVER SOL-GEL MAGNESIUM-OXIDE CATALYSTS -
   EFFECT ON SELECTIVITY TO OLEFIN FORMATION}},
Journal = {{STUDIES IN SURFACE SCIENCE AND CATALYSIS}},
Year = {{1993}},
Volume = {{75}},
Number = {{C}},
Pages = {{2213-2216}},
Abstract = {{A comparative study on the partial oxidation of methane was made in
   Li-doped magnesium oxide catalysts prepared by the sol-gel method, (i.e.
   gelation of magnesium ethoxide) or by wet impregnation of commercial MgO
   support. Activities were comparable in both preparations. However,
   selectivities to ethylene are up to five times higher in sol-gel than in
   impregnated catalysts. Theoretical calculations on selected model
   clusters were made through semi-empirical and ab-initio methods.
   Semi-empirical results show a trend towards the formation of {[}Li+ O-]
   centers when the Li concentration increases. HF-SCF calculations using
   large gaussian basis sets on linear clusters indicate a loss of more
   than 1 electron on oxygen when a magnesium atom is replaced by lithium.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
