@article{ ISI:A1993KK56100008,
Author = {SOUSA, M and RUDD, VE},
Title = {{REVISION OF THE GENUS STYPHNOLOBIUM (LEGUMINOSAE, PAPILIONOIDEAE,
   SOPHOREAE)}},
Journal = {{ANNALS OF THE MISSOURI BOTANICAL GARDEN}},
Year = {{1993}},
Volume = {{80}},
Number = {{1}},
Pages = {{270-283}},
Abstract = {{The genus Styphnolobium is recognized as a segregate from Sophora on the
   basis of habit, stipels, bracteoles, legume, and chromosome number. It
   is enlarged from four to nine species through the description of five
   species new to science: Styphnolobium burseroides, S. monteviridis, S.
   parviflorum, S. protantherum, and S. sporadicum. Two sections are
   recognized, one requiring the new combination section Oresbios. The
   genus is centered in the Mexican-Mesoamerican area. Through similarities
   in habit, bracteoles, and dehiscence of the legume, it is clearly most
   closely related to Calia.}},
