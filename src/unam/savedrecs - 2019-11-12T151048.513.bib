@article{ ISI:A1993ML28000036,
Author = {LOPEZLOPEZ, D and WONGMORENO, A and MARTINEZ, L},
Title = {{CARBURIZATION PROCESSES INVOLVED IN BOILER-TUBE FAILURES}},
Journal = {{CORROSION SCIENCE}},
Year = {{1993}},
Volume = {{35}},
Number = {{5-8}},
Pages = {{1151-1158}},
Abstract = {{A carburization process was identified in many austenitic
   stainless-steel tubes with specification SA213.TP321H and SA213.TP304H,
   which were in service as elements of a high-temperature superheater or
   reheater in residual oil-fired utility boilers. Some of them were used
   for failure-analysis examination to determine the causes of high
   fireside-corrosion rates. The methodology employed in these analyses
   mainly involved microhardness profiles, electron probe microanalysis and
   metallographic studies of the external surfaces of the superheater and
   reheater tubes. Also, transmission electron microscopy (TEM) was used
   for studying the phases in one of the tubes, which was failed after
   service for about 37,500 h. It was concluded that the carburization
   process was involved in the tube-failure mechanism and resulted in the
   high corrosion rates shown by all the materials. Some of the most
   important features in each case and the role of operational and design
   factors involved in its development are also discussed.}},
