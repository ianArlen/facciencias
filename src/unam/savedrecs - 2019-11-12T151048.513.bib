@article{ ISI:A1993NY99100124,
Author = {CASTILLO, F and HERRERA, JJE and ESPINOSA, G},
Title = {{ENERGY STUDY OF ACCELERATED IONS FROM A DENSE-PLASMA FOCUS BY MEANS OF
   CR-39 TRACK DETECTOR}},
Journal = {{NUCLEAR TRACKS AND RADIATION MEASUREMENTS}},
Year = {{1993}},
Volume = {{22}},
Number = {{1-4}},
Pages = {{551-553}},
Abstract = {{A most interesting feature of Dense Plasma Focii is the acceleration of
   charged particles at energies in the range of MeV per nucleon. This work
   describes the studies of this phenomenon in a low energy device
   (approximately 6kJ) with a 3.0cm. diameter copper inner electrode, 10cm.
   long. The outer electrode is 10.0cm. in diameter. The device is operated
   with two 1.85muf capacitors in parallel at 54kV. Accelerated particles
   arc analyzed with filtered CR-39 plastic detectors for different filling
   pressures. A detailed study is made of track diameters when the plastic
   is chemically etched. As a result, accelerated protons in the range of
   11 to 16 McV arc observed.}},
