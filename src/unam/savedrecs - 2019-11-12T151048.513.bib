@article{ ISI:A1993LE94800004,
Author = {BEST, R and ISLAS, J and MARTINEZ, M},
Title = {{EXERGY EFFICIENCY OF AN AMMONIA WATER-ABSORPTION SYSTEM FOR ICE
   PRODUCTION}},
Journal = {{APPLIED ENERGY}},
Year = {{1993}},
Volume = {{45}},
Number = {{3}},
Pages = {{241-256}},
Abstract = {{An energy analysis, using a numerical simulation of a continuous
   ammonia-water absorption cycle for the production of ice, is presented.
   Its performance is studied by considering the effect on the coefficient
   of performance and the energy efficiency of varying the most important
   parameters. The main conclusions are that flat-plate collectors are
   acceptable as a heat source; the generator, absorber and economiser are
   the components with larger energy losses; and the economiser and
   absorber are the components with larger possibilities for increasing the
   global performance of the system.}},
