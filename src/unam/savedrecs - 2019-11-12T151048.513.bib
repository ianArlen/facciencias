@article{ ISI:A1993MP62200003,
Author = {PEREZMITRE, GG},
Title = {{DETECTING EATING DISORDERS IN UNIVERSITY-STUDENTS - OBESITY, BULIMIA AND
   ANOREXIA-NERVOSA}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1993}},
Volume = {{10}},
Number = {{1}},
Pages = {{17-27}},
Month = {{JAN-JUN}},
Abstract = {{The purpose of the present study was to detect tendencies or indices of
   sating disorders. A non probabilistic sample (N = 197) of male and
   female, college, undergraduate and graduate students was used.
   Descriptive statistics analyzed the frequency distributions and a
   principal component factor analysis established the initial structure
   and composition of the variables studied. Results indicate a negligible
   problem of extreme obesity (2\%), while there is a 41\% prevalence of
   overweight. The study revealed a series of eating behavior patterns and
   habits considered as risk factors related to bulimianorexia. The
   multifactorial structure showed two types of clusters: three factors
   clearly supported by existing related theory, and two others, consistent
   of newly studied variables. These factors are expected to contribute to
   a better understanding of complex eating behavior disorders.}},
