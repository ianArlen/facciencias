@article{ ISI:A1992JZ21300010,
Author = {GARCIAFERREIRA, S},
Title = {{ON FREE P-COMPACT GROUPS}},
Journal = {{ANNALS OF THE NEW YORK ACADEMY OF SCIENCES}},
Year = {{1992}},
Volume = {{659}},
Pages = {{68-77}},
Month = {{SEP 25}},
Abstract = {{A. R. Bernstein introduced the concept of p-compactness (p
   is-an-element-of omega{*}). Years later, W. W. Comfort defined a
   (pre)-order less-than-or-equal-to C on omega{*} by p
   less-than-or-equal-to C q iff every q-compact space is p-compact. It is
   natural to consider the relation less-than-or-equal-to GC on omega{*}
   given by p less-than-or-equal-to GC q iff every q-compact topological
   group is p-compact. It is shown that less-than-or-equal-to RK,
   less-than-or-equal-to C, and less-than-or-equal-to GC coincide on the
   set of weak P-points of omega{*} (less-than-or-equal-to RK denotes the
   Rudin-Keisler order on omega{*}), and that less-than-or-equal-to C and
   less-than-or-equal-to GC have several properties in common, but the
   question whether less-than-or-equal-to C = less-than-or-equal-to GC
   still remains unsolved. Some properties of free M-compact groups (M
   subset-or-equal-to alpha{*}) are also studied. In particular, it is
   shown that if alpha is a regular infinite cardinal and X is <
   alpha-bounded, then X is a closed subspace of the free < alpha-bounded
