@article{ ISI:A1991GX59100074,
Author = {MENDEZ, JM and MUHL, S and FARIAS, M and SOTO, G and COTAARAIZA, L},
Title = {{PREPARATION OF CUBIC BORON-NITRIDE FILMS BY PLASMA-ENHANCED CHEMICAL
   VAPOR-DEPOSITION OF BF3, N2 AND H2 GAS-MIXTURES}},
Journal = {{SURFACE \& COATINGS TECHNOLOGY}},
Year = {{1991}},
Volume = {{49}},
Number = {{1-3}},
Pages = {{422-426}},
Month = {{DEC 10}},
Note = {{18TH INTERNATIONAL CONF ON METALLURGICAL COATINGS AND THIN FILMS, SAN
   DIEGO, CA, APR 22-26, 1991}},
Organization = {{AMER VACUUM SOC, DIV VACUUM TECHNOL; AMER VACUUM SOC, DIV THIN FILMS}},
Abstract = {{Boron nitride thin films were prepared by plasma-enhanced chemical
   vapour deposition (PECVD) of BF3, H-2 and N2 gas mixtures.  Fourier
   transform IR (FTIR) spectroscopy and X-ray analysis reveal that the
   films deposited on crystalline substrates show signs of cubic and
   hexagonal phases present within the atomic network.  The index of
   refraction and the film thickness were determined by ellipsometry.  The
   deposition rate increases with temperature and saturates a high plasma
   powers.  Values of n are in the range 1.63-1.77, and the optical gap
   varies from 5.0 to 5.6 eV depending on the preparation conditions (the
   presence of contaminants also alters this property).  The ratio of N2 to
   H-2 in the flow affects the properties of the films.}},
