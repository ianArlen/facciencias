@article{ ISI:A1992JC20000001,
Author = {JUAREZISLAS, JA and PEREZ, R and SAVAGE, SJ},
Title = {{HREM STUDIES OF RAPIDLY SOLIDIFIED NI-FE-AL-B SHAPE MEMORY ALLOYS}},
Journal = {{MATERIALS LETTERS}},
Year = {{1992}},
Volume = {{14}},
Number = {{1}},
Pages = {{1-6}},
Month = {{JUN}},
Abstract = {{Two Ni-Fe-Al-B alloys. solidified into ribbons by melt spinning. were
   characterized by HREM, DSC, X-rav diffraction and recoverable strain
   measurements. The as-cast alloys exhibit excellent bend ductility ( in
   contrast to B2-type alloys prepared by conventional casting). X-ray
   diffraction shows the presence of beta-NiAl, beta'-NiAl, gamma-NiAl.
   gamma'-Ni3Al, Ni and other phases such as Fe3Al, FeAl, FeNi and Al5Fe2.
   It is the beta --> beta' diffusionless transformation which is
   responsible for the shape memory effect. Transmission electron
   microscopy studies show two different types of crystalline grains: one
   possesses a high density of twins which are the fingerprints of the
   martensite transformation. The other crystalline grains exhibit very
   poor image contrast due to the transformation beta' --> beta. There are
   also sections in the specimens with domains of both crystalline grains.
   HREM images from the twinned areas suggest different kinds of atomic
   structures for the parent and martensite crystalline sections. These
   results give some insights into the nature of the martensite
   transformation.}},
