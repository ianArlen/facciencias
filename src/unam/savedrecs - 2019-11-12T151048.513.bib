Author = {GALEANASANCHEZ, H},
Title = {{ON THE EXISTENCE OF KERNELS AND H-KERNELS IN DIRECTED-GRAPHS}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{1992}},
Volume = {{110}},
Number = {{1-3}},
Pages = {{251-255}},
Month = {{DEC 11}},
Abstract = {{A directed graph D with vertex set V is called cyclically h-partite (h
   greater-than-or-equal-to 2) provided one can partition V = V0 + V1 + ...
   + V(h-1) so that if (u, v) is an arc of D then u is-an-element-of V(i),
   and v is-an-element-of V(i+1) (notation mod h). In this communication we
   obtain a characterization of cyclically h-partite strongly connected
   digraphs. As a consequence we obtain a sufficient condition for a
   digraph to have a h-kernel.}},
