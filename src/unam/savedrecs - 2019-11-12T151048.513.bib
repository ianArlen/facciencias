@article{ ISI:A1992JV11300044,
Author = {GERMAN, G and LYNKER, M and MACIAS, A},
Title = {{STATIC QUARK-ANTIQUARK POTENTIAL FOR THE NAMBU-GOTO STRING OCCUPIED BY
   BOSE OR FERMI FIELDS}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{1992}},
Volume = {{46}},
Number = {{8}},
Pages = {{3640-3644}},
Month = {{OCT 15}},
Abstract = {{We study the static quark-antiquark potential for a Nambu-Goto string
   occupied by Bose or Fermi fields. The potential is similar to the
   Nambu-Goto potential in regard to shape and to the ever present
   square-root singularity. However the presence of Bose or Fermi fields,
   which live in the string, produce a shift in the critical distance
   between quarks r(c) (the value of r for which the potential vanishes).
   This would allow phenomenological values for r(c) to be reached without
   apparent difficulty.}},
