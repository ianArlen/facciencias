@article{ ISI:A1991GV32700012,
Author = {TAPIA, M and COSTERO, R and ECHEVARRIA, J and ROTH, M},
Title = {{NEAR-INFRARED AND STROMGREN PHOTOMETRY OF THE OPEN CLUSTERS NGC-663,
   NGC-1502 AND NGC-1893}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{1991}},
Volume = {{253}},
Number = {{4}},
Pages = {{649-661}},
Month = {{DEC 15}},
Abstract = {{Near-infrared JHK, and Stromgren uvby and H-beta photometry of the
   central parts of three open clusters, NGC 663, NGC 1502 and NGC 1893,
   are presented.  The following results were derived from the data:  NGC
   663 is at a distance of 2.5 kpc (m - M = 12.03 +/- 0.20) and is obscured
   by A(V) = 1.98 +/- 0.04, its age is 9 x 10(6) yr; NGC 1502 is at a
   distance of 1.5 kpc (m - M = 10.84 +/- 0.10) and is obscured by A(V) =
   1.83 +/- 0.03, its age is 5 x 10(6) yr; NGC 1893 is at a distance of 4.3
   kpc (m - M = 13.18 +/- 0.11) and is obscured by A(V) = 1.68 +/- 0.06,
   its age is 4 x 10(6) yr.  The total-to-selective extinction ratio at V,
   R(V) = A(V)/E(B - V), is slightly smaller towards NGC 663 and 1893 than
   the average galactic value, but towards NGC 1502, it is found to be 2.42
   +/- 0.09 (root mean squared), significantly lower than that
   characteristic of the general interstellar medium.  This result suggests
   that the size distribution of the dust particles in this line of sight
   is biased towards very small values.}},
