@article{ ISI:A1993KA21900009,
Author = {RUBLUO, A and CHAVEZ, V and MARTINEZ, AP and MARTINEZVAZQUEZ, O},
Title = {{STRATEGIES FOR THE RECOVERY OF ENDANGERED ORCHIDS AND CACTI THROUGH
   INVITRO CULTURE}},
Journal = {{BIOLOGICAL CONSERVATION}},
Year = {{1993}},
Volume = {{63}},
Number = {{2}},
Pages = {{163-169}},
Abstract = {{Orchids and cacti have many members vulnerable to extinction because of
   over-collection and loss of habitat. Efficient propagation techniques
   are urgently required to help stop disappearance of these endangered
   species. We propose in-vitro techniques as a strategy for the recovery
   of threatened species for both the Orchidaceae and Cactaceae. We
   cultured nine orchids and three cacti species in vitro, and after
   acclimatization some of these species were restored into their natural
   environment where they continue their life cycle. The near-extinct
   Mammillaria san-angelensis (Cactaceae) specimens, with just five
   individuals left in the wild, were successfully restored to the site.
   Individuals of this species derived in vitro flowered and produced
   fertile seeds after being restored.}},
