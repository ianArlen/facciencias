@article{ ISI:A1993KF88500044,
Author = {NAIR, MTS and ALVAREZGARCIA, G and ESTRADAGASCA, CA and NAIR, PK},
Title = {{CHEMICALLY DEPOSITED BI2S3-CUXS SOLAR CONTROL COATINGS}},
Journal = {{JOURNAL OF THE ELECTROCHEMICAL SOCIETY}},
Year = {{1993}},
Volume = {{140}},
Number = {{1}},
Pages = {{212-215}},
Month = {{JAN}},
Abstract = {{Solar control characteristics are described f or chemically deposited
   thin film coatings consisting of an initial layer of Bi2S3 film of
   approximately 0.05 mum thickness deposited at room temperature
   (25-degrees-C) for 45-75 min from baths containing bismuth nitrate,
   triethanolamine (TEA) and thioacetamide and a CuxS thin film of 0.1-0.25
   mum thickness deposited either at room temperature, for 3-6 h, or at
   50-degrees-C, for 1-2 h using baths consisting of copper(II) chloride,
   TEA, thioureau (TU), NaOH and NH3(aq). Air annealing of these films at
   150-degrees-C for 30 min yield well-desired solar control
   characteristics: sheet resistance of 30-60 OMEGA/square which signifies
   a low thermal emittance; integrated transmittance of 15-32\% in the
   visible region and 11-22\% in the near-infrared region; integrated
   transmittance of approximately 13-25\% for solar radiation and a range
   of shades in reflected daylight. Using a mathematical model, the solar
   control performance of these coatings in a 6 mm sheet glass is predicted
   for an architectural window in a tropical location showing that solar
   radiation rejection of approximately 50\% is obtainable for a T(vis){*}
   of 17\%.}},
