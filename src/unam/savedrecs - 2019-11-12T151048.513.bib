@article{ ISI:A1992KE69700048,
Author = {GERMAN, G and LYNKER, M},
Title = {{MASS DEPENDENCE OF THE DECONFINEMENT TRANSITION FOR A NAMBU-GOTO STRING
   OCCUPIED BY SCALAR FIELDS}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{1992}},
Volume = {{46}},
Number = {{12}},
Pages = {{5678-5681}},
Month = {{DEC 15}},
Abstract = {{The deconfinement temperature for a system of scalar fields living in a
   Nambu-Goto string is studied as a function of the scalar mass parameter.
   We find that there is excellent agreement with Monte Carlo simulations
   of an SU(3) gauge theory in four dimensions. The value of the scalar
   mass for which the correct deconfinement temperature is obtained
   corresponds remarkably well with the average of an effective gluon mass
   calculated independently.}},
