@article{ ISI:A1993MP62200001,
Author = {NIETO, J and LARIOS, RM and YOSEFF, JJ},
Title = {{ASSESSMENT OF CAUSAL RELATIONS BETWEEN STIMULI BY HUMANS - EFFECTS OF
   PROBABILITY AND TEMPORAL CONTIGUITY}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{1993}},
Volume = {{10}},
Number = {{1}},
Pages = {{1-8}},
Month = {{JAN-JUN}},
Abstract = {{The aim of this experiment was to assess the effects of parametric
   manipulations of probability and delay Of association between two
   stimuli on causality judgements made by undergraduate students. They
   were exposed to a task involving the presentation of a romboidal figure
   associated with the presentation of a second romboidal figure on a
   computer monitor, stimulus presentation was independent of the subject's
   behavior. The students had to assess, in a 0 to 100 scale, the
   probability of association between the two figures. In the experiment
   the probability of presentation of the second figure given the
   presentation of the antecedent figure was varied between groups, the
   probability values were 0.25, 0.50, 0.75 and 1.0. In addition, each
   group was exposed to three different delays (0, 4 and 8 sec) between
   presentation of the figures. The results showed that the different
   groups of students did not assess accurately the probability of
   association between events, the students underestimated the nominal
   probabilities when they were high, but overestimated the probability
   when the nominal value was 0.25. Finally, there was no significant
   effect of the delay on the presentation of the figures.}},
