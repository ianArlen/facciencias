@article{ ISI:A1993MQ32900003,
Author = {MANERO, O and RODRIGUEZ, RF and DEHARO, ML},
Title = {{ANISOTROPY AND THERMAL-STRESSES IN THE STEADY FLOW OF VISCOELASTIC
   FLUIDS}},
Journal = {{JOURNAL OF NON-EQUILIBRIUM THERMODYNAMICS}},
Year = {{1993}},
Volume = {{18}},
Number = {{4}},
Pages = {{336-352}},
Abstract = {{We examine the relevance of heat effects on the steady flow behavior of
   viscoelastic fluids. These fluids are described by a generalized Oldroyd
   8-constant model arising from an irreversible thermodynamic approach
   proposed previously by us which predicts a coupling between the heat
   flux, the stress tensor and the velocity gradient. We show that the
   coupling between the heat flux and the rate of deformation leads to an
   anisotropy in the thermal conductivity for an arbitrary flow, whereas
   its coupling with the stress tensor produces thermal stresses which
   arise from spatial inhomogeneities in the temperature field. We
   illustrate these effects for uniaxial elongation and simple shear flows.
   In a previous publication we have shown that for the former case all the
   phenomenological coefficients of our theory can be identified with
   quantities that are measurable. For the case of shear flows, in this
   paper we suggest the way in which all the unknown phenomenological
   coefficients can be determined and the predictions of the theory might
   be tested. We also discuss the features of the new constitutive
   relations incorporating thermal stresses.}},
