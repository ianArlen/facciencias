@article{ ISI:A1992JT71900023,
Author = {SANIGER, JM and HU, HL and CASTANO, VM},
Title = {{KINETIC-STUDIES OF THE DEHYDRATION PROCESS FOR POLYACRYLIC-ACID
   METAL-OXIDE COMPOUNDS}},
Journal = {{MATERIALS LETTERS}},
Year = {{1992}},
Volume = {{15}},
Number = {{1-2}},
Pages = {{113-117}},
Month = {{OCT}},
Abstract = {{A thermogravimetric study of polyacrylic acid-metal oxide compounds,
   PAA-MO, with M = Mg, Ca and Zn, was carried out. The amount of water
   fixed to each particular compound was obtained from the
   thermogravimetric curves. The relationships that best fit the
   dehydration experimental data show the existence of two mechanisms in
   the kinetics of the process: one which could be related to the breakup
   of the water-(PAA-MO) bond, and the other related to the out diffusion
   of the water molecules. Finally, the calculated activation energy of the
   dehydration process suggests that the water is hydrogen-bonded to the
   PAA-MO compounds.}},
