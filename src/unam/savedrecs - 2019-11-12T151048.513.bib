@article{ ISI:A1993LM46900002,
Author = {CABALLERO, SC and SUMANO, HS},
Title = {{CHARACTERIZATION OF STRESS IN BOVINES - A REVIEW}},
Journal = {{ARCHIVOS DE MEDICINA VETERINARIA}},
Year = {{1993}},
Volume = {{25}},
Number = {{1}},
Pages = {{15-30}},
Abstract = {{Stress is a common notion in veterinary medicine and it has steadly
   evolved since it first appeared in physiological research as a concept
   of animal discomfort. Although at first the idea of stress can be easily
   understood, it is clear that a detailed characterization requires
   further systematic analysis. Because recently stress has been viewed as
   a key factor in animal production and health, a review was carried out
   in an attempt to gather the various approaches to stress. This was done
   in order to narrow the gap between the busy veterinarian involved in
   bovine production and research in the physiological, physiopathological
   and ethological fields. An insight into the manner in which stress can
   be measured is herein included, with the overall aim of contributing to
   a better and more humane animal husbandry.}},
