@article{ ISI:A1993KD75600033,
Author = {BOHIGAS, J and PERSI, P and TAPIA, M},
Title = {{BIPOLAR STRUCTURE OF THE HERBIG-HARO OBJECT RNO-40}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{1993}},
Volume = {{267}},
Number = {{1}},
Pages = {{168-176}},
Month = {{JAN}},
Abstract = {{Images of the Herbig-Haro object RNO 40 and its vicinity are presented
   covering the wavelength range lambdalambda600-2400 nanometers. These
   include narrow band images in the lines Halpha, {[}NII], {[}SII] and
   {[}OI]. A bipolar morphology centred in RNO 40 is found. Both lobes are
   defined by a series of fainter HH knots to the SE and NW of the
   brightest central peak. An elongated bar-like emission is seen on the SE
   lobe. The infrared images show the central nebular peak and a series of
   knots not always coincident with those found in the optical images.
   Although no star was detected in the nearby 23'', the morphology of the
   complex argues in favour of the location of the exciting source close to
   the central emission peak and against the identification of the far
   infrared source IRAS05173-0555, located 2' away, as the source of
   excitation.}},
