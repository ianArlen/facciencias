@article{ ISI:A1991GW60400078,
Author = {ROMEROROCHIN, V and VAREA, C and ROBLEDO, A},
Title = {{MICROSCOPIC EXPRESSIONS FOR INTERFACIAL BENDING CONSTANTS AND
   SPONTANEOUS CURVATURE}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{1991}},
Volume = {{44}},
Number = {{12}},
Pages = {{8417-8420}},
Month = {{DEC 15}},
Abstract = {{We derive the interfacial-curvature free energy for a simple fluid from
   density-functional theory, and find a form matching that for a
   two-dimensional shearless elastic media.  The fourth moment of the
   direct correlation function and the density gradient determine the
   bending moduli kappa and kappaBAR, while the spontaneous curvature c0 is
   given by asymmetry in the density.  We obtain the critical indices of
   these quantities and corrections to the Laplace equation for curved
   interfaces.}},
