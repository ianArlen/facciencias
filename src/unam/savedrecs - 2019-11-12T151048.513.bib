@article{ ISI:A1993KM96100001,
Author = {RUELASMAYORGA, RA and TEAGUE, PF},
Title = {{DISTRIBUTION AND STUDIES OF THE INFRARED STELLAR POPULATION IN THE
   GALAXY .5. OTHER CLEAR REGIONS AROUND THE GALACTIC-CENTER}},
Journal = {{ASTRONOMY \& ASTROPHYSICS SUPPLEMENT SERIES}},
Year = {{1993}},
Volume = {{97}},
Number = {{3}},
Pages = {{587-601}},
Month = {{FEB}},
Abstract = {{We present K-counts and JHK detailed photometry for a number of stars
   with K less-than-or-equal-to +9.0 in three clear regions near the
   galactic centre. These regions are all located approximately at l
   approximately 0.0 and at latitudes b approximately -3.5, -4.0 and -4.5
   respectively. Their Cumulative Counts Functions (CCF's) are formed, and
   with the aid of an exponential disk model for the Galaxy they are
   separated into disk and bulge CCF. As for the case in Baade's Window
   (BW) (Ruelas-Mayorga \& Teague 1992a) the disk is dominant at bright
   magnitudes whereas the bulge dominates at the faint end of the CCF. The
   slope of the bulge CCF is steeper than that for the disk in all three
   cases corroborating the result obtained for BW. The two colour JHK
   diagram and the K vs J - K diagram for the three regions show that the
   stellar population in these areas is similar to that studied in BW by us
   and by Frogel \& Whitford (1987). An average value of E(J - K)
   approximately 0.42 mag for the reddening is obtained, which is somewhat
   larger than that found for BW (0.27). Finally it is shown that at K
   less-than-or-equal-to +9.0, the disk population may be accounted for by
   those sources with J - K less-than-or-equal-to +1.6 whereas the bulge
   population presents values for J - K in excess of +1.6.}},
