@article{ ISI:A1993NY99100026,
Author = {ESPINOSA, G and MARTENEZ, A and GOLZARRI, JI},
Title = {{SHRIMP SHELLS AS NUCLEAR TRACK DETECTOR}},
Journal = {{NUCLEAR TRACKS AND RADIATION MEASUREMENTS}},
Year = {{1993}},
Volume = {{22}},
Number = {{1-4}},
Pages = {{119-120}},
Abstract = {{In the search for new and better materials for Solid State Nuclear Track
   Detectors (SSNTD), some options are: 1) Synthesis of polymers with
   selective response to the type of particle, the energy of the ionizing
   radiation, high sensitivity, very low background, good mechanical
   strength, simple to etch, huge tracks, etc.; and 2) Investigation of
   naturally available materials determining their characteristics in order
   to use them in the appropriate applications. Shrimp shell, which has a
   high chitin a natural polymer contents, belong to the last option.
   In this work, we report the response of shrimp shells as SSNTD. We
   indicate the method of preparation, pre-treatment, etching conditions
   and measurements.}},
