@article{ ISI:A1991GZ32200002,
Author = {ORTIZ, A and FALCONY, C and FARIAS, M and COTAARAIZA, L and SOTO, G},
Title = {{SIO2 PREPARED BY REMOTE PLASMA-ENHANCED CHEMICAL VAPOR-DEPOSITION USING
   SICL4 AND O-2 AT SUBSTRATE TEMPERATURES OF LESS THAN 200-DEGREES-C}},
Journal = {{THIN SOLID FILMS}},
Year = {{1991}},
Volume = {{206}},
