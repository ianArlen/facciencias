@article{ ISI:A1992KD61100037,
Author = {SEBASTIAN, PJ},
Title = {{THE ELECTRICAL-PROPERTIES OF VACUUM-EVAPORATED STOICHIOMETRIC AND
   NONSTOICHIOMETRIC CDTE THIN-FILMS FOR OPTOELECTRONIC APPLICATIONS}},
Journal = {{THIN SOLID FILMS}},
Year = {{1992}},
Volume = {{221}},
Number = {{1-2}},
Pages = {{233-238}},
Month = {{DEC 10}},
Abstract = {{The influence of various film and growth parameters (such as the film
   thickness, substrate temperature and deposition rate) and the heat
   treatment in vacuum and under ambient conditions on the electrical
   conduction in vacuum-evaporated stoichiometric and non-stoichiometric
   CdTe thin films was investigated. Heat treatment of the films in vacuum
   reduced the sheet resistance by a few orders of magnitude. The films
   deposited at high deposition rates and low substrate temperatures
   exhibited an excess of tellurium and showed a p-type conductivity; those
   deposited at high substrate temperatures and low deposition rates
   contained excess cadmium and showed an n-type conductivity. Films
   deposited at about 375-400 K and 10-15 angstrom s-1 were stoichiometric.}},
