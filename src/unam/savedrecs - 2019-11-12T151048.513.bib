@article{ ISI:A1992JX47400015,
Author = {MARTINEZMEKLER, G and COCHO, G and GELOVER, A and BULAJICH, R},
Title = {{MODELING GENETIC EVOLUTION WITH COUPLED MAP LATTICES}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{1992}},
Volume = {{38}},
Number = {{1}},
Pages = {{127-141}},
Month = {{SEP}},
Note = {{INTERNATIONAL COLLOQUIUM ON TOPICS IN PHYSICS AND CHEMISTRY, IN HONOUR
   OF PROFESSOR DR A MONDRAGON BALLESTEROS ON THE OCCASION OF HIS 35 YEARS
   OF ACADEMIC LABOUR, UNIV NACL AUTONOMA MEXICO, MEXICO, AUG 15-16, 1991}},
Abstract = {{The evolution of genetic sequences, in particular the case of
   retro-virus as encountered in AIDS, is analyzed by means of a coupled
   Map lattice model. Mutations are introduced as coupling terms and
   several ecological constraints are considered. The resulting equations
   are of the reaction-diffusion type. The model establishes an evolution
   fitness criteria in terms of dynamical and structural properties at the
   molecular level. Under a global ecological constraint, the model
   dynamics generates quasi-species and presents an error threshold.
   Qualitative predictions regarding the AIDS vims RNA stability under
   mutations are corroborated experimentally.}},
