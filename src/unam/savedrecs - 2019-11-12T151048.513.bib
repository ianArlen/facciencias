@article{ ISI:A1993KP93600108,
Author = {ARAGON, JL and ROMEU, D and TORRES, M and FAYOS, J},
Title = {{RECURSIVE GROWTH OF THE ALCULI ICOSAHEDRAL AND CRYSTALLINE PHASES}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{1993}},
Volume = {{153}},
Pages = {{525-529}},
Month = {{FEB}},
Abstract = {{An interpenetrating cluster-based model for the icosahedral i-AlCuLi
   phase is proposed based on the concepts of decahedral growth and maximum
   coincidence sites. The resulting structure consists of big
   interpenetrated icosahedral clusters with cluster centers distributed in
   the direction of the twenty vectors pointing the vertices of a regular
   dodecahedron. The same mechanism can also yield the bcc R-AlCuLi phase,
   showing the close resemblance between both phases and clarifying the
   process of quasicrystal formation.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
