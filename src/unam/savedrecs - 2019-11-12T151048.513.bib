@article{ ISI:A1992KA73500022,
Author = {MENDEZ, JM and MUHL, S and CONTRERASPUENTE, G and AGUILARHERNANDEZ, J},
Title = {{OPTICAL-PROPERTIES OF AMORPHOUS-CARBON THIN-FILMS PREPARED BY PLASMA
   DEPOSITION IN A GRAPHITE HOLLOW-CATHODE}},
Journal = {{THIN SOLID FILMS}},
Year = {{1992}},
Volume = {{220}},
Number = {{1-2}},
Pages = {{125-131}},
Month = {{NOV 20}},
Abstract = {{Carbon films have been produced using a graphite hollow cathode and an
   r.f plasma of hydrogen or hydrogen and argon. No hydrocarbon gas was
   used. The films were subjected to heat treatments up to 700-degrees-C in
   air. The optical properties of the films were studied using Fourier
   tranform IR, UV-visible, Raman, ellipsometry and photoluminescence
   techniques before and after the annealing. Films prepared without argon
   have a narrower band gap and a different photoluminescence response from
   films prepared with argon. The index of refraction was seen to have
   values between 1.7 and 2.2 depending on the preparation conditions. The
   optical band gap of the films was generally greater than 2 eV and the
   annealing process both increased the gap and reduced the density of
   states responsible for the large photoluminescence signal.}},
