@article{ ISI:000251544800009,
Author = {Garcia-Zepeda, E. A. and Rojas-Lopez, A. and Esquivel-Velazquez, M. and
   Ostoa-Saloma, P.},
Title = {{Regulation of the inflammatory immune response by the cytokine/chemokine
   network in amoebiasis}},
Journal = {{PARASITE IMMUNOLOGY}},
Year = {{2007}},
Volume = {{29}},
Number = {{12}},
Pages = {{679-684}},
Month = {{DEC}},
Abstract = {{Amoebiasis is caused by the protozoa Entamoeba histolytica and persists
   as one of the leading parasitic diseases affecting millions worldwide.
   This parasite invades the intestinal mucosa, causing amoebic colitis and
   ulcers. It may also spread to other organs, mainly the liver, causing
   amoebic liver abscess (ALA). Current research efforts have focused on
   the development of specific diagnostic tests and animal models searching
   for a better understanding of the complex physiopathology of this
   disease. Analysis of the inflammatory immune response during intestinal
   amoebiasis in both human disease and animal murine models has revealed
   an important regulatory role for chemokines and cytokines. Recruitment
   and actin vation of inflammatory cells can also be modulated by specific
   protease-mediated cleavage of cytokines and by secreted amoebic factors
   such as amoebapores and monocyte locomotion inhibitory factor (MLIF).
   Unlike intestinal amoebiasis, analysis of the immune response in ALA has
   mainly been done in the hamster model. This has limited our information
   regarding the immune response during this phase of the disease. However,
   even with these limitations, several Th1/2 cytokines, such IL-6 and
   IL-4, and regulatory cytokines, like IL-10 and TGF beta, have been
   associated to the development of this disease.}},
