@article{ ISI:000251327400001,
Author = {Punzi, Raffaele and Schuller, Frederic P. and Wohlfarth, Mattias N. R.},
Title = {{Geometry for the accelerating universe}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2007}},
Volume = {{76}},
Number = {{10}},
Month = {{NOV}},
Abstract = {{The Lorentzian spacetime metric is refined to an area metric which
   naturally emerges as a generalized geometry in quantum string and gauge
   theory. Employing the area metric curvature scalar, the Einstein-Hilbert
   gravitational action is reinterpreted as dynamics for an area metric.
   The area metric cosmology of the radiation-dominated early universe does
   not depart from general relativity, enabling successful nucleosynthesis.
   But intriguingly, without the need for dark energy or fine-tuning, area
   metric cosmology explains the observed small acceleration of the late
   universe.}},
