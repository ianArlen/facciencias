@article{ ISI:000251351400010,
Author = {Perez-Fontes, S. E. and Perez-Tello, M. and Prieto-Lopez, L. O. and
   Brown, F. and Castillon-Barraza, F.},
Title = {{Thermoanalytical study on the oxidation of sulfide minerals at high
   temperatures}},
Journal = {{MINERALS \& METALLURGICAL PROCESSING}},
Year = {{2007}},
Volume = {{24}},
Number = {{4}},
Pages = {{275-283}},
Month = {{NOV}},
Abstract = {{An experimental study on the oxidation of chalcopyrite (CuFeS2), pyrite
   (FeS2), covellite (CuS) and chalcocite (Cu2S) particles was conducted by
   means of differential scanning calorimetry (DSC) and thermogravimetric
   analysis (TGA) techniques. The oxygen concentration in the process gas
   was set to 40\% and 70\% (by volume) and a heating rate of 40 degrees
   C/min was used. Response variables included the temperature of incipient
   reaction, the total exothermic heat of reaction, particle mass,
   morphology and mineralogy of the reacted particles. Based on the
   experimental data and phase stability calculations, reaction mechanisms
   were proposed to represent the behavior of the particles during
