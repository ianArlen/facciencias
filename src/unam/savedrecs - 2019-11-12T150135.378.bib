@article{ ISI:000249146400001,
Author = {Fragiel, A. and Serna, S. and Campillo, B. and Cota, L.},
Title = {{Dissimilar mechanical properties - micro structures microalloyed
   pipeline steels cracking performance under sour environment}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2007}},
Volume = {{467}},
Number = {{1-2}},
Pages = {{1-7}},
Month = {{OCT 15}},
Abstract = {{The cracking behavior of an API 5L X52 microalloyed pipeline steel at 25
   and 50 degrees C, under a sour environment, is compared with the
   cracking behavior of an API 5L X65 microalloyed pipeline steel with
   different microstructure and mechanical properties. The results show
   that the microstructure of X65 steel is much more resistant to develop
   and propagate a crack under proportional stress intensity factors and
   the same sour environmental conditions at room temperature (25 degrees
   C). The reverse result was obtained at 50 degrees C, however, a
   comparative growth in \% criteria shows that X52 steel does not present
   a much better resistance to propagate a crack than X65 steel. These
   behaviors at both temperatures are indicative that steels
   microstructures might play an important role to resist cracking. (c)
   2007 Elsevier B.V. All rights reserved.}},
