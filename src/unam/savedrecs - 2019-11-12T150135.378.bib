@article{ ISI:000251450600070,
Author = {Ciuchi, F. and Mazzulla, A. and Pane, A. and Reyes, J. Adrian},
Title = {{ac and dc electro-optical response of planar aligned liquid crystal
   cells}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2007}},
Volume = {{91}},
Number = {{23}},
Month = {{DEC 3}},
Abstract = {{Interaction of static electric fields with a nematic is complicated
   because of the presence of charges, which originates from aligning
   layers or from molecular breakdown, affecting the electro-optic response
   of a liquid crystal cell. We analyze the aligning layers effect on the
   electro-optical response and develop a model based on a description of
   the ion balance on the conducting plates taking into account both drift
   current and saturation effects due to the ion migration inside the cell,
   neglecting diffusion current. The model well agrees with data for low
   voltages, while for higher ones, diffusion current should be taken into
   account. (c) 2007 American Institute of Physics.}},
