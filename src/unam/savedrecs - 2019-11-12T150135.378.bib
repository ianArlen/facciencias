@article{ ISI:000250585900029,
Author = {Huerfano, R. S. and Lopez, M. A. and Socolovsky, M.},
Title = {{Geometry of the Aharonov-Bohm effect}},
Journal = {{INTERNATIONAL JOURNAL OF THEORETICAL PHYSICS}},
Year = {{2007}},
Volume = {{46}},
Number = {{11}},
Pages = {{2961-2966}},
Month = {{NOV}},
Abstract = {{We show that the connection responsible for any Abelian or non-Abelian
   Aharonov-Bohm effect with n parallel ``magnetic{''} flux lines in R-3,
   lies in a trivial G-principal bundle P -> M, i.e. P is isomorphic to the
   product M x G, where G is any path connected topological group; in
   particular a connected Lie group. We also show that two other bundles
   are involved: the universal covering space M -> M, where path integrals
   are computed, and the associated bundle P x (G) C-m -> M, where the wave
   function and its covariant derivative are sections.}},
