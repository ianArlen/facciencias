@article{ ISI:000251003600006,
Author = {Grether, M. and de Llano, M. and Baker, Jr., George A.},
Title = {{Bose-Einstein condensation in the relativistic ideal Bose gas}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2007}},
Volume = {{99}},
Number = {{20}},
Month = {{NOV 16}},
Abstract = {{The Bose-Einstein condensation (BEC) critical temperature in a
   relativistic ideal Bose gas of identical bosons, with and without the
   antibosons expected to be pair-produced abundantly at sufficiently hot
   temperatures, is exactly calculated for all boson number densities, all
   boson point rest masses, and all temperatures. The Helmholtz free energy
   at the critical BEC temperature is lower with antibosons, thus implying
   that omitting antibosons always leads to the computation of a metastable
   state.}},
