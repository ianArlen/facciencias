@article{ ISI:000260636200013,
Author = {Ruiz-Trejo, Enrique and Santoyo-Salazar, Jaime and Vilchis-Morales,
   Ruben and Benitez-Rico, Adriana and Gomez-Garcia, Francisco and
   Flores-Morales, Carlos and Chavez-Carvayar, Jose and Tavizon, Gustavo},
Title = {{Microstructure and electrical transport in nano-grain sized
   Ce0.9Gd0.1O2-delta ceramics}},
Journal = {{JOURNAL OF SOLID STATE CHEMISTRY}},
Year = {{2007}},
Volume = {{180}},
Number = {{11}},
Pages = {{3093-3100}},
Month = {{NOV}},
Abstract = {{An enhancement of the electrical conductivity has been found in
   nano-grain sized Ce0.9Gd0.1O2-delta ceramics when measured in N-2 (pO(2)
   = 3.5 x 10(-6) atm) in comparison with the most commonly accepted values
   of bulk ionic conductivity. We first present the synthesis and
   characterisation of the nanoparticles later used for the preparation of
   dense nanoceramics of Gd-doped CeO2. The nanoparticles were
   characterised by X-ray diffraction (XRD), atomic force microscopy (AFM)
   and transmission electron microscopy (TEM). The good sintering
   properties of these nanopowders allowed us to obtain very dense ceramics
   (490\% theoretical density) while keeping the grain size close to 100
   nm. The microstructure of these nanoceramics was analysed by AFM and
   scanning electron microscopy (SEM) while the electrical characterisation
   was performed by the 4-point dc technique between 500 and 950 degrees C
   in air or N-2 and ac impedance between 150 and 400 degrees C in air and
   or argon. We briefly discuss the possibilities of electron vs. oxygen
   ion conduction and grain boundary vs. bulk conductivity. The features
   exhibited by these ceramics represent an increased potential to process
   solid electroceramics materials with specific levels of electronic
   and/or ionic conductivities for a variety of electrochemical devices.
   (C) 2007 Elsevier Inc. All rights reserved.}},
