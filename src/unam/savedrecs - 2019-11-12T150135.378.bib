@article{ ISI:000250145700003,
Author = {Brudzinski, M. and Cabral-Cano, E. and Correa-Mora, F. and DeMets, C.
   and Marquez-Azua, B. M.},
Title = {{Slow slip transients along the Oaxaca subduction segment from 1993 to
   2007}},
Journal = {{GEOPHYSICAL JOURNAL INTERNATIONAL}},
