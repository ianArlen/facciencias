@article{ ISI:000250351400041,
Author = {Juarez-Calderon, J. M. and Negron-Mendoza, A. and Ramos-Bernal, S.},
Title = {{Irradiation of ferrous ammonium sulfate for its use as high absorbed
   dose and low-temperature dosimeter}},
Journal = {{RADIATION PHYSICS AND CHEMISTRY}},
Year = {{2007}},
Volume = {{76}},
Number = {{11-12}},
Pages = {{1829-1832}},
Month = {{NOV-DEC}},
Note = {{14th International Meeting on Radiation Processing (IMRP-2006), Kuala
   Lumpur, MALAYSIA, FEB 26-MAR 03, 2006}},
Organization = {{Sterilgamma; Malaysian Inst Nucl Technol Res}},
Abstract = {{In the present paper, we study the response of crystalline ammonium
   ferrous sulfate as a function of the irradiation dose and temperature.
   The dose studied ranged from 33.5 to 546 kGy. The temperature regimen
   varied from 77 K (liquid nitrogen) to 311 K. The analysis of the samples
   was made by UV spectroscopy and EPR.
   The results show that the change in absorbance of the dosimeter was
   linear with respect to the absorbed dose in the range studied. There is
   a small influence of the irradiation temperature in the response of the
   iron salt. The dose rate and storage time after irradiation was of no
   importance in this application. (C) 2007 Elsevier Ltd. All rights
   reserved.}},
