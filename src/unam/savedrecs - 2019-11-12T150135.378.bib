@article{ ISI:000250495000004,
Author = {Chuluunbaatar, O. and Gusev, A. A. and Abrashkevich, A. G. and
   Amaya-Tapia, A. and Kaschiev, M. S. and Larsen, S. Y. and Vinitsky, S.
   I.},
Title = {{KANTBP: A program for computing energy levels, reaction matrix and
   radial wave functions in the coupled-channel hyperspherical adiabatic
   approach}},
Journal = {{COMPUTER PHYSICS COMMUNICATIONS}},
Year = {{2007}},
Volume = {{177}},
Number = {{8}},
Pages = {{649-675}},
Month = {{OCT 15}},
Abstract = {{A FORTRAN 77 program is presented which calculates energy values,
   reaction matrix and corresponding radial wave functions in a
   coupled-channel approximation of the hyperspherical adiabatic approach.
   In this approach, a multi-dimensional Schrodinger equation is reduced to
   a system of the coupled second-order ordinary differential equations on
   the finite interval with homogeneous boundary conditions of the third
   type. The resulting system of radial equations which contains the
   potential matrix elements and first-derivative coupling terms is solved
   using high-order accuracy approximations of the finite-element method.
   As a test desk, the program is applied to the calculation of the energy
   values and reaction matrix for an exactly solvable 2D-model of three
   identical particles on a line with pair zero-range potentials.}},
