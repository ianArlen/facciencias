@article{ ISI:000250775200040,
Author = {Argiro, S. and Barroso, S. L. C. and Gonzalez, J. and Nellen, L. and
   Paul, T. and Porter, T. A. and Prado, Jr., L. and Roth, M. and Ulrich,
   R. and Veberic, D.},
Title = {{The offline software framework of the Pierre auger observatory}},
