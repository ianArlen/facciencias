   Morales-Morales, David},
Title = {{Reactivity of fluorinated thioether ligands of the type
   {[}C6H4Br-2-(CH2SRF)] towards transition metal complexes of the group 10}},
Journal = {{INORGANICA CHIMICA ACTA}},
Year = {{2007}},
Volume = {{360}},
