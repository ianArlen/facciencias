@article{ ISI:000251865200016,
Author = {Lockhart, Richard A. and O'Reilly, Federico J. and Stephens, Michael A.},
Title = {{Use of the gibbs sampler to obtain conditional tests, with applications}},
Journal = {{BIOMETRIKA}},
Year = {{2007}},
Volume = {{94}},
Number = {{4}},
Pages = {{992-998}},
Month = {{DEC}},
Abstract = {{A random sample is drawn from a distribution which admits a minimal
   sufficient statistic for the parameters. The Gibbs sampler is proposed
   to generate samples, called conditionally sufficient or co-sufficient
   samples, from the conditional distribution of the sample given its value
   of the sufficient statistic. The procedure is illustrated for the gamma
   distribution. Co-sufficient samples may be used to give exact tests of
   fit; for the gamma distribution these are compared for size and power
   with approximate tests based on the parametric bootstrap.}},
