@article{ ISI:000251196000011,
Author = {Lamas, Monica and Lee-Rivera, Irene and Ramirez, Monica and
   Lopez-Colome, Ana Maria},
Title = {{D-Serine regulates CREB phosphorylation induced by NMDA receptor
   activation in Muller glia trom the retina}},
Journal = {{NEUROSCIENCE LETTERS}},
Year = {{2007}},
Volume = {{427}},
Number = {{1}},
Pages = {{55-60}},
Month = {{OCT 29}},
Abstract = {{D-Serine is an N-methyl-D-aspartate (NMDA) receptor coagonist
   predominantly produced by glial cells in the brain and the retina.
   Whereas a role for D-serine as a modulator of NMDA receptors in neurons
   has been suggested, its function in glial cells has not been analyzed.
   We here show that D-serine modulates gene expression in Muller glial
   cells from the retina through the induction of transcription factor CREB
   phosphorylation and the expression of the immediate-early gene c-fos.
   Pharmacological analysis indicates that D-serine effect involves NNIDA
   receptor activation. Comparison of the effect Of D-serine in Muller
   cells, hippocampal astrocytes and hippocampal neurons suggests that
   D-serine could function as a retinal NNIDA receptor coagonist activating
   functionally relevant transcription factor pathways in glial cells. (c)
   2007 Elsevier Ireland Ltd. All rights reserved.}},
