Author = {Escobar-Alarcon, L. and Arrieta, A. and Camps, E. and Muhl, S. and
   Rodil, S. and Vigueras-Santiago, E.},
Title = {{An alternative procedure for the determination of the optical band gap
   and thickness of amorphous carbon nitride thin films}},
Journal = {{APPLIED SURFACE SCIENCE}},
Year = {{2007}},
Volume = {{254}},
Number = {{1, SI}},
Pages = {{412-415}},
