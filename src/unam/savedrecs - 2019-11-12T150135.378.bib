@article{ ISI:000209358000003,
Author = {Chavez, E. and Huerta, A. and Ortiz, M. E. and Rodriguez, P. and Favela,
   F. and Marin, D. and Moreno, E. and Murillo, G. and Policroniades, R.
   and Varela, A. and Barron-Palos, L.},
Title = {{Neutron elastic scattering on lead at 3.0 MeV}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2007}},
Volume = {{53}},
Number = {{6, S}},
Pages = {{13-16}},
Month = {{DEC}},
Abstract = {{Recent interest on precise information of the elastic scattering of fast
   (MeV) neutrons on Pb-208 revealed the lack of sufficient experimental
   information. In this work we present new data obtained at the EN-Tandem
   Accelerator Laboratory of the ``Instituto Nacional de Investigaciones
   Nucleares{''} (ININ). A tagged mono-energetic flux of neutrons is
   generated through the D(d,n)He-3 reaction by detecting and identifying
   the conjugated Helium particles. The neutron ``cone{''} produced this
   way is directed towards a Pb-nat target. The angular distribution of the
   scattered neutrons is measured between five and twenty degrees relative
   to the neutron direction by an array of plastic scintillators.
   Comparison with the previous data and optical model calculations is
   presented. Future perspectives are discussed.}},
