@article{ ISI:000251581500009,
Author = {Mata Mendoza, Angeles and Macotela Flores, Silvia},
Title = {{Effectiveness of an educational support program on the academic
   trajectory of undergraduate college students}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2007}},
Volume = {{24}},
Number = {{2}},
Pages = {{243-257}},
Month = {{DEC}},
Abstract = {{The present study evaluated an instructional program designed to improve
   the achievement of undergraduate students at a public university (UNAM).
   The program included a 1 semester preparation period which covered 3
   curricular courses and 3 meta-curricular courses. The experimental group
   was composed of 4 subgroups of students that participated in the program
   (n=132) and a non-equivalent control group (n=61). The groups were
   similar in gender, age, family income, and scholastic average, but
   different in the origin of their school. Instructors in the study were
   trained to use specially developed teaching materials. Students in the
   experimental group showed a main effect in better grades and reduced
   probability of dropping out of school during the first 3 semesters.
   Results demonstrate the importance of courses related to career
   development and courses designed to train learning strategies.}},
