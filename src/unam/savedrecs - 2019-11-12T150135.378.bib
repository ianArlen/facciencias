@article{ ISI:000250097600026,
Author = {Martinez-Aguilar, Juan Fco. and Ibarra-Montano, Emma L.},
Title = {{Complete quality analysis of commercial surface-active products by
   Fourier-transform near infrared spectroscopy}},
Journal = {{TALANTA}},
Year = {{2007}},
Volume = {{73}},
Number = {{4}},
Pages = {{783-790}},
Month = {{OCT 15}},
Abstract = {{Using proper calibration data Fourier-transform near infrared
   spectroscopy is used for developing multivariate calibrations for
   different analytical determinations routinely used in the surfactants
   industry. Four products were studied: oleyl-cetyl alcohol
   polyethoxylated, cocamidopropyl betaine (CAPB), sodium lauryl sulfate
   (SLS) and nonylphenol polyethoxylated (NPEO). Calibrations for major as
   well as very low concentrated compounds were achieved and every model
   was validated through linearity, bias, accuracy and precision tests,
   showing good results and the viability of NIR spectroscopy as a full
   quality control method for this products. Duplicate and complete
   analysis on a single sample takes at most 3 min, requiring neither
   sample preparation nor the use of reagents. The analytical reference
   procedures involved in this work represent the typical ones used in the
   industry and the NIR method shows good results in the analysis of
   components with weight concentrations less than 1\%. (c) 2007 Elsevier
   B.V. All rights reserved.}},
