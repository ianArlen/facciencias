@article{ ISI:000250983700075,
Author = {Rivera-Munoz, Eric M.},
Title = {{Relationship between structural features of MoS2 nanotubes and
   crystalline structure of precursor}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2007}},
Volume = {{102}},
Number = {{9}},
Month = {{NOV 1}},
Abstract = {{MoS2 nanotubes have been synthesized by thermal decomposition of
   ammonium thiomolybdate (ATM) precursor and by using the aluminum oxide
   template method. Hollow tubes of very specific diameter and wall
   thickness were obtained presenting the so-called ``bamboo{''} texture.
   These nanotubes are closed in sections; each one 29 nm wide and a length
   of 63 nm in average. Evidence of a strong relationship between these
   structural features of MoS2 nanotubes and crystalline structure of
   precursor is exposed, suggesting that both size and shape of tube
   sections are determined by crystalline structure of ATM precursor as
   well as by the diameter of template pores. (c) 2007 American Institute
   of Physics.}},
