@article{ ISI:000252031300006,
Author = {Salinas-Vazquez, M. and Vicente-Rodriguez, W. and Choi-Orea, E. and
   Garcia, V. Leyva},
Title = {{Turbulence simulation of flow that passes around a square cylinder from
   the utilization of large-scale simulation and immersed boundaries}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2007}},
Volume = {{53}},
Number = {{6}},
Pages = {{461-469}},
Month = {{DEC}},
Abstract = {{A Large Eddy Simulation study of flow around a square cylinder at low
   Reynolds number is presented. The Large Eddy Simulation is based on
   structure function subgrid models. To assess this method's relevance to
   industrial and engineering flows, immersed boundary conditions are
   implemented to render the solid surface, i.e. the square rod. We also
   introduce an artificial acoustic stiffness reduction for fully
   compressible flows which allows a much higher time step to be
   implemented. Very high quality results are obtained without the usually
   high cost of process time and memory requirements associated with LES.
   The proposed method opens a new approach to high precision modeling for
   heat transfer, acoustics and mechanical vibrations problems of real
   flows with complex geometries.}},
