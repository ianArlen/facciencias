@article{ ISI:000250638800002,
Author = {Ake, C. Sanchez and Sobral, H. and Villagran-Muniz, M.},
Title = {{Plasma characterization of cross-beam pulsed-laser ablation used for
   carbon thin film deposition}},
Journal = {{THIN SOLID FILMS}},
Year = {{2007}},
Volume = {{516}},
Number = {{1}},
Pages = {{8-12}},
Month = {{NOV 1}},
Abstract = {{The dynamics of the interaction between two delayed plasmas induced by
   cross-beam pulsed-laser ablation was analyzed by fast photography using
   narrow interference filters. In this configuration, two perpendicular
   rotating carbon targets were ablated by two synchronized laser beams
   generating two interacting plasma plumes. A Nd: yttrium-aluminum-garnet
   (1064 rim) laser beam is focused onto a target generating a highly
   directed plume; subsequently an excimer laser (248 nm) produces a second
   perpendicular plasma, which expands through the plume region generated
   by the first laser. In the cross-beam configuration, collision processes
   cause a reduction in the C II ion kinetic energy from similar to 110 to
   35 eV; moreover, the species of the second plasma which travel on the
   normal direction to the target surface (toward the substrate) are mainly
   C II. Interaction between plasmas has been compared with laser-induced
