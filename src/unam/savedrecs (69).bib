Title = {{Nuclear modification factor for heavy flavors: An energy loss effect or
   more baryons than mesons?}},
Booktitle = {{XIII MEXICAN WORKSHOP ON PARTICLES AND FIELDS}},
Series = {{Journal of Physics Conference Series}},
