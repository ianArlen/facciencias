@article{ ISI:000302037500038,
Author = {Yepez-Martinez, H. and Fraser, P. R. and Hess, P. O.},
Title = {{APPLICATIONS OF A SEMI-MICROSCOPIC CLUSTER MODEL FOR ASTROPHYSICAL
   PROCESSES}},
Journal = {{ROMANIAN JOURNAL OF PHYSICS}},
Year = {{2012}},
Volume = {{57}},
Number = {{1-2}},
Pages = {{513-542}},
Abstract = {{The Semimicroscopic Algebraic Cluster Model (SACM) is applied to the
   two-cluster systems C-14+alpha and Ne-20+alpha, which are of
   astrophysical interest. The parameters of this algebraic model are
   adjusted to experimental data, being eigenenergies and B(E2) values.
   After that, sole B(M1) values and alpha-spectroscopic factors are
   calculated. The last represent a prediction of this model. Also the
   geometrical mapping of the SACM is shortly reviewed. The geometric
   potential, as a function of the coherent state parameter alpha is
   constructed. The minimum of the potential shows the position of the
   systems in the phase space diagram.}},
