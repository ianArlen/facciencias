@article{ ISI:000300123900022,
Author = {Quintanar, Liliana and Montiel, Teresa and Marquez, Maripaz and
   Gonzalez, Alejandra and Massieu, Lourdes},
Title = {{Calpain activation is involved in acute manganese neurotoxicity in the
   rat striatum in vivo}},
Journal = {{EXPERIMENTAL NEUROLOGY}},
