Title = {{Climate interaction mechanism between solar activity and terrestrial
   biota}},
Booktitle = {{COMPARATIVE MAGNETIC MINIMA: CHARACTERIZING QUIET TIMES IN THE SUN AND
   STARS}},
Series = {{IAU Symposium Proceedings Series}},
Year = {{2012}},
Number = {{286}},
