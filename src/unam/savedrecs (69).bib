@article{ ISI:000305760300004,
Author = {Cabrera-Trujillo, R. and Dominguez-Gutierrez, F. J.},
Title = {{Numerical study of laser-assisted transmission and reflection
   coefficients of a wave packet by an attractive impurity}},
Journal = {{RADIATION EFFECTS AND DEFECTS IN SOLIDS}},
Year = {{2012}},
Volume = {{167}},
Number = {{7}},
Pages = {{464-477}},
Note = {{REM-4 Conference, Padova, ITALY, SEP 18-22, 2011}},
Abstract = {{We study the transmission and reflection probabilities for a wave packet
   that collides with an attractive impurity (e. g. a quantum dot, a
   perturbation on a waveguide, etc.) in the presence of an intense,
   ultra-fast Gaussian laser pulse with intensity in the range I =
   10(12)-10(14) W/cm(2), wavelength of 780 nm and FWHM of 10 fs. The study
   is based on a numerical solution to the time-dependent Schrodinger
   equation by means of a finite difference approach. The attractive
   impurity is modeled by different 1-D potential shape (square, harmonic,
   triangular, circular, and double harmonic) wells. We find a strong
   effect of the laser intensity on the reflection and transmission
   coefficients as a function of energy of the incoming wave-packet, with
   the strongest effect in the low collisions energy range and high laser
   intensity. Furthermore, we find that contrary to the laser-free case
   where there is no trapping probability by the impurity, in the presence
   of the laser pulse appears a trapping probability for low collision
   energies. The probability of trapping increases as the laser intensity
   increases. We present different scenarios where these results can be of
   interest.}},
