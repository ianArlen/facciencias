Title = {{Texture analysis based on the Hermite transform for image classification
   and segmentation}},
Booktitle = {{OPTICS, PHOTONICS, AND DIGITAL TECHNOLOGIES FOR MULTIMEDIA APPLICATIONS
   II}},
Series = {{Proceedings of SPIE}},
