@article{ ISI:000301141400030,
Author = {Pacheco-Alvarez, Diana and Vazquez, Norma and Castaneda-Bueno, Maria and
   de-los-Heros, Paola and Cortes-Gonzalez, Cesar and Moreno, Erika and
   Meade, Patricia and Bobadilla, Norma A. and Gamba, Gerardo},
Title = {{WNK3-SPAK Interaction is Required for the Modulation of NCC and other
   Members of the SLC12 Family}},
Journal = {{CELLULAR PHYSIOLOGY AND BIOCHEMISTRY}},
Year = {{2012}},
Volume = {{29}},
Number = {{1-2}},
Pages = {{291-302}},
Abstract = {{The serine/threonine with no lysine kinase 3 (WNK3) modulates the
   activity of the electroneutral cation-coupled chloride cotransporters
   (CCC) to promote Cl- influx and prevent Cl- efflux, thus fitting the
   profile for a putative ``Cl--sensing kinase{''}. The Ste20-type kinases,
   SPAK/OSR1, become phosphorylated in response to reduction in
   intracellular chloride concentration and regulate the activity of NKCC1.
   Several studies have now shown that WNKs function upstream of SPAK/OSR1.
   This study was designed to analyze the role of WNK3-SPAK interaction in
   the regulation of CCCs with particular emphasis on NCC. In this study we
   used the functional expression system of Xenopus laevis oocytes to show
   that different SPAK binding sites in WNK3 ((241, 872, 1336)RFxV) are
   required for the kinase to have effects on CCCs. WNK3-F1337A no longer
   activated NKCC2, but the effects on NCC, NKCC1, and KCC4 were preserved.
   In contrast, the effects of WNK3 on these cotransporters were prevented
   in WNK3-F242A. The elimination of F873 had no consequence on WNK3
   effects. WNK3 promoted NCC phosphorylation at threonine 58, even in the
   absence of the unique SPAK binding site of NCC, but this effect was
   abolished in the mutant WNK3-F242A. Thus, our data support the
   hypothesis that the effects of WNK3 upon NCC and other CCCs require the
   interaction and activation of the SPAK kinase. The effect is dependent
   on one of the three binding sites for SPAK that are present in WNK3, but
   not on the SPAK binding sites on the CCCs, which suggests that WNK3 is
   capable of binding both SPAK and CCCs to promote their phosphorylation.
   Copyright (C) 2012 S. Karger AG, Basel}},
