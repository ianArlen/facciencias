@article{ ISI:000304453200006,
Author = {Dominguez M, R. and Arganis J, M. L.},
Title = {{Validation of methods to estimate design discharge flow rates for dam
   spillways with large regulating capacity}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2012}},
Volume = {{57}},
Number = {{3}},
Pages = {{460-478}},
Abstract = {{The estimation and review of discharge flow rates in hydraulic works is
   a fundamental problem in water management. In the case of dams with
   large regulating capacity, in order to estimate return periods of
   discharge flow rates from the spillways, it becomes necessary to
   consider both peak flow and volume of the incoming floods. In this
   paper, the results of the validation for several methods of assessing
   design floods for spillways of dams with a large flood control capacity
   are presented; the validation is performed by comparing the maximum
   outflows (or the maximum levels reached in the reservoir) obtained from
   the routing of the design floods with those obtained from the routing of
   the historical annual maximum floods. The basin of Malpaso Dam, Mexico,
   is used as the case study.}},
