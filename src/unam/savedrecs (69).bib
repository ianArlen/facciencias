Editor = {{Jimenez, JB and Cembranos, JAR and Dobado, A and Maroto, AL and Dombriz, AD}},
Title = {{MOND as the weak-field limit of an extended metric theory of gravity}},
Booktitle = {{TOWARDS NEW PARADIGMS: PROCEEDING OF THE SPANISH RELATIVITY MEETING 2011}},
Series = {{AIP Conference Proceedings}},
Year = {{2012}},
Volume = {{1458}},
Pages = {{483-486}},
