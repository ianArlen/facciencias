@inproceedings{ ISI:000307540500014,
Author = {Gonzalez Canales, F. and Mondragon, A.},
Editor = {{Barranco, J and Contreras, G and Delepine, D and Napsuciale, M}},
Title = {{The flavour symmetry S-3 and the neutrino mass matrix with two texture
   zeroes}},
Booktitle = {{XIII MEXICAN WORKSHOP ON PARTICLES AND FIELDS}},
Series = {{Journal of Physics Conference Series}},
Year = {{2012}},
Volume = {{378}},
Note = {{13 Mexican Workshop on Particles and Fields (MWPF), Univ Guanajuato,
   Dept Cultural Studies, Leon, MEXICO, OCT 20-26, 2011}},
Organization = {{Mexican Phys Soc, Div Particles \& Fields; Consejo Nacl Ciencia
   Tecnologia (CONACYT); Consejo Ciencia Tecnologico Estado Guanajuato
   (CONCyTEG); Univ Guanajuato; Univ Nacional Autonoma Mexico; Ctr
   Investigaciones Estudios Avanzados IPN (CINVESTAV); Univ Michoacana San
   Nicolas Hidalgo; Benemerita Univ Autonoma Puebla; Univ Autonoma San Luis
   Potosy}},
Abstract = {{In this work, discuss neutrino masses and mixings in the framework of a
   minimal S-3 symmetric extension of the Standard Model. In this model,
   the mass matrices of all fermions take the same generic form with two
   texture zeroes. The mass matrices of the neutrinos and charged leptons
   are re-parameterized in terms of their eigenvalues, then the neutrino
   mixing matrix, V-PMNS, is computed and exact, explicit analytical
   expressions for the neutrino mixing angles as functions of the masses of
   neutrinos and charged leptons are obtained in excellent agreement with
   the latest experimental data. We also compute the branching ratios of
   some selected flavour-changing neutral current (FCNC) processes, as well
   as the contribution of the exchange of neutral flavour-changing scalars
   to the anomaly of the magnetic moment of the muon, as functions of the
   masses of charged leptons and the neutral Higgs bosons. We find that the
   S-3 x Z(2) flavour symmetry and the strong mass hierarchy of the charged
   leptons strongly suppress the FCNC processes in the leptonic sector,
   well below the present experimental bounds by many orders of magnitude.
   The contribution of FCNC's to the anomaly of the muon's magnetic moment
   is small, but not negligible.}},
