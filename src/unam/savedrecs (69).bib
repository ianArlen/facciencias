@inproceedings{ ISI:000305508300012,
Author = {Nava, Rodrigo and Cristobal, Gabriel and Escalante-Ramirez, Boris},
Editor = {{Schelkens, P and Ebrahimi, T and Cristobal, G and Truchetet, F and Saarikko, P}},
Title = {{A comprehensive study of texture analysis based on local binary patterns}},
Booktitle = {{OPTICS, PHOTONICS, AND DIGITAL TECHNOLOGIES FOR MULTIMEDIA APPLICATIONS
   II}},
Series = {{Proceedings of SPIE}},
Year = {{2012}},
Volume = {{8436}},
