@inproceedings{ ISI:000307540500012,
Author = {Magana, Juan and Matos, Tonatiuh},
Editor = {{Barranco, J and Contreras, G and Delepine, D and Napsuciale, M}},
Title = {{A brief Review of the Scalar Field Dark Matter model}},
Booktitle = {{XIII MEXICAN WORKSHOP ON PARTICLES AND FIELDS}},
