@article{ ISI:000298125900003,
Author = {Velazquez-Hernandez, R. and Melnikov, A. and Mandelis, A. and
   Sivagurunathan, K. and Rodriguez-Garcia, M. E. and Garcia, J.},
Title = {{Non-destructive measurements of large case depths in hardened steels
   using the thermal-wave radar}},
Journal = {{NDT \& E INTERNATIONAL}},
Year = {{2012}},
Volume = {{45}},
Number = {{1}},
Pages = {{16-21}},
Month = {{JAN}},
Abstract = {{The thermal-wave radar method was used to resolve and measure deep
   hardness case depths (2-3 mm) of AISI9310 and Pyrowear53 steels used in
   the aerospace industry. Several gears made of these steels were
   evaluated and correlations between radiometric signal and case depths
   were obtained using the cross-correlation peak delay time. The foregoing
   case depth range is significantly deeper than achievable with
   point-by-point laser frequency scans (800 pm) and swept-sine modulation
   of the laser beam (1.16 mm). (C) 2011 Elsevier Ltd. All rights reserved.}},
