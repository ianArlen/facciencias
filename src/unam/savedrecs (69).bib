@inproceedings{ ISI:000307540500001,
Author = {Barranco, J. and Miranda, O. G. and Moura, C. A. and Rashba, T. I. and
   Rossi-Torres, F.},
Editor = {{Barranco, J and Contreras, G and Delepine, D and Napsuciale, M}},
Title = {{Ultra-high energy extragalactic neutrinos interacting with ultra-light
   dark matter}},
Booktitle = {{XIII MEXICAN WORKSHOP ON PARTICLES AND FIELDS}},
Series = {{Journal of Physics Conference Series}},
Year = {{2012}},
Volume = {{378}},
Note = {{13 Mexican Workshop on Particles and Fields (MWPF), Univ Guanajuato,
   Dept Cultural Studies, Leon, MEXICO, OCT 20-26, 2011}},
Organization = {{Mexican Phys Soc, Div Particles \& Fields; Consejo Nacl Ciencia
   Tecnologia (CONACYT); Consejo Ciencia Tecnologico Estado Guanajuato
   (CONCyTEG); Univ Guanajuato; Univ Nacional Autonoma Mexico; Ctr
   Investigaciones Estudios Avanzados IPN (CINVESTAV); Univ Michoacana San
   Nicolas Hidalgo; Benemerita Univ Autonoma Puebla; Univ Autonoma San Luis
   Potosy}},
Abstract = {{We show the results and discussions of the study of a possible
   suppression of the extragalactic neutrino flux during its propagation
   due to a nonstandard interaction with a candidate field to dark matter.
   In particular, we show the study of neutrino interaction with an
   ultra-light scalar field. It is shown that the extragalactic neutrino
   flux may be suppressed by such an interaction, leading to a mechanism to
   reduce the ultra-high energy neutrino flux. We calculate both the cases
   of non-self-conjugate as well as self-conjugate ultra-light dark matter.
   In the first case, the suppression is independent of the neutrino and
   dark matter masses. We conclude that care must be taken when explaining
   limits on the neutrino flux through source acceleration mechanisms only,
   since there could be other mechanisms, as absorption during propagation,
   for the reduction of the neutrino flux {[}1].}},
