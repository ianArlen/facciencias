Booktitle = {{WDM SYSTEMS AND NETWORKS: MODELING, SIMULATION, DESIGN AND ENGINEERING}},
Series = {{Optical Networks}},
Year = {{2012}},
Pages = {{63-97}},
Abstract = {{This chapter presents an overview of the most recent modeling and
   simulation techniques for the analysis and engineering of all the major
