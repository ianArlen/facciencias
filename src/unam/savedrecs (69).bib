@article{ ISI:000297873300009,
Author = {McCall, Michael K. and Dunn, Christine E.},
Title = {{Geo-information tools for participatory spatial planning: Fulfilling the
   criteria for `good' governance?}},
Journal = {{GEOFORUM}},
