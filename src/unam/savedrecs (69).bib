@inproceedings{ ISI:000300400100132,
Author = {Lopez, S. E. and Ayala, A. G. and Hernandez, R.},
Editor = {{Mazzolani, F and Herrera, R}},
Title = {{Multilevel displacement-robustness based seismic design for ductile
   steel framed structures}},
Booktitle = {{STESSA 2012: PROCEEDINGS OF THE 7TH INTERNATIONAL CONFERENCE ON
   BEHAVIOUR OF STEEL STRUCTURES IN SEISMIC AREAS}},
Year = {{2012}},
Pages = {{943-949}},
Note = {{7th International Conference on Behaviour of Steel Structures in Seismic
   Areas (STESSA), Santiago, CHILE, JAN 09-11, 2012}},
Organization = {{Univ Chile, Dept Civil Engn; GERDAU AZA; ILAFA; ECCS CECM EKS; Chilean
   Assoc Seismol \& Earthquake Engn (ACHISINA)}},
Abstract = {{This paper presents an alternative multilevel performance based seismic
   design method that, through an effective control of damage and
   displacements, guarantees in a more rational and efficient manner, the
   parameters defining the required performance levels. An advance of the
   robustness based seismic assessment approach under development by this
   research group is discussed. The goal of the approach is to incorporate
   robustness concepts in a performance based formulation to define an
   integral performance-robustness based seismic assessment-design method.
   The design method proposed is based on the concept of a simplified
   reference system, which behavior is evaluated considering the properties
   of the fundamental mode. This formulation, based on concepts of
   structural dynamics, leads to a simple and straightforward design
   procedure. To illustrate the potential of the method proposed, the
   performance based design of an eight-storey steel frame and its
   validation through non linear dynamic analysis are shown.}},
