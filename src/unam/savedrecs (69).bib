@inproceedings{ ISI:000301281900018,
Author = {Castro, J. J. and Soto, J. R. and Molina, B.},
Editor = {{CondeGallardo, A and AyonBeato, E and GodinaNava, JJ and HernandezContreras, M and VelascoSevilla, L}},
Title = {{Jahn-Teller Analysis of the Electronic Properties of the Endohedral
   Clusters M@Al-12 (M= B, Al, Ga) and their Anions}},
Booktitle = {{ADVANCED SUMMER SCHOOL IN PHYSICS 2011 (EAV 2011)}},
Series = {{AIP Conference Proceedings}},
