@inproceedings{ ISI:000310687200004,
Author = {Bijker, Roelof and Ferretti, Jacopo and Santopinto, Elena},
Editor = {{Bijker, R}},
Title = {{The strange beauty of the proton}},
Booktitle = {{BEAUTY IN PHYSICS: THEORY AND EXPERIMENT: IN HONOR OF FRANCESCO LACHELLO
   ON THE OCCASION OF HIS 70TH BIRTHDAY}},
Series = {{AIP Conference Proceedings}},
Year = {{2012}},
Volume = {{1488}},
Pages = {{35-43}},
Note = {{Meeting on Beauty in Physics - Theory and Experiment in Honor of
   Francesco Lachello on the Occasion of his 70th Birthday, MEXICO, MAY
   14-18, 2012}},
Organization = {{UNAM, Consejo Tecnico Investigac Cientifica; UNAM, Inst Ciencias Nucl;
   SMF, Div Fisica Nucl}},
Abstract = {{The contribution of strange quarks to the proton is addressed in two
   different models of the nucleon, a phenomenological two-component model
   in which the nucleon is described in terms of an intrinsic three-quark
   structure surrounded by a meson cloud, and the unquenched quark model in
   which the effects of the sea quarks are taken into account through a
   P-3(0) quark-antiquark pair creation mechanism. The results for the
   strange magnetic moment and the strangeness radius of the proton are
   found to be small, in agreement with the latest experimental results
   from parity-violating electron scattering and recent lattice
   calculations.}},
