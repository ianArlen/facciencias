@article{ ISI:000301331400009,
Author = {Salas-Torres, O. and Zuniga-Segundo, A. and Magana-Solis, L. F.},
Title = {{Non-stationary holographic currents in BSO under an external sinusoidal
   electric field a}},
Journal = {{OPTIK}},
Year = {{2012}},
Volume = {{123}},
Number = {{5}},
Pages = {{414-416}},
Abstract = {{We calculated the non-stationary holographic currents in an external
   sinusoidal electric field. We first solved numerically the set of
   nonlinear material rate differential equations to find the full overall
   space charged field and the mobile carrier density for a photorefractive
   BSO crystal. With these results we calculated the photocurrents and we
   obtained an additional term in the photocurrent when an ac field is
   applied. We have a good agreement between our theoretical and
   experimental data previously reported. (C) 2011 Elsevier GmbH. All
   rights reserved.}},
