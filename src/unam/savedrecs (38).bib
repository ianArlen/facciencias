@article{ ISI:000448565500006,
Author = {Cruz, Moritz},
Title = {{THE NEED FOR OFFICIAL RESERVES IN LATIN AMERICA: ASSESSING THE
   PRECAUTIONARY MOTIVE, 1995-2011}},
Journal = {{CUADERNOS DE ECONOMIA}},
Year = {{2015}},
Volume = {{34}},
Number = {{65}},
Pages = {{327-347}},
Month = {{JUL-DEC}},
Abstract = {{In this paper we estimate the demand for official reserves in Latin
   America during the period 1995-2011. We assume that the main concern of
   the monetary authorities to demand reserves is the fear of suffering
   external drains, and its associated output costs. In other words, we
   attempt to show that the so-called precautionary motive drives the
   demand for international reserves in the region. Our econometric results
   confirm that Latin American countries demand ever increasing amounts of
   foreign exchange to protect themselves against the likelihood of
   external drains.}},
