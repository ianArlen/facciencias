Author = {Romero-Rodriguez, Alba and Ruiz-Villafan, Beatriz and Rocha-Mendoza,
   Diana and Manzo-Ruiz, Monserrat and Sanchez, Sergio},
Title = {{Biochemistry and regulatory functions of bacterial glucose kinases}},
