@article{ ISI:000355343600011,
Author = {Zamora-Zamora, R. and Adame-Arana, O. and Romero-Rochin, V.},
Title = {{Macroscopic Excitations in Confined Bose-Einstein Condensates, Searching
   for Quantum Turbulence}},
