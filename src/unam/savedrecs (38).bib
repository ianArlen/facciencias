@article{ ISI:000357759400027,
Author = {Bednarz, Pawel and Wloch, Iwona and Hernandez-Cruz, Cesar},
Title = {{On the existence and the number of (2-d)-kernels in graphs}},
Journal = {{ARS COMBINATORIA}},
Year = {{2015}},
Volume = {{121}},
Pages = {{341-351}},
Month = {{JUL}},
Abstract = {{In this paper we study (2-d)-kernels in graphs. We shall show that the
   problem of the existence of (2-d)-kernels is NP-complete for a general
   graph. We also give some results related to the problem of counting of
   (2-d)-kernels in graphs. For special graphs we show that the number of
   (2-d)-kernels is equal to the Fibonacci numbers.}},
