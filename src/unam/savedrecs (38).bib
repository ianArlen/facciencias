@article{ ISI:000359968200017,
Author = {Oliva-Leyva, M. and Naumis, Gerardo G.},
Title = {{Tunable dichroism and optical absorption of graphene by strain
   engineering}},
Journal = {{2D MATERIALS}},
Year = {{2015}},
Volume = {{2}},
Number = {{2}},
Month = {{JUN}},
Abstract = {{We theoretically study the transmittance for normal incidence of
   linearly polarized light between two media separated by a strained
   graphene monolayer. We analytically characterize the degree of dichroism
   and the transparency of graphene as a function of an arbitrary uniform
   strain and the incident polarization. We discuss how measurements of
   dichroism and transparency for two different polarization directions can
   be used to determine the magnitude and direction of strain. Our findings
   result in very useful tools to tune the graphene absorption by
   mechanical strain, as well as to design nano-devices to determine crack
   propagation in materials.}},
