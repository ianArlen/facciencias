@article{ ISI:000357541800002,
Author = {Hacyan, Shahen},
Title = {{Decoherence and the paradox of the observed observer in quantum
   mechanics}},
Journal = {{PHYSICA SCRIPTA}},
Year = {{2015}},
Volume = {{90}},
Number = {{7, SI}},
Month = {{JUL}},
Abstract = {{If an atomic system is being observed, and its observer is also
   observed, would the first observer be in a superposition of states and
   evolve deterministically before being observed? In this paper, a simple
   model of non-demolition measurement is analyzed in order to elucidate
   the so-called `Wigner's friend' paradox. The model illustrates the
   decoherence of an atomic system and its observer (the `friend') as the
   latter is being observed (by Wigner).}},
