
@article{ ISI:000353786100008,
Author = {Villarreal, Aline and Ramirez, Jorge and Cedeno Caero, Luis and Castillo
   Villalon, Perla and Gutierrez-Alejandre, Aida},
Title = {{Importance of the sulfidation step in the preparation of highly active
   NiMo/SiO2/Al2O3 hydrodesulfurization catalysts}},
Journal = {{CATALYSIS TODAY}},
Year = {{2015}},
Volume = {{250}},
