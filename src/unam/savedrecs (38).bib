@article{ ISI:000370036300025,
Author = {Michel, Norma L. and Flores, Dora L. and Hirata, Gustavo A.},
Title = {{Magnetic-luminescent spherical particles synthesized by ultrasonic spray
   pyrolysis}},
Journal = {{MATERIALS RESEARCH EXPRESS}},
Year = {{2015}},
Volume = {{2}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{The combination of magnetic and luminescent properties in a single
   particle system, opens-up a wide range of potential applications in
   biotechnology and biomedicine. In this work, we performed the synthesis
   of magnetic-luminescent Gd2O3:Eu3+@Fe2O3 particles by ultrasonic spray
   pyrolysis performed in a tubular furnace. In order to achieve the
   composite formation, commercial superparamagnetic Fe3O4 nanoparticles
   were coated with a luminescent Eu3+-doped Gd2O3 shell in a low-cost
   one-step process. The spray pyrolysis method yields deagglomerated
   spherical shape magneto/luminescent particles. The photoluminescence
   spectra underUVexcitation (lambda(Exc) = 265 nm) of the magnetic
   Gd2O3:Eu3+@Fe2O3 compound showed the characteristic red emission of Eu3+
   (lambda(Em) = 612 nm). This magneto/luminescent system will find
   applications in biomedicine and biotechnology.}},
