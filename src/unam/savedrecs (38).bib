@article{ ISI:000359181900010,
Author = {Garcia-Gonzalez, Victor and Delgado-Coello, Blanca and Perez-Torres,
   Armando and Mas-Oliva, Jaime},
Title = {{Reality of a Vaccine in the Prevention and Treatment of Atherosclerosis}},
Journal = {{ARCHIVES OF MEDICAL RESEARCH}},
Year = {{2015}},
Volume = {{46}},
