@article{ ISI:000359022400004,
Author = {Fragoso-Martinez, Itzi and Martinez-Gordillo, Martha and De Luna, Efrain},
Title = {{Salvia semiscaposa (Lamiaceae) a new species from Nanchititla, Mexico}},
