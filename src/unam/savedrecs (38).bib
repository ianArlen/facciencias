@article{ ISI:000357992200002,
Author = {Madhavaraju, Jayagopal and Scott, Robert W. and Lee, Yong Il and Bincy,
   Kunjukrishnan Sathy and Gonzalez-Leon, Carlos M. and Ramasamy,
   Sooriamuthu},
Title = {{Facies, biostratigraphy, diagenesis, and depositional environments of
   Lower Cretaceous strata, Sierra San Jose section, Sonora (Mexico)}},
Journal = {{CARNETS DE GEOLOGIE}},
Year = {{2015}},
Volume = {{15}},
Number = {{9-11}},
Pages = {{103-122}},
Month = {{JUL 14}},
Abstract = {{We used petrofacies analysis, carbon, oxygen and strontium isotope data
   to interpret the isotopic variations in the carbonate rocks of the Mural
   Formation of Sonora (Sierra San Jose section), Mexico. The petrographic
   study reveals a range of lithofacies from wackestone to packstone. The
   analyzed limestones show significant negative delta O-18 values (-18.6
   to -10.9 VPDB) and delta C-13 values ranging from negative to positive
   (-2.6 to + 2.5\% VPDB). The absence of correlation between delta C-13
   and delta O-18 values suggests a primary marine origin for the delta
   C-13 values of limestones from the Sierra San Jose section. The
   limestones have large variations in Sr-87/Sr-86 values (0.707479 to
   0.708790). Higher Sr-87/Sr-86 ratios in various levels of the studied
   section suggest that most of the sediments were derived from the
   Proterozoic basement of the Caborca block during Early Cretaceous time.
   A decrease in Sr-87/Sr-86 ratios at certain levels indicates an influx
   of lesser amounts of radiogenic Sr that could have been caused by
   contribution of sediments from the Triassic and Jurassic volcanic rocks.}},
