@article{ ISI:000356674300010,
Author = {Guadalupe del Rio, M. and Morrone, Juan J. and Lanteri, Analia A.},
Title = {{Evolutionary biogeography of South American weevils of the tribe
   Naupactini (Coleoptera: Curculionidae)}},
Journal = {{JOURNAL OF BIOGEOGRAPHY}},
