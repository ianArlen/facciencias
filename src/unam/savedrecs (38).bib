@article{ ISI:000361356600010,
Author = {Cordova Osnaya, Martha and Rosales Perez, Jose Carlos and Escobedo
   Ramirez, Alejandra},
Title = {{Research Description of Suicidal Ideation among Mexican High School
   Students}},
Journal = {{ACTA PSIQUIATRICA Y PSICOLOGICA DE AMERICA LATINA}},
Year = {{2015}},
Volume = {{61}},
Number = {{2}},
Pages = {{149-159}},
Month = {{JUN}},
Abstract = {{The aim of this study is to describe the research that has been
   conducted on Mexican high school students who were administered an
   instrument measuring suicidal ideation. published research in journals
   between the years of 1990-2013 were reviewed. the results were as
   follows: 23 articles were identified; 56.29\% did not define the
   theoretical construct of suicidal ideation used in the study; the
   43.71\% that defined the construct of suicidal ideation used seven
   different definitions; six different types of instruments were used to
   measure suicidal ideation; the percentages of suicidal ideation in the
   total sample ranged from 5.7\% to 56\%; the forms to record the presence
   of suicidal ideation considering the same instrument were different and
   the variables associated with the presence of suicidal ideation,
   diverse. it is concluded that research on suicidal ideation in high
   school students lacks a consensus on a methodology restricted to a
   theoretical viewpoint, covering a definition of this theoretical
   construct, the measuring instrument, the cutoff for the classification
   of presence - absence of suicidal ideation, and the theoretical location
   of the associated variables.}},
