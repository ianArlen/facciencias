@article{ ISI:000219924300006,
Author = {Poncela, Lorena and Vieyra, Antonio and Mendez-Lemus, Yadira},
Title = {{Intra-municipal participatory processes as steps towards local
   governance in peri-urban territories. Experience in the municipality of
   Tarimbaro, Michoacan, Mexico}},
Journal = {{JOURNAL OF LATIN AMERICAN GEOGRAPHY}},
Year = {{2015}},
Volume = {{14}},
Number = {{2}},
Pages = {{129-157}},
Month = {{JUL}},
Abstract = {{Urban sprawl on the periphery of cities generates new dynamic and
   heterogeneous periurban spaces. The municipality of Tarimbaro,
   Michoacon, has experienced rapid urbanization resulting from the
   expansion of Morelia, the neighboring midium-size city. This has
   provoked a significant reduction of agricultural systems and
   environmental and social impacts. Due to this, management capacities of
   local government have been exceeded, thus promoting institutional
   capacity building is a priority. This study proposes to do so through
   participatory intra-municipal processes. For this, participatory mapping
   was undertaken with local government actors to locate changes caused by
   urbanization. Priority areas in the territory and social capital among
   participants were identified. The method contributes positively to the
   process of local governance by supporting the capacity building of a
   government with these characteristics.}},
