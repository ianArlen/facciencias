@article{ ISI:000370618600008,
Author = {Gonzalez Martin, Nuria},
Title = {{INTERNATIONAL PARENTAL CHILD ABDUCTION AND MEDIATION: TWO CASES TO
   REFLECT UPON: MEXICO (DIRECT AMPARO UNDER REVISION 903/2014) AND THE
   UNITED STATES OF AMERICA (LOZANO V. MONTOYA ALVAREZ)}},
Journal = {{REVISTA ELECTRONICA DE ESTUDIOS INTERNACIONALES}},
Year = {{2015}},
Number = {{29}},
Month = {{JUN}},
Abstract = {{Nowadays there is an increase of international families, that is, those
   formed by individuals under different countries' jurisdictions.
   International families are not exempt from suffering a crisis, hence the
   increase in cross border family conflicts. This way, in cases where
   there are minors and couple's relationship is terminated, we face the
   regrettable increase of international abductions of minors by one of the
   parents. This paper analyzes the 1980 Hague Convention on Civil Aspects
   of International Child Abduction; provides background related to the
   particular situation between Mexico and the United States of America and
   analyzes two recent cases brought to both Supreme Courts. Finally, we
   outline the need to cooperate between different authorities to
   accomplish and expedite solution in the best interest of the child,
   where international family mediation plays a key role in international
   parental child abduction.}},
