@article{ ISI:000357899900003,
Author = {Febres, Gerardo and Jaffe, Klaus and Gershenson, Carlos},
Title = {{Complexity measurement of natural and artificial languages}},
Journal = {{COMPLEXITY}},
Year = {{2015}},
Volume = {{20}},
Number = {{6}},
Pages = {{25-48}},
Month = {{JUL-AUG}},
Abstract = {{We compared entropy for texts written in natural languages (English,
   Spanish) and artificial languages (computer software) based on a simple
   expression for the entropy as a function of message length and specific
   word diversity. Code text written in artificial languages showed higher
   entropy than text of similar length expressed in natural languages.
   Spanish texts exhibit more symbolic diversity than English ones. Results
   showed that algorithms based on complexity measures differentiate
   artificial from natural languages, and that text analysis based on
   complexity measures allows the unveiling of important aspects of their
   nature. We propose specific expressions to examine entropy related
   aspects of tests and estimate the values of entropy, emergence,
   self-organization, and complexity based on specific diversity and
   message length. (c) 2014 Wiley Periodicals, Inc. Complexity 20: 25-48,
   2015}},
