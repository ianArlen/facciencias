@article{ ISI:000358310700013,
Author = {Antonyan, Sergey and Dobrowolski, Tadeusz},
Title = {{Locally contractible coset spaces}},
Journal = {{FORUM MATHEMATICUM}},
Year = {{2015}},
Volume = {{27}},
Number = {{4}},
Pages = {{2157-2175}},
Month = {{JUL}},
Abstract = {{We prove that for any closed subgroup H of a locally compact Hausdorff
   group G the following properties are mutually equivalent: (1) the coset
   space G/H is locally contractible, (2) G/H is finite-dimensional and
   locally connected, (3) G/H is a manifold. Assume that G is a locally
   compact group with compact space of connected components. If the natural
   action of G on a locally contractible coset space G/H is effective, then
   we prove that G is a Lie group.}},
