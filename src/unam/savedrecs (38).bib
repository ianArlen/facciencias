@article{ ISI:000356355000008,
Author = {Sebastian, Miguel Angel},
Title = {{What panpsychists should reject: on the incompatibility of panpsychism
   and organizational invariantism}},
Journal = {{PHILOSOPHICAL STUDIES}},
Year = {{2015}},
Volume = {{172}},
Number = {{7, SI}},
Pages = {{1833-1846}},
Month = {{JUL}},
Abstract = {{Some philosophers, like David Chalmers, have either shown their sympathy
   for, or explicitly endorsed, the following two principles:
   Panpsychism-roughly the thesis that the mind is ubiquitous throughout
   the universe-and Organizational Invariantism-the principle that holds
   that two systems with the same (sufficiently) fine-grained functional
   organization will have qualitatively identical experiences. The purpose
   of this paper is to show the tension between the arguments that back up
   both principles. This tension should lead, or so I will argue, defenders
   of one of the principles to give up on the other.}},
