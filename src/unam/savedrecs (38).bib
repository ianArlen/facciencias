@article{ ISI:000358483200006,
Author = {Guzman-Vargas, Ariel and de la Rosa-Pineda, Jesus E. and
   Oliver-Tolentino, Miguel A. and Lima, Enrique and Ramirez-Rosales,
   Daniel and Flores-Moreno, Jorge L. and Martinez-Ortiz, Maria de J.},
Title = {{Stability of Cu species and zeolite structure on ecological
   heterogeneous Fenton discoloration-degradation of yellow 5 dye:
   Efficiency on reusable Cu-Y catalysts}},
Journal = {{ENVIRONMENTAL PROGRESS \& SUSTAINABLE ENERGY}},
Year = {{2015}},
Volume = {{34}},
Number = {{4}},
Pages = {{990-998}},
Month = {{JUL-AUG}},
Abstract = {{The discoloration-degradation of yellow 5 dye was achieved through the
   heterogeneous Fenton reaction using Cu-Y catalysts. Cu-Y materials were
   prepared by an aqueous ion exchange method from the NH4-Y parent
   zeolite. The solids are characterized by X-ray diffraction, nitrogen
   physisorption, EPR and, Al-27 and Si-29 NMR spectroscopies.
   Discoloration around 100\% with 60\% of COD removal was achieved before
   90 min at 50 degrees C using the next reaction conditions: 50 mL of
   colored solution containing 50 ppm of dye, 0.129 mmol of H2O2, catalyst
   mass/volume of solution ratio (g L-1)= 0.016 and pH=4-5. The influence
   of temperature and H2O2 concentration was also studied. The
   discoloration kinetics were modeled using the pseudo-first order
   reaction. High catalytic activity was observed even after four reaction
   cycles, which is credited to the good stability of the Cu species and
   the zeolite structure. Because the Fenton reaction was favored by the
   acid media provided by the Cu-Y catalyst and temperature, the addition
   of any acid was not necessary, avoiding the generation of sludge,
   resulting in an eco-friendly process. (c) 2015 American Institute of
   Chemical Engineers Environ Prog, 34: 990-998, 2015}},
