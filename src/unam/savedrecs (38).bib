@article{ ISI:000355842300010,
Author = {Vela-Amieva, M. and Abreu-Gonzalez, M. and Gonzalez-del Angel, A. and
   Ibarra-Gonzalez, I. and Fernandez-Lainez, C. and Barrientos-Rios, R. and
   Monroy-Santoyo, S. and Guillen-Lopez, S. and Alcantara-Ortigoza, M. A.},
Title = {{Phenylalanine hydroxylase deficiency in Mexico: genotype-phenotype
   correlations, BH4 responsiveness and evidence of a founder effect}},
Journal = {{CLINICAL GENETICS}},
Year = {{2015}},
Volume = {{88}},
Number = {{1}},
Pages = {{62-67}},
Month = {{JUL}},
Abstract = {{The mutational spectrum of the phenylalanine hydroxylase gene (PAH) in
   Mexico is unknown, although it has been suggested that PKU variants
   could have a differential geographical distribution. Genotype-phenotype
   correlations and genotype-based predictions of responsiveness to
   tetrahydrobiopterin (BH4) have never been performed. We sequenced the
   PAH gene and determined the geographic origin of each allele,
   mini-haplotype associated, genotype-phenotype correlations and
   genotype-based prediction of BH4 responsiveness in 48 Mexican patients.
