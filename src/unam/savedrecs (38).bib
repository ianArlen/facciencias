@article{ ISI:000395259800009,
Author = {Ramirez Martinell, Alberto and Aguilar Trejo, Jose Luis},
Title = {{El Espia 2.0: Computational tool for exploring the psycholinguistic
   processes of text production}},
Journal = {{REVISTA DE INVESTIGACION EDUCATIVA DE LA ESCUELA DE GRADUADOS EN
   EDUCACION}},
Year = {{2015}},
Volume = {{6}},
Number = {{11}},
Pages = {{55-60}},
Month = {{JUL-DEC}},
Abstract = {{El Espia 2.0 is a software tool that allows the researcher with
   interests in the field of psycholinguistics to deal with the text
   produced by their subjects of study as a process rather than as a
   finished product. This tool is able to record the process of
   construction of texts at a level of addition, insertion and deletion of
   characters. By using this tool, the researcher can see the characters
   -that form words and sentences the writer typed and added to the text
   she is composing; the pauses - short or long- she took during the
