@article{ ISI:000255190200009,
Author = {Ramirez-Diaz, Martha I. and Diaz-Perez, Cesaar and Vargas, Erendira and
   Riveros-Rosas, Hector and Campos-Garcia, Jesus and Cervantes, Carlos},
Title = {{Mechanisms of bacterial resistance to chromium compounds}},
