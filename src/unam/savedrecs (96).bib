@article{ ISI:000255953700040,
Author = {de Leon, Aned and Jalbout, Abraham F. and Basiuk, Vladimir A.},
Title = {{SWNT-amino acid interactions: A theoretical study}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{2008}},
Volume = {{457}},
Number = {{1-3}},
Pages = {{185-190}},
Month = {{MAY 20}},
Abstract = {{In the present work we discuss the interactions between an armchair (5,
   5) single walled carbon nanotube (SWNT) and amino acids. After multiple
   configurations and test calculations we have deduced that the highest
   dissociation energies are obtained when metallic SWNT systems interact
   with biological molecules. The DFT-BLYP/DND calculations reveal that the
   most favorable interactions of the SWNT system is with arginine,
   cysteine, alanine and asparagine which is related to the backbone
   structure of the corresponding amino acids. (C) 2008 Elsevier B.V. All
   rights reserved.}},
