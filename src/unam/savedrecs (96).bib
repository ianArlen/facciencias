@article{ ISI:000255195500008,
Author = {Rodriguez, R. and Estevez, M. and Vargas, S. and Salazar, R. and
   Pacheco, F.},
Title = {{Hap-based porous material with potential application as bio-packages for
   MEMS}},
Journal = {{JOURNAL OF MATERIALS SCIENCE-MATERIALS IN ELECTRONICS}},
Year = {{2008}},
Volume = {{19}},
Number = {{7}},
Pages = {{646-652}},
Month = {{JUL}},
Abstract = {{Novel hydroxyapatite-based hybrid materials with controlled porosity and
   good adhesion to silicon surfaces were designed as bio-package for MEMS
   with potential application for implants in the human body. These
   materials were prepared using synthetic Hydroxyapatite (HAp) powder with
   three different polymeric agglutinants. These porous materials have high
   ceramic content (up to 60 wt.\% respect to resin) with a pore size
   between 100 and 350 microns and a pore volume fraction in the 25-60\%
   range. These hybrid materials have high wearing resistance and
   hydrolytic stability. The samples were characterized mechanical and
   morphologically using XRD, SEM, densitometry, abrasion and mechanical
   tests.}},
