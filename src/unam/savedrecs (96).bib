@article{ ISI:000254742900004,
Author = {Uribe, Alejandro and Villegas-Blas, Carlos},
Title = {{Asymptotics of spectral clusters for a perturbation of the hydrogen atom}},
Journal = {{COMMUNICATIONS IN MATHEMATICAL PHYSICS}},
Year = {{2008}},
Volume = {{280}},
Number = {{1}},
Pages = {{123-144}},
Month = {{MAY}},
Abstract = {{We consider perturbations of the Schrodinger operator of the hydrogen
   atom, of size O(h(1+delta)) with delta > 0. We show that, if h is
   restricted to take values along a certain sequence converging to zero,
   the multiple eigenvalue -1/2 of the unperturbed Hamiltonian breaks into
   a cluster of eigenvalues disjoint from the rest of the spectrum. Then we
   obtain a Szego limit theorem for the spectral shifts in this cluster, as
   h -> 0. Specifically, we prove: The weak limit of the normalized
   spectral measure of the spectral shifts is the push-forward of Liouville
   measure of the unperturbed energy surface H = -1/2 by the averaged
   symbol of the perturbation.}},
