Author = {Espinosa, G. and Golzarri, J. I. and Bogard, J. and Gaso, I. and
   Ponciano, G. and Mena, M. and Segovia, N.},
Title = {{Indoor radon measurements in Mexico City}},
Journal = {{RADIATION MEASUREMENTS}},
Year = {{2008}},
Volume = {{43}},
