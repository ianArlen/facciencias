@article{ ISI:000257221800014,
Author = {Barbero, C. and Castro, G. Lopez and Mariano, A.},
Title = {{Single pion production in CC nu N-mu scattering within a consistent
   effective Born approximation}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2008}},
Volume = {{664}},
Number = {{1-2}},
Pages = {{70-77}},
Month = {{JUN 12}},
Abstract = {{The one pion production in charged current nu N-mu scattering is
   analyzed within an extended version of a model used previously to
   include the A(1232) contribution in elastic and radiative pion-nucleon
   scattering, and in pion photoproduction. Because the resonant amplitude
   needs to be invariant under contact transformations, we identify the
   correct forms of the A propagator and the W-N Delta vertex that are
   consistent with this requirement. The only free parameter of the model,
   the axial form factor at zero momentum transfer, is fixed from data on
   the differential cross section for v(mu)p -> mu(-)p pi(+) scattering. A
   reasonable agreement with the experimental data of all the nu N -> mu
   N'pi total cross sections is obtained. We show that the use of the
   complete A propagator instead of the Rarita-Schwinger one improves the
   theoretical results, leading to differences ranging from 10 to 30\%,
   depending on the specific process. (C) 2008 Published by Elsevier B.V.}},
