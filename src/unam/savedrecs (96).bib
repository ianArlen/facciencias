@article{ ISI:000257283800002,
Author = {Zenit, Roberto and Magnaudet, Jacques},
Title = {{Path instability of rising spheroidal air bubbles: A shape-controlled
   process}},
Journal = {{PHYSICS OF FLUIDS}},
Year = {{2008}},
Volume = {{20}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{The conditions for which the paths of freely rising bubbles become
   oscillatory are studied experimentally using silicone oils with
   viscosities ranging from 0.5 to 9.4 times that of water. Since these
   fluids are nonpolar, as opposed to water, the gas-liquid interfaces
   remain clean without the need of an ultrapure environment. We find the
   Reynolds number at incipient transition to vary from 70 to 470, for
   decreasing liquid viscosity. Correspondingly, the bubble aspect ratio
   remains almost constant, ranging only from 2.36 to 2.0 for the same set
   of conditions. Hence, we argue that the dominant parameter to trigger
   the instability is the bubble shape and not the Reynolds number. Since
   vorticity generated at the bubble surface is almost independent of the
   Reynolds number and mostly depends on the bubble aspect ratio in the
   parameter range covered by our experiments, present results strongly
   support the view that path instability is a direct consequence of the
   wake instability that occurs when this surface vorticity exceeds a
   certain threshold. (C) 2008 American Institute of Physics.}},
