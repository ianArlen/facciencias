@article{ ISI:000256371200006,
Author = {Elizabeth Jimenez-Cruz, Blanca and Silva-Gutierrez, Cecilia},
Title = {{Effects of a brief exposure to women's media images on levels of anxiety
   and depression of university women considering their Body Mass Index and
   their attitudes towards weight and food}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2008}},
Volume = {{25}},
Number = {{1}},
Pages = {{89-98}},
Month = {{JUN}},
Abstract = {{The purpose of the study was to determine if a brief exposure to women's
   media images affects the levels of anxiety and depression of university
   women. Their Body Mass Index (BMI) and their attitudes towards weight
   and food were measured in order to evaluate if these were related to
   anxiety and depression levels after seeing the images. A total of 376
   college women, divided in three groups, participated in the study. The
   media images were selected from different magazines. One group (n = 140)
   saw images of thin women, a second group (n = 127) saw images of
   overweight women, and the third group (n = 109) was exposed to images of
   women exercising. Participants answered the Eating Attitude Test
   (EAT-40), the State-Trait Anxiety Inventory (STAI), and the Axelrod
   Adjective Checklist both before and after being exposed to the media
   images. Results showed a significant increase of anxiety and depression
   for all the participants after seeing the images, but the increase was
   larger for women with high BMI. Significant correlations were found
   between BMI and motivation to be thin, avoidance of fattening foods,
   worries about food, and compensatory behaviors. These findings suggest
   that anxiety is produced by exposure to both, thin and overweight women
   bodies. It was concluded that the exposure to the images was related to
   an increased risk of developing an alimentary pathology behavior when
   BMI is high and when attitudes towards body weight and food are
   negative.}},
