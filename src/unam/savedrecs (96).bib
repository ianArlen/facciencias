@article{ ISI:000256492200002,
Author = {Valencia, Israel and Chavez, Victor and Castro, Miguel},
Title = {{Bonding of benzene with excited states of Fe-7}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY A}},
Year = {{2008}},
Volume = {{112}},
Number = {{23}},
Pages = {{5028-5033}},
Month = {{JUN 12}},
Abstract = {{The interaction between high-spin Fe-7 clusters and a benzene molecule
   was studied using the BPW91/6-311++G(2d,2p) method. The Fe-7-C6H6 ground
   state has a T-shaped structure, similar to that of the benzene dimer,
   and a multiplicity M = 2S + 1 = 19 (S = total spin). The carbon atoms
