@article{ ISI:000257150700018,
Author = {Smith, Geoffrey R. and Lemos-Espinal, Julio A. and Dibble, Christopher
   J. and Iverson, J. Peter},
Title = {{Natural history of Procinura aemula (Serpentes : Colubridae) from
   Chinipas, Chihuahua, Mexico}},
Journal = {{SOUTHWESTERN NATURALIST}},
Year = {{2008}},
Volume = {{53}},
Number = {{2}},
Pages = {{261-264}},
Month = {{JUN}},
Abstract = {{We examined several aspects of the biology of Procinura aemula,
   including diet, sexual dimorphism, and clutch size, from Chinipas,
   Chihuahua, Mexico. Diet of P. aemula was composed entirely of
   invertebrates. Orthopterans and crickets were the majority of the diet,
   both numerically and volumetrically. Prey size was significantly related
   to snake size (snout-vent length, head size). There was no sexual
   dimorphism in snout-vent length or head size. Mean clutch size was 4.6
   eggs. Clutch size tended to increase with snout-vent length of females,
   but the trend was not statistically significant.}},
