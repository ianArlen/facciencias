@article{ ISI:000257630400003,
Author = {Dharmalingam, K. and Jalbout, A.},
Title = {{Molecular interaction of alkanols with acrylic esters: A spectroscopic
   study}},
Journal = {{JOURNAL OF MOLECULAR LIQUIDS}},
Year = {{2008}},
Volume = {{141}},
Number = {{1-2}},
Pages = {{17-18}},
Month = {{MAY 30}},
Abstract = {{Using FTIR spectroscopy, the complexation of alkanols viz. n-pentanol,
   sec-pentanol and tert-pentanol with acrylic esters viz. methyl
   methacrylate (MMA), ethyl methacrylate (EMA) and butyl methacrylate
   (BMA) in organic solvents viz. n-heptane, CCl4 and benzene has been
   studied. The formation constants and free energy changes of the
   complexation were determined. It was observed that n-pentanol forms
   stronger complexes with acrylic esters than tert-pentanol in the latter
   systems, which is attributed to the difference in steric hindrance. The
   strength and the nature of interaction between the molecules of pentanol
   with esters and acrylic esters have been discussed. The effect of
   solvent on the formation of the alkanol-ester complexes is also
   discussed. (C) 2008 Elsevier B.V. All rights reserved.}},
