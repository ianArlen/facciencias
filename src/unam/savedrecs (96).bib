@article{ ISI:000256597600012,
Author = {Kite, Geoffrey C. and Salazar, Gerardo A.},
Title = {{Chemical composition of the inflorescence odor of Malaxis rzedowskiana
   (Orchidaceae)}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2008}},
Volume = {{79}},
Number = {{1}},
Pages = {{153-157}},
Month = {{JUN}},
Abstract = {{Malaxis rzedowskiana R.Gonzalez (Malaxideae, Orchidaceae) from Mexico
   produces a pleasant floral odor reminiscent of violets in contrast to
   the unpleasant odors noted for several other members of Malaxideae.
   Analysis of the floral odor of M. rzedowskiana by headspace trapping and
   thermal desorption-gas chromatography-mass spectrometry revealed the
   presence of kaurene (76\%), (E)-beta-ionone (18\%) and (E)-a-ionone
   (4\%) as the main components. This is the first report of a floral odor
   containing a high proportion of kaurene.}},
