@article{ ISI:000267196000071,
Author = {Manuel Juarez-Calderon, J. and Ramos-Bernal, Sergio and Negron-Mendoza,
   Alicia},
Title = {{Evaluation of Commercial Grade Ferrous Ammonium Sulfate as Potential
   Dosimeter for Technological Irradiations}},
Journal = {{JOURNAL OF NUCLEAR SCIENCE AND TECHNOLOGY}},
Year = {{2008}},
Number = {{5}},
Pages = {{275-277}},
Month = {{JUN}},
Note = {{4th International Symposium on Radiation Safety and Detection
   Technology, Hanyang Univ, Seoul, SOUTH KOREA, JUL 18-20, 2007}},
Organization = {{Innovat Technol Ctr Radiat Safety; Korean Minist Educ, Sci \& Technol;
   Korean Assoc Radiat Protect}},
Abstract = {{In this work, we have studied the behavior of crystalline ferrous
   ammonium sulfate (FAS) under gamma irradiation. The doses studied ranged
   from 33.5 to 270 kGy. The purpose of this study is to explain the setup,
   measurement, and reporting procedures for using FAS as a dosimeter.
   The results obtained in the present Study show that this salt very
   easily gave reproducible results, a linear response, as well as, simple
   sample preparation and reading. The irradiation temperature and dose
   rate in the response of the iron salt were found to have slight
   influence. Any storage time up to 18 months resulted in a 36\% decrease
   in the response. The variation in the response obtained for short
   periods of storage is negligible for this application. Due to the
   properties of the FAS system also can be use as transfer dosismeter.}},
