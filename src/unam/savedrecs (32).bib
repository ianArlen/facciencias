@article{ ISI:000389650900004,
Author = {Rio Torres-Murciano, Antonio},
Title = {{The sphragis of Ovid's Metamorphoses (XV 871-879). Metempsychosis,
   apotheosis and literary durability}},
Journal = {{EMERITA}},
Year = {{2016}},
Volume = {{84}},
Number = {{2}},
Pages = {{269-289}},
Abstract = {{Ovid's prediction of his own immortality in the sphragis of the
   Metamorphoses has been understood as a last transformation comparable,
   for its dualism, to metempsychosis and apotheosis. Yet an inquiry on the
   causes of these processes permits to highlight the specificity of
   literary durability, which is a product of the favor of the readers and
   not of a natural law, like metempsychosis, or of a gift of the gods,
   like apotheosis.}},
