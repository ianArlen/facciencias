Editor = {{Winston, R and Gordon, JM}},
Title = {{Modeling of efficient light extraction in light-pipes through specular
   surfaces with elliptical and lineal front shapes}},
Booktitle = {{NONIMAGING OPTICS: EFFICIENT DESIGN FOR ILLUMINATION AND SOLAR
