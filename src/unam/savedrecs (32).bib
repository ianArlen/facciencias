@inproceedings{ ISI:000389510100009,
Author = {Castaneda, Armando and Delporte, Carole and Fauconnier, Hugues and
   Rajsbaum, Sergio and Raynal, Michel},
Editor = {{Bonakdarpour, B and Petit, F}},
Title = {{Making Local Algorithms Wait-Free: The Case of Ring Coloring}},
