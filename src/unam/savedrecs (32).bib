@inproceedings{ ISI:000390697400132,
Author = {Murrieta-Rico, Fabian N. and Petranovskii, Vitalii and Raymond-Herrera,
   Oscar and Sergiyenko, Oleg and Lindner, Lars and Hernandez-Balbuena,
   Daniel and Rodriguez-Quinonez, Julio C. and Nieto-Hipolito, Juan I. and
