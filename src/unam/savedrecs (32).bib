@article{ ISI:000393466300001,
Author = {Arevalo-Guizar, Gabriela and Rueda-Beltran, Mario},
Title = {{Academic Reviews: between the evaluation and the completion of their
   social function}},
Journal = {{RELIEVE-REVISTA ELECTRONICA DE INVESTIGACION Y EVALUACION EDUCATIVA}},
Year = {{2016}},
Volume = {{22}},
Number = {{1}},
Abstract = {{This paper presents the necessity of reviewing the context of the
   evaluation of academic work in order to understand the difficulties
   faced by Latin American academic journals in order to comply with
   evaluation requirements through the use of an impact factor and
   interaction with companies that make use of it. It further describes the
   influence exercised by commercial publishers over the production and
   circulation of knowledge. It examines some of the strategies followed by
   Latin American journals to increase visibility of the knowledge, while
