@inproceedings{ ISI:000382818600024,
Author = {Moreno, Jaime A.},
Book-Group-Author = {{IEEE}},
Title = {{Discontinuous integral control for mechanical systems}},
Booktitle = {{2016 14TH INTERNATIONAL WORKSHOP ON VARIABLE STRUCTURE SYSTEMS (VSS)}},
Series = {{IEEE International Workshop on Variable Structure Systems}},
Year = {{2016}},
Pages = {{142-147}},
Note = {{14th International Workshop on Variable Structure Systems (VSS),
   Southeast Univ, Nanjing, PEOPLES R CHINA, JUN 01-04, 2016}},
Organization = {{IEEE Ind Elect Soc; IEEE Control Syst Soc}},
Abstract = {{For mechanical systems we present a controller able to track an unknown
   smooth signal, converging in finite time and by means of a continuous
   control signal. The control scheme is insensitive against unknown
   perturbations with bounded derivative. The controller consists of a non
   locally Lipschitz state feedback control law, and a discontinuous
   integral controller, that is able to estimate the unknown perturbation
   and to compensate for it. To complete an output feedback control a
   continuous observer for the velocity is added. It is shown that the
   closed loop consisting of state feedback, state observer and
   discontinuous integral controller has an equilibrium point that is
   globally, finite time stable, despite of perturbations with bounded
   derivative. The proof is based on a new smooth Lyapunov function.}},
