Title = {{Secondary Electron Fluence Generated in LiF:Mg, Ti by Low-Energy Photons
   and its contribution to the Absorbed Dose}},
Booktitle = {{MEDICAL PHYSICS: FOURTEENTH MEXICAN SYMPOSIUM ON MEDICAL PHYSICS}},
Series = {{AIP Conference Proceedings}},
