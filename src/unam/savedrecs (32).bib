@inproceedings{ ISI:000387795900007,
Author = {Beckman, John and Camps Farina, Artemi and Zaragoza Cardiel, Javier and
   Font, Joan},
Editor = {{Jablonka, P and Andre, P and VanDerTak, F}},
Title = {{Detection and measurement of expanding ionized shells around young star
   clusters}},
Booktitle = {{FROM INTERSTELLAR CLOUDS TO STAR-FORMING GALAXIES: UNIVERSAL PROCESSES?}},
