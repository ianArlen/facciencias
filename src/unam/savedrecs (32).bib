Author = {Perez Ortega, Edgar and Ayala Gomez, Mario Manuel},
Title = {{CONFIABILIZATION OF A SCALE FOR EVALUATE THE SOCIAL DIMENTION OF THE
   SELF-CONCEPT IN STREET CHILDREN}},
