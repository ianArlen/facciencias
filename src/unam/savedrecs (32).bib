Title = {{Unifier register to protect an efficient modular exponentiation
   algorithm}},
Booktitle = {{20TH INTERNATIONAL CONFERENCE ON CIRCUITS, SYSTEMS, COMMUNICATIONS AND
   COMPUTERS (CSCC 2016)}},
Series = {{MATEC Web of Conferences}},
Year = {{2016}},
