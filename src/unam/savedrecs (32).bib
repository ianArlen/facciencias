@article{ ISI:000383257000002,
Author = {Jose, Negrete-Martinez and Roberto, Cruz-Estrada and Santiago,
   Negrete-Yankelevich},
Title = {{Functional modularization of a Neurocranium: a robotic brain for a
   robotic body}},
Journal = {{ENEUROBIOLOGIA}},
Year = {{2016}},
Volume = {{7}},
Number = {{15}},
Abstract = {{We have designed and constructed a modular neurocranium with functional
   modules from skeleton modules, motor modules, pre-motor modules,
   sensorial modules, and even integrative modules. We will show that the
   superior modules, the integrative ones, are determined by the nature of
   their precedent sensorial and motor modules, and that these last ones,
   in turn, are determined by the skeleton modules in a true modular
   functional cascade that reinforces the philosophical position that there
   is a specific brain for each body.}},
