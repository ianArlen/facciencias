@inproceedings{ ISI:000387731100024,
Author = {Toledo-Ramirez, Gengis K. and Bringas-Rico, Vicente and Reyes, Noe and
   Uribe, Jorge and Lopez, Aldo and Tovar, Carlos and Caballero, Xochitl
