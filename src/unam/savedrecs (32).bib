@article{ ISI:000389773100003,
Author = {Bhuyan, Rupaleem and Osborne, Bethany J. and Juanico Cruz, Janet Flor},
Title = {{``Once You Arrive, Se Te Sala Todo{''} (Everything is Salted): Latina
   Migrants' Search for ``Dignity and a Right to Life{''} in Canada}},
Journal = {{JOURNAL OF IMMIGRANT \& REFUGEE STUDIES}},
Year = {{2016}},
Volume = {{14}},
Number = {{4}},
Pages = {{411-431}},
Abstract = {{This study explores how Latina women fleeing gender-related violence
   seek protection for themselves and their children under Canada's
   humanitarian laws. Rising emphasis on border control contributes to a
   growing number of forced migrants whose transnational movement is
   constructed as ``illegal.{''} Migrants who fall outside legal migration
   channels are exposed to precarious conditions that can lead to further
   violence. Through interpretive analysis of in-depth interviews with
   women from Mexico and Central America, we explore how immigration
   policies produce gendered forms of ``illegality.{''} We also highlight
   how women's migration in search for rights and protection represents a
   form of substantive citizenship.}},
