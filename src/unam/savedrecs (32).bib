@article{ ISI:000377931300003,
Author = {Basiuk, Vladimir A. and Gonzalez-Luciano, Efrain},
Title = {{Noncovalent interactions of amino acids with fullerene C-60: A
   dispersion-corrected DFT study}},
Journal = {{FULLERENES NANOTUBES AND CARBON NANOSTRUCTURES}},
Year = {{2016}},
Volume = {{24}},
Number = {{6}},
Pages = {{371-379}},
Abstract = {{By using the general gradient approximation functional PBE with Grimme's
   empirical dispersion correction in conjunction with the double numerical
   basis set DNP, we studied the noncovalent interaction of twenty
   proteinogenic l-amino acids (AAs) with fullerene C-60. The calculations
   were performed both under vacuum conditions and in aqueous medium. We
   analyzed the calculated geometries and binding energies for AA+C-60
   complexes, the shape of HOMO and LUMO orbitals and the corresponding gap
   energies. Generally, we found a poor correlation between binding
   energies calculated for AA+C-60 complexes in aqueous medium and
   hydrophobicity scales proposed by other other authors. Despite of C-60
   cage can be envisioned as a typical hydrophobic surface, the AA
   adsorption apparently takes place according to a mechanism different
   from classical hydrophobic interactions, or due to a combination of
   several types of interactions with a limited contribution of hydrophobic
   mechanism.}},
