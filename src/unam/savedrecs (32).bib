@article{ ISI:000382070700001,
Author = {Romero-Rebollar, Cesar and Jimenez-Angeles, Luis and Antonio
   Dragustinovis-Ruiz, Eduardo and Medina-Banuelos, Veronica},
Title = {{Neural Modulation in Aversive Emotion Processing: An Independent
   Component Analysis Study}},
Journal = {{COMPUTATIONAL AND MATHEMATICAL METHODS IN MEDICINE}},
Year = {{2016}},
Abstract = {{Emotional processing has an important role in social interaction. We
   report the findings about the Independent Component Analysis carried out
   on a fMRI set obtained with a paradigm of face emotional processing. The
   results showed that an independent component, mainly
   cerebellar-medial-frontal, had a positive modulation associated with
   fear processing. Also, another independent component, mainly
   parahippocampal-prefrontal, showed a negative modulation that could be
   associated with implicit reappraisal of emotional stimuli. Independent
   Component Analysis could serve as a method to understand complex
   cognitive processes and their underlying neural dynamics.}},
