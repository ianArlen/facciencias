@article{ ISI:000387015600005,
Author = {Martinez, Araceli and Zuniga-Villarreal, Noe and Gutierrez, Selena and
   Tlenkopatchev, Mikhail A.},
Title = {{New Ru-Vinylidene Catalysts in the Cross-Metathesis of Natural Rubber
   and Poly(Styrene-co-Butadiene) with Essential Oils}},
