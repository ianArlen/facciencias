@article{ ISI:000391588600013,
Author = {Franci, Alessio and Sepulchre, Rodolphe},
Title = {{A Three-Scale Model of Spatio-Temporal Bursting}},
Journal = {{SIAM JOURNAL ON APPLIED DYNAMICAL SYSTEMS}},
Year = {{2016}},
Volume = {{15}},
Number = {{4}},
Pages = {{2143-2175}},
Abstract = {{We study spatio-temporal bursting in a three-scale reaction diffusion
   equation organized by the winged cusp singularity. For large time-scale
   separation the model exhibits traveling bursts, whereas for large
   space-scale separation the model exhibits standing bursts. Both
   behaviors exhibit a common singular skeleton, whose geometry is fully
   determined by persistent bifurcation diagrams of the winged cusp. The
   modulation of spatio-temporal bursting in such a model naturally
   translates into paths in the universal unfolding of the winged cusp.}},
