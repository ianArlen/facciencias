@incollection{ ISI:000364397100005,
Author = {Masera, Omar R. and Bailis, Rob and Drigo, Rudi and Ghilardi, Adrian and
   Ruiz-Mercado, Ilse},
Editor = {{Gadgil, A and Tomich, TP}},
Title = {{Environmental Burden of Traditional Bioenergy Use}},
