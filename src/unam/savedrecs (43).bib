@article{ ISI:000364073800001,
Author = {Arellano-Vazquez, Magali and Benitez-Perez, Hector and Ortega-Arjona, J.},
Title = {{A Consensus Routing Algorithm for Mobile Distributed Systems}},
Journal = {{INTERNATIONAL JOURNAL OF DISTRIBUTED SENSOR NETWORKS}},
Year = {{2015}},
Abstract = {{This paper introduces a consensus routing algorithm based on an
   availability function. Such a function obtains a consensus among a group
   of nodes by evaluating the idle time in the scheduler of each node,
   along with the general conditions of the network and determining the
   next step of the route. In this regard, the algorithm proposed here
   makes it possible to avoid flooding while reducing bandwidth use and
   keeping changes of the network only at a local scale.}},
