Title = {{Rotations of the most asymmetric molecules via 4-step and 1step ladder
   operators}},
Booktitle = {{XXXTH INTERNATIONAL COLLOQUIUM ON GROUP THEORETICAL METHODS IN PHYSICS
   (ICGTMP) (GROUP30)}},
Series = {{Journal of Physics Conference Series}},
Year = {{2015}},
