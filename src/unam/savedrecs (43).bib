@article{ ISI:000359020200008,
Author = {Hernandez-Gutierrez, Rodrigo and Illanes, Alejandro and
   Martinez-de-la-Vega, Veronica},
Title = {{RIGIDITY OF HYPERSPACES}},
Journal = {{ROCKY MOUNTAIN JOURNAL OF MATHEMATICS}},
Year = {{2015}},
Volume = {{45}},
Number = {{1}},
Pages = {{213-236}},
Abstract = {{Given a metric continuum X, we consider the following hyperspaces of X:
   2(X), C-n(X) and F-n(X) (n is an element of N). Let F-1(X) = \{\{x\} : x
   is an element of X\}. A hyperspace K(X) of X is said to be rigid,
   provided that for every homeomorphism h : K(X) -> K(X), we have
   h(F-1(X)) = F-1(X). In this paper, we study conditions under which a
   continuum X has a rigid hyperspace C-n(X). Among others, we consider
   families of continua, such as dendroids, Peano continua, hereditarily
   indecomposable continua and smooth fans.}},
