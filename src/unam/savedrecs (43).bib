@inproceedings{ ISI:000364552600018,
Author = {Ahmad, A. and Ayala, A. and Bashirl, A. and Gutierrez, E. and Raya, A.},
Book-Group-Author = {{IOP}},
Title = {{QCD Phase Diagram and the Constant Mass Approximation}},
Booktitle = {{XIV MEXICAN WORKSHOP ON PARTICLES AND FIELDS}},
