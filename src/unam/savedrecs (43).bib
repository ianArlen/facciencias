@article{ ISI:000355056700014,
Author = {Moreno Leoni, Alvaro M.},
Title = {{Memory, history and hatred in Pausanias's Periegesis}},
Journal = {{AGORA-ESTUDOS CLASSICOS EM DEBATE}},
Year = {{2015}},
Volume = {{17}},
Pages = {{347-369}},
Abstract = {{In this article, we look into the problem of memory and hatred among the
   Greeks, as presented in Pausanias's Periegesis. Trophies and spoils of
   victories of Greeks over Greeks emerge as signs that perpetuate in the
   landscapes of each of the locations visited the victories of the past,
   thereby acting as reminders that rescue from oblivion. We intend to show
   the importance of this situation within the context of the historical
   reading of the past proposed by the Periegete which stresses the nature
   of the perpetuated hatred as an annihilating force opposed to
   Panhellenism, and therefore helps to explain Greece's loss of freedom.}},
