@inproceedings{ ISI:000352366400038,
Author = {Lopez, M. and Vazquez, F. and Solis-Najera, S. and Rodriguez, A.},
Book-Group-Author = {{IOP}},
Title = {{Numerical study of remote detection outside the magnet with travelling
   wave Magnetic Resonance Imaging at 3T}},
Booktitle = {{VII INTERNATIONAL CONGRESS OF ENGINEERING PHYSICS}},
