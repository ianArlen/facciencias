Title = {{Dynamics of magnetic islands and confinement transitions in TJ-II}},
Booktitle = {{15TH LATIN AMERICAN WORKSHOP ON PLASMA PHYSICS (LAWPP 2014) AND 21ST
   IAEA TM ON RESEARCH USING SMALL FUSION DEVICES (RUSFD)}},
Series = {{Journal of Physics Conference Series}},
