@inproceedings{ ISI:000364552600019,
Author = {Roig, Pablo},
Book-Group-Author = {{IOP}},
Title = {{Failure of Breit-Wigner and success of dispersive descriptions of the
   tau(-) -> K-eta nu(T), decays}},
Booktitle = {{XIV MEXICAN WORKSHOP ON PARTICLES AND FIELDS}},
Series = {{Journal of Physics Conference Series}},
Year = {{2015}},
Volume = {{651}},
