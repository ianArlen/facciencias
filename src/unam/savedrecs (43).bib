Title = {{Degeneracy on a two-Higgs CP non-invariant Higgs sector}},
Booktitle = {{4TH SYMPOSIUM ON PROSPECTS IN THE PHYSICS OF DISCRETE SYMMETRIES
   (DISCRETE2014)}},
Series = {{Journal of Physics Conference Series}},
