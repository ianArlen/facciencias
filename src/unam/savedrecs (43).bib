Author = {Hernandez, C. and Figueroa, I. A. and Alfonso, I. and Braham, C. and
   Castillo, P. and Gonzalez, G.},
Title = {{Microstructure and Texture Evolution of the Al-20Sn Alloy Processed by
   Equal-Channel Angular Pressing Using Route C}},
Journal = {{MATERIALS TRANSACTIONS}},
