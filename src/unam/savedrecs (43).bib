@inproceedings{ ISI:000364552600003,
Author = {Bietenholz, Wolfgang and Hofheinz, Frank and Mejia-Diaz, Hector and
   Panero, Marco},
Book-Group-Author = {{IOP}},
Title = {{Scalar fields in a non-commutative space}},
