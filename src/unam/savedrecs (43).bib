@inproceedings{ ISI:000365890500016,
Author = {Avendano-Alejo, Maximino},
Editor = {{Winston, R and Gordon, JM}},
Title = {{Exact wavefront surface refracted by a smooth arbitrary surface
   considering a plane wavefront incident}},
Booktitle = {{NONIMAGING OPTICS: EFFICIENT DESIGN FOR ILLUMINATION AND SOLAR
   CONCENTRATION XII}},
Series = {{Proceedings of SPIE}},
Year = {{2015}},
Volume = {{9572}},
Note = {{Conference on Nonimaging Optics - Efficient Design for Illumination and
   Solar Concentration XII, San Diego, CA, AUG 09-10, 2015}},
Organization = {{SPIE}},
Abstract = {{We study the formation of wavefronts produced by smooth arbitrary
   surfaces with symmetry of revolution considering a plane wavefront
   propagating parallel to the optical axis and impinging on the refracting
   surface. The wavefronts are obtained by using the Malus-Dupin theorem
   and they represent the monochromatic aberrations which can be called
   image errors, furthermore their shapes could be modified by changing the
   parameters of the lens in such a way that if a caustic surface is
   vanished the optical system produces a perfect image, on the other hand
   for a caustic possessing a large area it could be applied to design
   non-imaging optical systems. The shape of the wavefront depends only on
   the indices of refraction and geometrical properties of the refracting
   surface such as the first derivative and their parameters associated.
   This analytic formula has potential applications in the microscopy
   field, illumination or corrector plates.}},
