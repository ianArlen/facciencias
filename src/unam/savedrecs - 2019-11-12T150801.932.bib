@article{ ISI:000089800500002,
Author = {Brailovsky, H},
Title = {{A revision of the tribe Colpurini (Hemiptera : Heteroptera : Coreidae)
   from Sulawesi}},
Journal = {{TRANSACTIONS OF THE AMERICAN ENTOMOLOGICAL SOCIETY}},
Year = {{2000}},
Volume = {{126}},
Number = {{2}},
Pages = {{175-220}},
Month = {{JUN}},
Abstract = {{The tribe Colpurini (Coreidae) from Sulawesi is revised. Four genera,
   two subgenera, and 20 species are recorded. Among them are one new genus
   (Sulawesihygia) and 12 new species (Homalocolpura delectabilis, Hygia
   (Sphinctocolpura) dumoga, H. (S.) lepida, H. (S.) palumae, H. (S.)
   presigna, H. (S.) pronotata, H. (S.) scitula, H. (S.) tengaha, H. (S.)
   terminalis, H. (S.) utaranus, Sulawesihygia tyttha and Typhlocolpura
   amblysa. New distributional records are given for Hygia (Caracolpura)
   planiceps (Breddin), Hygia (Sphinctocolpura) guttatipes (Breddin), H.
   (S.) minahassae Blote, H. (S.) roratipes (Breddin), and Typhlocolpura
   decoratula Breddin. A diagnosis for previously known species are
   provided. Diagnostic taxonomic characters of head, pronotum, male and
   female genitalia, and dorsal view of some species are illustrated. A key
   is provided for the known genera, subgenera and species.}},
