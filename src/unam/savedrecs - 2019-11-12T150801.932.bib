@article{ ISI:000088237800009,
Author = {Gomez-Peresmitre, G and Granados, A and Jauregui, J and Garcia, GP and
   Ramos, SAT},
Title = {{Body image measurement: paper and pencil and computarized test versions}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2000}},
Volume = {{17}},
Number = {{1}},
Pages = {{89-99}},
Month = {{JUN}},
Abstract = {{The purpose of this study was to evaluate from a psychometric point of
   view, easy to administer paper and pencil and computerized tests that
   measure body image, and to establish their advantages. Two contrasting
   samples, one of normal (n = 127) and one of subclinical women (n = 185)
   answered both forms of the test. Comparisons on pertinent variables with
   both forms were statistically Significant and theoretically consistent
   with expected results. The computerized version was psychometrically
   better and more acceptable (predictive capacity, discriminant function,
   and internal consistency) than the paper and pencil version.}},
