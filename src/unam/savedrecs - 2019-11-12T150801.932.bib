@article{ ISI:000086219600014,
Author = {Hinojosa-Torres, J and Zhu, YH and Herrera-Vazquez, A and Castano, VM},
Title = {{Strengthening of Zn-22Al-2Cu alloy wire by artificial ageing}},
Journal = {{MATERIALS LETTERS}},
Year = {{2000}},
Volume = {{43}},
Number = {{3}},
Pages = {{153-157}},
Month = {{APR}},
Abstract = {{Changes in hardness, measured at room temperature, were detected after
   continuous artificial ageing at 250 degrees C, for different periods, in
   a Zn-22Al-2Cu (wt.\%) alloy wire. The presence of the ternary stable
   tau' phase precipitate (Al(4)Cu(3)Zn) and the decomposition of the
   Zn-rich hcp metastable eta'(T) phase as a consequence of the ageing, are
   shown in this work. Then, X-ray diffraction showed that after 1 h of
   ageing the hcp eta phase is present in the microstructure of the wire,
   this phase remaining stable along the heat treatment. The wire was
   prepared by rapid solidification by using the semicontinuous casting
   technique, followed by extrusion of the semicontinuous casting rods and,
   finally, wiredrawing of the extruded material. (C) 2000 Elsevier Science
   B.V. All rights reserved.}},
