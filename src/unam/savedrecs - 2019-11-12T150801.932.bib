@article{ ISI:000086042300003,
Author = {Busi, E and Howes, BD and Pogni, R and Basosi, R and Tinoco, R and
   Vazquez-Duhalt, R},
Title = {{Modified cytochrome c/H2O2 system: spectroscopic EPR investigation of
   the biocatalytic behaviour}},
Journal = {{JOURNAL OF MOLECULAR CATALYSIS B-ENZYMATIC}},
Year = {{2000}},
Volume = {{9}},
Number = {{1-3}},
Pages = {{39-48}},
Month = {{MAR 20}},
Abstract = {{In recent years there has been growing interest in methods for the
   degradation of polycyclic aromatic hydrocarbons. Cytochrome c (Cyt c)
   systems in the presence of H2O2 are able to oxidize various aromatic
   compounds. In order to investigate ways of improving the performance of
   Cyt c/H2O2 oxidation systems, site-directed mutagenesis, and chemical
   modifications on the hemoprotein surface with poly(ethylene glycol) and
   methylation of the active site have been performed. The EPR technique
   and UV-VIS spectroscopy have been used to identify radical intermediates
   and heme iron spin states of the chemical modified Cyt c (PEG-Cyt-Met)
   and Cyt c mutants. (C) 2000 Elsevier Science B.V. All rights reserved.}},
