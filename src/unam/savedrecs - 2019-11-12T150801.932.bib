@article{ ISI:000089132400001,
Author = {Rojas-Avelizapa, NG and Rodriguez-Vazquez, R and Saval-Bohorquez, S and
   Alvarez, PJJ},
Title = {{Effect of C/N/P ratio and nonionic surfactants on polychlorinated
   biphenyl biodegradation}},
Journal = {{WORLD JOURNAL OF MICROBIOLOGY \& BIOTECHNOLOGY}},
Year = {{2000}},
Volume = {{16}},
Number = {{4}},
Pages = {{319-324}},
Month = {{JUN}},
Abstract = {{This work investigated whether polychlorinated biphenyl (PCB) removal
   from a highly contaminated soil (7000-p.p.m.) could be enhanced by
   manipulating the carbon to nitrogen to phosphorus (C/N/P) ratio, and by
   nonionic surfactant addition. A Box-Behnken statistical experimental
   design was used to evaluate the combined effect of surfactant type,
   surfactant concentration, and C/N/P ratio in a relatively short
   treatment period (35 days). The variable with the greatest effect on PCB
   degradation was the type of surfactant used. Higher PCB removal
   efficiencies (39-60\%) were obtained with Tween 80 (compared to Tergitol
   NP 10 and Triton X-100). This was attributed to its lower critical
   micelle concentration. Higher C/N/P ratios (increased by biphenyl
   addition) significantly stimulated the soil heterotrophic activity
   without enhancing PCB removal. This suggests that nonionic surfactants
   have a greater potential to enhance bioremediation of PCB-contaminated
   soil than efforts to enhance the soil heterotrophic activity through
   nutrient and analogue substrate addition.}},
