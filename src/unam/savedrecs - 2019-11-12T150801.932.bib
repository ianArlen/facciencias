@article{ ISI:000087534100020,
Author = {Sharma, VK and Rendon, RA and Millero, FJ and Vazquez, FG},
Title = {{Oxidation of thioacetamide by ferrate(VI)}},
Journal = {{MARINE CHEMISTRY}},
Year = {{2000}},
Volume = {{70}},
Number = {{1-3}},
Pages = {{235-242}},
Month = {{MAY}},
Abstract = {{The rates of thioacetamide oxidation with ferrate(VI) (Fe(VI)) were
   determined as a function of pH (9.14-12.00) and temperature (15-35
   degrees C) in 0.01 M phosphate. The reaction was found to be first order
   for each reactant. The activation energy, Delta E(not equal), obtained
   was 59.7 +/- 0.7 kJ mol(-1). The rates decrease with an increase in pH.
   The stoichiometry determined was 8HFeO(4)(-)+ 3CH(3)CSNH(2) + 9H(2)O -->
   8Fe(OH)(3) + 3CH(3)CONH(2), + 3SO(4)(2-)+ 2OH(-). A mechanism is
