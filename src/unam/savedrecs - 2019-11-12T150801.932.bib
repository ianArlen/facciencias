@article{ ISI:000086704100021,
Author = {Palomino-Ovando, M and Coyotecatl, HA and Esquivel-Sirvent, R and
   Cocoletzi, GH},
Title = {{Transit times for electromagnetic waves in metallic layered systems}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2000}},
Volume = {{267}},
Number = {{5-6}},
Pages = {{429-433}},
Month = {{MAR 27}},
Abstract = {{Metallic multilayered arrays are considered, in the local Drude theory,
   to investigate transit times of electromagnetic waves and the fast
   response mediated by plasmon polaritons. The transit times are
   calculated for frequencies corresponding to band gaps in the dispersion
   relation of periodic layered media. At these frequencies, and depending
   on the thickness of the structure, a fast response or even a
