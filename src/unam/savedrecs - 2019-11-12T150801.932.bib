@article{ ISI:000088291600004,
Author = {Dechoum, K and de la Pena, L and Santos, E},
Title = {{The problem of light detection in the presence of zeropoint
   fluctuations: A physical model for the vision of weak signals}},
Journal = {{FOUNDATIONS OF PHYSICS LETTERS}},
Year = {{2000}},
Volume = {{13}},
Number = {{3}},
Pages = {{253-264}},
Month = {{JUN}},
Abstract = {{We examine the possibility of obtaining a model of human vision within
   the context of a purely wave theory of light, including the zero-point
   field. We show that the model meets one of the main objections to this
   assumption, namely that photodetectors should be activated to saturation
   by the zeropoint field alone. A straightforward consequence of the model
   is the existence of a minimal signal intensity which may be
   discriminated by the human eye from the (huge) zeropoint intensity. The
   threshold predicted by the model roughly agrees with empirical results.}},
