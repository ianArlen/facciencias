@article{ ISI:000088555000005,
Author = {Ramos, G and Carrasco, S and Medina, V},
Title = {{Time-frequency analysis of the heart rate variability during the
   Valsalva manoeuvre}},
Journal = {{JOURNAL OF MEDICAL ENGINEERING \& TECHNOLOGY}},
Year = {{2000}},
Volume = {{24}},
Number = {{2}},
Pages = {{73-82}},
Month = {{MAR-APR}},
Abstract = {{We present an analysis of the heart rate variability during the Valsalva
   manoeuvre. The more frequently used time-frequency distributions were
   applied in order to analyse the dynamical behaviour of several spectral
   indexes during the manoeuvre. The influence of the branches of the
   autonomous system can be predicted following the evolution of the
   studied indexes. The exponential time-frequency distribution showed the
   best results in the graphical representation, as well as in the indexes
   calculation. The total power, the low-to-high frequency ratio and the
   fractal dimension were analysed throughout the different phases of the
   manoeuvre and a representative model of these parameters evolution was
   proposed.}},
