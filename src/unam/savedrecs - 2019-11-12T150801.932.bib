Author = {Salamanca, MAO and Ruvalcaba-Sil, JL and Bucio, L and Manzanilla, L and
   Miranda, J},
Title = {{Ion beam analysis of pottery from Teotihuacan, Mexico}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
