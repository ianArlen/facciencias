@article{ ISI:000086636300011,
Author = {De la Lanza-Espino, G and Garcia-Calderon, JL and Pulido, SH and Diaz,
   PR},
Title = {{Employment of halophylous compost in coastal aquaculture and its
   relation to water quality.}},
Journal = {{ENVIRONMENTAL TECHNOLOGY}},
Year = {{2000}},
Volume = {{21}},
Number = {{4}},
Pages = {{467-473}},
Month = {{APR}},
Abstract = {{Detritus and nutrients derived from compost (NH4+ PO43-, COD) were
   obtained experimentally after a 90 day decomposition pretreatment of
   coastal halophytes (Sesuvium sp, Cressa sp. Suaeda sp. and Salicornia
   sp.). The detritus obtained was added as fertilizer to 15 cages placed
   in Caimanero Lagoon for 35 days. During the first 10 days in situ
   orthophosphates were recorded at levels of 7.3 to 2.0 mu g-at l(-1), and
   ammonium varied from 7.3 to 4.9 mu g-at l(,)(-1) values that are
   considered adequate for primary producers and the quality of the water.
   The availability of nutrients showed that the use of this compost, added
   to the shrimp culture cages with a surface area of 4 m(2) and a height
   of 1.2 m, at a rate of one kilogram of halophytes per 200 liters of
   water, did not modify the content of nutrients in the water inside and
   outside of the cages, The halophylous detritus (quantified by BOD and
   COD, representative of the easily available organic carbon load), went
   through three phases: first, abatement phase during in the first four
   days; second, up to 10 days, and third, up to 35 days.}},
