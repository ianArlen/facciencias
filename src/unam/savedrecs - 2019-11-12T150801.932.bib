@article{ ISI:000088755300008,
Author = {Palacios-Vargas, JG and Mendoza, S and Villalobos, FJ},
Title = {{New genus and species of hypogastruridae (collembola) from a Mexican
   biosphere reserve and remarks on its ecology}},
Journal = {{SOUTHWESTERN ENTOMOLOGIST}},
Year = {{2000}},
Volume = {{25}},
Number = {{2}},
Pages = {{139-144}},
Month = {{JUN}},
Abstract = {{A new genus close to Hypogastrura and its type species from the ``El
   Cielo{''} Biosphere Reserve, Tamaulipas, are described and illustrated.
   The proposed genus, Celegastrura, lacks the postantennal organ, which is
   present in all other members of the Hypogastruridae close to
   Hypogastrura. There are other genera in the family which lack the
   postantennal organ, but these have less than 8 + 8 eyes. The type
   species was associated with soil from the fermentation zone (A horizon)
   in early and intermediate successional stages of secondary vegetation in
   a slash burning agricultural system.}},
