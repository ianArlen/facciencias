@article{ ISI:000085868600028,
Author = {Bravo, S and Gonzalez-Esparza, JA},
Title = {{The north-south asymmetry of the solar and heliospheric magnetic field
   during activity minima}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2000}},
Volume = {{27}},
Number = {{6}},
Pages = {{847-849}},
Month = {{MAR 15}},
Abstract = {{Recent in situ spacecraft observations and the analysis of the galactic
   cosmic ray intensity at Earth have shown that the heliospheric magnetic
   field exhibited a north-south asymmetry and that the heliospheric
   current sheet was inclined with respect to the solar equator during the
   years around the last sunspot minimum (1995-1996). In this paper we show
   that both heliospheric characteristics are due to the particular way the
   solar magnetic field evolves. The north-south asymmetry can be explained
   with the evolution of the quadrupolar component of the field in the
   solar magnetosphere and the inclination of the heliospheric current
   sheet could be a result of the presence of an inclined relic dipolar
   field in the Sun, as has been suggested by Bravo and Stewart {[}1995].}},
