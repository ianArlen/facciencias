@article{ ISI:000085203500003,
Author = {Verma, SP},
Title = {{Geochemical evidence for a lithospheric source for magmas from Los
   Humeros caldera, Puebla, Mexico}},
Journal = {{CHEMICAL GEOLOGY}},
