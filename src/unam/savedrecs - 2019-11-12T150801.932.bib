@article{ ISI:000088237800002,
Author = {Canto, EAA and Balderas, RC and Padilla, EH and Bonilla, LC},
Title = {{The influence of generational factors on the spoken vocabulary of
   Mexican schoolchildren}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2000}},
Volume = {{17}},
Number = {{1}},
Pages = {{19-27}},
Month = {{JUN}},
Abstract = {{A comparison was made of spoken vocabulary of 959 Mexican school
   children from two social classes in two samples taken ten years apart.
   The children were tape-recorded in semi-structured situations of
   spontaneous interactions in groups of six. The types and tokens of the
   vocabulary and of nine grammatical categories produced were analyzed.
   The words common to different samples are the so-called ``function
   words{''} and that the differences lie in the ``content words{''}. Upper
   class children from the contemporary sample had larger vocabularies than
   the earlier sample, the reverse being true for the children from the
   lower social class. Social Class showed a consistent influence within
   both samples: the means of the higher social class were greater than
   those of the lower social class. It was inferred that the existence of
   environmental stimulation affects the production of a child's oral
   language and that linguistic competence is polarized because of
   differences of environment (school and family) over time.}},
