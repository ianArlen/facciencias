@article{ ISI:000086106200001,
Author = {Ortega-Ortiz, JG and Villa-Ramirez, B},
Title = {{Polydactyly and other features of the manus of the vaquita, Phocoena
   sinus}},
Journal = {{MARINE MAMMAL SCIENCE}},
Year = {{2000}},
Volume = {{16}},
Number = {{2}},
Pages = {{277-286}},
Month = {{APR}},
Abstract = {{The vaquita (Phocoena sinus) manus was examined in 16 individuals. The
   carpus has a proximal row of three bones and a cartilaginous accessory
   carpal element distocaudal to the ulna. Five metacarpal bones are
   present. A process in distocaudal aspect of Metacarpal III and an
   additional digital ray were present in all individuals examined. The
   number of phalanges in the additional digital ray varied among
   individuals and, sometimes, between the left and the right flipper of
   the same individual. The suggested phalangeal formula is I-1, II-7-8,
   III-6-7, IV-0-3, V-3-4, VI-1. The presence of this particular form of
   polydactyly may be the result of genetic drift in the small vaquita
   population.}},
