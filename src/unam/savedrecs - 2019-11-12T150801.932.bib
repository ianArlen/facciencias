
@article{ ISI:000087787600021,
Author = {Sanchez-Mendoza, A and Lopez-Sanchez, P and Vazquez-Cruz, B and Rios, A
   and Martinez-Ayala, S and Escalante, B},
Title = {{Angiotensin II modulates ion transport in rat proximal tubules through
   CYP metabolites}},
