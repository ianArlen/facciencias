@article{ ISI:000086487300013,
Author = {Melendez, J and Maldonado, V and Bingle, CD and Selman, M and Pardo, A},
Title = {{Cloning and expression of guinea pig TIMP-2. Expression in normal and
   hyperoxic lung injury}},
Journal = {{AMERICAN JOURNAL OF PHYSIOLOGY-LUNG CELLULAR AND MOLECULAR PHYSIOLOGY}},
Year = {{2000}},
Volume = {{278}},
