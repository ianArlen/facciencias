@article{ ISI:000086952900032,
Author = {Picos-Vega, A and Zelaya-Angel, O and Ramirez-Bon, R},
Title = {{Time-delayed charge-injection effects on photocurrent shape in a double
   layer}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2000}},
Volume = {{61}},
Number = {{18}},
Pages = {{12045-12049}},
Month = {{MAY 1}},
Abstract = {{Transient photocurrents of double-layer structures were simulated by a
   random walk through a cubic lattice in this work. The canonical
   technique of time of flight was simulated to find the characteristics of
   the photocurrents shape when each layer had a different mobility. The
   results reproduce the main features of the experimental data from
   similar real experiments reported in the literature. Also, the origin of
   the peak shape of photocurrents is discussed and it is shown that it is
   related to a stochastic process which forces the out-of-equilibrium
   charge carriers to be injected from one layer to the other within a
   period of time (t,t+dt) as given by a time-dependent probability
   phi(t)dt.}},
