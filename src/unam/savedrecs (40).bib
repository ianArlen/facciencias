@article{ ISI:000359035300008,
Author = {Hernandez de la Torre, Benigno and Gaxiola Castro, Gilberto and Alvarez
   Borrego, Saul and Gallegos Garcia, Artemio and Aguirre Gomez, Raul},
Title = {{New organic carbon in front of the Baja California Peninsula: time
   series and climatology}},
Journal = {{HIDROBIOLOGICA}},
Year = {{2015}},
Volume = {{25}},
Number = {{1}},
Pages = {{74-85}},
Month = {{APR}},
Abstract = {{Time series of new phytoplankton production (Pnew) off Baja California
   from 1970 to 2008 computed using statistical models of
   temperature-nitrate relationship in ten groups of coastal and oceanic
   stations (Lines 90, 107, 120, 137 and 157) of the CaICOFI-IMECOCAL
   network, are presented. Pnew was calculated from the f-ratio and total
   primary production (Ptotal). Spatially, the most productive areas of the
   region were Lines 90, 120 and 157. Pnew of lines 107 and 137 had
   relatively low values. These two lines marked the border between the
   northern and southern areas of the studied region. Seasonally, Pnew was
   high in the spring-summer transition, decreased in summer and autumn,
   and increased in winter. Pnew was related to coastal upwelling index,
   and we conclude that for an index of less than 200 m3/s/100 m, Pnew
   increases and its maximum occurs in phase with the coastal upwelling
   index. For an upwelling index greater than 200 m3/s/100 m Pnew decreases
   and both series are out of phase.}},
