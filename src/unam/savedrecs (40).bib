@article{ ISI:000353538400001,
Author = {Rodriguez-Romero, Elizabeth and Antonio Suarez-Cuenca, Juan and Ivan
   Elizalde-Barrera, Cesar and Mondragon-Teran, Paul and Enrique
   Martinez-Hernandez, Jose and Gomez-Cortes, Eduardo and Perez-Cabeza de
   Vaca, Rebeca and Hernandez-Munoz, Rolando E. and Melchor-Lopez, Alberto
   and Gabriela Jimenez-Saab, Nayeli},
Title = {{Low Serum Levels of Alpha 1 Anti-trypsin (alpha(1)-AT) and Risk of
   Airflow Obstruction in Non-Primary alpha(1)-AT-Deficient Patients with
   Compensated Chronic Liver Disease}},
Journal = {{MEDICAL SCIENCE MONITOR}},
Year = {{2015}},
Volume = {{21}},
Pages = {{1194-1199}},
Month = {{APR 27}},
Abstract = {{Background: Alpha(1) anti-trypsin (alpha(1)-AT), a serine protease
   inhibitor synthesized in the liver, is a major circulating antiprotease
   that provides defense against proteolytic damage in several tissues. Its
   deficiency is associated with airflow obstruction. The present study
   aimed to explore the role of alpha(1)-AT as a biomarker of airflow
   performance in chronic liver disease (CLD).
   Material/Methods: Serum alpha(1)-AT levels and lung function
   (spirometry) were evaluated in non-primary alpha(1)-AT-deficient,
   alcoholic CLD patients without evident respiratory limitations.
   Results: Thirty-four patients with airflow obstruction (n=11), airflow
   restriction (n=12), and normal airflow (n=11, age-matched controls) were
   eligible. a1-AT was decreased in the airflow obstruction group.
   ROC-cutoff alpha(1)-AT=24 mg/dL effectively discriminated airflow
   obstruction (AUC=0.687) and was associated with a 10-fold higher risk
   (p=0.0007).
   Conclusions: Lower alpha(1)-AT increased the risk of airflow obstruction
   in CLD patients without primary alpha(1)-AT deficiency.}},
