Year = {{2015}},
Volume = {{38}},
Number = {{1-2}},
Pages = {{1-8}},
Month = {{MAR}},
Abstract = {{Six new diorganotin (IV) complexes, {[}Ph2Sn(Pr-ACDA)(2)] 1,
   {[}Bu2Sn(Pr-ACDA)(2)](2), {[}Ph2Sn(Bz-ACDA)(2)](3),
   {[}Bu2Sn(Bz-ACDA)(2)](4), {[}Me2Sn(Bz-ACDA)(2)](5),
