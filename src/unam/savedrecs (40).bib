ISSN = {{0016-7169}},
Keywords = {{Citlaltepetl; glacier; cartography; photogrammetry; ortho-photos;
   geographic correction}},
Keywords-Plus = {{JAMAPA GLACIER; IZTACCIHUATL; PICO}},
