@article{ ISI:000352202300014,
Author = {Bartolo, Ramon and Merchant, Hugo},
Title = {{beta Oscillations Are Linked to the Initiation of Sensory-Cued Movement
   Sequences and the Internal Guidance of Regular Tapping in the Monkey}},
