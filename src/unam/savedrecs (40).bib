@article{ ISI:000351755200034,
Author = {Aguilar-Ovando, E. and Negron-Mendoza, A.},
Title = {{Stability of alanine in a high radiation field, adsorbed onto solid
   surfaces}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2015}},
Volume = {{304}},
Number = {{1}},
Pages = {{213-217}},
Month = {{APR}},
Abstract = {{The aim of this work was to study the stability of alanine in a high
   radiation field, in the presence of a mineral surface, as a possible
   phase for chemical evolution processes that might have occurred on the
   primitive Earth or in extraterrestrial environments. For these purposes,
   the behavior of D-, L- and DL-alanine adsorbed onto solid surfaces
   (olivine, Allende meteorite and a clay mineral) under irradiation was
   studied. The results showed that alanine was adsorbed onto the surfaces
   as a function of pH, and its yield of decomposition by gamma radiation
   in mineral suspension was lower than that obtained by irradiating the
   system without the solid surface.}},
