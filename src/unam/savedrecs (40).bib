@article{ ISI:000351858000006,
Author = {Rowlett, Julie and Suarez-Serrato, Pablo and Tapie, Samuel},
Title = {{DYNAMICS AND ZETA FUNCTIONS ON CONFORMALLY COMPACT MANIFOLDS}},
