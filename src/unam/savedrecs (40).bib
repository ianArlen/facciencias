@article{ ISI:000360074200018,
Author = {Gallegos Jimenez, Oswaldo and Lopez Lopez, Alvaro},
Title = {{Time-space perspective of sex and tourism in the modern and contemporary
   society}},
Journal = {{PASOS-REVISTA DE TURISMO Y PATRIMONIO CULTURAL}},
Year = {{2015}},
Volume = {{13}},
Number = {{3}},
Pages = {{709-726}},
Month = {{APR}},
Abstract = {{Based on a theoretical exploration, this article focuses on the
   space-time transformation of the sex-tourism link, from understanding
   their evolution on moral, socio-economic and territorial environment,
   until reaching an actual perspective, where tourism is seen as an
   economic activity embedded in the post-modernity. In which the involved
   tourist services tend to specialize/atomize, resulting on a sex supply
   more open and increasing, acting as one more attraction, articulated in
   a complex relations system within its dynamics. This article is divided
   in two sections; the first one recognizes the stages in nexus:
   sex-tourism while the second explores the different theoretical
   positions of this link, from which this paper proposes an interpretation
   of sexual tourism.}},
