@article{ ISI:000362916400003,
Author = {Clavijo Cortes, Pedro Hugo and Ros Bosch, Jaime},
Title = {{The Law of Thirlwall: a critical reading}},
Journal = {{INVESTIGACION ECONOMICA}},
