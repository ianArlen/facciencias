@article{ ISI:000348488700019,
Author = {Duran, Antonio J. and de la Iglesia, Manuel D.},
Title = {{Constructing Krall-Hahn orthogonal polynomials}},
Journal = {{JOURNAL OF MATHEMATICAL ANALYSIS AND APPLICATIONS}},
Year = {{2015}},
Volume = {{424}},
Number = {{1}},
Pages = {{361-384}},
Month = {{APR 1}},
Abstract = {{Given a sequence of polynomials (p(n))(n), an algebra of operators A
   that acts in the linear space of polynomials and an operator D-p is an
   element of A with D-p(p(n)) = theta P-n(n), where theta(n) is any
   arbitrary eigenvalue, we construct a new sequence of polynomials
   (q(n))(n) by considering a linear combination of m + 1 consecutive p(n):
   q(n) = p(n) + Sigma(m)(j=1)beta(n,j)p(n-j). Using the concept of a
   V-operator, we determine the structure of the sequences beta(n,j), j =
   1,...,m, such that the polynomials (q(n))(n). are eigenfUnctions of an
   operator in the algebra A. As an application, from the classical
   discrete family of Hahn polynomials, we construct orthogonal polynomials
   (q(n))(n) that are also eigenfunctions of higher-order difference
   operators. (C) 2014 Elsevier Inc. All rights reserved.}},
