@article{ ISI:000354259700002,
Author = {Torres Salcido, Gerardo and Pensado Leglise, Mario del Roble and
   Smolski, Andrew},
Title = {{Food distribution's socio-economic relationships and public policy:
   Mexico City's municipal public markets}},
Journal = {{DEVELOPMENT IN PRACTICE}},
Year = {{2015}},
Volume = {{25}},
Number = {{3}},
Pages = {{293-305}},
Month = {{APR 3}},
Abstract = {{Traditional food supply systems, like municipal public markets (MPM),
   are in crisis. Nevertheless, MPMs continue to demonstrate importance in
   the lives of the cities. In this article we discuss the case of Mexico
   City and the importance of the public markets for its neighbourhoods. We
   present the results of two research projects, completed in Mexico City
   at two different historical times and interpreted longitudinally. The
   results demonstrate the importance of socio-economic relationships for
   MPM's survival and potential. The article concludes with public policy
   recommendations to permit conservation, given the MPM's importance for
   the city's social cohesion.}},
