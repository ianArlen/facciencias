@article{ ISI:000350765400002,
Author = {Ezewudo, Matthew N. and Joseph, Sandeep J. and Castillo-Ramirez,
   Santiago and Dean, Deborah and del Rio, Carlos and Didelot, Xavier and
   Dillon, Jo-Anne and Selden, Richard F. and Shafer, William M. and
   Turingan, Rosemary S. and Unemo, Magnus and Read, Timothy D.},
Title = {{Population structure of Neisseria gonorrhoeae based on whole genome data
   and its relationship with antibiotic resistance}},
Journal = {{PEERJ}},
Year = {{2015}},
Volume = {{3}},
Month = {{MAR 5}},
Abstract = {{Neisseria gonorrhoeae is the causative agent of gonorrhea, a sexually
   transmitted infection (STI) of major importance. As a result of
   antibiotic resistance, there are now limited options for treating
   patients. We collected draft genome sequence data and associated
   metadata data on 76 N. gonorrhoeae strains from around the globe and
   searched for known determinants of antibiotics resistance within the
   strains. The population structure and evolutionary forces within the
   pathogen population were analyzed. Our results indicated a cosmopolitan
   gonoccocal population mainly made up of five subgroups. The estimated
   ratio of recombination to mutation (r/m = 2.2) from our data set
   indicates an appreciable level of recombination occurring in the
   population. Strains with resistance phenotypes to more recent
   antibiotics (azithromycin and cefixime) were mostly found in two of the
   five population subgroups.}},
