@article{ ISI:000433465800003,
Author = {Saigado, Eneida Desiree and Hualde, Alejandro Perez},
Title = {{The internal democracy of political parties as premise of democratic
   authenticity}},
Journal = {{A\&C-REVISTA DE DIREITO ADMINISTRATIVO \& CONSTITUCIONAL}},
Year = {{2015}},
Volume = {{15}},
Number = {{60}},
Pages = {{63-83}},
Month = {{APR-JUN}},
Abstract = {{The frequency of terms in a republican democracy loses its meaning when
   there is no alternation of political representatives. Alternation, in
   addition to periodicity of elections, is an essential element of
   democratic authenticity and this logic also applies to the operation of
   political parties. Consecrated as essential elements of representative
   democracy in the Argentine and Brazilian Constitutions, the parties
   failed to incorporate the democratic demands and there is no concern
   over the respect for the fundamental rights of its members. The
   political instability which Brazil and Argentina experience shows the
   low density of democratic culture and the party practices do not
   contribute to change this scenario.}},
