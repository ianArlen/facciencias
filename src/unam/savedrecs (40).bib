@article{ ISI:000351765900029,
Author = {Brzobohaty, Oto and Arzola, Alejandro V. and Siler, Martin and Chvatal,
   Lukas and Jakl, Petr and Simpson, Stephen and Zemanek, Pavel},
Title = {{Complex rotational dynamics of multiple spheroidal particles in a
   circularly polarized, dual beam trap}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{6}},
Pages = {{7273-7287}},
Month = {{MAR 23}},
Abstract = {{We examine the rotational dynamics of spheroidal particles in an optical
   trap comprising counter-propagating Gaussian beams of opposing helicity.
   Isolated spheroids undergo continuous rotation with frequencies
   determined by their size and aspect ratio, whilst pairs of spheroids
   display phase locking behaviour. The introduction of additional
   particles leads to yet more complex behaviour. Experimental results are
   supported by numerical calculations. (C) 2015 Optical Society of America}},
