@article{ ISI:000353241300007,
Author = {Achilleos, Nicholas and Andre, Nicolas and Blanco-Cano, Xochitl and
   Brandt, Pontus C. and Delamere, Peter A. and Winglee, Robert},
Title = {{1. Transport of Mass, Momentum and Energy in Planetary Magnetodisc
   Regions}},
Journal = {{SPACE SCIENCE REVIEWS}},
