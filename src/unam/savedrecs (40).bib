@article{ ISI:000350087100044,
Author = {Mejia-Uriarte, E. V. and Kolokoltsev, O. and Montesinos, M. Navarrete
   and Camarillo, E. and Hernandez A, J. and Murrieta, H.},
Title = {{Study of NaCl:Mn2+ nanostructures in the Suzuki phase by optical
   spectroscopy and atomic force microscopy}},
Journal = {{JOURNAL OF LUMINESCENCE}},
