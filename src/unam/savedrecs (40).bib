@article{ ISI:000349613300002,
Author = {Duarte Alaniz, Victor and Rocha-Rinza, Tomas and Cuevas, Gabriel},
Title = {{Assessment of Hydrophobic Interactions and Their Contributions Through
   the Analysis of the Methane Dimer}},
Journal = {{JOURNAL OF COMPUTATIONAL CHEMISTRY}},
