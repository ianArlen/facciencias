@article{ ISI:000352150600003,
Author = {Contreras Campos, Maria Elena and Rodriguez-Cervantes, Nora and
   Reza-Lopez, Sandra and Avila-Esparza, Marina and Virginia Chavez-Corral,
   Dora and Levario-Carrillo, Margarita},
Title = {{Body composition and newborn birthweight in pregnancies of adolescent
   and mature women}},
Journal = {{MATERNAL AND CHILD NUTRITION}},
