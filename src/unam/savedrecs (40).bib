@article{ ISI:000352521100002,
Author = {Aceves, H. and Tamayo, F. J. and Altamirano-Devora, L. and Ramon-Fox, F.
   G. and Canas, R. and Reyes-Ruiz, M.},
Title = {{INTRAGROUP DARK MATTER DISTRIBUTION IN SMALL GROUPS OF HALOS IN A ACDM
   COSMOLOGY}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2015}},
Volume = {{51}},
Number = {{1}},
Pages = {{13-25}},
Month = {{APR}},
Abstract = {{We study the distribution of intragroup dark matter in small groups of
   dark matter galaxy size halos in a ACDM cosmology. These groups are
   identified using a physical criterion and may be an appropriate
   representation of small galaxy groups. We quantify the amount of
   intra-group dark matter and characterize its distribution. We find that
   compact associations of halos, as well as intermediate and loose groups,
   have rather flat intragroup dark matter profiles, with logarithmic
   slopes of gamma approximate to 0 and approximate to -0.2, respectively.
   Hence, the intra-group dark matter of these halo systems does not follow
   the same cuspy tendency of galactic halos. In intermediate and loose
   associations of galaxy-size halos, the intragroup matter tends to be
   less than or similar to 50\% of the total mass of the group, while in
   compact associations it is less than or similar to 20\% of the mass
   within their group radius.}},
