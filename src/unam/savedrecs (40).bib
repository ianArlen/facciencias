@article{ ISI:000353832500037,
Author = {Pizio, O. and Rzysko, W. and Sokolowski, S. and Sokolowska, Z.},
Title = {{Mixtures of ions and amphiphilic molecules in slit-like pores: A density
   functional approach}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
