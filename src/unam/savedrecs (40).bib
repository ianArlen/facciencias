@article{ ISI:000351163200006,
Author = {Okon, Elias and Sudarsky, Daniel},
Title = {{The Black Hole Information Paradox and the Collapse of the Wave Function}},
Journal = {{FOUNDATIONS OF PHYSICS}},
Year = {{2015}},
Volume = {{45}},
Number = {{4}},
Pages = {{461-470}},
Month = {{APR}},
Abstract = {{The black hole information paradox arises from an apparent conflict
   between the Hawking black hole radiation and the fact that time
   evolution in quantum mechanics is unitary. The trouble is that while the
   former suggests that information of a system falling into a black hole
   disappears, the latter implies that information must be conserved. In
   this work we discuss the current divergence in views regarding the
   paradox, we evaluate the role that objective collapse theories could
   play in its resolution and we propose a link between spontaneous
   collapse events and microscopic virtual black holes.}},
