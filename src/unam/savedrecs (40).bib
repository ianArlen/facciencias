@article{ ISI:000358701900013,
Author = {Lopez Castro, G. and Roig, P. and Toledo Sanchez, G.},
Title = {{Low's soft photon corrections to dipion tau decays and their impact on
   alpha(mu)}},
Journal = {{NUCLEAR AND PARTICLE PHYSICS PROCEEDINGS}},
Year = {{2015}},
Volume = {{260}},
Pages = {{70-74}},
Month = {{MAR}},
Note = {{13th International Workshop on Tau Lepton Physics (Tau), RWTH Aachen
   Univ, Aachen, GERMANY, SEP 15-19, 2014}},
Abstract = {{The O(alpha) real-photon corrections to tau(-) --> pi(-)pi(0)nu(tau),
   decays are revisited and updated. We show that two existing calculations
   of the corresponding radiative amplitude up to O(k(0)) (k, the photon
   momentum), contribute to radiative corrections with almost identical
   numerical results for the di-pion invariant-mass spectrum. The pion form
   factor required to compute the corrections includes three isovector
   resonances, which allows to extend the range of validity of radiative
   corrections in the whole range of momentum transfer accessible to tau
   lepton decays. An estimate of the corresponding isospin breaking
   correction to be applied to tau decay data used to predict the leading
   hadronic contribution to the muon anomalous magnetic moment is provided.}},
