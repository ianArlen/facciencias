@article{ ISI:000352924600003,
Author = {Aire, Carlos and Carmona, Sergio and Aguado, Antonio and Molins, Climent},
Title = {{Double-Punch Test of Fiber-Reinforced Concrete: Effect of Specimen
   Origin and Size}},
Journal = {{ACI MATERIALS JOURNAL}},
Year = {{2015}},
Volume = {{112}},
Number = {{2}},
Pages = {{199-208}},
Month = {{MAR-APR}},
Abstract = {{One of the main advantages of the Barcelona test is that the properties
   of fiber-reinforced concrete (FRC) can be determined by using
   cylindrical specimens, thereby permitting the use of cores to control
   the quality of concrete used in construction.
   Using the results of an extensive experimental campaign, this paper
   establishes the relationship between the unitary load of cracking,
   residual strength and toughness of molded specimens, and cores of
   concretes reinforced with different types of fibers but with the same
   aspect ratio. The fiber diameter is observed to signifidantly influence
   the properties of molded specimens, whereas in the cores, the number of
   fibers that lose anchoring due to drilling determines the response of
   the FRC.}},
Publisher = {{AMER CONCRETE INST}},
Address = {{38800 COUNTRY CLUB DR, FARMINGTON HILLS, MI 48331 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Aire, C (Reprint Author), Univ Nacl Autonoma Mexico, Inst Ingn, Div Mat, Mexico City 04510, DF, Mexico.
   Aire, Carlos, Univ Nacl Autonoma Mexico, Inst Ingn, Div Mat, Mexico City 04510, DF, Mexico.
