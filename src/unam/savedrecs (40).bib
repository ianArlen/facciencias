@article{ ISI:000370665400003,
Author = {Rubio Vega, Blanca},
Title = {{Crisis of Hegemony and Capitalist Transition in the World Agri-food
   Environment}},
Journal = {{ESPACIO ABIERTO}},
Year = {{2015}},
Volume = {{24}},
Number = {{2}},
Pages = {{235-254}},
Month = {{APR-JUN}},
Abstract = {{This paper aims to analyze the role played by basic foods in the
   struggle for world hegemony led by the United States, as a result of the
   hegemonic crisis that this country has been facing since the seventies.
   Through forms of domination, such as the devaluation of food prices
   during neoliberalism, or the financialization of commodities during the
   capitalist transition and the food crisis, this country has tried to
   overpower its rivals and maintain a privileged position in the global
   agri-food sector. The greatest impact of this strategy has been the
   devastation of agriculture in countries of the global South, among them
   the Latin American countries, to the detriment of farmers and the
   lower-income population. On the other hand, those who have benefited are
   the food corporations, which have increased their power on a commercial,
   financial and territorial level.}},
