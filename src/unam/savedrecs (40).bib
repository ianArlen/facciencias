@article{ ISI:000352707900004,
Author = {Huerta Aguilar, Carlos Alberto and Sharma, Hemant and Thangarasu,
   Pandiyan and Singh, Narinder},
Title = {{Nanomolar Detection of Iodide in Aqueous Medium Using Organic-Inorganic
   Hybrid Nanoparticles: Application in Urine Analysis}},
Journal = {{CHEMPLUSCHEM}},
