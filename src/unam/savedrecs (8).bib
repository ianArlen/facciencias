@article{ ISI:000427342500002,
Author = {Keshtkar, Sajjad and Moreno, Jaime A. and Kojima, Hirohisa and Uchiyama,
   Kenji and Nohmi, Masahiro and Takaya, Keisuke},
Title = {{Spherical gyroscopic moment stabilizer for attitude control of
   microsatellites}},
Journal = {{ACTA ASTRONAUTICA}},
Year = {{2018}},
Volume = {{143}},
Pages = {{9-15}},
Month = {{FEB}},
Abstract = {{This paper presents a new and improved concept of recently proposed
   two-degrees of freedom spherical stabilizer for triaxial orientation of
   microsatellites. The analytical analysis of the advantages of the
   proposed mechanism over the existing inertial attitude control devices
   are introduced. The extended equations of motion of the stabilizing
   satellite including the spherical gyroscope, for control law design and
   numerical simulations, are studied in detail. A new control algorithm
   based on continuous high-order sliding mode algorithms, for managing the
   torque produced by the stabilizer and therefore the attitude control of
   the satellite in the presence of perturbations/uncertainties, is
   presented. Some numerical simulations are carried out to prove the
   performance of the proposed mechanism and control laws.}},
