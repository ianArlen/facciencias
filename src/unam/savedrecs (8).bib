@article{ ISI:000419733500009,
Author = {Valencia-A, Susana and Coombes, Allen and Luis Villasenor, Jose},
Title = {{Quercus candicans (Fagaceae) is not a Quercus but a Roldana (Asteraceae)}},
Journal = {{PHYTOTAXA}},
