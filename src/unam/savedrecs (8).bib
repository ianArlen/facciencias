@article{ ISI:000425281800011,
Author = {Araiza-Olivera, D. and Feng, Y. and Semenova, G. and Prudnikova, T. Y.
   and Rhodes, J. and Chernoff, J.},
Title = {{Suppression of RAC1-driven malignant melanoma by group A PAK inhibitors}},
