@article{ ISI:000425058900001,
Author = {Gabriela Valencia-Resendiz, Diana and Palomino-Vizcaino, Giovanni and
   Virginia Tapia-Vieyra, Juana and Luisa Benitez-Hess, Maria and Gabriela
   Leija-Montoya, Ana and Marat Alvarez-Salas, Luis},
Title = {{Inhibition of Human Papillomavirus Type 16 Infection Using an RNA
   Aptamer}},
Journal = {{NUCLEIC ACID THERAPEUTICS}},
Year = {{2018}},
Volume = {{28}},
Number = {{2}},
Pages = {{97-105}},
Month = {{APR}},
Abstract = {{Human papillomavirus type 16 (HPV16) DNA has been found in approximate
   to 50\% of cervical tumors worldwide. HPV infection starts with the
   binding of the virus capsid to heparan sulfate (HS) receptors exposed on
   the surface of epithelial basal layer keratinocytes. Previously, our
   group isolated a high-affinity RNA aptamer (Sc5c3) specific for HPV16 L1
   virus-like particles (VLPs). In this study, we report the inhibition of
   HPV16 infection by Sc5c3 in a pseudovirus (PsVs) model. 293TT cells were
   infected by HPV16 PsVs containing the yellow fluorescent protein (YFP)
   as reporter gene. Incubation of HPV16 PsVs with Sc5c3 before infection
   resulted in a dose-dependent decrease in YFP fluorescence, suggesting
   infection inhibition. Aptamer degradation by RNase A restored PsVs
   infectivity, supporting the previous observation that Sc5c3 aptamer can
   inhibit infection. VLP mutants with removed HS binding sites were used
   in binding assays to elucidate the Sc5c3 blocking mechanism; however, no
   binding difference was observed between wild-type and mutant VLPs,
   suggesting that pseudoinfection inhibition relies on mechanisms
   additional to electrostatic HS binding site interaction. A DNA/RNA Sc5c3
   version also inhibited HPV PsVs infection, suggesting that a modified,
   nuclease-resistant Sc5c3 may be used to inhibit HPV16 infection in vivo.}},
