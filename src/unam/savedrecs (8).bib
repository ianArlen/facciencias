@article{ ISI:000427866000001,
Author = {Gonzalez-Gutierrez, C. A. and Solis-Valles, D. and Rodriguez-Lara, B. M.},
Title = {{Microscopic approach to field dissipation in the Jaynes-Cummings model}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2018}},
Volume = {{51}},
Number = {{1}},
Month = {{JAN 5}},
Abstract = {{We use the microscopic derivation of the Jaynes-Cummings model master
   equation under field losses to study the dynamics of initial field-qubit
   states in the single-excitation manifold and beyond. We show that
   field-qubit detuning, as well as finite temperature, modify the
   effective decay rate of the system using measures like field-qubit
   purity and concurrence for the case of one-excitation, and for initial
   Fock states for more excitations. For initial semiclassical states of
   the field, we show that the microscopic approach, in phase space,
   provides an evolution to thermal equilibrium that is smoother than the
