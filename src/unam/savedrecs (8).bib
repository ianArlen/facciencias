@incollection{ ISI:000471072400004,
Author = {Romo-Gonzalez, Tania and Gonzalez-Ochoa, Raquel and Leon-Diaz, Rosalba
   and Gutierrez-Ospina, Gabriel},
Editor = {{Marvan, ML and LopezVazquez, E}},
Title = {{Stress and Nutrition During Pregnancy: Factors Defining
   Transgenerational Future Health Within the Family}},
Booktitle = {{PREVENTING HEALTH AND ENVIRONMENTAL RISKS IN LATIN AMERICA}},
Series = {{Anthropocene-Politik Economics Society Science}},
Year = {{2018}},
Volume = {{23}},
Pages = {{15-30}},
Abstract = {{A normal pregnancy is defined as the physiological state of a woman from
   fertilization to the birth of one or more living, healthy and full-term
   infants. Meanwhile, the term high-risk pregnancy refers to anomalous
   gestational conditions (e.g. eclampsia), which increase the possibility
   of pathophysiological states in the mother and/or the infant. There are
   several biological, gynaeco-obstetric and social risk factors for a high
   risk pregnancy. Conditions such as malnutrition, negative emotions,
   perceived stress and maternal anxiety are predictors of pathological
   conditions in both mother and child (e.g. obesity, diabetes, different
   types of cancer and some neurological and cardiovascular pathology).
   This scenario requires a modification of the concept of `high-risk
   pregnancy', and advocates taking into account the monitoring of stress,
   anxiety, depression and food quality during pregnancy as potential risk
   factors not only for the baby, but also for subsequent generations.}},
