@article{ ISI:000417661200006,
Author = {de la Pena, Jose A.},
Title = {{On the trace of the Coxeter polynomial of an algebra}},
Journal = {{LINEAR ALGEBRA AND ITS APPLICATIONS}},
Year = {{2018}},
Volume = {{538}},
Pages = {{103-115}},
Month = {{FEB 1}},
Abstract = {{Let A = KQ/I be a finite dimensional triangular K-algebra. Let phi(A) be
   the Coxeter matrix of A. We relate homological conditions for A with
   properties of the traces of the Coxeter transformation phi(A). For
   instance, a finite dimensional accessible algebra A is strongly
   accessible if and only if Tr (phi(A)) = -1. We say A is of cyclotomic
   type if the eigenvalues of phi(A) lie on the unit circle. Clearly, if A
   is of cyclotomic type then vertical bar Tr (phi(A))(k)vertical bar <= n,
   for k >= 0.We prove that A is of cyclotomic type if vertical bar Tr
   (phi(A))(k)vertical bar <= n holds for 0 <= k <= n. We illustrate the
   results with examples of Nakayama algebras. (C) 2017 Elsevier Inc. All
   rights reserved.}},
