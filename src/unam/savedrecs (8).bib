@article{ ISI:000422716900097,
Author = {Quinto-Sanchez, Mirsha and Munoz-Munoz, Francesc and Gomez-Valdes, Jorge
   and Cintas, Celia and Navarro, Pablo and Silva de Cerqueira, Caio Cesar
   and Paschetta, Carolina and de Azevedo, Soledad and Ramallo, Virginia
   and Acuna-Alonzo, Victor and Adhikari, Kaustubh and Fuentes-Guajardo,
   Macarena and Hunemeier, Tabita and Everardo, Paola and de Avila,
   Francisco and Jaramillo, Claudia and Arias, Williams and Gallo, Carla
   and Poletti, Giovani and Bedoya, Gabriel and Bortolini, Maria Catira and
   Canizales-Quinteros, Samuel and Rothhammer, Francisco and Rosique,
   Javier and Ruiz-Linares, Andres and Gonzalez-Jose, Rolando},
Title = {{Developmental pathways inferred from modularity, morphological
   integration and fluctuating asymmetry patterns in the human face}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2018}},
Volume = {{8}},
Month = {{JAN 17}},
Abstract = {{Facial asymmetries are usually measured and interpreted as proxies to
   developmental noise. However, analyses focused on its developmental and
   genetic architecture are scarce. To advance on this topic, studies based
   on a comprehensive and simultaneous analysis of modularity,
   morphological integration and facial asymmetries including both
   phenotypic and genomic information are needed. Here we explore several
   modularity hypotheses on a sample of Latin American mestizos, in order
   to test if modularity and integration patterns differ across several
   genomic ancestry backgrounds. To do so, 4104 individuals were analyzed
   using 3D photogrammetry reconstructions and a set of 34 facial landmarks
   placed on each individual. We found a pattern of modularity and
   integration that is conserved across sub-samples differing in their
   genomic ancestry background. Specifically, a signal of modularity based
   on functional demands and organization of the face is regularly observed
   across the whole sample. Our results shed more light on previous
   evidence obtained from Genome Wide Association Studies}},
