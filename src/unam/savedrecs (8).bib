@article{ ISI:000423656700005,
Author = {Ortiz, Antonio and Vazquez, Omar},
Title = {{Energy density and path-length dependence of the fractional momentum
   loss in heavy-ion collisions at root S (N N) from 62.4 to 5020 GeV}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{2018}},
Volume = {{97}},
Number = {{1}},
Month = {{JAN 31}},
Abstract = {{In this work a study of the fractional momentum loss (S-loss) as a
   function of the characteristic path length (L) and the Bjorken energy
   density times the equilibration time (epsilon(Bj)tau(0)) for heavy-ion
   collisions at different root S-NN is presented. The study has been
   conducted using inclusive charged particles from intermediate to large
   transverse momentum (5 < p(T) < 20 GeV/c). Within uncertainties and for
   all the transverse momentum values which were explored, the fractional
   momentum loss is found to increase linearly with
   (epsilon(Bj)tau(0))(3/8) L. For identified hadrons, albeit a smaller
   slope of S-loss VS. (epsilon(Bj)tau(0))(3/8) L is observed for
   (anti)protons at p(T) = 5 GeV/c, S-loss is also found to grow linearly
   with L. The behavior of data could provide important information aimed
   at understanding the parton energy loss mechanism in heavy-ion
   collisions and some insight into the expected effect for small systems.}},
