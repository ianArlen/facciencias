@incollection{ ISI:000487272300009,
Author = {Zapata-Penasco, Icoquih and Bautista-Lopez, Santiago and Souza, Valeria},
Editor = {{GarciaOliva, F and Elser, J and Souza, V}},
Title = {{The Magnetotactic Bacteria of the Churince Lagoon at Cuatro Cienegas
   Basin}},
