@article{ ISI:000423202000006,
Author = {Pablo Diaz, Juan and Verjovsky, Alberto and Vlacci, Fabio},
Title = {{Quaternionic Kleinian modular groups and arithmetic hyperbolic orbifolds
   over the quaternions}},
Journal = {{GEOMETRIAE DEDICATA}},
Year = {{2018}},
Volume = {{192}},
Number = {{1}},
Pages = {{127-155}},
Month = {{FEB}},
Abstract = {{Using the rings of Lipschitz and Hurwitz integers H (Z) and Hur (Z) in
   the quaternion division algebra H, we define several Kleinian discrete
   subgroups of PSL(2, H). We define first a Kleinian subgroup PSL (2, )
   pound of PSL (2, H(Z)). This group is a generalization of the modular
   group PSL (2, Z). Next we define a discrete subgroup PSL (2, ) pound of
   PSL (2, H) which is obtained by using Hurwitz integers. It contains as a
   subgroup PSL (2, ) pound. In analogy with the classical modular case,
   these groups act properly and discontinuously on the hyperbolic
   quaternionic half space. We exhibit fundamental domains of the actions
   of these groups and determine the isotropy groups of the fixed points
   and describe the orbifold quotients H-H(1)/ PSL (2, ) pound and H-H(1)/
   PSL (2, ) pound which are quaternionic versions of the classical modular
   orbifold and they are of finite volume. Finally we give a thorough study
   of their descriptions by Lorentz transformations in the
   Lorentz-Minkowski model of hyperbolic 4-space.}},
