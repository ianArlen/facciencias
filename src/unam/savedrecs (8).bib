@article{ ISI:000418211600088,
Author = {Jain, Akhil and Gonzalez, Claudia A. E. and Tejeda, Elias M. and Duran,
   Alejandro and Contreras, Oscar E. and Hirata, Gustavo A.},
Title = {{Covering the optical spectrum through different rare-earth ion-doping of
   YAG nanospheres produced by rapid microwave synthesis}},
Journal = {{CERAMICS INTERNATIONAL}},
Year = {{2018}},
Volume = {{44}},
Number = {{2}},
Pages = {{1886-1893}},
Month = {{FEB 1}},
Abstract = {{In this work, YAG:RE3+ (RE = Pr3+, Tm3+, Tb3+, Ce3+, Eu3+, Nd3+, and
   Er3+;Yb3+) luminescent nanospheres with tunable photoluminescence (PL)
   emission were prepared by using fast and energy-saving
   microwave-assisted synthesis (MWAS). The nanopowders were post-annealed
   at 1100 degrees C for 3 h to obtain nanocrystalline phosphors. X-ray
   diffraction (XRD) analysis confirmed the presence of nanocrystals that
   were well indexed to the cubic YAG phase. Surface chemical groups were
   detected using Fourier transform infrared spectroscopy (FTIR).
   Transmission electron microscopy (TEM) showed that the synthesized
   samples consist of nanospheres with an average size of 110 nm. The
   effect of aluminum precursors on the morphology of synthesized
   nanoparticles is discussed. The produced nanophosphors showed strong
   emission at different wavelengths (ultraviolet, visible and
   near-infrared) corresponding to different RE3+ ions. Altogether, the
   merits of spherical morphology and PL emission at various wavelengths
   endow rare-earth-doped YAG nanoparticles with suitable characteristic
   for potential applications in the field of nanomedicine (e.g.,
   bioimaging and nanoscintillators), light display systems, optoelectronic
   devices, and lasers.}},
