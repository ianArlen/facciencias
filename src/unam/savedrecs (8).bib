Editor = {{Souza, V and OlmedoAlvarez, G and Eguiarte, LE}},
Title = {{Hyperdiverse Viral Communities in an Oligotrophic Oasis (Cuatro
   Cienegas): Marine Affinities and Microgeographic Differentiation}},
Booktitle = {{CUATRO CIENEGAS ECOLOGY, NATURAL HISTORY AND MICROBIOLOGY}},
