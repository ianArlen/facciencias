@article{ ISI:000423008400022,
Author = {Casarrubias-Segura, F. and Garcia-Ferreira, S. and Rojas-Hernandez, R.},
Title = {{Every Sigma(s)-product of K-analytic spaces has the Lindelof
   Sigma-property}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2018}},
Volume = {{234}},
Pages = {{285-297}},
Month = {{FEB 1}},
Abstract = {{Given compact spaces X and Y, if X is Eberlein compact and C-p,C-n (X)
   is homeomorphic to C-p,C-n (Y) for some natural n, then Y is also
   Eberlein compact; this result answers a question posed by Tkachuk.
   Assuming existence of a Souslin line, we give an example of a Corson
   compact space with a Lindelof subspace that fails to be Lindelof Sigma;
   this gives a consistent answer to another question of Tkachuk. We
   establish that every Sigma(s)-product of K-analytic spaces is Lindelof
   Sigma and Cp(X) is a Lindeltif Sigma-space for every Lindelof
   Sigma-space X contained in a Sigma(s)-product of real lines. We show
   that C-p(X) is Lindelof for each Lindelof Sigma-space X contained in a
   Sigma-product of real lines. We prove that Cp(X) has the Collins-Roscoe
   property for every dyadic compact space X and generalize a result of
   Tkachenko by showing, with a different method, that the inequality w(X)
   <= nw(X)(Nag(X)) holds for regular spaces. (C) 2017 Elsevier B.V. All
   rights reserved.}},
