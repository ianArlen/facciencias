@incollection{ ISI:000471072400014,
Author = {Oswald Spring, Ursula},
Editor = {{Marvan, ML and LopezVazquez, E}},
Title = {{Human, Gender and Environmental Security at Risk from Climate Change}},
Booktitle = {{PREVENTING HEALTH AND ENVIRONMENTAL RISKS IN LATIN AMERICA}},
