@article{ ISI:000398161500051,
Author = {Avila, Remy and Ascencio-Rodriguez, Joaquin and Tapia-Merino, Daniel and
   Rodriguez-Herrera, Oscar G. and Gonzalez-Suarez, Arturo},
Title = {{Optical concatenation of a large number of beads with a single-beam
   optical tweezer}},
Journal = {{OPTICS LETTERS}},
Year = {{2017}},
