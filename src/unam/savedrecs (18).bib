@article{ ISI:000404517800131,
Author = {Concepcion Lozada-Garcia, Maria and Enriquez, Raul G. and Ramirez-Apan,
   Teresa O. and Nieto-Camacho, Antonio and Francisco Palacios-Espinosa,
   Juan and Custodio-Galvan, Zeltzin and Soria-Arteche, Olivia and
   Perez-Villanueva, Jaime},
Title = {{Synthesis of Curcuminoids and Evaluation of Their Cytotoxic and
   Antioxidant Properties}},
Journal = {{MOLECULES}},
Year = {{2017}},
Volume = {{22}},
Number = {{4}},
Month = {{APR}},
Abstract = {{Curcumin (1) and ten derivatives (2-11) were synthesized and evaluated
   as cytotoxic and antioxidant agents. The results of primary screening by
   Sulforhodamine B assay against five human cancer cell lines (U-251 MG,
   glioblastoma; PC-3, human prostatic; HCT-15, human colorectal; K562,
   human chronic myelogenous leukemia; and SKLU-1, non-small cell lung
   cancer) allowed us to calculate the half maximal inhibitory
   concentration (IC50) values for the more active compounds against HCT-15
   and K562 cell lines. Compounds 2 and 10 were the most active against
   both cell lines and were more active than curcumin itself.
   Thiobarbituric acid reactive substances (TBARS) assay showed that 7 has
   potent activity; even stronger than curcumin, alpha-tocopherol, and
   quercetin.}},
