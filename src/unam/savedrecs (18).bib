@article{ ISI:000406354900007,
Author = {Aguirre-Bolanos, Manuel and Benitez-Flores, Jose C. and Gonzalez-Valle,
   Maria R. and Hernandez-Portilla, Luis B. and Quintanar-Zuniga, Rafael E.
   and Flores-Ortiz, Cesar M.},
Title = {{LONG TERM-STORAGE EFFECT ON VIABILITY AND FATTY ACIDS PROFILE OF
   Encyclia adenocarpa (Lex.) Schltr SEEDS}},
