@article{ ISI:000399762700004,
Author = {Stern, Jennifer C. and Sutter, Brad and Jackson, W. Andrew and
   Navarro-Gonzalez, Rafael and McKay, Christopher P. and Ming, Douglas W.
   and Archer, P. Douglas and Mahaffy, Paul R.},
Title = {{The nitrate/(per)chlorate relationship on Mars}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2017}},
Volume = {{44}},
Number = {{6}},
Pages = {{2643-2651}},
Month = {{MAR 28}},
Abstract = {{Nitrate was recently detected in Gale Crater sediments on Mars at
   abundances up to similar to 600mg/kg, confirming predictions of its
   presence at abundances consistent with models based on impact-generated
   nitrate and other sources of fixed nitrogen. Terrestrial Mars analogs,
   Mars meteorites, and other solar system materials help establish a
   context for interpreting in situ nitrate measurements on Mars,
   particularly in relation to other cooccuring salts. We compare the
   relative abundance of nitrates to oxychlorine (chlorate and/or
   perchlorate, hereafter (per)chlorate) salts on Mars and Earth. The
   nitrate/(per)chlorate ratio on Mars is <1, significantly lower than on
   Earth (nitrate/(per)chlorate>10(3)), suggesting not only the absence of
   biological activity but also different (per)chlorate formation
   mechanisms on Mars than on Earth.}},
