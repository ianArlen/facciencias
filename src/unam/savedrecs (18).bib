@article{ ISI:000404050100008,
Author = {Schulte, Joshua J. and Smith, Geoffrey R. and Lemos-Espinal, Julio A.},
Title = {{DIET OF JUVENILE LIZARDS, UMA EXSUL, FROM COAHUILA, MEXICO}},
Journal = {{SOUTHWESTERN NATURALIST}},
Year = {{2017}},
Volume = {{62}},
Number = {{1}},
Pages = {{69-71}},
Month = {{MAR}},
Abstract = {{We examined the diets of 43 juveniles of the lizard Uma exsul from
   Coahuila, Mexico. The diet was composed primarily of adult and larval
   beetles. The diets of two females collected at the same time and place
   as the juveniles included beetles as the most important item but
   hemipterans, ants, and other hymenopterans were also important. Our
