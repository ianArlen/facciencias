@article{ ISI:000398334000010,
Author = {Espiritu, Jose N. and Peimbert, Antonio and Delgado-Inglada, Gloria and
   Teresa Ruiz, Mara},
Title = {{VLT SPECTROSCOPIC ANALYSIS OF HH 202. IMPLICATIONS ON DUST DESTRUCTION
   AND THERMAL INHOMOGENEITIES}},
