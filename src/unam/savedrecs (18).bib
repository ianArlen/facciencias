@article{ ISI:000392687100006,
Author = {Flores, Gilberto},
Title = {{On the dynamic pull-in instability in a mass-spring model of
   electrostatically actuated MEMS devices}},
Journal = {{JOURNAL OF DIFFERENTIAL EQUATIONS}},
Year = {{2017}},
Volume = {{262}},
Number = {{6}},
Pages = {{3597-3609}},
Month = {{MAR 15}},
Abstract = {{In this work we study the mass-spring system
   <(x)double over dot> + alpha<(x) over dot> + x = -lambda/(1+x)(2) (1)
   which is a simplified model for an electrostatically actuated MEMS
   device. The static pull-in value is lambda{*} = 4/27, which corresponds
   to the largest value of lambda for which there exists at least one
   stationary solution. For lambda > lambda{*} there are no stationary
   solutions and x(t) achieves the value -1 in finite time: touchdown
   occurs. The maximal displacement achieved by a stationary solution,
   known as the pull-in distance, is equal to -1/3 in this model. Assuming
   that the motion starts from rest, we establish the existence of a
   dynamic pull-in value lambda(d){*}(alpha) is an element of (0,
   lambda{*}), defined for alpha is an element of {[}0, infinity), which is
   a threshold in the sense that x(t) approaches a stable stationary
   solution as t -> infinity for 0 < lambda < lambda(d){*}(alpha), while
   touchdown occurs for lambda > lambda(d){*}(alpha). This dynamic pull-in
   value is a continuous, strictly increasing function of a and lim(alpha
   ->infinity)lambda(d){*}(alpha) = lambda{*}. A similar result is obtained
   for initial conditions of the form x(0) is an element of (-1/3, 1), <(x)
   over dot>(0) = 0. (C) 2016 Elsevier Inc. All rights reserved.}},
