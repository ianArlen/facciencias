@article{ ISI:000399856700003,
Author = {Melendez, Josue and Palmas, Oscar},
Title = {{Hypersurfaces with constant higher order mean curvature in space forms}},
Journal = {{DIFFERENTIAL GEOMETRY AND ITS APPLICATIONS}},
Year = {{2017}},
Volume = {{51}},
Pages = {{15-32}},
Month = {{APR}},
Abstract = {{Let M-c(n+1), n >= 3, be a space form of constant sectional curvature c
   = 0,1, -1 and M a complete oriented hypersurface of M-c(n+1) having
   constant r-th mean curvature H-r for some 2 <= r <= n-1 and two
   principal curvatures of multiplicities (n-1) and 1. We suppose further
   that vertical bar H-r vertical bar > 0 for c = 0, vertical bar H-r
   vertical bar > 1 for c = -1 and being H-r any value for c = 1. We prove
   that the infimum and the supremum of the squared norm of the second
   fundamental form of M are attained, obtain sharp bounds for them and
   characterize those hypersurfaces where the bounds is attained. (C) 2016
   Elsevier B.V. All rights reserved.}},
