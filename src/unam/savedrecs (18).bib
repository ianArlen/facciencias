@article{ ISI:000397857300007,
Author = {Trejo-Solis, Cristina and Alvarez-Lemus, Mayra A. and Jimenez-Farfan,
   Dolores and Anaya-Rubio, Isabel and Lopez-Gonzalez, Rosendo and
   Palencia, Guadalupe and Frias-Marquez, Dora M. and Gonzalez-Garcia,
