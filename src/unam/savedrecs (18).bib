@article{ ISI:000395725400107,
Author = {Tomasini-Montenegro, C. and Santoyo-Castelazo, E. and Gujba, H. and
   Romero, R. J. and Santoyo, E.},
Title = {{Life cycle assessment of geothermal power generation technologies: An
   updated review}},
