@article{ ISI:000397554000001,
Author = {Martinez, Ana and Reina, Miguel},
Title = {{Copper or free radical scavenger?}},
Journal = {{COMPUTATIONAL AND THEORETICAL CHEMISTRY}},
Year = {{2017}},
Volume = {{1104}},
Pages = {{1-11}},
Month = {{MAR 15}},
Abstract = {{Free radicals are very reactive molecules that produce so-called
   oxidative stress. To prevent oxidative stress there are two types of
   molecules: primary and secondary antioxidants. The chelation of copper
   is an example of a secondary antioxidant process. Therefore, these
   secondary antioxidants can be named copper scavengers. The main purpose
   of this investigation is to compare the capacity of four molecules:
   silybin (SIL); astaxanthin (ASTA); the hormone melatonin (MELA) and one
   of the metabolites of MELA named cyclic 3-hydroxymelatonin (3OHM) to
   scavenge copper, instead of a free radical. In order to decide whether
   these molecules are predominantly free radical scavengers rather than
   copper scavengers, we studied their primary antioxidant capacity
   analyzing their interaction with center dot OH and two mechanisms:
   Radical Adduct Formation (RAF) and Hydrogen Atom Transfer (HAT). In
   order to study copper scavenger capacity, we investigated the formation
   of chelate complexes with Cu(II) by these four molecules and their
   interaction with Cu(I). In the presence of Cu ions and free radicals,
   SIL, ASTA, MELA and 3OHM preferentially react with center dot OH. These
   molecules are able to act as copper scavengers but not in the presence
   of center dot OH and therefore play a limited role as chelating agents.
   (C) 2017 Elsevier B.V. All rights reserved.}},
