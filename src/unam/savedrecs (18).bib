@article{ ISI:000398719300010,
Author = {Carballo-Villalobos, A. I. and Gonzalez-Trujano, M. E. and
   Alvarado-Vazquez, N. and Lopez-Munoz, F. J.},
Title = {{Pro-inflammatory cytokines involvement in the hesperidin
   antihyperalgesic effects at peripheral and central levels in a
   neuropathic pain model}},
Journal = {{INFLAMMOPHARMACOLOGY}},
Year = {{2017}},
Volume = {{25}},
Number = {{2}},
Pages = {{265-269}},
Month = {{APR}},
Abstract = {{Emerging evidence proposes a link between immune changes and pain, which
   is consistent with the inflammation theory and the increased incidence
   of neurodegenerative diseases. Flavonoids have long been used because of
   their anti-inflammatory potential activity and they are considered a
   promising alternative to alleviate neuropathic pain. The aim of this
   study was to investigate the antihyperalgesic effect of hesperidin and
   the presence of pro-inflammatory cytokines evaluated at peripheral and
   central levels in the chronic constriction injury as model of
   neuropathic pain in rats. Mechanical and thermal hyperalgesia were
   assessed in the aesthesiometer and plantar tests, respectively, as
   related to the presence of cytokines concentrations (TNF-alpha, IL-1
   beta and IL-6) in sciatic nerve and segments of the spinal cord after 15
   days chronic constriction injury model in rats receiving vehicle or
   hesperidin. Antihyperalgesic response of hesperidin (100 mg/kg) was
   associated to the presence of cytokines mainly at several sections of
   the spinal cord suggesting not only peripheral but also its involvement
   in central sensitization in the experimental neuropathic pain.}},
