@article{ ISI:000400398500008,
Author = {Goguitchaichvili, Avto and Morales, Juan and Aguayo Haro, Ramiro and
   Quiroz Castanon, Humberto and Robles Camacho, Jasinto},
Title = {{First evidence of complex dental practice about 1300 BP in Mesoamerica
   revealed by absolute geomagnetic intensity}},
Journal = {{STUDIA GEOPHYSICA ET GEODAETICA}},
Year = {{2017}},
Volume = {{61}},
Number = {{2}},
Pages = {{310-317}},
Month = {{APR}},
Abstract = {{Deliberated dental modifications and mutilations are known since
   prehistoric times. The Mayas and other ancient cultures in Mesoamerica
   were considered pioneers of such practice. The evidences of therapeutic
   intervention, however, are still poorly documented and credible examples
   are extremely scarce. One of the burials excavated at the locality of La
   Mina (Michoacan, Mexico) corresponds to a 30-35 year old male individual
   named Alvaro, who presented a well-marked dental deformation. Moreover,
   Alvaro had a quite deep and symmetrical perforation in the upper right
   canine-a treatment similar to the procedure of endodontics or root
   canal. The archaeological context of Alvaro's habitat was dated using
   the archaeomagnetic method applied to pottery samples unambiguously
   correlated to the burial. The analysis supplied a time interval between
   647 and 825 AD suggesting the first evidence of complex dental practice
   in the Americas.}},
