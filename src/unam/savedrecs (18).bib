@article{ ISI:000396045900006,
Author = {Allmond, J. M. and Stuchbery, A. E. and Baktash, C. and Gargano, A. and
   Galindo-Uribarri, A. and Radford, D. C. and Bingham, C. R. and Brown, B.
   A. and Coraggio, L. and Covello, A. and Danchev, M. and Gross, C. J. and
   Hausladen, P. A. and Itaco, N. and Lagergren, K. and Padilla-Rodal, E.
   and Pavan, J. and Riley, M. A. and Stone, N. J. and Stracener, D. W. and
   Varner, R. L. and Yu, C. -H.},
Title = {{Electromagnetic Moments of Radioactive Te-136 and the Emergence of
   Collectivity 2p circle plus 2n Outside of Double-Magic Sn-132}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2017}},
Volume = {{118}},
Number = {{9}},
Month = {{MAR 3}},
Abstract = {{Radioactive Te-136 has two valence protons and two valence neutrons
   outside of the Sn-132 double shell closure, providing a simple
   laboratory for exploring the emergence of collectivity and
   nucleon-nucleon interactions. Coulomb excitation of Te-136 on a titanium
   target was utilized to determine an extensive set of electromagnetic
   moments for the three lowest-lying states, including B(E2; 01(+) ->
   2(1)(+)), Q(2(1)(+)), and g(2(1)(+)). The results indicate that the
   first-excited state, 2(1)(+), composed of the simple 2p circle plus 2n
   system, is prolate deformed, and its wave function is dominated by
   excited valence neutron configurations, but not to the extent previously
   suggested. It is demonstrated that extreme sensitivity of g(2(1)(+))to
   the proton and neutron contributions to the wave function provides
   unique insight into the nature of emerging collectivity, and g(2(1)(+))
   was used to differentiate among several state-of-the-art theoretical
   calculations. Our results are best described by the most recent shell
   model calculations.}},
