@article{ ISI:000395214200009,
Author = {Rosetti, Marcos F. and Valdez, Bertha and Hudson, Robyn},
Title = {{Effect of spatial scale on children's performance in a searching task}},
Journal = {{JOURNAL OF ENVIRONMENTAL PSYCHOLOGY}},
Year = {{2017}},
Volume = {{49}},
Pages = {{86-95}},
Month = {{APR}},
Abstract = {{The decision processes involved in searching behavior are often assumed
   to be similar despite differences in the dimensions of the situations in
   which the subjects search. However, many sensorimotor processes are
   modified when searching on real-life field situations, experimental
   rooms or computer screens. In this study we tested children in a series
   of setups where they searched for objects hidden beneath opaque covers.
   We reduced the search area from (1) a 50 x 70 m sports field, (2) a 5 x
   7 m floor area, (3) a 50 x 70 cm table top, and (4) a computer game.
   While we found that performance was similar in the three ``real-life{''}
   versions of the task, it was poorer in the computer version. In the
   light of these findings we discuss similarities and differences between
   the conditions as well as some of the implications for the use of
   virtual tasks in psychological assessment. (C) 2016 Elsevier Ltd. All
   rights reserved.}},
