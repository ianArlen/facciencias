@article{ ISI:000416793500012,
Author = {Miroshnichenko, Anatoly S. and Danford, Steven and Zharikov, Sergei V.
   and Manset, Nadine and Levato, Hugo and Grosso, Monica and Korcakova,
   Daniela and Kusakin, Anatoly V. and Khokhlov, Serik A. and Prendergast,
   Peter},
Title = {{Long-Term Spectroscopic Monitoring and Surveys of Early-Type Stars with
   and without Circumstellar Envelopes}},
Journal = {{OPEN ASTRONOMY}},
Year = {{2017}},
