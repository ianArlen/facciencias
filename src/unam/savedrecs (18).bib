@article{ ISI:000388112500062,
Author = {Vega Rojas, Lineth J. and Rojas Molina, Isela and Gutierrez Cortez, Elsa
   and Rincon Londono, Natalia and Acosta Osorio, Andres A. and Del Real
   Lopez, Alicia and Rodriguez Garcia, Mario E.},
Title = {{Physicochemical properties of nixtamalized corn flours with and without
   germ}},
Journal = {{FOOD CHEMISTRY}},
Year = {{2017}},
Volume = {{220}},
Pages = {{490-497}},
Month = {{APR 1}},
Abstract = {{This research studied the influence of the germ components on the
   physicochemical properties of cooked corn and nixtamalized corn flours
   as a function of the calcium hydroxide content (from 0 to 2.1 w/w) and
   steeping time (between 0 and 9 h). A linear relationship was found
   between calcium content in germ and steeping time used during
   nixtamalization process. X-ray diffraction analysis showed that calcium
   carbonate is formed into the germ structure to 2.1 w/w of calcium
   hydroxide and 9 h steeping time. The presence of the germ improves the
   development of peak viscosity in flours, and it is related to the
   increases in calcium concentration in germ and the formation of
   amylose-lipid complexes. No significant changes were observed in
   palmitic, stearic, oleic and linoleic acids of corn oil. The levels of
   further corn oil deterioration were 2.1 w/w of calcium hydroxide
   concentration and 9 h of steeping time. (C) 2016 Elsevier Ltd. All
   rights reserved.}},
