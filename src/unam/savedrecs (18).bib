@article{ ISI:000402811300002,
Author = {Dupey Garcia, Elodie},
Title = {{THE MATERIALITY OF COLOR IN PRE-COLUMBIAN CODICES: INSIGHTS FROM
   CULTURAL HISTORY}},
Journal = {{ANCIENT MESOAMERICA}},
Year = {{2017}},
Volume = {{28}},
Number = {{1}},
Pages = {{21-40}},
Month = {{SPR}},
Abstract = {{This article explores the materiality of the colors of some Nahua
   codices from pre-Columbian Mexico, comparing data obtained from two
   bodies of evidence: scientific investigation-through macroscopic
   inspection and physicochemical studies- and historical accounts. In
   doing so, this article shows that the Colonial period literature
   contributes to the understanding of the creation process of the codices
   and their chromatic palettes, in particular regarding some coloring
   materials, the identification of which still remains a challenge for
   scientists. The comparison carried out in this article also elucidates
   certain aspects of the historical texts on Mesoamerican colors, which
   were previously poorly understood or misinterpreted.}},
