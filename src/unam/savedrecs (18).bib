@article{ ISI:000395678000001,
Author = {Corichi, Alejandro and Montoya, Edison},
Title = {{Loop quantum cosmology of Bianchi IX: effective dynamics}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
