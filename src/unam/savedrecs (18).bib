@article{ ISI:000397798700007,
Author = {Romero-Romero, Sergio and Gomez Lagunas, Froylan and Balleza, Daniel},
Title = {{Side chain flexibility and coupling between the S4-S5 linker and the TRP
   domain in thermo-sensitive TRP channels: Insights from protein modeling}},
Journal = {{PROTEINS-STRUCTURE FUNCTION AND BIOINFORMATICS}},
Year = {{2017}},
Volume = {{85}},
Number = {{4}},
Pages = {{630-646}},
Month = {{APR}},
Abstract = {{The transient receptor potential (TRP) superfamily is subdivided into
   several subfamilies on the basis of sequence similarity, which is highly
   heterogeneous but shows a molecular architecture that resembles the one
   present in members of the Kv channel superfamily. Because of this
   diversity, they produce a large variety of channels with different
   gating and permeability properties. Elucidation of these particular
