@article{ ISI:000309897000016,
Author = {Ventura, Janeth and Alarcon-Aguilar, Francisco and Roman-Ramos, Ruben
   and Campos-Sepulveda, Efrain and Reyes-Vega, Maria L. and Daniel
   Boone-Villa, V. and Ivan Jasso-Villagomez, Edgar and Aguilar, Cristobal
   N.},
Title = {{Quality and antioxidant properties of a reduced-sugar pomegranate juice
   jelly with an aqueous extract of pomegranate peels}},
Journal = {{FOOD CHEMISTRY}},
Year = {{2013}},
Volume = {{136}},
Number = {{1}},
Pages = {{109-115}},
Month = {{JAN 1}},
Abstract = {{In the present study, the production of a reduced-sugar pomegranate
   juice jelly supplemented with an aqueous extract of pomegranate peel
   (PE) is described. Influence of different carbohydrate polymers (guar
   (G), xanthan (X) and tragacanth (T) gums) on rheological properties was
   studied. Combination GXT presented the most similar rheological
   behaviour to commercial jelly. Jelly (J) and jelly with PE (JE) were
   stored at 4 degrees C over an 8 week period for physical, chemical,
   antioxidant, microbiological and sensory analysis. J and JE showed
   similar values for degrees Brix, colour and Aw, though the pH of JE was
   lower than J. Thiol and phenolic compounds were higher in JE than in J.
   Antioxidant activity (radical scavenging activity and autoxidation of
   linoleic acid) was higher in JE than in J at 0 weeks, and were
   decreasing with time. Pomegranate juice with additives was generally
   less accepted than J and JE. (C) 2012 Elsevier Ltd. All rights reserved.}},
