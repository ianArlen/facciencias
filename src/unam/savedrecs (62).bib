@article{ ISI:000209520500007,
Author = {Balvanera, Patricia and Uriarte, Maria and Almeida-Lenero, Lucia and
   Altesor, Alice and DeClerck, Fabrice and Gardner, Toby and Hall,
   Jefferson and Lara, Antonio and Laterra, Pedro and Pena-Claros, Marielos
