@article{ ISI:000311310800010,
Author = {Ruelas-Inzunza, Jorge and Federico Soto-Jimenez, Martin and Carolina
   Ruiz-Fernandez, Ana and Bojorquez-Leyva, Humberto and Perez-Bernal,
   Hascibe and Paez-Osuna, Federico},
Title = {{Po-210 Activity and Concentrations of Selected Trace Elements (As, Cd,
   Cu, Hg, Pb, Zn) in the Muscle Tissue of Tunas Thunnus albacares and
   Katsuwonus pelamis from the Eastern Pacific Ocean}},
Journal = {{BIOLOGICAL TRACE ELEMENT RESEARCH}},
Year = {{2012}},
Volume = {{149}},
Number = {{3}},
Pages = {{371-376}},
Month = {{DEC}},
Abstract = {{Daily mineral intake (DMI) of Cu and Zn, percentage weekly intake (PWI)
   of As, Cd, Hg, Pb, and doses of Po-210 were estimated by using their
   elemental concentration in muscle of two tuna species and the average
   tuna consumption in Mexico. Skipjack tuna Katsuwonus pelamis had
   significantly (p < 0.05) higher levels of As (1.38 mu g g(-1) dw) and Cu
   (1.85 mu g g(-1) dw) than yellowfin tuna Thunnus albacares, whereas Pb
   concentrations (0.18 mu g g(-1) dw) were significantly (p < 0.05) higher
   in T. albacares. The sequence of elemental concentrations in both
   species was Zn > Cu > As > Hg > Pb > Cd. In T. albacares, concentrations
   of Cd and Pb in muscle tissue were positively correlated (p < 0.05) with
   weight of specimens, while Cu was negatively correlated. DMI values were
   below 10 \%. PWI figures (< 2 \%) are not potentially harmful to human
   health. Po-210 concentration in T. albacares and K. pelamis accounts for
   13.5 to 89.7 \% of the median individual annual dose (7.1 mu Sv) from
   consumption of marine fish and shellfish for the world population.}},
