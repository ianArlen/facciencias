@article{ ISI:000311977900013,
Author = {Sotres-Bayon, Francisco and Sierra-Mercado, Demetrio and
   Pardilla-Delgado, Enmanuelle and Quirk, Gregory J.},
Title = {{Gating of Fear in Prelimbic Cortex by Hippocampal and Amygdala Inputs}},
Journal = {{NEURON}},
Year = {{2012}},
Volume = {{76}},
Number = {{4}},
Pages = {{804-812}},
Month = {{NOV 21}},
Abstract = {{The prefrontal cortex (PFC) regulates emotional responses, but it is
   unclear how PFC integrates diverse inputs to select the appropriate
   response. We therefore evaluated the contribution of basolateral
   amygdala (BLA) and ventral hippocampus (vHPC) inputs to fear signaling
   in the prelimbic (PL) cortex, a PEG region critical for the expression
   of conditioned fear. In conditioned rats trained to press for food, BLA
   inactivation decreased the activity of projection cells in PL, and
   reduced PL conditioned tone responses. In contrast, vHPC inactivation
   decreased activity of interneurons in PL and increased PL conditioned
   tone responses. Consistent with hippocampal gating of fear after
   extinction, vHPC inactivation increased fear and PL pyramidal activity
   in extinguished, but not in conditioned, rats. These results suggest a
   prefrontal circuit whereby hippocampus gates amygdala-based fear. Thus,
   deficient hippocampal inhibition of PEG may underlie emotional
   disorders, especially in light of reduced hippocampal volume observed in
   depression and PTSD.}},
