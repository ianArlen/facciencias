@article{ ISI:000310333300005,
Author = {Biro, Susana},
Title = {{Astronomy by Correspondence: A Study of the Appropriation of Science by
   the Mexican Public (1927-1947)}},
Journal = {{SCIENCE COMMUNICATION}},
Year = {{2012}},
Volume = {{34}},
Number = {{6}},
Pages = {{803-819}},
Month = {{DEC}},
Abstract = {{This study of the correspondence between the director of the National
   Astronomical Observatory in Mexico and a variety of individuals produces
   a portrait of astronomy in the Mexican culture of the period. The
   letters analyzed here can be divided into three groups according to the
   author's main interests: observing the sky, creating theories, and daily
   life. By inquiring about the previous knowledge, intentions, and
   attitudes of the senders, it is possible to understand the process of
   appropriation of knowledge about astronomy. The letters shed light on
   aspects of the public interested in science that other approaches
   cannot.}},
