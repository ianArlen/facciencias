@article{ ISI:000312678600003,
Author = {Fernandez, Jesus A.},
Title = {{Phylogenetics and biogeography of the microendemic rodent
   Xerospermophilus perotensis (Perote ground squirrel) in the Oriental
   Basin of Mexico}},
Journal = {{JOURNAL OF MAMMALOGY}},
