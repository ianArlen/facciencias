@article{ ISI:000311762000007,
Author = {Alvarez-Amparan, M. A. and Cedeno-Caero, L.},
Title = {{OXIDATIVE DESULFURIZATION OF DIBENZOTHIOPHENES USING VOX/ZrO2-Al2O3}},
Journal = {{REVISTA MEXICANA DE INGENIERIA QUIMICA}},
Year = {{2012}},
Volume = {{11}},
Number = {{3}},
Pages = {{431-438}},
Month = {{DEC}},
Abstract = {{Vanadium oxide catalysts were prepared by incipient wetness impregnation
   of ammonium metavanadate on a zirconia-alumina mixed oxide with
   different vanadium loading (8, 10 and 15 wt.\% of V), in order to
   increase the performance in oxidative desulfurization (ODS) of
   dibenzothiophene compounds, using H2O2 or tert-butyl hydroperoxide as
   oxidant. In addition, the catalyst with 15\% of V was modified creating
   vanadium oxides partially reduced. The oxidation states and reducibility
   of vanadium catalysts were studied by temperature-programmed reduction
   (TPR). The catalysts were characterized by scanning electron microscopy
   (SEM-EDX) and X-ray diffraction (XRD). Catalytic performance was
