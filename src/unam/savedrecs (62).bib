@article{ ISI:000311550500018,
Author = {Balankin, Alexander S. and Mena, Baltasar and Martinez-Gonzalez, C. L.
   and Morales Matamoros, Daniel},
Title = {{Random walk in chemical space of Cantor dust as a paradigm of
   superdiffusion}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2012}},
Volume = {{86}},
Number = {{5, 1}},
Month = {{NOV 26}},
Abstract = {{We point out that the chemical space of a totally disconnected Cantor
   dust K-n subset of E-n is a compact metric space C-n with the spectral
   dimension d(s) = d(l) = n > D, where D and d(l) = n are the fractal and
   chemical dimensions of K-n, respectively. Hence, we can define a random
   walk in the chemical space as a Markovian Gaussian process. The mapping
   of a random walk in C-n into K-n subset of E-n defines the quenched Levy
   flight on the Cantor dust with a single step duration independent of the
   step length. The equations, describing the superdiffusion and
   diffusion-reaction front propagation ruled by the local quenched Levy
   flight on K-n subset of E-n, are derived. The use of these equations to
   model superdiffusive phenomena, observed in some physical systems in
   which propagators decay faster than algebraically, is discussed.}},
