@article{ ISI:000311469900032,
Author = {Peskov, V. and Bencze, G. and Di Mauro, A. and Martinengo, P. and
   Mayani, D. and Molnar, L. and Nappi, E. and Paic, G. and Smirnov, N. and
   Anand, H. and Shukla, I.},
Title = {{First observation of Cherenkov rings with a large area CsI-TGEM-based
   RICH prototype}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
Year = {{2012}},
Volume = {{695}},
Pages = {{154-158}},
Month = {{DEC 11}},
Note = {{International Conference on New Developments In Photodetection-NDIP11,
   Lyon, FRANCE, JUL 04-08, 2011}},
Abstract = {{We have built a RICH detector prototype consisting of a liquid C6F14
   radiator and six triple Thick Gaseous Electron Multipliers (TGEMs), each
   of them having an active area of 10 x 10 cm(2). One triple TGEM has been
   placed behind the liquid radiator in order to detect the beam particles,
   whereas the other five have been positioned around the central one at a
   distance to collect the Cherenkov photons. The upstream electrode of
   each of the TGEM stacks has been coated with a 0.41 mu m thick CsI
   layer.
   In this paper, we will present the results from a series of laboratory
   tests performed with this prototype carried out using UV light, 6 keV
   photons from Fe-55 and electrons from Sr-90 as well as recent results of
   tests with a beam of charged pions where for the first time Cherenkov
   Ring images have been successfully recorded with TGEM photodetectors.
   The achieved results prove the feasibility of building Cherenkov
   detector based on CsI coated TGEMs. (C) 2011 Elsevier B.V. All rights
   reserved.}},
