@article{ ISI:000311944300007,
Author = {Casillas, Gilberto and Pedro Palomares-Baez, Juan and Luis
   Rodriguez-Lopez, Jose and Luo, Junhang and Ponce, Arturo and Esparza,
   Rodrigo and Jesus Velazquez-Salazar, J. and Hurtado-Macias, Abel and
   Gonzalez-Hernandez, Jesus and Jose-Yacaman, Miguel},
Title = {{In situ TEM study of mechanical behaviour of twinned nanoparticles}},
Journal = {{PHILOSOPHICAL MAGAZINE}},
