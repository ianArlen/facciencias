Editor = {{Zank, GP and Borovsky, J and Bruno, R and Cirtain, J and Cranmer, S and Elliott, H and Giacalone, J and Gonzalez, W and Li, G and Marsch, E and Moebius, E and Pogorelov, N and Spann, J and Verkhoglyadova, O}},
Title = {{STEREO Interplanetary Shocks and Foreshocks}},
Booktitle = {{PROCEEDINGS OF THE THIRTEENTH INTERNATIONAL SOLAR WIND CONFERENCE (SOLAR
   WIND 13)}},
