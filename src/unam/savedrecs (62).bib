@article{ ISI:000313932200004,
Author = {Oceguera-Figueroa, Alejandro and Pacheco-Chaves, Bernald},
Title = {{Records of leeches from Costa Rica and taxonomic key for species
   identification with a redescription of Cylicobdella costaricae}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2012}},
Volume = {{83}},
Number = {{4}},
Pages = {{946-957}},
Month = {{DEC}},
Abstract = {{In contrast to the enormous efforts made to characterize the fauna and
   flora from Costa Rica, except for a few isolated records, very little is
   known about leeches (Annelida: Clitellata) in the country. In this
   paper, we present the redescription of a poorly known terrestrial leech:
   Cylicobdella costaricae, characterizing its external and internal
   morphology, the latter, completely unknown to date. In addition, we
   report the first list of leech species for Costa Rica, including in
   total 12 taxa with records from the 7 provinces of the country. We
   provide a brief description of the leech morphology, pictures and
   localities for each taxon. The information shown here is the result of
   the study of both, specimens deposited at the Museo de Zoologia of the
   Universidad de Costa Rica and the new collections. Finally, we provide a
   taxonomic key for species identification.}},
