@article{ ISI:000312425100005,
Author = {Teresa Ramirez-Herrera, Maria and Inbar, Moshe and Legorreta Paulin,
   Gabriel},
Title = {{One hundred years of the rock fall triggered by the 1912 Acambay
   earthquake, Mexico}},
Journal = {{ZEITSCHRIFT FUR GEOMORPHOLOGIE}},
