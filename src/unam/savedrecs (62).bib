@inproceedings{ ISI:000346798302088,
Author = {Velasco Sanchez, Enrique Ruiz and Beauchemin, Marielle},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{MOBILE EDUCATIONAL ROBOTICS (MER)}},
Booktitle = {{EDULEARN13: 5TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
