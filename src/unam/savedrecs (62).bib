@article{ ISI:000313413200013,
Author = {Mendoza-Hernandez, Pedro E. and Orozco-Segovia, Alma and Meave, Jorge A.
   and Valverde, Teresa and Martinez-Ramos, Miguel},
Title = {{Vegetation recovery and plant facilitation in a human-disturbed lava
   field in a megacity: searching tools for ecosystem restoration}},
