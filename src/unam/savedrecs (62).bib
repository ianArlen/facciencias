@article{ ISI:000311039500039,
Author = {Sanchez-Castellanos, M. and Lemus, R. and Carvajal, M. and Perez-Bernal,
   F. and Fernandez, J. M.},
Title = {{A study of the Raman spectrum of CO2 using an algebraic approach}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{2012}},
Volume = {{554}},
Pages = {{208-213}},
Month = {{DEC 3}},
Abstract = {{The vibrational Raman spectrum of CO2 is calculated by means of the
   algebraic model U(2) x U(3) x U(2). The Hamiltonian and the molecular
   polarizability tensor operators are expanded in terms of symmetry
   coordinates, which in turn are transformed into the algebraic
   representation. The derivatives of the polarizability with respect to
   symmetry coordinates are fitted to six experimental transition
   intensities at room temperature and compared with previous experimental
   and ab initio values. The Raman spectrum of the Fermi diad at 1750 K is
   then simulated and compared with the experimental one. (C) 2012 Elsevier
   B.V. All rights reserved.}},
