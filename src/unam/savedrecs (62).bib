@inproceedings{ ISI:000346798300132,
Author = {Rodriguez, Marta},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{LANGUAGE PROBLEMS IN CHEMISTRY LEARNING}},
Booktitle = {{EDULEARN13: 5TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES}},
Series = {{EDULEARN Proceedings}},
Year = {{2013}},
Pages = {{881-883}},
Note = {{5th International Conference on Education and New Learning Technologies
   (EDULEARN), Barcelona, SPAIN, JUL 01-03, 2013}},
Abstract = {{In the Inorganic Chemistry course with students of the 3th semester of
   the Chemistry Faculty of the National Autonomous University of Mexico is
   perceived that one of the problems in the students learning is the lack
   of comprehension of the chemical language, the encounter difficulties
   are: Lack of understanding of technical terms introduced in the study of
   chemistry, they have absence of learning conventions applied to
   specialized chemical language to the level of automatization required to
   read chemistry fluently, they do not understand common words used to
   teach chemistry. Then the efforts to overcome language problems must
   attack these sources of difficulties and encourage the students to
   deciphering symbols, translating language, chemistry formulas,
   equations, mathematical equations from ordinary language (Spanish).
   Students must be taught that technical language unlike ordinary (Spanish
   language), is deliberately concise, precise, and devoid of redundancy.
   The teachers must encourage students to reading-thinking activities in
   order to practice rational thought like listening to a lecture, watching
