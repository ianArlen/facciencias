@article{ ISI:000180451900130,
Author = {Villagran-Muniz, M and Navarrete, M and Mejia-Uriarte, EV},
Title = {{Photoacoustic determination of phase transition in BaTiO3 induced by
   high pressure at room temperature}},
Journal = {{REVIEW OF SCIENTIFIC INSTRUMENTS}},
Year = {{2003}},
