@article{ ISI:000179966500007,
Author = {Arteaga, MA},
Title = {{Robot control and parameter estimation with only joint position
   measurements}},
Journal = {{AUTOMATICA}},
Year = {{2003}},
Volume = {{39}},
Number = {{1}},
Pages = {{67-73}},
Month = {{JAN}},
Abstract = {{Most adaptive control schemes for rigid robots assume velocities
   measurements to be available. Although it is possible to measure
   velocities by using tachometers, this increases costs and the signals
   delivered may be contaminated with noise. Since the use of encoders
   allows to read joint position pretty accurately, it is desirable to
   estimate joint velocities through an observer. This paper presents an
   adaptive scheme designed in conjunction with a linear observer.
   Boundedness of the estimated parameters and uniform ultimate boundedness
   for the tracking and observation errors are guaranteed. Experimental
   results are included to support the developed theory. (C) 2002 Published
   by Elsevier Science Ltd.}},
