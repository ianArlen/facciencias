@inproceedings{ ISI:000186211700048,
Author = {Gonzalez-Esparza, JA and Lara, A and Santillan, A and Gopalswamy, N},
Editor = {{Velli, M and Bruno, R and Malara, F}},
Title = {{A numerical study on the evolution of CMEs and shocks in the
   interplanetary medium}},
Booktitle = {{SOLAR WIND TEN, PROCEEDINGS}},
