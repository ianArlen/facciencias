@inproceedings{ ISI:000184043400030,
Author = {Garcia, JA and Guijosa, A and Vergara, JD},
Editor = {{Cotti, E and Mondragon, M and TavaresVelasco, G}},
Title = {{M, membranes, and OM}},
Booktitle = {{PARTICLES AND FIELDS}},
Series = {{AIP Conference Proceedings}},
Year = {{2003}},
Volume = {{670}},
