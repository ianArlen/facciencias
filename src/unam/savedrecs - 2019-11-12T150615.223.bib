Author = {Pestryakov, AN and Lunin, VV and Fuentes, S and Bogdanchikova, N and
   Barrera, A},
Title = {{Influence of modifying additives on the electronic state of supported
   palladium}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{2003}},
