@article{ ISI:000180451900055,
Author = {Navarrete, M and Villagran-Muniz, M},
Title = {{Loss factor determination of composite laminates by the photoacoustic
   technique}},
Journal = {{REVIEW OF SCIENTIFIC INSTRUMENTS}},
Year = {{2003}},
Volume = {{74}},
Number = {{1, 2}},
Pages = {{479-481}},
Month = {{JAN}},
Note = {{12th International Conference on Photoacoustic and Photothermal
   Phenomena (12 ICPPP), UNIV TORONTO, TORONTO, CANADA, JUN 24-27, 2002}},
Abstract = {{This article presents a method for identifying elastic and damping
   properties of unidirectional composite laminates by using photoacoustic
   (PA) signal data. In this method, the laser pulse is used as a standard
   source of ultrasound. The analysis was developed considering that the
   acoustic wave propagation onto the material is the impulse response from
   a second-order system. The loss fact eta(theta) and the engineering
   constant E(theta) were determined for a five-fiber orientation angle in
   three samples with different thicknesses. Samples based on glass fibers
   and epoxy matrix were prepared with low fiber volume fraction, for this
   purpose. The constants for each direction were measured from the PA
   signal data obtained in successive positions between the laser source
   and the transducer. For E(theta) we used the first arrival time delay,
   and for eta(theta) the first peak amplitude. The values obtained by this
   method show good agreement with those in literature and demonstrate the
   efficiency of the proposed method. In principle, with this method, all
   elastic constants and damping factors can be determined simultaneously.
   (C) 2003 American Institute of Physics.}},
