@article{ ISI:000180277100008,
Author = {Aldape, GE and Solorio, GR},
Title = {{Load losses in small-diameter circular conduits for Reynold's numbers
   between 33 and 900.}},
Journal = {{INGENIERIA HIDRAULICA EN MEXICO}},
Year = {{2003}},
Volume = {{18}},
Number = {{1}},
Pages = {{119-125}},
Month = {{JAN-MAR}},
Abstract = {{An experimental research to find local and distributed losses in
   circular conduits for Reynolds numbers between 33 < Re < 900 is
   presented. The behavior of the local loss coefficient for different
   Reynolds numbers, which is quite different to the 0.5 value usually
   employed, was found. The distributed loss coefficient follows a relation
   similar to that of Hagen-Poiseuille, but with different coefficients and
   Reynolds exponents. Finally, it was found that both, the influence of
   the surface tension and the Hagenbach correction have negligible effects
   in the results. Design curves are proposed.}},
