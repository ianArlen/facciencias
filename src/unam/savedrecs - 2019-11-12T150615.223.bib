@inproceedings{ ISI:000184031800165,
Author = {Martinell, JJ and Guzdar, PN},
Editor = {{Falconer, IS and Dewar, RL and Khachan, J}},
Title = {{A model of self-consistent L-H transition based on finite-beta drift
   waves}},
Booktitle = {{PLASMA PHYSICS}},
Series = {{AIP CONFERENCE PROCEEDINGS}},
Year = {{2003}},
Volume = {{669}},
Pages = {{654-657}},
Note = {{International Congress on Plasma Physics, SYDNEY, AUSTRALIA, JUL 15-19,
   2002}},
Organization = {{Int Union Pure \& Appl Phys; Australian Inst Phys; New S Wales Dept
   State \& Reg Dev}},
Abstract = {{A set of equations is derived that describes the slow and fast evolution
   of a tokamak plasma, for finite P drift waves. This provides a
   self-consistent model for the L-H transition showing the interrelation
   of fast fluctuations and slow equilibrium quantities, that explains the
   zonal flows and fields in terms of drift wave turbulence. The relevant
   equations are separated in axisymmetric and fluctuating parts and
   transformed to a helical mode-aligned coordinate, thus reducing the
   problem to 2D. Only two toroidal modes are included. The slow equations,
   which describe the transport, are coupled to the fast equations, which
   have been shown to predict the L-H transition threshold {[}2] for
   several experiments, given in terms of the mode dimensionless parameters
   alpha(MHD) and alpha(D) that measure ideal stability and diamagnetic
   effects, respectively. This approach allows to relate this threshold, to
   the experimentally measured injected power. The full system of equations
   is numerically solved to get a power threshold for the L-H transition.}},
