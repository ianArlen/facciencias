@article{ ISI:000181169300041,
Author = {Petrosyan, P and Garcia-Varela, M and Luz-Madrigal, A and Huitron, C and
   Flores, ME},
Title = {{Streptomyces mexicanus sp nov., a xylanolytic micro-organism isolated
   from soil}},
Journal = {{INTERNATIONAL JOURNAL OF SYSTEMATIC AND EVOLUTIONARY MICROBIOLOGY}},
Year = {{2003}},
Volume = {{53}},
Number = {{1}},
Pages = {{269-273}},
Month = {{JAN}},
Abstract = {{The taxonomic position of a thermophilic actinomycete strain isolated
   from soil was examined using a polyphasic approach. The strain,
   designated CH-M-1035(T), was assigned to the genus Streptomyces on the
   basis of chemical and morphological criteria. It formed Rectiflexibiles
   aerial hyphae that carried long chains of rounded, smooth spores. The
   almost complete nucleoticle sequence of the 16S rRNA gene of strain
   CH-M-1035(T) was determined and its comparison with the 16S rDNA
   sequences of previously studied streptomycetes confirmed the assignment
   of the novel strain to the genus Streptomyces. Strain CH-M-1035(T)
   clustered with species belonging to the Streptomyces thermodiastaticus
   clade in the 16S-rDNA-based phylogenetic tree. However, the phenotypic
   properties of strain CH-M-1035(T) differed from those of the recognized
   species within this clade. Therefore, it is proposed that strain
   CH-M-1035(T) be classified as a novel species within the genus
   Streptomyces, as Streptomyces mexicanus (type strain CH-M-1035(T) = DSM
   41796(T) = BM-B-384(T) = NRRL B-24196(T)).}},
