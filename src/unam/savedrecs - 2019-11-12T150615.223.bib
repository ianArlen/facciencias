@inproceedings{ ISI:000189467000066,
Author = {Lopez, JA and Vazquez, R and Miranda, LF and Masegosa, J and Meaburn, J
   and Torrelles, JM and Garcia-Segura, G and Toledano, O},
Editor = {{Corradi, RLM and Mikolajewska, J and Mahoney, TJ}},
Title = {{The bipolar symbiotic nebula Sa 2-237}},
Booktitle = {{SYMBIOTIC STARS PROBING STELLAR EVOLUTION}},
Series = {{Astronomical Society of the Pacific Conference Series}},
Year = {{2003}},
Volume = {{303}},
Pages = {{441-445}},
Note = {{Euroconference on Symbiotic Stars Probing Stellar Evolution, Los
   Cancajos, SPAIN, MAY 27-31, 2002}},
Organization = {{ING; European Commiss; Excelentisimo Cabildo Insular Palma; Patronato
   Turismo; Inst Astrofis Canarias}},
Abstract = {{Deep narrow band images and low and high dispersion spectra have been
   obtained for the bipolar planetary nebula Sa 2-237. This nebula has a
   very narrow waist, prominent point-symmetric filaments along the inner
   borders of its opposite lobes, and compact jet-like structures
   protruding beyond the lobes. The nebular spectrum is of high excitation
   type; however, the photospheric spectrum from the central star
   corresponds to an early K-type giant, indicating that the core has a
   symbiotic nature. A thorough long-slit mapping across the nebula reveals
   a complex kinematics. The peculiar characteristics of Sa 2-237 represent
