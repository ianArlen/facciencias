@inproceedings{ ISI:000187839200021,
Author = {Pena, M and Medina, S and Stasinska, G},
Editor = {{ReyesRuiz, M and VazquezSemadeni, E}},
Title = {{Characteristics of planetary nebulae with {[}WC] central stars}},
Booktitle = {{EIGHTH MEXICO-TEXAS CONFERENCE ON ASTROPHYSICS: ENERGETICS OF COSMIC
   PLASMAS}},
