Author = {Olague, G and Hernandez, B and Dunn, E},
Editor = {{Raidl, G and Cagnoni, S and Cardalda, JJR and Corne, DW and Gottlieb, J and Guillot, A and Hart, E and Johnson, CG and Marchiori, E and Meyer, JA and Middendorf, M}},
Title = {{Accurate L-corner measurement using USEF functions and evolutionary
   algorithms}},
