@inproceedings{ ISI:000186888900025,
Author = {Mas-Oliva, J and Moreno, A and Ramos, S and Xicohtencatl-Cortes, J and
   Campos, J and Castillo, R},
Editor = {{Dhalla, NS and Chockalingam, A and Berkowitz, HI and Singal, PK}},
Title = {{Monolayers of apolipoprotein AII at the air/water interface}},
Booktitle = {{FRONTIERS IN CARDIOVASCULAR HEALTH}},
