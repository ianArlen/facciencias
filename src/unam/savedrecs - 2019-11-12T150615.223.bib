@article{ ISI:000180696100006,
Author = {Gil-Zepeda, SA and Montalvo-Arrieta, JC and Vai, K and Sanchez-Sesma, FJ},
Title = {{A hybrid indirect boundary element - discrete wave number method applied
   to simulate the seismic response of stratified alluvial valleys}},
Journal = {{SOIL DYNAMICS AND EARTHQUAKE ENGINEERING}},
