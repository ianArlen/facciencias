@inproceedings{ ISI:000187839200023,
Author = {Reyes-Ruiz, M and Perez-Tijerina, E and Sanchez-Salcedo, FJ},
Editor = {{ReyesRuiz, M and VazquezSemadeni, E}},
Title = {{The magneto-rotational instability in protoplanetary disks}},
Booktitle = {{EIGHTH MEXICO-TEXAS CONFERENCE ON ASTROPHYSICS: ENERGETICS OF COSMIC
   PLASMAS}},
