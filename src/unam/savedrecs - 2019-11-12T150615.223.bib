@inproceedings{ ISI:000187839200008,
Author = {Garcia-Segura, G and Lopez, JA and Franco, J},
Editor = {{ReyesRuiz, M and VazquezSemadeni, E}},
Title = {{MHD solutions for proto-planetary nebulae}},
Booktitle = {{EIGHTH MEXICO-TEXAS CONFERENCE ON ASTROPHYSICS: ENERGETICS OF COSMIC
   PLASMAS}},
Series = {{Revista Mexicana de Astronomia y Astrofisica-Serie de Conferencias}},
Year = {{2003}},
Volume = {{18}},
Pages = {{31-37}},
Note = {{8th Mexico-Texas Conference on Astrophysics, MEXICO CITY, MEXICO, OCT
   31-NOV 02, 2002}},
Organization = {{Univ Nacl Autonoma Mexico, Inst Astronomia}},
Abstract = {{This paper provides solutions for the origin of post-AGB winds, their
   acceleration up to high speed, and the subsequent formation of extremely
   collimated proto-planetary nebulae. Several wind models with terminal
   velocities from a few tens of km s(-1) up to 10(3) km s(-1) are
   calculated, which produce proto-planetary nebulae with linear momentum
   in the range 10(36)- 10(40) g cm s(-1) and with kinetic energies in the
   range 10(42) - 10(47) erg. These results match available observations of
   proto-planetary nebulae. In the present simplistic scheme, the driver of
   the wind is just the magnetic pressure at the stellar surface. Other
   forces are not taken into account in this study, except gravity. We
   conclude that mass-loss rates of post-AGB stars and transition times
   from late AGB up to planetary nebula central stars could be directly
   linked with the production of magnetic field at the stellar core. As an
   example, mass-loss rates as large as 8 x 10(-5) M-circle dot yr(-1) and
   transition times as short as 5,000 years are predicted.}},
Publisher = {{UNIVERSIDAD NACIONAL AUTONOMA MEXICO INSTITUTO ASTRONOMIA}},
Address = {{APARTADO POSTAL 70-264, MEXICO CITY 04510, MEXICO}},
