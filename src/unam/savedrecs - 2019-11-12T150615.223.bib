Editor = {{AvilaReese, V and Firmani, C and Frenk, CS and Allen, YC}},
Title = {{Chemical enrichment in four local DSPH galaxies}},
Booktitle = {{GALAXY EVOLUTION: THEORY AND OBSERVATIONS}},
