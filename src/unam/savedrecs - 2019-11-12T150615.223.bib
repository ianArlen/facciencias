@article{ ISI:000182986900021,
Author = {Antonyan, S},
Title = {{West's problem on equivariant hyperspaces and Banach-Mazur compacta}},
Journal = {{TRANSACTIONS OF THE AMERICAN MATHEMATICAL SOCIETY}},
Year = {{2003}},
Volume = {{355}},
