@article{ ISI:000180013500012,
Author = {Corona-Morales, AA and Castell, A and Escobar, A and Drucker-Colin, R
   and Zhang, LM},
Title = {{Fullerene C-60 and ascorbic acid protect cultured chromaffin cells
   against levodopa toxicity}},
Journal = {{JOURNAL OF NEUROSCIENCE RESEARCH}},
Year = {{2003}},
Volume = {{71}},
Number = {{1}},
Pages = {{121-126}},
Month = {{JAN 1}},
Abstract = {{Adrenal chromaffin cell (ACC) transplants, alone or combined with
   levodopa treatment, were used in attempted therapy for Parkinson's
   disease (PD). In a previous study, we demonstrated that levodopa caused
   chromaffin cell death either by necrosis or by apoptosis in cell
   culture. Here we report the beneficial effect of a water-soluble
   derivative of fullerene C-60 (a novel molecule with potent antioxidant
   properties) and of ascorbic acid when applied to chromaffin cell
   cultures exposed to levodopa. Both antioxidants remarkably increase the
   ACC survival and prevent cell death, including apoptosis. Although ACC
   transplants are not currently considered as an option for PD treatment,
   these observations should help in exploring the possibilities of
   preventing the neurotoxicity generated by levodopa and in envisaging new
   strategies for PD treatment by combining the clinical use of levodopa
   and potent antioxidants. Chemical properties of fullerene related to
   biological uses are discussed. Wiley-Liss, Inc.}},
