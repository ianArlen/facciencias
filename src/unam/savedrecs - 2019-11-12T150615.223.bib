@article{ ISI:000185560600004,
Author = {Estrada, RF and Hinojosa-Torres, J and Castano, VM},
Title = {{Growth and characterization of dimethylterephtalate crystals}},
Journal = {{DESIGNED MONOMERS AND POLYMERS}},
Year = {{2003}},
Volume = {{6}},
Number = {{3}},
Pages = {{267-275}},
Abstract = {{Dimethylterephthalate (DMT) crystals were grown using the Bridgeman
   method. X-ray analysis of the transparent-white crystals showed that the
   crystals have an orthorhombic structure and, with the aid of electron
   diffraction, the twinning relationships in the crystals were determined.
   Scanning Electron Microscope (SEM) and Atomic Force Microscopy (AFM)
   images of the crystal surface revealed that the crystal growth proceeded
   by propagation of twin planes forming a series of layers. This growth
   mechanism is supported by optical images of a cross section of the
   crystal, which clearly shows twinned crystals.}},
