@inproceedings{ ISI:000182216000003,
Author = {Romero-Rochin, V},
Editor = {{Kenkre, VM and Lindenberg, K}},
Title = {{Boltzmann, ratchets and avalanches}},
Booktitle = {{MODERN CHALLENGES IN STATISTICAL MECHANICS: PATTERNS, NOISE, AND THE
   INTERPLAY OF NONLINEARITY AND COMPLEXITY}},
