@inproceedings{ ISI:000181980700084,
Author = {Masciadri, E},
Editor = {{Wizinowich, PL and Bonaccini, D}},
Title = {{A meso-scale atmospherical model for a 3D optical turbulence
   characterization in astronomy}},
Booktitle = {{ADAPTIVE OPTICAL SYSTEM TECHNOLOGIES II, PTS 1 AND 2}},
