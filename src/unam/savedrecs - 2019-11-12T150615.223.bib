@inproceedings{ ISI:000188831100007,
Author = {Mochan, WL and Lopez-Bastidas, C and Maytorena, JA and Mendoza, BS and
   Brudny, VL},
Editor = {{Mendoza, BS}},
Title = {{Depth resolved nonlinear optical nanoscopy}},
Booktitle = {{PHYSICA STATUS SOLIDI C: CURRENT TOPICS IN SOLID STATE PHYSICS, VOL 0,
   NO 8}},
