@inproceedings{ ISI:000185703600006,
Author = {Cosio, FA and Castaneda, MAP},
Editor = {{Bruce, N and GamboaDeBuen, I and MartinezDavalos, A and RodriguezVillafuerte, M}},
Title = {{Computer assisted surgery}},
Booktitle = {{MEDICAL PHYSICS}},
