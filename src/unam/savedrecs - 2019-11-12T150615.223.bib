@inproceedings{ ISI:000189453300037,
Author = {Flores, E and Ayala, AG and Ordaz, M},
Editor = {{DerKiureghian, A and Madanat, S and Pestana, JM}},
Title = {{Second order statistical response of a bilinear hysteretic oscillator
   excited by non stationary stochastic processes}},
Booktitle = {{APPLICATIONS OF STATISTICS AND PROBABILITY IN CIVIL ENGINEERING, VOLS 1
   AND 2}},
Year = {{2003}},
Pages = {{275-282}},
Note = {{9th International Conference on Applications of Statistics and
   Probability in Civil Engineering, SAN FRANCISCO, CA, JUL 06-09, 2003}},
Organization = {{CERRA}},
Abstract = {{This paper presents a method to obtain the covariance matrix of the
   response of a bilinear hysteretic oscillator excited by signals
   corresponding to an evolutionary stochastic process. The process
   characterizes an ensemble of seismic signals simulated by the empirical
   Green's function method. The Green function used, corresponds to the
   earthquake of April 25 1989, registered at the lake-bed SCT station. The
   magnitude of the simulated quakes is 8.1. The oscillator represents an
   equivalent approximation to a multi degree of freedom structure defined
   through a consistent pushover based formulation. The differential
   equation that models the oscillator is linearaized using a stochastic
   linearization method considering Gaussian and non-Gaussian coefficients.
   The non Gaussian nature of the response is attributed to the variable
   that couples the equilibrium differential equation of the oscillator to
   that defining the hysteretic behavior of the system. The marginal
   probability density function of this variable is built by superposing a
   truncated Normal probability density function with two Dirac deltas
   weighted by factors equal to half the cut-off area of this function.
   Numerical results for illustrative systems with different resistances
   and periods are verified by Monte Carlo simulations.}},
