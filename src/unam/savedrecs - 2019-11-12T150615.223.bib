@inproceedings{ ISI:000184043400032,
Author = {de la Macorra, A},
Editor = {{Cotti, E and Mondragon, M and TavaresVelasco, G}},
Title = {{Late time phase transition as dark energy}},
Booktitle = {{PARTICLES AND FIELDS}},
Series = {{AIP Conference Proceedings}},
Year = {{2003}},
Volume = {{670}},
Pages = {{272-280}},
Note = {{10th Mexican School on Particles and Fields, PLAYA DEL CARMEN, MEXICO,
   OCT 30-NOV 06, 2002}},
Organization = {{Cinvestav; DPF MPS; ICN UNAM; IF UNAM; CLAF; Conacyt; IF UASLP; IFM
   UMSNH}},
Abstract = {{We study the possibility that the quintessence field responsible for the
   dark energy and the present day acceleration of the universe is given by
   a condensate field generated at a late phase transition scale determined
   by the condensation scale.
   We show that the quintessence field can be described naturally by the
   condensates of a non-abelian gauge group. The non-perturbative effects
   generated by a strong gauge coupling constant can de determined and a
   scalar potential for the dark meson fields is generated parameterizing
   the dark energy. On the other hand it is the massive particles, e.g.
   dark baryons, of the dark gauge group that give the corresponding dark
   matter.
