@inproceedings{ ISI:000185639500034,
Author = {Maya, C and Beltran, N and Jimenez, B and Bonilla, P},
Editor = {{Angelakis, AN and Paranychianakis, NV and Tsagarakis, KP}},
Title = {{Evaluation of the UV disinfection process in bacteria and amphizoic
   amoebae inactivation}},
Booktitle = {{WATER RECYCLING IN THE MEDITERRANEAN REGION}},
