@inproceedings{ ISI:A1996BH59M00023,
Author = {Galaviz, J and Kuri, A},
Editor = {{Soto, R and Sanchez, JM and Campbell, M and Cantu, FJ}},
Title = {{A self-adaptive genetic algorithm for function optimization}},
Booktitle = {{PROCEEDINGS ISAI/IFIS 1996 - MEXICO - USA COLLABORATION IN INTELLIGENT
