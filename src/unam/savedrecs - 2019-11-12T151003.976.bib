@article{ ISI:A1996TX91100012,
Author = {Fomina, L and Sansores, LE and Salcedo, R},
Title = {{On the aromaticity of a nonafulvene}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE-THEOCHEM}},
Year = {{1996}},
Volume = {{362}},
Number = {{2}},
Pages = {{257-261}},
Month = {{FEB 23}},
Abstract = {{Quantum chemical calculations were carried out on
   11,12-bis(diethylamino)nonatriafulvalene and its precursor in order to
   ensure that it is in fact `'aromatic''. The calculations were performed
   using Hartree Fock at the 3-21G level and configuration interaction with
   all double substitutions at the STO-3G level. Calculations were also
   carried out with the molecule immersed in a solvent, chloroform, in the
   Onsager approximation. The results indicate that the molecule is indeed
   `'aromatic'' and that the energy difference with the precursor is 4.47
   eV. The presence of the solvent does not change the `'aromatic''
   geometry and reduces its energy by 0.24 eV.}},
