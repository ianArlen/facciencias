@article{ ISI:A1996VC41400007,
Author = {GamboadeBuen, I and Buenfil, AE and RodriguezVillafuerte, M and Ruiz, CG
   and ZarateMorales, A and Brandan, ME},
Title = {{Supralinearity in the response of TLD-100 to 5.3 MeV alpha particles}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{1996}},
Volume = {{65}},
Number = {{1-4, 1}},
Pages = {{13-16}},
Note = {{11th International Conference on Solid State Dosimetry, BUDAPEST,
   HUNGARY, JUL 10-14, 1995}},
Organization = {{Bubble Technol Ind; Harshaw Bicron Technol Vertriebs GmbH; Hungarian
   Acad Sci; Hungarian Natl Comm Technol Dev; Inst Isotopes, Hungary;
   Landauer Inc; Nucl Technol Publ; Paks Nucl Power Plant Ltd; Rados
   Technol Oy; Siemens Environm Syst Ltd; Studsvik Instrument AB; Victoreen
   Inc}},
Abstract = {{The dose-response of TLD-100 exposed to 5.3 MeV alpha particles has been
   measured for fluences between 1.4 x 10(7) and 8.2 x 10(10)
   alphas.cm(-2). The irradiation geometry corresponds to `near isotropic'
   incidence and crystals were annealed in air. Peaks 3 and 4 are observed
   to be linear-sublinear and peak 9, supralinear. The maxima of the
   supralinearity function f(D) for peaks 5-8 are 1.1, 1.6, 1.4 and 3.4,
   respectively. These values, except for peak 5, are similar to previous
   observations at about the same energy performed under different
   annealing conditions or TL heating rate. Comparison with 4 MeV data
   agrees with track interaction model predictions.}},
