@article{ ISI:A1996UJ91100006,
Author = {DelaMacorra, A and Lola, S},
Title = {{Inflation in S-dual superstring models}},
Journal = {{PHYSICS LETTERS B}},
Year = {{1996}},
Volume = {{373}},
Number = {{4}},
Pages = {{299-305}},
Month = {{APR 25}},
Abstract = {{We study inflationary potentials in the framework of superstring
   theories, Successful inflation may occur due to chiral fields, but only
   after the dilaton and moduli are stabilized. This is achieved by
   demanding an S-duality invariant potential. Then, it is possible to have
   inflation at any scale and even to have more than one stage of
   inflation. This occurs for a limited class of scalar potentials, where
   certain conditions are obeyed, Density fluctuations of 10(-5) require
