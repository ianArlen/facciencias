@article{ ISI:A1996UE14700009,
Author = {Perez, JC and Briano, MS and Gutierrez, ME},
Title = {{A new species of Macrothrix (Anomopoda: Macrothricidae) from Central
   Mexico}},
Journal = {{HYDROBIOLOGIA}},
Year = {{1996}},
Volume = {{319}},
Number = {{2}},
Pages = {{159-166}},
Month = {{FEB 16}},
Abstract = {{Macrothrix mexicanus sp. nov. is described from central Mexico, a
   transition zone between the nearctic and neotropics. All localities
   where it was found are over 1800 meters above sea level. It shows many
   resemblances with M. laticornis, M. camjatae and M. rosea but is
   characterized by a persistent dorsal tooth on the valve keel, a spinous
   papilla on the basipodite of the antenna, the second thoracic limb with
   a long conical sensillum between scraper 1 and the gnathobase, the
   endopod of trunk limb IV having two setae; the postabdomen with the
   dorsal margin bilobed, and the distal segment of the seta natatoria
   which is unusually long.}},
