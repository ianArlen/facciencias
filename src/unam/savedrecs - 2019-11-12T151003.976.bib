@article{ ISI:A1996UP69600030,
Author = {OstroskySolis, F and Rigalt, C and Perez, M and Marcos, J},
Title = {{Brain potentials (ERPs) and syntactic comprehension: Effects of thematic
   role order}},
Journal = {{BRAIN AND COGNITION}},
Year = {{1996}},
Volume = {{30}},
Number = {{3}},
Pages = {{297-300}},
Month = {{APR}},
Note = {{TENNET VI - Theoretical and Experimental Neuropsychology Conference,
   MONTREAL, CANADA, MAY 10-14, 1995}},
Abstract = {{Event-related potentials (ERPs) were recorded from 32 scalp electrodes
   during a syntactic analysis task, where the subjects had to assign
   thematic roles (as agent or theme) on the basis of word order and
   syntactic structure. Sentences were divided in three different groups:
   canonic order (S-V-O), inverse order (O-V-S), and an anomalous condition
   without a syntactic order. The syntactic decision between both
   experimental conditions (canonic and inverse) elicited a slow negative
   response between 300 and 500 msec, with a maximum amplitude around 340
   msec (N340), which was largest over frotocentral regions. The asyntactic
   condition elicited a slow positive shift with a latency around 670 msec
   (P600) that was largest over right parieto-occipital regions. The
   distinct distribution of the two N400 components, one semantic
   (centro-parietal) and the other syntactic (fronto-central), provides the
   basis to use brain activity to study parsing and/or the processes that
   underlie the syntactic relationships among words. It also supports the
   proposal that semantic and syntactic processes are distinct subsystems
   within the language processing.}},
