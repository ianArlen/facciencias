@article{ ISI:A1996TX41600006,
Author = {GonzalezEspinosa, C and GarciaSainz, JA},
Title = {{Hormonal modulation of c-fos expression in isolated hepatocytes. Effects
   of angiotensin II and phorbol myristate acetate on transcription and
   mRNA degradation}},
Journal = {{BIOCHIMICA ET BIOPHYSICA ACTA-MOLECULAR CELL RESEARCH}},
Year = {{1996}},
Volume = {{1310}},
Number = {{2}},
Pages = {{217-222}},
Month = {{FEB 2}},
Abstract = {{It has been shown that angiotensin II and PMA increase the expression of
   proto-oncogenes (c-Sos, c-myc and c-mos) in liver cells. In this study
   the effects of angiotensin II and PMA on c-Sos transcription and mRNA
   stability were investigated. Using nuclear run-off transcription assays,
   it was observed that PMA and angiotensin II induced a rapid increase in
   c-Sos transcription. The transcription rate of the GAPDH gene did not
   change, indicating that the effects were not general on gene
   transcription. The ability of these agents to modulate proto-oncogene
   mRNA stability was tested by measuring c-Sos mRNA half-life. It was
   observed that c-fos mRNA half-life was relatively short (approximate to
   14-18 min) and that angiotensin II and PMA markedly stabilized mRNA,
   increasing its half-life (approximate to 4-fold and approximate to
   2-fold, respectively). The protein synthesis inhibitor cycloheximide
   increased mRNA stability to a much greater extent. Our results clearly
   demonstrate that angiotensin II and PMA increased c-Sos mRNA
   accumulation in liver cells through two actions: induction of c-Sos gene
   transcription and increase in mRNA stability.}},
