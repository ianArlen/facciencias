Title = {{Arsenic pollution of the groundwater of Zimapan, Mexico, and its health
   impact}},
Booktitle = {{INTERNATIONAL CONFERENCE ON ENVIRONMENTAL POLLUTION, THE PROCEEDINGS:
   VOLS 1 AND 2}},
Year = {{1996}},
Pages = {{579-583}},
