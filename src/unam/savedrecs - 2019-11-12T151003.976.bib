@article{ ISI:A1996TX76000008,
Author = {Bertrand, F and Tanguy, PA and BritoDelaFuente, E},
Title = {{A new perspective for the mixing of yield stress fluids with anchor
   impellers}},
Journal = {{JOURNAL OF CHEMICAL ENGINEERING OF JAPAN}},
Year = {{1996}},
Volume = {{29}},
Number = {{1}},
Pages = {{51-58}},
Month = {{FEB}},
Abstract = {{The mixing of yield stress fluids with an anchor impeller is
   investigated numerically in the laminar regime using a three-dimensional
   finite element method, First, it is shown that the flow behavior is
   strongly influenced by the level of viscoplasticity and that mixing is
   rather inefficient in such a case, Next, correlations of the power
   number versus the Reynolds number are presented for different values of
   the Bingham number, In particular, it is established that the
   Metzner-Otto concept remains valid for fluid exhibiting a yield stress
   and that the Metzner and Otto constant (K-s) varies slightly over a wide
   range of Bingham numbers (Bi less than or equal to 7500). Finally, the
   25-year old experimental findings of Nagata are reviewed and clarified.}},
