@article{ ISI:A1996WR23400006,
Author = {Eckertson, FW and Ohta, H},
Title = {{Weak pseudocompactness and zero sets in pseudocompact spaces}},
Journal = {{HOUSTON JOURNAL OF MATHEMATICS}},
Year = {{1996}},
Volume = {{22}},
Number = {{4}},
Pages = {{737-748}},
Abstract = {{We prove (1) if Y contains a zero set of the form X+omega such that
   every open cover of X of cardinality at most delta has a subcover of
   cardinality less than b, then Y is not pseudocompact; (2) for a limit
   ordinal gamma, the space gamma+omega embeds as a zero set in a
   pseudocompact space if and only if there is a less than or equal
   to{*}-unbounded, <{*}-increasing cf(gamma)-sequence in (omega)omega; (3)
   if a discrete space D embeds as a zero set in a pseudocompact space
   containing a dense set of isolated points then
   \textbackslash{}D\textbackslash{} greater than or equal to a; (4) there
   is a weakly pseudocompact space that cannot be embedded as a zero set in
   a pseudocompact space; (5) Lindelof G(delta) subsets of weakly
   pseudocompact spaces are Cech-complete; (6) a non-compact product of a
   Lindelof space and a Cech-complete Lindelof space is not weakly
   pseudocompact.}},
