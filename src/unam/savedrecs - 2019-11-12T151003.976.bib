@article{ ISI:A1996TX77800007,
Author = {ElizaldeTorres, J and Zironi, EP and TorresVillasenor, G and Cota, LS
   and Banos, L and Rickards, J and Saniger, JM},
Title = {{Corrosion of a zinc-aluminium-copper alloy by fluorine gas}},
Journal = {{MATERIALS LETTERS}},
Year = {{1996}},
Volume = {{26}},
Number = {{1-2}},
Pages = {{41-45}},
Month = {{JAN}},
Abstract = {{Corrosion of a commercial two-phase eutectoid zinc-aluminium alloy
   modified by 2\% Cu (Zinalco(TM)) was performed at high temperature in a
   dry fluorine environment. This environment was kept at a pressure of
   10(3) mbar for 100 and 300 h. The exposed samples were studied by
   resonant nuclear reaction, Auger electron spectroscopy, scanning
   electron microscopy and X-ray diffraction. The Knoop micro-hardness of
   the samples was measured before and after the exposure. Intergranular
   corrosion of the zinc-rich phase that tends to consume the material is
   observed, while the aluminium bonds to the fluorine forming a
   superficial AlF3 compound.}},
