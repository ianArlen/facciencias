@article{ ISI:A1996TY02900014,
Author = {Gopar, VA and Martinez, M and Mello, PA and Baranger, HU},
Title = {{The invariant measure for scattering matrices with block symmetries}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{1996}},
Volume = {{29}},
Number = {{4}},
Pages = {{881-888}},
Month = {{FEB 21}},
Abstract = {{We find the invariant measure for two new types of S matrices relevant
   for chaotic scattering from a cavity in a waveguide. The S matrices
   considered can be written as a 2 x 2 matrix of blocks, each of rank N,
   in which the two diagonal blocks are identical and the two off-diagonal
   blocks are identical. The S matrices are unitary; in addition, they may
   be symmetric because of time-reversal symmetry. The invariant measure,
   with and without the condition of symmetry, is given explicitly in terms
   of the invariant measures for the well known circular unitary and
   orthogonal ensembles. Some implications are drawn for the resulting
   statistical distribution of the transmission coefficient through a
   chaotic cavity.}},
