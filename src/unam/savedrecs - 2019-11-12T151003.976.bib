@article{ ISI:A1996UK07100014,
Author = {Marin, Y and FernandezMas, R and Cuellar, A and DelaPena, A and
   FernandezG, JM and Banos, G},
Title = {{Uterotrophic activity and effect on the contractility of the isolated
   rat myometrium of three synthetic alkanol-amino estrogens: Etolame,
   buolame and pentolame}},
Journal = {{MEDICAL SCIENCE RESEARCH}},
Year = {{1996}},
Volume = {{24}},
Number = {{4}},
Pages = {{247-249}},
Month = {{APR}},
Abstract = {{Three alkanol-emino estrogens have been synthesised and their effects on
   mouse's uterus in vivo as well as on isolated rat myometrium rings in
   vitro have been investigated. Ovariectomised juvenile female mice had
   two injections of either solvent, estradiol or one of the compounds
   etolame or pentolame. This last product was chosen for comparison only,
   as pentolame has already been described and found to be active. Enolame
   had estrogenic activity in vivo, which was nearly twice as large as
   pentolame. In vitro experiments were carried out in rings of isolated
   myometrium obtained from normal female rats in diestrus. A volume of 30
   mu L vehicle (DMSO) which would not produce any changes in the
   spontaneous contractions of the rings was used to dilute the compounds
   assayed. Their inhibiting effect varied, according to the concentration
   used, but it was comparable for the three synthetic compounds and
   significantly less than that observed with estradiol.}},
