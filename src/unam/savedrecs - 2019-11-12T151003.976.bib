@article{ ISI:A1996UC17900003,
Author = {Salas, L},
Title = {{Variable separation in curvature sensing: Fast method for solving the
   irradiance transport equation in the context of optical telescopes}},
Journal = {{APPLIED OPTICS}},
Year = {{1996}},
Volume = {{35}},
Number = {{10}},
Pages = {{1593-1596}},
Month = {{APR 1}},
Abstract = {{A method to evaluate wave-front aberrations in optical telescopes that
   is based on the method of curvature sensing but that solves the
   irradiance transport equation by variable separation is presented. This
   technique is simpler for processing than are previously released
   techniques and can perform more efficiently, as is required by active
   and adaptive optics. Testing for consistency of the method by evaluation
   of several sets of out-of-focus images obtained with the 2-m telescope
   at the Universidad Nacional Autonoma de Mexico was carried out, and a
   stability of 10\% for the derived values of Zernike coefficients was
   found.}},
