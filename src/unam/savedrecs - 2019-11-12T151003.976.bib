@article{ ISI:A1996TZ87900027,
Author = {Persi, P and Roth, M and Tapia, M and Marenzi, AR and Felli, M and
   Testi, L and FerrariToniolo, M},
Title = {{Shocked molecular hydrogen emission in the bipolar outflow NGC 6334 I}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{1996}},
Volume = {{307}},
Number = {{2}},
Pages = {{591-598}},
Month = {{MAR}},
Abstract = {{Sub-arcsec images in the J, H, K and H-2 and nearby continuum of the
   bipolar outflow NGC 6334 I are presented. Three knots of Hz emission are
   found which coincide with recently discovered NH3(3,3) masers. Two of
   these are aligned along the CO bipolar outflow, indicating that the
   hydrogen molecular emission is shock excited. We have also detected a
   loop-shaped infrared nebula extending to the east of the massive
   mid-infrared source IRS 1 which illuminates the reflection nebula. IRS1
   consists of at least four components in the near-infrared. The
   interaction of the stellar wind from these young stellar objects with
   the surrounding medium could excite the third H-2 emission knot and
   associated ammonia maser.}},
