@article{ ISI:A1996TZ27100003,
Author = {Dietz, B and Lombardi, M and Seligman, TH},
Title = {{Parabolic manifolds in the scattering map and direct quantum processes}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{1996}},
Volume = {{29}},
Number = {{5}},
Pages = {{L95-L99}},
Month = {{MAR 7}},
Abstract = {{We analyse the quantum effects of parabolic manifolds in Jung's iterated
   scattering map. For this purpose we consider the classical map proposed
   previously to be the exact classical analogue of Rydberg molecules
   calculated with the approximations relevant to the multichannel quantum
   defect theory for energies above the ionization threshold. The part
   corresponding to positive electron energies can be viewed as a Jung
   scattering map without the trivial direct processes. This map contains a
   parabolic manifold of Axed points which gives rise to a regular series
   of quantum states which behave very much like eigenchannels that miss
   the target.}},
