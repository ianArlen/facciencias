@article{ ISI:A1996TR77100004,
Author = {Garcia, IP},
Title = {{Major cold air outbreaks affecting coffee and citrus plantations in the
   eastern and northeastern Mexico}},
Journal = {{ATMOSFERA}},
Year = {{1996}},
Volume = {{9}},
Number = {{1}},
Pages = {{47-68}},
Month = {{JAN 1}},
Abstract = {{Several cases of the synoptic situation associated with intense frosts,
   which has affected the plantations of coffee and citrus in eastern and
   northeastern Mexico, are analyzed; these events also damaged citrus and
   vegetables in Texas, Louisiana and Florida. The origin and evolution of
   the anticyclonic circulations associated with the polar air mass, are
   traced. These originate during a blocking situation of the westerlies in
   the Northeastern Pacific with a confluence zone close to Alaska, and an
   intense cyclogenesis in the northeast of United States. The major cold
   air outbreak over the ease, northeast and Central Mexican Plateau is
   observed when an anticyclone associated with the polar air covers
   southeastern United States, Gulf of Mexico and the easthern Mexico.}},
