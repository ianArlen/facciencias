@article{ ISI:A1996TM13600021,
Author = {ValdesMartinez, J and Enriquez, A and Cabrera, A and EspinosaPerez, G},
Title = {{C-H center dot center dot center dot O hydrogen-bonding interactions as
   packing tools. Crystal structure of
   {[}W(CO)(5)(acetone-4-methylthiosemicarbazone)]}},
Journal = {{POLYHEDRON}},
Year = {{1996}},
Volume = {{15}},
Number = {{5-6}},
Pages = {{897-901}},
Month = {{MAR}},
Abstract = {{The crystal structure of the compound {[}W(CO)(5)(Ac4MeTSC)], Ac4MeTSC =
   acetone-4-methylthiosemicarbazone, was determined by single-crystal
   X-ray diffraction. The geometry around the tungsten atom is
   approximately octahedral with five CO ligands and the Ac4MeTSC
   coordinating as a monodentate ligand via the sulphur atom. It is
   possible to rationalize both the geometry and position of the
   coordinated Ac4MeTSC considering the intramolecular hydrogen bonds
   involved. Intermolecular C-H...O bonds help to construct a
   macromolecular three-dimensional structure.}},
