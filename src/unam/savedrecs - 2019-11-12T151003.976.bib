@inproceedings{ ISI:000071918900039,
Author = {Martinez, L and Amaya, M and Flores, O and Lawrynowics, D and Lavernia,
   E},
Editor = {{Earthman, JC and Mohamed, FA}},
Title = {{High temperature fracture during forging of FeAl intermetallic alloys
   fabricated using spray atomization and alumina particulate}},
Booktitle = {{CREEP AND FRACTURE OF ENGINEERING MATERIALS AND STRUCTURES}},
Year = {{1996}},
Pages = {{395-404}},
Note = {{7th International Conference on Creep and Fracture of Engineering
   Materials and Structures, UNIV CALIF IRVINE, IRVINE, CA, AUG 10-15, 1997}},
Organization = {{Minerals Met \& Mat Soc; Univ Calif Irvine}},
Abstract = {{An analysis is made of the high temperature fracture during forging of
   boron doped iron aluminum intermetallic compounds processed by spray
   atomization and deposition with the addition of alumina particulate.
   Crack formation and fracture modes during forging of FeAl40at\%
   intermetallics at 1373K exhibit mixed intergranular and transgranular
   fracture. We found a clear influence of boron in the transition from
   intergranular to transgranular fracture. This was expressed both in
   predominant intergranular fracture in boron free samples and due to the
   reduction of grain boundary concentration of boron in fine grained
   materials. The addition of alumina particulate reduces brittleness
   during forging.}},
