@article{ ISI:000325821200009,
Author = {Gonzalez-Fernandez, B. and Camacho, A.},
Title = {{Speed of Sound of a Bose-Einstein Condensate with Dipole-Dipole
   Interactions}},
Journal = {{JOURNAL OF LOW TEMPERATURE PHYSICS}},
Year = {{2013}},
Volume = {{173}},
Number = {{5-6}},
Pages = {{343-353}},
Month = {{DEC}},
Abstract = {{In the present work the case of a chromium Bose-Einstein condensate is
   considered. The model includes not only the presence of the so-called
   contact interaction but also a long range and anisotropic dipole-dipole
   interaction has been included. Some thermodynamical properties are
   analyzed. For instance, the size of the condensate, chemical potential,
   speed of sound, number of particles, etc., are deduced. It will be shown
   that this dipole-dipole interaction implies the emergence of anisotropy,
   for example, in the speed of sound. The possible use of this anisotropy
   as a tool for the analysis of dissipative mechanisms, for instance,
   Landau's criterion for superfluidity, will be also discussed.}},
