@article{ ISI:000327810800019,
Author = {Hartzell, Stephen and Mendoza, Carlos and Zeng, Yuehua},
Title = {{Rupture model of the 2011 Mineral, Virginia, earthquake from teleseismic
   and regional waveforms}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2013}},
Volume = {{40}},
Number = {{21}},
Pages = {{5665-5670}},
Month = {{NOV 16}},
Abstract = {{We independently invert teleseismic P waveforms and regional crustal
   phases to examine the finite fault slip model for the 2011 M-w 5.8
   Mineral, Virginia, earthquake. Theoretical and empirical Green's
   functions are used for the teleseismic and regional models,
   respectively. Both solutions show two distinct sources each about 2km
   across and separated by 2.5km. The source at the hypocenter is more
   localized in the regional model leading to a higher peak slip of 130cm
   and higher average stress drop of 250 bars compared with 86cm and 150
   bars for the same source in the teleseismic model. Both sources are
   centered at approximately 8km depth in the regional model, largely below
   the aftershock distribution. In the teleseismic model, the sources
   extend updip to approximately 6km depth, into the depth range of the
   aftershocks. The rupture velocity is not well resolved but appears to be
   near 2.7km/s.}},
