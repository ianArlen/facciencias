@article{ ISI:000330109500007,
Author = {Medina-Franco, Jose L. and Aguayo-Ortiz, Rodrigo},
Title = {{Progress in the Visualization and Mining of Chemical and Target Spaces}},
Journal = {{MOLECULAR INFORMATICS}},
