@article{ ISI:000327592400012,
Author = {Fuhr, J. D. and Ventura, C. I. and Barrio, R. A.},
Title = {{Formation of non-substitutional beta-Sn defects in Ge1-xSnx alloys}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2013}},
Volume = {{114}},
Number = {{19}},
Month = {{NOV 21}},
Abstract = {{Although group IV semiconductor alloys are expected to form
   substitutionally, in Ge1-xSnx this is true only for low concentrations
   (x < 0.13). The use of these alloys as a narrow gap semiconductor
   depends on the ability to produce samples with the high quality required
   for optoelectronic device applications. In a previous paper, we proposed
   the existence of a non-substitutional complex defect (beta-Sn),
   consisting of a single Sn atom in the center of a Ge divacancy, which
   may account for the segregation of Sn at large x. Afterwards, the
   existence of this defect was confirmed experimentally. In this paper we
   study the local environment and the interactions of the substitutional
   defect (alpha-Sn), the vacancy in Ge, and the beta-Sn defect by
   performing extensive numerical ab initio calculations. Our results
   confirm that a beta-Sn defect can be formed by natural diffusion of a
   vacancy around the substitutional alpha-Sn defect, since the energy
   barrier for the process is very small. (C) 2013 AIP Publishing LLC.}},
