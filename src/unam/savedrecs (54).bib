@article{ ISI:000324973000009,
Author = {Galeana-Sanchez, Hortensia and Hernandez-Cruz, Cesar and Alejandro
   Juarez-Camacho, Manuel},
Title = {{On the existence and number of (k + 1)-kings in k-quasi-transitive
   digraphs}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2013}},
Volume = {{313}},
Number = {{22}},
Pages = {{2582-2591}},
Month = {{NOV 28}},
Abstract = {{Let D = (V(D),A(D)) be a digraph and k >= 2 an integer. We say that D is
   k-quasi-transitive if for every directed path (v(0), v(1), ..., v(k)) in
   D we have (v(0), v(k)) is an element of A(D) or (v(k), v(0)) is an
   element of A(D). Clearly, a 2-quasi-transitive digraph is a
   quasi-transitive digraph in the usual sense.
   Bang-Jensen and Gutin proved that a quasi-transitive digraph D has a
   3-king if and only if D has a unique initial strong component, and if D
   has a 3-king and the unique initial strong component of D has at least
   three vertices, then D has at least three 3-kings. In this paper we
   prove the following generalization: a k-quasi-transitive digraph D has a
   (k + 1)-king if and only if D has a unique initial strong component, and
   if D has a (k + 1)-king, then either all the vertices of the unique
   initial strong components are (k + 1)-kings or the number of (k +
   1)-kings in D is at least (k + 2). Also, we obtain new results on the
   minimum number of 3-kings in quasi-transitive digraphs. (C) 2013
   Elsevier B.V. All rights reserved.}},
