@article{ ISI:000327146100002,
Author = {Garcia-Calderon, Gaston and Maldonado, Irene and Villavicencio, Jorge},
Title = {{Time-domain resonances and the ultimate fate of a decaying quantum state}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2013}},
Volume = {{88}},
Number = {{5}},
Month = {{NOV 14}},
Abstract = {{We derive an analytical expression for the propagation of a quantum
   particle at asymptotic long distances and times from the potential where
   the particle was initially confined. We obtain that the particle is
   described by an evolving resonance in time domain that possesses a
   peaked shape characterized by the resonance parameters of the dominant
   resonance coefficient of the decaying wave function. The above situation
   corresponds to an unexplored postexponential regime that represents the
   ultimate fate of a decaying particle.}},
