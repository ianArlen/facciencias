Title = {{IMMUNOLOGICAL EVALUATION OF CAPTIVE GREEN SEA TURTLE (CHELONIA MYDAS)
   WITH ULCERATIVE DERMATITIS}},
Journal = {{JOURNAL OF ZOO AND WILDLIFE MEDICINE}},
Year = {{2013}},
Volume = {{44}},
