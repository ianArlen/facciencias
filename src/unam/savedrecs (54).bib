Language = {{English}},
Affiliation = {{Solis, M (Reprint Author), Univ Nacl Autonoma Mexico, Inst Energias Renovables, Apartado Postal 34, Temixco 62580, Morelos, Mexico.
   Solis, Mauricio; Rincon, Marina E.; Calva, Julio C.; Alvarado, German, Univ Nacl Autonoma Mexico, Inst Energias Renovables, Temixco 62580, Morelos, Mexico.}},
DOI = {{10.1016/j.electacta.2013.08.165}},
