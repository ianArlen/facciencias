@article{ ISI:000326193200023,
Author = {Jardinez, Christiaan and Nicolas-Vazquez, Ines and Cruz-Borbolla, Julian
   and Gonzalez-Ramirez, Cesar A. and Cepeda, Miguel and Correa-Basurto,
   Jose and Pandiyan, Thangarasu and Miranda, Rene},
Title = {{DFT and docking studies of rhodostreptomycins A and B and their
   interactions with solvated/nonsolvated Mg2+ and Ca2+ ions}},
Journal = {{JOURNAL OF MOLECULAR MODELING}},
Year = {{2013}},
Volume = {{19}},
