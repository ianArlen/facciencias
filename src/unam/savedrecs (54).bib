@article{ ISI:000328754500030,
Author = {Zharikov, S. V. and Miroshnichenko, A. S. and Pollinann, E. and Danford,
   S. and Bjorkman, K. S. and Morrison, N. D. and Favaro, A. and Guano Flo,
   J. and Terry, J. N. and Desnoux, V. and Garrel, T. and Martineau, G. and
   Buchet, Y. and Ubaud, S. and Mauclaire, B. and Kalbertnatten, H. and
   Buil, C. and Sawicki, C. J. and Blank, T. and Garde, O.},
Title = {{Doppler tomography of the circumstellar disk of pi Aquarii}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{2013}},
Volume = {{560}},
Month = {{DEC}},
Abstract = {{Aims. The work is aimed at studying the circumstellar disk of the bright
   classical binary Be star pi Aqr.
   Methods. We analysed variations of a double-peaked profile of the
   H-alpha emission line in the spectrum of pi Aqr that was observed in
   many phases during similar to 40 orbital cycles in 2004-2013. We applied
   the discrete Fourier transform (DFT) method to search for periodicity in
   the peak intensity ratio (V/R). Doppler tomography was used to study the
   structure of the disk around the primary.
   Results. The dominant frequency in the power spectrum of the H-alpha V/R
   ratio is 0.011873 day(-1), which corresponds to a period of 84.2(2) days
   and agrees with the earlier determined orbital period of the system,
   P-orb = 84.1 days. The V/R shows a sinusoidal variation that is
   phase-locked with the orbital period. Doppler maps of all our spectra
   show a non-uniform structure of the disk around the primary: a ring with
   the inner and outer radii at V-in approximate to 450 km s(-1) and V-out
   approximate to 200 km s(-1), respectively, along with an extended stable
   region (spot) at V-x approximate to 225 km s(-1) and V-y approximate to
   100 km s(-1). The disk radius of approximate to 65 R-circle dot = 0.33
   AU was estimated by assuming Keplerian motion of a particle on a
   circular orbit at the disk's outer edge.}},
