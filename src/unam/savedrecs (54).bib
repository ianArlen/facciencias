@article{ ISI:000329378800008,
Author = {Moreno-Fierros, Leticia and Lilia Garcia-Hernandez, Ana and
   Ilhuicatzi-Alvarado, Damaris and Rivera-Santiago, Lucio and
   Torres-Martinez, Marilu and Rubio-Infante, Nestor and Legorreta-Herrera,
   Martha},
Title = {{Cryl Ac protoxin from Bacillus thuringiensis promotes macrophage
   activation by upregulating CD80 and CD86 and by inducing IL-6, MCP-1 and
   TNF-alpha. cytokines}},
Journal = {{INTERNATIONAL IMMUNOPHARMACOLOGY}},
Year = {{2013}},
Volume = {{17}},
Number = {{4, SI}},
Pages = {{1051-1066}},
Month = {{DEC}},
Abstract = {{Bacillus thuringiensis CrylAc protoxin (pCrylAc) is a promising mucosal
   adjuvant but its action mechanism is unknown. We examined in vivo
   whether pCrylAc promotes the activation of macrophages in the
   peritoneum, spleen and mesenteric lymph nodes or in the lungs and
   bronchoalveolar lavage after intraperitoneal or intranasal pCrylAc
   administration, respectively, in BALB/c mice. pCrylAc upregulated the
   costimulatory molecules CD80 and CD86 in these macrophages, but with
   distinct kinetics. In vitro stimulation of resident macrophages with
   pCrylAc upregulated CD80 and CD86 and enhanced the production of the
   pro-inflammatory cytokines INF-alpha, IL-6 and MCP-1. To investigate
   whether the pCiylAc-induced activation was mediated through MAPK
   pathways, we pretreated RAW 264.7 cells with signaling inhibitors of
   MEK, JNK and p38 MAPKs (PD98059, SP600125 and SB203580, respectively).
   pCrylAc-induced upregulation of CD86 and CD80 was partially inhibited by
   the MEK inhibitor. While LPS-induced upregulation mechanisms of CD80 and
   CD86 appear to be different; as these were particularly inhibited by MEK
   and JNK inhibitors, respectively. pCrylAc-induced IL-6 and MCP-1
   production was especially inhibited with the p38 MAPK inhibitor, whereas
   TNF-a was only slightly inhibited upon treatment with JNK and p38 MAPK
   inhibitors. Therefore macrophage stimulation with pCrylAc induced the
   upregulation of CD80 and CD86, and the production of IL-6, TNF-alpha and
   MCP-1, possibly, through the MEK and p38 MAPK pathways. It also promoted
   the nuclear translocation of NF-KB p50 and p65, the upregulation of
   MHC-II, and the activation of T CD4+ cells. These results suggest that
   pCrylAc induced macrophage activation through mechanisms which differ
   partially from the LPS-induced. (C) 2013 Elsevier BM. All rights
   reserved.}},
Publisher = {{ELSEVIER}},
Address = {{RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
