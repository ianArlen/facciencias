@article{ ISI:000330162100003,
Author = {House, Paul R. and Gomez-Hinostrosa, Carlos and Hernandez, Hector M.},
Title = {{A new species of Peniocereus (Cactaceae) from Honduras}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2013}},
Volume = {{84}},
Number = {{4}},
Pages = {{1077-1081}},
Month = {{DEC}},
Abstract = {{Peniocereus canoensis, a new cactus species from the vicinity of
   Tegucigalpa, Honduras, is described and illustrated. It is easily
   distinguished from the remaining species in the genus by the following
   combination of morphological characters: monomorphic, green-glaucous
   stems, and deep-pink, diurnal flowers. The species probably is
   critically endangered because it is restricted to a small, subtropical
   dry forest area and because only 6 individuals have been detected in its
   natural habitat.}},
