@article{ ISI:000328253500004,
Author = {Carrillo-Reyes, Pablo and Sahagun-Godinez, Eduardo and Ibarra-Manriquez,
   Guillermo},
Title = {{The genus Disciphania (Menispermaceae: Tinosporoideae, Tinosporeae) in
   Mexico}},
Journal = {{BRITTONIA}},
Year = {{2013}},
Volume = {{65}},
Number = {{4}},
Pages = {{439-451}},
Month = {{DEC}},
Abstract = {{A revision is presented for the four Mexican species of the genus
   Disciphania: D. cardiophylla, D. caloparpa, D. mexicana, and D.
   spadicea. The latter represents a new record for Mexico, while D.
   mexicana is reported for the first time in the Mexican states of
   Michoacan and Sinaloa. Additionally, we report the occurrence of the
   monoecious condition in D. spadicea, which represents the first report
   of this phenomenon for the genus. A key to identify the species,
   descriptions, illustrations, distribution maps, and assessments of the
   conservation status based on the IUCN criteria are provided.}},
