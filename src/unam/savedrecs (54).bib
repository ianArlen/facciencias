@article{ ISI:000328452900015,
Author = {Kelly-Perez, Ismael and Bruce, Neil C. and Berriel-Valdos, Luis R. and
   Werner, Annette and Delgado Atencio, Jose A.},
Title = {{Computational model of the effect of light scattering from cataracts in
   the human eye}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA A-OPTICS IMAGE SCIENCE AND
   VISION}},
Year = {{2013}},
Volume = {{30}},
Number = {{12}},
Pages = {{2585-2594}},
Month = {{DEC}},
Abstract = {{A model of the human eye has been developed, including scattering from
   cataracts inside the nucleus of the lens. The cataracts are modeled as
   spherical particles with refractive index different from that of the
   surrounding lens medium. Scattering from the retina is also included in
   the simulations. Variations of scattering particle diameter, number of
   particles, and wavelength of the illuminating light are investigated. It
   is shown that particle size is the most important parameter affecting
   the scattered light, and that the scattering from the retina can mask
   the effect of the scattering particles, for some range of the
   parameters. (C) 2013 Optical Society of America}},
Publisher = {{OPTICAL SOC AMER}},
Address = {{2010 MASSACHUSETTS AVE NW, WASHINGTON, DC 20036 USA}},
