@article{ ISI:000327681100014,
Author = {Montiel Gonzalez, M. and Hinojosa Palafox, J. and Estrada Gasca, C.},
Title = {{Numerical study of the Boussinesq approach validity for natural
   convection and surface thermal radiation in an open cavity}},
Journal = {{REVISTA MEXICANA DE FISICA}},
