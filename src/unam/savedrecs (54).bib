@article{ ISI:000328742300010,
Author = {Organista-Juarez, Diana and Carretero-Ortega, Jorge and Vicente-Fermin,
   Onasis and Vazquez-Victorio, Genaro and Sosa-Garrocho, Marcela and
   Vazquez-Prado, Jose and Macias-Silva, Marina and Reyes-Cruz, Guadalupe},
Title = {{Calcium-Sensing Receptor Inhibits TGF-beta-signaling by Decreasing Smad2
   Phosphorylation}},
Journal = {{IUBMB LIFE}},
Year = {{2013}},
Volume = {{65}},
Number = {{12}},
Pages = {{1035-1042}},
Month = {{DEC}},
Abstract = {{Calcium-sensing receptor (CaSR) contributes to maintain homeostatic
   levels of extracellular calcium. In addition, CaSR controls other
   cellular activities such as proliferation and migration, particularly in
   cells not related to extracellular calcium homeostasis, potentially by
   cross-talking with parallel signaling pathways. Here we report that CaSR
   attenuates transforming growth factor- (TGF-)-signaling in hepatic C9
   cells and in transfected HEK293 cells. Wild type CaSR interferes with
   TGF--dependent Smad2 phosphorylation and induces its proteasomal
   degradation, resulting in a decrease of TGF--dependent transcriptional
   activity, whereas an inactivating CaSR mutant does not transduce an
   inhibitory effect of extracellular calcium on TGF- signaling.
   Attenuation of TGF- signaling in response to extracellular calcium is
   linked to Rab11-dependent CaSR-trafficking with the intervention of CaSR
   carboxyl-terminal tail. Our data suggest that CaSR might regulate
   TGF--dependent cellular responses mediated by TGF- signaling inhibition.
