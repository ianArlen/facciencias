@article{ ISI:000328678600013,
Author = {Villavicencio, J. and Maldonado, I. and Cota, E. and Platero, G.},
Title = {{Spin-orbit effects in a triple quantum dot shuttle}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2013}},
Volume = {{88}},
Number = {{24}},
Month = {{DEC 10}},
Abstract = {{Spin transport properties of a triple quantum dot shuttle system are
   analyzed in the presence of spin-orbit interaction. We calculate the
   spin current through the device as a function of the bias voltage
   detuning. We find that new resonances in the current through the shuttle
   appear due to the presence of spin-orbit interaction, and the states
   involved in the conduction process are identified. Also, spin current
   polarization is analyzed as a function of tunnel coupling and detuning,
   and conditions are found where the device can act as a spin filter.}},
