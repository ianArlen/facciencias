@article{ ISI:000406253200007,
Author = {Seyler Mancilla, Alberto},
Title = {{CATASTROPHISM AND PHANTOM LIMB PAIN: A DESCRIPTIVE LONGITUDINAL STUDY}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2017}},
Volume = {{34}},
Number = {{2}},
Pages = {{142-147}},
Month = {{JUL-DEC}},
Abstract = {{Phantom limb pain refers to pain in an amputated limb; its prevalence
   reaches 90\%. Catastrophizing is the tendency to magnify the threat
   value of a painful stimulus. It has been characterized as a predictor of
   pain-related outcomes. There are contradictions in the literature and
   most studies are cross-sectional. The present study aimed at
   investigating whether phantom limb pain and catastro-phizing describe
   parallel trajectories over time. The sample was comprised of 29 amputees
   with ages between 20 and 86, who completed the Visual Analog Scale of
   pain and the Pain Catastrophizing Scale-Spanish version. A tendency to
   decrease the perception of pain along the successive measures was
   observed. Catastrophizing decreased in the immediate postoperative
   period, but increased 3 months later.}},
