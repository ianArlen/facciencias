@article{ ISI:000405599900012,
Author = {Geuenich, Jan and Labardini-Fragoso, Daniel},
Title = {{Species with potential arising from surfaces with orbifold points of
   order 2, part I: one choice of weights}},
Journal = {{MATHEMATISCHE ZEITSCHRIFT}},
