@article{ ISI:000407370200030,
Author = {Viera-Gonzalez, Perla M. and Sanchez-Guerrero, Guillermo E. and
   Martinez-Guerra, Edgar and Ceballos-Herrera, Daniel E.},
Title = {{Design of a segmented nonimaging Fresnel dome for nontracking solar
   collection}},
Journal = {{OPTICAL ENGINEERING}},
Year = {{2017}},
Volume = {{56}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{The efficiency of sunlight collection systems is related to the optical
   element used as a collector. On this subject, the design of a
   nontracking solar collector that consists of a segmented nonimaging
   Fresnel dome is presented. It is formed by the conjunction of different
   zones for solar collection, where each one is a nonimaging Fresnel lens
   that collects a specific angular range (theta(in)) of sunlight received
   in the northeast of Mexico, but the methodology presented can be easily
   extended to other geographic locations. The final design is a
   semistationary segmented collector with a 100-cm diameter and 50-cm
   focal length that needs a 180-deg rotation over the XY-plane in each
   equinox. The numerical simulations show that the nontracking segmented
   collector has a combined acceptance semiangle of theta(in) = +/- 105 deg
   with an average efficiency of over 67\% from 9:00 to 18:00 h. The
   spatial and angular distributions of the sunlight collected are also
   included. This design has a collection area equal to that of a single
   nonimaging Fresnel lens with an acceptance semiangle of theta(in) = +/-
   45 deg. These results are reproducible and provide valuable data for
   designing nontracking solar collectors based on nonimaging Fresnel lens.
   (C) 2017 Society of Photo-Optical Instrumentation Engineers (SPIE)}},
