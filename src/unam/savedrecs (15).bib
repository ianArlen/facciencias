@article{ ISI:000405129000002,
Author = {Mora, O. and Alvarez-Alonso, C. and Perez-Serrano, R. and
   Gonzalez-Davalos, L. and Vargas-Chavez, C. and Shimada, A. and Pina, E.},
Title = {{Lipoic acid promotes the down-regulation of hepatic gene expression in
   broilers}},
Journal = {{EUROPEAN POULTRY SCIENCE}},
Year = {{2017}},
Volume = {{81}},
Month = {{JUL 6}},
Abstract = {{It is known that supplementation with 40 mg/kg of lipoic acid (ALA)
   during the first 7 weeks of life significantly improve feed conversion
   in broilers. In this work, 7-week-old broilers raised on a commercial
   farm were fed on a diet with or without 40 mg/kg of ALA, livers were
   obtained, and the total levels and ratios of DNA, RNA and proteins were
   assessed. Messenger RNA (mRNA) was sequenced and mapped for the
   identification of differentially expressed genes, and the mRNAs encoding
   for certain selected proteins were analysed by quantitative PCR. The
   present results revealed that ALA treatment did not change the total DNA
   level or the DNA: protein ratio (P>0.1), but it did increase the RNA:
   DNA ratio (P<0.05). Liver RNA-Sequencing (RNA-Seq) analysis of
   differential expression identified 8 genes that showed a Log2 fold
   change (LogFC) in ALA-treated birds: avidin-related protein 2 (AVR2),
   avidin (AVD), crystallin beta-A2 (CRYBA2), gene decoy receptor 3 (DcR3),
   histone H2A-IV, LOC420789, aldose reductase (AR), and aldose
   reductase-like protein (ARL). In the same samples, the mRNA expression
   levels were measured for the genes encoding 4 enzymes involved in energy
   metabolism {[} Glutathione S-transferase theta 1 (GSTT1), oxoglutarate
   (alpha-ketoglutarate) dehydrogenase (OGDH), pyruvate dehydrogenase
   (lipoamide) alpha 1 (PDHA1) and dihydrolipoamide S-succinyltransferase
   (DLST)], as well as those encoding sirtuin 1 and 3. All of these levels
   showed significant decreases in ALA-treated broilers compared to
   untreated controls (P<0.01). These results indicate that the responses
   of broilers to ALA are associated with the down-regulation of certain
   liver enzymes, especially those involved in glucose metabolism and the
   tricarboxylic acid (TCA) cycle.}},
