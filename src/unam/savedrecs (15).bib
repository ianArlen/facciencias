@article{ ISI:000406183300073,
Author = {Guo, P. and Danilkin, I. V. and Fernandez-Ramirez, C. and Mathieu, V.
   and Szczepaniak, A. P.},
Title = {{Three-body final state interaction in eta -> 3 pi updated}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2017}},
Volume = {{771}},
Pages = {{497-502}},
Month = {{AUG 10}},
Abstract = {{In view of the recent high-statistic KLOE data for the eta -> pi(+)
   pi(-) pi(0) decay, a new determination of the quark mass double ratio
   has been done. Our approach relies on a dispersive model that takes into
   account rescattering effects between three pions via subenergy
   unitarity. The latter is essential to reproduce the Dalitz plot
   distribution. A simultaneous description of the KLOE and WASA-at-COSY
   data is achieved in terms of just two real parameters. From a global
   fit, we determine Q = 21.6 +/- 1.1. The predicted slope parameter for
   the neutral channel alpha = -0.025 +/- 0.004 is in reasonable agreement
   with the PDG average value. (C) 2017 The Authors. Published by Elsevier
   B.V.}},
