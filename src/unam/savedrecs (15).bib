@article{ ISI:000404982300002,
Author = {Luis Sanchez-Salgado, Jose and Ali Pereyra, Mohamed and Agundis,
   Concepcion and Vivanco-Rojas, Oscar and Sierra-Castillo, Claudia and
   Jose Alpuche-Osorno, Juan and Zenteno, Edgar},
Title = {{Participation of lectins in crustacean immune system}},
Journal = {{AQUACULTURE RESEARCH}},
Year = {{2017}},
Volume = {{48}},
Number = {{8}},
Pages = {{4001-4011}},
Month = {{AUG}},
Abstract = {{Several lectins have been found in crustaceans, and it has been
   suggested that they play roles in cell signalling, cell-cell
   interaction, protein synthesis and pathogen recognition. These functions
   are performed through their specificity for carbohydrates. This review
   analyses the carbohydrate specificity, tissue distribution and
   participation in the immune responses of lectins in crustaceans.
   Furthermore, we explore some data showing that lectin expression seems
   to be stimulated by pathogens, favouring crustacean survival.}},
