@article{ ISI:000417645000012,
Author = {Garcia Gutierrez, Ivo and Lopez Lopez, Alvaro},
Title = {{Cuatrocienegas: tourism-borne conflicts in a Natural Protected Area}},
Journal = {{CUADERNOS DE TURISMO}},
Year = {{2017}},
Number = {{40}},
Pages = {{295-314}},
Month = {{JUL-DEC}},
Abstract = {{In the Wildlife Protected Area of Cuatrocienegas, Coahuila, one of the
   most visited protected areas in Mexico, tourism-borne conflicts were
   analyzed between 2008 and 2014 from a spatial perspective, combining
   participant observation with surveys of key players, both locals and
   visitors. The study confirmed the negative effects of tourism on the
   physical environment as well as on the local economy and social
   dynamics, a fact that seems to go unnoticed by locals as well as
   tourists. Although they are aware that they have entered an NPA,
   tourists are indifferent to the responsibility their visit entails and
   ignorant of the environmental services the fragile ecosystem provides to
   society in general. For their part, local residents view tourism as a
   road to development, and rarely question the interests of those who
   benefit from the activity.}},
