@article{ ISI:000407011200023,
Author = {Teran-Hinojosa, E. and Sobral, H. and Sanchez-Perez, C. and
   Perez-Garcia, A. and Aleman-Garcia, N. and Hernandez-Ruiz, J.},
Title = {{Differentiation of fibrotic liver tissue using laser-induced breakdown
   spectroscopy}},
