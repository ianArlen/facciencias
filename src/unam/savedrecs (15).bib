@article{ ISI:000406764000027,
Author = {Much, Albert},
Title = {{Curving flat space-time by deformation quantization?}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2017}},
Volume = {{58}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{We use a deformed differential structure to obtain a curved metric by a
   deformation quantization of the flat space-time. In particular, by
   setting the deformation parameters to be equal to physical constants, we
   obtain the Friedmann-Robertson-Walker (FRW) model for inflation and a
   deformed version of the FRWspace-time. By calculating classical
   Einstein-equations for the extended space-time, we obtain non-trivial
   solutions. Moreover, in this framework, we obtain theMoyal-Weyl, i.e., a
   constant noncommutative space-time, as a consistency condition.
   Published by AIP Publishing.}},
