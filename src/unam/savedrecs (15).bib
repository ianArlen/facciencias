@article{ ISI:000406247700002,
Author = {Dammers, Enrico and Shephard, Mark W. and Palm, Mathias and
   Cady-Pereira, Karen and Capps, Shannon and Lutsch, Erik and Strong, Kim
   and Hannigan, James W. and Ortega, Ivan and Toon, Geoffrey C. and
   Stremme, Wolfgang and Grutter, Michel and Jones, Nicholas and Smale, Dan
   and Siemons, Jacob and Hrpcek, Kevin and Tremblay, Denis and Schaap,
