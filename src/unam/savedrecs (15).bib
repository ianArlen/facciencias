@article{ ISI:000407363100001,
Author = {Vincini, Stefano and Jhang, Yuna and Buder, Eugene H. and Gallagher,
   Shaun},
Title = {{Neonatal Imitation: Theory, Experimental Design, and Significance for
   the Field of Social Cognition}},
Journal = {{FRONTIERS IN PSYCHOLOGY}},
