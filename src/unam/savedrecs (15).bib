@article{ ISI:000405696300004,
Author = {Alejandra Cabrera-Reyes, Erika and Limon-Morales, Ofelia and Alejandra
   Rivero-Segura, Nadia and Camacho-Arroyo, Ignacio and Cerbon, Marco},
Title = {{Prolactin function and putative expression in the brain}},
