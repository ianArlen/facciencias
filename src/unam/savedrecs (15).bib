Author = {Sanchez Garcia, Jessica J. and Flores-Alamo, Marcos and Ortiz-Frade,
   Luis and Klimova, Elena I.},
Title = {{Reactions of 2,3-diferrocenylcyclopropenilium salts with
   bis-1,4-N,O-nucleophiles: Novel synthesis, characterization, chemical
   and electrochemical properties of the
   2-(Z-1,2-diferrocenylvinyl)-4,5-dihydrooxazole derivatives}},
Journal = {{JOURNAL OF ORGANOMETALLIC CHEMISTRY}},
Year = {{2017}},
