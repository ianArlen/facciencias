@article{ ISI:000405541600049,
Author = {Sanchez-Sanchez, Roberto and Romero-Montero, Alejandra and Montiel,
   Carmina and Melgarejo-Ramirez, Yaaziel and Sanchez-Ortega, Carmina and
   Lugo-Martinez, Haydee and Cabello-Arista, Beatriz and Garcia-Arrazola,
   Roeb and Velasquillo, Cristina and Gimeno, Miguel},
Title = {{Cytoprotective effect of the enzyme-mediated polygallic acid on
   fibroblast cells under exposure of UV-irradiation}},
Journal = {{MATERIALS SCIENCE \& ENGINEERING C-MATERIALS FOR BIOLOGICAL APPLICATIONS}},
Year = {{2017}},
Volume = {{76}},
Pages = {{417-424}},
Month = {{JUL 1}},
Abstract = {{The poly(gallic acid), produced by laccase-mediated oxidation of gallic
   acid in aqueous media (pH 5.5) to attain a novel material with
   well-defined molecular structure and high water solubility (500 mg/mL at
   25 degrees C), has been investigated to understand its potential
   biological activities. In this regard, a biomedical approach based on
   cytoprotective effect on human fibroblast cells exposed to
   IN-irradiation in the presence of the polymer has been demonstrated. The
   results also shows that 200 mu g/mL of poly(gallic acid) inhibits the
   growth and migration of dermal fibroblasts and cancer cell lines without
   affecting cell viability. Poly(gallic acid) pretreatment with 10 mu g/mL
   protects dermal fibroblasts from UV induced cell death and additionally,
   the cytoprotective effect reduce ROS presence in the cells. This
   property can be correlated with the antioxidant power (IC50 of 23.5 mu
   g/mL) of this novel material, which was ascertained by electronic
   paramagnetic resonance spectroscopy and spectrophotometrically.
   Additionally, the antimicrobial activity of this material was
   corroborated with the inhibition of Staphylococcus aureus (ATCC 25923)
   and Enterococcus faecalis (ATCC 29212) strains (MIC = 400 mg/mL) common
   bacteria found in hospitals. (C) 2017 Elsevier B.V. All rights reserved.}},
