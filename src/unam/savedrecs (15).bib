@article{ ISI:000406420500025,
Author = {Gruber, Christine and Quevedo, Hernando},
Title = {{Geometrothermodynamic model for the evolution of the Universe}},
Journal = {{JOURNAL OF COSMOLOGY AND ASTROPARTICLE PHYSICS}},
Year = {{2017}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{Using the formalism of geometrothermodynamics to derive a fundamental
   thermodynamic equation, we construct a cosmological model in the
   framework of relativistic cosmology. In a first step, we describe a
   system without thermodynamic interaction, and show it to be equivalent
   to the standard ACDM paradigm. The second step includes thermodynamic
   interaction and produces a model consistent with the main features of
   inflation. With the proposed fundamental equation we are thus able to
   describe all the known epochs in the evolution of our Universe, starting
   from the inflationary phase.}},
