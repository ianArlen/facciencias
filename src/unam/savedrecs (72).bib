@article{ ISI:000293136800001,
Author = {Romero-Valdovinos, Mirza and Cardenas-Mejia, Alexander and
   Gutierrez-Gomez, Claudia and Flisser, Ana and Kawa-Karasik, Simon and
   Ortiz-Monasterio, Fernando},
Title = {{Keloid skin scars: the influence of hyperbaric oxygenation on fibroblast
   growth and on the expression of messenger RNA for insulin like growth
   factor and for transforming growth factor}},
Journal = {{IN VITRO CELLULAR \& DEVELOPMENTAL BIOLOGY-ANIMAL}},
Year = {{2011}},
Volume = {{47}},
Number = {{7}},
Pages = {{421-424}},
Month = {{AUG}},
Abstract = {{Wound healing can result in the development of keloid scars that contain
   atypical fibroblasts and an overabundance of extracellular matrix
   components. Hyperbaric oxygenation (HBO) refers to exposure to pure
   oxygen under increased atmospheric pressure and is recognized as a
   valuable supplementary method of treatment for problematic wounds. The
   effect of HBO in the expression of insulin-like growth factor type 1
   (ILGF-1) and transforming growth factor beta (TGF-beta) messenger RNAs
   was determined by semiquantitative RT-PCR in fibroblasts obtained from
   keloid scars and nonwound involved skin fibroblast from the same
   patient. ILGF-1 and TGF-beta are the principal mitogens during wound
   regeneration. We found a decrease in the growth of fibroblasts and in
   the expression of ILGF-1 and TGF-beta messengers in keloid and nonkeloid
   fibroblast after chronic exposition to hyperbaric oxygenation compared
   with normal oxygen partial pressure.}},
