@article{ ISI:000295277300008,
Author = {Quevedo, Hernando},
Title = {{MASS QUADRUPOLE AS A SOURCE OF NAKED SINGULARITIES}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS D}},
