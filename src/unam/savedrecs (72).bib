@article{ ISI:000291776900006,
Author = {Lozano, Angelica and Munoz, Angeles and Macias, Luis and Pablo Antun,
   Juan},
Title = {{Hazardous materials transportation in Mexico City: Chlorine and gasoline
   cases}},
Journal = {{TRANSPORTATION RESEARCH PART C-EMERGING TECHNOLOGIES}},
Year = {{2011}},
Volume = {{19}},
Number = {{5, SI}},
Pages = {{779-789}},
Month = {{AUG}},
Note = {{4th International Workshop on Freight Transportation and Logistics,
   Cesme Izmir, TURKEY, MAY 26-29, 2009}},
Abstract = {{We analyze two types of hazardous materials (hazmats) transportation in
   an urban environment, line-haul transportation and physical
   distribution. Two real cases are presented, the chlorine and gasoline
   transportation in Mexico City. Alternative paths are obtained and their
   population exposures are estimated, in order to identify if better
   alternative paths can exist. The population exposure considers
   inhabitants within the exposure area and exposed travelers during a
   truck trip. Accumulated population exposure was estimated for the
   gasoline case. Finally, recommendations and conclusion for urban hazmats
   transportation are presented. (C) 2010 Elsevier Ltd. All rights
   reserved.}},
