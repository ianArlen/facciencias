@article{ ISI:000295086600002,
Author = {Tarighat, Somayeh S. and Lentz, David L. and Matter, Stephen F. and Bye,
   Robert},
Title = {{Morphometric Analysis of Sunflower (Helianthus annuus L.) Achenes from
   Mexico and Eastern North America}},
Journal = {{ECONOMIC BOTANY}},
