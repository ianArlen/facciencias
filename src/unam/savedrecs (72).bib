@article{ ISI:000295204600006,
Author = {del Rio, Rafael and Silva, Luis O.},
Title = {{SPECTRAL MEASURES OF JACOBI OPERATORS WITH RANDOM POTENTIALS}},
Journal = {{OPERATORS AND MATRICES}},
Year = {{2011}},
Volume = {{5}},
Number = {{3}},
Pages = {{435-448}},
Month = {{SEP}},
Abstract = {{Let H(omega) be a self-adjoint Jacobi operator with a potential sequence
   \{omega(n)\}(n) of independently distributed random variables with
   continuous probability distributions and let mu(omega)(phi) be the
   corresponding spectral measure generated by H(omega) and the vector phi.
   We consider sets A(omega) which depend on omega, but are independent of
   two consecutive given entries of the secuence omega, and prove that
   mu(omega)(phi)(A(omega)) = 0 for almost every omega. This result is
   applied to show equivalence relations between spectral measures for
   random Jacobi matrices and to study the interplay of the eigenvalues of
   these matrices and their submatrices.}},
