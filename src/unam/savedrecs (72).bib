@article{ ISI:000292877500005,
Author = {Angeles-Castellanos, M. and Amaya, J. M. and Salgado-Delgado, R. and
   Buijs, R. M. and Escobar, C.},
Title = {{Scheduled Food Hastens Re-Entrainment More Than Melatonin Does after a
   6-h Phase Advance of the Light-Dark Cycle in Rats}},
Journal = {{JOURNAL OF BIOLOGICAL RHYTHMS}},
