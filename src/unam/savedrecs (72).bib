@article{ ISI:000295913300017,
Author = {Ibanez, Ana L. and Espinosa-Perez, Hector and Garcia-Calderon, Jose L.},
Title = {{Recent data on the distribution of the exotic species used in Mexican
   freshwater fisheries based on fish stocking}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
