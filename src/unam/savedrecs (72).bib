@article{ ISI:000293773700007,
Author = {Alonso-Nunez, G. and Morales de la Garza, L. and Rogel-Hernandez, E. and
   Reynoso, E. and Licea-Claverie, A. and Felix-Navarro, R. M. and
   Berhault, G. and Paraguay-Delgado, F.},
Title = {{New organometallic salts as precursors for the functionalization of
   carbon nanotubes with metallic nanoparticles}},
