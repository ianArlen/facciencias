@article{ ISI:000293802400053,
Author = {Ortiz-Landeros, Jose and Gomez-Yanez, Carlos and Pfeiffer, Heriberto},
Title = {{Surfactant-assisted hydrothermal crystallization of nanostructured
   lithium metasilicate (Li2SiO3) hollow spheres: II-Textural analysis and
   CO2-H2O sorption evaluation}},
Journal = {{JOURNAL OF SOLID STATE CHEMISTRY}},
Year = {{2011}},
Volume = {{184}},
Number = {{8}},
Pages = {{2257-2262}},
Month = {{AUG}},
Abstract = {{In a previous work, the synthesis and structural-microstructural
   characterization of different nanocrystalline lithium metasilicate
   (Li2SiO3) samples were performed. Then, in this work, initially, a
   textural analysis was performed over the same samples. Li2SiO3 samples
   prepared with a non-ionic surfactant (TRITON X-114) presented the best
   textural properties. Therefore, this sample was selected to evaluate its
   water vapor (H2O) and carbon dioxide (CO2) sorption properties. Sorption
   experiments were performed at low temperatures (30-80 degrees C) in
   presence of water vapor using N-2 or CO2 as carrier gases. Results
   clearly evidenced that CO2 sorption on these materials is highly
   improved by H2O vapor, and of course, textural properties enhanced the
   H2O-CO2 sorption efficiency, in comparison with the solid-state
   reference sample. (C) 2011 Elsevier Inc. All rights reserved.}},
