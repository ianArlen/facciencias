@article{ ISI:000293447600003,
Author = {Leon, Alejandra and Isabel Chavez, Mara and Delgado, Guillermo},
Title = {{H-1 and DOSY NMR spectroscopy analysis of Ligusticum porteri rhizome
   extracts}},
Journal = {{MAGNETIC RESONANCE IN CHEMISTRY}},
