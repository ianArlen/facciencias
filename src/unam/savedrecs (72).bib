Author = {El Hamzaoui, Y. and Hernandez, J. A. and Silva-Martinez, S. and Bassam,
   A. and Alvarez, A. and Lizama-Bahena, C.},
Title = {{Optimal performance of COD removal during aqueous treatment of alazine
   and gesaprim commercial herbicides by direct and inverse neural network}},
Journal = {{DESALINATION}},
