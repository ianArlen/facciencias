@article{ ISI:000292310200006,
Author = {Montejano, L. and Karasev, R. N.},
Title = {{Topological transversals to a family of convex sets}},
Journal = {{DISCRETE \& COMPUTATIONAL GEOMETRY}},
Year = {{2011}},
Volume = {{46}},
Number = {{2}},
Pages = {{283-300}},
Month = {{SEP}},
Abstract = {{Let F be a family of compact convex sets in Rd. We say that F has a
   topological rho-transversal of index (m, k) (rho < m, 0 < k <= d -m) if
   there are, homologically, as many transversal m-planes to F as m-planes
   containing a fixed rho-plane in Rm+k.
   Clearly, if F has a rho-transversal plane, then F has a topological
   rho-transversal of index (m, k), for rho < m and k <= d - m. The
   converse is not true in general. We prove that for a family F of rho + k
   + 1 compact convex sets in R-d a topological rho-transversal of index
   (m, k) implies an ordinary rho-transversal. We use this result, together
   with themultiplication formulas for Schubert cocycles, the
   LusternikSchnirelmann category of the Grassmannian, and different
   versions of the colorful Helly theorem by Barany and Lovasz, to obtain
   some geometric consequences.}},
