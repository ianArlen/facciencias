@article{ ISI:000295913300012,
Author = {Eduardo Zacarias-Eslava, Luis and Cornejo-Tenorio, Guadalupe and
   Cortes-Flores, Jorge and Gonzalez-Castaneda, Nahu and Ibarra-Manriquez,
   Guillermo},
Title = {{Composition, structure and diversify of the cerro El Aguila, Michoacan,
   Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2011}},
Volume = {{82}},
Number = {{3}},
Pages = {{854-869}},
Month = {{SEP}},
Abstract = {{This study describes composition, structure and diversity of vegetation
   in the Cerro El Aguila, Michoacan. Five plant communities were
   recognized and for each one all woody individuals >= 10 cm DBH were
   tallied in sixteen 400 m(2) circular plots, whereas individuals between
   2.5 and 9.9 cm DBH were measured in a 100 m(2) circular subplot centered
   within each larger plot. A total of 46 species, 33 genera and 21
   families were recorded. The tropical deciduous forest and Quercus
   deserticola forest had low values of mean plant height (< 5 m) and basal
   area (17.8 and 26 m(2)/ha, respectively), but these communities showed
   the higher density. Forests dominated by Q. rugosa and Quercus-Pinus had
   the lowest density but the highest mean height (> 11 m) and basal area
   (> 40 m(2)/ha). Diversity was greater in the tropical deciduous forest
   (S= 23 and alpha= 5.4, respectively) and lower in the oak-pine forest
   (S= 13 and alpha= 2.71). All communities displayed different dominant
   species and high beta diversity. Our results suggest that composition
   and structure could be affected by altitude and human activities.
   Nevertheless, the Cerro El Aguila still has well conservation areas,
   mainly temperate forests located toward summit zone and we recommend
   their inclusion in a near future within the state reserve system.}},
