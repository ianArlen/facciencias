@article{ ISI:000294560000012,
Author = {Garcia-Guzman, Graciela and Espinosa-Garcia, Francisco J.},
Title = {{Incidence of Fungal Necrotrophic and Biotrophic Pathogens in Pioneer and
   Shade-tolerant Tropical Rain Forest Trees}},
Journal = {{BIOTROPICA}},
