@article{ ISI:000300817700003,
Author = {Martinez, Lizbeth and Ordonez Sanchez, Maria Luisa and Letona, Rosario
   and Olvera Sumano, Veronica and Miguel Guerra, Mariano and Teresa
   Tusie-Luna, Maria and Alberto Aguilar-Salinas, Carlos},
Title = {{Familial homozygous hypercholesterolemia due to the c2271delT mutation
   in the LDL receptor gene, detected exclusively in mexicans}},
Journal = {{GACETA MEDICA DE MEXICO}},
Year = {{2011}},
Volume = {{147}},
Number = {{5}},
Pages = {{394-398}},
Month = {{SEP-OCT}},
Abstract = {{We present the case of an 18-years old women with homozygous familial
