@article{ ISI:000293852900025,
Author = {Hernandez-Mena, Leonel and Saldarriaga-Norena, Hugo and Murillo-Tovar,
   Mario A. and Amador-Munoz, Omar and Lopez-Lopez, Alberto and
   Waliszewski, Stefan M.},
Title = {{Determination of Black Carbon in Fine Particles Using a Semi-Continuous
   Method at Two Sites in the City of Guadalajara, Mexico, During 2007}},
Journal = {{BULLETIN OF ENVIRONMENTAL CONTAMINATION AND TOXICOLOGY}},
Year = {{2011}},
Volume = {{87}},
Number = {{3}},
Pages = {{336-342}},
Month = {{SEP}},
Abstract = {{The black carbon is a pollutant species primarily emitted from the
   combustion of fossil fuels (diesel). Their concentrations associated to
   PM2.5 were monitoring at two sites in the city of Guadalajara. From
   January to May (except April), downtown site shown 2.7, 2.6, 4.0 and 2.3
   times higher monthly concentrations. The dry season two showed higher
   concentrations respect to at least one of the others seasons (p <
   0.0001) at each site, probably due to atmospheric conditions less
   favorable for the dispersal of pollutants. During the 24 h period were
   observed at the year two peaks of concentrations: the highest morning
   peak and lower night peak, both probably related to anthropogenic
   activity.}},
