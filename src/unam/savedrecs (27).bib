@article{ ISI:000384932300004,
Author = {Garcia-Guzman, Graciela and Trejo, Irma and Acosta-Calixto, Irma and
   Esther Sanchez-Coronado, Maria},
Title = {{Environmental factors associated with disease incidence in plant species
   from a Mexican seasonal tropical dry forest}},
