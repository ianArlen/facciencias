@article{ ISI:000395294800001,
Author = {Flores Novelo, Anel and Ojeda Villagomez, Fernando and Kim, Hyun Sook
   Lee and Ramirez Cedillo, Eduardo},
Title = {{Influence of Entrepreneurial Orientation on Bussiness Performance: The
   Software Firms of Yucatan, Mexico Case}},
Journal = {{FAEDPYME INTERNATIONAL REVIEW-FIR}},
Year = {{2016}},
Volume = {{5}},
Number = {{9}},
Pages = {{25-34}},
Month = {{JUL-DEC}},
Abstract = {{The Entrepreneuria Orientation (EO) has emerged as a important construct
   in the last few years. It has been used as a variable that allows to
   evaluate the impact of the innovations, proactivity and risk taking in
   bussiness performance. Besides the numerous studies, the discussion
   about this relationship is still open. In this study examined the
   influence of EO and bussiness performance by using data from 41 mexican
   software firms located in Yucatan, Mexico. The findings show that EO are
   positively and directly associated with firm performance. The results
   confirm the positive relation of EO on a firm's performance. These
   findings offer important theoretical and practical implications.}},
