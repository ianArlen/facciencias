@article{ ISI:000385269700003,
Author = {Morante Lopez, Daniela and Guevara Escobar, Aurelio and Suzan Azpiri,
   Humberto and Lemus Ramirez, Vicente and Sosa Ferreyra, Carlos Francisco},
Title = {{Tier II estimation of enteric methane emission in dairy cow herds in
   Queretaro, Mexico}},
Journal = {{REVISTA MEXICANA DE CIENCIAS PECUARIAS}},
