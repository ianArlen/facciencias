@article{ ISI:000380248700001,
Author = {del Campo, J. M.},
Title = {{B88 exchange functional recovering the local spin density linear
   response}},
Journal = {{THEORETICAL CHEMISTRY ACCOUNTS}},
Year = {{2016}},
Volume = {{135}},
Number = {{7}},
Month = {{JUL 6}},
Abstract = {{Following the strategy described in the landmark paper generalized
   gradient approximation (GGA) made simple (Perdew et al. in Phys Rev Lett
   77(18):3865, 1996), we have constructed a non-empirical Becke 1988
   enhancement factor (neB). The local spin density linear response
   constraint was imposed on the Becke 1988 enhancement factor (B88)
   leading to a parameter value of 0.00336, which is 20 \% lower than the
   original fitted value 0.0042 and, therefore, decreases the gradient
   contribution to the exchange energy with respect to the original B88
   functional. The results on typical test sets, in particular for
   predicting the heats of formation, showed that global hybrid neBPBE0
   delivers a mean average error of 4.59 kcal/mol, which competes with
   results obtained with non-empirical meta-GGA functionals, making it an
   attractive simple hybrid functional to be used for molecular
   calculations for this rung of Perdew's classification of functionals.}},
