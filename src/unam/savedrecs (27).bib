Title = {{Genetic and morphological evidence of a geographically widespread hybrid
   zone between two crocodile species, Crocodylus acutus and Crocodylus
   moreletii}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2016}},
Volume = {{25}},
