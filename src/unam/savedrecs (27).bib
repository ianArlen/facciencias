@article{ ISI:000384034000018,
Author = {Laura Lopez-Vega, Diana and Martinez-Barbosa, Xochitl},
Title = {{Dr. Ramon Perez Cirera: importance of its trajectory in mexican
   pharmacology (1936-1982)}},
Journal = {{GACETA MEDICA DE MEXICO}},
Year = {{2016}},
Volume = {{152}},
Number = {{4}},
Pages = {{558-567}},
Month = {{JUL-AUG}},
Abstract = {{Dr. Ramon Perez Cirera (1936-1982) arrived in Mexico from Spain in 1936.
   Since then, his links with compatriots abroad and their acquired
   preparation in Europe allows to contribute to Mexican Pharmacology. He
   participated in teaching and research; he formalized, presided and
   helped to structure the Department of Pharmacology of the Faculty of
   Medicine from the National Autonomous University of Mexico and promoted
   the training of specialized associations. He led the Section of
   Pharmacology in the National Academy of Medicine. Understanding the
   value of his contribution helps to visualize the growth and maturation
   of national Pharmacology}},
