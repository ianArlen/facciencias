@article{ ISI:000378205300004,
Author = {Chaverra, Eliana and Carlos Degollado, Juan and Moreno, Claudia and
   Sarbach, Olivier},
Title = {{Black holes in nonlinear electrodynamics: Quasinormal spectra and parity
   splitting}},
Journal = {{PHYSICAL REVIEW D}},
