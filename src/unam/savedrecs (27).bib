@article{ ISI:000382285100011,
Author = {Webb, R. and Buratini, J. and Hernandez-Medrano, J. H. and Gutierrez, C.
   G. and Campbell, B. K.},
Title = {{Follicle development and selection: past, present and future}},
Journal = {{ANIMAL REPRODUCTION}},
Year = {{2016}},
Volume = {{13}},
Number = {{3}},
Pages = {{234-249}},
Month = {{JUL-SEP}},
