@article{ ISI:000378841800004,
Author = {Salgado-Delgado, R. and Bustos-Figueroa, L. A. and Garcia-Hernandez, E.
   and Vargas-Galarza, Z. and Salgado-Delgado, A. M. and Olarte-Paredes, A.
   and Lopez-Lara, T. and Hernandez-Zaragoza, J. B. and Castano, V. M.},
Title = {{Poly(2-Hydroxyethyl Methacrylate-Bis{[}Trimethoxysilylpropyl]Amine)
   Hybrid Networks}},
Journal = {{JOURNAL OF INORGANIC AND ORGANOMETALLIC POLYMERS AND MATERIALS}},
Year = {{2016}},
Volume = {{26}},
Number = {{4}},
Pages = {{756-763}},
Month = {{JUL}},
Abstract = {{In this paper, our goal is to improve the understanding of structural
   control of hybrid materials synthesized by using acid-catalyzed sol-gel
   reactions of bis-{[}trimethoxysilylpropyl]amine (BisSi) and
   2-hydroxyethyl methacrylate (HEMA) via free radical in a common solvent.
   Polymer networks compositions were determined by FTIR and H-1-NMR
   spectroscopy. The thermal properties of the P(HEMA-BisSi) hybrids with
   different silica content (e.g. 10, 15 and 25 wt\%) were determined by
   thermogravimetric analysis and differential scanning calorimetry. Glass
   transition temperatures (T(g)A ` s) of P(HEMA-BisSi) networks were also
   compared with T-g of PHEMA homopolymer. The T(g)A ` of PHEMA homopolymer
   was found as 103.74 A degrees C. The thermal stability of these networks
   was increased with the BisSi content. Scanning electron microscopy
   images showed that an increase in acid content caused a decrease in the
   pore size and pore volume as well as in the surface area of the xerogel.}},
