Author = {Vazquez-Acevedo, Miriam and Vega-deLuna, Felix and Sanchez-Vasquez,
   Lorenzo and Colina-Tenorio, Lilia and Remade, Claire and Cardol, Pierre
   and Miranda-Astudillo, Hector and Gonzalez-Halphen, Diego},
Title = {{Dissecting the peripheral stalk of the mitochondrial ATP synthase of
   chlorophycean algae}},
Journal = {{BIOCHIMICA ET BIOPHYSICA ACTA-BIOENERGETICS}},
