@article{ ISI:000375740200015,
Author = {Lopez-Cardoso, Marcela and Vargas-Pineda, Gabriela and Patricia
   Roman-Bravo, Perla and Rodriguez-Narvaez, Cristina and Rosas-Valdez,
   Elena and Cea-Olivares, Raymundo},
Title = {{Synthesis, crystal structures and coordination modes of some
   triorganotin(IV) complexes with 2-N-propyl and
   2-N-benzyl-amino-1-cyclopentene-1-dithiocarboxylates}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE}},
Year = {{2016}},
Volume = {{1116}},
Pages = {{116-121}},
Month = {{JUL 15}},
Abstract = {{The syntheses and characterization of six new triorganotin(IV)
   complexes, Ph3Sn(PrACDA) (1), Bu3Sn(PrACDA) (2), Ph3Sn(BzACDA) (3),
   Bu3Sn(BzACDA) (4), Me3Sn(BzACDA) (5) and Cy3Sn(BzACDA) (6) (ACDA =
   2-amino-1-cyclopentene-1-carbodithioate anion) are reported. Compounds
   1-6 were synthesized by the reaction between the sodium salts of
   2-N-propyl- or 2-N-benzyl-2-amino-1-cyclopentene-1-carbodithioate and
   R3SnCl (R = Ph, Bu, Me, Cy) in a 1:1 M ratio. The complexes were
   characterized by elemental analyses, IR and NMR (H-1, C-13 and Sn-119)
   spectroscopy and by FAB(+) mass spectrometry. The experimental data
   reveal that the tin atom is coordinated to the ligand by means of the
   two sulfur atoms from the carbodithioate group in an anisobidentate
   mode, while the Sn-119\{H-1\} NMR spectra suggest a pentacoordinate
   metal center in 1-4 and a tetracoordinate tin atom for 5 and 6. The
   molecular structures of complexes 1, 3 and 5 were confirmed by single
   crystal X-ray diffraction analysis showing the presence of N-H center
   dot center dot center dot S hydrogen bonding and a distorted trigonal
   bipyramid geometry for the tin atoms. (C) 2016 Elsevier B.V. All rights
   reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
