@article{ ISI:000381770500101,
Author = {Voitsekhovich, V. V. and Orlov, V. G.},
Title = {{Temporal properties of Zernike modes for non-Kolmogorov turbulence}},
Journal = {{OPTICS EXPRESS}},
Year = {{2016}},
Volume = {{24}},
Number = {{14}},
Pages = {{16123-16131}},
Month = {{JUL 11}},
Abstract = {{Temporal properties of Zernike modes are investigated for the case of
   light wave propagation through the non-Kolmogorov turbulence. The
   calculations are performed for the weak-turbulence conditions with
   constant turbulence parameters along the propagation path and taking
   into account the effects of the outer and inner scale of the turbulence.
   It is shown that the difference between effects produced by the
   Kolmogorov and non-Kolmogorov turbulence is getting smaller when the
   aberration order is growing up and when the outer scale magnitude is
   decreasing. Also it is shown that the correlation time of an aberration
   is decreasing with increasing of its order and with decreasing of
   spectral index. (C) 2016 Optical Society of America}},
