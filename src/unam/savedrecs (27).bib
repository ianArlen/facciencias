@article{ ISI:000394274500006,
Author = {Crespo Guerrero, Jose Manuel and Jimenez Pelcastre, Araceli},
Title = {{Arrangement of the riverside commercial fishing sector in the Biosphere
   Reserve El Vizcaino (Mexico)}},
Journal = {{REVISTA GEOGRAFICA VENEZOLANA}},
Year = {{2016}},
Volume = {{57}},
Number = {{2}},
Pages = {{236-259}},
Month = {{JUL-DEC}},
Abstract = {{The Biosphere reserve El Vizcaino in Southern Lower California State
   (Mexico) dates back to 1988. After some years, the UNESCO Committee for
   the World Heritage would grant it, in two occasions, some
   acknowledgement because of its natural and cultural value. It is
   considered to be one of the largest reserves in the entire world, due to
   its 2,546,790 hectares. It means 10\% of the natural preserved areas in
   Mexico. Besides, it is both of the maritime and terrestrial type, since
   12\% of its area is located in the waters of both the Pacific Ocean and
   the California Gulf. It is not surprising that one of its main economic
   activities is the riverside commercial fishing. This research paper will
   shed some light, by means of some quantitative and qualitative data, on
   the organization of the four fishing areas of the reserve: Golfo (Gulf),
   Esteros y laguna de San Ignacio (Tideland and San Ignacio Pool),
   Pacifico Norte (Northern Pacific) and Laguna Ojo de Liebre (Ojo de
   Liebre Pool).}},
