@article{ ISI:000378449200004,
Author = {Botero, A. and Leyvraz, F.},
Title = {{The two-dimensional three-body problem in the large magnetic field limit
   is integrable}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2016}},
Volume = {{380}},
Number = {{27-28}},
Pages = {{2211-2218}},
Month = {{JUN 17}},
Abstract = {{The problem of N particles interacting through pairwise central forces
   is notoriously intractable for N >= 3. Some remarkable specific cases
   have been solved in one dimension. Here we show that the guiding center
   approximation-valid for charges moving in two dimensions in the limit of
   large constant magnetic fields-simplifies the three-body problem for an
   arbitrary interparticle interaction invariant under rotations and
   translations, making it solvable by quadratures. A spinorial
   representation for the system is introduced, which allows a
   visualization of its phase space as the corresponding Bloch sphere.
   Finally, a discussion of the quantization of the problem is presented.
   (C) 2016 Elsevier B.V. All rights reserved.}},
