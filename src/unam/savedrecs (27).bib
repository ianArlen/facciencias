@article{ ISI:000374678400027,
Author = {Escamilla, R. and Carvajal, E. and Cruz-Irisson, M. and Morales, F. and
   Huerta, L. and Verdin, E.},
Title = {{XPS study of the electronic density of states in the superconducting
   Mo2B and Mo2BC compounds}},
Journal = {{JOURNAL OF MATERIALS SCIENCE}},
Year = {{2016}},
Volume = {{51}},
Number = {{13}},
Pages = {{6411-6418}},
Month = {{JUL}},
Abstract = {{The electronic structure of the MoBC and MoB compounds was investigated
   by X-ray photoelectron spectroscopy. The Mo 3d, C 1s, and B 1s core
   levels are identified. For the MoBC, the core-level binding energies
   corresponding to Mo 3d, B 1s, and C 1s are localized at 227.90, 187.94,
   and 282.95 eV, respectively, whereas for the MoB, the Mo 3d, and B 1s
   are localized at 228.09 and 188.06 eV, respectively. Core-level binding
   energies shifts are observed in both compounds using the
   charge-potential model. The electronic density of states was calculated
   for MoB and MoBC using GGA approximation. Our results show that the
   electronic density of states at the Fermi level in the MoB is higher
   than that in the MoBC. The dominance of the Mo 4d states down to 8 eV
   below the Fermi level is found. The calculated total DOS was consistent
   with the XPS valence band spectra. Finally, within the BCS theory
   framework, the presence of superconductivity in both compounds can not
   be explained only as a function of the electronic density of states at
   the Fermi level. The electron-phonon coupling constant () was calculated
   using the McMillan equation; the obtained values were 0.75 for MoBC and
   0.70 for MoB. These values indicate that both compounds are intermediate
   coupled superconductors.}},
