@article{ ISI:000378351300003,
Author = {Luna-Martinez, Oscar D. and Vidal-Limon, Abraham and Villalba-Velazquez,
   Miryam I. and Sanchez-Alcala, Rosalba and Garduno-Juarez, Ramon and
   Uversky, Vladimir N. and Becerril, Baltazar},
Title = {{Simple approach for ranking structure determining residues}},
Journal = {{PEERJ}},
Year = {{2016}},
Volume = {{4}},
Month = {{JUN 22}},
Abstract = {{Mutating residues has been a common task in order to study structural
   properties of the protein of interest. Here, we propose and validate a
   simple method that allows the identification of structural determinants;
   i.e., residues essential for preservation of the stability of global
   structure, regardless of the protein topology. This method evaluates all
   of the residues in a 3D structure of a given globular protein by ranking
   them according to their connectivity and movement restrictions without
   topology constraints. Our results matched up with sequence-based
   predictors that look up for intrinsically disordered segments,
   suggesting that protein disorder can also be described with the proposed
   methodology.}},
