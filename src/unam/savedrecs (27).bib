@article{ ISI:000374575700002,
Author = {Alfonso, I. and Lara, G. and Gonzalez, G. and Bejar, L. and Aguilar, C.
   and Figueroa, A.},
Title = {{A novel solid state method for manufacturing Al foams by over solution
   heat treatment}},
Journal = {{MATERIALS LETTERS}},
Year = {{2016}},
Volume = {{174}},
Pages = {{6-9}},
Month = {{JUL 1}},
Abstract = {{The present study shows a new manufacturing process of foams of Al
   alloys by the modification of the conventional solution heat treatment
   of three alloys Al-6Si-3Cu-xMg (x=7, 9 and 11 wt\%). Over-heating the
   alloys during 6 h at 520, 560 and 600 degrees C allowed to obtain foams
   with maxima porosities of 40\% and pore sizes from 20 to 150 mu m,
   originated due to the localized melting of second phases. The
   characteristics of the foams highly depended on the Mg content and the
   heat treatment temperature. (C) 2016 Elsevier B.V. All rights reserved.}},
