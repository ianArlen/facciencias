@article{ ISI:000374865800038,
Author = {Montes, E. and Martinez-Merlin, I. and Guzman-Olguin, J. C. and
   Guzman-Mendoza, J. and Martin, I. R. and Garcia-Hipolito, M. and
   Falcony, C.},
Title = {{Effect of pH on the optical and structural properties of HfO2:Ln(3+),
   synthesized by hydrothermal route}},
Journal = {{JOURNAL OF LUMINESCENCE}},
Year = {{2016}},
Volume = {{175}},
Pages = {{243-248}},
Month = {{JUL}},
Abstract = {{In this work, the influence of the chlorine ions in the optical and
   structural properties of hafnium oxide (HfO2) doped with europium (Eu3+)
   and terbium (Tb3+) are reported. The synthesis was conducted by
   hydrothermal route at a temperature of 200 degrees C, with a reaction
   time of 80 min and a concentration of 3 mol\% of both dopants, in
   relation to hafnium in solution. In order to study the influence of
   acidity on the kinetics of reaction, the pH of the precursor solution
   was varied in the range of pH=4 to pH=12. X-ray powder diffraction
   patterns showed that the materials crystallized better under alkaline
   conditions with pH=11, noting a better crystallinity in the material
   doped with Eu3+ showed a better crystallinity that Tb3+ ions. The
   characteristic rhombohedral microstructure of HfO2 in the monoclinic
   phase was observed in alkaline media. Photoluminescent spectra showed
   the characteristic peaks of the emissions for both Eu an Tb dopants,
   where the intensity of the luminescent emission increases by decreasing
   the concentration of chlorine ions. On the other hand, it was found that
   radiative life kinetics of the dopant is related to the kinetics of
   crystallization, and therefore, to the acidity of the precursor
   solution. (C) 2016 Elsevier B.V. All rights reserved.}},
