@article{ ISI:000380106400001,
Author = {Rosado, A. S. and Franco-Villafane, J. A. and Pineda, C. and Sadurni, E.},
Title = {{Stern-Gerlach splitters for lattice quasispin}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2016}},
Volume = {{94}},
Number = {{4}},
Month = {{JUL 20}},
Abstract = {{We design a Stern-Gerlach apparatus that separates quasispin components
   on the lattice, without the use of external fields. The effect is
   engineered using intrinsic parameters, such as hopping amplitudes and
   on-site potentials. A theoretical description of the apparatus relying
   on a generalized Foldy-Wouthuysen transformation beyond Dirac points is
   given. Our results are verified numerically by means of wave-packet
   evolution, including an analysis of Zitterbewegung on the lattice. The
   necessary tools for microwave realizations, such as complex hopping
   amplitudes and chiral effects, are simulated.}},
