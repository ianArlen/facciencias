@article{ ISI:000263759500002,
Author = {Iturriaga, Renato and Sanchez-Morgado, Hector},
Title = {{Hyperbolicity and exponential convergence of the Lax-Oleinik semigroup}},
Journal = {{JOURNAL OF DIFFERENTIAL EQUATIONS}},
Year = {{2009}},
Volume = {{246}},
Number = {{5}},
Pages = {{1744-1753}},
Month = {{MAR 1}},
Abstract = {{For a convex superlinear Lagrangian L: TM -> R on a compact manifold M
   it is known that there is a unique number c such that the Lax-Oleinik
   semigroup L-t + ct: C(M, R) -> C(M, R) has a fixed point. Moreover for
   any u is an element of C(M, R) the uniform limit (u) over bar = lim(t
   ->infinity) L-tu + ct exists.
   In this paper we assume that the Aubry set consists in a finite number
   of periodic orbits or critical points and study the relation of the
   hyperbolicity of the Aubry set to the exponential rate of convergence of
   the Lax-Oleinik semigroup. (c) 2008 Elsevier Inc. All rights reserved.}},
