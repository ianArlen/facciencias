@article{ ISI:000263063400009,
Author = {Rosas, G. and Esparza, R. and Bedolla-Jacuinde, A. and Perez, R.},
Title = {{Room Temperature Mechanical Properties of Fe3Al Intermetallic Alloys
   with Li and Ni Additions}},
Journal = {{JOURNAL OF MATERIALS ENGINEERING AND PERFORMANCE}},
Year = {{2009}},
Volume = {{18}},
Number = {{1}},
Pages = {{57-61}},
Month = {{FEB}},
Abstract = {{This work analyzes the effect of very small additions of Li and Ni on
   the mechanical properties of the Fe72Al28 intermetallic alloy, close to
   the Fe3Al ratio. Hardness and tensile strength at room temperature were
   analyzed for the cast Fe3Al intermetallic alloys as a function of Li and
   Ni additions. For this purpose, high-purity raw materials were melted in
   an open induction furnace into a SiC crucible and the liquid alloys were
   poured into sand moulds to directly get tensile test-shaped specimens to
   minimize machining. Mechanical tests were carried out in the as-cast
   condition as well as after a homogenization heat treatment at 400
   degrees C for 120 h. The obtained microstructure was characterized by
   XRD and SEM. Results show an increase in ductility, particularly when Li
   was added.}},
