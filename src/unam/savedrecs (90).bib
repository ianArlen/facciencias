@article{ ISI:000264117600002,
Author = {Mercado-Ruaro, Pedro and Delgado-Salinas, Alfonso and Chiang, Fernando},
Title = {{Taxonomic re-assessment of Phaseolus dasycarpus (Leguminosae):
   Systematic position, chromosome studies and re-description}},
Journal = {{BRITTONIA}},
Year = {{2009}},
Volume = {{61}},
Number = {{1}},
Pages = {{8-13}},
Month = {{MAR}},
Abstract = {{Based on new information on floral structures, seedling, fruit, seed,
   root, and leaflet characters, Phaseolus dasycarpus is re-described and
   illustrated. Its chromosome number was determined as 2n = 22, with
   metacentric and submetacentric chromosomes, and karyotype formula of 9m
   + 2sm. Previous phylogenetic analyses of ITS and trnK sequence data, and
   those obtained in the present study, show P. dasycarpus to be aligned
   within section Pedicellati.}},
