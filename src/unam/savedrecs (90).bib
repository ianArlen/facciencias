@article{ ISI:000263706600008,
Author = {Lozano, Lucia and Herraiz, Miguel and Singh, Shri Krishna},
Title = {{Site effect study in central Mexico using H/V and SSR techniques:
   Independence of seismic site effects on source characteristics}},
