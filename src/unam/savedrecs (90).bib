@article{ ISI:000262782400014,
Author = {Pineyro-Nelson, A. and Van Heerwaarden, J. and Perales, H. R. and
   Serratos-Hernandez, J. A. and Rangel, A. and Hufford, M. B. and Gepts,
   P. and Garay-Arroyo, A. and Rivera-Bustamante, R. and Alvarez-Buylla, E.
   R.},
Title = {{Transgenes in Mexican maize: molecular evidence and methodological
   considerations for GMO detection in landrace populations}},
Journal = {{MOLECULAR ECOLOGY}},
