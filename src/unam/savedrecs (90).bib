@article{ ISI:000268148400007,
Author = {del Carmen Alejo-Plata, Maria and Luis Gomez-Marquez, Jose and Ramos
   Carrillo, Samuel and Eduardo Herrera-Galindo, Jorge},
Title = {{Reproduction, diet and fishery of Octopus (Octopus) hubbsorum (Mollusca:
   Cephalopoda) in the coast of Oaxaca, Mexico.}},
