@article{ ISI:000264138900004,
Author = {Rivera, T. and Kitis, G. and Azorin, J. and Furetta, C.},
Title = {{DECONVOLUTION METHOD ON OSL CURVES FROM ZrO2 IRRADIATED BY BETA AND UV
   RADIATIONS}},
Journal = {{MODERN PHYSICS LETTERS B}},
Year = {{2009}},
Volume = {{23}},
Number = {{5}},
Pages = {{699-707}},
Month = {{FEB 20}},
Abstract = {{This paper reports the optically stimulated luminescent (OSL) response
   of ZrO2 to beta and ultraviolet radiations in order to investigate the
   potential use of this material as a radiation dosimeter. The
   experimentally obtained OSL decay curves were analyzed using the
   computerized curve de-convolution (CCD) method. It was found that the
   OSL curve structure, for the short (practical) illumination time used,
   consists of three first order components. The individual OSL dose
   response behavior of each component was found. The values of the time at
   the OSL peak maximum and the decay constant of each component were also
   estimated.}},
