@article{ ISI:000263567000024,
Author = {Boiko, Igor and Fridman, Leonid and Pisano, Alessandro and Usai, Elio},
Title = {{On the Transfer Properties of the ``Generalized Sub-Optimal{''}
   Second-Order Sliding Mode Control Algorithm}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
