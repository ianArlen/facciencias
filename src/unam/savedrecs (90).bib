@article{ ISI:000264090900003,
Author = {Lugo, J. E. and de la Mora, B. and Doti, R. and Nava, R. and Taguena, J.
   and del Rio, A. and Faubert, J.},
Title = {{Multiband negative refraction in one-dimensional photonic crystals}},
Journal = {{OPTICS EXPRESS}},
Year = {{2009}},
Volume = {{17}},
Number = {{5}},
Pages = {{3042-3051}},
Month = {{MAR 2}},
Abstract = {{We simulate a lossless one-dimensional photonic crystals (1D-PC)
   structure and show that negative refraction could be present near the
   low frequency edge of at least the second, fourth and sixth bandgaps. We
   experimentally demonstrate for the first time negative refraction in
   strongly modulated porous silicon 1D-PC in the visible and near infrared
   regions. This 1D-PC structure may allow the realization of short-focus
   Veselago lenses in different optical bands. An advantage of our
   structure is its simplicity allowing for cheap and rapid fabrication of
   samples. (C) 2009 Optical Society of America}},
