@article{ ISI:000264424200011,
Author = {Panayotaros, Panayotis},
Title = {{Linear stability of breathers of the discrete NLS}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2009}},
Volume = {{373}},
Number = {{10}},
Pages = {{957-963}},
Month = {{MAR 2}},
Abstract = {{We consider real breather solutions of the discrete cubic nonlinear
   Schrodinger equation near the limit of vanishing coupling between the
   lattice sites and present leading order asymptotics for the eigenvalues
   of the linearization around the breathers. The expansion is given in
   fractional powers of the intersite coupling parameter and determines the
   linear stability of the breathers. The method we use relies on normal
   form ideas and applies to one and higher-dimensional lattices. We also
   present some examples. (C) 2009 Elsevier B.V. All rights reserved.}},
