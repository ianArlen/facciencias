@article{ ISI:000264665600001,
Author = {Ghirlanda, G. and Nava, L. and Ghisellini, G. and Celotti, A. and
   Firmani, C.},
Title = {{Short versus long gamma-ray bursts: spectra, energetics, and
   luminosities}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
