@article{ ISI:000263940300025,
Author = {Arellano-Jimenez, M. J. and Garcia-Garcia, R. and Reyes-Gasga, J.},
Title = {{Synthesis and hydrolysis of octacalcium phosphate and its
   characterization by electron microscopy and X-ray diffraction}},
Journal = {{JOURNAL OF PHYSICS AND CHEMISTRY OF SOLIDS}},
Year = {{2009}},
Volume = {{70}},
Number = {{2}},
Pages = {{390-395}},
Month = {{FEB}},
Abstract = {{Octacalcium phosphate (OCP) powder was produced by precipitating 250 mL
   Ca(CH3COO)(2) 0.04 M into 750 L of phosphate solution (5 mmol Na2HPO4
   and 5 mmol NaH2PO4) at a constant temperature of 60 degrees C and pH 5,
   which resulted in a dry white powder. X-ray diffraction (XRD),
   transmission electron microscopy (TEM) analysis, and the electron
   diffraction pattern (SAED) all showed only OCP. Hydroxyapatite (HAP) was
   directly obtained through hydrolysis of the powder. The total
   transformation of OCP into HAP was registered over a period of 6 h.
   During the first 30 min of hydrolysis both phases coexisted. The two
   phases and the OCP-HAP interface were structurally analyzed through XRD
   and TEM. OCP parameters (calculated by the Rietveld method) are a =
   19.70, b = 9.50, c = 6.85 angstrom; alpha = 90.03 degrees, beta = 92.48
   degrees, gamma = 108.32 degrees (triclinic P-1) with average crystal
   size of 13.5+/-0.2 nm, while HAP parameters were a = 9.45, c = 6.87
   angstrom (hexagonal P6(3)/M) with average crystal size of 16.9+/-0.2 nm.
   (C) 2008 Elsevier Ltd. All rights reserved.}},
