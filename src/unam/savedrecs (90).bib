@article{ ISI:000263079300018,
Author = {Guerra-Araiza, Christian and Amorim, Miguel A. R. and Pinto-Almazan,
   Rodolfo and Gonzalez-Arenas, Aliesha and Campos, Maria G. and
   Garcia-Segura, Luis M.},
Title = {{Regulation of the Phosphoinositide-3 Kinase and Mitogen-Activated
   Protein Kinase Signaling Pathways by Progesterone and Its Reduced
   Metabolites in the Rat Brain}},
Journal = {{JOURNAL OF NEUROSCIENCE RESEARCH}},
Year = {{2009}},
Volume = {{87}},
Number = {{2}},
Pages = {{470-481}},
Month = {{FEB 1}},
Abstract = {{Several growth factors, such as vascular endothelial growth factor,
   brain-derived neurotrophic factor, and insulin-like growth factor-I are
   involved in the actions of progesterone in the central nervous system.
   Previous studies in neuronal and glial cultures have shown that
   progesterone may regulate growth factor signaling, increasing the
   phosphorylation of extracellular-signal regulated kinase (ERK) and the
   phosphorylation of Akt, components of the mitogen-activated protein
   kinase (MAPK) and the phosphoinositicle-3 kinase (PI3K) signaling
   pathways, respectively. In this study, we have evaluated whether
   progesterone and its reduced metabolites, dihydroprogesterone and
   tetrahydroprogesterone, regulate PI3K and MAPK signaling in the brain of
   ovariectomized rats in vivo. Significant increases in the
   phosphorylation of ERK, in the expression of the catalytic (p110) and
   the regulatory (p85) subunits of PI3K and in the phosphorylation of Akt
   were observed in the hypothalamus, the hippocampus, and the cerebellum
   24 hr after progesterone administration. Progesterone metabolites
   partially mimicked the effect of progesterone and had a stronger effect
   on MAPK and PI3K signaling in the hypothalamus than in the other brain
   regions. These findings suggest that progesterone regulates MAPK and
   PI3K signaling pathways in the central nervous system in vivo by direct
   hormonal actions and by mechanisms involving progesterone metabolites.
   (c) 2008 Wiley-Liss, Inc.}},
