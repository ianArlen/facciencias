@article{ ISI:000263758400019,
Author = {Matos, Tonatiuh and Vazquez-Gonzalez, Alberto and Magana, Juan},
Title = {{phi(2) as dark matter}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2009}},
Volume = {{393}},
