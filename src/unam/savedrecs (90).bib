@article{ ISI:000263699600007,
Author = {Jalbout, Abraham F.},
Title = {{Endohedral metallo {[}80] fullerene interactions with small polar
   molecules}},
Journal = {{COMPUTATIONAL MATERIALS SCIENCE}},
