@article{ ISI:000265647900008,
Author = {Ramirez-Carrillo, H. F. and Luna-Pabello, V. M. and Arredondo-Figueroa,
   J. L.},
Title = {{EVALUATION OF AN INTERMITENT ARTIFICIAL VERTICAL FLOW WETLAND, TO OBTAIN
   GOOD QUALITY WATER FOR AQUACULTURE}},
Journal = {{REVISTA MEXICANA DE INGENIERIA QUIMICA}},
Year = {{2009}},
Volume = {{8}},
Number = {{1}},
Pages = {{93-99}},
Month = {{APR}},
Abstract = {{An inadequate water quality in epicontinental waterbodies could provoke
   risks oil aquatic species especially in the endemic species that are
   less tolerant to several kinds of pollutants. In this paper the results
   of artificial wetland (AW) with it puliment filter (PF) to obtain a good
   water quality for aquaculture activities are presented. All AW of 55
   m(2) was connected to all intermitent vertical flow, with four closes
   daily of 1,000 L of water. that is equivalent to 72 mm(3)/mm(2) day of
   superficial hydraulic charge (SHC). The finality of this AW, was to
   remove organic material, nitrogen and phosphorous. A PF of 1.8 m(2) was
   installed in order to eliminate PO43-. During 12 months of operation.
   the total remotion reached 92\% of chemical oxygen demand (COD). 85\% of
   N-NH4 and 80\% Of PO43-. The final water quality obtained in this system
   can be considered good for aquaculture purposes.}},
