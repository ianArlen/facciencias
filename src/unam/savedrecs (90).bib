@article{ ISI:000262479300020,
Author = {Sanchez-Salcedo, F. J.},
Title = {{Gaseous drag on a gravitational perturber in Modified Newtonian Dynamics
   and the structure of the wake}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2009}},
Volume = {{392}},
Number = {{4}},
Pages = {{1573-1584}},
Month = {{FEB 1}},
Abstract = {{We calculate the structure of a wake generated by, and the dynamical
   friction force on, a gravitational perturber travelling through a
   gaseous medium of uniform density and constant background acceleration
   g(ext), in the context of Modified Newtonian Dynamics (MOND). The wake
   is described as a linear superposition of two terms. The dominant part
   displays the same structure as the wake generated in the Newtonian
   gravity scaled up by a factor mu(-1)(gext/a(0)), where a(0) is the
   constant MOND acceleration and mu the interpolating function. The
   structure of the second term depends greatly on the angle between gext
   and the velocity of the perturber. We evaluate the dynamical drag force
   numerically and compare our MOND results with the Newtonian case. We
   mention the relevance of our calculations to orbit evolution of globular
   clusters and satellites in a gaseous protogalaxy. Potential differences
   in the X-ray emission of gravitational galactic wakes in MOND and in
   Newtonian gravity with a dark halo are highlighted.}},
