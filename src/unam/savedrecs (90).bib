@article{ ISI:000264281800041,
Author = {Salazar-Salinas, Karim and Jauregui, Luis A. and Kubli-Garfias, Carlos
   and Seminario, Jorge M.},
Title = {{Molecular biosensor based on a coordinated iron complex}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{2009}},
Volume = {{130}},
Number = {{10}},
Month = {{MAR 14}},
Abstract = {{A sensor model based on the porphyrin nucleus of the soluble guanylate
   cyclase enzyme is modeled and tested with nitric oxide and carbon
   monoxide. Molecular oxygen is tested as a possible interferer.
   Geometries and electronic structures of the model are assessed by
   density functional theory. Vibrational circular dichroism (VCD),
   infrared, and Raman spectra are obtained for the iron complexes
   uncoordinated and coordinated with the gas moieties. The sensor is
   capable of detecting the ligands to different extents. Carbon monoxide
   is less detectable than nitric oxide due to the adopted position of the
   molecule in the sensor; carbon oxide is aligned with the iron atom,
   while nitric oxide and molecular oxygens bend with an angle detectable
   by the VCD. It is suggested that pollutants may be detected and measured
   with the proposed biosensors.}},
