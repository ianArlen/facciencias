@article{ ISI:000221530400017,
Author = {Marchi, MC and Castaneda-Guzman, R and Perez-Pacheco, A and Bilmes, SA
   and Villagran-Muniz, M},
Title = {{Photoacoustic analysis of the evolution from sols to aged gels:
   Comparison of different initial titania sols}},
Journal = {{INTERNATIONAL JOURNAL OF THERMOPHYSICS}},
