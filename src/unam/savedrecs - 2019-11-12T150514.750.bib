@article{ ISI:000221085600005,
Author = {Michaud, F and Sosson, M and Royer, JY and Chabert, A and Bourgois, J
   and Calmus, T and Mortera, C and Bigot-Cormier, F and Bandy, W and
   Dyment, J and Pontoise, B and Sichler, B},
Title = {{Motion partitioning between the Pacific plate, Baja California and the
   North America plate: The Tosco-Abreojos fault revisited}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2004}},
Volume = {{31}},
Number = {{8}},
Month = {{APR 20}},
Abstract = {{During the FAMEX cruise of the R/V L'Atalante (April 2002), swath
   bathymetry and seismic profiles were recorded along the Baja California
   margin from 23degrees to 27degreesN. The upper-slope of the margin
   exhibits active faulting of recent sediments along the Tosco-Abreojos
   fault system (TAFS). Flower structures evidence strike-slip motion along
   the TAFS. Right lateral strike slip faulting, associated in places with
   small extensional features, are consistent with present-day slip and
   extension rates predicted along the TAFS. The TAFS thus belongs to the
   western active boundary of a Baja California Block yet not totally
   transferred to the Pacific plate.}},
