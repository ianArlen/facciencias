@article{ ISI:000189038200029,
Author = {Brostow, W and Castano, VM and Martinez-Barrera, G and Pietkiewicz, D},
Title = {{Pressure-volume-temperature properties of an epoxy plus fluorinated
   poly(aryl ether ketone) system}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
