@article{ ISI:000223706600006,
Author = {Moraes, SM and Romero-Fuster, MC and Sanchez-Bringas, F},
Title = {{Principal configurations and umbilicity of submanifolds in R-N}},
Journal = {{BULLETIN OF THE BELGIAN MATHEMATICAL SOCIETY-SIMON STEVIN}},
