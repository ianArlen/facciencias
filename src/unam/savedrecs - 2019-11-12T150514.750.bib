@article{ ISI:000223590900003,
Author = {Barron, MDC and Herrera, JG and Suarez, ME and Zamora, MM and Lemus, C},
Title = {{Evaluation of carcass traits in three rabbit breeds}},
Journal = {{CUBAN JOURNAL OF AGRICULTURAL SCIENCE}},
Year = {{2004}},
Volume = {{38}},
Number = {{1}},
Pages = {{17-22}},
Month = {{MAR}},
Abstract = {{Forty-two rabbits from the New Zealand White, California and Chinchilla
   breeds were slaughtered at 70 d of age for evaluating the carcass
   characteristics of these breeds. A model of fixed effects was used
   including breed, sex and their interaction and liveweight at slaughter
   was used as covariant. There were differences among the breeds (P <
   0.05) in shoulder blade and leg weight and loin width, favoring the
   Chinchilla breed. Sex affected (P < 0.05) red viscera and kidneys
   weight, being higher in females. The interaction breed x sex was not
   significant, except for hindquarters weight. Liveweight at slaughter
   affected (P < 0.01) all the characteristics, except for shoulder blade
   and liver weight and carcass yield. It is concluded that the rabbits
   from the Chinchilla breed had the best weights of the different carcass
   components.}},
