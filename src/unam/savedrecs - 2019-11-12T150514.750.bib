@article{ ISI:000220566200009,
Author = {Villa, RM and Saorin, M},
Title = {{Koszul equivalences and dualities}},
Journal = {{PACIFIC JOURNAL OF MATHEMATICS}},
Year = {{2004}},
Volume = {{214}},
Number = {{2}},
Pages = {{359-378}},
Month = {{APR}},
Abstract = {{For every positively graded algebra A, we show that its categories of
   linear complexes of projectives and almost injectives (see definition
   below) are both naturally equivalent to the category of graded modules
   over the quadratic dual algebra A(!). In case A = Lambda is a graded
   factor of a path algebra with Yoneda algebra Gamma, we show that the
   category Lc(Gamma) of linear complexes of finitely generated right
   projectives over G is dual to the category of locally finite graded left
   modules over the quadratic algebra (Lambda) over tilde associated to
   Lambda. When Lambda is Koszul and Gamma is graded right coherent, we
   also prove that the suspended category (gr) over bar (Lambda) has a (
   triangulated) stabilization S((gr) over bar (Lambda)) which is
   triangle-equivalent to the bounded derived category of the `category of
   tails' fpgr(Gamma)/L(Gamma).}},
