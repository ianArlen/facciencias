@article{ ISI:000220972500008,
Author = {Torres, M and Vucetich, H},
Title = {{Limits on charge non-conservation from possible seasonal variations of
   the solar neutrino experiments}},
Journal = {{MODERN PHYSICS LETTERS A}},
Year = {{2004}},
Volume = {{19}},
Number = {{8}},
Pages = {{639-644}},
Month = {{MAR 14}},
Abstract = {{Variable speed of light (VSL) theories may lead to large violations of
   charge conservation that can be written in terms of a dimensionless
   parameter A. Similarly, charge non-conservation can arise in theories
   that break down Einstein's Equivalence Principle, parametrized in terms
   of re. It is shown that for such theories, the motion of the Earth with
   respect to the Sun would produce a seasonal variation for the SAGE and
   GALLEX-GNO experiments. Analyzing the reported counting rates for these
   experiments, a very stringent bound
   \textbackslash{}lambda\textbackslash{} less than or equal to 2 x 10(-19)
   is obtained, some 10(9) times smaller than previous ones. For the second
   example, the corresponding bound is
   \textbackslash{}Gamma(0)\textbackslash{} < 10(-18), which represents an
   improvement of 107 on the previous result. Furthermore, assuming that
   the previous conditions apply, the existing bound for the lifetime of
   the Ga-71 --> Ge-71 charge-nonconserving decay is improved as: tau(CNC)
   greater than or equal to 1.4 x 10(27) years. Similarly a new upper limit
   for the ratio of the charge-nonconserving to the normal weak decay of
   the neutron is obtained: Gamma(n --> p + v(e) + (v) over bar)/Gamma(n
   --> p + e + (v) over bar) less than or equal to 2 x 10(-27).}},
