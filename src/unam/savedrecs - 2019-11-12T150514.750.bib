@article{ ISI:000220381500021,
Author = {Guzman-Novoa, E and Uribe-Rubio, JL},
Title = {{Honey production by European, Africanized and hybrid honey bee (Apis
   mellifera) colonies in Mexico}},
Journal = {{AMERICAN BEE JOURNAL}},
Year = {{2004}},
Volume = {{144}},
Number = {{4}},
Pages = {{318-320}},
Month = {{APR}},
Abstract = {{The honey production of 118 European, 80 Africanized and 84 hybrid honey
   bee (Apis mellifera) colonies was recorded and compared. Hybrid colonies
   were of two types: 1) those obtained from open-mated European queens,
   and 2) those obtained from open-mated Africanized queens. European bee
   colonies yielded significantly more honey than Africanized and hybrid
   colonies of Africanized maternal ancestry. Hybrid bee colonies of
   European maternal ancestry produced slightly more honey than European
   ones, but their yields were not statistically different. The honey
   yields of Africanized bee colonies were not different from those of
   hybrid colonies of Africanized maternal ancestry. These results suggest
   maternal or dominance effects for honey production, at least from the
   European ancestor, and demonstrate that a high degree of Africanization
   in honey bee populations, decreases their honey yields. The artificial
   introduction of European genes into honey bee populations located in
   Africanized areas could contribute to increase their honey production.
   Different strategies to accomplish this goal are discussed.}},
