@article{ ISI:000221351000019,
Author = {Salcedo, R},
Title = {{Pnicogen heterobenzenes}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE-THEOCHEM}},
Year = {{2004}},
Volume = {{674}},
Number = {{1-3}},
Pages = {{125-129}},
Month = {{APR 9}},
Abstract = {{The behavior of the group 15 heterobenzenes is analyzed from a
   theoretical point of view (BPW91/6-311-G{*}{*}level) and compared with
   the very well-known behavior of pyridine. Special emphasis is deserved
