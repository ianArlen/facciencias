@article{ ISI:000220743700003,
Author = {Sobral, H and de Castro, RS and Villagran-Muniz, M and Navarro-Gonzalez,
   R},
Title = {{Time resolved study of simulated volcanic lightning by laser induced
   plasma in a plume of ablated basalt}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2004}},
Volume = {{31}},
Number = {{7}},
Month = {{APR 1}},
Abstract = {{Lightning activity is typically generated by explosive volcanic
   activity. In this work we report an experimental simulation of a
   volcanic lightning by focusing into a basaltic rock the emission of a
   pulsed Nd:YAG laser in air at atmospheric pressure (laser ablation). The
   temporal evolution of air breakdown in an ash plume was studied by
   emission spectroscopy and shadowgraphy. The electron temperature exceeds
   18x10(3) K and the electron density reaches about 10(18) cm(-3). The
   production yield of nitric oxide by simulated volcanic lightning was
   determined by gas chromatography coupled to a mass spectrometer and no
   substantial difference was found with that of simulated thunderstorm
   lightning by laser induced plasma.}},
