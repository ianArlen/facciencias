@article{ ISI:000220607200117,
Author = {Hirsch, JG and Frank, A and Velazquez, V},
Title = {{Residual regularities in liquid drop mass calculations}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{2004}},
Volume = {{69}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{A systematic study of correlations in the chart of nuclear masses
   calculated using the finite range droplet model of Moller is presented.
   It is shown that the differences between the calculated and measured
   masses have a well defined oscillatory component as function of the
   proton and neutron numbers, which can be removed with an appropriate
   fit, concentrating the error distribution on a single peak around zero.
   The presence of this regular residual correlations suggests that the
   Strutinsky method of including microscopic fluctuations in nuclear
   masses could be improved.}},
