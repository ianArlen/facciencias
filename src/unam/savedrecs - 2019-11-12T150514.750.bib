@article{ ISI:000188498900016,
Author = {Rojas-Escudero, E and Alarcon-Jimenez, AL and Elizalde-Galvan, P and
   Rojo-Callejas, F},
Title = {{Optimization of carbohydrate silylation for gas chromatography}},
