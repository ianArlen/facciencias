Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Moran, J (Reprint Author), Univ Nacl Autonoma Mexico, Inst Cell Physiol, Mexico City 04510, DF, Mexico.
   Univ Nacl Autonoma Mexico, Inst Cell Physiol, Mexico City 04510, DF, Mexico.
   Univ Nacl Autonoma Mexico, Inst Biotechnol, Mexico City 04510, DF, Mexico.}},
DOI = {{10.1111/j.0953-816X.2004.03321.x}},
ISSN = {{0953-816X}},
EISSN = {{1460-9568}},
