@article{ ISI:000221238700029,
Author = {Gonzalez-Rodriguez, JG and Rosales, I and Casales, M and Serna, S and
   Martinez, L},
Title = {{Corrosion performance of molybdenum silicides in acid solutions}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2004}},
Volume = {{371}},
Number = {{1-2}},
Pages = {{217-221}},
Month = {{APR 25}},
Abstract = {{The corrosion performance of Mo-22 at.\% Si, Mo-24 at.\% Si and Mo-25
   at.\% Si alloys in 0.5 M hydrochloric acid (HCI) and 0.5 M sulfuric acid
   (H2SO4), at room temperature, has been evaluated using electrochemical
   techniques. Employed techniques included potentiodynamic polarization
   curves (PC). linear polarization resistance (LPR) and electrochemical
   noise in current (ENC). The alloys contained either alpha-Mo or Mo5Si3
   in a Mo3Si matrix. The results show that in almost all cases the three
   alloys exhibit a passive behavior, and the corrosion resistance
   increases as follows: Mo-22 at.\% Si < Mo-25 at.\% Si < Mo-24 at.\% Si
   in both solutions. Also, for each alloy, in all cases there was a
   selective corrosion of the alpha-Mo and Mo5Si3 phases with respect to
   the Mo3Si matrix. (C) 2003 Elsevier B.V. All rights reserved.}},
