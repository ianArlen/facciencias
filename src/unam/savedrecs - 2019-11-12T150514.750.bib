@article{ ISI:000220163800050,
Author = {Deng, WY and Puente, JL and Gruenheid, S and Li, Y and Vallance, BA and
   Vazquez, A and Barba, J and Ibarra, JA and O'Donnell, P and Metalnikov,
   P and Ashman, K and Lee, S and Goode, D and Pawson, T and Finlay, BB},
Title = {{Dissecting virulence: Systematic and functional analyses of a
   pathogenicity island}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
Year = {{2004}},
Volume = {{101}},
Number = {{10}},
Pages = {{3597-3602}},
Month = {{MAR 9}},
Abstract = {{Bacterial pathogenicity islands (PAI) often encode both effector
   molecules responsible for disease and secretion systems that deliver
   these effectors to host cells. Human enterohemorrhagic Escherichia coli
   (EHEC), enteropathogenic E. coli, and the mouse pathogen Citrobacter
   rodentium (CR) possess the locus of enterocyte effacement (LEE) PAI. We
   systematically mutagenized all 41 CR LEE genes and functionally
   characterized these mutants in vitro and in a murine infection model. We
   identified 33 virulence factors, including two virulence regulators and
   a hierarchical switch for type III secretion. In addition, 7 potential
   type III effectors encoded outside the LEE were identified by using a
   proteomics approach. These non-LEE effectors are encoded by three
   uncharacterized PAIs in EHEC O157, suggesting that these PAIs act
   cooperatively with the LEE in pathogenesis. Our findings provide
   significant insights into bacterial virulence mechanisms and disease.}},
