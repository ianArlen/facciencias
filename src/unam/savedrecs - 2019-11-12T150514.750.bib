@article{ ISI:000221947200005,
Author = {Barron-Gutierrez, S and Garcia-Bojalil, C and Mora-Izaguirre, O and
   Shimada-Miyasaka, A},
Title = {{Economic impact of adipose tissue pigmentation in grazing cattle in the
   tropics}},
Journal = {{AGROCIENCIA}},
Year = {{2004}},
Volume = {{38}},
Number = {{2}},
Pages = {{173-179}},
Month = {{MAR-APR}},
Abstract = {{Yellow pigmentation of carcass adipose tissue of pasture-fed cattle is a
   marketing restriction, since the consumer often rejects the meat
   assuming that it comes from old and/or culled animals. To quantify
   losses caused by pigmented carcasses, information was collected from the
   packing plant operated by the Regional Cattlemen's Association of the
   State of Tabasco for the years 19881989 and 1993-1999, when 1339 720
   animals were slaughtered. The information was analyzed with a linear
   model that included year effects, season of the year, type of cattle and
   two-way interactions. There were more pigmented carcasses in 1994, 1995,
   1998 (pless than or equal to0.05); there were no significant effects for
   season of the year or for the interactions. The percentage of cows with
   pigmented carcasses (0.86\%) was higher (pless than or equal to0.05)
   than that of steers (0.45\%) and bulls (0.50\%); the lowest percentage
   was found in heifers (0.28\%). Yellow fat in the carcasses reduces meat
   grade by one point, as well as its price at the packing plant. In 1999,
   the domestic volume of beef carcasses with problems of pigmentation was
   calculated at 3855.1 t, with approximate economic losses of 10 million
   pesos (US\$900 000).}},
