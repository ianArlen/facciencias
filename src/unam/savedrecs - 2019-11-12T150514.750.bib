@article{ ISI:000221044100008,
Author = {Boullosa, RR and Santillan, AO},
Title = {{Ultrasound radiation from simple thermoacoustic transducers}},
Journal = {{ACTA ACUSTICA UNITED WITH ACUSTICA}},
Year = {{2004}},
Volume = {{90}},
Number = {{2}},
Pages = {{277-284}},
Month = {{MAR-APR}},
Abstract = {{Thermoacoustic equations are derived to describe the sound pressure
   radiation by a heated plane over a material substrate. By determining
   the velocity of an equivalent piston, the sound pressure emitted by a
   thermoacoustic source of finite dimensions can be calculated at any
   frequency. The measured sound pressures, obtained with devices
   constructed ad hoc for the experiments, are in good agreement with the
   theoretical expectations in the audio and ultrasound region, from 500 Hz
