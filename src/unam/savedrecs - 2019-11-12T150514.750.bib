@article{ ISI:000221363000002,
Author = {Raga, AC and Reipurth, B},
Title = {{Herbig-Haro jets emerging from a neutral cloud into a HII region}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2004}},
Volume = {{40}},
Number = {{1}},
Pages = {{15-23}},
Month = {{APR}},
Abstract = {{We present numerical simulations of a Herbig-Haro flow that bursts out
   of a dense cloud core into a surrounding Hit region. Such blow-outs from
   cloud cores have recently been imaged in various star-forming regions,
   and we here explore the properties of such outflows by varying selected
   input parameters to the models. We present time-series of temperature,
   density, ionization fraction, and Ha emission. We find that the Ha
   emission of the outflows is mostly controlled by the impinging ionizing
   photon field rather than by the intrinsic properties of the outflows.}},
