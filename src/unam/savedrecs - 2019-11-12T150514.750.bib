@article{ ISI:000229917100024,
Author = {Garcia, AJ and Angulo, E and Blackall, PJ and Ortiz, AM},
Title = {{The presence of nicotinamide adenine dinucleotide-independent
   Haemophilus paragallinarum in Mexico}},
Journal = {{AVIAN DISEASES}},
Year = {{2004}},
Volume = {{48}},
Number = {{2}},
Pages = {{425-429}},
Month = {{APR-JUN}},
Abstract = {{Two isolates of Haemophilus paragallinarum were obtained from a layer
   chicken in Mexico. The isolates were confirmed as H. paragallinarum by
   polymerase chain reaction and conventional biochemical identification.
   The isolates were nicotinamide adenine dinucleotide (NAD)
   independent-growing on blood agar without the need of a nurse colony as
   well as on a complex medium that lacked both NAD and chicken serum. Both
   isolates were pathogenic, causing the typical clinical signs of
   infectious coryza in susceptible chickens. One isolate was Page serovar
   B/Kume serovar B-1 and the other isolate was Page serovar C/Kume serovar
   C-2. The isolates were associated with a field outbreak that involved an
   egg drop of 20\% over a 3-wk period and a doubling of weekly mortality
   (from 0.1\% to 0.2\%). This is the first report of NAD-independent H.
   paragallinarum outside South Africa and is the first time that
   NAD-independent H. paragallinarum of serovar B has been reported.}},
