@article{ ISI:000189381900021,
Author = {Ryan, MP},
Title = {{Quantum collapse of Newtonian dust shells}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{2004}},
Volume = {{21}},
Number = {{3}},
Pages = {{S323-S338}},
Month = {{FEB 7}},
Abstract = {{This paper considers a Newtonian approach to the quantum collapse of
   infinitesimally thin dust shells. It is possible to use elementary
   Newtonian calculations to find the equations of motion of the shell in
   terms of variables defined on the shell. The quantum problem reduces to
   the analogue of the non-relativistic hydrogen atom. One can study the
   Newtonian analogue of horizon formation by defining a `horizon' as the
   point where the escape velocity at the surface of the shell becomes the
   speed of light. Various definitions of horizon formation in the quantum
   case will be considered.}},
