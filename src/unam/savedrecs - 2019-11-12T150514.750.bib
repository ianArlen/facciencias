@article{ ISI:000189075000022,
Author = {Ozuna, O and Hirata, GA and McKittrick, J},
Title = {{Luminescence enhancement in Eu3+-doped alpha- and gamma-Al2O3 produced
   by pressure-assisted low-temperature combustion synthesis}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2004}},
Volume = {{84}},
Number = {{8}},
Pages = {{1296-1298}},
Month = {{FEB 23}},
Abstract = {{Intense red luminescence in Eu3+-doped gamma (gamma) and alpha (alpha)
   alumina (Al2O3) phosphors obtained by direct and indirect combustion
   synthesis at low-temperatures is reported. gamma and
   alpha-(Al1-xEux)(2)O-3 are easily produced by combustion synthesis at
   280 degreesC in the range of x=0.001-0.06 at. \%. The well-defined
   direct synthesis allows europium ions to incorporate into the alpha or
   gamma alumina lattice in spite of the large size difference between
   these ions and the aluminum cations in Al2O3. These materials yield a
   strong fluorescence at room temperature due to f-f transition lines
   within Eu3+ (4f(6)) electron emission configuration. Furthermore, from
   luminescence measurements, it is deduced that Eu3+ occupy low-symmetry
   sites in the Al2O3 lattice. (C) 2004 American Institute of Physics.}},
