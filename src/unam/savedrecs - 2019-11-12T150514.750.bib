@article{ ISI:000221056100002,
Author = {Hernandez, S and Castillo, EA and Ogawa, T},
Title = {{Diacetylene-containing polymers XIV:
   Poly(octa-3,5-diynylene-1,8-dicarboxyalates) containing some polar azo
   dyes}},
Journal = {{POLYMER BULLETIN}},
Year = {{2004}},
Volume = {{51}},
Number = {{5-6}},
Pages = {{333-342}},
Month = {{APR}},
Abstract = {{A series of novel polymers having polar azo dyes were synthesized and
   characterized. Although their chromophore contents are very high, their
   second order nonlinear (2-NLO) response was very poor. The poor 2-NLO
   responses are thought to be due to poor orientation because of their
   improper main chain conformations, as well as their low glass transition
   temperatures. They undergo slow cross-linking on standing at room
   temperature, and rapidly on heating at temperalures above 150degreesC,
   through the diacetylene units of the main chains. Nevertheless, these
   polymers are interesting materials as azo-polymers because of their very
   high concentration of azo dyes, which are distributed homogeneously in
   the polymers. Synthesis and characterization are described, and the most
   important factor for the 2-NLO polymers is discussed.}},
