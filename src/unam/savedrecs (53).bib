@article{ ISI:000329158900030,
Author = {Batta, Aldo and Lee, William H.},
Title = {{Cooling-induced structure formation and evolution in collapsars}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
