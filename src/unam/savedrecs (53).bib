@inproceedings{ ISI:000370073805107,
Author = {Sanchez, Tonametl and Moreno, Jaime A.},
Book-Group-Author = {{IEEE}},
Title = {{A Constructive Lyapunov function design method for a class of
   homogeneous systems}},
Booktitle = {{2014 IEEE 53RD ANNUAL CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2014}},
Pages = {{5500-5505}},
