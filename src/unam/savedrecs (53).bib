@article{ ISI:000331391800014,
Author = {Sieron, Katrin and Capra, Lucia and Rodriguez-Elizararras, Sergio},
Title = {{Hazard assessment at San Martin volcano based on geological record,
   numerical modeling, and spatial analysis}},
Journal = {{NATURAL HAZARDS}},
Year = {{2014}},
Volume = {{70}},
Number = {{1}},
Pages = {{275-297}},
Month = {{JAN}},
Abstract = {{The San Martin shield volcano, located in the Los Tuxtlas Volcanic
   Field, has experienced effusive shield-building activity, as well as
   explosive eruptions, as evidenced by direct observations during the last
   eruption in 1793. The threat to the surrounding villages consists
   principally of lahars, especially because of the tropical climate in the
   region. Ash fallout and lava flows represent additional hazards. In
   addition, the surrounding Quaternary monogenetic field includes more
   than 300 scoria cones and about 40 explosion craters (mainly maars) that
   also represent a hazard source. In the present study we constructed
   hazard maps using field data, orthophotos, spatial analysis, and
   specialized software (LAHARZ and HAZMAP) to deliminate lahar inundation
   zones, areas that could potentially be affected by ash fallout
   (including the evaluation of houses prone to roof collapse due to ash
   load), and the most susceptible areas for hosting future monogenetic
   vent formation.}},
