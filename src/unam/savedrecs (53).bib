@article{ ISI:000330621500006,
Author = {Francisco Flores-Resendiz, J. and Tang, Yu},
Title = {{Adaptive control for coupled lagrangian systems with structured
   uncertainty in the interconnections}},
Journal = {{INTERNATIONAL JOURNAL OF ADAPTIVE CONTROL AND SIGNAL PROCESSING}},
Year = {{2014}},
Volume = {{28}},
Number = {{1}},
Pages = {{63-76}},
Month = {{JAN}},
Abstract = {{In this paper, an adaptive control scheme for coupled lagrangian systems
   is proposed. The interconnection terms among subsystems are assumed to
   have known structure and depend nonlinearly on uncertain parameters.
   Also, these coupling functions are restricted to those which are concave
   or convex with respect to the parameters. Two parameter adaptation laws
   are used to estimate local and interconnection unknown parameters. Both
   centralized and decentralized controls are designed. Ultimately bounded
   tracking with arbitrary precision is achieved regardless of ability to
   share information among subsystem. A numerical simulation, consisting of
   two pendulums interconnected by a nonlinear spring, is included to
   illustrate the effectiveness of the proposed schemes. Copyright (c) 2013
   John Wiley \& Sons, Ltd.}},
