@article{ ISI:000331215200027,
Author = {Figueroa, Aldo and Meunier, Patrice and Cuevas, Sergio and Villermaux,
   Emmanuel and Ramos, Eduardo},
Title = {{Chaotic advection at large Peclet number: Electromagnetically driven
   experiments, numerical simulations, and theoretical predictions}},
Journal = {{PHYSICS OF FLUIDS}},
Year = {{2014}},
Volume = {{26}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{We present a combination of experiment, theory, and modelling on laminar
   mixing at large Peclet number. The flow is produced by oscillating
   electromagnetic forces in a thin electrolytic fluid layer, leading to
   oscillating dipoles, quadrupoles, octopoles, and disordered flows. The
   numerical simulations are based on the Diffusive Strip Method (DSM)
   which was recently introduced (P. Meunier and E. Villermaux, ``The
   diffusive strip method for scalar mixing in two-dimensions,{''} J. Fluid
   Mech. 662, 134-172 (2010)) to solve the advection-diffusion problem by
   combining Lagrangian techniques and theoretical modelling of the
   diffusion. Numerical simulations obtained with the DSM are in reasonable
   agreement with quantitative dye visualization experiments of the scalar
   fields. A theoretical model based on log-normal Probability Density
   Functions (PDFs) of stretching factors, characteristic of homogeneous
   turbulence in the Batchelor regime, allows to predict the PDFs of scalar
   in agreement with numerical and experimental results. This model also
   indicates that the PDFs of scalar are asymptotically close to log-normal
   at late stages, except for the large concentration levels which
   correspond to low stretching factors. (C) 2014 AIP Publishing LLC.}},
