Title = {{Three-Component Reaction of Tautomeric Amidines with
   3-Ferrocenylmethylidene-2,4-pentanedione. Formation of Polymeric
   Coordination Complexes of Potassium Ferrocenyl-(hexahydro)pyrimidoxides}},
Journal = {{MOLECULES}},
Year = {{2014}},
Volume = {{19}},
