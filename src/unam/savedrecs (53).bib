Title = {{Photoluminescence Spectroscopy as a Tool for Quality Control of GaN Thin
   Film to be used in Solar Cell Devices}},
Booktitle = {{2014 IEEE 40TH PHOTOVOLTAIC SPECIALIST CONFERENCE (PVSC)}},
Year = {{2014}},
