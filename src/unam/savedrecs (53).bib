@article{ ISI:000334303300010,
Author = {Dalla Via, Lisa and Garcia-Argaez, Aida N. and Braga, Alessandra and
   Martinez-Vazquez, Mariano and Grancara, Silvia and Martinis, Pamela and
   Agostinelli, Enzo and Toninello, Antonio},
Title = {{An Eudesman Derivative from Verbesina persicifolia DC as a Natural Mild
   Uncoupler in Liver Mitochondria. A New Potential Anti-obesity Agent?}},
Journal = {{CURRENT PHARMACEUTICAL DESIGN}},
Year = {{2014}},
Volume = {{20}},
Number = {{2}},
Pages = {{253-261}},
Month = {{JAN}},
Abstract = {{4 beta-cinnamoyloxy,1 beta,3 alpha-dihydroxyeudesm-7,8-ene (CDE)
   extracted from Verbesina persicifolia induces bioenergetic collapse in
   rat liver mitochondria (RLM), monitored as a fall in the respiratory
   control index and ADP/O values. This fall in energy is accompanied by a
   protonophore effect and membrane potential (Delta psi) collapse,
   demonstrating that CDE behaves as a typical uncoupling agent. However,
   when examining the effect of CDE in detail, we found that it acts as a
   ``mild{''} uncoupler because it drops Delta psi and increases
   respiratory state 4. The proposed mechanism is based on the interaction
   of CDE with membrane protein cytochrome C oxidase, which is implicated
   in proton permeability, and with the respiratory chain for the
   generation of reactive oxygen species which mediate and regulate the
   activity of the above membrane protein. Considering the energy collapse,
   ``mild{''} uncoupling, and the fact that CDE is largely used in folk
   medicines, this extract may be viewed as a potentially effective
   anti-obesity drug and a natural lead compound for developing new natural
   uncouplers against obesity.}},
