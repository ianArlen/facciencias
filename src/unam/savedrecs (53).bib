@article{ ISI:000342604300010,
Author = {Lemus-Santana, Elia and Sanvicente-Anorve, Laura and Hermoso-Salazar,
   Margarita and Flores-Coto, Cesar},
Title = {{The holoplanktonic Mollusca from the southern Gulf of Mexico. Part 2:
   pteropods}},
Journal = {{CAHIERS DE BIOLOGIE MARINE}},
Year = {{2014}},
Volume = {{55}},
Number = {{2}},
Pages = {{241-258}},
Abstract = {{This study analyzes the vertical and horizontal distribution of pteropod
   mollusks in the neritic waters from the southern Gulf of Mexico. A total
   of 187 plankton samples were collected at five strata within the water
   column (0-6, 6-12, 12-18, 45-55, and 95-105 m) over a grid of 28
   oceanographic stations using an opening-closing net system during May
   and November 1995. In total, 27 pteropod species were identified, from
   which Creseis clava (57\%), Limacina inflata (18\%), L. trochiformis
   (13\%) and C. virgula (7\%) were the most abundant species, accounting
   for almost 95\% of the total abundance. Horizontally, these species had
   a wide distribution over the shelf waters, and vertically, the major
   pteropod densities were recorded in the 0-18 m upper layer. The
   congeneric species L. trochiformis and L. inflata registered their
   highest abundance in May and November respectively, suggesting a pattern
   of seasonal dominance to avoid competition for feeding resources. The
   gymnosomes Cephalobrachia macrochaeta, Notobranchaea macdonaldi and
   Pneumoderma violaceum registered the lowest abundances, likely related
   to their highly specialized diets. The species Cephalobrachia
   macrochaeta and Corolla intermedia were recorded for the first time in
   the western Atlantic. Worldwide distribution of the 27 pteropod species
   is exhaustively documented.}},
