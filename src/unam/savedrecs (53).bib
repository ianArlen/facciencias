Title = {{Potential for genotoxic and reprotoxic effects of vanadium compounds due
   to occupational and environmental exposures: An article based on a
   presentation at the 8th International Symposium on Vanadium Chemistry,
   Biological Chemistry, and Toxicology, Washington DC, August 15-18, 2012}},
Journal = {{JOURNAL OF IMMUNOTOXICOLOGY}},
Year = {{2014}},
Volume = {{11}},
