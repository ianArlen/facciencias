@article{ ISI:000329598900008,
Author = {Alejandra Gonzalez-Garcia, Diana and Acuna, Laura},
Title = {{SOCIAL ACCEPTANCE AND REJECTION OF PEOPLE WITH DIFFERENT BODY SIZE}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2014}},
Volume = {{31}},
Number = {{1}},
Pages = {{58-70}},
Month = {{JAN-JUN}},
Abstract = {{Although thinness is socially accepted and obesity rejected, how people
   react to different body sizes is unknown. According to body size, 275
   adults ordered silhouettes of men and women. The body mass index of men
   and women correlated with the order given to their silhouettes by the
   participants. Participants indicated which silhouette represented each
   of 14 positive and 14 negative characteristics. Noticeably, all the
   silhouettes received both positive and negative nominations, although in
   different degree. The second feminine and the fourth masculine
   silhouettes received the highest number of positive nominations, while
   the heaviest silhouettes received the highest number of negative
   nominations. It was concluded that there is neither an indiscriminate
   acceptance towards thinness, nor a generalized rejection towards
   obesity.}},
