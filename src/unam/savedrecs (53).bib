Author = {Ortiz-Rascon, E. and Bruce, N. C. and Rodriguez-Rosales, A. A. and
   Garduno-Mejia, J. and Ortega-Martinez, R.},
Title = {{Improved spatial resolution in time-resolved transillumination imaging
   using temporal extrapolation with the cumulant expansion solution to the
   transport equation}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2014}},
Volume = {{60}},
Number = {{1}},
Pages = {{1-7}},
Month = {{JAN-FEB}},
Abstract = {{This paper presents results of a time-resolved transillumination imaging
   method using temporal extrapolation. The temporal extrapolation is
   performed with the cumulant expansion solution to the transport
   equation. The results obtained are compared to results of the same
   method but using the diffusion approximation solution. It is found that
   the results are consistent but that the cumulant expansion method gives
   better resolution, by a factor of approximately 3, for the imaging
   process, because it gives a better estimation of the photon contribution
   for shorter integration times.}},
