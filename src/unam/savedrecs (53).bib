@article{ ISI:000332429400002,
Author = {Del Olmo-Ruiz, Mariana and Arnold, A. Elizabeth},
Title = {{Interarmual variation and host affiliations of endophytic fungi
   associated with ferns at La Selva, Costa Rica}},
