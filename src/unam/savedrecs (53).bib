@article{ ISI:000344997800051,
Author = {Perez-Gonzalez, A. and Munoz-Rugeles, L. and Alvarez-Idaboy, J. R.},
Title = {{Tryptophan: antioxidant or target of oxidative stress? A quantum
   chemistry elucidation}},
Journal = {{RSC ADVANCES}},
Year = {{2014}},
Volume = {{4}},
Number = {{99}},
Pages = {{56128-56131}},
Abstract = {{The Density Functional Theory was used to investigate oxidative stress
   related reactions of tryptophan in its free zwitterionic form. It was
   concluded that free tryptophan cannot be considered as an antioxidant,
   and that tryptophan residues in proteins are not especially good targets
   of oxidative stress. Its previously observed antioxidant abilities can
   then be attributed to tryptophan metabolites.}},
