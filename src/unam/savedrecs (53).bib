@article{ ISI:000331163300010,
Author = {Soto-Hernandez, Silvano and Naumis-Pena, Catalina},
Title = {{Library science analysis of Mexican television news on the web}},
Journal = {{PROFESIONAL DE LA INFORMACION}},
Year = {{2014}},
Volume = {{23}},
Number = {{1}},
Pages = {{80-86}},
Month = {{JAN-FEB}},
Abstract = {{Television is unquestionably a vehicle that socializes the journalistic
   tale of the human experience. The evolution of journalistic
   documentation and the need to respond to the organization of content
   generated in video collections and television archives, in particular
   news broadcasts, have created new areas and niches of opportunity for
   library and information science professionals. The research that is
   being carried out in Mexico on the analysis of news broadcasts and
   content representation on the web is presented. The documentary news
   treatment is crucial to strengthen the quality of content representation
   and ensure the relevance of discourse analysis for its organization,
   accessibility and preservation. The visibility of television news on the
   Web could be lessened due to deficiencies in metadata description.}},
Publisher = {{EPI}},
Address = {{APARTADO 32 280, BARCELONA, 08080, SPAIN}},
