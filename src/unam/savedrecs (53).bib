@inproceedings{ ISI:000453217600081,
Author = {Catalan-Torrecilla, Cristina and Gil de Paz, Armando and
   Castillo-Morales, Africa and Iglesias-Paramo, Jorge and Sanchez,
   Sebastian F. and CALIFA Collaboration},
Editor = {{Ziegler, BL and Combes, F and Dannerbauer, H and Verdugo, M}},
Title = {{Star Formation in the Local Universe from the CALIFA sample: calibration
   and contribution of disks to the SFR density}},
Booktitle = {{GALAXIES IN 3D ACROSS THE UNIVERSE}},
Series = {{IAU Symposium Proceedings Series}},
Year = {{2014}},
