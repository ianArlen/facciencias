@article{ ISI:000330752500019,
Author = {Licea-Rodriguez, Jacob and Rocha-Mendoza, Israel and Rangel-Rojo, Raul
   and Rodriguez-Fernandez, Luis and Oliver, Alicia},
Title = {{Femtosecond laser writing over silver nanoparticles system embedded in
   silica using nonlinear microscopy}},
Journal = {{OPTICAL MATERIALS}},
Year = {{2014}},
Volume = {{36}},
Number = {{3}},
Pages = {{682-686}},
Month = {{JAN}},
Abstract = {{We present results for the induction and monitoring of structural
   modification of a composite consisting of elongated silver nanoparticles
   films embedded in silica using ultrafast femtosecond laser irradiation
   and second harmonic generation imaging, respectively. Waveguide-like
   patterns are written and characterized under a laser scanning nonlinear
   microscope system by simply changing the laser fluence in the sample;
   switching in this way between two different physical processes occurring
   only within the composite film: second harmonic generation and laser
   induced nanoparticles removal. A study of the nanoparticles damage
   process as a function of the laser energy, polarization and scan
   velocity is also presented and discussed. The use of the non-irradiated
   zone between two written channels is proposed as a potential linear and
   nonlinear optical waveguide. (C) 2013 Elsevier B.V. All rights reserved.}},
