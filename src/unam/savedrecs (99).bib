@inproceedings{ ISI:000263872000019,
Author = {Cervantes-Sodi, B. and Hernandez, X. and Park, Ch. and Kim, J.},
Editor = {{Allen, C and Ruelas, A and Teixeira, R}},
Title = {{ENVIRONMENT AND MASS DEPENDENCIES OF GALACTIC lambda SPIN PARAMETER:
   COSMOLOGICAL SIMULATIONS AND SDSS GALAXIES COMPARED}},
Booktitle = {{IV REUNION SOBRE ASTRONOMIA DINAMICA EN LATINOAMERICA (ADELA 2008)}},
