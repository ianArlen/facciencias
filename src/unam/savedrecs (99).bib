@article{ ISI:000257806700018,
Author = {Magali Alvarez-Chavez, Luz and de los Angeles Valdivia-Lopez, Maria and
   de Lourdes Aburto-Juarez, Maria and Tecante, Alberto},
Title = {{Chemical characterization of the lipid fraction of Mexican chia seed
   (Salvia hispanica L.)}},
Journal = {{INTERNATIONAL JOURNAL OF FOOD PROPERTIES}},
Year = {{2008}},
Volume = {{11}},
Number = {{3}},
Pages = {{687-697}},
Abstract = {{The lipid fraction of Jalisco and Sinaloa chia seeds was analyzed for
   oil content, fatty acids, squalene and phytosterols. The mean oil
   content of seeds from these two regions was significantly (p <= 0.05)
   different, but with a similar composition of alpha-linolenic, linoleic,
   oleic, palmitic and stearic acids. Total phytosterols in the oil ranged
   from 7 to 17 g/kg. beta-sitosterol accounted for up to 74\% of the total
   unsaponified fraction. The seeds contained less than 0.5 g/kg of
   squalene. The oil is an attractive source of omega-3 linolenic acid and
   phytosterols but a poor source of squalene.}},
