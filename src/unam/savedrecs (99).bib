@article{ ISI:000254041300114,
Author = {Hernandez-Valle, F. and Navarrete, M. and Mejia, E. V. and
   Villagran-Muniz, M.},
Title = {{Trace analysis of pesticides in water using pulsed photoacoustic
   technique}},
Journal = {{EUROPEAN PHYSICAL JOURNAL-SPECIAL TOPICS}},
Year = {{2008}},
Volume = {{153}},
Pages = {{507-510}},
Month = {{JAN}},
Note = {{14th International Conference on Photoacoustic and Photothermal
   Phenomena/1st US-Egypt Workshop on Photoacoustic and Photothermal
   Phenomena, Cairo, EGYPT, JAN 07-11, 2007}},
Abstract = {{Trace concentration of pesticides such as Atrazine and Methyl parathion
   in water solutions has harmful health effects. In this work a
   methodology to detect trace concentrations for both pesticides has been
   developed. The pesticide solution contained into a quartz cell is
   irradiated by a pulsed laser at lambda = 230 nm, this energy is absorbed
   depending on the pesticide concentration; the non absorbed light that
   sample allows to pass is absorbed by the NaCl: Mn(2+) doped single
   crystal fixed to a piezoelectric sensor of 5MHz to obtain the
   photoacoustic (PA) signal. The results showed that is possible to detect
   concentrations around tenths of ng/l.}},
