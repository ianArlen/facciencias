@article{ ISI:000259036700002,
Author = {Ana Tovar-Hernandez, Maria},
Title = {{Phylogeny of Chone Kroyer, 1856 (Polychaeta : Sabellidae) and related
   genera}},
Journal = {{JOURNAL OF NATURAL HISTORY}},
Year = {{2008}},
Volume = {{42}},
Number = {{33-34}},
Pages = {{2193-2226}},
Abstract = {{An analysis was undertaken to test the monophyly of the sabellid genus
   Chone Kroyer using maximum parsimony methodology. Cladistic analysis
   revealed three monophyletic genera whose respective members were
   assigned previously to Chone. Chone sensu stricto (containing the type
   species) is defined by the presence of broadly rounded dorsal lips and
   thoracic uncini with a large tooth above the main fang, followed by a
   series of smaller teeth. Dialychone Claparede is re-established for the
   second genus and a new name - Paradialychone - is proposed for terminals
   nested in the third clade. Dialychone and Paradialychone gen. nov. are
   the sister groups of Chone, defined by the presence of a simple
   pre-pygidial depression and modified posterior abdominal uncini.
   Amphicorina Claparede is basal within Sabellinae and Euchone Malmgren is
   the sister group of Chone, Paradialychone gen. nov. and Dialychone. The
   analysis supports the transfer of C. princei and C. reayi to Jasmineira
   Langerhans.}},
