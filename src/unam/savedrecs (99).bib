Title = {{The Guerrero Composite Terrane of western Mexico: Collision and
   subsequent rifting in a supra-subduction zone}},
Booktitle = {{FORMATION AND APPLICATIONS OF THE SEDIMENTARY RECORD IN ARC COLLISION
   ZONES}},
Series = {{Geological Society of America Special Papers}},
Year = {{2008}},
