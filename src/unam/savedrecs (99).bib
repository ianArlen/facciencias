Author = {Caldu-Primo, A. and Cruz-Gonzalez, I.},
Editor = {{Allen, C and Ruelas, A and Teixeira, R}},
Title = {{MASSIVE STAR FORMATION IN TWO SPIRAL GALAXIES: NGC 3938 AND NGC 3184;
   MULTIWAVELENGTH ANALYSIS}},
