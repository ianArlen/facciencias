@inproceedings{ ISI:000262400000019,
Author = {Suzan, Gerardo and Marce, Erika and Giermakowski, J. Tomasz and Armien,
   Blas and Pascale, Juan and Mills, James and Ceballos, Gerardo and Gomez,
   Andres and Aguirre, A. Alonso and Salazar-Bravo, Jorge and Armien,
   Anibal and Parmenter, Robert and Yates, Terry},
Editor = {{Sparagano, OAE and Maillard, JC and Figueroa, JV}},
Title = {{The Effect of Habitat Fragmentation and Species Diversity Loss on
   Hantavirus Prevalence in Panama}},
Booktitle = {{Animal Biodiversity and Emerging Diseases: Prediction and Prevention}},
Series = {{ANNALS OF THE NEW YORK ACADEMY OF SCIENCES}},
Year = {{2008}},
