@inproceedings{ ISI:000262477000014,
Author = {Tecpoyotl-Torres, M. and Escobedo-Alatorre, J. and Gomez-Vicario, M. A.
   and Campos-Alvarez, J. and Espinoza-Mendoza, J. and Vera-Dimas, G. and
   Vargas-Bernal, R.},
Editor = {{SymkoDavies, M}},
Title = {{Dspic Control System of a Solar Follower}},
Booktitle = {{HIGH AND LOW CONCENTRATION FOR SOLAR ELECTRIC APPLICATIONS III}},
