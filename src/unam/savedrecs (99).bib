Editor = {{Oleschko, K and Cherkasov, S and PalacioPrieto, JL and TorresArguelles, V and GaonaSalado, CI and CastanedaMiranda, AG and ZamoraCastro, SA}},
Title = {{Fractal image informatics: From SEM to DEM}},
Booktitle = {{GIS IN GEOLOGY AND EARTH SCIENCES}},
