@article{ ISI:000255022100007,
Author = {Marin, L. E. and Balcazar, M. and Ortiz, M. and Steinich, B. and
   Hernandez-Espriu, J. A.},
Title = {{Comparison of elevation heights using a Differential Global Positioning
   System (DGPS) and a Total Station}},
Journal = {{GEOFISICA INTERNACIONAL}},
Year = {{2008}},
Volume = {{47}},
Number = {{1}},
Pages = {{81-83}},
Month = {{JAN-MAR}},
Abstract = {{Differences in elevation for a set of 12 different points were
   determined using a Total Station and a DGPS three receivers with
   occupation times of 90 minutes per station. A comparison between both
   sets of elevation values shows differences of two to 36 millimeters for
   a maximum baseline of 2,420 meters. These results show that differential
   GPS surveys may be used to determined differences in elevation using the
   differential GPS technique.}},
