Author = {Simon, J. -Abraham and Burgos-Vargas, Ruben},
Title = {{Vitiligo improvement in a patient with Ankylosing spondylitis treated
   with infliximab}},
Journal = {{DERMATOLOGY}},
Year = {{2008}},
Volume = {{216}},
Number = {{3}},
Pages = {{234-235}},
Abstract = {{Ankylosing spondylitis (AS) is a disabling disease affecting the
   enthesis and joints of the spine and peripheral sites. Tumour necrosis
   factor alpha (TNF-alpha) is a proinflammatory mediator which plays a
   significant role in the pathogenesis of AS. Infliximab (a chimeric
   monoclonal antibody which blocks the activity of TNF-alpha) is one of
   the most effective therapies of AS thus far. Vitiligo is a depigmenting
   disorder of the skin of unknown aetiology affecting 0.5-1\% of the
   population. Here we describe the effect of infliximab on vitiligo in a
   patient with AS. Copyright (c) 2008 S. Karger AG, Basel.}},
