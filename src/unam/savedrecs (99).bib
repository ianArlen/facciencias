@article{ ISI:000252297300016,
Author = {Jimenez-Estrada, M. and Reyes-Chilpa, R. and De Esparza-Villarreal, R.
   Ruiz and Jankowski, C. K. and Vancalsteren, M. R.},
Title = {{Structure of 1 beta-glucosyl-piquerol A: Storage of an allelopathic
   monoterpene}},
Journal = {{ALLELOPATHY JOURNAL}},
Year = {{2008}},
Volume = {{21}},
Number = {{1}},
Pages = {{191-197}},
Month = {{JAN}},
Abstract = {{The aerial parts of Piqueria trinervia Cav. (Asteraceae) afforded a new
   compound, 1 beta-glucosyl-piquerol A and its structure was established
   by 1D and 2D NMR, IR, UV and Mass Spectrometry. In contrast with its
   aglycone, 1 beta-glucosyl-piquerol A did not inhibit seed germination,
   and radicle elongation of Amaranthus hypochondriacus and Echinochloa
   crus-galli. Considering P. trinervia have medicinal properties, 1
   beta-glucosyl piquerol A was found inactive against the human pathogenic
   microorganisms Bacillus subtilis, Escherichia coli, Vibrio cholerae,
   Salmonella typhi. and Staphylococcus aureus. It is proposed that
   synthesis of this monoterpene glycoside could be a potential mechanism
   to, avoid autotoxicity, while hydrolysis in the soil by a microbial
   glycosidase could release the allelopathic compound Piquerol A.
   Alternatively mechanical damage of P. trinervia leaves, may also induce
   hydrolysis of 1 beta-glucosyl-piquerol A by an endogenous glycosidase.}},
