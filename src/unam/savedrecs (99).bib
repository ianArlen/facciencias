@article{ ISI:000252932200002,
Author = {de la Macorra, A.},
Title = {{Interacting dark energy: generic cosmological evolution for two scalar
   fields}},
Journal = {{JOURNAL OF COSMOLOGY AND ASTROPARTICLE PHYSICS}},
Year = {{2008}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{We study the cosmological evolution of two coupled scalar fields with an
   arbitrary interaction term V-T(phi,phi) in the presence of a barotropic.
   uid, which can be matter or radiation. The force between the barotropic.
   uid and the scalar fields is only gravitational. We show that the
   dynamics is completely determined by only three parameters lambda(i), i
   = 1, 2, 3. We determine all critical points and study their stability.
   We find six different attractor solutions depending on the values of
   lambda(i) and we calculate the relevant cosmological parameters. We
   discuss the possibility of having one of the scalar fields as of dark
   energy while the other could be a scalar field redshifting as matter.}},
