@article{ ISI:000252651700003,
Author = {Terrados, Jorge and Ramirez-Garcia, Pedro and Hernandez-Martinez, Oscar
   and Pedraza, Karla and Quiroz, Agustin},
Title = {{State of Thalassia testudinum Banks ex Konig meadows in the Veracruz
   Reef System, Veracruz, Mexico}},
Journal = {{AQUATIC BOTANY}},
