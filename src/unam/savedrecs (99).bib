@article{ ISI:000255022100003,
Author = {Flores-Marquez, E. L. and Martinez-Serrano, R. G. and Chavez, R. E. and
   Crusillo, Y. and Jimenez, G. and Campos-Enriquez, O.},
Title = {{Numerical modeling of Etla Valley aquifer, Oax., Mexico: Evolution and
   remediation scenarios}},
Journal = {{GEOFISICA INTERNACIONAL}},
Year = {{2008}},
Volume = {{47}},
Number = {{1}},
Pages = {{27-40}},
Month = {{JAN-MAR}},
Abstract = {{Short-term evolution (for 2001, 2005 and 2015) for the shallow aquifer
   of Etla Valley, Oaxaca, southern Mexico, was simulated based in a 3D
   hydrological model elaborated from the available geological,
   geophysical, geochemical, and hydrologic parameters. The numerical
   simulations were based on Visual MODFLOW code. These simulations
   indicate that, if the actual extraction regime is maintained, the
   drawdown of the potentiometric surface will get worse to the SE of the
   study area (i.e. beneath Oaxaca city). The prevailing aquifer flow
   direction favors the ground water pollution by fertilizers and leakage
   from the sewage network (dumped to the Atoyac river).
   According to the numerical simulation, remediation of this situation is
   possible if the wells located in the neigborhood of Oaxaca City are
   relocated at the recharge zones (i.e. at the feet from Sierra de
   Juarez). This remediation scenario will allow a recovery of the drawdown
   of the potentiometric surface.}},
