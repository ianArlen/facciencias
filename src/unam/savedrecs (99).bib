@article{ ISI:000262284400002,
Author = {Castaneda de la Paz, Maria},
Title = {{Illegal conversion of elements and symbols of legitimacy in indigenous
   nobility. The case of the cacicazgo of Tlatelolco}},
Journal = {{ANUARIO DE ESTUDIOS AMERICANOS}},
Year = {{2008}},
Volume = {{65}},
Number = {{1}},
Pages = {{21-47}},
Abstract = {{The documentation concerning the cacicazgo of Tlatelolco is abundant but
   problematic due to the incongrous nature of much of the information. It
   is necessary to apply a meticulous philological analysis of the sources
   in order to avoid errors in our historical interpretations. As such, the
   present article is a sample of the fascinating world of historical
   elaboration which includes false documents and illegitimate appropiation
   of symbols and historical feats. I will show this through a case study
   of the Tlatelolco cacique Don Diego de Mendoza ( 1549-1562).}},
