@article{ ISI:000208974900015,
Author = {Hayashi, Nakao and Naumkin, Pavel I.},
Title = {{ASYMPTOTIC BEHAVIOR FOR A QUADRATIC NONLINEAR SCHRODINGER EQUATION}},
Journal = {{ELECTRONIC JOURNAL OF DIFFERENTIAL EQUATIONS}},
Year = {{2008}},
Abstract = {{We study the initial-value problem for the quadratic nonlinear
   Schrodinger equation
   iu(t) + 1/2u(xx) = partial derivative(x)(u) over bar(2), x is an element
   of R, t > 1,
   u(1, x) = u(1)(x), x is an element of R.
   For small initial data u(1) is an element of H-2,H-2 we prove that there
   exists a unique global solution u is an element of C({[}1, infinity);
   H-2,H-2) of this Cauchy problem. Moreover we show that the large time
   asymptotic behavior of the solution is defined in the region vertical
   bar x vertical bar <= C root t by the self-similar solution 1/root t
   MS(x/root t) such that the total mass
   1/root t integral(R) MS( x/root t )dx = integral(R) u(1)(x)dx,
   and in the far region vertical bar x vertical bar > root t the
   asymptotic behavior of solutions has rapidly oscillating structure
   similar to that of the cubic nonlinear Schrodinger equations.}},
