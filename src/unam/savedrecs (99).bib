@inproceedings{ ISI:000263872000011,
Author = {Poveda, Arcadio and Lara, Patricia},
Editor = {{Allen, C and Ruelas, A and Teixeira, R}},
Title = {{THE EXO-PLANETARY SYSTEM OF 55 CANCRI AND THE TITIUS-BODE LAW}},
Booktitle = {{IV REUNION SOBRE ASTRONOMIA DINAMICA EN LATINOAMERICA (ADELA 2008)}},
Series = {{Revista Mexicana de Astronomia y Astrofisica-Serie de Conferencias}},
