@article{ ISI:000252564800039,
Author = {Montesmos-Castellanos, A. and Zepeda, T. A. and Pawelec, B. and Lima, E.
   and Fierro, J. L. G. and Olivas, A. and de Los Reyes, J. A.},
Title = {{Influence of reduction temperature and metal loading on the performance
   of molybdenum phosphide catalysts for dibenzothiophene
   hydrodesulfurization}},
Journal = {{APPLIED CATALYSIS A-GENERAL}},
Year = {{2008}},
Volume = {{334}},
Number = {{1-2}},
Pages = {{330-338}},
Month = {{JAN}},
Abstract = {{Two series of alumina-supported molybdenum phosphide (MoP) catalysts
   with low and high metal loadings were prepared by temperature-programmed
   reduction of the oxidic catalyst precursors in hydrogen to different
   temperatures (823, 923, 1023 and 1123 K, respectively). Effects of
   reduction temperature and metal loading on the surface distribution and
   the type of species formed were studied by TPR, S-BET, XRD, HRTEM, P-31
   NMR, Al-27 NMR and in the reaction of dibenzothiophene (DBT)
   hydrodesulfurization (HDS) performed in a flow reactor at 553 K and
   total hydrogen pressure of 3.4 MPa. HRTEM and P-31 NMR confirmed
   formation of Mo phase on all catalysts. The 9.9 wt\% Mo catalyst
   activated at lowest reduction temperature (823 K) was found to be most
   active among the catalysts studied. The presence of a low amount of Mo-0
   species on the surface of this catalyst does not appear to be a drawback
   for the catalytic activity. The increase in both metal loading (from 9.9
   to 15 wt\% Mo and from 3.2 to 4.8 wt\% P) and reduction temperature
   (from 823 to 1123 K) was found to be detrimental for HDS activity due to
   sintering of active phase, and also to decrease in specific area and
   formation of phosphate species. (c) 2007 Elsevier B.V. All rights
   reserved.}},
