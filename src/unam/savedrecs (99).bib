@article{ ISI:000252346800001,
Author = {Mas, J. F. and Flores, J. J.},
Title = {{The application of artificial neural networks to the analysis of
   remotely sensed data}},
Journal = {{INTERNATIONAL JOURNAL OF REMOTE SENSING}},
Year = {{2008}},
Volume = {{29}},
Number = {{3}},
Pages = {{617-663}},
Abstract = {{Artificial neural networks (ANNs) have become a popular tool in the
   analysis of remotely sensed data. Although significant progress has been
   made in image classification based upon neural networks, a number of
   issues remain to be resolved. This paper reviews remotely sensed data
   analysis with neural networks. First, we present an overview of the main
   concepts underlying ANNs, including the main architectures and learning
   algorithms. Then, the main tasks that involve ANNs in remote sensing are
   described. The limitations and crucial issues relating to the
   application of the neural network approach are discussed. A brief review
   of the implementation of ANNs in some of the most popular image
   processing software packages is presented. Finally, we discuss the
   application perspectives of neural networks in remote sensing image
   analysis.}},
