Title = {{Study of the Electronic Structure of the Unconventional Superconductor
   Sr2RuO4 by the Embedded Cluster Method}},
Booktitle = {{FRONTIERS IN QUANTUM SYSTEMS IN CHEMISTRY AND PHYSICS}},
Series = {{Progress in Theoretical Chemistry and Physics}},
