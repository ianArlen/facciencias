@inproceedings{ ISI:000263558400027,
Author = {Bernardo Wolf, Kurt},
Editor = {{Kielanowski, P and Odzijewicz, A and Schlichenmaier, M and Voronov, T}},
Title = {{Finite Hamiltonian Systems: Linear Transformations and Aberrations}},
