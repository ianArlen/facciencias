@article{ ISI:000257141900010,
Author = {Mayoral, M. A. and Mayoral, C. and Meneses, A. and Villalvazo, L. and
   Guzman, A. and Espinosa, B. and Ochoa, J. L. and Zenteno, E. and
   Guevara, J.},
Title = {{Identification of Galectin-3 and mucin-type O-glycans in breast cancer
   and its metastasis to brain}},
Journal = {{CANCER INVESTIGATION}},
Year = {{2008}},
Volume = {{26}},
Number = {{6}},
Pages = {{615-623}},
Abstract = {{Galectin-3 has been implicated in tumor progression. We demonstrated
   immunohistochemically that Galectin-3 was negative in normal breast
   tissue, but it was highly increased in breast cancer and in metastatic
   tissues to brain. Similarly, histochemistry with mucin-specific lectins
   showed increased recognition in breast tumor and metastasis with
   Machaerocereus eruca agglutinin (Fu alpha 1,2 (GalNAc alpha 1,3) Gal
   beta 1,4 in complex mucin) but not for Amaranthus leucocarpus (Gal beta
   1,3-GalNAc-alpha 1,0-Ser/Thr) and Arachis hypogaea lectins (Gal beta
   1,3GaINAc/Gal beta 1,4GlcNAc). Mucin-type glycans and Galectin-3
   colocalized in breast cancer and metastasis, but not in normal tissue,
   suggesting upregulated biosynthesis of complex O-glycosidically linked
   glycans and Galectin-3 favor breast cancer progression and brain
   metastasis.}},
