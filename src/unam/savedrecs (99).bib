@article{ ISI:000254301500007,
Author = {Huerta-Beristain, Gerardo and Utrilla, Jose and Hernandez-Chavez,
   Georgina and Bolivar, Francisco and Gosset, Guillermo and Martinez,
   Alfredo},
Title = {{Specific ethanol production rate in ethanologenic Escherichia coli
   strain KO11 is limited by pyruvate decarboxylase}},
Journal = {{JOURNAL OF MOLECULAR MICROBIOLOGY AND BIOTECHNOLOGY}},
Year = {{2008}},
Volume = {{15}},
Number = {{1}},
Pages = {{55-64}},
Abstract = {{Modification of ethanol productivity and yield, using mineral medium
   supplemented with glucose or xylose as carbon sources, was studied in
   ethanologenic Escherichia coli KO11 by increasing the activity of five
   key carbon metabolism enzymes. KO11 efficiently converted glucose or
   xylose to ethanol with a yield close to 100\% of the theoretical maximum
   when growing in rich medium. However, when KO11 ferments glucose or
   xylose in mineral medium, the ethanol yields decreased to only 70 and
   60\%, respectively. An increase in GALP(Ec) (permease of
   galactose-glucose-xylose) or PGK(Ec) (phosphoglycerate kinase)
   activities did not change xylose or glucose and ethanol flux. However,
   when PDCZm (pyruvate decarboxylase from Zymomonas mobilis) activity was
   increased 7-fold, the yields of ethanol from glucose or xylose were
   increased to 85 and 75\%, respectively, and organic acid formation rates
   were reduced. Furthermore, as a response to a reduction in acetate and
   ATP yield, and a limited PDCZm activity, an increase in PFKEc
   (phosphofructokinase) or PYKBs (pyruvate kinase from Bacillus
   stearothermophilus) activity drastically reduced glucose or xylose
   consumption and ethanol formation flux. This experimental metabolic
   control analysis showed that ethanol flux in KO11 is negatively
   controlled by phosphofructokinase and pyruvate kinase, and positively
   influenced by the PDCZm activity level. Copyright (c) 2008 S. Karger AG,
   Basel.}},
Publisher = {{KARGER}},
Address = {{ALLSCHWILERSTRASSE 10, CH-4009 BASEL, SWITZERLAND}},
