@inproceedings{ ISI:000306271300018,
Author = {Guerrero, Lilian},
Editor = {{VanValin, RD}},
Title = {{Alternative expressions of ``want{''} complements}},
Booktitle = {{INVESTIGATIONS OF THE SYNTAX-SEMANTICS-PRAGMATICS INTERFACE}},
Series = {{Studies in Language Companion Series}},
Year = {{2008}},
Volume = {{105}},
Pages = {{321-336}},
Note = {{Role and Reference Grammar Conference (RRG), Univ Leipzig, Leipzig,
   GERMANY, SEP, 2006}},
Abstract = {{The complements of ``want{''} predicate may vary depending on the
   identity of the main and the dependent subjects. When identical, five
   main tendencies are found cross-linguistically: (i) the dependent
   subject is omitted, (ii) the dependent subject is overtly expressed,
   (iii) ``want{''} is a desiderative affix or (iv) an uninflected
   particle, and (v) there are alternative choices (Haspelmath 2005). Using
   data from the Uto-Aztecan family, this article focuses on one of the
   less common means, the co-existence of alternative expressions. The
   analysis provides an account for this phenomenon in light of the
   Interclausal Relations Hierarchy (Van Valin 2005): whenever there are
   available structures in a language, the tightest linkage encodes
   intention, and the less tight expresses different mental states.}},
