Year = {{2008}},
Volume = {{28}},
Number = {{4}},
Pages = {{379-384}},
Abstract = {{We prove a colorful version of Hadwiger's transversal line theorem; if a
   family of colored and numbered convex sets in the plane has the property
   that any three differently colored membres have a transversal line that
   meet the sets consistently with the numbering, then there exists a color
   such that all the convex sets of that color have a transversal line.}},
