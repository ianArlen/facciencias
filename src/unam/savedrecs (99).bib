@inproceedings{ ISI:000262710800016,
Author = {Rojas-Ramirez, J. S. and Goldhahn, R. and Moser, P. and Huerta-Ruelas,
   J. and Hernandez-Rosas, J. and Lopez-Lopez, M.},
Editor = {{MontanoZetina, LM and Vega, GR and Rocha, MG and Ochoa, LFR and Fernandez, RL}},
Title = {{Temperature Dependence of the Photoluminescence Linewidth of
   In0.2Ga0.8As Quantum Wells on GaAs(311) Substrates}},
Booktitle = {{EAV08: ADVANCED SUMMER SCHOOL IN PHYSICS 2008: FRONTIERS IN CONTEMPORARY
   PHYSICS, 4TH EDITION}},
