@article{ ISI:000252811600011,
Author = {Contreras-Torres, Flavio F. and Arzate-Jacinto, Alejandro and Basiuk,
   Vladimir A.},
Title = {{Direct esterification of oxidized defects in single-walled carbon
   nanotubes: A comparison of several ONIOM computation schemes}},
Journal = {{JOURNAL OF COMPUTATIONAL AND THEORETICAL NANOSCIENCE}},
Year = {{2008}},
Volume = {{5}},
Number = {{1}},
Pages = {{93-101}},
Month = {{JAN}},
Abstract = {{We performed an ONIOM(B3LYP:UFF) study of the direct esterification of
   oxidized single-walled carbon nanotube (SWNT) models with methanol and
   ethanol, by expanding the higher level to six fused aromatic rings at
   the nanotube edge, along with maintaining a sufficiently large basis set
   (3-21G{*} and 6-31G{*}) in the higher ONIOM partition. The higher-level
   partition also included different combinations of COOH C = O and OH
   functionalites. We attempted to give a further insight into the chemical
   mechanisms of esterification reaction, and to confirm (or disprove) its
   energetic preference on armchair versus zigzag SWNT tips by calculating
   energy of activation, enthalpy of reaction, Gibbs free energy, and
   kinetic constants. The thermodynamic and kinetic results were always
   very sensitive to the choice of SWNT model, namely, to the composition
   and distribution of oxygenated functionalities. Especially contradictory
   were the calculated thermodynamic values, whereas the kinetic constants
   rather favored an enhanced reactivity of the zigzag isomers.}},
