@article{ ISI:000085669900010,
Author = {Gaso, MI and Segovia, N and Cervantes, ML and Herrera, T and
   Perez-Silva, E},
Title = {{Internal radiation dose from Cs-137 due to the consumption of mushrooms
   from a Mexican temperate mixed forest}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{2000}},
Volume = {{87}},
Number = {{3}},
Pages = {{213-216}},
Abstract = {{Cs-137 and K-40 specific activities were determined in soil samples and
   in different edible wild mushroom species collected from a temperate
   mixed forest located at the Nuclear Centre of Mexico and in several
   surrounding localities. The activity measurements were performed using a
   HpGe detector. The aggregated transfer factors (ATF) for Cs-137 in 21
   local mushroom species collected from 1993 to 1997, showed differences
   as large as 3 orders of magnitude. The annual effective dose for Cs-137
   due to the consumption of mushrooms was estimated to range between be
   0.22 and 1.2 mu Sv.y(-1) depending on the method used for calculation.
   Using the geometric mean of the caesium concentration, the contribution
   of mushrooms represents 56\% of the total Cs-137 dietary intake.}},
