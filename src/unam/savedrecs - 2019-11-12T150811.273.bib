@article{ ISI:000086524000001,
Author = {Caballero, CE and Sanchez, E and Cano, U and Gonzalez, JG and Castano, V},
Title = {{On the effect of fly ash on the corrosion properties of reinforced
   mortars}},
Journal = {{CORROSION REVIEWS}},
Year = {{2000}},
Volume = {{18}},
Number = {{2-3}},
Pages = {{105-112}},
Abstract = {{The mechanical and corrosion properties of steel reinforced, fly
   ash-modified mortars are presented. The study spans a 9 months period in
   which the samples were exposed to immersion-drying into two solutions
   (artificial sea water and 2\% sulfuric acid) and a control solution
   (drinkable water). The residual strength was measured, as well as the
   corresponding electrochemical potential and corrosion rates, through the
   half-cell potential and linear polarization methods. The results show a
   remarkable improvement in the corrosion behavior of the material by
   adding fly ash.}},
