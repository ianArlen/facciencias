@article{ ISI:000085334800008,
Author = {Colpi, M and Geppert, U and Page, D},
Title = {{Period clustering of the anomalous X-ray pulsars and magnetic field
   decay in magnetars}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2000}},
Volume = {{529}},
Number = {{1, 2}},
Pages = {{L29-L32}},
Month = {{JAN 20}},
Abstract = {{We confront theoretical models for the rotational, magnetic, and thermal
   evolution of an ultramagnetized neutron star, or magnetar, with
   available data on the anomalous X-ray pulsars (AXPs). We argue that, if
   the AXPs are interpreted as magnetars, their clustering of spin periods
   between 6 and 12 s (observed at present in this class of objects), their
   period derivatives, their thermal X-ray luminosities, and the
   association of two of them with young supernova remnants can only be
   understood globally if the magnetic field in magnetars decays
   significantly on a timescale of the order of 10(4) yr.}},
