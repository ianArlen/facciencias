@article{ ISI:000085275000006,
Author = {Harmony, T and Bernal, J and Fernandez, T and Silva-Pereyra, J and
   Fernandez-Bouzas, A and Marosi, E and Rodriguez, M and Reyes, A},
Title = {{Primary task demands modulate P3a amplitude}},
Journal = {{COGNITIVE BRAIN RESEARCH}},
