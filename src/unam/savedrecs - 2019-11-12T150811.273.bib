@article{ ISI:000165487300023,
Author = {Terrones, M and Kamalakaran, R and Seeger, T and Ruhle, M},
Title = {{Novel nanoscale gas containers: encapsulation of N-2 in CNx nanotubes}},
Journal = {{CHEMICAL COMMUNICATIONS}},
Year = {{2000}},
Number = {{23}},
Pages = {{2335-2336}},
Abstract = {{Gaseous N-2 has been efficiently introduced in the hollow cavities of
   aligned CNx nanotubes (15-80 nm od) by pyrolysing a jet (spray) solution
   of Fe(C5H5)(2) and PhCH2NH2 in an Ar atmosphere at 850 degreesC; the
   aligned material consist of large arrays (2.5 cm(2)) of CNx (x < 0.05)
   `bamboo-like' nanotubes (< 100 mum in length); high resolution electron
   energy loss spectroscopy (HREELS) line-scans and elemental mapping
   studies reveal that N gas was encapsulated within the hollow
   compartments of the CNx nanotubes; these results demonstrate for the
   first time, that CNx nanotubes can be used to fabricate gas storage
   components.}},
