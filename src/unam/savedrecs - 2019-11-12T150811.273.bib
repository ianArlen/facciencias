@article{ ISI:000086241000009,
Author = {Li, CL and Novaro, O and Bokhimi, X and Munoz, E and Boldu, JL and Wang,
   JA and Lopez, T and Gomez, R and Batina, N},
Title = {{Coke formation on an industrial reforming Pt-Sn/gamma-Al2O3 catalyst}},
Journal = {{CATALYSIS LETTERS}},
Year = {{2000}},
Volume = {{65}},
Number = {{4}},
Pages = {{209-216}},
Abstract = {{The characterization of the coke deposited on an industrial
   Pt-Sn/gamma-Al2O3 catalyst, used in a continuous reforming process, was
   performed with AFM, XRD, FTIR, EPR, NMR, TG-DTG and DTA techniques.
   Composition, structure and location of the coke on the catalyst were
   investigated. The coke was predominantly deposited on the catalyst
   surface and in the interstices between the catalyst particles. Its
   content increased along the reactor from top to bottom. Coke was
   deposited in the form of uniform films and clusters of three-dimensional
   disks with diameters between 0.12 and 0.18 mu m. It had a
   pseudo-graphite structure produced by the dehydrogenation and
   polymerization of the aromatic precursor compounds. The coked catalyst
   showed a good combustion behavior; it was regenerated below 550 degrees
   C. These results are important to elucidate the coke formation
   mechanism, to generate new continuous reforming catalysts, and to
   optimize the reactor operation parameters.}},
