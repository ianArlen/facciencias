@article{ ISI:000089180400021,
Author = {Iturriaga, R and Sanchez-Morgado, H},
Title = {{Finsler metrics and action potentials}},
Journal = {{PROCEEDINGS OF THE AMERICAN MATHEMATICAL SOCIETY}},
Year = {{2000}},
Volume = {{128}},
Number = {{11}},
Pages = {{3311-3316}},
Abstract = {{We study the behavior of Mane's action potential Phi(k) associated to a
   convex superlinear Lagrangian, for k bigger than the critical value
   c(L). We obtain growth estimates for the action potential as a function
   of k. We also prove that the action potential can be written as Phi(k)
   (x, y) = D-F(x, y) + f(y) - f(x) where f is a smooth function and D-F is
   the distance function associated to a Finsler metric.}},
