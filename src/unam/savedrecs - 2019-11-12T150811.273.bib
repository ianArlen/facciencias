@article{ ISI:000088239700021,
Author = {Benet, L and Seligman, TH},
Title = {{Narrow rings: A scattering billiard model}},
Journal = {{PROGRESS OF THEORETICAL PHYSICS SUPPLEMENT}},
Year = {{2000}},
Number = {{139}},
Pages = {{234-245}},
Note = {{4th International Summer School/Conference on Lets Face Chaos Through
   Nonlinear Dynamics, UNIV MARIBOR, MARIBOR, SLOVENIA, JUN 27-JUL 11, 1999}},
Organization = {{Repub Slovenia, Minist Sci \& Technol; Japan Soc Promot Sci; British
   Council, Ljubljana; Italian Embassy, Slovenia; French Embassy, Slovenia;
   Nova Kreditna Banka Maribor; Telekom Slovenia; City of Maribor; Public
   Hlth Labs Maribor; Slovenian Govt Informat Serv Off; Univ Marburg}},
Abstract = {{We propose a generic mechanism for the formation of narrow rings in
   rotating systems. For this purpose we use a system of discs rotating
   about a common center lying well outside the discs. a discussion of this
   system shows that narrow rings occur, if we assume non-interacting
   particles. A saddle-center bifurcation is responsible for the relevant
   appearance of elliptic regions in phase space, that will generally
   assume ring shapes in the synodic frame, which will suffer a precession
   in the sidereal frame. Finally we discuss possible applications of this
   mechanism and find that it may be relevant for planetary rings as well
   as for semiclassical considerations.}},
