@inproceedings{ ISI:000169940700027,
Author = {Nocetti, FG and Gonzalez, JS and Acosta, ER and Hernandez, EM},
Editor = {{Hernandez, V and Irwin, GW}},
Title = {{Towards the simplified computation of time-frequency distributions for
   signal analysis}},
Booktitle = {{ALGORITHMS AND ARCHITECTURES FOR REAL-TIME CONTROL 2000}},
Year = {{2000}},
Pages = {{169-174}},
Note = {{6th IFAC Workshop on Algorithms and Architectures for Real-Time Control
   (AARTC 2000), PALMA DE MALLORCA, SPAIN, MAY 15-17, 2000}},
Organization = {{Spanish Soc Automat Control; Int Federat Automat Control; Tech Comm
   Algorithms \& Architectures Real Time Control}},
Abstract = {{Conventional methods for signal analysis utilise the Fourier Transform
   to estimate the spectral response of a signal. However this current
   practice suffers from poor frequency resolution when estimating
   non-stationary signals. This paper presents some alternative methods
   based on time-frequency distributions such as the Wigner Ville, the Choi
   Williams, the Bessel and the Born Jordan from a Cohen's class point of
   view. For each case. a continuous and discrete distribution is
   formulated, a criterion for determining the interaction between the
   spectral components of the signal is given and the simplified
   discretised expression for the calculation of the distribution is
   proposed that can produce a reduction of at least half of the
   computations realised when using the original time-frequency
   distribution definition. A general parallel architecture for the
   parallel computation of the distribution is also proposed Copyright (C)
   2000 IFAC.}},
