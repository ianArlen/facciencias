@article{ ISI:000166856600001,
Author = {Vachard, D and De dios, AF and Pantoja, J and Buitron, BE and Arellano,
   J and Grajales, M},
Title = {{Fusulinids from Mexico, a biostratigraphical and paleogeographical
   review}},
Journal = {{GEOBIOS}},
Year = {{2000}},
Volume = {{33}},
Number = {{6}},
Pages = {{655-679}},
Abstract = {{This paper focuses attention on a bibliographical review about the
   Mexican fusulinids, with some new data. The Early Paleozoic and the
