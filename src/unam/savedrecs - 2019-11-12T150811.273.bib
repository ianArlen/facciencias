@inproceedings{ ISI:000168122500014,
Author = {Rodriguez, LF},
Editor = {{Garzon, F and Eiroa, C and DeWinter, D and Mahoney, TJ}},
Title = {{Observations of protoplanetary disks at 7 mm}},
Booktitle = {{DISKS, PLANETESIMALS, AND PLANETS, PROCEEDINGS}},
