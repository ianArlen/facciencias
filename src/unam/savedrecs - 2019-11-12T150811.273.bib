   Chaput, F and Lahlil, K and Boilot, JP and Brun, A and Levy, Y},
Editor = {{Eich, M and Kuzyk, MG and Lawson, CM and Norwood, RA}},
Title = {{Material optimization for electro-optic modulation and cascading}},
Booktitle = {{LINEAR, NONLINEAR, AND POWER-LIMITING ORGANICS}},
