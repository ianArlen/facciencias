@article{ ISI:000085593200011,
Author = {Poujol, FT and Rojas, J and Ramos, E},
Title = {{Natural convection of a high Prandtl number fluid in a cavity}},
Journal = {{INTERNATIONAL COMMUNICATIONS IN HEAT AND MASS TRANSFER}},
Year = {{2000}},
Volume = {{27}},
Number = {{1}},
Pages = {{109-118}},
Month = {{JAN}},
Abstract = {{Transient natural convection in a square cavity heated with a
   time-dependent heat flux on one vertical wall and cooled by maintaining
   the opposite wall at a constant temperature was studied experimentally
   and numerically. The working fluid was silicon oil (Dow Coming fluid
   20-200) with a Prandtl number of 230. All experiments were carried out
   in a cubic cavity of 0.13 m in each side. The heating rate used was 460
   W/m(2), which corresponds to a Rayleigh number of 4x10(9). Experimental
   data included temperature records at particular points and velocity
   measurements obtained from video images of tracers. The dynamics of the
   transition to the steady stare is characterized by a vortex structure
   that forms near the heated wall. This structure is generated by shear at
   the heated wall boundary layer. The results were compared with a
   numerical simulation and qualitative agreement was obtained. (C) 2000
   Elsevier Science Ltd.}},
