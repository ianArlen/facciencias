@article{ ISI:000085079600055,
Author = {Klein-Kreisler, M and Ruiz-Altaba, M},
Title = {{Quantum isospin and q-hadrons}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2000}},
Volume = {{61}},
Number = {{3}},
Month = {{FEB 1}},
Abstract = {{We consider the possibility that the SU(2) isospin symmetry, exact in
   strong interactions but only approximate in nature, is in fact a quantum
   group. Using a doublet of q-quarks, we build the wave functions of
   pi-mesons, nucleons, and a baryons. We redo the usual quark model
   computation for the magnetic moments and mass relations, and everything
   fits with experimental data. We find, nevertheless, that it is
   impossible to parameterize successfully the large mass difference
   between the charged and neutral ions with the single q of the quantum
   group U-q(sl(2)) derived from baryons.}},
