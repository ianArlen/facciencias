@inproceedings{ ISI:000088056900037,
Author = {Nair, PK and Parmananda, P and Nair, MTS},
Editor = {{Chalamala, BR and Friend, RH and Jackson, TN and Libsch, FR}},
Title = {{Growth mechanism of thin film wide-gap semiconductors by chemical bath
   deposition technique}},
Booktitle = {{FLAT-PANEL DISPLAYS AND SENSORS: PRINCIPLES, MATERIALS AND PROCESSES}},
Series = {{MATERIALS RESEARCH SOCIETY SYMPOSIUM PROCEEDINGS}},
Year = {{2000}},
Volume = {{558}},
