@article{ ISI:000326645300011,
Author = {Milanese, M. and Niedbalski, J. and Moroso, R. and Barbaglia, M. and
   Mayer, R. and Castillo, F. and Guichon, S.},
Title = {{Small plasma focus as neutron pulsed source for nuclides identification}},
