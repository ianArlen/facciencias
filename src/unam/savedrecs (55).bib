@article{ ISI:000324113200010,
Author = {Calva-Yanez, J. C. and Rincon, M. E. and Solis de la Fuente, M. and
   Alvarado-Tenorio, G.},
Title = {{Structural and photoelectrochemical characterization of MWCNT-TiO2
   matrices sensitized with Bi2S3}},
Journal = {{JOURNAL OF SOLID STATE ELECTROCHEMISTRY}},
