@article{ ISI:000325624300030,
Author = {Ordonez-Regil, E. and Almazan-Torres, M. G. and Sanchez-Cabeza, J. A.
   and Ruiz-Fernandez, A. C.},
Title = {{Presence of uranium and plutonium in marine sediments from gulf of
   tehuantepec, mexico}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY}},
Year = {{2013}},
Volume = {{298}},
Number = {{2}},
Pages = {{981-986}},
Month = {{NOV}},
Abstract = {{Uranium and plutonium were determined in the Tehua II-21 sediment core
   collected from the Gulf of Tehuantepec, Mexico. The analyses were
   performed using radiochemical separation and alpha spectroscopy.
   Activity concentrations of alpha emitters in the sediment samples were
   from 2.56 to 43.1 Bq/kg for U-238, from 3.15 to 43.1 Bq/kg for U-234 and
   from 0.69 to 2.95 Bq/Kg for Pu239+240. Uranium activity concentration in
   marine sediment studied is generally high compared with those found in
   sediments from other marine coastal areas in the world. The presence of
   relatively high concentrations of anthropogenic plutonium in the
   sediments from the Gulf of Tehuantepec suggests that anthropogenic
   radionuclides have been incorporated and dispersed into the global
   marine environment.}},
