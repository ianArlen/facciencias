@article{ ISI:000324819900006,
Author = {Bergshoeff, Eric A. and Kovacevic, Marija and Parra, Lorena and Rosseel,
   Jan and Yin, Yihao and Zojer, Thomas},
Title = {{New massive supergravity and auxiliary fields}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{2013}},
Volume = {{30}},
Number = {{19}},
Month = {{OCT 7}},
Abstract = {{We construct a supersymmetric formulation of three-dimensional
   linearized new massive gravity without introducing higher derivatives.
   Instead, we introduce supersymmetrically a set of bosonic and fermionic
   auxiliary fields which, upon elimination by their equations of motion,
   introduce fourth-order derivative terms for the metric and third-order
   derivative terms for the gravitino. Our construction requires an
   off-shell formulation of the three-dimensional supersymmetric massive
   Fierz-Pauli theory. We discuss the nonlinear extension of our results.}},
