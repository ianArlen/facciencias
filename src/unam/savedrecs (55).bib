@article{ ISI:000331145700025,
Author = {Suarez-Bonnet, Elena and Carvajal, Magda and Mendez-Ramirez, Ignacio and
   Castillo-Urueta, Pavel and Cortes-Eslava, Josefina and Gomez-Arroyo,
   Sandra and Maria Melero-Vara, Jose},
Title = {{Aflatoxin (B-1, B-2, G(1), and G(2)) Contamination in Rice of Mexico and
   Spain, from Local Sources or Imported}},
Journal = {{JOURNAL OF FOOD SCIENCE}},
Year = {{2013}},
Volume = {{78}},
Number = {{11}},
Pages = {{H1822-H1829}},
Month = {{NOV}},
Abstract = {{Rice is an important cereal but it is often contaminated with aflatoxins
   (AFs). The purpose of this study was to identify and quantify AF (B1 ,
   B2 , G1 , and G2 ) in 67 rice samples cultivated in Mexico and Spain,
   and from imported crops collected in 2008 and 2009. The methodology was
   validated, the rice samples were concentrated and purified with
   immunoaffinity columns and were quantified by high-pressure liquid
   chromatography (HPLC). The average total AF (AFt) in the Spanish rice
   was 37.3 mu g/kg, the range was from 1.6 to 1383 mu g/kg, the most
   contaminated samples being from San Juan de Aznalfarache, Sevilla (AFt =
   138.6 mu g/kg), from Tortosa, Tarragona (AFt = 104.6 mu g/kg), and
   Calasparra, Murcia (AFt = 103.9 mu g/kg). The rice imported from France
   to Spain had AFt of 26.6 mu g/kg and from Pakistan AFt of 18.4 mu g/kg,
   showing less AF contamination than the local one. The rice which
   originated from Mexico contained (AFt = 16.9 mu g/kg), and those
   imported from the United States (AFt = 14.4 mu g/kg) and Uruguay (AFt =
   15.6 mu g/kg). The imported rice had better quality in terms of the
   presence of AFs.}},
