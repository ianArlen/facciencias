@article{ ISI:000326148000007,
Author = {Turbiner, A. V. and Lopez Vieyra, J. C.},
Title = {{Stable He- can Exist in a Strong Magnetic Field}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2013}},
Volume = {{111}},
Number = {{16}},
Month = {{OCT 18}},
Abstract = {{The existence of bound states of the system (alpha, e, e, e) in a
   magnetic field B is studied using the variational method. It is shown
   that for B greater than or similar to 0.13 a.u. this system gets bound
   with total energy below the one of the (alpha, e, e) system. It
   manifests the existence of the stable He- atomic ion. Its ground state
   is a spin doublet (2)(-1)(+) at 0.74 a.u. greater than or similar to B
   greater than or similar to 0.13 a.u. and it becomes a spin quartet
   (4)(-3)(+) for larger magnetic fields. For 0.8 a.u. greater than or
   similar to B greater than or similar to 0.7 a.u. the He- ion has two
   (stable) bound states (2)(-1)(+) and (4)(-3)(+).}},
