Author = {Mussali-Galante, Patricia and Tovar-Sanchez, Efrain and Valverde, Mahara
   and Valencia-Cuevas, Leticia and Rojas, E.},
Title = {{Evidence of population genetic effects in Peromyscus melanophrys
   chronically exposed to mine tailings in Morelos, Mexico}},
Journal = {{ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH}},
