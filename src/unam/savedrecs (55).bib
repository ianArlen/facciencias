@article{ ISI:000325115900035,
Author = {Durruthy-Rodriguez, M. D. and Gervacio-Arciniega, J. J. and Portelles,
   J. and Fuentes, J. and Perez, A. and Yanez-Limon, J. M. and
   Espinoza-Beltran, F. J. and Raymond, O. and Siqueiros, J. M.},
Title = {{PFM characterization of (K0.5Na0.5)(0.95)La-0.05(Nb0.9Ti0.05)O-2.9
   ceramics lead free}},
Journal = {{APPLIED PHYSICS A-MATERIALS SCIENCE \& PROCESSING}},
Year = {{2013}},
Volume = {{113}},
Number = {{2}},
Pages = {{515-519}},
Month = {{NOV}},
Abstract = {{The results of ferroelectric properties studies of KNN doped with La and
   Ti and sintered at temperatures in the interval of 1100 A degrees C-1190
   A degrees C are presented in this work. The doping was achieved by the
   substitution of La for ions in A sites and Ti for ions in the B sites.
   Values of 94 \% of the theoretical density were accomplished. The effect
   of the sintering temperature and the inclusion of the La and Ti cations
   in the KNN structure is evident through the shift in the
   ferroelectric-paraelectric transition temperature of similar to 110 A
   degrees C with respect to that of pure KNN (420 A degrees C).
   Microstructure and ferroelectric analyses were carried out using
   Piezoresponse Force Microscopy (PFM) and hysteresis loops with
   interesting results, Delta P (r)=9 (mu C/cm(2)) and P (r)/P (max)=0.41,
   even when the saturation of the materials is not reached.}},
