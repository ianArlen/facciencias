@article{ ISI:000324897900013,
Author = {Kaur, Balwinder and Pandiyan, Thangarasu and Satpati, Biswarup and
   Srivastava, Rajendra},
Title = {{Simultaneous and sensitive determination of ascorbic acid, dopamine,
   uric acid, and tryptophan with silver nanoparticles-decorated reduced
   graphene oxide modified electrode}},
Journal = {{COLLOIDS AND SURFACES B-BIOINTERFACES}},
Year = {{2013}},
Volume = {{111}},
Pages = {{97-106}},
Month = {{NOV 1}},
Abstract = {{In this paper, we report the synthesis of silver nanoparticle-decorated
   reduced graphene oxide composite (AgNPs/rGO) by heating the mixture of
   graphene oxide and silver nitrate aqueous solution in the presence of
   sodium hydroxide. This material was characterized by means of X-ray
   diffraction, UV-vis spectroscopy, and transmission electron microscopy.
   AgNPs/rGO based electrochemical sensor was fabricated for the
   simultaneous determination of ascorbic acid, dopamine, uric acid, and
   tryptophan. Electrochemical studies were carried out by using cyclic
   voltammetry, linear sweep voltammetry, and chronoamperometry. AgNPs/rGO
   modified electrode exhibited excellent electrocatalytic activity,
   stability, sensitivity, and selectivity with well-separated oxidation
   peaks toward ascorbic acid, dopamine, uric acid, and tryptophan in the
   simultaneous determination of their quaternary mixture. The analytical
   performance of this material as a chemical sensor was demonstrated for
   the determination of ascorbic acid and dopamine in commercial
   pharmaceutical samples such as vitamin C tablets and dopamine
   injections, respectively. The applicability of this sensor was also
   extended in the determination of uric acid in human urine samples. (C)
   2013 Elsevier B.V. All rights reserved.}},
