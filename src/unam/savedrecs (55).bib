@article{ ISI:000320206900016,
Author = {Caballero-Benitez, Santiago F. and Paredes, Rosario and Romero-Rochin,
   Victor},
Title = {{The contact in the BCS-BEC crossover for finite range interacting
   ultracold Fermi gases}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2013}},
Volume = {{377}},
Number = {{28-30}},
Pages = {{1756-1759}},
Month = {{OCT 15}},
Abstract = {{Using mean-field theory for the Bardeen-Cooper-Schriefer (BCS) to the
   Bose-Einstein condensate (BEC) crossover we investigate the ground state
   thermodynamic properties of an interacting homogeneous Fermi gas. The
   interatomic interactions modelled through a finite range potential
   allows us to calculate the thermodynamic behaviour as a function of the
   potential parameters in the whole crossover region. We concentrate in
   studying the Contact variable, the thermodynamic conjugate of the
   inverse of the s-wave scattering length. Our analysis leads to predict a
   quantum phase transition - like in the case of large potential range.
   This finding is a direct consequence of the k-dependent energy gap. (c)
   2013 Elsevier B.V. All rights reserved.}},
