@article{ ISI:000325508100014,
Author = {Dzib, Sergio A. and Rodriguez, Luis F. and Araudo, Anabella T. and
   Loinard, Laurent},
Title = {{RADIO SOURCES EMBEDDED IN THE DENSE CORE B59, THE ``MOUTHPIECE{''} OF
   THE PIPE NEBULA}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2013}},
Volume = {{49}},
Number = {{2}},
Pages = {{345-349}},
Month = {{OCT}},
Abstract = {{We present Very Large Array continuum observations made at 8.3 GHz
   toward the dense core B59, in the Pipe Nebula. We detect six compact
   sources, of which five are associated with the five most luminous
   sources at 70 mu m in the region, while the remaining one is probably a
   background source. We propose that the radio emission is free-free from
   the ionized outflows present in these protostars. We discuss the
   kinematical impact of these winds in the cloud. We also propose that
   these winds are optically thick in radio but optically thin in X-rays,
   and that this feature can explain why X-rays from the magnetosphere are
   detected in three of them, while the radio emission is probably
   dominated by the free-free emission from the external layers of the
   wind.}},
