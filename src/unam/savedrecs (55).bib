@article{ ISI:000325471600003,
Author = {Cante-Mota, Ivon and Moreno-Alcantar, Guillermo and Flores-Alamo, Marcos
   and Castillo, Ivan},
Title = {{Benzimidazole-derived calix{[}4]arenes with polymerizable styrene groups
   and their Cu(II) complexes}},
