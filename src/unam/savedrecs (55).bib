@article{ ISI:000326300300026,
Author = {Rosas-Ramirez, Daniel and Pereda-Miranda, Rogelio},
Title = {{Resin Glycosides from the Yellow-Skinned Variety of Sweet Potato
   (Ipomoea batatas)}},
