Author = {Manzo-Merino, Joaquin and Thomas, Miranda and Fuentes-Gonzalez, Alma M.
   and Lizano, Marcela and Banks, Lawrence},
Title = {{HPV E6 oncoprotein as a potential therapeutic target in HPV related
   cancers}},
