@article{ ISI:000324615800072,
Author = {Bernal, Cristian G. and Sanchez-Salcedo, F. J.},
Title = {{THE GRAVITATIONAL DRAG FORCE ON AN EXTENDED OBJECT MOVING IN A GAS}},
