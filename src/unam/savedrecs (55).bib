@article{ ISI:000326661200078,
Author = {Roller, Justin M. and Josefina Arellano-Jimenez, M. and Yu, Haoran and
   Jain, Rishabh and Carter, C. Barry and Maric, Radenka},
Title = {{Catalyst nanoscale assembly from the vapor phase on corrosion resistant
   supports}},
