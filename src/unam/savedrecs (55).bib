@article{ ISI:000325144300012,
Author = {Roedel, Heiko G. and Landmann, Christina and Starkloff, Anett and Kunc,
   Hansjoerg P. and Hudson, Robyn},
Title = {{Absentee Mothering - Not So Absent? Responses of European Rabbit
   (Oryctolagus cuniculus) Mothers to Pup Distress Calls}},
Journal = {{ETHOLOGY}},
Year = {{2013}},
Volume = {{119}},
Number = {{11}},
Pages = {{1024-1033}},
Month = {{NOV}},
Abstract = {{Longer-range acoustic parent-offspring communication is widespread, but
   might be absent in species in which young are hidden in burrows during
   the mother's absence. The European rabbit (Oryctolagus cuniculus) is
   such a species, with unusually limited maternal care largely restricted
   to a short daily nursing bout. Based, however, on evidence of frequent
   infanticide in this species, we hypothesize that rabbits possess a
   mechanism promoting a maternal response to pup distress calls. We
   conducted playback experiments with distress calls of pre-weaning pups
   played next to the breeding burrows of mothers in a field enclosure
   (i.e. next to the burrows where mothers give birth and raise their
   young). Calls were played shortly after pups were born (T1) when
   infanticide risk is maximal, and shortly before the pups start
   dispersing from the breeding burrow (T2). A high proportion of mothers
   (60.6\%) responded to pup calls by rapidly returning to their breeding
   burrow and 40\% of them investigated the area around the entrance.
   Return responses to the playback of pup calls did not differ between
   mothers during T1 and T2. Thus, our results confirm that rabbit mothers
   respond rapidly to pup distress calls and that this responsiveness may
   adaptively serve to repel potentially infanticidal females.}},
