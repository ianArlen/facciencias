@inproceedings{ ISI:000393361700059,
Author = {Moravejalahkami, Bita and Mostafazadeh-Fard, Behrouz and Haidarpour,
   Manouchehr and Abbasi, Fariborz and Eslamian, S. Saeid and
   Vazquez-Fernandez, Ernesto},
Editor = {{Baby, S and Sandhu, PS}},
Title = {{The effects of variable inflow hydrographs on water saving in furrow
   irrigation using zero-inertia model}},
Booktitle = {{PROCEEDINGS OF THE 2010 INTERNATIONAL CONFERENCE ON ENVIRONMENTAL
   SCIENCE AND TECHNOLOGY (ICEST 2010)}},
Year = {{2010}},
Pages = {{307-312}},
Note = {{International Conference on Environmental Science and Technology (ICEST
   2010), Bangkok, THAILAND, APR 23-25, 2010}},
Organization = {{IACSIT; Applied Chem Engn Soc; Environm Sci Soc}},
Abstract = {{Surface irrigation is one of the common methods of irrigation in which
   soil surface is used to convey and infiltrate water. This method of
   irrigation as compared with sprinkler or trickle methods is inexpensive.
   The performance of furrow irrigation system as one of the surface
   irrigation methods can be improved through optimal management practices
   such as selection of proper inflow rates, inflow hydrographs and cut-off
   times. In this study, the effect of variable furrow inflow hydrographs
   on infiltration and water use saving was investigated under actual field
   conditions using zero-inertia model. Different furrow inflow hydrographs
   including constant inflow, modified gradual reducing inflow, modified
   nonlinear (sigma form) reducing inflow and modified cutback inflow were
   studied. To achieve the objective of the study, an automatic device was
   designed and used in the field to develop different furrow inflow
   hydrograph shapes. The proposed device can be programmed for constant
   and variable inflow hydrographs based on actual field conditions. The
   device was used in a furrow irrigation field with sandy loam soil. The
   applied volume of irrigation water was the same for different furrow
   inflow hydrographs. The field results showed minimum irrigation water
   saving of 8.9\%, 13.3\% and 23.4\% for modified gradual reducing inflow
   hydrograph, modified cutback inflow hydrograph and modified nonlinear
   (sigma form) reducing inflow hydrograph, respectively. The performance
   of a zero- inertia model for the above inflow hydrograph shapes under
   different field slopes and furrow lengths showed that the irrigation
   water saving became higher under higher field slopes and lower furrow
   lengths.}},
