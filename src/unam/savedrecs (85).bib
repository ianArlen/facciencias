@article{ ISI:000280057100011,
Author = {Jimenez, Rogelio and Maldonado, Luis A. and Salgado-Zamora, Hector},
Title = {{Synthesis of demethylated nidulol via an intramolecular Michael reaction}},
Journal = {{NATURAL PRODUCT RESEARCH}},
Year = {{2010}},
Volume = {{24}},
Number = {{13}},
Pages = {{1274-1281}},
Abstract = {{An expeditious synthesis of 5,7-dihydroxy-6-methylphthalide from
   open-chain precursors is described. The key intermediates, synthons 3
   and 4, were readily obtained from accessible materials and were further
   transformed to a common precursor, a five-membered lactone derivative,
   via an intramolecular Michael addition. Lactone 2 was aromatised to the
   phthalide system under basic conditions. The process thus constitutes a
   formal synthesis of the phthalide framework.}},
