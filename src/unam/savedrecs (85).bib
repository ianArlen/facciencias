@article{ ISI:000272515800011,
Author = {Jaramillo, Benjamin and Martinez-Y-Romero, R. P. and Nunez-Yepez, H. N.
   and Salas-Brito, A. L.},
Title = {{On the one-dimensional Coulomb problem}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2009}},
Volume = {{374}},
Number = {{2}},
Pages = {{150-153}},
Month = {{DEC 28}},
Abstract = {{We analyse the one-dimensional Coulomb problem (1DCP) pointing out some
   mistaken beliefs on it. We show that no eigenstates of even or odd
   parity can represent states of the system. The 1DCP exhibits a sort of
   spontaneous breaking of parity. We also show that a superselection rule
   operates in the system. Such rule explains some of its peculiarities. We
   build the superpotential associated to the 1DCR (C) 2009 Elsevier B.V.
   All rights reserved.}},
