@article{ ISI:000276818800006,
Author = {Ortega Leon, Alvaro and Hernandez Ceron, Joel and Gutierrez, Carlos G.},
Title = {{Glycerol supplementation after artificial insemination increases
   conception rate in Holstein cows}},
Journal = {{REVISTA MEXICANA DE CIENCIAS PECUARIAS}},
Year = {{2010}},
Volume = {{1}},
Number = {{1}},
Pages = {{69-74}},
Month = {{JAN-MAR}},
Abstract = {{In the present study, the possibility to improve conception rate (CR) in
   Holstein cows, due to an increase in insulin concentration promoted by
   oral administration of a glycogenic substance (glycerol). was tested. A
   total of one to three inseminations from 190 Holstein cows, body
   condition 2.5 to 3 were used. At insemination (day 0), cows were divided
   at random in two groups. i) glycerol group (n=95) was given 1 L of a
   glycerol/water solution (90:10 v/v) orally, and again on d 2, 4 and 6;
   whereas the ii) Control group (n=95), was given 1 L water at the same
   time. Pregnancy diagnosis was performed 45 d post insemination through
   rectal examination. Conception rate was compared by X-2 (Chi Square)
   test. In six animals in each group, plasma insulin concentrations were
   determined through radioimmunoassay (R1A) before being drenched with
   glycerol and in the following 15, 30. 60, 90, 120 and 240 min and
   compared between groups by ANOVA for repeated measurements. CR was
   higher in the glycerol group (P < 0.05) (47.4) than in control (31.6).
   Glycerol increased insulin concentration (P<0.05). It is concluded that
   glycerol administered in the first six days post insemination increases
   the conception rate in Holstein cows. The increase in insulin levels
   might be involved in this effect.}},
