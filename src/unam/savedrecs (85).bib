@inproceedings{ ISI:000286947600012,
Author = {Healy, John J.},
Editor = {{Benet, L and Hess, PO and Torres, JM and Wolf, KB}},
Title = {{The linear canonical transform as a propagation model in digital
   holography}},
Booktitle = {{SYMMETRIES IN NATURE}},
