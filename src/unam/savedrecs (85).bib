@incollection{ ISI:000284508500006,
Author = {Adolfo Garcia-Sainz, J. and Teresa Romero-Avila, M. and del Carmen
   Medina, Luz},
Editor = {{Conn, PM}},
Title = {{alpha(1D)-ADRENERGIC RECEPTORS: CONSTITUTIVE ACTIVITY AND REDUCED
   EXPRESSION AT THE PLASMA MEMBRANE}},
