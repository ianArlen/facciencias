@inproceedings{ ISI:000286947600005,
Author = {Castanos, O. and Nahmad-Achar, E. and Lopez-Pena, R. and Hirsch, J. G.},
Editor = {{Benet, L and Hess, PO and Torres, JM and Wolf, KB}},
Title = {{Symmetry adapted states and the quantum phase transition in the Dicke
   model}},
Booktitle = {{SYMMETRIES IN NATURE}},
Series = {{AIP Conference Proceedings}},
Year = {{2010}},
Volume = {{1323}},
Pages = {{40-51}},
Note = {{Symposium in Memoriam of Marcos Moshinsky, Cuernavaca, MEXICO, AUG
   07-14, 2010}},
Organization = {{Ctr Int Ciencias AC; Inst Ciencias Fis; Univ Nacl Autonoma Mexico}},
Abstract = {{We study the phase transition properties of the ground state of the
   Dicke model by means of a variational procedure together with the
   catastrophe formalism. The proposed variational state is the tensorial
   product of the SU(2) and Weyl coherent states, and the expectation value
   of the Dicke Hamiltonian with respect this state is calculated. The
   stability properties of the energy surface exhibit a phase transition
   from the normal to the super-radiant behavior of the two-level atoms,
   when (omega) over tilde (a)(omega) over tilde (F) = +/- 4 (gamma) over
   bar (2). At the same time, the analytic form of the ground state in
   terms of the minimum critical points of the system is obtained. The
   Dicke Hamiltonian is invariant under transformations of the point group
   C-2 = \{I, exp(i pi(Lambda) over cap)\}, with (Lambda) over cap the
   excitation number operator. To restore this parity symmetry, we separate
   the variational proposed states into orthogonal even and odd components,
   which give rise to analytic expressions for the ground and first excited
   states. The fidelity of these projected variational states is verified
   by studying their overlap with the exact quantum results.}},
