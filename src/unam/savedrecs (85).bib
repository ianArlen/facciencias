@article{ ISI:000281166600016,
Author = {Morales-Saavedra, Omar G. and Esther Mata-Zamora, Maria and
   Ontiveros-Barrera, Fernando G. and Banuelos, Jose G. and
   Ortega-Martinez, Roberto and Pelzl, Gerhard},
Title = {{Optical Properties of Fluorinated Bent-Core Liquid Crystals Confined in
   Anodic Alumina Nanotubes}},
Journal = {{MOLECULAR CRYSTALS AND LIQUID CRYSTALS}},
Year = {{2010}},
Volume = {{526}},
Pages = {{139-155}},
Note = {{13th International Topical Meeting on Optics of Liquid Crystals, Ettore
   Majorana Fdn, Erice, ITALY, SEP 28-OCT 02, 2009}},
Organization = {{Ctr Sci Culture}},
Abstract = {{In the present work, oxalic amorphous porous anodic alumina membranes
   with highly ordered porous arrays and average nanometric porous
   dimensions of 70 nm in diameter and 17 microns in depth (nanotubes) were
   prepared and successfully used as host matrix for fluorinated bent-core
   liquid crystals. Atomic force microscopy studies were performed on the
   organic-inorganic hybrid samples in order to explore surface morphology
   and optimal insertion of these liquid crystalline (LC)-compounds into
   this environment. The quadratic nonlinear optical (NLO) and
   spectroscopic properties of the implemented mesogen with the
   nanotube-like confinement were systematically studied in order to
   evaluate its optical performance. Bent core molecules have shown
   interesting optical properties which have not yet been intensively
   investigated in solid-state hybrid structures. Hence, the obtained
   hybrid composites represent a promising field of investigation in the
   route to functional bent-core based materials, where different bent-core
   mesomorphic structures can be obtained and are of interest for new and
   improved applications in nanotechnology.}},
