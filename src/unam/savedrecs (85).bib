@inproceedings{ ISI:000285837400021,
Author = {Arzola, Alejandro V. and Volke-Sepulveda, K. and Mateos, Jose L.},
Editor = {{Dholakia, K and Spalding, GC}},
Title = {{Deterministic Optical Rocking Ratchet: Theory and Experiment}},
Booktitle = {{OPTICAL TRAPPING AND OPTICAL MICROMANIPULATION VII}},
Series = {{Proceedings of SPIE-The International Society for Optical Engineering}},
Year = {{2010}},
Volume = {{7762}},
