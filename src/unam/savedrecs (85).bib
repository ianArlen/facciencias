@article{ ISI:000280873800007,
Author = {Bladt, Mogens and Nielsen, Bo Friis},
Title = {{ON THE CONSTRUCTION OF BIVARIATE EXPONENTIAL DISTRIBUTIONS WITH AN
   ARBITRARY CORRELATION COEFFICIENT}},
Journal = {{STOCHASTIC MODELS}},
Year = {{2010}},
Volume = {{26}},
Number = {{2}},
Pages = {{295-308}},
Abstract = {{In this article we use the concept of multivariate phase-type
   distributions to define a class of bivariate exponential distributions.
   This class has the following three appealing properties. Firstly, we may
   construct a pair of exponentially distributed random variables with any
   feasible correlation coefficient (also negative). Secondly, the class
   satisfies that any linear combination (projection) of the marginal
   random variables is a phase-type distribution. The latter property is
   partially important for the development of hypothesis testing in linear
   models. Finally, it is easy to simulate the exponential random vectors.}},
