@article{ ISI:000289740000010,
Author = {Baronnet, Bruno},
Title = {{Zapatism and autonomous education: from rebellion to indigenous dignity}},
Journal = {{SOCIEDADE E CULTURA}},
Year = {{2010}},
Volume = {{13}},
Number = {{2}},
Pages = {{247-258}},
Abstract = {{From the late `90s, the Mayan communities belonging to the Zapatista
   autonomous municipalities of Chiapas are implementing the right of
   indigenous peoples to build and manage their schools according to their
   own modes of internal organization. This means that families of
   militants have the ability to select, train and evaluate the bilingual
   educator. In the daily routine of their educational rebellion, the
   support bases of the Zapatista movement in the rural area of the valley
   of the Lacandon Jungle are committed internally through the practices of
   assembly and delegations of community posts. In opposition to the
   centralization of national Indian policy, communitarian organizing of
   the autonomous education helps to strengthen the collective dignity of
   direct actors, who are proud to be able to build and get their hands on
   local control of formal education in the rebels' territories in
   southeastern Mexico.}},
