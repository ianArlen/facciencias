@inproceedings{ ISI:000281438100109,
Author = {Aguilar-Rodriguez, E. and Blanco-Cano, X. and Russel, C. T. and Jian, L.
   K. and Luhmann, J. G. and Ramirez Velez, J. C.},
Editor = {{Maksimovic, M and Issautier, K and MeyerVernet, N and Moncuquet, M and Pantellini, F}},
Title = {{Study of Interplanetary Shocks Using Multi-Spacecraft Observations}},
Booktitle = {{TWELFTH INTERNATIONAL SOLAR WIND CONFERENCE}},
