@inproceedings{ ISI:000392008600146,
Author = {Villalobos-Aragon, A. and Ellis, A. S. and Armienta, Ma A. and Morton
   Bermea, O.},
Editor = {{Birkle, P and TorresAlvarado, IS}},
Title = {{Chromium contamination in Leon valley, Guanajuato, Mexico: A Cr stable
   isotope perspective}},
Booktitle = {{WATER-ROCK INTERACTION (WRI-13)}},
