   Lopez-Santiago, N. R.},
Editor = {{Birkle, P and TorresAlvarado, IS}},
Title = {{Interactions between four polycyclic aromatic hydrocarbons (PAHs) and
   the birnessite surface}},
Booktitle = {{WATER-ROCK INTERACTION (WRI-13)}},
