@article{ ISI:000271491300013,
Author = {Meneses, Gabriela and Berzunza, Miriam and Becker, Ingeborg and Bobes,
   Raul J. and Rosas, Gabriela and Sciutto, Edda and Fragoso, Gladis},
Title = {{Taenia crassiceps cysticercosis: Variations in its parasite growth
   permissiveness that encounter with local immune features in BALB/c
   substrains}},
Journal = {{EXPERIMENTAL PARASITOLOGY}},
Year = {{2009}},
Volume = {{123}},
Number = {{4}},
Pages = {{362-368}},
Month = {{DEC}},
Abstract = {{This study describes the first days of Taenia crassiceps infection in
   BALB/c substrains, BALB/cAnN and BALB/cJ, using two stocks of the same
   strains which were kept in different animal facilities, conventional and
   pathogen-free conditions, respectively. This study shows that parasite
   growth restriction shown by conventional BALB/cJ mice changed to
   parasite growth permissiveness when pathogen-free BALB/cJ mice were
   used. In addition, the higher number of macrophages, NK cells and
   intraperitoneal level of IFN-gamma found in the conventional restrictive
   BALB/cJ substrain vanished when the permissiveness to the parasite
   growth increased. No differences were found in DNA sequences of
   parasites collected before and after the change in the permissiveness to
   parasite growth which favors the possibility that the observed
   modifications could be due to changes in the murine strains and/or their
   maintenance conditions. (c) 2009 Elsevier Inc. All rights reserved.}},
