@article{ ISI:000284635100010,
Author = {Sabin, John R. and Oddershede, Jens and Cabrera-Trujillo, Remigio and
   Sauer, Stephan P. A. and Deumens, Erik and Oehrn, Yngve},
Title = {{Stopping power of molecules for fast ions}},
Journal = {{MOLECULAR PHYSICS}},
Year = {{2010}},
Volume = {{108}},
Number = {{21-23, SI}},
Pages = {{2891-2897}},
Abstract = {{The interaction of fast (tens of keV/amu) ions with atoms, molecules,
   and solids, has been a subject of interest to some in the Quantum Theory
   Project and their collaborators for some time, resulting in theoretical
   and computational investigations of various aspects of the problem. This
