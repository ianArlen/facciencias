Author = {Orozco-Villasenor, F. J.},
Editor = {{Birkle, P and TorresAlvarado, IS}},
Title = {{Mineralogy and genesis of the Rayas ore shoot, and its geological
   relation with other ore bodies, Veta Madre, Guanajuato, Mexico}},
Booktitle = {{WATER-ROCK INTERACTION (WRI-13)}},
