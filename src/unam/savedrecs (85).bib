@inproceedings{ ISI:000392008600192,
Author = {Perez-Zarate, D. and Torres-Alvarado, I. S. and Santoyo, E. and
   Martinez-Frias, J.},
Editor = {{Birkle, P and TorresAlvarado, IS}},
Title = {{Statistical methodology to predict reaction time of water-rock
   experiments under quasi-steady state conditions}},
Booktitle = {{WATER-ROCK INTERACTION (WRI-13)}},
Year = {{2010}},
Pages = {{809-813}},
Note = {{13th International Symposium on Water-Rock Interaction (WRI),
   Guanajuato, MEXICO, AUG 16-20, 2010}},
