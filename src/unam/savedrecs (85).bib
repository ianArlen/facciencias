@article{ ISI:000272764000005,
Author = {Ramirez, Alfredo and Cocoletzi, Gregorio H. and Canto, G. and Takeuchi,
   Noboru},
Title = {{First principles calculations of the adsorption and diffusion of Y on
   the Si(001)-c(4 x 2) surface}},
Journal = {{SURFACE SCIENCE}},
Year = {{2009}},
Volume = {{603}},
Number = {{24}},
Pages = {{3414-3419}},
Month = {{DEC 15}},
Abstract = {{We have studied the adsorption and diffusion of yttrium on the Si(0
   01)-c(4 x 2) surface in the early stages of growth. Our first principles
   total energy calculations are based on the density functional theory as
   implemented in the SIESTA code. The exchange and correlation energies
   are treated within the generalized gradient approximation according to
   the Perdew, Burke and Ernzerhof parametrization. Our results demonstrate
   that the most favorable adsorption site is in the trench between two
   silicon dinner rows, identified as valley-bridge (V). Our studies show
   that the diffusion of an Y adatom on Si(0 0 1)-c(4 x 2) surface presents
   an anisotropic behavior. We found two values for the barriers along the
   valley (0.54 and 1.07 eV) and one of 1.24 eV in the perpendicular
   direction, showing that diffusion along the valley is more probable. The
   analysis of the Mulliken overlap populations shows that the bonding
   between an Y adatom and the surface is partially covalent. Two Y atoms
   on the surface do not form dimers instead they are adsorbed as adatoms.
   (C) 2009 Elsevier B.V. All rights reserved.}},
