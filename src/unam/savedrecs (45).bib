@article{ ISI:000349305300001,
Author = {Cristadoro, Giampaolo and Gilbert, Thomas and Lenci, Marco and Sanders,
   David P.},
Title = {{Machta-Zwanzig regime of anomalous diffusion in infinite-horizon
   billiards}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2014}},
Volume = {{90}},
Number = {{5}},
Month = {{NOV 26}},
Abstract = {{We study diffusion on a periodic billiard table with an infinite horizon
   in the limit of narrow corridors. An effective trapping mechanism
   emerges according to which the process can be modeled by a Levy walk
   combining exponentially distributed trapping times with free propagation
   along paths whose precise probabilities we compute. This description
   yields an approximation of the mean squared displacement of
   infinite-horizon billiards in terms of two transport coefficients, which
   generalizes to this anomalous regime the Machta-Zwanzig approximation of
   normal diffusion in finite-horizon billiards.}},
