@article{ ISI:000345216100016,
Author = {Bautista-Flores, C. and Sato-Berru, R. Y. and Mendoza, D.},
Title = {{Charge transfer in the fullerene C-60-few layer graphene system and the
   existence of negative photoconductivity}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2014}},
Volume = {{105}},
Number = {{19}},
Month = {{NOV 10}},
Abstract = {{The electrical conductance of few layer graphene during thermal
   evaporation of fullerene C-60 as well as the conductance under
   illumination of this bilayer junction is reported. We obtained few layer
   graphene films by chemical vapor deposition technique, and then C-60 was
   thermally evaporated on these films. We found an increase in conductance
   of few layer graphene during the thermal evaporation of C-60 and a kind
   of negative photoconductivity when the bilayer junction was under
   illumination. We observed that at low light intensities few layer
   graphene is p-type doped, and we propose that its behavior changes to
   n-type for high light intensities. (C) 2014 AIP Publishing LLC.}},
