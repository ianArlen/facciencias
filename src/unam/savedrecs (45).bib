@article{ ISI:000339455500022,
Author = {Clapp, Monica and Ghimenti, Marco and Micheletti, Anna Maria},
Title = {{Solutions to a singularly perturbed supercritical elliptic equation on a
   Riemannian manifold concentrating at a submanifold}},
Journal = {{JOURNAL OF MATHEMATICAL ANALYSIS AND APPLICATIONS}},
Year = {{2014}},
Volume = {{420}},
Number = {{1}},
Pages = {{314-333}},
Month = {{DEC 1}},
Abstract = {{Given a smooth Riemannian manifold (M, g) we investigate the existence
   of positive solutions to the equation
   -epsilon(2) Delta(g)u + u = u(p-1) on M
   which concentrate at some submanifold of M as epsilon -> 0, for
   supercritical nonlinearities. We obtain a positive answer for some
   manifolds, which include warped products. Using one of the projections
   of the warped product Or some harmonic morphism, we reduce this problem
   to a problem of the form
   -epsilon(2)div(h)(c(x)del(h)u) + a(x)u = b(x)u(p-1),
   with the the same exponent p, on a Riemannian manifold (M, h) of smaller
   dimension, so that p turns out to be subcritical for this last problem.
   Then, applying Lyapunov-Schmidt reduction, we establish existence of a
   solution to the last problem which concentrates at a point as epsilon ->
   0. (C) 2014 Elsevier Inc. All rights reserved.}},
