@article{ ISI:000349465900407,
Author = {Lopez Noda, Rene and Acevedo Salas, Ulises and Gaudisson, Thomas and
   Calderon Pinar, Francisco and Ammar, Souad and Valenzuela, Raul},
Title = {{Magnetoelectric Coupling in BaTiO3-CoFe2O4 Nanocomposites Studied by
   Impedance Spectroscopy Under Magnetic Field}},
Journal = {{IEEE TRANSACTIONS ON MAGNETICS}},
Year = {{2014}},
Volume = {{50}},
Number = {{11}},
