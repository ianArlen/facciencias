@article{ ISI:000348245400011,
Author = {Gonzalez-Garcia, Conne L. and Reyes-Mendez, Miguel A. and
   Ortega-Pierres, Luz E. and Rodriguez-Sanchez, Adriana P. and
   Sandoval-Guido, Veronica and Sereno-Colo, Jose A.},
Title = {{Seroprevalence and detection of primary infection by cytomegalovirus
   with IgG avidity test during the first quarter of pregnancy}},
Journal = {{SALUD PUBLICA DE MEXICO}},
Year = {{2014}},
Volume = {{56}},
Number = {{6}},
Pages = {{619-624}},
Month = {{NOV-DEC}},
Abstract = {{Objective. To determine the seroprevalence and detection of primary
   infection by cytomegalovirus (CMV) with immunoglobulin G (IgG) avidity
   test during the first quarter of pregnancy in the General Hospital in
   Morelia, Michoacan. Materials and methods. A total of 177 patients were
   studied employing a modified Elisa test using a chemiluminescent
   microparticle immunoassay (CMIA) for the detection of CMV antibodies
   (IgG and immunoglobulin M {[}IgM]), and IgG avidity. Results. 90.4\%
   were positive for IgG, and of these, 2.3\% were also reactive for IgM,
   and in this group the IgG avidity test reported low avidity for 1.1\%
   and higher avidity in the same percentage. 9.6\% were seronegative.
   Conclusions. Similarity was found with published studies in Mexico.
   Health professionals should know the clinical algorithms for diagnosis
   and proper management of CMV infection using the IgG avidity test.}},
Publisher = {{INST NACIONAL SALUD PUBLICA}},
Address = {{AV UNIVERSIDAD 655, COL SANTA MARIA AHUACATITLAN, CUERNAVACA 62508,
   MORELOS, MEXICO}},
