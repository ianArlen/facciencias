@article{ ISI:000346793200098,
Author = {Hernandez-Perez, Abigail and Bah, Moustapha and Ibarra-Alvarado, Cesar
   and Fausto Rivero-Cruz, Jose and Rojas-Molina, Alejandra and Isela
   Rojas-Molina, Juana and Alejandro Cabrera-Luna, Jose},
Title = {{Aortic Relaxant Activity of Crataegus gracilior Phipps and
   Identification of Some of Its Chemical Constituents}},
Journal = {{MOLECULES}},
Year = {{2014}},
Volume = {{19}},
Number = {{12}},
Pages = {{20962-20974}},
Month = {{DEC}},
Abstract = {{This study focused on the assessment of the vasorelaxant activity of the
   organic and aqueous extracts obtained from leaves and fruits of a
   Mexican hawthorn (Crataegus gracilior) on isolated rat aorta, and on the
   purification and identification of some of their secondary metabolites
   by the use of chromatographic and spectroscopic techniques. The results
   obtained showed that the methanol extract has a significantly more
   potent and effective vasorelaxant effect than the other tested extracts,
   with an EC50 = 8.69 +/- 4.34 mu g/mL and an E-max = 94.6\% +/- 11.30\%,
   values that are close to that of acetylcholine, the positive control.
   From the same extract, two major triterpenes were isolated and
   identified as ursolic and corosolic acids by comparison of their
   experimental NMR spectroscopic data with those reported in the
   literature. Chlorogenic acid, rutin, quercetin, kaempferol and
   (+)-catechin were also identified using HPLC coupled with PDAD. All
   these compounds have already been proven to possess on their own
   antihypertensive effect and other benefits on cardiovascular diseases
   and they can support, at least in part, the traditional use of this
