@article{ ISI:000345135400001,
Author = {Guillot, Adolfo and Sanchez Godinez, Antonia},
Title = {{A classification of locally homogeneous affine connections on compact
   surfaces}},
Journal = {{ANNALS OF GLOBAL ANALYSIS AND GEOMETRY}},
Year = {{2014}},
Volume = {{46}},
Number = {{4}},
Pages = {{335-349}},
Month = {{DEC}},
Abstract = {{We classify the affine connections on compact orientable surfaces for
   which the pseudogroup of local isometries acts transitively. We prove
   that such a connection is either torsion-free and flat, the Levi-Civita
   connection of a Riemannian metric of constant curvature or the quotient
