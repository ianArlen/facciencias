@article{ ISI:000342463400020,
Author = {Cremades, A. and Herrera, M. and Bartolome, J. and Vasquez, G. C. and
   Maestre, D. and Piqueras, J.},
Title = {{On the thermal growth and properties of doped TiO2 and In2O3 elongated
   nanostructures and nanoplates}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
Year = {{2014}},
