@article{ ISI:000359114500001,
Author = {Gutierrez-Estrada, Alejandra and Ramirez-Santos, Jesus and del Carmen
   Gomez-Eichelmann, Maria},
Title = {{Role of chaperones and ATP synthase in DNA gyrase reactivation in
   Escherichia coli stationary-phase cells after nutrient addition}},
Journal = {{SPRINGERPLUS}},
Year = {{2014}},
Volume = {{3}},
Month = {{NOV 6}},
Abstract = {{Escherichia coli stationary-phase (SP) cells contain relaxed DNA
   molecules and recover DNA supercoiling once nutrients become available.
   In these cells, the reactivation of DNA gyrase, which is a DNA
   topoisomerase type IIA enzyme, is responsible for the recovery of DNA
   supercoiling. The results presented in this study show that DNA gyrase
   reactivation does not require cellular chaperones or polyphosphate.
   Glucose addition to SP cells induced a slow recovery of DNA
   supercoiling, whereas resveratrol, which is an inhibitor of ATP
   synthase, inhibited the enzyme reactivation. These results suggest that
   DNA gyrase, which is an ATP-dependent enzyme, remains soluble in SP
   cells, and that its reactivation occurs primarily due to a rapid
   increase in the cellular ATP concentration.}},
