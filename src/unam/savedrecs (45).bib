@article{ ISI:000347163300043,
Author = {Gonzalez, F. and Jung, C.},
Title = {{A development scenario connecting the ternary symmetric horseshoe with
   the binary horseshoe}},
Journal = {{CHAOS}},
Year = {{2014}},
Volume = {{24}},
Number = {{4}},
Month = {{DEC}},
Abstract = {{It is explained in which way the ternary symmetric horseshoe can be
   obtained along a development scenario starting with a binary horseshoe.
   We explain the case of a complete ternary horseshoe in all detail and
   then give briefly some further incomplete cases. The key idea is to
   start with a three degrees of freedom system with a rotational symmetry,
   reduce the system with the help of the conserved angular momentum to one
   with two degrees of freedom where the value of the conserved angular
   momentum acts as a parameter and then let its value go to zero. (C) 2014
   AIP Publishing LLC.}},
