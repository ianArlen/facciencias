@article{ ISI:000345581300055,
Author = {Flores-Ramos, Miguel and Ibarra-Velarde, Froylan and Hernandez-Campos,
   Alicia and Vera-Montenegro, Yolanda and Jung-Cook, Helgi and
   Canto-Alarcon, Germinal J. and Misael del Rivero, Lauro and Castillo,
   Rafael},
Title = {{A highly water soluble benzimidazole derivative useful for the treatment
   of fasciolosis}},
Journal = {{BIOORGANIC \& MEDICINAL CHEMISTRY LETTERS}},
Year = {{2014}},
Volume = {{24}},
Number = {{24}},
Pages = {{5814-5817}},
Month = {{DEC 15}},
Abstract = {{This study describes the synthesis of compound (7), a highly
   hydrosoluble phosphonooxymethyl prodrug of compound alpha (4). Compound
   (7) improved the aqueous solubility of its precursor compound (4) by
   50,000 times and it is stable at neutral pH. The prodrug showed
   faciolicidal activity when evaluated in vitro against excysted Fasciola
   hepatica metacercariae. The in vivo evaluation of (7) was carried out
   via oral, intramuscular and subcutaneous administration in sheep
   artificially infected with F. hepatica metacercariae. At an
   intramuscular dose of 4 mg/kg, the activity of (7) was similar to that
   of compound alpha (4) at an oral dose of 15 mg/kg. (C) 2014 Elsevier
   Ltd. All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
Type = {{Article}},
