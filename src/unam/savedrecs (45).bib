@article{ ISI:000345720600028,
Author = {Medina-Torres, Luis and Calderas, Fausto and Sanchez-Olivares, Guadalupe
   and Nunez-Ramirez, Diola-Marina},
Title = {{Rheology of Sodium Polyacrylate as an Emulsifier Employed in Cosmetic
   Emulsions}},
