@article{ ISI:000348646000008,
Author = {Arikit, Siwaret and Xia, Rui and Kakrana, Atul and Huang, Kun and Zhai,
   Jixian and Yan, Zhe and Valdes-Lopez, Oswaldo and Prince, Silvas and
   Musket, Theresa A. and Nguyen, Henry T. and Stacey, Gary and Meyers,
