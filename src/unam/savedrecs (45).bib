@article{ ISI:000361550000010,
Author = {Adriana Aguilar-Castillo, Bethsy and Santos, Jose Luis and Luo, Hanying
   and Aguirre-Chagala, Yanet E. and Palacios-Hernandez, Teresa and
   Herrera-Alonso, Margarita},
Title = {{Nanoparticle stability in biologically relevant media: influence of
   polymer architecture}},
