@article{ ISI:000229195800010,
Author = {Torres, M and Kunold, A},
Title = {{A model for the microwave assisted magnetoresistance oscillations in a
   2D electron system}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
