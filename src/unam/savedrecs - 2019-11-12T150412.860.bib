@article{ ISI:000231081000010,
Author = {Salinas, MEO and Flores, GSM and Rodriguez, FL},
Title = {{Academic achievement and its relationship with interests and previous
   knowledge in university students}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2005}},
Volume = {{22}},
Number = {{1}},
Pages = {{103-106}},
Month = {{JUN}},
Abstract = {{We evaluated the relationship among previous knowledge, interest, and
   school achievement in university students. Three groups enrolled in the
   Learning and Memory course participated, as well as one group that had
   already taken such course, and eight professors. An initial knowledge
   exam, an attitude scale, and a final exam were used. The previous
   knowledge questionnaire and the attitude scale were presented at the
   beginning of the course and the final exam was applied at the end.
   Positive correlations were found between previous knowledge and the
   average obtained during the course, and between previous knowledge and
   score obtained in the final exam. Although the attitude scale showed
   high internal consistency, no relation was found between data from the
   attitude scale, grade average, or the final grade of the course.}},
