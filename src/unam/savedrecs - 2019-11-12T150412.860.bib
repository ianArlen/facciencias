@article{ ISI:000234266300032,
Author = {Ghisellini, G and Ghirlanda, G and Firmani, C and Lazzati, D and
   Avila-Reese, V},
Title = {{Cosmology with Gamma-Ray Bursts}},
Journal = {{NUOVO CIMENTO DELLA SOCIETA ITALIANA DI FISICA C-COLLOQUIA ON PHYSICS}},
Year = {{2005}},
Volume = {{28}},
