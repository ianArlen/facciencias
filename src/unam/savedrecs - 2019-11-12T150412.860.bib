@article{ ISI:000230102300013,
Author = {Martinez-Galicia, R and Romero-Rochin, V},
Title = {{On the necessity of the postulate of the collapse of the wave function:
   an explicit calculation of the ``double-slit{''} problem}},
Journal = {{REVISTA MEXICANA DE FISICA}},
Year = {{2005}},
Volume = {{51}},
Number = {{3}},
Pages = {{304-310}},
Month = {{JUN}},
Abstract = {{Based on numerical calculations of the two-slit problem, we object van
   Kampen's proposal that the collapse of the wave function is an
   unnecessary postulate of Quantum Mechanics if the measuring apparatus is
   included in a unitary evolution or the system-apparatus. We argue that
   van Kampen's interpretation does not substitute the collapse postulate,
   it is not precise and it appears impracticable.}},
