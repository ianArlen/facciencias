@article{ ISI:000230797300007,
Author = {Uribe-Hernandez, R and Perez-Zapata, AJ},
Title = {{In vitro DNA fragmentation induction by anthracene and benzo(a)pirene in
   human polimorphonuclear leukocytes}},
Journal = {{INTERCIENCIA}},
Year = {{2005}},
Volume = {{30}},
Number = {{7}},
Pages = {{419-423}},
Month = {{JUL}},
Abstract = {{The induction of the DNA fragmentation in human polymorphonuclear
   leukocytes by exposure to anthracene and benzo(a)pyrene (B(a)P) was
   determined, and the relation with radical superoxide production and
   lethal cytotoxicity of these compounds were evaluated. Anthracene showed
   higher citotoxicity (LC50= 3.23 mu M) than B(a)P (0.352 mu M). A
   dose-dependent increase in superoxide radical production was only
   observed after B(a)P exposure, with a proportional relation (R=0.96; P <
   0.01). Anthracene did not induce a significant genotoxicity effect,
   while B(a)P produced significant induction of DNA fragmentation at
   concentrations of 0.023-2.35yM (P < 0.05). The results show the
   relationship between superoxide radical production and the induction of
   DNA fragmentation and are suggestive of the possibility that the
   production of superoxide radical be involved in the genotoxicity
   mechanism of B(a)P It is concluded that anthracene has a higher
   citotoxicity potential than B(a)P; however, the genotoxic effect is only
   produced by B(a)P. The dose-effect relationship between B(a)P and DNA
   fragmentation is suggestive of a good biomarker of genetic damage by
   exposure to this type of environmental pollutant.}},
