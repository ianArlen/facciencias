@article{ ISI:000230257100009,
Author = {Mahalingam, T and Chitra, JSP and Chu, JP and Velumani, S and Sebastian,
   PJ},
Title = {{Structural and annealing studies of potentiostatically deposited Cu2O
   thin films}},
