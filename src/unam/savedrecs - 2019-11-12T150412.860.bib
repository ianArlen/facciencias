@article{ ISI:000230776400034,
Author = {Suarez-Gomez, A and Calderon-Pinar, F and Durruthy, MD and
   Pelaiz-Barranco, A and Saniger-Blesa, J and de Frutos, J},
Title = {{Characterization of PZT (54/46) ferroelectric ceramics under the
   influence of a `soft' double modification with La and Nb 2}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
Year = {{2005}},
Volume = {{242}},
Number = {{9}},
Pages = {{1892-1896}},
Month = {{JUL}},
Note = {{17th Latin American Symposium on Solid State Physics, Havana, CUBA, DEC
   06-09, 2004}},
Organization = {{Cuban Phys Soc; Univ Havana; Latin Amer Ctr Phys; Int Ctr Theoret Phys;
   Int Union Pure \& Appl Phys; Alexander VonHumboldt Fdn; Wiley VCH}},
Abstract = {{In the present work, a `soft-doped' PZT ceramic system with a near MPB
   Zr/Ti ratio of about 54/46, is characterized when simultaneously
   modified with Lanthanum (La) and Niobium (Nb) at the same doping level,
   x = 0.4, 1.0 and 1.4 mol\%. X-rays diffraction patterns showed the
   rombohedral/tetragonal perovskite phase coexistence as reported for
   Zr/Ti ratios near the Morphotropic Phase Boundary (MPB). Scanning
   Electron Microscopy (SEM) showed a decrease of the mean grain size when
   increasing the doping level. On the other hand, permittivity and AC
   conductivity were analysed in the ferro- and paraelectric phases.
   Samples showed a lower Curie temperature for higher doping levels as
   well as normal ferroelectric/paraelectric transitions. The piezoelectric
   response of thin polarized cylinder-shaped samples was measured for a
   near-saturation poling field. The behaviour of some electromechanical
   parameters and possible applications for these materials are also
   discussed herein. (c) 2005 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim.}},
