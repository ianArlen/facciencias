Author = {Hurtado-Castaneda, DM and Fernandez, J and Velazquez, R and Estevez, M
   and Vargas, S and Rodriguez, R and Rodriguez, ME},
Title = {{Kinetics of the drying process of an anti-adherent coating using
   Photothermal Radiometry and Micro-Raman}},
Journal = {{JOURNAL DE PHYSIQUE IV}},
Year = {{2005}},
Volume = {{125}},
Pages = {{593-596}},
