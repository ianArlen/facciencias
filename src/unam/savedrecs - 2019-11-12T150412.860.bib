@article{ ISI:000230436600050,
Author = {Ascanio, G and Caballero-Ruiz, A and Ruiz-Huerta, L and Gonzalez-Cardel,
   M and Diaz-Uribe, R},
Title = {{Scanning laser system to determine the corneal shape}},
Journal = {{REVIEW OF SCIENTIFIC INSTRUMENTS}},
Year = {{2005}},
Volume = {{76}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{The development and tests of a scanning system to be used to determine
   the corneal topography with the laser deflectometry method are
   presented. In this equipment, a He-Ne laser beam scans the cornea by
   describing a spiral trajectory generated by two components: radial and
   angular. The first component is produced by the displacement of a plane
   mirror moved by a linear pneumatic actuator. The second component is
   produced by passing the beam through a Dove prism which is rotating by
   means of a belt drive coupled to a high-speed electric motor. Tests were
   first performed by analyzing both components independently and then they
   were characterized by combining the two components. Results are
   discussed and compared to those of an earlier cited work. (c) 2005
   American Institute of Physics.}},
