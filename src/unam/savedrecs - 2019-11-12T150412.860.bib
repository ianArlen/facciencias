@article{ ISI:000231237000005,
Author = {Fernandez, EM and Balbas, LC},
Title = {{Structural properties of bimetallic clusters from density functional
   calculations}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2005}},
Volume = {{19}},
Number = {{15-17, 1}},
Pages = {{2339-2344}},
