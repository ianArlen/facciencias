@article{ ISI:000230156000006,
Author = {Zhu, XP and Garcia-Reyna, P and Fu, BQ and Yang, J and Li, CV and Yang,
   YP and Liu, MY and Ortega-Pierres, GG and Boireau, P},
Title = {{A stage-specific open reading frame from three-day old adult worms of
   Trichinella spiralis encodes zinc-finger motifs}},
Journal = {{PARASITE-JOURNAL DE LA SOCIETE FRANCAISE DE PARASITOLOGIE}},
Year = {{2005}},
Volume = {{12}},
Number = {{2}},
Pages = {{151-157}},
Month = {{JUN}},
Abstract = {{The aim of the study was to isolate genes coding for stage-specific
   antigens of T. spiralis. Such antigens may then be associated with local
   and systemic immune responses against adult T spiralis. Recombinant
   clones were obtained with an adult stage specific probe from a cDNA
   library of three-day old adult T. spiralis. Several cDNA clones encoding
   the some peptide were identified and their stage specificity was
   confirmed by northern blot analysis. Three independent clones were fully
   sequenced, end the resulting sequence found to code for a 487 amino acid
   peptide with a deduced molecular weight of approximate to 55 kDa.
   Sequence analysis,showed that the 55 kDa peptide contained putative DNA
   binding motifs, suggesting That this protein may be involved in
