@article{ ISI:000231249600008,
Author = {Zhou, XL and Loukin, SH and Coria, R and Kung, C and Saimi, Y},
Title = {{Heterologously expressed fungal transient receptor potential channels
   retain mechanosensitivity in vitro and osmotic response in vivo}},
Journal = {{EUROPEAN BIOPHYSICS JOURNAL WITH BIOPHYSICS LETTERS}},
