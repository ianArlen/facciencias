@article{ ISI:000228958100002,
Author = {Chaos-Cador, L and Ley-Koo, E},
Title = {{Two-dimensional hydrogen atom confined in circles, angles, and circular
   sectors}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2005}},
Volume = {{103}},
Number = {{4}},
Pages = {{369-387}},
Month = {{JUN 15}},
Abstract = {{The Schrodinger equation for the 2D hydrogen atom is separable in
   circular coordinates (rho = root x(2) + y(2), phi = tan(-1) y/x). In
   this article the energy eigenvalues and eigenfunctions of such an atom
   in three different situations of confinement inside (a) a circle (0 <=
   rho <= rho(0), 0 <= phi <= 2 pi), (b) an angle (0 <= rho <= infinity, 0
   <= phi <= phi(0)), and (c) a circular sector (0 <= rho <= rho(0), 0 <=
   phi <= phi(0)) are explicitly constructed. Characteristic properties of
   the atom in its ground state for each situation of confinement such as
   the polarizability for (a) and electric dipole moment for (b) and (c)
   are also evaluated. (c) 2005 Wiley Periodicals, Inc.}},
