@article{ ISI:000230441300015,
Author = {de Gomez-Puyou, MT and Dominguez-Ramirez, L and Perez-Hernadndez, G and
   Gomez-Puyou, A},
Title = {{Effect of denaturants on multisite and unisite ATP hydrolysis by bovine
   heart submitochondrial particles with and without inhibitor protein}},
