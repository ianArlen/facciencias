@article{ ISI:000231081000014,
Author = {Balboa-Verduzco, AM and Sanchez-Castillo, H and Velazquez-Martinez, DN},
Title = {{Effects of limited training in children's temporal estimation}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2005}},
Volume = {{22}},
Number = {{1}},
Pages = {{159-166}},
Month = {{JUN}},
Abstract = {{Previous reports have shown that giving children extensive training in a
   time interval bisection task, the slope of their psychophysical curves
   gets steeper with age. The goal of this study was to separate a posible
   developmental trend from the direct effects from training in a temporal
   bisection task in 8 and 11 year olds, by limiting the amount of
   training. The experiment had 3 phases, in all of which children had to
   identify the duration of a visual stimulus ({''}short{''} or ``long{''})
   by pressing one of two keys. During the pre-training and training
   phases, the standar durations were presented randomly, whereas in the
   generalization phase, standard durations were randomly mixed with
   intermediate durations. Results showd that the slope of the
   psychophysical function of the two age groups is significatively
   different, being shallower for the 8 year old group. Thus, limited
   training in a temporal bisection-task shows that temporal sensitivity
   increases with age; additionally, the bisection point values suggest
   that a time estimation mechanism is operational since early childhood.}},
