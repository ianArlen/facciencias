@article{ ISI:000229223200010,
Author = {Vazquez-Fernandez, E and Gracia-Sanchez, J},
Title = {{Review of the hydraulic capacity of bridges in a coastal area}},
Journal = {{JOURNAL OF HYDRAULIC ENGINEERING}},
Year = {{2005}},
Volume = {{131}},
Number = {{6}},
Pages = {{517-520}},
Month = {{JUN}},
Abstract = {{This technical note presents an analysis of the hydraulic capacity of
   several bridges in a coastal area in Mexico, following cyclonic floods.
   The heights and lengths of the bridges and the flow rates of the rivers
   were the comparison parameters used. The analysis showed that for floods
   occurring within a 100-year return period these bridges were
   insufficient in length and/or height. A comparative graph shows that the
   failed and nonfailed bridges are clearly separated by a tendency line
   and its equation was obtained. Without intending this equation to be
   taken as a universal rule, its application could be a useful point of
   departure in reviewing the design of bridges in coastal areas.}},
