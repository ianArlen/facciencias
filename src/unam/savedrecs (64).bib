@article{ ISI:000308243400042,
Author = {Hernandez-Santoyo, Alejandra and Dominguez-Ramirez, Lenin and
   Reyes-Lopez, Cesar A. and Gonzalez-Mondragon, Edith and Hernandez-Arana,
   Andres and Rodriguez-Romero, Adela},
Title = {{Effects of a Buried Cysteine-To-Serine Mutation on Yeast Triosephosphate
   Isomerase Structure and Stability}},
Journal = {{INTERNATIONAL JOURNAL OF MOLECULAR SCIENCES}},
Year = {{2012}},
Volume = {{13}},
Number = {{8}},
Pages = {{10010-10021}},
Month = {{AUG}},
Abstract = {{All the members of the triosephosphate isomerase (TIM) family possess a
   cystein residue (Cys126) located near the catalytically essential
   Glu165. The evolutionarily conserved Cys126, however, does not seem to
   play a significant role in the catalytic activity. On the other hand,
   substitution of this residue by other amino acid residues destabilizes
   the dimeric enzyme, especially when Cys is replaced by Ser. In trying to
   assess the origin of this destabilization we have determined the crystal
   structure of Saccharomyces cerevisiae TIM (ScTIM) at 1.86 angstrom
   resolution in the presence of PGA, which is only bound to one subunit.
   Comparisons of the wild type and mutant structures reveal that a change
   in the orientation of the Ser hydroxyl group, with respect to the Cys
   sulfhydryl group, leads to penetration of water molecules and apparent
   destabilization of residues 132-138. The latter results were confirmed
   by means of Molecular Dynamics, which showed that this region, in the
   mutated enzyme, collapses at about 70 ns.}},
