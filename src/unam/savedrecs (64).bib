@article{ ISI:000307790900002,
Author = {Barranco, Juan and Bernal, Argelia and Carlos Degollado, Juan and
   Diez-Tejedor, Alberto and Megevand, Miguel and Alcubierre, Miguel and
   Nunez, Dario and Sarbach, Olivier},
Title = {{Schwarzschild Black Holes can Wear Scalar Wigs}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2012}},
Volume = {{109}},
Number = {{8}},
Month = {{AUG 22}},
Abstract = {{We study the evolution of a massive scalar field surrounding a
   Schwarzschild black hole and find configurations that can survive for
   arbitrarily long times, provided the black hole or the scalar field mass
   is small enough. In particular, both ultralight scalar field dark matter
   around supermassive black holes and axionlike scalar fields around
   primordial black holes can survive for cosmological times. Moreover,
   these results are quite generic in the sense that fairly arbitrary
   initial data evolve, at late times, as a combination of those long-lived
   configurations.}},
