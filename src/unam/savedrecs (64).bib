@article{ ISI:000306106000033,
Author = {Torres, Alfredo Ramirez and Cocoletzi, Gregorio H. and Vazquez-Nava, R.
   A. and Lopez-Fuentes, M. and Takeuchi, Noboru},
Title = {{Theoretical study of the formation of a GaAs bilayer on Si(111)}},
Journal = {{COMPUTATIONAL MATERIALS SCIENCE}},
Year = {{2012}},
Volume = {{62}},
Pages = {{216-220}},
Month = {{SEP}},
Abstract = {{We have performed first principles total energy calculations, within the
   density functional theory, to investigate the formation of a GaAs
   bilayer on the Si(111) surface. For the exchange-correlation energies we
   have used the generalized gradient approximation, while the electron-ion
   interactions were treated using ultrasoft pseudo-potentials. We have
   first studied the adsorption of As and Ga atoms on the Si(111) surface.
   A monolayer of As atoms substitutes the topmost Si layer forming an
   almost ideally bulk terminated configuration. On the other hand, the
   adsorption of 1/3 ML of Ga results in the formation of a root 3 x root 3
   reconstruction, with Ga atoms occupying T4 sites. The deposit of 1/3 ML
   of Ga atoms on the As terminated Si(111) surface also results in the
   occupation of T4 sites. However, at full monolayer coverage, the Ga
   atoms occupy near top sites and form one dimensional atomic chain. (C)
   2012 Elsevier B. V. All rights reserved.}},
