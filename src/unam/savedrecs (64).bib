@article{ ISI:000308644300012,
Author = {Barkla, Bronwyn J. and Vera-Estrella, Rosario and Pantoja, Omar},
Title = {{Protein profiling of epidermal bladder cells from the halophyte
   Mesembryanthemum crystallinum}},
Journal = {{PROTEOMICS}},
Year = {{2012}},
