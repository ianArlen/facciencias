@article{ ISI:000309427500045,
Author = {Espinosa-Ceron, A. and Malomed, B. A. and Fujioka, J. and Rodriguez, R.
   F.},
Title = {{Symmetry breaking in linearly coupled Korteweg-de Vries systems}},
