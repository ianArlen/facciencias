@article{ ISI:000306646900037,
Author = {Blancas-Flores, Gerardo and Alarcon-Aguilar, Francisco J. and
   Garcia-Macedo, Rebeca and Almanza-Perez, Julio C. and Flores-Saenz, Jose
   L. and Roman-Ramos, Ruben and Ventura-Gallegos, Jose L. and Kumate,
   Jesus and Zentella-Dehesa, Alejandro and Cruz, Miguel},
Title = {{Glycine suppresses TNF-alpha-induced activation of NF-kappa B in
   differentiated 3T3-L1 adipocytes}},
Journal = {{EUROPEAN JOURNAL OF PHARMACOLOGY}},
Year = {{2012}},
Volume = {{689}},
Number = {{1-3}},
Pages = {{270-277}},
Month = {{AUG 15}},
Abstract = {{Glycine strongly reduces the serum levels of pro-inflammatory cytokines
   and increases the levels of anti-inflammatory cytokines. Recently,
   glycine has been shown to decrease the expression and secretion of
   pro-inflammatory adipokines in monosodium glutamate-induced obese
   (MSG/Ob) mice. It has been postulated that these effects may be
   explained by a reduction in nuclear factor kappa B (NF-kappa B)
   activation. NF-kappa B is a transcription factor, which is crucial to
   the inflammatory response. Hasegawa et al. (2011 and 2012) recently
   reported a glycine-dependent reduction in NF-kappa B levels. Here, we
   have investigated the role of glycine in the regulation of NF-kappa B in
   differentiated 3T3-L1 adipocytes. The results revealed that pretreatment
   with glycine interfered with the activation of NF-kappa B, which has
   been shown to be stimulated by tumor necrosis factor-alpha (TNF-alpha).
   Glycine alone stimulated NF-kappa B activation in an unusual way such
   that the inhibitor kappa B-beta (I kappa B-beta) degradation was more
   significant than that of the inhibitor kappa B-alpha (I kappa B-alpha)
   and led to NF-kappa B complexes comprised of p50 and p65 subunits; I
   kappa B-epsilon degradation did not affect by glycine. These findings
   suggest that glycine could be used as an alternative treatment for
   chronic inflammation, which is a hallmark of obesity and other
   comorbidities, and is characterized by an elevated production of
   pro-inflammatory cytokines. (C) 2012 Elsevier B.V. All rights reserved.}},
