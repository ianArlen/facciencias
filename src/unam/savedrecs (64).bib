@article{ ISI:000308289900008,
Author = {Pinzon-Escobar, E. F. and Sandoval-Romero, G. E.},
Title = {{Analysis of the experimental results of the superluminescent fiber laser
   sources designed for fiber optic sensors}},
Journal = {{LASER PHYSICS}},
Year = {{2012}},
Volume = {{22}},
Number = {{9}},
Pages = {{1406-1410}},
Month = {{SEP}},
Abstract = {{We present a simple analysis and comparison between the theoretical and
   experimental results, in an erbium-doped optical fiber operating in the
   superluminescent regime. Experimental results for different pumping
   power levels and fiber length shows, that the theoretical results given
   for the numerical model could render useful information for predicting
   parameters such as total output power. These types of sources could have
   direct application in wavelength multiplexed arrangements of fiber
   sensors, fiber gyroscopes or in general, in any sensors in which a broad
   wavelength and stable light source is required.}},
