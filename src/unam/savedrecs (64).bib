@article{ ISI:000309206900078,
Author = {Hernandez-Perez, I. and Maubert, A. M. and Rendon, Luis and Santiago,
   Patricia and Herrera-Hernandez, H. and Diaz-Barriga Arceo, L. and
   Garibay Febles, V. and Palacios Gonzalez, Eduardo and Gonzalez-Reyes, L.},
Title = {{Ultrasonic Synthesis: Structural, Optical and Electrical Correlation of
   TiO2 Nanoparticles}},
Journal = {{INTERNATIONAL JOURNAL OF ELECTROCHEMICAL SCIENCE}},
Year = {{2012}},
Volume = {{7}},
Number = {{9}},
Pages = {{8832-8847}},
Month = {{SEP}},
Abstract = {{Sonochemical synthesis of nanometric anatase phase has been successfully
   produced at room temperature by using a simple and environment-friendly
   method. The effect of the heat treatment on the structural properties of
   the as-prepared sample has been evaluated and discussed in this research
   by X-ray powder diffraction and transmission electron microscopy
   techniques. The structural properties are expected to be strongly
   correlated with the optical band gap and electrical properties. The
   results show that the average size of nanocrystallites is in the order
   of 5 nm with large surface area, where as the system presents pores at
   the junctions of crystallites. Optical band gap of as-prepared sample
   has a blue shift, which could be attributed to a quantum size effect.
   The heat-treated reveals structural changes from anatase to rutile
   structure at 550 degrees C. Crystal size increase from 5 to 35 nm, in
   which anatase and rutile phases coexist. However, by electrochemical
   technique the high current-charge magnitude is observed in samples with
   crystallite size of 17 nm but if anatase and rutile are simultaneously
   present the conductivity decreased. This phenomenon is observed if the
   difference between anatase and rutile is about 36\% in size, indicating
   that the conduction and valence bands could be further away in the
   sample.}},
