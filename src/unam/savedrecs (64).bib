@article{ ISI:000308335400077,
Author = {Perez-Osuna, F. and Siqueiros, J. M. and Duran, A. and Cruz, M. P. and
   Salamanca-Riba, L. and Heiras, J.},
Title = {{Magnetic properties of Al doped TbMnO3 thin films grown by pulsed laser
   deposition}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2012}},
Volume = {{112}},
Number = {{3}},
Month = {{AUG 1}},
Abstract = {{We report experimental results on the characterization of the
   structural, compositional, and magnetic properties of c-oriented
   Tb1-xAlxMnO3 (x = 0, 0.05, and 0.10) thin films grown by pulsed laser
   deposition on SrTiO3 (001) substrates. Film thicknesses were 10, 25, 50,
   and 100 nm. X-ray diffraction analysis showed that films were single
   phase preferentially oriented in the (001) direction. The substrate-film
   strain highly increases the magnetization of the thinner films for
   samples with x <= 0.05. The introduction of Al in the films makes
   evident a magnetic anomaly at the temperature T-lock (similar to 27 K)
   of the TbMnO3 single crystal. Furthermore, the dopant causes the
   displacement of the transition temperatures T-N1 due to the Mn+3
   ordering and T-N2 due to the Tb+3 ordering, toward higher temperatures
   for all samples. The most important effect of Al in the samples is to
   enhance the ferromagnetic behavior of the TbMnO3 thin films, improving
   the magnetic performance of this material, leading to a potential
   multiferroic with ferromagnetic ordering. (C) 2012 American Institute of
   Physics. {[}http://dx.doi.org/10.1063/1.4745879]}},
