@article{ ISI:000309315800012,
Author = {Gomez-Rueda, Jose L. and Illanes, Alejandro and Mendez, Hector},
Title = {{Dynamic properties for the induced maps in the symmetric products}},
Journal = {{CHAOS SOLITONS \& FRACTALS}},
Year = {{2012}},
Volume = {{45}},
Number = {{9-10}},
Pages = {{1180-1187}},
Month = {{SEP-OCT}},
Abstract = {{For a metric continuum X, let F-n(X) be the hyperspace of nonempty
   subsets of X with at most n points. Given a map f:X -> X we consider the
   induced map f(n):F-n(X) -> F-n(X) given by f(n)(A) = f(A) (the image of
   A under f). Given a dynamic property P, we study the implication: if A
   is an element of F-n(X) has property P in the system (F-n(X),f(n)), then
   each element of A has property P in the system (X,f), and we also study
   the converse implication. We consider properties as periodicity,
   recurrence, regular recurrence, etc. (C) 2012 Elsevier Ltd. All rights
   reserved.}},
