@article{ ISI:000309019500001,
Author = {Hidalgo-Millan, A. and Zenit, R. and Palacios, C. and Yatomi, R. and
   Horiguchi, H. and Tanguy, P. A. and Ascanio, G.},
Title = {{On the hydrodynamics characterization of the straight Maxblend (R)
   impeller with Newtonian fluids}},
Journal = {{CHEMICAL ENGINEERING RESEARCH \& DESIGN}},
Year = {{2012}},
Volume = {{90}},
Number = {{9}},
Pages = {{1117-1128}},
Month = {{SEP}},
Abstract = {{The hydrodynamics generated by the straight version of the Maxblend (R)
   impeller with Newtonian fluids in a baffled stirred vessel under the
   transitional and turbulent regime has been experimentally characterized
   by means of the particle image velocimetry technique. The flow fields
   obtained with the Maxblend were compared with those obtained with a
   double stage classical pitched blade turbine (PBT) and a double Ekato
   Intermig (R) impellers under the same specific power draw. It is shown
   that these open impellers induce complex local flows in the radial and
   axial direction, with an intensity decreasing away from the blades. By
   contrast, the Maxblend impeller generates a more regular circulation
   pattern, with efficient top-to-bottom pumping. (c) 2012 The Institution
   of Chemical Engineers. Published by Elsevier B.V. All rights reserved.}},
