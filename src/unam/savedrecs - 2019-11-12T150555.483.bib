@article{ ISI:000184650400017,
Author = {Boullosa, RR},
Title = {{A note on the sound radiation from the classical guitar: Influence of
   energy input via the string termination at the fret}},
Journal = {{ACTA ACUSTICA UNITED WITH ACUSTICA}},
Year = {{2003}},
Volume = {{89}},
Number = {{4}},
Pages = {{718-721}},
Month = {{JUL-AUG}},
Abstract = {{The radiated sound due to input of string energy at the frets of a
   classical guitar was estimated and compared to the radiation due to the
   energy input at the bridge. The results show that the energy flows from
   the string to the body of the guitar not only through the bridge, but
   also through the string termination at the fret. The radiated sound due
   to this latter effect is in general much lower than the radiation due to
   the bridge input, but for some of the upper frets the contribution may
   be considerable in certain frequency bands.}},
