@article{ ISI:000182582800047,
Author = {Santoyo, E and Garcia, A and Espinosa, G and Santoyo-Gutierrez, S and
   Gonzalez-Partida, E},
Title = {{Convective heat-transfer coefficients of non-Newtonian geothermal
   drilling fluids}},
Journal = {{JOURNAL OF GEOCHEMICAL EXPLORATION}},
Year = {{2003}},
Volume = {{78-9}},
Number = {{SI}},
Pages = {{249-255}},
Month = {{MAY}},
Note = {{GEOFLUIDS IV Meeting, UTRECHT, NETHERLANDS, MAY 12-16, 2003}},
Abstract = {{A. comprehensive investigation of correlations for estimating convective
   heat-transfer coefficients (CHTC) of non-Newtonian drilling fluids used
   in the geothermal well completion was carried out. Five well-known
   empirical correlations were used to calculate CHTC at fully developed
   laminar and turbulent flow conditions. Transport properties of Newtonian
   and non-Newtonian drilling fluids (measured at the theology laboratory)
   were compared and later used for computing CHTC in a useful temperature
   interval for geothermal drilling purposes (50-180 degreesC). Viscosity
   was identified as one of the main properties that strongly affect the
   CHTC estimation. Newtonian and non-Newtonian fluid viscosities as a
   function of temperature showed significant differences, up to 98\%. The
   use of Newtonian fluid viscosities generally produced an overestimation
   of the mud CHTC. This problem enables us to explain why the temperature
   profiles usually computed by some wellbore thermal simulators predict a
   fast cooling process in the well surrounding formation in comparison
   with CHTC estimated from actual non-Newtonian drilling fluid
   viscosities. (C) 2003 Elsevier Science B.V. All rights reserved.}},
