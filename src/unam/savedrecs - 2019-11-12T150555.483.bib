@article{ ISI:000182582800013,
Author = {Gonzalez-Partida, E and Levresse, G and Gutierrez, ES},
Title = {{Paleo-fluid and actual-fluid in the Los Azufres geothermal field,
   central Mexico}},
Journal = {{JOURNAL OF GEOCHEMICAL EXPLORATION}},
Year = {{2003}},
Volume = {{78-9}},
Number = {{SI}},
Pages = {{67-70}},
Month = {{MAY}},
Note = {{GEOFLUIDS IV Meeting, UTRECHT, NETHERLANDS, MAY 12-16, 2003}},
Abstract = {{After 20 years of exploitation of the Los Azufres field, paleo-fluids
   and actual brines show the same thermodynamic conditions and spatial
   distribution of temperature, pressure, fluids and non-condensate gases
   (NCG) composition. The deepest brines have temperature, salinity and
   pressure ranges of 300-340 degreesC, 0.2-0.8 eq. wt.\% NaCl and 180 bar,
   respectively. A pressure drop induced a boiling zone characterized by a
   temperature of ca. 300 degreesC, apparent salinity range of 1.2-4.9
   wt.\% NaCl and a pressure of 85 bar. The upper zone presents a
   ``clathrate CO2 cap{''} with a temperature range of 120-150 degreesC.
   This steam-dominated zone is principally composed of CO2. (C) 2003
   Elsevier Science B.V. All rights reserved.}},
