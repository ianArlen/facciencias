@article{ ISI:000183589900005,
Author = {Escobedo, LZ and Estrella, AP and Manriquez, VC},
Title = {{Variables affecting expositive text reading in fourth grade elementary
   school children}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{1}},
Pages = {{59-73}},
Month = {{JUN}},
Abstract = {{Four success criteria of reading comprehension were investigated in a
   group of 26 fourth grade elementary school children. Performance tasks
   included detecting one of four defects: 1. False statements. 2. Unknown
   words. 3. Inconsistent propositions. 4. Insufficient text information.
   The texts had three different sentence length, and a total of 223
   expositive texts were designed. 70\% had one experimental defect
   according to the above criteria. The easiest criteria to cope were:
   Identify unknown words and detect information against his/her usual
   knowledge; the hardest included detecting insufficient information. Text
   length always decreased performance, especially in detecting
   inconsistent propositions and insufficient information. The results are
   discussed in terms Of Support to theoretical premises and the
   educational concerns about text comprehension.}},
