@article{ ISI:000183211200058,
Author = {Muller, A and Phaneuf, RA and Aguilar, A and Gharaibeh, MF and
   Schlachter, AS and Alvarez, I and Cisneros, C and Hinojosa, G and
   McLaughlin, BM},
Title = {{Photoionization of C2+ ions}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2003}},
Volume = {{205}},
Pages = {{301-305}},
Month = {{MAY}},
Note = {{11th International Conference on the Physics of Highly Charged Ions (HCI
   2002), CAEN, FRANCE, SEP 01-06, 2002}},
Abstract = {{We have investigated valence-shell photoionization (PI) of S-1 ground
   state and P-3(o) metastable states of C2+ ions. Absolute PI
   cross-sections were measured in the range 40.7-56.4 eV with an energy
   spread of 30 meV using a photon-ion merged beams arrangement at the
   Advanced Light Source (ALS). Detailed calculations based on the
   semi-relativistic Breit-Pauli R-matrix approach suggest a fraction of
   40\% of metastable ions in the primary beam of the experiment.
   Additional high resolution measurements have been carried out with an
   energy spread as low as 7.5 meV. The new data permit a sensitive test of
   theory with respect to the individual PI cross-section contributions
   originating from the different P-3(0.1.2) fine-structure components of
   the initial triplet states. (C) 2003 Elsevier Science B.V. All rights
   reserved.}},
