@article{ ISI:000183589900004,
Author = {Rangel, ALGC and Sanchez-Sosa, JJ},
Title = {{Effects of a cognitive-behavioral intervention on the quality of life of
   older adults}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{1}},
Pages = {{43-58}},
Month = {{JUN}},
Abstract = {{The present Study examined the effects of an intervention on the QOL of
   52 lower middle class older adults. A first study tested the viability
   of several scales and a behavioral recording procedure. A second study
   analyzed the effects of an intervention on participants aged 55 to 74,
   through an experimental (n = 27) and a waiting list condition (n = 25).
   Measurements included scales on Subjective well-being, depression,
   self-efficacy, QOL, and neuropsychological functioning. The intervention
   included: establishing new behaviors, providing information, and
   cognitive restructuring. Results revealed highly acceptable validity and
   reliability features of scales and procedures as well as statistically
   and clinically significant improvement in most measures for the vast
   majority of participants.}},
