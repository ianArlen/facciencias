@article{ ISI:000184126900007,
Author = {Acevedo, E and Aguilar, M},
Title = {{Early warning system for exchange rate crises in Mexico}},
Journal = {{TRIMESTRE ECONOMICO}},
Year = {{2003}},
Volume = {{70}},
Number = {{279}},
Pages = {{613-634}},
Month = {{JUL-SEP}},
Abstract = {{High economic costs associated with recent currency crises have
   motivated the creation of several early warning systems (EWS) to prevent
   their occurrence. However, to date the structure of the EWS does not
   detect with enough implementation of economic policy corrections. The
   warning index proposed in this paper corrects this flaw, given that it
   detects economic fragilities at least nine months in advance. The
   pattern of the index for the Mexican economy is compared with those of
   the EWS proposed by Sachs, Tornell y Velasco; Kaminsky, Lizondo y
   Reinhart; and, Esquivel y Larrain, in order to evaluate its
   effectiveness.}},
