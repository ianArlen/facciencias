@article{ ISI:000183985300004,
Author = {Zepeda, A and Vaca, L and Arias, T and Sengpiel, F},
Title = {{Reorganization of visual cortical maps after focal ischemic lesions}},
Journal = {{JOURNAL OF CEREBRAL BLOOD FLOW AND METABOLISM}},
