@article{ ISI:000183459600007,
Author = {Sanchez-Blazquez, P and Gorgas, J and Cardiel, N and Cenarro, J and
   Gonzalez, JJ},
Title = {{Differences in carbon and nitrogen abundances between field and cluster
   early-type galaxies}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2003}},
Volume = {{590}},
Number = {{2, 2}},
Pages = {{L91-L94}},
Month = {{JUN 20}},
Abstract = {{Central line-strength indices were measured in the blue spectral region
   for a sample of 98 early-type galaxies in different environments. For
   most indices (Mg b and {[}Fe] in particular), elliptical galaxies in
   rich clusters and in low-density regions follow the same index-sigma
   relations. However, striking spectral differences between field
   elliptical galaxies and their counterparts in the central region of the
   Coma Cluster are found for the first time, with galaxies in the denser
   environment showing significantly lower C4668 and CN2 absorption
   strengths. The most convincing interpretation of these results is that
   they represent a difference in abundance ratios arising from distinct
   star formation and chemical-enrichment histories of galaxies in
   different environments. A scenario in which elliptical galaxies in
   clusters are fully assembled at earlier stages than their low-density
   counterparts is discussed.}},
