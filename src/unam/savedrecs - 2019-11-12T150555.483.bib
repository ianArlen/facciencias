@article{ ISI:000183589900003,
Author = {de Jesus, AI},
Title = {{Affective neuroscience. Current views in the psychophysiology of emotion}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{1}},
Pages = {{29-41}},
Month = {{JUN}},
Abstract = {{The considerable increase of interest on the psychophysiological
   processes related with emotional behavior has produced an emerging
   discipline: Affective Neuroscience. Even though emotional processes can
   hardly be separated from other cognitive processes, some related brain
   structures are in fact independent. The present work reviews recent
   findings on emotional brain processes as well as appetitive and aversive
   emotional systems. The analysis includes psychophysiological responses
   built during the processing of emotional stimuli and individual
   differences on affective style. It is proposed that, although the study
   of emotion and subjective emotional feelings should be done separately,
   these components, however, remain related and influence human behavior.}},
