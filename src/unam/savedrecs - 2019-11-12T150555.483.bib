@article{ ISI:000183139200002,
Author = {Espinosa, MA and De la Torre, GC and Porcayo-Calderon, J and
   Martinez-Villafane, A and Chacon-Nava, JG and Casales, M and
   Gonzalez-Rodriguez, JG},
Title = {{Corrosion of atomized Fe40Al based intermetallics in molten Na2SO4}},
Journal = {{MATERIALS AND CORROSION-WERKSTOFFE UND KORROSION}},
Year = {{2003}},
Volume = {{54}},
Number = {{5}},
Pages = {{304-310}},
Month = {{MAY}},
Abstract = {{The hot corrosion resistance of sprayed and atomized Fe-40at.\%Al,
   Fe40Al+0.1B and Fe40Al+0.1 B+ 10Al(2)O(3) intermetallic materials have
   been evaluated in molten Na2SO4 at 900 and 1000 degreesC using
   polarization curves and polarization resistance measurements. The
   results are supported by electron microscopy and microchemical studies.
   The tests lasted 5 days. At 900 degreesC the Fe40Al material had the
   lowest corrosion rate (0.03 mA/cm(2)), and the Fe40Al+0.1B+10Al(2)O(3)
   exhibited the highest. At 1000degreesC, the Fe40Al+0.1 B material, was
   the material that had the best corrosion resistance with less than 0.02
   mA/cm(2) in the first 50 hours, whereas the Fe40Al presented the worst
   corrosion resistance with 0.20 mA/cm(2). The results are discussed in
   terms of the establishment of an Al2O3 layer that gives corrosion
   resistance to the materials and promotes an Al depletion in the FeAl
   matrix which allows the sulfides formation.}},
