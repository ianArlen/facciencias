@article{ ISI:000183905500029,
Author = {Riera, A and Raga, AC and Reipurth, B and Amram, P and Boulesteix, J and
   Canto, J and Toledano, O},
Title = {{Fabry-Perot observations of the HH 110 jet}},
Journal = {{ASTRONOMICAL JOURNAL}},
Year = {{2003}},
Volume = {{126}},
Number = {{1}},
Pages = {{327-338}},
Month = {{JUL}},
Abstract = {{We have obtained an Halpha position-velocity cube from Fabry-Perot
   interferometric observations of the HH 110 flow. We analyze the results
   in terms of anisotropic wavelet transforms, from which we derive the
   spatial distribution of the knots, as well as their characteristic sizes
   ( along and across the outflow axis). We then study the spatial behavior
   of the line width and the central radial velocity. The results are
   interpreted in terms of a simple ``mean flow plus turbulent eddy
   `'jet/wake model. We find that most of the observed kinematics appears
   to be a direct result of the mean flow, on which are superposed
   low-amplitude (similar to35 km s(-1)) turbulent velocities.}},
