@article{ ISI:000183896900019,
Author = {Garcia, EN},
Title = {{History of malacology in Mexico with emphasis on continental malacology}},
Journal = {{REVISTA DE BIOLOGIA TROPICAL}},
Year = {{2003}},
Volume = {{51}},
Number = {{3}},
Pages = {{463-471}},
Month = {{JUN}},
Abstract = {{Late 20th Century malacology in Mexico represents the fusion of two
   lines of action from the previous century: natural history museums
   established in the 18th Century and field collecting that began in the
   16th Century. Museum activity suffered from political influence, mainly
   in the 19th century. Field work by expeditions from Europe and the USA
   were important around the 19th century (e.g. the large works by Fischer
   and Crosse, 1870-1902; von Martens, 1890-1901; Strebel, 1873-1882 and
   Diaz de Leon, 1912), but continental mollusc research done by Mexicans
   was very sporadic (specially in the 19th Century, less in the 20s and
   50s of the 20th century) until 1980.}},
