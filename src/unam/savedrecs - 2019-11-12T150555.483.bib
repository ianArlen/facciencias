@article{ ISI:000184687800025,
Author = {Garzon, IL and Beltran, MR and Gonzalez, G and Gutierrez-Gonzalez, I and
   Michaelian, K and Reyes-Nava, JA and Rodriguez-Hernandez, JI},
Title = {{Chirality, defects, and disorder in gold clusters}},
