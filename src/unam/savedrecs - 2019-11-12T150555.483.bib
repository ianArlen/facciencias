@article{ ISI:000183183500018,
Author = {Lemus, R and Frank, A},
Title = {{A realization of the dynamical group for the square-well potential and
   its coherent states}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{2003}},
Volume = {{36}},
Number = {{17}},
Pages = {{4901-4910}},
Month = {{MAY 2}},
Abstract = {{A realization of the ladder operators for the infinitely deep well
   potential is presented. Both the asymmetric and symmetric wells are
   discussed. It is shown that these operators satisfy the commutation
   relations for the SU(1, 1) group. Closed analytical expressions for the
   matrix elements of some relevant functions are obtained. Perelomov
   coherent states for this system are explicitly constructed. The
   behaviour of the dispersion relation and the time evolution for such
   states are examined.}},
