@article{ ISI:000183589900009,
Author = {Corona, DM and Izquierdo, MAL},
Title = {{A Mexican masculinity-feminity inventory: Psychometrics and development
   of a preliminary version}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2003}},
Volume = {{20}},
Number = {{1}},
Pages = {{113-126}},
Month = {{JUN}},
Abstract = {{The present study included the development of the preliminary version of
   an inventory designed to measure masculinity and feminity. Attributes
   were first identified through an initial sample of 665 participants by
   using a list of adjectives. Participants included women, heterosexual
   males and homosexual males. Participants were asked to make two
   responses, one requesting a self-description (self-image) and one
   describing their ideal couple partner. Adjectives with the highest
   discrimination properties and high internal consistency were later
   subject to factorial analysis. A preliminary version of the inventory
   containing masculinity and feminity subscales with good internal
   consistency resulted. Some findings related to sexual roles are
   specifically discussed.}},
