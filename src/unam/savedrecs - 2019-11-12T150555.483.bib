@article{ ISI:000182582800098,
Author = {Levresse, G and Gonzalez-Partida, E and Tritlla, J and Camprubi, A and
   Cienfuegos-Alvarado, E and Morales-Puente, P},
Title = {{Fluid characteristics of the world-class, carbonate-hosted Las Cuevas
   fluorite deposit (San Luis Potosi, Mexico)}},
