@article{ ISI:000183473600008,
Author = {Aladro-Lubel, MA and Martinez-Murillo, ME},
Title = {{Metacystis borrori n. sp (Ciliophora : Metacystidae) on the Seagrass
   Thalassia testudinum}},
Journal = {{JOURNAL OF EUKARYOTIC MICROBIOLOGY}},
Year = {{2003}},
Volume = {{50}},
Number = {{3}},
Pages = {{204-209}},
Month = {{MAY-JUN}},
Abstract = {{A new epibiontic ciliate of the genus Metacystis is described on the
   seagrass Thalassia testudinum of the coral reef lagoons of Veracruz,
   Mexico. The ciliate was studied in living and stained specimens and
   under the scanning electron microscope. The cell body (10-35 X 10-18 mum
   in vivo) is transversely annulated (4-6 rings). The somatic ciliature
   consists of 22-30 longitudinal kineties, and patterned as 5-7 transverse
   kinetics. The circumoral kinety is composed of kinetosomes closely
   spaced. The macronucleus diameter measures about 3-7 mum. The lorica
   (18-61 X 11-26 mum) has the posterior end round to conical or irregular
   with mucoid filaments. This prostomatid colonizes both natural and
   artificial substrates placed in an aquarium. Metacystis borrori n. sp.
   is a species that forms part of the ciliate community on Thalassia
   testudinum with a temperature range of 21-26 degreesC and a salinity of
   32-40parts per thousand.}},
