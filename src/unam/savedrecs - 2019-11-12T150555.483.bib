@article{ ISI:000183417300008,
Author = {Kubo, J and Mondragon, A and Mondragon, M and Rodriguez-Jauregui, E},
Title = {{The flavor symmetry}},
Journal = {{PROGRESS OF THEORETICAL PHYSICS}},
Year = {{2003}},
Volume = {{109}},
Number = {{5}},
Pages = {{795-807}},
Month = {{MAY}},
Abstract = {{Assuming that the lepton, quark and Higgs fields belong to the
   three-dimensional reducible representation of the permutation group S-3,
   we suggest a minimal S-3 invariant extension of the standard model. We
   find that in the leptonic sector, the exact S-3 x Z(2) symmetry, which
   allows 6 real independent parameters, is consistent with experimental
   data and predicts the bi-maximal mixing of the left-handed neutrinos and
   that the third neutrino is the lightest neutrino. Z(2) is anomaly-free,
   but it forbids CP-violations in the leptonic, as well as in the hadronic
   sector. Therefore, the origin of CP-violations can be identified with
   the breaking of the Z(2) symmetry, which may be understood in a more
   fundamental theory. With the exact S-3 only, there are 10 real
   independent parameters and one independent phase, on which the
   Cabibbo-Kobayashi-Maskawa mixing matrix V-CKM depends. A set of values
   of these parameters that are consistent with experimental observations
   is given.}},
