@article{ ISI:000261654100044,
Author = {Morales, J. and Apatiga, L. M. and Castano, V. M.},
Title = {{Synthesis of diamond films from organic compounds by Pulsed Liquid
   Injection CVD}},
Journal = {{SURFACE \& COATINGS TECHNOLOGY}},
Year = {{2008}},
Volume = {{203}},
