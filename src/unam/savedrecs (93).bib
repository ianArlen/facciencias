Author = {Damian-Almazo, Juanita Y. and Lopez-Munguia, Agustin and
   Soberon-Mainero, Xavier and Saab-Rincon, Gloria},
Title = {{Role of the phenylalanine 260 residue in defining product profile and
   alcoholytic activity of the alpha-amylase AmyA from Thermotoga maritima}},
Journal = {{BIOLOGIA}},
Year = {{2008}},
Volume = {{63}},
Number = {{6}},
Pages = {{1035-1043}},
