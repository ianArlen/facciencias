@inproceedings{ ISI:000271630600025,
Author = {Pena, M. and Hernandez-Martinez, L.},
Editor = {{Magris, G and Bruzual, G and Carigi, L}},
Title = {{AN INTERESTING COMPARISON BETWEEN PNE AND H II REGIONS IN NGC 3109 AND
   NGC 6822}},
