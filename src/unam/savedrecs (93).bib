@article{ ISI:000260495400007,
Author = {Ferreira, Claudia Pio and Yang, Hyun Mo and Esteva, Lourdes},
Title = {{ASSESSING THE SUITABILITY OF STERILE INSECT TECHNIQUE APPLIED TO AEDES
   AEGYPTI}},
Journal = {{JOURNAL OF BIOLOGICAL SYSTEMS}},
Year = {{2008}},
Volume = {{16}},
Number = {{4}},
Pages = {{565-577}},
Month = {{DEC}},
Abstract = {{The efficacy of biological control of Aedes aegypti mosquitoes using
   Sterile Insect Technique (SIT) is analyzed. This approach has shown to
   be very efficient on agricultural plagues and has become an alternative
   control strategy to the usual technique of insecticide application,
   which promotes resistance against chemical controls and is harmful to
   other species that live in the same mosquito habitat. By using a
   discrete cellular automata approach we have shown that in the case of
   Aedes aegypti, the spatially heterogeneous distribution of oviposition
   containers and the mosquito behavior, especially with respect to mating,
   make the application of STI difficult or impracticable.}},
