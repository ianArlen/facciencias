@article{ ISI:000207533100013,
Author = {Waldman M., Gilda},
Title = {{THE TWO MODALITIES OF CURRENT LITERARY CHRONICLES ON THE CITY OF
   SANTIAGO A HISTORY OF THE PRESENT AND POSTCARDS IN SEPIA}},
Journal = {{ANALES DE LITERATURA CHILENA}},
Year = {{2008}},
Volume = {{10}},
Number = {{9}},
Pages = {{179-190}},
Month = {{DEC}},
Abstract = {{If throughout 20th century Chilean literature the city of Santiago had
   been the object of representation mainly in the genre of narrative
   fiction. today this junction has been taken over by the literary
   chronicle This is due in large part to the accuracy. urgency and
   immediacy that allows this genre to interpret the ways of living,
   feeling and thinking the different contemporary urban spaces and their
   cultural dynamics The actuality of the chronicle responds to a double
   Imperative First, to map the present urban lifestyles in order to
   contrast them with the idyllic vision that turns Chile into ``the
   felicitous image of Eden'' Secondly as an exercise in memory which aims
   at reconstructing a city that has been made to disappear not only by
   natural disasters, but also by government policies that have modernized
   Santiago while setting aside no tune to mourn the lost urban and
   cultural patrimony}},
Publisher = {{PONTIFICIA UNIV CATOLICA CHILE, CENTRO ESTUD LIT CHILENA}},
Address = {{FAC LETRAS, CAMPUS SAN JOAQUIN, AVDA VICUNA MACKENNA 4860, SANTIAGO DE
   CHILE, CP782-0436, CHILE}},
