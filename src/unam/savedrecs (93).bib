@article{ ISI:000263653700021,
Author = {Acebey, Amparo and Kroemer, Thorsten},
Title = {{Diversity and distribution of Araceae of the Reserva de la Biosfera Los
   Tuxtlas, Veracruz, Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2008}},
Volume = {{79}},
Number = {{2}},
Pages = {{465-471}},
Month = {{DEC}},
Abstract = {{This study documents the diversity, altitudinal and geographical
   distribution of Araceae in the region of Los Tuxtlas, Veracruz. An
   altitudinal transect was selected in mature tropical rain and cloud
   forest from 100 in elevation to the summit of the San Martin Tuxtla
   volcano at 1 700 m. In each range of 100 m elevation we carried out
   inventories of Araceae in 3-4 plots of 400 m(2). The list of aroids
   presented for the region of the Reserva de la Biosfera Los Tuxtlas is
   based on our own data, supplemented With information from published
   sources, data bases and revision of herbarium specimens. We recorded it
   total of 34 species in 9 genera, of which 5 are endemic to Mexico. The
   (hemi-) epiphytic aroids represent about 11\% of the epiphytic flora at
   the Biological Research Station Los Tuxtlas. A constant decline of the
   number of species was observed with increasing elevation along the
   altitudinal gradient. The discovery of 3 new records of aroid species
   for the region of Los Tuxtlas and the state of Veracruz shows that it
   would be important to carry out more floristic inventories.}},
