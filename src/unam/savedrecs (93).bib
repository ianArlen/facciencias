@article{ ISI:000261232200027,
Author = {Peimbert, Manuel},
Title = {{The primordial helium abundance}},
Journal = {{CURRENT SCIENCE}},
Year = {{2008}},
Volume = {{95}},
Number = {{9}},
Pages = {{1165-1176}},
Month = {{NOV 10}},
Abstract = {{present a brief review on the determination of the primordial helium
   abundance by unit mass, YP. I discuss the importance of the primordial
   helium abundance in: (a) cosmology, (b) testing the standard big bang
   nucleosynthesis, (c) studying the physical conditions in H II regions,
   (d) providing the initial conditions for stellar evolution models, and
