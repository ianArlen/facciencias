@article{ ISI:000261969100018,
Author = {Escobar Briones, Elva and Estrada Santillan, Erika Laura and Legendre,
   Pierre},
Title = {{Macrofaunal density and biomass in the Campeche Canyon, Southwestern
   Gulf of Mexico}},
