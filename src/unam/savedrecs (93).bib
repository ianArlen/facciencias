@article{ ISI:000263067600002,
Author = {Jalbout, Abraham F.},
Title = {{Prebiotic Synthesis of Simple Sugars by an Interstellar Formose Reaction}},
Journal = {{ORIGINS OF LIFE AND EVOLUTION OF BIOSPHERES}},
Year = {{2008}},
Volume = {{38}},
Number = {{6}},
Pages = {{489-497}},
Month = {{DEC}},
Abstract = {{The prebiotic possibilities for the synthesis of interstellar
   carbohydrates through a protic variant of the formose reaction under gas
   phase conditions were studied. Ab initio calculations were used to
