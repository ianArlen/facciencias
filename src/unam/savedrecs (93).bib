@article{ ISI:000261553500012,
Author = {Aguilar-Setien, A. and Romero-Almaraz, M. L. and Sanchez-Hernandez, C.
   and Figueroa, R. and Juarez-Palma, L. P. and Garcia-Flores, M. M. and
   Vazquez-Salinas, C. and Salas-Rojas, M. and Hidalgo-Martinez, A. C. and
   Pierle, S. Aguilar and Garcia-Estrada, C. and Ramos, C.},
Title = {{Dengue virus in Mexican bats}},
Journal = {{EPIDEMIOLOGY AND INFECTION}},
Year = {{2008}},
Volume = {{136}},
Number = {{12}},
Pages = {{1678-1683}},
Month = {{DEC}},
Abstract = {{Individuals belonging to five families, 12 genera, and 19 different
   species of bats from dengue endemic areas in the Gulf and Pacific coasts
   of Mexico were examined by ELISA, RT-PCR, and for the presence of dengue
   virus (DV) NSI protein. Nine individuals from four species were
   seropositive by ELISA: three insectivorous, Myotis nigricans (four
   positives/12 examined), Pteronotus parnellii (3/19), and Natalus
   stramineus (1/4), and one frugivorous Artibeus jamaicensis (1/35)
   (12.86\% seroprevalence in positive species). DV serotype 2 was detected
   by RT-PCR in four samples from three species (all from the Gulf coast -
   rainy season): two frugivorous, A. jamaicensis (2/9). and Carollia
   brevicauda (1/2), and one insectivorous, M. nigricans (1/11). The latter
   was simultaneously positive for NSI protein. DV RT-PCR positive animals
   were all antibody seronegative. M. nigricans showed positive individuals
   for all three tests. This is the first evidence suggesting the presence
   of DV in bats from Mexico.}},
