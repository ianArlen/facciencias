@article{ ISI:000263653700001,
Author = {Raymundo, Tania and Valenzuela, Ricardo and Cifuentes, Joaquin},
Title = {{Two new species of the genus Phellinus ((Hymenochaetales, Basidiomycota)
   from Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2008}},
Volume = {{79}},
Number = {{2}},
Pages = {{295-301}},
Month = {{DEC}},
Abstract = {{The macroscopic and microscopic morphological characters of 2 resupinate
   species of the genus Phellinus collected in tropical vegetation from
   Mexico are described for the first time, Phellinus guzmanii and
   Phellinus herrerae. The first species was collected from Sierra Gorda
   Biosphere Reserve in Queretaro State and has subglobose to ellipsoid and
   yellowish spores and hyphal setae on the margin of the basidiomata; the
   latter species was collected in the Calakmul Biosphere Reserve in
   Campeche State and is characterized by its large oblong-ellipsoid golden
   yellow to yellowish brown spores. The studied specimens were deposited
   in the ENCB herbarium with duplicates in FCME, MEXU, and XAL.}},
Publisher = {{INST BIOLOGIA, UNIV NACIONAL AUTONOMA MEXICO}},
