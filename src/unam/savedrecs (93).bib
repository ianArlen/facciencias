@inproceedings{ ISI:000292151700004,
Author = {Aguilar-Velazquez, G. and Santacruz-Valdes, C. and Robles-Contreras, A.
   and Ayala, Balboa J. and Baulista-de-Lucio, V and Chavez, R. and
   Estrada-Parra, S. and Garfias, Y. and Perez-Tapla, M. and
   Jimenez-Martinez, M. C.},
Editor = {{Canonica, GW and BaenaCagnani, CE}},
Title = {{Tear TNF as a Potential Link in the Pathogenesis of Allergic
   Conjunctivitis, Correlation Between Tear IFNg/IL-5 and Clinical Data}},
Booktitle = {{WORLD ALLERGY CONGRESS (WAC)}},
Year = {{2009}},
Pages = {{19+}},
Note = {{World Allergy Congress (WAC), Buenos Aires, ARGENTINA, DEC 06-10, 2009}},
Abstract = {{Allergic conjunctivitis (AC) is initated by Th2 immune polarization
   resulting in generation of IgF, antibodies against environmental agents.
   Recent evidence suggests that surface inflammation is sustained by IFN-g
   migrating cells; nevertheless other cytokines could be involved in
   conjunctival inflammation. The aims of this study were to determine
   Th1/Th2 tear-cytokines (TC) and to correlate immunological findings with
   clinical data. TC were determined by cytometric bead arrays in 36
   AC-Patients. We observed a significant higher concentration of IFN-g
   when was compared with IL-4 (p 0.01) and IL-5(p<0.0001). Despite IL-10
   concentration was significantly increased when was compared with TNF (p
   0.04). Only TNF positively correlated with IFN-g (p=0.04) and IL-5
   (p=0.03). When we associated clinical data and TC, we observed a
   positive correlation with IFN-g (p=0.01) and IL-5 (p=0.04). Our results
   suggest that TNF could act as a potential proinflammatory mediator in
   AC, synergizing the activities of IFN-g and IL-5 at the ocular surface.}},
