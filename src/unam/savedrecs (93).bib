@article{ ISI:000263653700002,
Author = {Lozada-Perez, Lucio},
Title = {{A new species of Sloanea (Elaeocarpaceae) from Guerrero and Chiapas,
   Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2008}},
Volume = {{79}},
Number = {{2}},
Pages = {{303-306}},
Month = {{DEC}},
Abstract = {{Sloanea ahuatoso n. sp., is described from material collected in the
   mountain range of Atoyac, Guerrero, and the Reserva de la Biosfera El
   Triunfo, Chiapas, Mexico. This species is characterized by its simple
   dichasial inflorescence, closed flower buds, fleshy sepals with dense
   tomentum abaxially, ellipsoidal capsules, and ellipsoidal seeds
   completely covered with an orange-colored aril. It is included in
   subgenus Quadrisepala for the inflorescence type and the closed flower
   buds. Its relationship with other species of the subgenus is discussed.}},
