@article{ ISI:000261908000001,
Author = {Van Calsteren, Marie-Rose and Jankowski, Christopher K. and
   Reyes-Chilpa, Ricardo and Jimenez-Estrada, Manuel and Campos, Maria G.
   and Zarazua-Lozada, Abraham and Oropeza, Martha and Lesage, Denis},
Title = {{X-ray and high-resolution H-1 and C-13 NMR of smooth muscle relaxant
   sesquiterpene lactones}},
Journal = {{CANADIAN JOURNAL OF CHEMISTRY-REVUE CANADIENNE DE CHIMIE}},
Year = {{2008}},
Volume = {{86}},
Number = {{12}},
Pages = {{1077-1084}},
Month = {{DEC}},
Abstract = {{The structure and stereochemistry of four sesquiterpene lactones,
   budlein A (1), zaluzanin A (2), and glaucolides D (3a) and E (3b),
   isolated from Mexican Asteraceae species, for which only partial NMR
   data could be found in the literature, were determined. A combination of
   1D and 2D high-resolution NMR experiments, such as DEPT, COSY, NOESY,
   ROESY, HMQC, HSQC, and HMBC, were used to completely assign the H-1 and
   C-13 spectra. The crystal structures of zaluzanin A (2) and glaucolide E
   (3b) were also determined. Glaucolides D and E have been previously
   reported to relax KCl-induced contraction in rat uterine smooth muscle;
   therefore, the effects of zaluzanin A and budlein A were examined in the
   same model. It was found that both compounds can relax contraction
   induced by KCl, but only zaluzanin A induced relaxation when contraction
   was induced with oxytocin. The preliminary biological test results
   according to these profiles are reported in this paper.}},
