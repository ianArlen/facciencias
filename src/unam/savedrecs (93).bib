@inproceedings{ ISI:000271630600016,
Author = {Calvet, N. and Espaillat, C. and Hernandez, J. and Muzerolle, J. and
   Briceno, C. and D'Alessio, P. and Hartmann, L. and Watson, D.},
Editor = {{Magris, G and Bruzual, G and Carigi, L}},
Title = {{CVS0 224: A TRANSITIONAL DISK IN THE 10 MYR 25 ORI AGGREGATE}},
