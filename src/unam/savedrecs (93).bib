@article{ ISI:000261521100007,
Author = {Dugas, Alex S. and Martinez-Villa, Roberto},
Title = {{Stable equivalences of graded algebras}},
Journal = {{JOURNAL OF ALGEBRA}},
Year = {{2008}},
Volume = {{320}},
Number = {{12}},
Pages = {{4215-4241}},
Month = {{DEC 15}},
Abstract = {{We extend the notion of stable equivalence to the class of locally
   finite graded algebras. For such all algebra Lambda, we focus on the
   Krull-Schmidt category gr(Lambda) of finitely generated Z-graded
   Lambda-modules with degree 0 maps, and the stable category gr(Lambda)
   obtained by factoring out those maps that factor through a graded
   projective module. We say that Lambda and Gamma are graded stably
   equivalent if there is an equivalence alpha : gr(Lambda) -> gr(Gamma)
   that commutes with the grading shift. Adapting arguments of Auslander
   and Reiten involving functor categories, we show that a graded stable
   equivalence of commutes with the syzygy operator (where defined) and
   preserves finitely presented Modules. As a result, we see that if Lambda
   is right noetherian (resp. right graded coherent), then so is any graded
   stably equivalent algebra. Furthermore, if Lambda is right noetherian or
   k is artinian, we use almost split sequences to show that a graded
   stable equivalence preserves finite length modules. Of particular
   interest in the nonartinian case, we prove that any graded stable
   equivalence involving an algebra Lambda with soc Lambda = 0 must be a
   graded Morita equivalence. (C) 2008 Elsevier Inc. All rights reserved.}},
