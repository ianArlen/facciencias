@article{ ISI:000261630700021,
Author = {Wang, Chumin and Salazar, Fernando and Sanchez, Vicenta},
Title = {{Renormalization Plus Convolution Method for Atomic-Scale Modeling of
   Electrical and Thermal Transport in Nanowires}},
