@article{ ISI:000261848600049,
Author = {Armentano, Ilaria and Antonio Alvarez-Perez, Marco and
   Carmona-Rodriguez, Bruno and Gutierrez-Ospina, Ivan and Kenny, Jose
   Maria and Arzate, Higinio},
Title = {{Analysis of the biomineralization process on SWNT-COOH and F-SWNT films}},
Journal = {{MATERIALS SCIENCE \& ENGINEERING C-BIOMIMETIC AND SUPRAMOLECULAR SYSTEMS}},
Year = {{2008}},
Volume = {{28}},
Number = {{8}},
Pages = {{1522-1529}},
Month = {{DEC 1}},
Abstract = {{In vitro biomineralization process was investigated on functionalized
   single wall nanotube (SWNT) films. The films were prepared by solvent
   casting method by using carboxylated and fluorinated nanotubes. SWNT
   films were characterized by means of electron microscopy. contact angle
   measurements and optical absorption. The in vitro assays were performed
   On Cultured human alveolar bone-derived cells (HABDC) to determine the
   capabilities of carboxylated single-walled nanotubes (SWNTs-COOH) and
   fluorinated single-walled nanotubes (F-SWNTs) to promote the deposit of
   mineral-like tissue. The results showed that the Cellular response of
   HABDC in secreting a mineralized extracellular matrix and their
   consequent mineralization is dependent on the degree of
   functionalization of the SWNTs. Differences were found related to the
   kind of sidewall functionalization. Both structures promoted
   hydroxyapatite formation, however, calcium uptake on SWNTs-COOH
   increased and it was related to crystal density. From our results, it is
   possible to infer that CNT functionalization opens a path to future
   developments in new bone graft materials and techniques. (c) 2008
   Elsevier B.V. All rights reserved.}},
