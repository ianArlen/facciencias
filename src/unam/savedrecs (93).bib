@article{ ISI:000261996500011,
Author = {Friedlander, John B. and Luca, Florian},
Title = {{Residue classes having tardy totients}},
Journal = {{BULLETIN OF THE LONDON MATHEMATICAL SOCIETY}},
Year = {{2008}},
Volume = {{40}},
Pages = {{1007-1016}},
Month = {{DEC}},
Abstract = {{It is shown, in an effective way, that there exists a sequence of
   congruence classes a(k) (mod m(k)) such that the minimal solution n=n(k)
   of the congruence phi(n)equivalent to a(k) (mod m(k)) exists and that it
   satisfies log n(k)/log m(k)->infinity as k ->infinity. Here, phi(n) is
   the Euler function. This answers a question raised by the first author
   and Shparlinski (Bull. London Math. Soc. 39 (2007) 425-432; Bull. London
   Math. Soc. 40 (2008) 532). It is also shown that every congruence class
   a(k) (mod m(k)) containing an even integer contains infinitely many
   values of the Carmichael function lambda(n) and the least such n
   satisfies n < m(82.5).}},
