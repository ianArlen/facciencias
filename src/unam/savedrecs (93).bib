@article{ ISI:000261015900005,
Author = {Alicia, Sampieri and Angelica, Zepeda and Carlos, Saldana and Alfonso,
   Salgado and Vaca, Luis},
Title = {{STIM1 converts TRPC1 from a receptor-operated to a store-operated
   channel: Moving TRPC1 in and out of lipid rafts}},
Journal = {{CELL CALCIUM}},
Year = {{2008}},
Volume = {{44}},
Number = {{5}},
Pages = {{479-491}},
Month = {{NOV}},
Abstract = {{White the role of members from the TRPC family of channels as
   receptor-operated channels (ROC) is well established and supported by
   numerous studies, the role of this family of channels as store-operated
   channels (SOC) has been the focus of a heated controversy over the last
   few years. In the present study, we have explored the modulation of
   STIM1 on human TRPC1 channel. We show that the association of STIM1 to
   TRPC1 favors the insertion of TRPC1 into lipid rafts, where TRPC1
   functions as a SOC. In the absence of STIM1, TRPC1 associates to other
   members from the TRPC family of channels to form ROCs. A novel
   TIRFM-FRET method illustrates the relevance of the dynamic association
   between STIM1 and TRPC1 for the activation of SOC and the lipid raft
   localization of the STIM1-TRPC1 complex. This study provides new
   evidence about the dual activity of TRPC1 (forming ROC or SOC) and the
   partners needed to determine TRPC1 functional fate. It highlights also
   the role of plasma membrane microdomains and ER-PM junctions in
   modulating TRPC1 channel function and its association to STIM1. 0 2008
   Elsevier Ltd. All rights reserved.}},
