@article{ ISI:000263653700023,
Author = {Dominguez-Dominguez, Omar and Zambrano, Luis and Humberto
   Escalera-Vazquez, Luis and Perez-Rodriguez, Rodolfo and Perez-Ponce de
   Leon, Gerardo},
Title = {{Changes in the distribution of goodeids (Osteichthyes:
   Cyprinodontiformes: Goodeidae) in river basins of Central Mexico}},
Journal = {{REVISTA MEXICANA DE BIODIVERSIDAD}},
Year = {{2008}},
Volume = {{79}},
