@article{ ISI:000171513300010,
Author = {Aranda, E and Mendoza, GD and Garcia-Bojalil, C and Castrejon, F},
Title = {{Growth of heifers grazing stargrass complemented with sugar cane, urea
   and a protein supplement}},
Journal = {{LIVESTOCK PRODUCTION SCIENCE}},
Year = {{2001}},
Volume = {{71}},
Number = {{2-3}},
Pages = {{201-206}},
Month = {{OCT}},
Abstract = {{Sugar cane can be used as a supplement in tropical areas during critical
   periods of forage availability; however, its protein content is limited
   and protein supplementation may be required to improve intake,
   digestibility and daily gain. A grazing trial utilizing 32 individually
   supplemented growing heifers (242 +/- 13 kg initial body weight (BW))
   was conducted to study the effect of sugar cane offered as 3\% of the BW
   with or without urea (1\%) and with a protein supplement (1
   kg/head/day), on the performance of heifers grazing stargrass with a
   stocking density of 6 heifers ha(-1). The protein supplement contained
   (DM basis) blood meal (10\%), poultry litter (50\%), rice polishing
   (25\%) and cane molasses (15\%). Heifers receiving the protein
   supplement had the highest (P < 0.0 1) gain (0.528(a) kg/day) compared
   to the other treatments (control group 0.320(b) kg/day; sugar cane
   without urea 0.327(b) kg/day; sugar cane plus urea 0.366(b) kg/day).
   There were no differences on total DM intake, but stargrass and sugar
