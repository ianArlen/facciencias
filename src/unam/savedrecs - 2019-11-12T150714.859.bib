@article{ ISI:000170157400002,
Author = {Gao, RS and Richard, EC and Popp, PJ and Toon, GC and Hurst, DF and
   Newman, PA and Holecek, JC and Northway, MJ and Fahey, DW and Danilin,
   MY and Sen, B and Aikin, K and Romashkin, PA and Elkins, JW and Webster,
   CR and Schauffler, SM and Greenblatt, JB and McElroy, CT and Lait, LR
   and Bui, TP and Baumgardner, D},
Title = {{Observational evidence for the role of denitrification in Arctic
   stratospheric ozone loss}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2001}},
Volume = {{28}},
Number = {{15}},
Pages = {{2879-2882}},
Month = {{AUG 1}},
Abstract = {{Severe and extensive denitrification, chlorine activation, and
   photochemical ozone loss were observed throughout the lower stratosphere
   in the 1999-2000 Arctic vortex. A large number of air parcels sampled
   between late February and mid-March, 2000, were photochemically
   intercomparable for chemical O-3 loss rates. In these air parcels, the
   temporal evolution of the correlations Of O-3 with the NOy remaining
   after denitrification provides strong evidence for the role of NOy in
   moderating O-3 destruction. In 71\%-denitrified air parcels, a chemical
   O-3 destruction rate of 63 ppbv/day was calculated, while in
   43\%-denitrified air parcels the destruction rate was only 43 ppbv/day.
   These observational results show that representative denitrification
   models will be required for accurate prediction of future Arctic O-3
   changes.}},
