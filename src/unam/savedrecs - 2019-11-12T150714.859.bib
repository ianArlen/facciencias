@article{ ISI:000171955500002,
Author = {Santiago, E and Mora, L and Bautista, M and Montesinos, JJ and Martinez,
   I and Ramos, G and Zambrano, IR and Manrique, B and Weiss-Steider, B},
Title = {{Granulocyte colony-stimulating factor induces neutrophils to secrete
   macrophage colony-stimulating factor}},
Journal = {{CYTOKINE}},
Year = {{2001}},
Volume = {{15}},
Number = {{6}},
Pages = {{299-304}},
Month = {{SEP 21}},
Abstract = {{In this work we provide evidence showing that granulocytes produce
   macrophage colony-stimulating factor (M-CSF) from the band cell stage
   and secrete this factor when induced to differentiate into
   polymorphonuclear cells by recombinant human granulocyte
   colony-stimulating factor (rhG-CSF). Using an enriched population of
   myeloid band cells from murine bone marrow, we identified the presence
   of M-CSF with a chromophore-labelled monoclonal anti-M-CSF antibody.
   Using ELISA we detected the secretion of M-CSF in the supernatants of
   cultures of enriched band cells when induced with rhG-CSF to
   differentiate into mature neutrophils. We also found that M-CSF is the
   only factor responsible for the colony forming activity in the
   supernatants and lysates of band cells treated with rhG-CSF.}},
