@article{ ISI:000170599700008,
Author = {Gonzalez-Soriano, E},
Title = {{An unusual male aggregation in the Odonata: An aerial mating swarm in
   Protoneura cara calvert and notes on other Mexican Protoneuridae
   (Zygoptera)}},
Journal = {{ODONATOLOGICA}},
Year = {{2001}},
Volume = {{30}},
Number = {{3}},
Pages = {{335-340}},
Month = {{SEP 1}},
Abstract = {{On several occasions aggregations were seen hovering high among trees
   beside streams in Mexico. Intermittent arrival of conspecific female
   female at some aggregations and their departure, sometimes in tandem.
   and the apparent absence of foraging within aggregations supports the
   inference that such aggregations are mating swarms. It remains to be
   discovered whether trees over which swarms form are potential
   oviposition sites for this sp., or are places in which both male male
   and female female overnight. Flying aggregations of male male in other
   spp. of Protoneuridae are described and discussed.}},
