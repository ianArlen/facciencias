@article{ ISI:000170480500016,
Author = {German, G and Ross, G and Sarkar, S},
Title = {{Low-scale inflation}},
Journal = {{NUCLEAR PHYSICS B}},
Year = {{2001}},
Volume = {{608}},
Number = {{1-2}},
Pages = {{423-450}},
Month = {{AUG 6}},
Abstract = {{We show that the scale of the inflationary potential may be the
   electroweak scale or even lower, while still generating an acceptable
   spectrum of primordial density perturbations. Thermal effects readily
   lead to the initial conditions necessary for low scale inflation to
   occur, and even the moduli problem can be evaded if there is such an
