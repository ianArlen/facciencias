@article{ ISI:000169565900018,
Author = {Sanchez-Sesma, FJ and Madariaga, R and Irikura, K},
Title = {{An approximate elastic two-dimensional Green's function for a
   constant-gradient medium}},
Journal = {{GEOPHYSICAL JOURNAL INTERNATIONAL}},
Year = {{2001}},
Volume = {{146}},
Number = {{1}},
Pages = {{237-248}},
Month = {{JUL}},
Abstract = {{Approximate analytical formulae for elastic 2-D Green's functions for a
   constant-gradient propagation velocity medium are presented. These
   solutions correspond to unit line forces per unit length: the antiplane
   SH line source and the in-plane P-SV line sources, respectively. They
   are based on the asymptotic ray theory and account for both near-source
   effects and low frequencies. The analytical expressions obtained are
   tested by means of the pseudospectral method with a velocity-stress
   staggered grid. The agreement of the approximate analytical displacement
   and stress fields with their numerical counterparts is generally very
   good. A measure of relative error is proposed.}},
