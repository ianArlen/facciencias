@article{ ISI:000169365700046,
Author = {Nishigaki, T and Zamudio, FZ and Possani, LD and Darszon, A},
Title = {{Time-resolved sperm responses to an egg peptide measured by stopped-flow
   fluorometry}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{2001}},
Volume = {{284}},
Number = {{2}},
Pages = {{531-535}},
Month = {{JUN 8}},
Abstract = {{Speract, a decapeptide from sea urchin egg jelly, induces various sperm
   responses. Stopped-flow fluorometry was used to examine the binding of
   labeled speract and the intracellular changes in pH (pH(i)) and Ca(2+)
   ({[}Ca(2+)](i)) it induces in sperm. We observed significant time delays
   for the increase in pH(i), and {[}Ca(2+)](i) induced by 200 nM speract
   (69 and 190 ms, respectively). Also, we found that the receptor
   undergoes a pH(i)-dependent affinity change at around 129 ms. These time
   delays probably reflect biochemical processes underlying each sperm
   response to speract that circumscribe the time sequence of the signaling
   events. (C) 2001 Academic Press.}},
