@article{ ISI:000170215900016,
Author = {Gu, QS and Huang, JH and de Diego, JA and Dultzin-Hacyan, D and Lei, SJ
   and Benitez, E},
Title = {{The nuclear starburst activity in the Seyfert 2 galaxy NGC 7679}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{2001}},
Volume = {{374}},
Number = {{3}},
Pages = {{932-935}},
Month = {{AUG}},
Abstract = {{We present our recent spectrophotometric results of the infrared
   luminous Seyfert 2 galaxy NGC 7679. We find compelling evidence for the
   presence of a recent nuclear starburst, revealed by the observations of
   a) the spectral features of high order Balmer absorption lines, b) the
   weak equivalent widths for CaIIK lambda 3933, CN lambda 4200, G-band
   lambda 4300 and MgIb lambda 5173, and c) the suggested UV stellar wind
   resonance lines (N V lambda 1240, Si IV lambda 1400 and C IV lambda
   1550) in the IUE spectrum. Using the simple stellar population synthesis
   model, we find that in the nuclear 2{''} x 2{''} region, the
   contributions from the old, intermediate and young components are
   21.7\%, 42.9\% and 35.4\%, respectively. These nuclear starburst
   activities might have been triggered by the close encounter with NGC
   7682, as suggested by recent numerical simulations.}},
