@article{ ISI:000171072300008,
Author = {Xiao, MF},
Title = {{Competing intra- and inter-band transitions in quantum dots as
   manifested in optical absorption spectra}},
Journal = {{PHYSICS LETTERS A}},
Year = {{2001}},
Volume = {{288}},
Number = {{1}},
Pages = {{37-40}},
Month = {{SEP 10}},
Abstract = {{We have theoretically studied the optical absorption spectra of spheric
   gold particles of radius as small as several angstroms to several
   nanometers. In these particles the number of valence electrons ranges
   from about 10 to 10(5). The electrons are so confined that their
   movements under external electromagnetic field have to be treated within
   quantum mechanics. Two apparent transitions, namely intra- and
   inter-band transitions, as well as their relation with the classic
   small-particle resonance omega (p)/root3 (omega (p) is plasmon
   frequency), have been rigorously studied in the framework of the
   electron-in-a-box model with every-electron-count method. A number of
   interesting phenomena are observed, which would also be generally
   applicable to other quantum dot systems. (C) 2001 Elsevier Science B.V.
   All rights reserved.}},
