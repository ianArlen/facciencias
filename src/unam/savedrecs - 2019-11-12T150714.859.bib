@article{ ISI:000170963800003,
Author = {Hernandez-Becerril, DU and Bravo-Sierra, E and Ramirez-Valdez, Y},
Title = {{Coccolithophorids from the west coast of Baja California, Mexico}},
Journal = {{HYDROBIOLOGIA}},
Year = {{2001}},
Volume = {{452}},
Number = {{1-3}},
Pages = {{31-45}},
Month = {{JUN}},
Abstract = {{Studies on coccolithophorids in Mexico are rather scarce, probably due
   to the use of traditional methods for studying marine phytoplankton
   (e.g. collection, preservation and analysis). This is the first study of
   the coccolithophorid flora from west coast of Baja California, Mexico.
   Bottle samples (41) were taken at various depths (5, 25 and 50 m), at
   fixed stations located on perpendicular transects along the west coast
   of Baja California, considering three different zones: North, Central
   and South. A total of 32 samples were studied by optical and scanning
   electron microscopy, yielding 24 taxa identified (17 species and 5
   nominal varieties, plus 2 different varieties). The coccolithophorid
   assemblages are considered subtropical and coastal (with many
   cosmopolitan taxa). Emiliania huxleyi and Gephyrocapsa oceanica were the
   most abundant (relative abundance) and widespread species in the study
   area. We report 15 new records for the Mexican Pacific. The zonal and
   vertical distribution of some taxa are briefly discussed, as well as the
   taxonomy of Rhabdosphaera claviger, Helicosphaera carteri, H. hyalina
   and related taxa.}},
