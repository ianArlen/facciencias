@article{ ISI:000169352700025,
Author = {Raga, A and Sobral, H and Villagran-Muniz, M and Navarro-Gonzalez, R and
   Masciadri, E},
Title = {{A numerical and experimental study of the time-evolution of a low Mach
   number jet}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2001}},
Volume = {{324}},
Number = {{1}},
Pages = {{206-212}},
Month = {{JUN 11}},
Abstract = {{We present a comparison between a plasma-generated `starting jet'
   experiment and an axisymmetric numerical simulation of the flow. The
   experimental flow and the numerical simulation give results that agree
   both qualitatively and quantitatively, showing that the complex vortical
   structures arising in the flow are surprisingly well reproduced by the
   numerical model. This result inspires confidence in the accuracy of
   astrophysical jet numerical simulations. Also, even though the Mach
   number of our laboratory jet is somewhat low (M similar to 0.5), the
   dimensionless parameters of this jet are not very far from those
   expected for Faranoff-Riley class I radio jets.}},
