@article{ ISI:000170325700020,
Author = {Aguilar-Aguilar, R and Moreno-Navarrete, RG and Salgado-Maldonado, G and
   Villa-Ramirez, B},
Title = {{Gastrointestinal helminths of spinner dolphins Stenella longirostris
   (Gray, 1828) (Cetacea : Delphinidae) stranded in La Paz Bay, Baja
   California Sur, Mexico}},
Journal = {{COMPARATIVE PARASITOLOGY}},
Year = {{2001}},
Volume = {{68}},
Number = {{2}},
Pages = {{272-274}},
Month = {{JUL}},
Abstract = {{Thirty-one spinner dolphins Stenella longirostris stranded in La Paz
   Bay, Baja California Sur, Mexico, were examined for endoparasitic
   helminths. The following species were identified: Zalophotrema pacificum
   and Hadwenius tursionis (Digenea); Strobilocephalus triangularis,
   Trigonocotyle sp., and Tetraphyllidea gen. sp. larva (Cestoda); immature
   Bolbosoma hamiltoni (Acanthocephala); and Anisakis typica (Nematoda).
   Except for H. tursionis, all the identified helminths are reported for
   the first time in Mexico. Stenella longirostris represents a new host
   for H. tursionis and A. typica.}},
