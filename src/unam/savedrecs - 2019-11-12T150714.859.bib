@article{ ISI:000169809200031,
Author = {Cuellar-Quintero, JL and Garcia, DE and Cruzblanca, H},
Title = {{The antidepressant imipramine inhibits the M-type K+ current in rat
   sympathetic neurons}},
Journal = {{NEUROREPORT}},
Year = {{2001}},
Volume = {{12}},
Number = {{10}},
Pages = {{2195-2198}},
Month = {{JUL 20}},
Abstract = {{This study aimed to assess the effects of the antidepressant drug
   imipramine (IMI) on the neuronal M-type K+ current (I-K(M)) We show that
   IMI reversibly reduces I-K(M) with an IC50 of 7 muM The V-0.5 and slope
   factor of the steady state activation curve remained unchanged after
   IMI, indicating a mode of action that is voltage insensitive for
   blocking the M-channel. Patch pipette application of IMI elicits same
   inhibitory response suggesting a binding site on the M-channel
   accessible from both sides of the cell membrane. Accordingly, the
   inhibitory effect of IMI is larger by rising external pH near to the
   pK(a) of the drug. Therefore, we propose that a neutral form of IMI
   binds more efficiently to M-channels to exert its inhibitory action by a
   voltage-independent mechanism. NeuroReport 12:2195-2198 (C) 2001
   Lippincott Williams \& Wilkins.}},
