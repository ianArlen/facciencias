@article{ ISI:000171885300004,
Author = {Loske, AM and Prieto, FE},
Title = {{Dual-phase reflectors for extracorporeal shock wave lithotripsy}},
Journal = {{PHYSICA MEDICA-EUROPEAN JOURNAL OF MEDICAL PHYSICS}},
Year = {{2001}},
Volume = {{17}},
Number = {{3}},
Pages = {{141-149}},
Month = {{JUL-SEP}},
Abstract = {{The goal of this study was to describe: the design and performance of
   dual-phase reflectors to be used in electrohydraulic lithotripters in
   order to increase efficiency during extracorporeal shock wave
   lithotripsy (ESWL). These reflectors generate two phase-inverted pulses,
   instead of only one positive peak, followed by a trough, as in
   conventional systems. Pressure measurements and stone fragmentation
