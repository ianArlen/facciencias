@article{ ISI:000170664900005,
Author = {Benammi, M},
Title = {{Discovery of two new species of Myocricetodon genus in the Upper Miocene
   of Ait Kandoula basin (Morocco)}},
Journal = {{COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE II FASCICULE A-SCIENCES
   DE LA TERRE ET DES PLANETES}},
Year = {{2001}},
Volume = {{333}},
Number = {{3}},
Pages = {{187-193}},
Month = {{AUG 15}},
Abstract = {{The Ait Kandoula basin is known for its abundance of micromammal fauna.
   The systematic study of the fauna of two layers of the Afoud section
   (AF6 and AF5), highlights the presence of two new species: Myocricetodon
