@article{ ISI:000172618100008,
Author = {Castaneda, PE and Aparicio, ED and Andrade, LH and Arango, CJJ},
Title = {{Identification and typing of Yersinia enterocolitica biotypes and
   serotypes isolated}},
Journal = {{REVISTA DE SAUDE PUBLICA}},
Year = {{2001}},
Volume = {{35}},
Number = {{4}},
Pages = {{380-384}},
Month = {{AUG}},
Abstract = {{Objective To assess the presence of Yersinia enterocolitica in otherwise
   healthy pigs for human consumption.
   Methods One hundred pharyngeal tonsils were sampled in a slaughterhouse
   in the state of Mexico. The minimum sample size (n = 100) was calculated
   based oil a preliminary sample of 20 cases, which had 20\% positive
   cases. The collected tonsil samples were inoculated in Rappaport broth,
   and Salmonella-Shigella and McConkey media. The biotyping identification
   process was based on biochemical and serological tests using O:3, O:8
   and O:9 antisera.
   Results Twenty-two isolates were obtained. Most were biotype 1 (8 cases
   of O:3 and 8 cases of O:9), but 6 cases could not be serotyped. None of
   the isolates were of O:8 group.
   Conclusions This was the first time that Y. enterocolitica serotypes
   were isolated from pig tonsils in Mexico. Its importance rely on the
   fact that the isolated serotypes are the most commonly found in public
   health problems.}},
