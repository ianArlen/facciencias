@article{ ISI:000170644500003,
Author = {Jung, C and Mejia-Monasterio, C and Seligman, TH},
Title = {{Quantum and classical echoes in scattering systems described by simple
   Smale horseshoes}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{2001}},
Volume = {{55}},
Number = {{5}},
Pages = {{616-622}},
Month = {{SEP}},
Abstract = {{We explore the quantum scattering of systems classically described by
   binary and other low-order Smale horseshoes, in a stage of development
   where the stable island associated with the inner periodic orbit is
   large, but chaos around this island is well developed. For short
   incoming pulses we nd periodic echoes modulating an exponential decay
   over many periods. The period is directly related to the development
   stage of the horseshoe. We exemplify our studies with a one-dimensional
   system periodically kicked in time and we mention possible experiments.}},
