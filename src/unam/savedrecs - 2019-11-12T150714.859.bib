@article{ ISI:000171706400007,
Author = {Gonzalez-Aguero, G and Ondarza, R and Gamboa-Dominguez, A and Cerbon, MA
   and Camacho-Arroyo, I},
Title = {{Progesterone receptor isoforms expression pattern in human astrocytomas}},
Journal = {{BRAIN RESEARCH BULLETIN}},
Year = {{2001}},
Volume = {{56}},
