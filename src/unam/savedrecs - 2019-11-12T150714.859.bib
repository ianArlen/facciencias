@article{ ISI:000172746700009,
Author = {Monreal-Gomez, MA and Molina-Cruz, A and Salas-De-Leon, DA},
Title = {{Water masses and cyclonic circulation in Bay of La Paz, Gulf of
   California, during June 1998}},
