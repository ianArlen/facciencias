@article{ ISI:000373881900002,
Author = {Fernandez-Ramirez, Cesar and Danilkin, Igor V. and Mathieu, Vincent and
   Szczepaniak, Adam P. and Joint Phys Anal Ctr},
Title = {{Understanding the nature of Lambda (1405) through Regge physics}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2016}},
Volume = {{93}},
Number = {{7}},
Month = {{APR 13}},
Abstract = {{It appears that there are two resonances with J(P) = 1/2(-) quantum
   numbers in the energy region near the Lambda(1405) hyperon. The nature
   of these states is a topic of current debate. To provide further insight
   we use Regge phenomenology to access how these two resonances fit the
   established hyperon spectrum. We find that only one of these resonances
   is compatible with a three-quark state.}},
