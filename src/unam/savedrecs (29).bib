@article{ ISI:000373722700007,
Author = {Spradling, Theresa A. and Demastes, James W. and Hafner, David J. and
   Milbach, Paige L. and Cervantes, Fernando A. and Hafner, Mark S.},
Title = {{Systematic revision of the pocket gopher genus Orthogeomys}},
Journal = {{JOURNAL OF MAMMALOGY}},
