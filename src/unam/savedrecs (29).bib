@article{ ISI:000373952300002,
Author = {Bernes, Sylvain and Melendez, Francisco J. and Torrens, Hugo},
Title = {{Cis-trans isomerism in a square-planar platinum(II) complex bearing
   bulky fluorinated phosphane ligands}},
Journal = {{ACTA CRYSTALLOGRAPHICA SECTION C-STRUCTURAL CHEMISTRY}},
Year = {{2016}},
Volume = {{72}},
Number = {{4}},
Pages = {{268+}},
Month = {{APR}},
Abstract = {{Transition-metal complexes bearing fluorinated phosphane and thiolate
   ligands has been an area of study in recent years and the chemical
   context of the current work is related to the metal-assisted
   functionalization of fluorinated derivatives. The cis and trans isomers
   of the square-planar complex
   bis{[}(pentafluorophenyl)diphenylphosphane-kappa P]
   bis(2,3,5,6-tetrafluorobenzenethiolato-kappa S) platinum(II),
   {[}Pt(C6HF4S)(2)\{P(C6H5)(2)(C6F5)\}(2)], have been crystallized from a
   single chromatographic fraction and characterized by X-ray diffraction
   analysis. The stabilization of the cis isomer results from weak
   intramolecular pi-stacking interactions and possibly from the formation
   of a C-F center dot center dot center dot Pt contact, characterized by
   an F center dot center dot center dot Pt separation of 2.957 (6)
   angstrom. The natural bond orbital analysis (NBO) for this isomer
   confirms that the corresponding F -> Pt charge transfer accounts for
   6.92 kcal mol(-1) in the isomer stabilization. Such interactions are not
   present in the centrosymmetric trans isomer.}},
