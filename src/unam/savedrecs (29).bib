@article{ ISI:000373316200001,
Author = {Gomez-Arredondo, Carmen M. and Montalvo-Arrieta, Juan C. and
   Iglesias-Mendoza, Arturo and Espindola-Castro, Victor H.},
Title = {{Relocation and seismotectonic interpretation of the seismic swarm of
   August - December of 2012 in the Linares area, northeastern Mexico}},
Journal = {{GEOFISICA INTERNACIONAL}},
Year = {{2016}},
Volume = {{55}},
Number = {{2}},
Pages = {{95-106}},
Month = {{APR-JUN}},
Abstract = {{We relocated 52 events of 2.5 <= Mc <= 3.6 from a seismic sequence of
   over 250 events that occurred during July-December 2012 southwest of the
   Linares area, northeastern Mexico. To examine this swarm four seismic
   stations were installed in the region and operated during different time
   periods from September to December. Relocation of the swarm showed that
   the earthquake hypocentral depths were at 8 (+/- 5) km, and the time
   residuals had values <= 0.38 s. The fault plane solutions were generated
   for individual earthquakes and through the use of the composite
   mechanism technique. The focal mechanism solutions show pure reverse
   faulting; the SW dipping NNW - SSE trending nodal plane is the inferred
   fault plane (strike similar to 150 degrees, dip similar to 50 degrees
   and rake similar to 67 degrees), which reveals that maximum horizontal
   stress (S-Hmax > S-hmin > S-v) predominates in the area.}},
