@article{ ISI:000378152400002,
Author = {Vega y Ortega, Rodrigo},
Title = {{Pharmaceutical studies during the Second Empire in the Gaceta Medica de
   Mexico}},
Journal = {{HISTORIA CIENCIAS SAUDE-MANGUINHOS}},
Year = {{2016}},
Volume = {{23}},
Number = {{2}},
Month = {{APR-JUN}},
Abstract = {{Pharmaceutical activities during the Second Empire have been neglected
   by historians of science in recent decades, even though pharmacy was an
   important discipline in Mexico City. This is shown by analyzing 12
   papers published in the Gaceta Medica de Mexico, the journal of the
   Sociedad Medica de Mexico. Examination of these papers helps us
   understand the interests, practices and pharmaceutical activities of
   some of this group's physicians and pharmacists, as well as detailing
   scientific endorsement of the therapeutic use of Mexican flora. This
   allows us to trace a historical continuity in the activities of
   pharmacists in the capital city throughout the nineteenth century.}},
