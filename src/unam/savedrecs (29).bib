@article{ ISI:000374602400008,
Author = {Vannasing, Phetsamone and Florea, Olivia and Gonzalez-Frankenberger,
   Berta and Tremblay, Julie and Paquette, Natacha and Safi, Dima and
   Wallois, Fabrice and Lepore, Franco and Beland, Renee and Lassonde,
   Maryse and Gallagher, Anne},
Title = {{Distinct hemispheric specializations for native and non-native languages
   in one-day-old newborns identified by fNIRS}},
Journal = {{NEUROPSYCHOLOGIA}},
Year = {{2016}},
Volume = {{84}},
Pages = {{63-69}},
Month = {{APR}},
Abstract = {{This study assessed whether the neonatal brain recruits different neural
   networks for native and non-native languages at birth. Twenty-seven
   one-day-old full-term infants underwent functional near-infrared
   spectroscopy (fNIRS) recording during linguistic and non-linguistic
   stimulation. Fourteen newborns listened to linguistic stimuli (native
   and non-native language stories) and 13 newborns were exposed to
   non-linguistic conditions (native and non-native stimuli played in
   reverse). Comparisons between left and right hemisphere oxyhemoglobin
   (HbO(2)) concentration changes over the temporal areas revealed clear
   left hemisphere dominance for native language, whereas non-native
   stimuli were associated with right hemisphere lateralization. In
   addition, bilateral cerebral activation was found for non-linguistic
   stimulus processing. Overall, our findings indicate that from the first
   day after birth, native language and prosodic features are processed in
   parallel by distinct neural networks. (C) 2016 The Authors. Published by
   Elsevier Ltd.}},
