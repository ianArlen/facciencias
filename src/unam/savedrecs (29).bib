@article{ ISI:000370342100024,
Author = {LondonO-Restrepo, Sandra M. and Ramirez-Gutierrez, Cristian F. and del
   Real, Alicia and Rubio-Rosas, Efrain and Rodriguez-Garcia, Mario E.},
Title = {{Study of bovine hydroxyapatite obtained by calcination at low heating
   rates and cooled in furnace air}},
