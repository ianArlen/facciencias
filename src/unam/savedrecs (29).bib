@article{ ISI:000371652900024,
Author = {Valdez-Calderon, Alejandro and Gonzalez-Montiel, Simplicio and
   Martinez-Otero, Diego and Martinez-Torres, Ataulfo and Manuel
   Vasquez-Perez, Jose and Molina-Vera, Carlos and Martin Torres-Valencia,
   J. and Alvarado-Rodriguez, Jose G. and Cruz-Borbolla, Julian},
Title = {{Synthesis, structural study and biological activity of new derivatives
   of chrysin containing a 2-mercaptopyridyl or
   5-(trifluoromethyl)-2-mercaptopyridyl fragments}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE}},
Year = {{2016}},
Volume = {{1110}},
Pages = {{196-207}},
Month = {{APR 15}},
Abstract = {{New derivatives of chrysin containing 2-mercaptopyridine (2a-2e) or
   5-(trifluoromethyl)-2-mercaptopyridine (3a-3e) moieties were prepared
   from the reaction between bromides (1a-1e) and 2-mercaptopyridine or
   5-(trifluoromethyl)-2-mercaptopyridine, respectively. Their structures
   were elucidated by NMR, IR and elemental analysis. The molecular
   structure of compounds 1a, 1c-1e, 2b-2e and 3a was determined by
   single-crystal X-ray diffraction studies. All rings in these structures
   are nearly coplanar and they showed an intramolecular hydrogen bond
   between the phenolic hydroxyl H atom and the carbonyl 0 atom that forms
   a six membered ring. The crystal packing also showed a wide variety of
   intermolecular contacts such as C-H center dot center dot center dot A,
   pi-pi, C-H center dot center dot center dot pi and lone pair center dot
   center dot center dot pi interactions which were supported by quantum
   theory of atoms in molecules (QTAIM), Hirshfeld surface, and fingerprint
   plot analyses. Biological activity of all compounds was tested in growth
   assays of the nematode Caenorhabiditis elegans. Compounds 2e, 3b and 3c
   inhibited larval development. (C) 2016 Elsevier B.V. All rights
   reserved.}},
