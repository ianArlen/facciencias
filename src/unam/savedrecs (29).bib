@article{ ISI:000373863200052,
Author = {Heard, C. L. and Rivera, W. and Best, R.},
Title = {{Characteristics of an ammonia/lithium nitrate double effect heat
   pump-transformer}},
Journal = {{APPLIED THERMAL ENGINEERING}},
Year = {{2016}},
Volume = {{99}},
Pages = {{518-527}},
Month = {{APR 25}},
Abstract = {{The modelled operating characteristics of an ammonia/lithium nitrate
   double effect absorption heat pump transformer (Type III absorption heat
   pump) are presented and compared to other working pair options and
   absorption heat pump cycles. Heat and mass balance equations are given.
   The effect of sub-optimal cycle design is shown on cycle thermal
   efficiency and solution pump power. It is shown that the ammonia/lithium
   nitrate working pair would achieve a performance a little less efficient
   than a water/lithium bromide system but is somewhat more tolerant of
   less than optimum operating conditions with respect to cycle thermal
   efficiency and solution pump power. Ratios of useful heat delivered to
   driving heat of nearly four are shown to be achievable with this system.
   (C) 2016 Elsevier Ltd. All rights reserved.}},
