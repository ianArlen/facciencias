@article{ ISI:000374062800023,
Author = {Huelgas-Morales, Gabriela and Giovanni Silva-Garcia, Carlos and Salinas,
   Laura S. and Greenstein, David and Navarro, Rosa E.},
Title = {{The Stress Granule RNA-Binding Protein TIAR-1 Protects Female Germ Cells
   from Heat Shock in Caenorhabditis elegans}},
Journal = {{G3-GENES GENOMES GENETICS}},
Year = {{2016}},
Volume = {{6}},
Number = {{4}},
