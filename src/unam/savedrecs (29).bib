@article{ ISI:000373702000026,
Author = {Perez, Nora A. and Bucio, Lauro and Bokhimi, Xim and Lima, Enrique and
   Soto, Enrique},
Title = {{Quantification of amorphous phases in the silt fraction of Mexican
   pre-Hispanic adobe earth bricks}},
