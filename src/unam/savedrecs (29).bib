Title = {{OPTICAL SPECTROSCOPIC OBSERVATIONS OF GAMMA-RAY BLAZAR CANDIDATES. VI.
   FURTHER OBSERVATIONS FROM TNG, WHT, OAN, SOAR, AND MAGELLAN TELESCOPES}},
Journal = {{ASTRONOMICAL JOURNAL}},
Year = {{2016}},
Volume = {{151}},
