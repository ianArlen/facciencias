@article{ ISI:000372770700012,
Author = {Lopez-Cuautle, Camilo and Vazquez-Arevalo, Rosalia and Manuel
   Mancilla-Diaz, Juan},
Title = {{Muscle Dysmorphia Diagnostic evaluation: a systematic review.}},
Journal = {{ANALES DE PSICOLOGIA}},
Year = {{2016}},
Volume = {{32}},
Number = {{2}},
Pages = {{405-416}},
Month = {{MAY}},
Abstract = {{The aim of this study was to examine the actual state of the Muscle
   Dysmorphia (MD) diagnostic evaluation. A systematic search of the
   Medline and PsycINFO databases was conducted, from 1997-2012,
   descriptors were ``muscle dysmorphic disorder{''} ``vigorexia{''} and
   ``male{''}. 17 studies met the inclusion criteria, of these, the
   diagnostic criteria proposed by Pope et al. (1997) were used by all.
   Those with MD are concerned about body size, lift weights 3-7 days a
   week, performed protein diets, sometimes they uses food supplements and
   steroids. In conclusion, Pope et al. (1997) criteria are the basis for
   the MD diagnosis and the DSM-5 take it to account to recognize and
   include the MD as a pathology.}},
