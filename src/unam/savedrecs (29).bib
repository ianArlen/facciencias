@article{ ISI:000373551300008,
Author = {Koenigsberger, Gloria and Moreno, Edmund},
Title = {{TIDAL SHEAR AND THE V1309 SCO MERGER}},
Journal = {{REVISTA MEXICANA DE ASTRONOMIA Y ASTROFISICA}},
Year = {{2016}},
Volume = {{52}},
Number = {{1}},
Pages = {{113-124}},
Month = {{APR}},
Abstract = {{We show that the observed decline in the orbital period of the merger
   candidate V1309 Sco could have been driven by tidal shear energy
   dissipation, (E) over dot(s). This mechanism becomes relevant once the
   expanding layers of an evolving star rotate asynchronously. For a
   1M(circle dot) + 0.8M(circle dot) system with orbital period P=1.44 d,
   we find that (E) over dot(s) can power a growth in stellar radius from
   1.50R(circle dot) to 1.85R(circle dot) in the primary over the course of
   similar or equal to 5 years, during which the rate of period change goes
   from similar or equal to 1000 yr to similar or equal to 170 yr, in
   agreement with the observations. The kinematical viscosity used for
   these calculations is estimated from the maximum tidal flow speed and
   from the extension of the the tidal bulge, and is thus a function of the
   stellar and orbital parameters.}},
