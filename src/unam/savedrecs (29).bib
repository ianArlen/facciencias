@article{ ISI:000375155000045,
Author = {Martynyuk, Oxana and Kotolevich, Yulia and Velez, Rodrigo and Ortega,
   Jesus Efren Cabrera and Tiznado, Hugo and Partida, Trino Zepeda and
   Mota-Morales, Josue D. and Pestryakov, Alexey and Bogdanchikova, Nina},
Title = {{On the High Sensitivity of the Electronic States of 1 nm Gold Particles
   to Pretreatments and Modifiers}},
Journal = {{MOLECULES}},
Year = {{2016}},
Volume = {{21}},
Number = {{4}},
Month = {{APR}},
Abstract = {{In this paper, the effect of modifiers and pretreatments on the
   electronic states of 1 nm gold nanoparticles (AuNPs) supported on silica
   was systematically studied. AuNPs deposited on silica (particle size of
   2-4 nm) modified with Ce, La and Fe oxides, were studied by FTIR of
   adsorbed CO after different redox treatments at 100, 300 and 500 degrees
   C. This study was conducted at room temperature to allow detecting the
   electronic states of gold, which is more likely involved in CO oxidation
   at the same temperature. AuNP size distribution was measured by HRTEM.
   It is shown that the electronic state of gold species (Au-n(delta-),
   Au-0, Au-n(delta+), Au+) in 1 nm AuNPs is sensitive to the modifier as
   well as to the temperatures of redox pretreatments. Supports modified
   with the same additives but containing larger AuNPs (similar to 3, 4, 5,
   and 7 nm) were also studied. They showed that Au0 remains stable
   irrespective of additives and redox pretreatments, indicating no
   significant effect of such treatments on the electronic properties of
   larger AuNPs. Samples with a predominant AuNP size of 2 nm are an
   intermediate case between these two groups of materials.}},
