@article{ ISI:000273896400002,
Author = {Thome, Roberto C. A. and Yang, Hyun Mo and Esteva, Lourdes},
Title = {{Optimal control of Aedes aegypti mosquitoes by the sterile insect
   technique and insecticide}},
Journal = {{MATHEMATICAL BIOSCIENCES}},
