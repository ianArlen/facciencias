@article{ ISI:000272895600005,
Author = {Ibarra Colado, Eduardo},
Title = {{Herbert A. Simon and his Monomania: Human Behavior as Artificial
   Behavior}},
Journal = {{GESTION Y POLITICA PUBLICA}},
Year = {{2010}},
Volume = {{19}},
Number = {{1}},
Pages = {{155-170}},
Abstract = {{The article advances a critical assessment of the conception of human
   behavior as artificial behavior proposed by Herbert A. Simon. It
   highlights some of their implications in the context of a society
   characterized by the complex articulation between freedom and control.
   The main argument stresses the paradoxical nature of the hypothesis that
   sustains that the computers could do any work made by a person, showing
   that perhaps what it really poses is that any person must do his work as
   if they were a computer. This current discussion aims to highlight the
   relevance and actuality of Simon's contributions in his book The
   sciences of the artificial to understand the principles under which has
   been improved the design of that artificial iron cage we call modernity.}},
