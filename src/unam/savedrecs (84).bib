@inproceedings{ ISI:000286947200022,
Author = {Martinez-Garcia, N. P. and Lerma-Gonzalez, C. and Infante-Vazquez, O.},
Editor = {{Brandan, ME and HerreraMartinez, F and RamirezR, V and RodriguezVillafuerte, M}},
Title = {{Evaluation of Cardiovascular Control Through Analysis of Inter Beat
   Interval, Systolic Blood Pressure and Photoplethysmographic Volume}},
Booktitle = {{ELEVENTH MEXICAN SYMPOSIUM ON MEDICAL PHYSICS}},
Series = {{AIP Conference Proceedings}},
