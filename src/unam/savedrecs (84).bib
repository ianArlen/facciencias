@article{ ISI:000272464800006,
Author = {Elinos-Calderon, Diana and Robledo-Arratia, Yolanda and Perez-De La
   Cruz, Veronica and Maldonado, Perla D. and Galvan-Arzate, Sonia and
   Pedraza-Chaverri, Jose and Santamaria, Abel},
Title = {{Antioxidant strategy to rescue synaptosomes from oxidative damage and
   energy failure in neurotoxic models in rats: protective role of
   S-allylcysteine}},
Journal = {{JOURNAL OF NEURAL TRANSMISSION}},
Year = {{2010}},
Volume = {{117}},
Number = {{1}},
Pages = {{35-44}},
Month = {{JAN}},
Abstract = {{The functional preservation of nerve endings since the early stages of
   toxicity in a given damaging insult-either acute or chronic-by means of
   antioxidant and neuroprotective agents is a primary need to design
   therapeutic strategies for neurodegenerative disorders, with particular
   emphasis on those diseases with excitotoxic and depleted energy
   metabolism components. S-allylcysteine (SAC), a well-known antioxidant
   agent, was tested as a post-treatment in different in vitro and in vivo
   neurotoxic models. Quinolinic acid (QUIN) was used as a typical
   excitotoxic/pro-oxidant inducer, 3-nitropropionic acid (3-NP) was
   employed as a mitochondrial function inhibitor, and their combination
   (QUIN + 3-NP) was also evaluated in in vitro studies. For in vitro
   purposes, increasing concentrations of SAC (10-100 mu M) were added to
   isolated brain synaptosomes at different times (1, 3 and 6 h) after the
   incubation with toxins (100 mu M QUIN, 1 mM 3-NP or the combination of
   QUIN (21 mu M) + 3-NP (166 mu M). Thirty minutes later, lipid
   peroxidation (LP) and mitochondrial dysfunction (MD) were evaluated. For
