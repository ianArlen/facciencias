@inproceedings{ ISI:000283042900007,
Author = {Sudarsky, Daniel},
Editor = {{MoralesTecotl, HA and UrenaLopez, LA and LinaresRomero, R and GarciaCompean, HH}},
Title = {{The Quantum Origin of the Seeds of Cosmic Structure: The case for a
   missing link.}},
Booktitle = {{GRAVITATIONAL PHYSICS: TESTING GRAVITY FROM SUBMILLIMETER TO COSMIC
   SCALE}},
Series = {{AIP Conference Proceedings}},
Year = {{2010}},
Volume = {{1256}},
