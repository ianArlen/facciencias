@article{ ISI:000284613200029,
Author = {Martinez Villa, Roberto and Martsinkovsky, Alex},
Title = {{STABLE PROJECTIVE HOMOTOPY THEORY OF MODULES, TAILS, AND KOSZUL DUALITY}},
Journal = {{COMMUNICATIONS IN ALGEBRA}},
Year = {{2010}},
Volume = {{38}},
Number = {{10}},
Pages = {{3941-3973}},
Abstract = {{A contravariant functor is constructed from the stable projective
   homotopy theory of finitely generated graded modules over a
   finite-dimensional algebra to the derived category of its Yoneda algebra
   modulo finite complexes of modules of finite length. If the algebra is
   Koszul with a noetherian Yoneda algebra, then the constructed functor is
   a duality between triangulated categories. If the algebra is
   self-injective, then stable homotopy theory specializes trivially to
   stable module theory. In particular, for an exterior algebra the
   constructed duality specializes to (a contravariant analog of) the
   Bernstein-Gelfand-Gelfand correspondence.}},
