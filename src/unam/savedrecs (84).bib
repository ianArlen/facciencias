@article{ ISI:000279431100001,
Author = {Adem, E. and Galvan, D. H. and Cota, L. and Burillo, G.},
Title = {{Formation of Nanostructures in Highly Irradiated Poly (Vinyl Chloride)}},
Journal = {{FULLERENES NANOTUBES AND CARBON NANOSTRUCTURES}},
Year = {{2010}},
Volume = {{18}},
Number = {{3}},
Pages = {{179-185}},
Abstract = {{Different samples of PVC irradiated with -rays at higher dose up to 20
   MGy were investigated. Structures fluctuating among nanowires, nanotubes
   and onion-like structures were found throughout different areas of the
   observed samples. To our knowledge, it is the first investigation which
   claims the existence of such structures. Moreover, the structures are
   associated mainly with 2H-graphite, C10Cl10 and an unidentified phase,
   which we assume is composed of C=CCl-C. Research is ongoing in order to
   identify this carbonaceous phase.}},
