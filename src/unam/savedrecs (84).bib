Address = {{NORDBUNTE 23, D-21385 OLDENDORF LUHE, GERMANY}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Mendoza, VM (Reprint Author), Univ Nacl Autonoma Mexico, Ctr Ciencias Atmosfera, Ciudad Univ, Mexico City 04510, DF, Mexico.
   Mendoza, V. M.; Garduno, R.; Adem, J., Univ Nacl Autonoma Mexico, Ctr Ciencias Atmosfera, Mexico City 04510, DF, Mexico.
   Mendoza, B., Univ Nacl Autonoma Mexico, Inst Geofis, Mexico City 04510, DF, Mexico.}},
DOI = {{10.3354/cr00938}},
ISSN = {{0936-577X}},
