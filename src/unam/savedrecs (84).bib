@inproceedings{ ISI:000289008900028,
Author = {Koenigsberger, Gloria and Moreno, Edmund and Harrington, David M.},
Editor = {{Leitherer, C and Bennett, PD and Morris, PW and VanLoon, JT}},
Title = {{Tidal Flows from Asynchronous Rotation in Binaries}},
Booktitle = {{HOT AND COOL: BRIDGING GAPS IN MASSIVE-STAR EVOLUTION}},
Series = {{Astronomical Society of the Pacific Conference Series}},
Year = {{2010}},
Volume = {{425}},
Pages = {{209+}},
Note = {{Workshop on Hot and Cool: Bridging Gaps in Massive-Star Evolution,
   California Inst Technol, Pasadena, CA, NOV 10-12, 2008}},
Abstract = {{Asynchronous rotation in binary stars produces non-radial oscillations
   that are known to cause observable variability on orbital timescales.
   The horizontal perturbations of the surface velocity fields are referred
   to as ``tidal flows.{''} In this paper we illustrate the manner in which
   tidal flows perturb the surface velocity field from that of uniform
   rotation, using a one-layer stellar model for the calculations. We
   justify the validity of this simplified model by the striking similarity
   between the photospheric absorption line-profiles it predicts and
   observational data of the binary system alpha Virginis. The velocity
   perturbations are used to compute the mechanical energy dissipation
   rates, (E) over dot, due to the shearing flows for the case of a massive
   (50 + 28 M-circle dot) binary system having a moderately eccentric (e =
   0.3) orbit. The largest value of (E) over dot around periastron phases
   is found on the hemisphere facing the companion. However, at other
   orbital phases the maximum (E) over dot may migrate towards the poles.
   Assuming that (E) over dot plays a role in the mass-loss characteristics
   of massive binary systems, this suggests that peculiar binaries such as
   HD 5980 and eta Carinae may have a highly non-spherically symmetric
   mass-loss distribution which, in addition, is time-variable.}},
