@article{ ISI:000272638600001,
Author = {Ayala, A. and Bashir, A. and Raya, A. and Sanchez, A.},
Title = {{Impact of a uniform magnetic field and non-zero temperature on explicit
   chiral symmetry breaking in QED: arbitrary hierarchy of energy scales}},
Journal = {{JOURNAL OF PHYSICS G-NUCLEAR AND PARTICLE PHYSICS}},
Year = {{2010}},
Volume = {{37}},
Number = {{1}},
Month = {{JAN}},
Abstract = {{Employing Schwinger's proper-time method, we calculate the <(psi) over
   bar psi >-condensate for massive Dirac fermions of charge e interacting
   with a uniform magnetic field in a heat bath. We present general results
   for arbitrary hierarchy of the energy scales involved, namely the
   fermion mass m, the magnetic field strength root eB and temperature T.
   Moreover, we study particular regimes in detail and reproduce some of
   the results calculated or anticipated earlier in the literature. We also
   discuss possible applications of our findings.}},
