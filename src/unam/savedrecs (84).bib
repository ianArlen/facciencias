@article{ ISI:000280769300009,
Author = {Gonzalez-Mendoza, Roberto and Lopez-Gonzalez, Hilario and
   Rojas-Hernandez, Alberto},
Title = {{Spectrophotometric Determination of the First Hydrolysis Constant of
   Praseodymium (III)}},
Journal = {{JOURNAL OF THE MEXICAN CHEMICAL SOCIETY}},
Year = {{2010}},
Volume = {{54}},
Number = {{1}},
Pages = {{51-58}},
Month = {{JAN-MAR}},
Abstract = {{The behavior of the trivalent ion praseodymium in 2M of NaCl at 303 K
   and in CO(2) free conditions, was studied. Spectrophotometric titrations
   of the soluble species were used, in order to obtain the value of the
   first hydrolysis constant of Pr(III). The data obtained were treated
   with both the program SQUAD and by a graphic method, respectively. The
   result obtained using SQUAD was log{*}beta(1) = -8.94 +/- 0.03, while it
   was log{*}beta(1) = -8.77 +/- 0.03, when calculated graphically. These
   results are similar to the value obtained previously with the
   potentiometric method.}},
