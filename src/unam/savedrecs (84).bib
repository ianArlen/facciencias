@article{ ISI:000286923100006,
Author = {di Castro, Elisabetta},
Title = {{Inequality, Exclusion and Global Justice}},
Journal = {{ISEGORIA}},
Year = {{2010}},
Number = {{43}},
Pages = {{459-478}},
Abstract = {{The process of globalization has affected the national State to the
   point of crisis and has increased inequality and exclusion in the world.
   Therefore, justice cannot keep being thought exclusively, as done in the
   past century, within certain limits all concerned to the State. Facing
   citizenship and State process of hierarchy in the new world order, a
   global justice theory must consider long-term the consolidation of a
   global constitutionalism that guarantees fundamental rights for everyone
   and promotes a fair distribution of the benefits generated by
   globalization.}},
