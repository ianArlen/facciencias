@article{ ISI:000272320500008,
Author = {Maycotte, Paola and Guemez-Gamboa, Alicia and Moran, Julio},
Title = {{Apoptosis and Autophagy in Rat Cerebellar Granule Neuron Death: Role of
   Reactive Oxygen Species}},
Journal = {{JOURNAL OF NEUROSCIENCE RESEARCH}},
