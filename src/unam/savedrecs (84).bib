Title = {{Self-gravitating dissipative sources in the ``fluid flow{''} approach.}},
Booktitle = {{IV MEXICAN MEETING ON MATHEMATICAL AND EXPERIMENTAL PHYSICS:
   RELATIVISTIC FLUIDS AND BIOLOGICAL PHYSICS}},
Series = {{AIP Conference Proceedings}},
Year = {{2010}},
Volume = {{1312}},
