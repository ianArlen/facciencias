@inproceedings{ ISI:000298025200072,
Author = {Auvinet, G. and Mendez, E. and Matus, U.},
Editor = {{CarreonFreyre, D and Cerca, M and Galloway, DL and SilvaCorona, JJ}},
Title = {{Evaluation of Mexico City subsidence}},
Booktitle = {{LAND SUBSIDENCE, ASSOCIATED HAZARDS AND THE ROLE OF NATURAL RESOURCES
   DEVELOPMENT}},
