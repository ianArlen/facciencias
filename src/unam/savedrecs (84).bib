@article{ ISI:000277588600002,
Author = {Nabifar, Afsaneh and McManus, Neil T. and Vivaldo-Lima, Eduardo and
   Penlidis, Alexander},
Title = {{Kinetic Aspects of Styrene Polymerization with an Acyloxyamine}},
Journal = {{JOURNAL OF MACROMOLECULAR SCIENCE PART A-PURE AND APPLIED CHEMISTRY}},
Year = {{2010}},
Volume = {{47}},
Number = {{6}},
Pages = {{496-502}},
Abstract = {{The present paper evaluates largely unstudied kinetic aspects of styrene
   polymerization with a (relatively new) acyloxyamine over several
   temperature levels and contrasts these features with regular styrene
   polymerization and styrene polymerization with TEMPO. These comparisons
   show that the system behaves rather like regular thermal polymerization
   of styrene at temperatures between 120-180 degrees C. However, at higher
   temperatures ( 180 degrees C), acyloxyamine has an initiator-like
   contribution, giving the rate of polymerization an extra boost while
   decreasing molecular weights. This is further corroborated by
   mathematical modeling for both conversion and molecular weight averages.}},
