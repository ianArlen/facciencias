@article{ ISI:000415598900011,
Author = {Sanchez-Camacho, Pedro and Francisco Gomez-Garcia, J. and Pfeiffer,
   Heriberto},
Title = {{Thermokinetic and conductivity analyzes of the high CO2 chemisorption on
   Li5AlO4 and alkaline carbonate impregnated Li5AlO4 samples: Effects
   produced by the use of CO2 partial pressures and oxygen addition}},
Journal = {{JOURNAL OF ENERGY CHEMISTRY}},
Year = {{2017}},
Volume = {{26}},
Number = {{5}},
Pages = {{919-926}},
Month = {{SEP}},
Abstract = {{The effect of CO2 partial pressure was evaluated during the CO2
   chemisorption in penta lithium aluminate (Li5AlO4), using different CO2
   and O-2 partial pressures in the presence or absence of alkaline
   carbonates. Results showed that using low P-O2 (0.1) did not affect the
   kinetic and final CO2 chemisorption process. Moreover, small additions
   of oxygen (P-O2 = 0.05) into the mixture flue gas, seemed to increase
   the CO2 chemisorption. Additionally, the presence of alkaline carbonates
   modified the CO2 capture temperature range. CO2 chemisorption kinetic
   parameters were determined assuming a double exponential model where
   direct CO2 chemisorption and CO2 chemisorption controlled by diffusion
   processes are considered. Finally, ionic diffusion was analyzed by ionic
   conduction analysis, where all the gravimetric and ionic measurements
   were in good agreement showing different diffusion processes depending
   on temperature. Finally, the oxygen and alkaline carbonate additions
   have positive effects during the CO2 chemisorption process in Li5AlO4,
   and a possible reaction mechanism is presented. (C) 2017 Science Press
   and Dalian Institute of Chemical Physics, Chinese Academy of Sciences.
   Published by Elsevier B.V. and Science Press. All rights reserved.}},
