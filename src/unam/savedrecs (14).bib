@article{ ISI:000416494100003,
Author = {Teresa Cabrero G, Maria},
Title = {{Tablets in the Bolanos Culture and their Probable Function}},
Journal = {{ARQUEOLOGIA IBEROAMERICANA}},
Year = {{2017}},
Volume = {{35}},
Pages = {{16-22}},
Month = {{SEP}},
Abstract = {{Tablets are objects whose function is unknown. Their shape is
   rectangular with variable dimensions. The great majority are flat, but
   there are slightly curved specimens; the former were recovered inside
   housing units and, in Bolanos, there is a single curved specimen
   associated with a burial. These types of objects have a wide diversity
   of decoration; they can be painted, punctured, incised or engraved, but
   always around a quadrangular center with crossed lines. The distribution
   of the tablets encompasses the main sites of Zacatecas and Jalisco with
   local variants.}},
