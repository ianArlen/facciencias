@article{ ISI:000411161600025,
Author = {Nieves-Uribe, Sandra and Flores-Gallardo, Adrian and Llorente-Bousquets,
   Jorge},
Title = {{Chorion of Orophila cardases campaspe (Hewitson, 1869) and Chorionic
   Characterization of the Subtribe Callicorina}},
Journal = {{SOUTHWESTERN ENTOMOLOGIST}},
Year = {{2017}},
Volume = {{42}},
Number = {{3}},
Pages = {{865-877}},
Month = {{SEP}},
Abstract = {{We investigated the chorionic structure of Orophila cardases campaspe,
   with a characterization of this structure in the Callicorina subtribe.
   The presence of windows at the base of the ridges, apical relief in
   semicircles, and partial or total absence of ribs between axes, typified
   them. The chorionic similarities between Epiphilina and Callicorina
   subtribes, as well as possibility of a reduction or optimization of the
   material to derive on the bridge-shaped rides of Eubagina subtribe were
   recognized. Greater exploration of the chorion in species of Callicorina
   is necessary for a source of characters and to contrast the hypothesis
   of Freitas et al. (2014) about the phylogenetic relationships among
   genera of Callicorina.}},
