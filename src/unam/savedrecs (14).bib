@article{ ISI:000406599400006,
Author = {Wolfesberger, Philipp},
Title = {{Coercive Inclusion in the Mexican War on Drugs: A Field Study of Human
   Rights and the State in Michoacan}},
Journal = {{LATIN AMERICAN PERSPECTIVES}},
Year = {{2017}},
Volume = {{44}},
Number = {{5}},
Pages = {{91-107}},
Month = {{SEP}},
Abstract = {{Qualitative ethnographic study of the human rights violations committed
   in the course of the militarized combat against drug trafficking
   organizations in rural Michoacan unmasks state practices of coercive
   inclusion. The violation of human rights and the subsequent processing
   of human rights claims paradoxically bind the marginalized population to
   the formal state and foster its subordination. The practical
   configuration of the current arena of human rights is not the lever for
   a democratic, inclusive Mexico but a curtain that conceals the
   repressive practices that it makes possible. In the processing of human
   rights complaints, the legal rights of physical integrity and private
   property become moral rights with no effect of legal justice.}},
