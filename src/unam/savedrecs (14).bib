@article{ ISI:000406232900011,
Author = {De Los Santos, I. Montoya and Courel, Maykel and Mathews, N. R. and
   Mathew, X.},
Title = {{Study on the effect of annealing under pressure on the material
   properties of Cu2ZnSn(S,Se)(4) thin films}},
Journal = {{MATERIALS SCIENCE IN SEMICONDUCTOR PROCESSING}},
Year = {{2017}},
Volume = {{68}},
Pages = {{68-75}},
Month = {{SEP}},
Abstract = {{In this work, we report the processing of Cu2ZnSn(S,Se)(4) (CZTSSe) thin
   films by doctor blade technique, a vacuum-free and low-cost method. For
   this purpose, nanoparticles of the precursor materials ZnS, SnS and CuS
   were synthesized by wet chemical reaction. A paste of these three binary
   compounds mixed in appropriate proportions were coated on glass
   substrates, and submitted to thermal treatment under Se atmosphere at
   different temperatures and pressures to obtain the CZTSSe films. The
   impact of temperature and pressure on structural, compositional, optical
   and electrical properties was analyzed and the information was used to
   optimize the CZTSSe physical properties. By increasing temperature from
   450 degrees C to 600 degrees C, improvements in CZTSSe crystalline
   quality as well as better Se incorporation were found for all chamber
   pressures. As an interesting result, it was observed that the band-gap
   of kesterite film can be tailored as a function of annealing temperature
   and pressure. In particular, band-gap energy values in the range of 1.16
   -1.25 eV were obtained. Finally, it was found that sample annealed at
   600 degrees C under 375 Torr for 30 min showed the best physical
   properties suitable for solar cell applications.}},
