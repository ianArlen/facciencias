@article{ ISI:000411773600028,
Author = {Sixto-Camacho, Lazaro M. and Bravo-Castillero, Julian and Brenner,
   Renald and Guinovart-Diaz, Raul and Mechkour, Houari and
   Rodriguez-Ramos, Reinaldo and Sabina, Federico J.},
Title = {{Asymptotic homogenization of periodic thermo-magneto-electro-elastic
