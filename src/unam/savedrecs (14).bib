
@article{ ISI:000409352200015,
Author = {Ferrera, Patricia and Zepeda, Angelica and Arias, Clorinda},
Title = {{Nonsteroidal anti-inflammatory drugs attenuate amyloid-beta
   protein-induced actin cytoskeletal reorganization through Rho signaling
   modulation}},
Journal = {{CELLULAR AND MOLECULAR NEUROBIOLOGY}},
Year = {{2017}},
Volume = {{37}},
Number = {{7}},
Pages = {{1311-1318}},
Month = {{OCT}},
Abstract = {{Amyloid-beta protein (A beta) neurotoxicity occurs along with the
   reorganization of the actin-cytoskeleton through the activation of the
   Rho GTPase pathway. In addition to the classical mode of action of the
   non-steroidal anti-inflammatory drugs (NSAIDs), indomethacin, and
   ibuprofen have Rho-inhibiting effects. In order to evaluate the role of
   the Rho GTPase pathway on A beta-induced neuronal death and on neuronal
   morphological modifications in the actin cytoskeleton, we explored the
   role of NSAIDS in human-differentiated neuroblastoma cells exposed to A
   beta. We found that A beta induced neurite retraction and promoted the
   formation of different actin-dependent structures such as stress fibers,
   filopodia, lamellipodia, and ruffles. In the presence of A beta, both
   NSAIDs prevented neurite collapse and formation of stress fibers without
   affecting the formation of filopodia and lamellipodia. Similar results
   were obtained when the downstream effector, Rho kinase inhibitor Y27632,
   was applied in the presence of A beta. These results demonstrate the
   potential benefits of the Rho-inhibiting NSAIDs in reducing A
   beta-induced effects on neuronal structural alterations.}},
