@article{ ISI:000410731100007,
Author = {Martin, Alexey and Camas, Alicia},
Title = {{Regulation and market of soil in Spain. Budgets for the debate}},
Journal = {{EURE-REVISTA LATINOAMERICANA DE ESTUDIOS URBANO REGIONALES}},
Year = {{2017}},
Volume = {{43}},
Number = {{130}},
Pages = {{141-160}},
Month = {{SEP}},
Abstract = {{Experts, policy makers and economic agents have focused on the study of
   the rise of land prices in Spain. One of the solutions proposed by land
   use public policy was to increase supply, based on the premise that by
   increasing it, prices would decrease. This strategy did not produce the
   expected results. Nevertheless, in recent years there have been efforts
   to bring this approach back. This paper analyzes the main features of
   the land market, its interdependence with urban planning and law. In
   this way, the causes of failure are exposed, as a way of ensuring the
   same mistakes are not repeated.}},
