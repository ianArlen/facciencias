Journal = {{COMPUTERS \& MATHEMATICS WITH APPLICATIONS}},
Year = {{2017}},
Volume = {{74}},
Number = {{6}},
Pages = {{1525-1527}},
Month = {{SEP 15}},
Abstract = {{This corrigendum supplies corrected numerical examples of those
   appearing in Comput. Math. Appl. 66 (2013) 2056-2074. The correction is
   based on a change of the sign of the magnetic permeability mu(22) of the
   cobalt ferrite. As a consequence, is corrected the erratic behavior near
