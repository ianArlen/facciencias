@article{ ISI:000409453300001,
Author = {Villamil, Nora},
Title = {{Why are flowers sweeter than fruits or buds? Variation in extrafloral
   nectar secretion throughout the floral ontogeny of a myrmecophile}},
Journal = {{BIOTROPICA}},
Year = {{2017}},
Volume = {{49}},
Number = {{5}},
Pages = {{581-585}},
Month = {{SEP}},
Abstract = {{Extrafloral nectar provision during floral ontogeny in ant-plants has
   not been widely studied. Extrafloral nectar secretion differed between
   leaves associated with buds, flowers, and fruits, and peaked during
   anthesis when pollinators were present. This ontogenetic variation may
   result from mixed selective pressures involving strategies for defense
   and mutualist management.
   Resumen La variacion en el nectar extrafloral a lo largo del desarrollo
   floral de plantas mirmecofilas ha sido poco estudiado. La secrecion de
   nectar extrafloral difirio fuertemente entre hojas asociadas a botones,
   flores y frutos. El pico de produccion se presenta durante la antesis,
   cuando visitan los polinizadores. Esta variacion ontogenetica puede ser
   producto de varias presiones selectivas relacionadas con estrategias de
   defensa y de manejo de mutualistas.}},
