@article{ ISI:000409073000009,
Author = {Ruiz Carrillo, Edgardo and Bravo Sanchez, Luisa and Meraz Martinez,
   Samuel and Cruz Gonzalez, Jose Luiz},
Title = {{IRE/F Structure Analysis in Student Expositions during Nursing Practices
   with SDIS-GSEQ}},
Journal = {{ELECTRONIC JOURNAL OF RESEARCH IN EDUCATIONAL PSYCHOLOGY}},
