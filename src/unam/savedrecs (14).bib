@article{ ISI:000424972900004,
Author = {Velasco Santos, Paola},
Title = {{DENIM, CONSUMPTION AND THE CONFIGURATION OF THE NEOLIBERAL RURAL
   SUBJECTS IN TLAXCALA, MEXICO}},
Journal = {{REVISTA SAN GREGORIO}},
Year = {{2017}},
Number = {{18, SI}},
Pages = {{34-43}},
Month = {{SEP}},
Abstract = {{I present the case of a rural village in south Tlaxcala that thirty
   years ago switched from agriculture to a particular type of garment
   maquila. In addition to labor relations, their consumption practices,
   their cultural and family dynamics, as well as their environment, were
   reconfigured in multiple and complex ways. This is explained, in part,
   by the changes in global consumption patterns and the rise of cheap
   commodities, and to a greater extent by changes in the global
   organization of production. This article advances some results of a
   mayor project, and places empirical data under a framework that
   articulates these socio-cultural and environmental transformations
   within a broader history. It seeks to recover the anthropological
   analysis of consumption, recognizing the dialectical relationship
   between consumption and production, as well as the material conditions
   in which decisions and economic processes are made.}},
