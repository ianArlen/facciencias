@article{ ISI:000408033200002,
Author = {Armenta, Silvia and Moreno-Mendieta, Silvia and Sanchez-Cuapio, Zaira
   and Sanchez, Sergio and Rodriguez-Sanoja, Romina},
Title = {{Advances in molecular engineering of carbohydrate-binding modules}},
Journal = {{PROTEINS-STRUCTURE FUNCTION AND BIOINFORMATICS}},
