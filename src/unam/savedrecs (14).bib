@article{ ISI:000411161600026,
Author = {Garcia-Jimenez, Jesus and Luis-Martinez, Armando and Garcia-Morales,
   Irmin and Llorente-Bousquets, Jorge},
Title = {{A New Subspecies of Prepona brooksiana Godman \& Salvin, 1889}},
Journal = {{SOUTHWESTERN ENTOMOLOGIST}},
Year = {{2017}},
Volume = {{42}},
Number = {{3}},
Pages = {{879-888}},
Month = {{SEP}},
Abstract = {{A new subspecies of Prepona brooksiana G. \& S. is described and named,
   which is the most northerly occurrence of all geographical races of this
   taxon, occurring on the slope of the Gulf of Mexico, in the most boreal
   portion of the cloud forest. We describe the population variation of the
   characters that diagnose it and mention aspects of its behavior and
   habitat. It has been determined that the Mesoamerican subspecies of the
   Prepona deiphile group actually correspond to Prepona brooksiana, whose
   distribution occurs only in Mexico and Central America. In this way, the
   deiphile group of Prepona would be composed of three species: P.
   deiphile Godart and P. xenagoras Hewitson, with distribution in South
   America, and Prepona brooksiana in Mexico and Central America.}},
