@article{ ISI:000410800500003,
Author = {Torres-Freyermuth, Alec and Brocchini, Maurizio and Corvaro, Sara and
   Carlos Pintado-Patino, Jose},
Title = {{Wave attenuation over porous seabeds: A numerical study}},
