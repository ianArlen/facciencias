@article{ ISI:000411416700028,
Author = {Campos-Garcia, Victor R. and Herrera-Fernandez, Daniel and Espinosa-de
   la Garza, Carlos E. and Gonzalez, German and Vallejo-Castillo, Luis and
   Avila, Sandra and Munoz-Garcia, Leslie and Medina-Rivero, Emilio and
   Perez, Nestor O. and Gracia-Mora, Isabel and Mayra Perez-Tapia, Sonia
   and Salazar-Ceballos, Rodolfo and Pavon, Lenin and Flores-Ortiz, Luis F.},
Title = {{Process signatures in glatiramer acetate synthesis: structural and
   functional relationships}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2017}},
Volume = {{7}},
Month = {{SEP 21}},
Abstract = {{Glatiramer Acetate (GA) is an immunomodulatory medicine approved for the
   treatment of multiple sclerosis, whose mechanisms of action are yet to
   be fully elucidated. GA is comprised of a complex mixture of
   polypeptides with different amino acid sequences and structures. The
   lack of sensible information about physicochemical characteristics of GA
   has contributed to its comprehensiveness complexity. Consequently, an
   unambiguous determination of distinctive attributes that define GA is of
   highest relevance towards dissecting its identity. Herein we conducted a
   study of characteristic GA heterogeneities throughout its manufacturing
   process (process signatures), revealing a strong impact of critical
   process parameters (CPPs) on the reactivity of amino acid precursors;
   reaction initiation and polymerization velocities; and peptide
   solubility, susceptibility to hydrolysis, and size-exclusion properties.
   Further, distinctive GA heterogeneities were correlated to defined
   immunological and toxicological profiles, revealing that GA possesses a
   unique repertoire of active constituents (epitopes) responsible of its
   immunological responses, whose modification lead to altered profiles.
   This novel approach established CPPs influence on intact GA peptide
   mixture, whose physicochemical identity cannot longer rely on reduced
   properties (based on complete or partial GA degradation), providing
   advanced knowledge on GA structural and functional relationships to
   ensure a consistent manufacturing of safe and effective products.}},
