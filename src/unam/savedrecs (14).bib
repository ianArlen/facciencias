@article{ ISI:000411541600049,
Author = {Chavez Portillo, M. and Mathew, X. and Santiesteban Juarez, H. and Pacio
   Castillo, M. and Cortes Santiago, A. and Portillo Moreno, O.},
Title = {{Influence of Li+ doping on the optical properties of PbS}},
Journal = {{SUPERLATTICES AND MICROSTRUCTURES}},
Year = {{2017}},
Volume = {{109}},
Pages = {{423-430}},
Month = {{SEP}},
Abstract = {{We investigate the influence of Li+ in the optical properties of PbS
   thin films. PbS and PbS:Li doped films were prepared by chemical bath
   deposition. Optical studies using the first derivative absorption
   spectra reveals the multiple exciton generation process associated to
   the excitonic transitions. The refractive index (n), extinction
   coefficient (k), and dielectric parameters (epsilon(1) and epsilon(2))
   were obtained. The analysis of current -temperature measurement in the
   12-320 K range shows that the activation energy exhibits lower values
   with increasing Li+ concentration. The mechanism of current conduction
   in the films is discussed. (C) 2017 Elsevier Ltd. All rights reserved.}},
