@article{ ISI:000410559600004,
Author = {Testo, Weston L. and Sundue, Michael and Vasco, Alejandra},
Title = {{Diplazium hybrids involving D-plantaginifolium and D. ternatum from
   Mexico and Central America}},
Journal = {{BRITTONIA}},
Year = {{2017}},
Volume = {{69}},
Number = {{3}},
Pages = {{295-306}},
Month = {{SEP}},
Abstract = {{Here we evaluate the origins and relationships of Mexican and Central
   American Diplazium hybrids derived from crosses involving either D.
   plantaginifolium or D. ternatum. Based on study of live plants and
   herbarium specimens, we distinguish D. xverapax from the similar D.
   riedelianum and present evidence that the former is a sterile hybrid
   derived from a cross between D. plantaginifolium and D. werckleanum. We
   also describe new hybrids, D. xtorresianum and D. xsubternatum from
   Mexico and northern Central America. Both involve D. ternatum as one
   parent. Diplazium. cristatum is the other putative parent of D.
   xtorresianum, and D. plantaginifolium is the second parent of D.
   xsubternatum. We also designate lectotypes for D. cordovense and D.
   dissimile.}},
