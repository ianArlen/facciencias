@article{ ISI:000406363500001,
Author = {D'Alba, Liliana and Torres, Roxana and Waterhouse, Geoffrey I. N. and
   Eliason, Chad and Hauber, Mark E. and Shawkey, Matthew D.},
Title = {{What Does the Eggshell Cuticle Do? A Functional Comparison of Avian
   Eggshell Cuticles}},
