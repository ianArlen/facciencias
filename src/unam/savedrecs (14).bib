@article{ ISI:000439895900001,
Author = {Hortiales Gonzalez, Arantxa Fatima and Gomez Navarro, Jorge Antonio and
   Barajas Nava, Leticia Andrea and Garduno Espinosa, Juan},
Title = {{Archibald Cochrane: evidence, effectiveness and decision-making in
   health}},
Journal = {{BOLETIN MEDICO DEL HOSPITAL INFANTIL DE MEXICO}},
Year = {{2017}},
Volume = {{74}},
Number = {{5}},
Pages = {{319-323}},
Month = {{SEP-OCT}},
Abstract = {{Nowadays, Evidence-Based Medicine plays a fundamental role while making
   medical decisions, considering that through the methods of science, it
   attempts to justify the variety of alternatives that may be offered to
   patients.
   In order to understand the historical evolution of this way of
   practicing medicine, it is necessary to review the contribution of one
   of the main participants in this cultural movement: Archibald Leman
   Cochrane, who helped to define the theoretical framework that has
   allowed the integration of science into the practice of medicine. Since
   he insisted in the need of integrating scientific evidence into clinical
   experience, his role became a fundamental and decisive element in the
   development of a new discipline: Evidence-Based Medicine. (C) 2017
   Hospital Infantil de Mexico Federico Gomez. Published by Masson Doyma
   Mexico S.A.}},
