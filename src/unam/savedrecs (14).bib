@article{ ISI:000413793100005,
Author = {Moreno-Tovar, Raul and Annel Yanez-Hernandez, Osiris and Perez-Moreno,
   Fidel and Cruz Rivera, Jose de Jesus and Leonor Rivera, Ana and
   Rodriguez-Lugo, Ventura},
Title = {{Clays for Brick Manufacturing in Actopan, Hidalgo: Physical, Chemical
   and Mineralogical Characterization}},
Journal = {{MATERIALS RESEARCH-IBERO-AMERICAN JOURNAL OF MATERIALS}},
Year = {{2017}},
Volume = {{20}},
Number = {{5}},
Pages = {{1185-1192}},
