@article{ ISI:000408434700001,
Author = {Pakou, A. and Keeley, N. and Cappuzzello, F. and Acosta, L. and Agodi,
   C. and Aslanoglou, X. and Calabrese, S. and Carbone, D. and Cavallaro,
   M. and Foti, A. and Marquinez-Duran, G. and Martel, I. and Mazzocco, M.
   and Parascandolo, C. and Pierroutsakou, D. and Rusek, K. and Sgouros, O.
   and Soukeras, V. and Strano, E. and Zagatto, V. A. B.},
Title = {{The Li-7(d, p)Li-8 reaction in inverse kinematics at 5.44 MeV/u}},
Journal = {{EUROPEAN PHYSICAL JOURNAL A}},
Year = {{2017}},
Volume = {{53}},
Number = {{8}},
Month = {{AUG 25}},
Abstract = {{New data are presented for the Li-7(d, p)Li-8 stripping reaction which,
   together with previously reported elastic scattering data taken in the
   same experiment, provide a coherent set. These data, plus existing
   measurements of the elastic scattering and stripping at 6MeV/u were
   analysed within the same coupled reaction channels scheme. Good
   descriptions of the stripping data to the 0.0MeV 2(+) and 0.98MeV 1(+)
   states of Li-8 were obtained using a set of < Li-8 vertical bar Li-7 + n
   > overlaps taken from the literature, provided that the elastic
   scattering was also well described. Multi-step reaction paths made
   significant contributions to the description of the larger angle data.
   The asymptotic normalisation coefficients are compared with previous
   determinations.}},
