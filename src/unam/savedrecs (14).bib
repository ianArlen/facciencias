@article{ ISI:000408429600005,
Author = {Alvarado-Cardenas, Leonardo O. and Lozada-Perez, Lucio},
Title = {{A new species of Mandevilla (Apocynaceae; Apocynoideae) from Mexico}},
Journal = {{PHYTOTAXA}},
Year = {{2017}},
Volume = {{319}},
Number = {{1}},
Pages = {{93-102}},
Month = {{AUG 29}},
Abstract = {{We describe and illustrate a new species of Mandevilla for the states of
   Oaxaca and Veracruz, Mexico. Mandevilla sherlockii sp. nov. can be
   separated from the other similar species of the genus by its
   subcoriaceous leaf blades and inflorescences with a conspicuously curved
   rachis between each flower. Including this new species, Mexico is a
   hotspot for the genus with a 24 species, 11 of which are endemic. We
   provide a distribution map, suggest a conservation status for the new
   species, and provide a key for the species of Mandevilla found in Mexico
   and Central America.}},
