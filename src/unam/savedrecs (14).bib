@article{ ISI:000434668700006,
Author = {Monroy Vite, Nadja and Garcia-Lopez, Eric},
Title = {{Forensic neuropsychology, a contribution to the justice system in Mexico}},
Journal = {{REVISTA CRIMINALIDAD}},
