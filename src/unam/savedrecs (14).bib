@article{ ISI:000416746100010,
Author = {Aguilar-Pinto, Emma Del Carmen and Tunon-Pablos, Esperanza and
   Morales-Barragan, Federico},
Title = {{Microcredit and poverty. The experience of banmujer's social
   microenterprise program in Chiapas}},
Journal = {{ECONOMIA SOCIEDAD Y TERRITORIO}},
Year = {{2017}},
Volume = {{17}},
Number = {{55}},
Pages = {{809-835}},
Month = {{SEP-DEC}},
Abstract = {{This article analyzes the experiences of beneficiaries of Banmujer's
   Social Microenterprises program which has been in operation in Chiapas
   since 2001, based on the conceptual trilogy of the poor (subject),
   poverty (context), and impoverishment (process) as proposed by
   Vega-Estrada. Our results show that the role of microcredit in poverty
   eradication must not be overestimated and that while they can contribute
   to reverse the process of impoverishment of poor people, do not affect
   the structural problem of poverty. With this we hope to contribute to
   the discussion about whether women microcredit is an effective tool to
   combat poverty.}},
