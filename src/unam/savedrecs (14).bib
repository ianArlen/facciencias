@article{ ISI:000410601000018,
Author = {Rios-Ramirez, Ariadna and Giordano, Magda and Tamariz, Elisa},
Title = {{Thermoreversible Hydrogels of Methylcellulose: A Biocompatible
   Alternative for Delivery of Proteins into the Brain}},
Journal = {{JOURNAL OF BIOMATERIALS AND TISSUE ENGINEERING}},
Year = {{2017}},
Volume = {{7}},
Number = {{9}},
Pages = {{889-900}},
Month = {{SEP}},
Abstract = {{The characterization of recombinant proteins with neurotrophic effects
   on axon outgrowth and guidance has advanced considerably in recent
   decades. This knowledge, however, needs to be supplemented with
   information about brain-delivery vectors for them to be optimally used
   in neuro-regenerative therapies. We analyzed if thermoreversible
   hydrogels of methylcellulose are suitable for neurotrophic protein
   delivery and if hydrogel injection into the brain preserves the
   integrity of the tissue. Protein released from methylcellulose formed a
   gradient as far as 500 mu m from the release point, and the released
   neurotrophic protein, Semaforin 3A, retained its biological function in
   dorsal root ganglia and ventral mesencephalon neuron cultures.
   Histological, immunological and behavioral assays performed after
   methylcellulose implantation in the rat striatum showed no differences
   between control and treated groups in the activation of microglia or
   astrocytes; only a transient recruitment of macrophages limited to the
   area with mechanical damage was observed. Sensory and motor coordination
   tests showed no significant alterations after methylcellulose
   implantation, only a transient increase in spontaneous locomotor
   activity was observed in methylcellulose-implanted at the first two
   weeks. Our results showed that methylcellulose is a suitable and
   compatible delivery vector for bioactive proteins in the brain without
   deleterious and chronicle effects in this organ.}},
