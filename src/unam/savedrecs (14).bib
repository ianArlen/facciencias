@article{ ISI:000415770500009,
Author = {Fraile, Aurora and McLeish, Michael J. and Pagan, Israel and
   Gonzalez-Jara, Pablo and Pinero, Daniel and Garcia-Arenal, Fernando},
Title = {{Environmental heterogeneity and the evolution of plant-virus
   interactions: Viruses in wild pepper populations}},
Journal = {{VIRUS RESEARCH}},
Year = {{2017}},
Volume = {{241}},
Number = {{SI}},
Pages = {{68-76}},
Month = {{SEP 15}},
Note = {{13th International Plant Virus Epidemiology Symposium, Avignon, FRANCE,
   JUN 06-10, 2016}},
Organization = {{Int Soc Plant Pathol, Int Comm Plant Virus Epidemiol; Natl Inst Agron
   Res}},
Abstract = {{Understanding host-pathogen interactions requires analyses to address
   the multiplicity of scales in heterogeneous landscapes. Anthropogenic
   influence on plant communities, especially cultivation, is a major cause
   of environmental heterogeneity. We have approached the analysis of how
   environmental heterogeneity determines plant-virus interactions by
   studying virus infection in a wild plant currently undergoing incipient
   domestication, the wild pepper or chiltepin, across its geographical
   range in Mexico. We have shown previously that anthropogenic disturbance
   is associated with higher infection and disease risk, and with disrupted
   patterns of host and virus genetic spatial structure. We now show that
   anthropogenic factors, species richness, host genetic diversity and
   density in communities supporting chiltepin differentially affect
   infection risk according to the virus analysed. We also show that in
   addition to these factors, a broad range of abiotic and biotic variables
   meaningful to continental scales, have an important role on the risk of
   infection depending on the virus. Last, we show that natural virus
   infection of chiltepin plants in wild communities results in decreased
   survival and fecundity, hence negatively affecting fitness. This
   important finding paves the way for future studies on plant-virus
   co-evolution.}},
