@article{ ISI:000404490800034,
Author = {Heatley Tejada, Ana and Montero, Maria and Dunbar, Robin},
Title = {{Being unempathic will make your loved ones feel lonelier: Loneliness in
   an evolutionary perspective}},
Journal = {{PERSONALITY AND INDIVIDUAL DIFFERENCES}},
Year = {{2017}},
Volume = {{116}},
Pages = {{223-232}},
Month = {{OCT 1}},
Abstract = {{Loneliness has been conceptualized as an alarm against social isolation
   under an evolutionary theoretical framework. Loneliness, as an adaptive
   feature, would have to be linked to environmental conditions and not
   only to the subjective evaluation of our social network. This research
   investigates lack of received empathy as an environmental trigger of the
   loneliness alarm. Lack of received empathy could explain previously
   observed correlations between loneliness and low satisfaction with
   social relationships, attachment insecurity, and certain age groups like
   teenagers and the elderly. A well-defined evolutionary model opens up
   new possibilities for the development of more efficient strategies to
   reduce loneliness. (C) 2017 Elsevier Ltd. All rights reserved.}},
